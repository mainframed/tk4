         MACRO
&LABEL  $CALL  &NAME
         LCLC  &LNAME
         AIF   ('&LABEL' EQ '').SKIP01
&LABEL   DS    0H
.SKIP01  ANOP
         AIF   ('&NAME' EQ '').MNOTE01
         AIF   ('&NAME'(1,1) EQ '(').SKIP02
&LNAME   SETC  'V&NAME'            ENTRY NAME OF ROUTINE
&LNAME   SETC  '&LNAME'(1,8)       ASSEMBLER SPECIFICATIONS
         LA    15,&LNAME-SCRICVT   GET ENTRY OFFSET OF ROUTINE
         BAL   6,SCVTENT           BRANCH TO ENTRY SEQUENCE
         MEXIT
.SKIP02  AIF   ('&NAME' EQ '(R15)').SKIP03
         LR    15,&NAME            GET ENTRY OFFSET OF ROUTINE
.SKIP03  ANOP
         BAL   6,SCVTENTR          BRANCH TO REG ENTRY SEQUENCE
         MEXIT
.MNOTE01 MNOTE 8,'$CALL  --  MISSING ROUTINE NAME TO CALL'
         MEND
