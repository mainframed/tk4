         MACRO
&NAME   SUBOUT &RESTORE=,&PREFIX=,&RETURN=
.*
.*       PDSDOC *MACRO FOR SUB-ROUTINE EXIT...........................*
.*
         GBLA  &SUBINC
         LCLA  &N,&S
         LCLC  &R,&P,&PP,&B
         AIF   ('&NAME' EQ '').NONAME
&NAME    DS    0H È
.NONAME  ANOP
&N       SETA  N'&RESTORE
         AIF   (&N NE 0).C3
.C1      ANOP
&B       SETC  'R4'
         AIF   ('&RETURN' EQ '').C2
         AIF   ('&RETURN'(1,1) NE '(').C11
&B       SETC  '&RETURN(1)'
         AGO   .C2
.C11     ANOP
&B       SETC  '&RETURN'
         B     &B È                RETURN TO CALLER
         MEXIT
.C2      BR    &B È                RETURN TO CALLER
         MEXIT
.C3      ANOP
&P       SETC  '&PREFIX'
         AIF   ('&P' EQ '').C4
&PP      SETC  '&P.SAVE'
         AGO   .C5
.C4      ANOP
&PP      SETC  '$&SUBINC'
.C5      ANOP
&S       SETA  &S+1
         AIF   (&S GT &N).C1
         L     &RESTORE(&S),&PP&RESTORE(&S) È     RESTORE REGISTER
         AGO   .C5
         MEND
