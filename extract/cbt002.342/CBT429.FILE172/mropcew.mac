//*
//*      PDSDOC *USER PROGRAM TO COMMUNICATE WITH OPC/A...............*
//*
//ASM      EXEC  ASMHCL
//C.SYSIN  DD    DATA,DLM=ZZ
MROPCEW  CSECT
         USING *,12
         SAVE  (14,12),,*
         LR    12,15
         ST    13,SAVEA+4
         LR    2,13
         LA    13,SAVEA
         ST    13,8(2)
*
         L     10,0(1)         ADDRESSE PARMLIST INTO REG 10
*
         MVC   PARM,0(10)      LOAD INTERN PARMLIST
*
         CLC   PLL,=H'8'       CHECK PARM LENGTH
         BL    L1
         CLC   PLL,=H'17'
         BL    L2
*        CLC   PLL,=H'26'
*        BL    L3
*
L3       EQU   * MOVE JOBNAME
         MVC   OPCJOB,PJOB
L2       EQU   *
         MVC   OPCAPPL(8),PAPPL
L1       EQU   *
*
         CLI   PTYP,C'S'              CHECK  TYPE
         BE    LAB1
         CLI   PTYP,C'C'
         BE    LAB1
         CLI   PTYP,C'I'
         BE    LAB1
         CLI   PTYP,C'E'
         BE    LAB1
         CLI   PTYP,C'X'
         BE    LAB1
         MVC   OUTAREA(121),MSG1
         B     END
LAB1     EQU   *
*
         MVC   OPCTYPE,PTYP            MOVE TYPE
*
         CLC   PWORK,DUMY              CHECK  WORKSTATION
         BE    LAB2
         CLC   PWORK,JOBP
         BE    LAB2
         CLC   PWORK,CPU3
         BE    LAB2
         CLC   PWORK,CPU4
         BE    LAB2
         CLC   PWORK,OPER
         BE    LAB2
         CLC   PWORK,TAPI
         BE    LAB2
         CLC   PWORK,TAPO
         BE    LAB2
         CLC   PWORK,AUTO
         BE    LAB2
         CLC   PWORK,DISP
         BE    LAB2
         CLC   PWORK,DUMY
         BE    LAB2
         MVC   OUTAREA,MSG2
         B     END
*
LAB2     EQU   *
         MVC   OPCWSNA,PWORK            MOVE WORKSTATION
*
*        CLI   POPRNR,C'0' CHECK OPERATION NUMBER
*        BE    LAB3 NOT ACTIV
*        CLI   POPRNR,C'1'
*        BE    LAB3
*        CLI   POPRNR,C'2'
*        BE    LAB3
*        CLI   POPRNR,C'3'
*        BE    LAB3
*        CLI   POPRNR,C'4'
*        BE    LAB3
*        CLI   POPRNR,C'5'
*        BE    LAB3
*        CLI   POPRNR,C'6'
*        BE    LAB3
*        CLI   POPRNR,C'7'
*        BE    LAB3
*        CLI   POPRNR,C'8'
*        BE    LAB3
*        CLI   POPRNR,C'9'
*        BE    LAB3
*        MVC   OUTAREA,MSG4
*        B     END
*
LAB3     EQU   *
*        MVC   OPCNUM,POPRNR
*
*     CALL OPCA INTERFACE
*
*
         CALL  DRKUSINT,(OPCTYPE,OPCWSNA,OPCJOB,OPCAPPL,OPCNUM,OPCIA,  *
               OPCDUR,OPCERR,OPCFORM,OPCSCLA,OPCSUBS,OPCTIME,OPCDATE,  *
               OPCRET)
*
*      CHECK RETURN CODE
*
         CLC   OPCRET,=F'0'
         BE    SETCD0
         CLC   OPCRET,=F'4'
         BE    SETCD4
         MVC   RETURNCD,=C'08'
         MVC   OUTAREA,MSG3
         B     END
SETCD0   EQU   *
         MVC   RETURNC,=C'00'
         MVC   OUTAREA,MSG5
         B     END
SETCD4   EQU   *
         MVC   RETURNCD,=C'04'
         MVC   OUTAREA,MSG3
*
END      EQU   *
*
*      WRITE ERROR OR FINAL MESSAGE
*
         OPEN  (OUTDCB,(OUTPUT))
         PUT   OUTDCB,OUTAREA
         CLOSE (OUTDCB)
*
         L     13,SAVEA+4
         RETURN (14,12),RC=0
*
PACKF    DC    PL8'0'
         DS    0D
SAVEA    DS    18F
OUTDCB   DCB   DSORG=PS,MACRF=PM,DDNAME=OUTDD
PARM     DS    0CL100
PLL      DS    H
PTYP     DS    CL1
PF1      DS    CL1
PWORK    DS    CL4
PF2      DS    CL1
PJOB     DS    CL8
PF3      DS    CL1
PAPPL    DS    CL8
PF4      DS    CL1
POPRNR   DS    CL2
*
OUTAREA  DS    CL121
*
MSG1     DC    CL121'OPC MSG1    FALSCHE OPC EVENT TYPE      '
MSG2     DC    CL121'OPC MSG2    FALSCHE WORK-STATION        '
MSG3     DC    CL063'OPC MSG3    EVENTWRITER CALL NICHT ERFOLGREICH    *
               RETURNCODE=  '
RETURNCD DC    CL2'  '
         DC    CL56'                                  '
MSG4     DC    CL121'OPC MSG4    OPC WORK STATION NICHT NUMERISCH'
MSG5     DC    CL063'OPC MSG5    EVENTWRITER CALL  ERFOLGREICH         *
               RETURNCODE=  '
RETURNC  DC    CL2'  '
         DC    CL56'                                  '
DUMY     DC    CL4'DUMY'
JOBP     DC    CL4'JOBP'
CPU3     DC    CL4'CPU3'
CPU4     DC    CL4'CPU4'
OPER     DC    CL4'OPER'
TAPI     DC    CL4'TAPI'
TAPO     DC    CL4'TAPO'
AUTO     DC    CL4'AUTO'
DISP     DC    CL4'DISP'
*
OPCTYPE  DC    CL1' '
OPCWSNA  DC    CL4'    '
OPCJOB   DC    CL8'        '
OPCAPPL  DC    CL16'               '
OPCNUM   DC    H'0'
OPCIA    DC    CL10'           '
OPCDUR   DC    CL4'    '
OPCERR   DC    CL4'    '
OPCFORM  DC    CL8'        '
OPCSCLA  DC    CL1' '
OPCSUBS  DC    CL4'OPC2'
OPCTIME  DC    CL4'    '
OPCDATE  DC    CL4'    '
OPCRET   DC    CL4'    '
         END
ZZ
//*
//L.SYSLIB   DD   DISP=SHR,DSN=SYS1.LINKLIB
//L.SYSLMOD  DD   DSN=SYS2.LINKLIB,DISP=SHR
//L.SYSIN    DD   *
 SETCODE AC(1)
 NAME MROPCEW(R)
/*
