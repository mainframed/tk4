//*
//*      PDSDOC *RMF OVER A MONTH FROM COMPRESSED DATA................*
//*
//SORT1    EXEC PGM=SORT,REGION=8000K
//SYSUDUMP DD  SYSOUT=D
//SYSUT1   DD  DISP=SHR,DSN=COMPRESS.RMF.MONTH,
//             DCB=BFTEK=A
//SORTOUT  DD  DISP=(,CATLG),
//             DSN=TEST.COMPACT.SORT,
//             DCB=(RECFM=VBS,LRECL=32756,BLKSIZE=23476),
//             SPACE=(CYL,(150,150),RLSE),UNIT=(TEST,3)
//MODSLIB  DD  DISP=SHR,DSN=TEST.LOADLIB
//SYSPRINT DD  SYSOUT=*
//SYSOUT   DD  SYSOUT=*
//SYSIN    DD *
 INCLUDE COND=(6,1,FI,NE,4)    <--- DELETE SMF04 RECORDS
 MODS E15=(SSDCE15,120000,MODSLIB,N)  <-- DECOMPRESSION EXIT
 RECORD TYPE=V,LENGTH=(32756,32756,4)   <-- RECORD LENGTHS (MAX)
 SORT FIELDS=COPY
 END
/*
//*
//RMF     EXEC PGM=ERBRMFPP,REGION=6000K
//MFPINPUT DD  DISP=SHR,DSN=TEST.COMPACT.SORT,
//             DCB=BFTEK=A
//MFPMSGDS DD  SYSOUT=*
//SYSIN    DD  *
SYSOUT(H)
MAXPLEN(66)
PLOTS(CPUID(0),TPAG,HUIC)
PTOD(1301,1601)
PINTV(0300)
DINTV(0300)
SUMMARY(TOT)
