//*
//*      PDSDOC *SAMPLE JCL FOR MAPDISK PROGRAMS......................*
//*
//*
//*             *CREATE MAPDISK SUMMARY FILE..........................*
//*
//MAPDISK EXEC PGM=MAPDISK,REGION=8M
//SYSUDUMP DD  SYSOUT=D
//SYSPRINT DD  SYSOUT=*
//SYSOUT   DD  SYSOUT=*
//SYSUT2   DD  DISP=(,CATLG,DELETE),UNIT=DISK,
//             SPACE=(TRK,(450,45),RLSE),
//             DCB=(MDS,RECFM=FB,LRECL=160,BLKSIZE=23360),
//             DSN=MAPDISK.SUMMARY(+01)
//SYSUT1   DD  DUMMY
//*
//VD240    DD  DISP=SHR,UNIT=3380,VOL=SER=TEST01
//VD241    DD  DISP=SHR,UNIT=3380,VOL=SER=TEST08
//VD242    DD  DISP=SHR,UNIT=DISK,VOL=SER=TST004
//*D243    DD  DISP=SHR,UNIT=3380,VOL=SER=SYSR01
//VD244    DD  DISP=SHR,UNIT=3380,VOL=SER=SHAR01
//*D245    DD  DISP=SHR,UNIT=3380,VOL=SER=SYSC01
//VD246    DD  DISP=SHR,UNIT=DISK,VOL=SER=TEST03
//*D247    DD  DISP=SHR,UNIT=3380,VOL=SER=SYSR02
//VD248    DD  DISP=SHR,UNIT=DISK,VOL=SER=TST006
//VD249    DD  DISP=SHR,UNIT=DISK,VOL=SER=PROD13
//*D24A    DD  DISP=SHR,UNIT=3380,VOL=SER=SYSC02
//VD24B    DD  DISP=SHR,UNIT=DISK,VOL=SER=TEST02
//VD24C    DD  DISP=SHR,UNIT=3380,VOL=SER=PROD08
//*D24D    DD  DISP=SHR,UNIT=3380,VOL=SER=SYSM01
//VD24E    DD  DISP=SHR,UNIT=3380,VOL=SER=TEST09
//VD24F    DD  DISP=SHR,UNIT=DISK,VOL=SER=TST005
//*
//*             *SUMMARY OF WASTE SPACE USING MAPDISK LIST PROGRAM....*
//*
//SORT1   EXEC PGM=ICEMAN,PARM='SIZE=MAX',REGION=9M
//SYSOUT   DD  SYSOUT=*
//SORTIN   DD  DISP=SHR,DSN=MAPDISK.SUMMARY(+1)
//SORTOUT  DD  DISP=(,PASS),
//             DSN=&&DSNAME,
//             DCB=(RECFM=FB,LRECL=160,BLKSIZE=23360),
//             SPACE=(TRK,(50,10),RLSE),UNIT=WORK
//*
//SYSIN    DD  *
 SORT FIELDS=(31,44,CH,A,97,5,CH,D)
/*
//*
//*             *COMBINE RECORDS FOR MULTI-VOLUME FILES...............*
//*
//SORT2   EXEC PGM=ICEMAN,PARM='SIZE=MAX',REGION=9M
//SYSOUT   DD  SYSOUT=*
//SORTIN   DD  DISP=(OLD,PASS),DSN=&&DSNAME
//SORTOUT  DD  DISP=(,CATLG),
//             DSN=MAPDISK.TESTFILE,
//             DCB=(RECFM=FB,LRECL=160,BLKSIZE=23360),
//             SPACE=(TRK,(50,10),RLSE),UNIT=PROD
//*
//SYSIN    DD  *
 OPTION EQUALS
 SORT FIELDS=(31,44,CH,A)
 SUM  FIELDS=(112,5,ZD,117,5,ZD,126,3,ZD)
/*
//*
//*             *SELECT FILES WITH MORE THAN 500 FREE TRACKS..........*
//*
//SORT3   EXEC PGM=ICEMAN,PARM='SIZE=MAX',REGION=9M
//SYSOUT   DD  SYSOUT=*
//SORTIN   DD  DISP=SHR,DSN=MAPDISK.TESTFILE
//SORTOUT  DD  DISP=(,PASS),
//             DSN=&&MAPDISK,
//             DCB=(RECFM=FB,LRECL=160,BLKSIZE=23360),
//             SPACE=(TRK,(50,10),RLSE),UNIT=WORK
//*
//SYSIN    DD  *
 INCLUDE COND=(146,5,CH,GT,C'  500')
 SORT FIELDS=(146,5,CH,D,1,6,CH,A,31,44,CH,A)
/*
//*
//*             *SORT OUT FRAGMENTED FILES............................*
//*
//FRAG1   EXEC PGM=ICEMAN,PARM='SIZE=MAX',REGION=9M
//SYSOUT   DD  SYSOUT=*
//SORTIN   DD  DISP=SHR,DSN=MAPDISK.TESTFILE
//SORTOUT  DD  DISP=(,PASS),
//             DSN=&&FRAGS,
//             DCB=(RECFM=FB,LRECL=160,BLKSIZE=23360),
//             SPACE=(TRK,(50,10),RLSE),UNIT=TEST
//*
//SYSIN    DD  *
 INCLUDE COND=(112,5,ZD,GT,300,AND,(129,3,CH,GT,C' 50',
               OR,133,3,CH,GT,C' 50'))
 SORT FIELDS=(31,44,CH,A)
