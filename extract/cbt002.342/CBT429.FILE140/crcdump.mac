PROC 0 DEBUG(0)
/* REF: ABL.CMDPROC(ARCDUMP)
/* DOC: CLIST FOR SUBMITTING BATCH DF/DSS DASD ARCHIVE JOBSTREAM
/*      THIS CLIST INVOKED VIA SPF 3.A.ARC.DUMP
CONTROL MSG NOLIST
IF &DEBUG = DEBUG THEN CONTROL MSG LIST SYMLIST CONLIST
ISPEXEC DISPLAY PANEL(ARCDUMP)
IF &LASTCC EQ 8 THEN +
 DO
  ISPEXEC SETMSG MSG(ARC011B)
  EXIT CODE(0)
 END
ISPEXEC VGET (ZTEMPF ARCQTR ARCYEAR)
IF &SYSDSN('ABL.ARCDASD.USERDATA.D0&ARCQTR&ARCYEAR..VUSER01')  +
 = &STR(VOLUME NOT ON SYSTEM) THEN +
  DO
   /* PROCEED NO FURTHER .... ALREADY HAVE A CATALOG ENTRY FOR THE
   /* SPECIFIED QUARTER AND YEAR.  DON'T BOTHER SUBMITTING DUMP JOB.
   ISPEXEC SETMSG MSG(ARC011F)
   EXIT CODE(0)
  END
ISPEXEC FTOPEN TEMP
ISPEXEC FTINCL ARCDUMP
ISPEXEC FTCLOSE
SUBMIT '&ZTEMPF'
ISPEXEC SETMSG MSG(ARC011C)
SE '#ABLUTIL: QUARTERLY USERDATA ARCHIVE (DUMP) SUBMITTED BY',CN(01)
%OUTLIST
EXIT CODE (0)
