         MACRO
&NAME    TOP
         GBLC  &SRR,&XSAVE(200),&BALREG,&XSTACK
         GBLA  &XSAVEX
         GBLB  &TOPSET
         GBLB  &DOREGF
         STRCHECK   BOT
&DOREGF  SETB  0
&TOPSET  SETB  1
&NAME    DS    0H
         AIF   ('&XSTACK' EQ 'YES').USEBAKR
&XSAVEX  SETA  &XSAVEX+1
&XSAVE(&XSAVEX)   SETC   'SAVEREG_&SYSNDX'
         ST    &BALREG,&XSAVE(&XSAVEX)
         MEXIT
.USEBAKR   ANOP
         BAKR  R14,R0
         MEND
