SET &CTR = 0
DISPLAY: +
SET &PANEL = &&PANEL&CTR
ISPEXEC DISPLAY PANEL(&PANEL)
IF &KEY = PF03 THEN DO
GOTO EXIT
END
IF &KEY = PF15 THEN DO
GOTO EXIT
END
IF &KEY = PF07 THEN DO
IF &DATATYPE(&ZCMD) = NUM THEN DO
IF &EVAL(&ZCMD) <= &CTR THEN SET &CTR = &CTR -(&EVAL(&ZCMD))
IF &EVAL(&ZCMD) > &CTR THEN SET &CTR = 0
SET &ZCMD =
GOTO DISPLAY
END
SET &CTR = &EVAL(&CTR-1)
GOTO DISPLAY
END
IF &KEY = PF19 THEN DO
IF &DATATYPE(&ZCMD) = NUM THEN DO
IF &EVAL(&ZCMD) <= &CTR THEN SET &CTR = &CTR -(&EVAL(&ZCMD))
IF &EVAL(&ZCMD) > &CTR THEN SET &CTR = 0
SET &ZCMD =
GOTO DISPLAY
END
SET &CTR = &EVAL(&CTR-1)
GOTO DISPLAY
END
IF &DATATYPE(&ZCMD) = NUM THEN DO
SET &CTR = &CTR + &EVAL(&ZCMD)
IF &CTR > &PANMAX THEN SET &CTR = &EVAL(&PANMAX -1)
SET &ZCMD =
GOTO DISPLAY
END
SET &CTR = &CTR + 1
ERROR DO
GOTO EXIT
END
GOTO DISPLAY
EXIT: +
ISPEXEC VGET (SAVEPF03 SAVEPF15 SAVEPF08 SAVEPF20 SAVEPF07 SAVEPF19) &P
SET &ZPF03 = &SAVEPF03
