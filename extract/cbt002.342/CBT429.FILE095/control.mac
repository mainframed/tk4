ISREDIT MACRO (HELP) NOPROCESS
     IF &HELP = ? THEN +
       DO
         ISPEXEC DISPLAY PANEL(CONTROL)
         EXIT
       END
START: +
IF &HELP = ON THEN DO
SET &CNTL = &STR(     CONTROL MSG LIST SYMLIST CONLIST)
GOTO DEST
END
IF &HELP = OFF THEN DO
SET &CNTL = &STR(     CONTROL NOMSG NOLIST NOSYMLIST NOCONLIST)
GOTO DEST
END
SET &HELP = ON
GOTO START
DEST: +
  ISREDIT PROCESS DEST
  IF &LASTCC = 0 THEN DO
  ISREDIT LOCATE .ZDEST
  IF &LASTCC = 8 THEN GOTO EMPTY
       ISREDIT MASKLINE = "&CNTL"
       ISREDIT LINE_AFTER .ZDEST = MASKLINE
    GOTO ERROROUT
EMPTY: +
     ISPEXEC VGET ZLLGJOB1
       ISREDIT MASKLINE = "&CNTL"
       ISREDIT LINE_AFTER 0 = MASKLINE
ERROROUT:+
       ISREDIT MASKLINE = ' '
      SET RC = 0
  END
  END
  EXIT CODE(&RC)
