ISREDIT MACRO (DSNOUT)
      IF &DSNOUT = ? THEN DO
      ISPEXEC DISPLAY PANEL(REPOTHER)
      EXIT
      END
ISREDIT (CRDSN) = DATASET
ISREDIT (CRMEM) = MEMBER
SET &CROUT = &STR('&CRDSN&STR((ZZZZ@&STR())')
ISPEXEC VPUT (CROUT) PROFILE
 ISREDIT REPLACE ZZZZ@ .ZFIRST .ZLAST
  COPY &CROUT &DSNOUT
 ISREDIT (WORKDSN) = DATASET
SET  PDSDSN = &WORKDSN
ISPEXEC  LMINIT DATAID(PDSID) DATASET('&PDSDSN') +
                ENQ(SHRW)                        +
                ORG(PDSORG)
SET LMRC = &LASTCC
IF &LMRC ^= 0 THEN +
   DO
      WRITE LMINIT FAILED - RETURN CODE &LMRC
      WRITE DATA SET IS NOT ALLOCATED OR NOT PARTITIONED
      EXIT CODE(8)
   END

ISPEXEC LMOPEN DATAID(&PDSID) OPTION(OUTPUT)
SET RET1 = &LASTCC
IF &RET1 ^= 0 THEN GOTO EXIT1
ISPEXEC LMMDEL DATAID(&PDSID) MEMBER(ZZZZ@)
SET LMRC = &LASTCC
IF &LMRC ^= 0 THEN GOTO EXIT1
ISPEXEC LMCLOSE DATAID(&PDSID)
ISPEXEC LMFREE DATAID(&PDSID)
EXIT
EXIT1: +
IF &LMRC = 8 THEN +
   DO
      WRITE ERROR CHECK FOR PDS MEMBER ZZZZ@
         ISPEXEC LMCLOSE DATAID(&PDSID)
         ISPEXEC LMFREE DATAID(&PDSID)
      EXIT CODE(0)
   END
ISPEXEC LMCLOSE DATAID(&PDSID)
ISPEXEC LMFREE DATAID(&PDSID)
EXIT
ISPEXEC LMCLOSE DATAID(&PDSID)
ISPEXEC LMFREE DATAID(&PDSID)

