)ATTR
 ^ TYPE(INPUT) INTENS(LOW)
 Ö TYPE(OUTPUT) INTENS(HIGH)
)BODY
+-------------------% EDS FIND OCCURRENCE OF MEMBER PANEL +------------
%COMMAND ===>_ZCMD
%       % SEARCHING  DDNAME ÖTARGETDD    +USERID -%&ZUSER    +TIME   -%&
        +
+SEARCH FOR MEMBER %===>_MEM     + TYPE OF SEARCH %==>_SEARCH +(%FIRST+
        +
CMD:   ( EDIT, BROWSE)
+
^CM1    +   ÖFMEM1 +DSN 01===>_DSN1
^CM2    +   ÖFMEM2 +DSN 02===>_DSN2
^CM3    +   ÖFMEM3 +DSN 03===>_DSN3
^CM4    +   ÖFMEM4 +DSN 04===>_DSN4
^CM5    +   ÖFMEM5 +DSN 05===>_DSN5
^CM6    +   ÖFMEM6 +DSN 06===>_DSN6
^CM7    +   ÖFMEM7 +DSN 07===>_DSN7
^CM8    +   ÖFMEM8 +DSN 08===>_DSN8
^CM9    +   ÖFMEM9 +DSN 09===>_DSN9
^CM10   +   ÖFMEM10+DSN 10===>_DSN10
^CM11   +   ÖFMEM11+DSN 11===>_DSN11
^CM12   +   ÖFMEM12+DSN 12===>_DSN12
%
+PRESS%END KEY+TO RETURN
)INIT
  &CM1  = ' '
  &CM2  = ' '
  &CM3  = ' '
  &CM4  = ' '
  &CM5  = ' '
  &CM6  = ' '
  &CM7  = ' '
  &CM8  = ' '
  &CM9  = ' '
  &CM10 = ' '
  &CM11 = ' '
  &CM12 = ' '
  &WKDSN = ' '
  &WKCMD = ' '
  &HAVECMD = ' '
)PROC
 VER(&SEARCH,LIST,FIRST,ALL)
 VER(&DSN1,NB)
 IF (&CM1  ^= ' ')
    &HAVECMD = YES
    &WKDSN = &DSN1
    &WKCMD = &CM1
 IF (&CM2  ^= ' ')
    &HAVECMD = YES
    &WKDSN = &DSN2
    &WKCMD = &CM2
 IF (&CM3  ^= ' ')
    &HAVECMD = YES
    &WKDSN = &DSN3
    &WKCMD = &CM3
 IF (&CM4  ^= ' ')
    &HAVECMD = YES
    &WKDSN = &DSN4
    &WKCMD = &CM4
 IF (&CM5  ^= ' ')
    &HAVECMD = YES
    &WKDSN = &DSN5
    &WKCMD = &CM5
 IF (&CM6  ^= ' ')
    &HAVECMD = YES
    &WKDSN = &DSN6
    &WKCMD = &CM6
 IF (&CM7  ^= ' ')
    &HAVECMD = YES
    &WKDSN = &DSN7
    &WKCMD = &CM7
 IF (&CM8  ^= ' ')
    &HAVECMD = YES
    &WKDSN = &DSN8
    &WKCMD = &CM8
 IF (&CM9  ^= ' ')
    &HAVECMD = YES
    &WKDSN = &DSN9
    &WKCMD = &CM9
 IF (&CM10 ^= ' ')
    &HAVECMD = YES
    &WKDSN = &DSN10
    &WKCMD = &CM10
 IF (&CM11 ^= ' ')
    &HAVECMD = YES
    &WKDSN = &DSN11
    &WKCMD = &CM11
 IF (&CM12 ^= ' ')
    &HAVECMD = YES
    &WKDSN = &DSN12
    &WKCMD = &CM12
 IF (&HAVECMD = ' ')
    VER(&MEM,NB)
 VPUT (SEARCH,HAVECMD,WKDSN,WKCMD) PROFILE
)END
