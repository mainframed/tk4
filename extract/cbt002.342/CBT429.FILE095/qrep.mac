ISREDIT MACRO (MEMBER)
      IF &MEMBER = ? THEN DO
      ISPEXEC DISPLAY PANEL(QREP)
      EXIT
      END
ISREDIT CREATE &MEMBER .ZFIRST .ZLAST
SET &CREATECC = &LASTCC
IF &CREATECC = 8 THEN DO
 ISREDIT REPLACE &MEMBER .ZFIRST .ZLAST
           SET ZEDSMSG = &STR(&MEMBER REPLACED)
             SET ZEDLMSG = &STR(MEMBER &MEMBER REPLACED IN THIS PDS)
           ISPEXEC SETMSG MSG(ISRZ001)
  EXIT
 END
IF &CREATECC = 0 THEN DO
           SET ZEDSMSG = &STR(&MEMBER CREATED)
             SET ZEDLMSG = &STR(MEMBER &MEMBER CREATED)
           ISPEXEC SETMSG MSG(ISRZ001)
  EXIT
 END
