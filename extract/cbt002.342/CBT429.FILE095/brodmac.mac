ISREDIT MACRO
  ISREDIT EXCLUDE ALL
  ISREDIT DELETE X ALL
  CONTROL ASIS
  SET &I = 1
  SET &SYSOUTTRAP = 400
  TSOEXEC LISTBC
  SET &BCNUM = &SYSOUTLINE
  DO WHILE &BCNUM >= &I
     SET &TEMP = &&SYSOUTLINE&I
     IF &SUBSTR(1:9,&TEMP) = IKJ56951I THEN DO
     SET &BCDISP = NONE
        END
     ISREDIT LINE_AFTER .ZLAST = "&TEMP"
     SET &I = &I + 1
  END
ISPEXEC VPUT BCNUM SHARED
ISPEXEC VPUT BCDISP SHARED
