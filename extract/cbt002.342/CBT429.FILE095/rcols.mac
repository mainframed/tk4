ISREDIT MACRO (HELP) NOPROCESS
 IF &HELP = ? THEN DO
 ISPEXEC DISPLAY PANEL(RCOLS)
 EXIT
 END
ISREDIT (THIS) = MEMBER
  ISREDIT PROCESS DEST
  IF &LASTCC = 0 THEN DO
  ISREDIT LOCATE .ZDEST
  IF &LASTCC = 8 THEN +
     DO
           SET ZEDSMSG = &STR(EMPTY DATA SET )
            SET ZEDLMSG = &STR(JC WORKS ONLY WHEN DATA SET HAS VALID +
                                 DATA AND LINE NUMBERS)
           ISPEXEC SETMSG MSG(ISRZ001)
         EXIT CODE(12)
   END
   ISREDIT (DS1) = DATASET
   LISTDSI '&DS1'
   SET &LN = &SYSLRECL
SET &RULEA = &STR(----0----1----1----2----2----3----3----4)
SET &RULEB = &STR(----4----5----5----6----6----7----7----8)
SET &RULEC = &STR(----8----9----9---10---10---11---11---12)
SET &RULED = &STR(---12---13---13---14---14---15---15---16)
SET &RULEE = &STR(---16---17---17---18---18---19---19---20)
SET &RULEF = &STR(---20---21---21---22---22---23---23---24)
SET &RULEG = &STR(---24---25---25---26---26---27---27---28)
SET &RULE5 = &STR(----5----0----5----0----5----0----5----0)
SET &RULEW = &STR(&RULEA&RULEB&RULEC&RULED&RULEE&RULEF&RULEG)
SET &RULEV = &STR(&RULE5&RULE5&RULE5&RULE5&RULE5&RULE5&RULE5)
SET &RULEX = &SUBSTR(1:&LN,&STR(&RULEW))
SET &RULEY = &SUBSTR(1:&LN,&STR(&RULEV))
       ISREDIT MASKLINE = '&RULEY'
       ISREDIT LINE_AFTER .ZDEST = MASKLINE
       ISREDIT MASKLINE = '&RULEX'
       ISREDIT LINE_AFTER .ZDEST = MASKLINE

ERROROUT:+
       ISREDIT MASKLINE = ' '


      SET RC = 0
  END
  ELSE DO
     IF &MAXCC = 8 THEN DO
         SET ZEDSMSG = &STR(INDICATE POSITION)
         SET ZEDLMSG = &STR(MARK 'A' ON LINE AFTER WHICH /*USERPARM +
                            DATA SHOULD BE INSERTED)
     END
     ELSE DO
         SET ZEDSMSG = &STR(ERROR ENCOUNTERED)
         SET ZEDLMSG = &STR(UNABLE TO PROCESS REQUEST, +
                             RECHECK INPUT)
     END
     ISPEXEC SETMSG MSG(ISRZ000)
     SET RC = 12
  END
  EXIT CODE(&RC)
