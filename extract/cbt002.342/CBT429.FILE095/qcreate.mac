ISREDIT MACRO (MEMBER)
      IF &MEMBER = ? THEN DO
      ISPEXEC DISPLAY PANEL(QCREATE)
      EXIT
      END
ISREDIT CREATE &MEMBER .ZFIRST .ZLAST
SET &CREATECC = &LASTCC
IF &CREATECC = 8 THEN DO
           SET ZEDSMSG = &STR(&MEMBER ALREADY EXISTS)
             SET ZEDLMSG = &STR(MEMBER &MEMBER ALREADY EXISTS IN THIS +
                               PDS CHOOSE ANOTHER MEMBER NAME)
           ISPEXEC SETMSG MSG(ISRZ001)
  EXIT
 END
IF &CREATECC = 0 THEN DO
           SET ZEDSMSG = &STR(&MEMBER CREATED)
             SET ZEDLMSG = &STR(MEMBER &MEMBER CREATED)
           ISPEXEC SETMSG MSG(ISRZ001)
  EXIT
 END
