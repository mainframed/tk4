    DO
      ISPEXEC DISPLAY PANEL(BIG)
      EXIT CODE(1)
    END
  ISREDIT (ROW,COL) = CURSOR

  ISPEXEC CONTROL ERRORS RETURN
  IF &LETTER = 1 THEN  SET &LETTER = &STR(N1)
  IF &LETTER = 2 THEN  SET &LETTER = &STR(N2)
  IF &LETTER = 3 THEN  SET &LETTER = &STR(N3)
