  IF &LETTER = 7 THEN  SET &LETTER = &STR(N7)
  IF &LETTER = 8 THEN  SET &LETTER = &STR(N8)
  IF &LETTER = 9 THEN  SET &LETTER = &STR(N9)
  IF &LETTER = 0 THEN  SET &LETTER = &STR(N0)
  GOTO &LETTER

