)ATTR
 ^ TYPE(INPUT) INTENS(LOW)
 Ö TYPE(OUTPUT) INTENS(HIGH)
)BODY
+-------------------% EDS FIND OCCURRENCE OF MEMBER PANEL +------------
%COMMAND ===>_ZCMD
%       % SEARCHING  DDNAME ÖTARGETDD    +USERID -%&ZUSER    +TIME   -%&
        +
+SEARCH FOR MEMBER %===>_MEM     + TYPE OF SEARCH %==>_SEARCH +(%FIRST+
        +
CMD:   (EDIT, BROWSE)
+              DSN #                                               BLOCK
^CM1    + ÖFM1 +01===>_DSN1                                       _BS1
^CM2    + ÖFM2 +02===>_DSN2                                       _BS2
^CM3    + ÖFM3 +03===>_DSN3                                       _BS3
^CM4    + ÖFM4 +04===>_DSN4                                       _BS4
^CM5    + ÖFM5 +05===>_DSN5                                       _BS5
^CM6    + ÖFM6 +06===>_DSN6                                       _BS6
^CM7    + ÖFM7 +07===>_DSN7                                       _BS7
^CM8    + ÖFM8 +08===>_DSN8                                       _BS8
^CM9    + ÖFM9 +09===>_DSN9                                       _BS9
^CM10   + ÖFM10+10===>_DSN10                                      _BS10
^CM11   + ÖFM11+11===>_DSN11                                      _BS11
^CM12   + ÖFM12+12===>_DSN12                                      _BS12
%
+PRESS%END KEY+TO RETURN
)INIT
  &CM1  = ' '
  &CM2  = ' '
  &CM3  = ' '
  &CM4  = ' '
  &CM5  = ' '
  &CM6  = ' '
  &CM7  = ' '
  &CM8  = ' '
  &CM9  = ' '
  &CM10 = ' '
  &CM11 = ' '
  &CM12 = ' '
  &WKDSN = ' '
  &WKCMD = ' '
  &HAVECMD = ' '
)PROC
 VER(&SEARCH,LIST,FIRST,ALL)
 VER(&DSN1,NB)
 IF (&CM1  ^= ' ')
    &HAVECMD = YES
    &WKDSN = &DSN1
    &WKCMD = &CM1
 IF (&CM2  ^= ' ')
    &HAVECMD = YES
    &WKDSN = &DSN2
    &WKCMD = &CM2
 IF (&CM3  ^= ' ')
    &HAVECMD = YES
    &WKDSN = &DSN3
    &WKCMD = &CM3
 IF (&CM4  ^= ' ')
    &HAVECMD = YES
    &WKDSN = &DSN4
    &WKCMD = &CM4
 IF (&CM5  ^= ' ')
    &HAVECMD = YES
    &WKDSN = &DSN5
    &WKCMD = &CM5
 IF (&CM6  ^= ' ')
    &HAVECMD = YES
    &WKDSN = &DSN6
    &WKCMD = &CM6
 IF (&CM7  ^= ' ')
    &HAVECMD = YES
    &WKDSN = &DSN7
    &WKCMD = &CM7
 IF (&CM8  ^= ' ')
    &HAVECMD = YES
    &WKDSN = &DSN8
    &WKCMD = &CM8
 IF (&CM9  ^= ' ')
    &HAVECMD = YES
    &WKDSN = &DSN9
    &WKCMD = &CM9
 IF (&CM10 ^= ' ')
    &HAVECMD = YES
    &WKDSN = &DSN10
    &WKCMD = &CM10
 IF (&CM11 ^= ' ')
    &HAVECMD = YES
    &WKDSN = &DSN11
    &WKCMD = &CM11
 IF (&CM12 ^= ' ')
    &HAVECMD = YES
    &WKDSN = &DSN12
    &WKCMD = &CM12
 IF (&HAVECMD = ' ')
    VER(&MEM,NB)
 VPUT (SEARCH,HAVECMD,WKDSN,WKCMD) PROFILE
)END
