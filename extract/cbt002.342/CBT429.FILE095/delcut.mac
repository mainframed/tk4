 ISREDIT MACRO (PARM) NOPROCESS
 ISPEXEC CONTROL ERRORS RETURN
 IF &PARM ^= &Z THEN +
   DO
     IF &PARM = ? THEN +
       DO
         ISPEXEC DISPLAY PANEL(DELCUT)
         EXIT
       END
     ELSE +
        IF &DATATYPE(&PARM) = NUM THEN +
          DO
           SET LEVEL = &PARM
           END
        ELSE +
          DO
           SET ZEDSMSG = &STR(INVALID ARGUMENT)
             SET ZEDLMSG = &STR(VALID ARGUMENTS ARE A "?" OR A LEVEL +
                               NUMBER)
           ISPEXEC SETMSG MSG(ISRZ001)
          EXIT
        END
  END
ELSE +
 SET LEVEL = 1
ISPEXEC VGET (RCUTLM&LEVEL,RCUTLB&LEVEL,RCUTRB&LEVEL) PROFILE
IF &LASTCC > 0 THEN +
  DO
           SET ZEDSMSG = &STR(LEVEL DOES NOT EXIST)
             SET ZEDLMSG = &STR(NO CUT LINES FOR LEVEL &LEVEL EXIST)
           ISPEXEC SETMSG MSG(ISRZ001)
           EXIT CODE(12)
        END
SET LINES = &&RCUTLM&LEVEL
SET LINESTOPASTE = &LINES
 DO WHILE &INDEX < &LINESTOPASTE
 ISPEXEC VERASE RCUT&LEVEL&EVAL(&INDEX+1) PROFILE
  ISPEXEC VERASE (RCUTLM&LEVEL,RCUTLB&LEVEL,RCUTRB&LEVEL) PROFILE
   SET INDEX  = &INDEX + 1
 END
           SET ZEDSMSG = &STR(CUT LEVEL &LEVEL DELETED)
             SET ZEDLMSG = &STR(&INDEX LINES FOR LEVEL &LEVEL DELETED)
           ISPEXEC SETMSG MSG(ISRZ001)
           EXIT CODE(1)
END
