ISREDIT MACRO (HELP) NOPROCESS
 IF &HELP = ? THEN DO
 ISPEXEC DISPLAY PANEL(STAMP)
 EXIT
 END
SET   L1 = &STR(                    /* ANOTHER FINE EDIT MACRO */  )
SET   L2 = &STR(                        /* BROUGHT TO YOU */       )
SET   L3 = &STR(                   /* BY YOUR TECHNICAL SUPPORT */ )
SET   L4 = &STR(                             /* STAFF */           )
  ISREDIT PROCESS DEST
  IF &LASTCC = 0 THEN DO
  ISREDIT LOCATE .ZDEST
  IF &LASTCC = 8 THEN GOTO EMPTY
       ISREDIT MASKLINE = "&L1"
       ISREDIT LINE_AFTER .ZDEST = MASKLINE

       ISREDIT (LINEPTR) = LINENUM .ZDEST
       SET &LINEPTR = &LINEPTR + 1
       ISREDIT MASKLINE = "&L2"
       ISREDIT LINE_AFTER &LINEPTR = MASKLINE

       ISREDIT (LINEPTR) = LINENUM .ZDEST
       SET &LINEPTR = &LINEPTR + 2
       ISREDIT MASKLINE = "&L3"
       ISREDIT LINE_AFTER &LINEPTR = MASKLINE

       ISREDIT (LINEPTR) = LINENUM .ZDEST
       SET &LINEPTR = &LINEPTR + 3
       ISREDIT MASKLINE = "&L4"
       ISREDIT LINE_AFTER &LINEPTR = MASKLINE
       GOTO ERROROUT
    END
  ELSE DO
EMPTY: +
       ISREDIT MASKLINE = "&L1"
       ISREDIT LINE_AFTER 0 = MASKLINE

       ISREDIT (LINEPTR) = LINENUM .ZCSR
       SET &LINEPTR = .ZLAST
       ISREDIT MASKLINE = "&L2"
       ISREDIT LINE_AFTER 1 = MASKLINE

       ISREDIT (LINEPTR) = LINENUM .ZCSR
       SET &LINEPTR = .ZLAST
       ISREDIT MASKLINE = "&L3"
       ISREDIT LINE_AFTER 2 = MASKLINE

       ISREDIT (LINEPTR) = LINENUM .ZCSR
       SET &LINEPTR = .ZLAST
       ISREDIT MASKLINE = "&L4"
       ISREDIT LINE_AFTER 3 = MASKLINE
ERROROUT:+
       ISREDIT MASKLINE = ' '
      SET RC = 0
  END
  END
  EXIT CODE(&RC)
