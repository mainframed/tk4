//PDSXREF    PROC   SO=1,LIB='SYS2.TLINKLIB',V1=
//*
//* THIS PROCEDURE WILL READ A PDS AND PRODUCE TWO REPORTS
//*
//*  1. LIST ALL CSECT NAMES WITHIN EACH MEMBER
//*
//*  2. LIST ALL MEMBERS WHICH REFERENCE EACH CSECT NAME
//*
//PDSXREF    EXEC   PGM=CBT842,REGION=384K
//PDSLIB     DD     DSN=&LIB,DISP=SHR,UNIT=SYSALLDA,VOL=SER=&V1
//PDSMEM     DD     DSN=&LIB,DISP=SHR,UNIT=SYSALLDA,VOL=SER=&V1
//NAMEIN     DD     UNIT=SYSDA,SPACE=(CYL,(10,1)),DCB=BLKSIZE=1600
//NAMEOUT    DD     UNIT=SYSDA,SPACE=(CYL,(10,1)),DCB=BLKSIZE=1600
//SORTLIB    DD     DSN=SYS1.SORTLIB,DISP=SHR
//SYSOUT     DD     SYSOUT=&SO
//LSTMEMBR   DD     SYSOUT=&SO
//LSTCSECT   DD     SYSOUT=&SO
//NAMEWK01   DD     UNIT=SYSDA,SPACE=(CYL,(10,10))
//NAMEWK01   DD     UNIT=SYSDA,SPACE=(CYL,(10,10))
//NAMEWK02   DD     UNIT=SYSDA,SPACE=(CYL,(10,10))
//NAMEWK03   DD     UNIT=SYSDA,SPACE=(CYL,(10,10))
//NAMEWK04   DD     UNIT=SYSDA,SPACE=(CYL,(10,10))
//NAMEWK05   DD     UNIT=SYSDA,SPACE=(CYL,(10,10))
//NAMEWK06   DD     UNIT=SYSDA,SPACE=(CYL,(10,10))
