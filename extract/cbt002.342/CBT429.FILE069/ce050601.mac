*
* CODING EXAMPLE 5.6.1
*
*
//jobname    JOB   ---
//STEP1      EXEC ASMAC
//C.SYSPUNCH DD   DSN=USER.OBJLIB(SUBRTN),DISP=SHR
//C.SYSIN DD *
*
*
SUBRTN	 CSECT
	 INITL 3,EQU=R
	 ...
	 RCNTL RC=0
	 END
//*
//*
//STEP2      EXEC ASMACLG
//C.SYSIN    DD   *
*
*
MAIN	 CSECT
	 INITL 3,EQU=R
	 ...
	 CALL  SUBRTN
	 ...
	 RCNTL RC=0
	 END
//*
//*
//L.OBJLIB   DD   DSN=USER.OBJLIB,DISP=SHR
//L.SYSIN    DD   *
  INCLUDE OBJLIB(SUBRTN)
//
