++USERMOD (RTU05A1) /* TSU IKJPARS REPLACEMENT */.
++VER (Z038) FMID(EBB1102) /*
  THIS USER MOD REPLACES THE IBM LOAD MODULE IKJPARS WITH THE
  TSU LOAD MODULE CLUTSPAR, TO SUPPORT A FRIENDLIER TSO ENVIRONMENT
  BY IMPROVED ERROR DETECTION, REPORTING AND CORRECTION.
      NOTE: UCLIN IS REQUIRED FOR THIS USERMOD TO DELETE THE
  LOAD MODULES IKJPARS AND IKJPARS2 FRON THE CDS. MANUAL ACTION IS
  REQUIRED TO RENAME THE IBM MODULES TO OKJPARS AND OKJPARS2. */.
++MOD (CLUTSPAR) DISTLIB(RICEDLIB) LEPARM(RENT,REUS)
                LKLIB(RICELOAD).
++JCLIN.
//RTU05A1 JOB (),'JCLIN FOR RTU05A1'
//OKJPARS EXEC LINKS,NAME=LINKLIB,PARM='RENT,REUS'
//SYSLIN DD *
 INCLUDE AOST4(IKJEFP00,IKJEFP03,IKJEFP10,IKJEFP20)
 ENTRY IKJPARS
 NAME OKJPARS(R)
 INCLUDE AOST4(IKJEFP60)
 ENTRY IKJPARS2
 NAME OKJPARS2(R)
/*
//CLUTSPAR EXEC LINKS,NAME=LPALIB,PARM='RENT,REUS,NCAL'
//SYSLIN DD *
 INCLUDE RICEDLIB(CLUTSPAR)
 ALIAS IKJPARS
 ENTRY CLUZPAR
 NAME CLUTSPAR(R)
/*
