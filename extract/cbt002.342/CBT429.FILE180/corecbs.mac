         EJECT ,
         MACRO
&N       CBSTART
         GBLC  &FIRST,&PREV,&CURR,&NEXT
&FIRST   SETC  '&SYSNDX'
&CURR    SETC  '&FIRST.E'
&NEXT    SETC  '&FIRST'
&N       EQU   *
         MEND
         SPACE 3
         MACRO
         CBSEG &OFF=,&LEN=,&NAME='CTRL BLK'
         GBLC  &FIRST,&PREV,&CURR,&NEXT
         GBLA  &SEGOFF,&HEX
         LCLC  &SEGSTR
         AIF   (&FIRST EQ &NEXT).NF    SKIP IF FIRST SEG
         DC    AL2(0)                  END PREVIOUS SEGMENT
.NF      ANOP
&PREV    SETC  '&CURR'
&CURR    SETC  '&NEXT'
&NEXT    SETC  '&SYSNDX'
CB&CURR  DC    AL4(CB&PREV,CB&NEXT)    PREV AND NEXT SEG ADDRS
         CBCONV &OFF,OFF
&SEGOFF  SETA  &HEX
&SEGSTR  SETC  '&HEX' GET AROUND SIGN PROBLEMS WITH SETC
         AIF   (&HEX GE 0).NOSIGN
&SEGSTR  SETC '-'.'&HEX'
.NOSIGN  ANOP
         CBCONV &LEN,LEN
         DC    FL2'&SEGSTR,&HEX'       SEGMENT DATA OFFSET AND LENGTH
         DC    CL8&NAME                SEGMENT NAME
.MEND    MEND
         SPACE 3
         MACRO
         CBITEM &TYPE,&ROW,&COL,&OFF,&LEN,&DIGIT=0
         GBLA  &SEGOFF,&HEX
         LCLA  &IOFF,&ILEN
         AIF   ('&TYPE' NE 'LABEL').NOLABEL SKIP IF NOT LABEL TYPE
         DC    AL2(8)                  LABEL TYPE
&ILEN    SETA  K'&OFF-3                CALCULATE LENGTH
         DC    AL2(&ILEN)              LENGTH
         DC    AL2(&ROW*79-79+&COL-1)  SCREEN OFFSET
         DC    C&OFF                   EBCDIC CONSTANT
         MEXIT
.NOLABEL CBCONV &OFF,OFFSET            CONVERT DATA OFFSET FROM HEX
&IOFF    SETA  &HEX-&SEGOFF            TO DECIMAL
         AIF   (&HEX LT &SEGOFF).OFFLOW SEE IF TOO LOW FOR SEGMENT
         AIF   (&HEX GT (&SEGOFF+255)).OFFHI OR IF TOO HIGH
         CBCONV &LEN,LENGTH            JUST CHECK FOR VALID HEX
&ILEN    SETA  &HEX-1                  CALCULATE LENGTH
         AIF   ('&TYPE' NE 'HEX').NOHEX SKIP IF NOT HEX TYPE
         AIF   ('&DIGIT' NE '0' AND '&DIGIT' NE '1').BADDIG
         DC    AL2(4)                  HEX TYPE
         DC    AL1(&DIGIT,&ILEN)       DIGIT OFFSET AND LENGTH
         DC    AL2(&ROW*79-79+&COL-1)  SCREEN OFFSET
         DC    AL2(&IOFF)              DATA OFFSET
         MEXIT
.NOHEX   AIF   ('&TYPE' NE 'EBCD').NOTYPE SKIP IF NOT EBCDIC TYPE
         DC    AL2(12)                 EBCDIC DATA TYPE
         DC    AL2(&ILEN)              LENGTH
         DC    AL2(&ROW*79-79+&COL-1)  SCREEN OFFSET
         DC    AL2(&IOFF)              DATA OFFSET
         MEXIT
.NOTYPE  MNOTE 8,'TYPE FIELD MISSING OR INVALID'
         MEXIT
.BADDIG  MNOTE 8,'DIGIT FIELD INVALID'
         MEXIT
.OFFLOW  MNOTE 8,'OFFSET SPECIFIED TOO LOW FOR SEGMENT'
         MEXIT
.OFFHI   MNOTE 8,'OFFSET SPECIFIED TOO HIGH FOR SEGMENT'
         MEND
         SPACE 3
         MACRO
         CBEND
         GBLC  &FIRST,&PREV,&CURR,&NEXT
         DC    AL2(0)                  END OF SEGMENT INDICATOR
CB&NEXT  EQU   CB&FIRST                WRAP AROUND SEG CHAIN
CB&FIRST.E EQU CB&CURR                 BACKWARDS WRAP
         MEND
         SPACE 3
         MACRO
         CBCONV &DATA,&NAME
         GBLA  &HEX
         LCLC  &CH
         LCLA  &N
&HEX     SETA  0
         AIF   ('&DATA'(1,1) NE '-').LOOP
&N       SETA  1
.LOOP    AIF   (&N GE K'&DATA).DONE
&N       SETA  &N+1
&CH      SETC  '&DATA'(&N,1)
         AIF   ('&CH' GE '0').ZEROTO9
         AIF   ('&CH' EQ 'A').AX
         AIF   ('&CH' EQ 'B').BX
         AIF   ('&CH' EQ 'C').CX
         AIF   ('&CH' EQ 'D').DX
         AIF   ('&CH' EQ 'E').EX
         AIF   ('&CH' EQ 'F').FX
         MNOTE 12,'&DATA FIELD NOT A VALID HEX NUMBER'
&HEX     SETA  0
         MEXIT
.ZEROTO9 ANOP
&HEX     SETA  &HEX*16+&CH
         AGO   .LOOP
.AX      ANOP
&HEX     SETA  &HEX*16+X'A'
         AGO   .LOOP
.BX      ANOP
&HEX     SETA  &HEX*16+X'B'
         AGO   .LOOP
.CX      ANOP
&HEX     SETA  &HEX*16+X'C'
         AGO   .LOOP
.DX      ANOP
&HEX     SETA  &HEX*16+X'D'
         AGO   .LOOP
.EX      ANOP
&HEX     SETA  &HEX*16+X'E'
         AGO   .LOOP
.FX      ANOP
&HEX     SETA  &HEX*16+X'F'
         AGO   .LOOP
.DONE    AIF ('&DATA'(1,1) NE '-').MEND
&HEX     SETA  0-&HEX
.MEND    MEND
