//ROLANDSF JOB (ACCT#),FTP3,
// NOTIFY=&SYSUID,REGION=4M,
// CLASS=A,MSGCLASS=X
//A   PROC M=
//ASM EXEC PGM=ASMA90,REGION=7000K,
//          PARM='OBJECT,XREF(SHORT),LINECOUNT(85),TEST'
//SYSUT1   DD  UNIT=VIO,SPACE=(CYL,1)
//SYSUT2   DD  UNIT=VIO,SPACE=(CYL,1)
//SYSUT3   DD  UNIT=VIO,SPACE=(CYL,1)
//SYSLIB   DD  DISP=SHR,DSN=ROLANDS.DCMMY.MACLIB
//         DD  DISP=SHR,DSN=SYS1.MODGEN
//         DD  DISP=SHR,DSN=SYS1.MACLIB
//SYSPRINT DD  SYSOUT=*
//SYSPUNCH DD  DUMMY
//SYSLIN   DD  DSN=&&O,DISP=(,PASS),UNIT=VIO,SPACE=(CYL,1),
//             DCB=(RECFM=FB,LRECL=80,BLKSIZE=3200)
//SYSIN    DD  DISP=SHR,DSN=ROLANDS.DCMMY.SRC(&M)
//*
//LKED    EXEC PGM=IEWL,COND=(4,LT,ASM),REGION=4000K,
//        PARM='XREF,LET,NCAL,LIST,RENT,REFR,REUS,TEST'
//SYSLMOD  DD  DSN=ROLANDS.LOAD(&M),DISP=SHR
//SYSUT1   DD  DSN=&&SYSUT1,UNIT=VIO,SPACE=(CYL,1)
//SYSPRINT DD  SYSOUT=*,DCB=(RECFM=FB,LRECL=121,BLKSIZE=1210)
//SYSLIN   DD  DSN=&&O,DISP=(OLD,DELETE)
//    PEND
//MAIN   EXEC A,M=DCMMAIN
//INIT   EXEC A,M=DCMINIT
//STATS  EXEC A,M=DCMSTATS
//CMBG   EXEC A,M=DCMCMBG
//INPUT  EXEC A,M=DCMINPUT
//DSPLY  EXEC A,M=DCMDSPLY
//SCRN   EXEC A,M=DCMSCRN
//IOERR  EXEC A,M=DCMIOERR
//CLEAR  EXEC A,M=DCMCLEAR
//CONV   EXEC A,M=DCMCONVD
//REPLAY EXEC A,M=REPLAYP
