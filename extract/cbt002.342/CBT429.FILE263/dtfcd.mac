         MACRO
&NAME    DTFCD &DEVADDR=,&IOAREA1=,&IOAREA2=,&BLKSIZE=,&CONTROL=,      X
               &CTLCHR=,&SSELECT=,&CRDERR=,&DEVICE=,&EOFADDR=,&IOREG=, X
               &WORKA=,&MODNAME=,&QUBLKSZ=,&RDONLY=,&SEPASMB=,         X
               &RECFORM=,&RECSIZE=,&TYPEFLE=
         DTFALL
&NAME    DS    0H
         MNOTE 0,'DTFCD NOT SUPPORTED BY O.S.'
         SPACE 2
*        BASIC CODE NECESSARY IS
*        DCB   DSORG=PS,.....      FOLLOWED BY
         SPACE 1
         AIF   (T'&DEVADDR EQ 'O').STEP1
*        DEVADDR=   ...,VOLUME=    IN THE DD STATEMENT
.STEP1   AIF   (T'&IOAREA1 EQ 'O').STEP2
*        IOAREA1=   ...,BUFNO=1,MACRF=(..M..)
.STEP2   AIF   (T'&IOAREA2 EQU 'O').STEP3
*        IOAREA2=   ...,BUFNO=2    OR MORE
.STEP3   AIF   (T'&BLKSIZE EQ 'O').STEP4
*        BLKSIZE=   ...,BLKSIZE=NNN
.STEP4   AIF   (T'&CONTROL EQ 'O').STEP5
*        CONTROL=   ...,MACRF=(..C..)
.STEP5   AIF   (T'&CTLCHR EQ 'O').STEP6
*        CTLCHR=    ...,RECFM=(...M/A)
.STEP6   AIF   (T'&SSELECT EQ 'O').STEP7
*        SSELECT=   ...,DEVD=(STACK=1/2)
.STEP7   AIF   (T'&CRDERR EQ 'O').STEP8
*        CRDERR=    NOT APPLICABLE
.STEP8   AIF   (T'&DEVICE EQ 'O').STEP9
*        DEVICE=    ...,UNIT=      IN THE DD STATEMENT
.STEP9   AIF   (T'&EOFADDR EQ 'O').STEP10
*        EOFADDR=   ...,EODAD=XXXXXXXX
.STEP10  AIF   (T'&IOREG EQ 'O').STEP11
*        IOREG=     ...,MACRF=(...L..)
.STEP11  AIF   (T'&WORKA EQ 'O').STEP12
*        WORKA=     ...,MACRF=(..M..)
.STEP12  AIF   (T'&MODNAME EQ 'O').STEP13
*        MODNAME=   NOT APPLICABLE
.STEP13  AIF   (T'&QUBLKSZ EQ 'O').STEP14
*        QUBLKSZ=   NOT APPLICABLE
.STEP14  AIF   (T'&RDONLY EQ 'O').STEP15
*        RDONLY=    NOT APPLICABLE
.STEP15  AIF   (T'&SEPASMB EQ 'O').STEP16
*        SEPASMB=   USER MUST CODE THE DCB AS A CSECT IF REQUIRED
.STEP16  AIF   (T'&RECFORM EQ 'O').STEP17
*        RECFORM=   ...,RECFM=(...)
.STEP17  AIF   (T'&RECSIZE EQ 'O').STEP18
*        RECSIZE=   ...,LRECL=NN
.STEP18  AIF   (T'&TYPEFLE EQ 'O').STEP19
*        TYPEFLE=   ...,DEVD=RD/PC,MACRF=(G/P..)
*                   NB THIS ELIMINATES DEVICE INDEPENDENCE .. CODE
*                   DEVD=DA IF DASD SYSIN & SYSOUT QUEUES TO BE USED.
.STEP19  ANOP
         SPACE 2
         MEND
