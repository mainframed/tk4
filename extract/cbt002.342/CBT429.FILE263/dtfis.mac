         MACRO
&NAME    DTFIS &DSKXTNT=,&IOROUT=,&KEYLEN=,&NRECDS=,&RECFORM=,         X
               &RECSIZE=,&CYLOFL=,&DEVICE=,&ERREXT=,&MSTIND=,          X
               &INDAREA=,&INDSIZE=,&INDSKIP=,&IOAREA2,&WORKL=,&WORKS=, X
               &WORKR=,&IOAREAL=,&IOAREAR=,&IOAREAS=,&IOREG=,&IOSIZE=, X
               &HINDEX=,&KEYARG=,&KEYLOC=,&TYPEFLE=,&VERIFY=,          X
               &MODNAME=,&RDONLY=,&SEPASMB=
         DTFALL
&NAME    DS    0H
         MNOTE 0,'DTFIS NOT SUPPORTED BY O.S.'
         SPACE 2
*        BASIC CODE NECESSARY IS
*        DCB   DSORG=IS,.....      FOLLOWED BY
         SPACE 1
         AIF   (T'&DSKXTNT EQ 'O').STEP1
*        DSKXTNT=   ...,SPACE=     IN THE DD STATEMENT
.STEP1   AIF   (T'&IOROUT EQ 'O').STEP2
*        IOROUT=    ...,MACRF=     VARIOUS OPTIONS
*                   ALSO USE OPEN MACRO OPTIONS
.STEP2   AIF   (T'&KEYLEN EQ 'O').STEP3
*        KEYLEN=    ...,KEYLEN=NN
.STEP3   AIF   (T'&NRECDS EQ 'O').STEP4
*        NRECDS=    ...,BLKSIZE=NNN
.STEP4   AIF   (T'&RECFORM EQ 'O').STEP5
*        RECFORM=   ...,RECFM=XXX
.STEP5   AIF   (T'&RECSIZE EQ 'O').STEP6
*        RECSIZE=   ...,LRECL=NNN
.STEP6   AIF   (T'&CYLOFL EQ 'O').STEP7
*        CYLOFL=    ...,CYLOFL=NN
.STEP7   AIF   (T'&DEVICE EQ 'O').STEP8
*        DEVICE=    ...,UNIT=      IN THE DD STATEMENT
.STEP8   AIF   (T'&ERREXT EQ 'O').STEP9
*        ERREXT=    ...,SYNAD=XXXXXXXX
.STEP9   AIF   (T'&MSTIND EQ 'O').STEP10
*        MSTIND=    ...,OPTCD=M,NTM=NN
.STEP10  AIF   (T'&INDAREA EQ 'O').STEP11
*        INDAREA=   ...,MSHI=XXXXXXXX
.STEP11  AIF   (T'&INDSIZE EQ 'O').STEP12
*        INDSIZE=   ...,SMSI=NNN
.STEP12  AIF   (T'&INDSKIP EQ 'O').STEP13
*        INDSKIP=   NO EQUIVALENT
.STEP13  AIF   (T'&IOAREA2 EQ 'O'E.STEP14
*        IOAREA2=   ...,BUFNO=(BLOCKS PER TRACK + 1)
.STEP14  AIF   (T'&WORKL NE 'O').STEP15
         AIF   (T'&WORKS NE 'O').STEP15
         AIF   (T'&WORKR EQ 'O').STEP16
.STEP15  ANOP
*        WORKL=  *
*        WORKS=  * ...,MACRF=(..M..)
*        WORKR=  *
.STEP16  AIF   (T'&IOAREAL NE 'O').STEP17
         AIF   (T'&IOAREAR NE 'O').STEP17
         AIF   (T'&IOAREAS NE 'O').STEP18
.STEP17  ANOP
*        IOAREAL=*
*        IOAREAR= * ...,BUFNO=1,MACRF=(..L..)
*        IOAREAS=*
.STEP18  AIF   (T'&IOREG EQ 'O').STEP19
*        IOREG=     ...,MACRF=(..L..)
.STEP19  AIF   (T'&IOSIZE EQ 'O').STEP20
*        IOSIZE=    ...,MSWA=XXXXXXXX,SMSW=NNN
.STEP20  AIF   (T'&HINDEX EQ 'O').STEP21
*        HINDEX=    ...,UNIT=      IN THE DD STATEMENT
.STEP21  AIF   (T'&KEYARG EQ 'O').STEP22
*        KEYARG=    SEQUENTIAL RETRIEVAL...SPECIFY IN SETL MACRO
*                   RANDOM RETRIEVAL...SPECIFY IN READ/WRITE MACROS
.STEP22  AIF   (T'&KEYLOC EQ 'O').STEP23
*        KEYLOC=    ...,BKP=N
.STEP23  AIF   (T'&TYPEFLE EQ 'O').STEP24
*        TYPEFLE=   ...,MACRF=     VARIOUS OPTIONS
*                   ALSO USE OPEN MACRO OPTIONS
.STEP24  AIF   (T'&VERIFY EQ 'O').STEP25
*        VERIFY=    ...,OPTCD=W    (NOT VALID FOR RANDOM ADDRESSING)
.STEP25  AIF   (T'&MODNAME EQ 'O').STEP26
*        MODNAME=   NOT APPLICABLE
.STEP26  AIF   (T'&RDONLY EQ 'O').STEP27
*        RDONLY=    NOT APPLICABLE
.STEP27  AIF   (T'&SEPASMB EQ 'O').STEP28
*        SEPASMB=   USER MUST CODE THE DCB AS A CSECT IF REQUIRED
.STEP28  ANOP
         SPACE 2
         MEND
