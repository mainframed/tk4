)ATTR DEFAULT(%+_)
       %   TYPE(TEXT)   INTENS(HIGH)
       +   TYPE(TEXT)   INTENS(LOW)
       _   TYPE(INPUT)  INTENS(HIGH) CAPS(ON) JUST(LEFT)
       ^   TYPE(INPUT)  INTENS(NON)
       $   TYPE(OUTPUT) INTENS(HIGH) COLOR(YELLOW)
       #   TYPE(OUTPUT) INTENS(LOW) CAPS(OFF)
       ~   TYPE(TEXT)   INTENS(LOW) HILITE(USCORE)
)BODY
%--------------------------- $REASID   +$REASVR % ----------------------
%COMMAND ===>_ZZCMD2
+
+
+       Specify SOURCE LIBRARY to save the project %CSECT   +
+
+
+
+SOURCE LIBRARY:
+   PROJECT%===>_PRJ2    +
+   GROUP  %===>_LIB2    +
+   TYPE   %===>_TYP2    +
+   MEMBER %===>_MEM2    +       (~* - to view member list+)
+
+OTHER PARTITIONED DATA SET:
+   DATA SET NAME %===>_DSN2
+   VOLUME SERIAL %===>_VOL2  +  (~If not cataloged+)
+
+DATA SET PASSWORD%===>^PSWD2   +(~If password protected+)
+
+REPLACE MEMBER   %===>_YN2+     (YES or NO)
+
)INIT
  .HELP = REAPRS00
  &ZZCMD2 = ' '

  VGET (PRJ2 LIB2 TYP2 YN2) PROFILE
  IF (&YN2 = ' ')
     &YN2 = 'NO'
  IF (&DSN2 ^= ' ')
    IF (.CURSOR ^= 'VOL2')
      .CURSOR = DSN2

  &ZLMDATE = &ZDATE
  &ZLCDATE = &ZDATE
  VPUT (ZLMDATE ZLCDATE)

)PROC
 IF (&MEM2 ^= '*')
    VER (&MEM2 NAME)
 IF (&DSN2 = ' ' )
    VER (&PRJ2 NONBLANK)
    VER (&LIB2 NONBLANK)
    VER (&TYP2 NONBLANK)
    IF (&VOL2 ^= ' ')
       .MSG = ISRE094

 &Y   = TRUNC (&YN2,1)
 &YN2 = TRANS (&Y Y,YES, N,NO)
 VER (&YN2, LIST, YES,NO)

 VPUT (PRJ2 LIB2 TYP2 YN2) PROFILE

)END
