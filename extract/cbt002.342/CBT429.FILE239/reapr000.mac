)ATTR DEFAULT(%+_)
       %   TYPE(TEXT)   INTENS(HIGH)
       +   TYPE(TEXT)   INTENS(LOW)
       $   TYPE(OUTPUT) INTENS(HIGH) COLOR(YELLOW)
)BODY
%TUTORIAL ------------------ $REASID   +$REASVR % -------------------- %
%OPTION  ===>_ZCMD

%                     +-------------------------------+
                      ×  INTERACTIVE    DISASSEMBLER  ×
                      ×    PRIMARY    COMMANDS        ×
                      +-------------------------------+
+
 The following topics are presented in sequence, or may be selected by
number.  Individual commands may be selected by entering the command nam
+
  %0+- Primary commands (general information)
  %1+- Basic commands                 %USING      DROP       RETRANSLATE
  %2+- FIND/LOCATE commands           %FIND       LOCATE
  %3+- Data presentation commands     %LINESIZE   DSECT      OFFSET    P
  %4+- External data commands         %SAVE
  %5+- Print commands                 %PRINT
  %6+- Miscellaneous commands         %RESET      CANCEL     RECOVERY
                                      %AUTO       END        MARK

  The following topic will be presented only if selected by number:
  %7+- User DSECT preparation         %SET
  %8+- Debug commands                 %#SNAP      TRACE

)PROC
   &ZSEL = TRANS(&ZCMD
                0,&REAPAN05
                1,REAPR001
                2,REAPR002
                3,REAPR003
                4,REAPR004
                5,REAPR005
                6,REAPR006
                7,*REAPR007
                8,*REAPR008
                USING,       *REAPRU00
                DROP,        *REAPRD00
                RETRANSLATE, *REAPRR00
                FIND,        *REAPRF00
                LOCATE,      *REAPRL00
                LINESIZE,    *REAPRI00
                DSECT,       *REAPRE00
                OFFSET,      *REAPRO00
                PROFILE,     *REAPRW00
                SAVE,        *REAPRS00
                PRINT,       *REAPRP00
                RESET,       *REAPRZ00
                CANCEL,      *REAPRQ00
                RECOVERY,    *REAPRV00
                AUTO,        *REAPRY00
                END,         *REAPRJ00
                MARK,        *REAPRM00
                SET,         *REAPR@00
                #SNAP,       *REAPR#00
                TRACE,       *REAPRT00
                )
&ZUP   = REA00000
)END
