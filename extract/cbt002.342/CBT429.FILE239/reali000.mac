)ATTR DEFAULT(%+_)
       %   TYPE(TEXT)   INTENS(HIGH)
       +   TYPE(TEXT)   INTENS(LOW)
       $   TYPE(OUTPUT) INTENS(HIGH) COLOR(YELLOW)
)BODY
%TUTORIAL ------------------ $REASID   +$REASVR % -------------------- %
%OPTION  ===>_ZCMD

%                     +-------------------------------+
                      ×  INTERACTIVE    DISASSEMBLER  ×
                      ×        LINE   COMMANDS        ×
                      +-------------------------------+

+
   The following topics are presented in sequence, or may be selected by
   number.

  %0+ - Line commands - general information
  %U+ - USING       line command
  %D+ - DROP        line command
  %R+ - RETRANSLATE line command
  %T+ - Type redefinition line commands (X C F H Y B A S #)

)PROC
   &ZSEL = TRANS(&ZCMD
                0,REALI001
                U,REALIU00
                D,REALID00
                R,REALIR00
                T,REALIT00
                )
&D7    = 'D7'         /* For REALID00 "DROP" panel */
&ZUP   = REA00000
)END
