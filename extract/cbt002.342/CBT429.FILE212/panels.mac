%-\-\- CDCSA/CSC Main Menu -\-\-
%OPTION ===>_ZCMD
%
+    System: &SYSID                                +Userid: &ZUSER     %
+    Gregorian: &ZDATE     Julian: &ZJDATE         +Time:   &ZTIME     %
+
+
+ ^DYNAREA                                                           ^
+                                                                      +
+                                                                     +
+ Enter END command to terminate.
)INIT
 .HELP    = ISP00005     /* Help for this master MENU CBIPO        */
 &ZPRIM   = YES          /* This is a primary option MENU          */
 &LINLEN = 68            /* Length of dynamic area lines           */
 &UID  = &ZUSER          /* set UID to the userid                  */
 &UID1 = TRUNC(&UID,1)   /* get first   character  of userid       */
 &UID2 = TRUNC(&UID,2)   /* get first 2 characters of userid       */
 &UID3 = TRUNC(&UID,3)   /* get first 3 characters of userid       */
 &UID4 = TRUNC(&UID,4)   /* get first 4 characters of userid       */
 &UID5 = TRUNC(&UID,5)   /* get first 5 characters of userid       */
 &UID6 = TRUNC(&UID,6)   /* get first 6 characters of userid       */
 &UID7 = TRUNC(&UID,7)   /* get first 7 characters of userid       */
 /*********************************************************************/
 /*                                                                   */
 /* The following section of code determines the MENUGRP of the user  */
 /* based on the naming standards of the userid.  MENUGRP is used     */
 /* by all the selection panels of the CDCSA MENU system to determine */
 /* what menus and what options within a menu will be available to    */
 /* a user.                                                           */
 /* If userids are assigned that are not picked up by this code       */
 /* you may add the new userid type to a current MENUGRP if the new   */
 /* menugroup has the same requirements for options as a current      */
 /* menugroup.                                                        */
 /*                                                                   */
 /*********************************************************************/
 &MENUGRP = ''  /* initialize menu group                  */
 IF (&UID3 = 'MJL' , 'LAW' , 'FMM')
   &MENUGRP = 'DB2SYS'
 ELSE
   IF (&UID3 = 'BOR' , 'MAN' , 'PET' , 'DB2' , 'DPB' , 'PXS' , 'MXP')
     &MENUGRP = 'DB2SYS'
   ELSE
      IF (&UID3 = 'AXG')
        &MENUGRP = 'DB2PGM'
      ELSE
       IF (&UID7 = 'DMV0016')
         &MENUGRP = 'DB2MGR'
       ELSE
         IF (&UID3 = 'KET' , 'SUE')
           &MENUGRP = 'DB2MGR'
         ELSE
           IF (&UID4 = 'C828')
             &MENUGRP = 'DB2WEC'
           ELSE
             IF (&UID4 = 'DD20')
               &MENUGRP = 'DB2SYS'
             ELSE
               IF (&UID7 = 'ACSCFSG', 'ACSCPAB')
                 &MENUGRP = 'DB2SYS'
               ELSE
                 IF (&UID7 = 'DD20008')
                   &MENUGRP = 'DB2MGR'
 /* second time through                                      */
 IF (&PASSTHRU ^= &Z)
     .RESP = ENTER
     &ZCMD = &PASSTHRU
     &PASSTHRU = &Z
 /* first time through                                       */
 IF (&SYSID = &Z)
     .RESP = ENTER
     &PASSTHRU = &ZCMD
     &ZCMD = GETSYSID
   /* Normal primary option MENU initialization               */
 &ZHTOP = ISR00003    /* Tutorial table of contents           */
 &ZHINDEX = ISR91000  /* Tutorial Index - 1st page            */
 VPUT (ZHTOP,ZHINDEX,MENUGRP) PROFILE
 /*                                                                   */
 /*********************************************************************/
 /* To customize this menu or other top-level selection menu.         */
 /*  1. Change the options for the appropriate menugroup below.       */
 /*  2. Change the corresponding option in the PROC section           */
 /*                                                                   */
 /* LIBDEFS have been implemented for a number of products.  The      */
 /* products which have been Libdeffed are all initiated by a clist   */
 /* with the first three letters of INV...                            */
 /*                                                                   */
 /* The LIBDEFs can be found in the CLIST library.                    */
 /*                                                                   */
 /*                                                                   */
 /*********************************************************************/
 /*                                                                   */
 /* Set default options                                               */
 /*                                                                   */
  &MENU = '+
  $                                                                   +
  $       Contact your PDS coordinator                                +
  $                                                                   +
  $  X @EXIT      @- Terminate ISPF Using List/Log Defaults          '
 /*                                                                   */
 /* Set Options for Data Base Administrators                          */
 /*                                                                   */
 IF (&MENUGRP = 'DB2DBA')
   &MENU = '+
  $  1 @PDF       @- ISPF/Program Development Facility                +
  $  2 @DBA       @- CDCSA/CSC DB2 DBA Functions                      +
  $  3 @SDSF      @- System Display and Search Facility               +
  $  4 @SYSPGM    @- CDCSA/CSC DB2 System Programmer Functions        +
  $ AP @APPLIC    @- Application Tools Menu                           +
