%-\-\- Database Facilities -\-\-
%OPTION ===>_ZCMD
%                                                                      %
%   +System: &SYSID                                +Userid: &ZUSER     %
%    DB Subsystem ===>_Z   +                       +Time:   &ZTIME     %
+    Choose from list: &SSIDVER
+    or you may experience%ABENDS!
+
+ Enter END command to terminate.                                      +
)INIT
  .ZVARS = '(DSNEOV01)'
  .HELP    = ISP00005     /* Help for this master menu  CBIPO      */
  &ZPRIM   = NO           /* This is a primary option menu         */
  &SSIDVER = ''
  IF (&DSNEOV01 = &Z)
    &DSNEOV01 = 'NONE'
    &SSIDVER = ''
  IF (&SYSID = MVSP)
    &SSIDVER = 'D1DB,P1DB'
    IF (&SYSPROC = DB2DEVL)
      &DSNEOV01 = 'D1DB'
  IF (&SYSID = MVSP)
    &SSIDVER = 'D1DB,P1DB'
    IF (&SYSPROC = DB2PROD)
      &DSNEOV01 = 'P1DB'
  IF (&SYSID = MVSP)
    &SSIDVER = 'D1DB,P1DB'
    IF (&SYSPROC = DB2DWEC)
      &DSNEOV01 = 'D2DB'
  IF (&SYSID = MVSW)
    &SSIDVER = 'D2DB'
    &DSNEOV01 = 'D2DB'
  IF (&SYSID = MVSX)
    &SSIDVER = 'DB2M'
    &DSNEOV01 = 'DB2M'
  IF (&SYSID = DOFD)
    &SSIDVER = 'DDB2,DSN'
    &DSNEOV01 = 'DDB2'
  IF (&SYSID = DOFP)
    &SSIDVER = 'PDB2'
    &DSNEOV01 = 'PDB2'
  IF (&SYSID = DOFX)
    &SSIDVER = 'DB2X'
    &DSNEOV01 = 'DB2X'
)PROC
  IF (&SYSID = MVSP)
    Ver (&DSNEOV01,LIST,D1DB,P1DB)
  IF (&SYSID = MVSW)
    Ver (&DSNEOV01,LIST,D2DB)
  IF (&SYSID = MVSX)
    Ver (&DSNEOV01,LIST,DB2M)
  IF (&SYSID = DOFD)
    Ver (&DSNEOV01,LIST,DDB2,DSN)
  IF (&SYSID = DOFP)
    Ver (&DSNEOV01,LIST,PDB2)
  IF (&SYSID = DOFX)
    Ver (&DSNEOV01,LIST,DB2X)
  VPUT (DSNEOV01) PROFILE
)END
./ ADD NAME=IMPDBSH1 0112-92009-92227-1604-00036-00033-00000-KETCSC
)ATTR DEFAULT(%+_)
