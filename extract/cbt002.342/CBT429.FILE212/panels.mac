./ ADD NAME=XDBSPNL  0119-91353-93197-1050-00061-00117-00000-MANN
)ATTR DEFAULT(%+_)
       %   TYPE(TEXT) INTENS(HIGH)
       +   TYPE(TEXT) INTENS(LOW)
       _   TYPE(INPUT) INTENS(HIGH) CAPS(ON) JUST(LEFT)
)BODY EXPAND(\\)
%-\-\- Cross Data Base Facilities on &SYSID -\-\-
%OPTION  ===>_ZCMD
%
%   T +Time       %-+Time Tracking
%  BK +Backups    %-+Special GLOBAL and DLIB backups
%   M +Migrations %-+Also known as CLONING
%   I +Implement  %-+Implement maintenance to%RUN-TIME+libraries
%   B +Backout    %-+Backout maintenance implementation
%   Z +ZoneMaint  %-+Delete zone entries and related datasets
%   V +View       %-+View backups
%   S +IESERV     %-+Process PTFs/APARs from IESERV
%   D +Directories%-+Browse ONLINE Program Directories
%   E +EDIT       %-+EDIT XDBS.TSOISPF datasets
% IVP +IVPs       %-+EDIT Installation Verification Procedures
%   L +LOG        %-+Log your changes
%   C +Clean-Up   %-+Clean up data sets
%   O +Other      %-+Execute OTHER CLIST/REXX Procedure under XDBS
%
%   X +Exit       %-+Leave this Menu
%
% Press HELP for other options available from this menu.
%
)INIT
  .HELP = XSYS001H
  &ZPRIM = YES        /* ALWAYS A PRIMARY OPTION MENU      */
  &ZHTOP = ISR00003   /* TUTORIAL TABLE OF CONTENTS        */
  &ZHINDEX = ISR91000 /* TUTORIAL INDEX - 1ST PAGE         */
  IF (&SYSID = &Z)
      .RESP = ENTER
      &ZCMD = GETSYSID
)PROC
  &ZSEL = TRANS( TRUNC (&ZCMD,'.')
                T,'CMD(%TRCK001)'
                B,'PANEL(BACKOUT)'
                D,'CMD(%BR XDBS.PGMDIR)'
                E,'CMD(%XSYS34 XDBS.TSOISPF)'
                I,'PANEL(IMPLEMNT)'
                S,'PANEL(PTFS)'
                L,'PANEL(SYS@CHGS)'
                M,'PANEL(MIGRATE)'
                O,'CMD(%XSYSMISC)'
               BK,'PANEL(BACKUPS)'
                V,'PANEL(VIEWING)'
                X,'EXIT'
                Z,'PANEL(DELDBSP1)'
                C,'PANEL(CLNDBSP1)'
            /**** 3.4                   ***************/
               DS,'PGM(ISRUDL) PARM(ISRUDLP)'
                7,'PGM(ISPYXDR) NOCHECK'
                GETSYSID,'CMD(%GETSYSID)'
                *,'?' )
            /**** SDSF ********************************/
                IF (&ZCMD = 'SD')
                   &ZSEL = 'PGM(ISFISP) NOCHECK NEWAPPL(ISF)'
  &ZTRAIL = .TRAIL
)END
./ ADD NAME=XDBSUDLP 0100-93140-93140-0815-00065-00065-00000-KETCSC
%--------------------------- DATA SET LIST UTILITY ---------------------
%OPTION  ===>_ZCMD
%
%  blank+- Display data set list%*         %P+ - Print data set list
%  V    +- Display VTOC information only   %PV+- Print VTOC information
%
+Enter one or both of the parameters below:
+  DSNAME LEVEL %===>_ZDLDSNLV                                    +
+  VOLUME       %===>_ZDLPVL+
%
%
+  INITIAL DISPLAY VIEW    %===>_ZUPIV   +(VOLUME,SPACE,ATTRIB,TOTAL)
+  CONFIRM DELETE REQUEST  %===>_ZUPCD+   (YES or NO)
%
%*+The following line commands will be available when the list is displa
%
%B+- Browse data set     %C+- Catalog data set     %F+- Free unused spac
%E+- Edit data set       %U+- Uncatalog data set   %=+- Repeat last comm
%D+- Delete data set     %P+- Print data set
%R+- Rename data set     %X+- Print index listing
%I+- Data set information%M+- Display member list
%S+- Information (short) %Z+- Compress data set    %TSO cmd, CLIST or RE
)INIT
  .HELP = ISR34000
  IF (.MSG = &Z)
    .CURSOR = ZCMD
  &ZCMD = &ZUOPT
  &ZUPIV = TRANS (&ZUVIEW 0,VOLUME 1,SPACE 2,ATTRIB 3,TOTAL *,VOLUME)
  IF (&ZUCDR = &Z)
    &ZUCDR = Y
  &ZUPCD = TRANS (&ZUCDR Y,YES N,NO *,*)
  IF (&XDBSDR = 'SKIP')
    &XDBSDR = 'SHOW'
    &ZDLDSNLV = &OLDPFX
    .RESP = END
  IF (&XDBSDR = 'BYPASS')
    &XDBSDR = 'SKIP'
    &OLDPFX = &ZDLDSNLV
    &ZDLDSNLV = &XDBSPFX
    &ZDLPVL = ''
    .RESP = ENTER
)PROC
  IF (&ZCMD ^= ' ')
    VER (&ZCMD,LIST,P,V,PV)
  &ZUOPT = TRUNC (&ZCMD,2)
  IF (&ZUOPT = 'V ','PV')
    VER (&ZDLPVL,NONBLANK)
  IF (&ZUOPT = '  ','P ')
    IF (&ZDLPVL = ' ')
      VER (&ZDLDSNLV,NONBLANK,MSG=ISRU187)
  &ZUCDR = TRUNC (&ZUPCD,1)
  VER (&ZUCDR,NB,LIST,Y,N)
  &ZVIEW = TRUNC (&ZUPIV,1)
  VER (&ZVIEW,NB,LIST,V,S,A,T)
  &ZUPIV = TRANS (&ZVIEW V,VOLUME S,SPACE A,ATTRIB T,TOTAL *,*)
  &ZUVIEW = TRANS (&ZVIEW V,0 S,1 A,2 T,3 *,*)
/*                                                                   */
  VPUT (ZDLPVL) SHARED
/*                                                                   */
)END
