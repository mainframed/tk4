       +   TYPE(TEXT) INTENS(LOW) SKIP(ON)
       _   TYPE(INPUT) INTENS(HIGH) CAPS(ON) JUST(LEFT)
)BODY EXPAND(\\)
%-\-\- Print As You like It -\-\-
%COMMAND ===>_ZCMD
%
+  Specify options to print &DATASETE
%
+  Way             ==>_Z+     (1=Clist, 2=IEBGENER, 3=IEBPTPCH)
+  RECFM           ==>_Z  +   (IF 1,FB,FBA, 2 or 3,FB,VB,FBA,VBA,FBM,VBM
+  SYSOUT=         ==>_Z+    Shift spaces ==>_SS+  User line count=>_UCN
+  OUTPUT=  PAGEDEF==>_Z       +    CHARS=>_Z       +  FORMDEF=>_Z
+  Destination     ==>_DEST                  +
+  Copies          ==>_Z + (Default 1)
+
+  Press END to cancel.
+
+ JOB CARD INFO:
+   _JA
+   _JB
+   _JC
+   _JD
+
)INIT
  .ZVARS = '(WAY, RECFM, SYSOUT, PDEF, FONT, FDEF, CPY)'
  &WAY = '1'
  &UCNT = '60'
  &SS = '0'
  &CPY = ' 1'
  IF (&JA = &Z)
    &JA  = '//&ZUSER  JOB (ACCT,room),''ROOM## : programmer-name'','
    &JB  = '//           MSGCLASS=Z,CLASS=S,NOTIFY=&ZUSER'
    &JC  = '//*'
    &JD  = '//*'
)PROC
  Ver  (&WAY,Range,1,3)
  If (&WAY = 1)
    Ver  (&RECFM,List,FB,FBA)
    Ver  (&SS,RANGE,0,25)
  If (&WAY = 2)
    Ver  (&RECFM,List,FB,FBA,VB,VBA,FBM,VBM)
  If (&WAY = 3)
    Ver  (&RECFM,List,FB,FBA,VB,VBA,FBM,VBM)
    Ver  (&SS,RANGE,0,25)
  Ver  (&SYSOUT,NONBLANK)
  Ver  (&CPY,NUM)
  Ver  (&UCNT,RANGE,1,88)
  VPUT (JA JB JC JD WAY RECFM SYSOUT PDEF FONT FDEF DEST) Profile
  VPUT (CPY SS UCNT) Profile
)END
