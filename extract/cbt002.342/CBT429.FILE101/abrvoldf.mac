PROC 0 DEBUG
CONTROL NOLIST NOMSG NOFLUSH PROMPT
IF &DEBUG NE &STR() THEN +
  CONTROL LIST MSG NOFLUSH PROMPT CONLIST SYMLIST
SET &UID EQ &STR()
SET &BEG EQ 1
SET &RC EQ 4
SET &UIDTAB EQ &STR(+
CICSP  +
COMH   +
COMP   +
DBDP   +
HRNES  +
IMSP   +
IPO2   +
JESP   +
LYDELL +
MVSP   +
NCCF   +
QATST  +
SYSP   +
SYSPCOM+
SYS2   +
T00289 +
T02728 +
T02930 +
T18749 +
T23305 +
T26568 +
T27611 +
T32809 +
T40710 +
T40809 +
T40839 +
T53009 +
T53031 +
T53033 +
T53043 +
T53093 +
T60403 +
VS1P   +
TZZZZZZ)
DO WHILE &UID NE &STR(TZZZZZZ) AND &RC NE 0
  SET &UID EQ &SUBSTR(&BEG:&EVAL(&BEG + 6),&STR(&UIDTAB))
  IF &SYSUID EQ &SUBSTR(1:&EVAL(&LENGTH(&SYSUID)),&STR(&UID)) THEN +
    SET &RC EQ 0
  ELSE +
    SET &BEG = &BEG + 7
END
EXIT CODE(&RC)
END
