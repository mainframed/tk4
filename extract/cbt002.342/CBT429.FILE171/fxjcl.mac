//........ JOB 'ALLOCATE FX DATA SETS'
//* ----------------------------------------------------------------- *
//*                                                                   *
//*  Before running this job:                                         *
//*                                                                   *
//*    1.  Add a valid jobcard                                        *
//*    2.  Change the prefix on the CLIST, DOC, ISPPLIB, MACLIB,      *
//*        OBJECT, SOURCE, and LOADLIB DD's.                          *
//*    3.  Change the unit and volsers to suitable units at your      *
//*        installation on the CLIST, DOC, ISPPLIB, MACLIB, OBJECT,   *
//*        SOURCE, and LOADLIB DD's.                                  *
//*    4.  Change the UNIT parameter to a suitable tape unit on       *
//*        THE FILE2 DD.                                              *
//*    5.  The SPACE values are adequate for 3380 devices.            *
//*                                                                   *
//* ----------------------------------------------------------------- *
//ALLOC    EXEC PGM=IEBCOPY,REGION=4096K
//SYSPRINT DD SYSOUT=*
//CLIST    DD  DSN=prefix.FX.CLIST,DISP=(NEW,CATLG,DELETE),
//             UNIT=uuuu,VOL=SER=vvvvvv,
//             SPACE=(TRK,(5,5,5)),
//             DCB=(RECFM=FB,LRECL=80,BLKSIZE=3120)
//DOC      DD  DSN=prefix.FX.DOC,DISP=(NEW,CATLG,DELETE),
//             UNIT=uuuu,VOL=SER=vvvvvv,
//             SPACE=(TRK,(5,5,5)),
//             DCB=(RECFM=FBA,LRECL=80,BLKSIZE=3120)
//ISPPLIB  DD  DSN=prefix.FX.ISPPLIB,DISP=(NEW,CATLG,DELETE),
//             UNIT=uuuu,VOL=SER=vvvvvv,
//             SPACE=(TRK,(2,2,2)),
//             DCB=(RECFM=FB,LRECL=80,BLKSIZE=3120)
//MACLIB   DD  DSN=prefix.FX.MACLIB,DISP=(NEW,CATLG,DELETE),
//             UNIT=uuuu,VOL=SER=vvvvvv,
//             SPACE=(TRK,(15,5,10)),
//             DCB=(RECFM=FB,LRECL=80,BLKSIZE=3120)
//OBJECT   DD  DSN=prefix.FX.OBJECT,DISP=(NEW,CATLG,DELETE),
//             UNIT=uuuu,VOL=SER=vvvvvv,
//             SPACE=(TRK,(10,5,10)),
//             DCB=(RECFM=FB,LRECL=80,BLKSIZE=3120)
//SOURCE   DD  DSN=prefix.FX.SOURCE,DISP=(NEW,CATLG,DELETE),
//             UNIT=uuuu,VOL=SER=vvvvvv,
//             SPACE=(TRK,(45,5,15)),
//             DCB=(RECFM=FB,LRECL=80,BLKSIZE=3120)
//LOADLIB  DD  DSN=prefix.FX.LOADLIB,DISP=(NEW,CATLG,DELETE),
//             UNIT=uuuu,VOL=SER=vvvvvv,
//             SPACE=(TRK,(15,5,10)),
//             DCB=(RECFM=U,BLKSIZE=6144)
//FILE2    DD DSN=FX.CLIST,DISP=(NEW,KEEP),
//            UNIT=tape,VOL=(,RETAIN,SER=FX0000),
//            LABEL=(02,SL,EXPDT=98000)
//FILE3    DD DSN=FX.DOC,DISP=(NEW,KEEP),
//            UNIT=AFF=FILE2,VOL=(,RETAIN,SER=FX0000),
//            LABEL=(03,SL,EXPDT=98000)
//FILE4    DD DSN=FX.ISPPLIB,DISP=(NEW,KEEP),
//            UNIT=AFF=FILE2,VOL=(,RETAIN,SER=FX0000),
//            LABEL=(04,SL,EXPDT=98000)
//FILE5    DD DSN=FX.MACLIB,DISP=(NEW,KEEP),
//            UNIT=AFF=FILE2,VOL=(,RETAIN,SER=FX0000),
//            LABEL=(05,SL,EXPDT=98000)
//FILE6    DD DSN=FX.OBJECT,DISP=(NEW,KEEP),
//            UNIT=AFF=FILE2,VOL=(,RETAIN,SER=FX0000),
//            LABEL=(06,SL,EXPDT=98000)
//FILE7    DD DSN=FX.SOURCE,DISP=(NEW,KEEP),
//            UNIT=AFF=FILE2,VOL=(,RETAIN,SER=FX0000),
//            LABEL=(07,SL,EXPDT=98000)
//SYSIN    DD *
  COPY INDD=FILE2,OUTDD=CLIST
  COPY INDD=FILE3,OUTDD=DOC
  COPY INDD=FILE4,OUTDD=ISPPLIB
  COPY INDD=FILE5,OUTDD=MACLIB
  COPY INDD=FILE6,OUTDD=OBJECT
  COPY INDD=FILE7,OUTDD=SOURCE
