 LISTC CAT(CATALOG.RESCUE/XXXX) ALL
/*
//*
//SYSCATLG EXEC PGM=IEBDG,REGION=512K
//*-----------------------------------------------------------
//* DOC: THE PURPOSE OF THIS STEP  TO REPLACE MEMBER SYSCATLG
//*     IN DATA SET "SYS1.NUCLEUS" TO FACILITATE THE USE OF AN
//*    EXISTING ICF OR VSAM MASTER CATALOG WITH A NEWLY CREATE
//*     IPL VOLUME.
//*
//* NOTES: ACCEPTABLE CATALOG TYPE VALUES FOR "FILL=" ON THE
//*        "CATTYPE" FD STATEMENT ARE:
//*          X'00' AND X'40'  -  VSAM MASTER CATALOG
//*          X'F1'            -  ICF MASTER CATALOG
//*
//*        ACCEPTABLE IPO SUPPORTED DEVICE TYPES FOR "FILL="
//*        ON THE "DEV" FD STATEMENT ARE:
//*          X'0B' 3350          X'0E' 3380
//*          X'0C' 3375
//*        THE IPO SUPPLIED DEFAULT IS X'0E' FOR A 3380
//*
//*        EXAMPLES OF OTHER DEVICE TYPE VALUES FOR "FILL="
//*        ON THE "DEV" FD STATEMENT ARE:
//*          X'0D' 3330-1        X'06' 2305-1
//*          X'09' 3330          X'07' 2305-2
//*          X'0A' 3340          X'08' 2314
//*    THESE DEVICES ARE NOT SUPPORTED BY IPO FOR IPO VOLUMES;
//*    THESE EXAMPLES ARE INCLUDED FOR COMPLETENESS ONLY.
//*
//*-----------------------------------------------------------
//SYSPRINT DD  SYSOUT=*
//NUCLEUS  DD  DSN=SYSX.NUCLEUS(SYSCATLG),
//             UNIT=3380,VOL=SER=RESCUE,
//             DISP=OLD
//SYSIN    DD  *
  DSD OUTPUT=(NUCLEUS)
  FD NAME=VOL,
     LENGTH=06,
     STARTLOC=01,
     FILL=X'40',
     PICTURE=6,'RESCUE'  <-VERIFY/CHANGE VOL OF CAT
  FD NAME=CATTYPE,
     LENGTH=01,
     STARTLOC=07,
     FILL=X'F1'
  FD NAME=DEV,
     LENGTH=01,
     STARTLOC=08,
     FILL=X'0E'   <--VERIFY D/T
  FD NAME=CAT,
     LENGTH=44,
     STARTLOC=11,
     FILL=X'40',
     PICTURE=44,'CATALOG.RESCUE                              '
  FD NAME=FIL,
     LENGTH=26,
     STARTLOC=55,
     FILL=X'40'
 CREATE QUANTITY=1,FILL=X'00',NAME=(VOL,CATTYPE,DEV,CAT,FIL)
/*
//*
//PRINTPTR  EXEC PGM=IDCAMS
//*  PRINT THE CATALOG POINTER
//SYSPRINT DD SYSOUT=*
//CATPTR   DD DSN=SYSX.NUCLEUS(SYSCATLG),DISP=SHR,
//            UNIT=3380,VOL=SER=RESCUE
//SYSIN    DD *
  PRINT INFILE(CATPTR) DUMP
