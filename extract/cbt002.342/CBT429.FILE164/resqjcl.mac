//* JOB: RESCUE1
//* USE: CREATE A VOLUME VOLSER(RESCUE) THAT WILL CONTAIN
//*      ALL THE NECESSARY DATASETS FOR AN IPL
//*
//*     _STEPS_   __________ FUNCTION _____________________
//*     DEFMCAT   DEFINE A MASTER CATALOG
//*     DEFVSAM   DEFINE ALL NECESSARY VSAM FILES
//*     DEFNVSAM  DEFINE ALL NECESSARY NON-VSAM FILES
//*               AND IMPORT THE CATALOG AS A UCAT TO THE MCAT
//*----------------------------------------------------------*
//*
//*
//DEFMCAT  EXEC PGM=IDCAMS,REGION=2M,COND=(0,LT)
//SYSPRINT  DD   SYSOUT=*
//TARGET    DD   DISP=SHR,UNIT=3380,VOL=SER=RESCUE
//SYSIN     DD   *
 DELETE -
         SYS1.VVDS.VRESCUE -
           FILE(TARGET) -
           CLUSTER -
           PURGE -
       CATALOG(CATALOG.MASTER01/XXXX);

 SET MAXCC=0

 DEFINE -
  CLUSTER ( -
           NAME( SYS1.VVDS.VRESCUE ) -
           CYL(1,1) -
           VOL(RESCUE) -
           FILE(TARGET) -
           NONINDEXED -
           OWNER(RESCUE) -
         ) -
       CATALOG(CATALOG.MASTER01/XXXX);
