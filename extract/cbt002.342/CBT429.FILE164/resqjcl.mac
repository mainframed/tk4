       MASTERCATALOG ( -
                       NAME( CATALOG.RESCUE ) -
                       CYL(10 2) -
                       VOL(RESCUE) -
                       ATTEMPTS(2) -
                       BUFFERSPACE(8192) -
                       BUFND(10) -
                       BUFNI(10) -
                       FILE(TARGET) -
                       ICFCATALOG -
                       OWNER(RESCUE) -
                       STRNO(9) -
                      ) -
       CATALOG(CATALOG.MASTER01/XXXX);
