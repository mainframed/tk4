/*
//*
//
//RESCUE3 JOB ('COMB,NCC,2,100'),'RICK-STECKER',
//           NOTIFY=RGS,
//           REGION=3M,
//           CLASS=S,MSGCLASS=T,TYPRUN=HOLD
//*------------------------------------------------------*
//*                                                      *
//*  _STEPS_   __________ FUNCTION _____________________ *
//*  IFCDIP00   INITS THE LOGREC DATASET                 *
//*  COPYUADS   COPIES CURRENT UADS FOR SELECTED USERS   *
//*  RNUSRS@    RENAMES UADS MEMBERS FOR RESQTSO STEP    *
//*  RESQTSO    SYNC UADS, ADD PROC, CHANGE PSWDS        *
//*  RNUSRS     RENAMES UADS MEMBERS BACK TO NORMAL      *
//*  IDUMP00    INITS DUMP DATASETS                      *
//*  FORMAT     FORMATS SMF DATASETS                     *
//*  PROTECT    PROTECTS MASTER CATALOG                  *
//*  SYSCATLG   BUILD, RELPACE, SYSCATLG MEMBER IN NUC   *
//*  PRINTPTR   PRINT NEW SYSCATLG POINTER               *
//*------------------------------------------------------*
//*
//IFCDIP00 EXEC PGM=IFCDIP00
//SERERDS DD DISP=SHR,DSN=SYSX.LOGREC,
// UNIT=3380,VOL=SER=RESCUE
//*
//COPYUADS EXEC PGM=IEBCOPY
//*-----------------------------------------------------
//* COPY UADS ENTRIES FOR SELECTED TSO USERS
//*-----------------------------------------------------
//SYSPRINT DD SYSOUT=*
//SYSUT2   DD UNIT=SYSDA,SPACE=(20,10)
//SYSUT3   DD UNIT=SYSDA,SPACE=(20,10)
//FUADS    DD DSN=SYS1.UADS,DISP=SHR
//TUADS    DD DSN=SYSX.UADS,DISP=SHR,
//            UNIT=3380,VOL=SER=RESCUE
//SYSIN    DD *
 COPY I=((FUADS,R)),O=TUADS
 SELECT MEMBER=(AMK0,AXS0,LLA0,RGS0,RXR0)
/*
//*
//RNUSRS@  EXEC PGM=IDCAMS,REGION=2M
//*-----------------------------------------------------
//* RENAME USERS - THIS ALLOWS THE ACCOUNT COMMAND TO
//* DO ITS THING EVEN IF A USER IS LOGGED ON WHEN
//* THIS JOB IS RUN
//*-----------------------------------------------------
//SYSPRINT DD  SYSOUT=*
//UADS     DD DSN=SYSX.UADS,
//            DISP=SHR,UNIT=3380,VOL=SER=RESCUE
//SYSIN   DD   *
 ALTER SYSX.UADS(AMK0) -
       FILE(UADS) -
       NEWNAME(SYSX.UADS(@AMK0))
 ALTER SYSX.UADS(AXS0) -
       FILE(UADS) -
       NEWNAME(SYSX.UADS(@AXS0))
 ALTER SYSX.UADS(LLA0) -
       FILE(UADS) -
       NEWNAME(SYSX.UADS(@LLA0))
 ALTER SYSX.UADS(RGS0) -
       FILE(UADS) -
       NEWNAME(SYSX.UADS(@RGS0))
 ALTER SYSX.UADS(RXR0) -
       FILE(UADS) -
       NEWNAME(SYSX.UADS(@RXR0))
/*
//*
//RESQTSO  EXEC PGM=IKJEFT01,
//        DYNAMNBR=15,
//        REGION=2M
//*-----------------------------------------------------
//* SYNC UADS, ADD LOGON PROC, CHANGE PASSWORD
//*-----------------------------------------------------
//SYSUADS  DD DSN=SYSX.UADS,DISP=SHR,
//            VOL=SER=RESCUE,UNIT=3380
//SYSLBC   DD DSN=SYSX.BRODCAST,DISP=SHR,
//            VOL=SER=RESCUE,UNIT=3380
//SYSPRINT DD SYSOUT=*
//SYSTSPRT DD SYSOUT=*
//SYSTERM  DD SYSOUT=*
//SYSIN    DD DUMMY
//SYSTSIN  DD *
 ACCOUNT
 SYNC
 A (@AMK * *) DATA($RESCUE)
 C (@AMK *) DATA(RESCUE)
 A (@AXS * *) DATA($RESCUE)
 C (@AXS *) DATA(RESCUE)
 A (@LLA * *) DATA($RESCUE)
 C (@LLA *) DATA(RESCUE)
 A (@RGS * *) DATA($RESCUE)
 C (@RGS *) DATA(RESCUE)
 A (@RXR * *) DATA($RESCUE)
 C (@RXR *) DATA(RESCUE)
 END
