//RESCUE0 JOB ('COMB,NCC,2,100'),'RICK-STECKER',
//           NOTIFY=RGS,
//           REGION=1M,
//           CLASS=S,MSGCLASS=T,TYPRUN=HOLD
//*---------------------------------------------------------*
//* JOB: RESCUE0
//* USE: TO INITALIZE THE VOLUME=RESCUE ON UNIT=17A
//*
//*     _STEPS_   __________ FUNCTION _____________________
//*     EXPDISC   EXPORT-DISCONNECT OLD USERCATALOG
//*     VARYOFF   VARY DEVICE OFFLINE
//*     DEALLOC   BURP (EXCUSE ME)
//*     INITVOL   INITIALIZE DISK VOLUME USING ICKDSF
//*---------------------------------------------------------*
//EXPDISC  EXEC  PGM=IDCAMS,REGION=1M
//SYSPRINT  DD   SYSOUT=*
//SYSIN     DD   *
   EXPORT                                              -
         CATALOG.RESCUE  -
          DCON

   SET MAXCC=0
/*
//*
//VARYOFF EXEC PGM=COMMAND,
//   PARM='V 17A,OFFLINE'
//*
//* VARIES THE VOLUME OFFLINE
//*
//DEALLOC EXEC PGM=COMMAND,
//   PARM='S DEALLOC'
//*
//* BURPS THE SYSTEM (GENERATES AN INTERRUPT)
//*
//INITVOL  EXEC PGM=ICKDSF
//*
//* NOTE THE FOLLOWING CONCATENATION
//*
//IPLTEXT  DD  DISP=SHR,VOL=SER=XA8RES,UNIT=SYSDA,
//             DSN=SYS1.SAMPLIB(IPLRECS)
//         DD  DISP=SHR,VOL=SER=XA8RES,UNIT=SYSDA,
//             DSN=SYS1.SAMPLIB(IEAIPL00)
//SYSPRINT DD   SYSOUT=*
//SYSIN    DD   *
 INIT   UNIT(17A) -
        VOLID(RESCUE) -
        OWNERID(RESCUE) -
        VTOC(0,1,14) -
        PURGE -
        MAP -
      VERIFY(RESCUE) /* VOLUME MUST BE LABLED */ -
        NOVALIDATE -
        NOCHECK -
      IPLDD(IPLTEXT) -
      NOBOOTSTRAP /* ICKDSF WILL SUPPLY IT */
/*
//*
//VARYON   EXEC PGM=COMMAND,COND=(0,LT),
//       PARM='V 17A,ONLINE'
//* VARY 17A ONLINE
//MOUNT    EXEC PGM=COMMAND,COND=(0,LT),
//       PARM='M 17A,VOL=(SL,RESCUE),USE=PRIVATE'
//* MOUNT RESCUE ON 17A
//
//RESCUE1 JOB ('COMB,NCC,2,100'),'RICK-STECKER',
//           NOTIFY=RGS,
//           REGION=2M,
//           CLASS=S,MSGCLASS=T,TYPRUN=HOLD
