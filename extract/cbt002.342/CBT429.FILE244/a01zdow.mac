ISPEXEC VGET (ZDOW ZDOWDATE ZJDATE) ASIS
IF &LASTCC ^= 0 THEN SET ZDOWDATE=
IF &ZDOWDATE=&ZJDATE THEN EXIT
SET ZDOW=(&EVAL(&SUBSTR(1:2,&ZJDATE))*5-1)/4+&EVAL(&SUBSTR(4:6,&ZJDATE)
SET ZDOW=(&ZDOW-&ZDOW/7*7)*8+1
SET ZDOW=&SUBSTR(&ZDOW:&ZDOW+7,+
    &STR(SUNDAY  MONDAY  TUESDAY WEDNSDAYTHURSDAYFRIDAY  SATURDAY)
SET ZDOWDATE=&ZJDATE
ISPEXEC VPUT (ZDOW ZDOWDATE) PROFILE
