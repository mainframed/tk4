)ATTR DEFAULT(%+_)
)BODY
%------------------------  MANUAL MANAGEMENT FACILITY  -----------------
%COMMAND ===>_ZCMD
%
%ENTER MANUAL NUMBER ===>_MANUALNO    +
)INIT
  .HELP = LMMPH070
)PROC
  &MAN1TO11 = TRUNC (&MANUALNO,11)
  &MAN12 = .TRAIL
  &MAN1TO9 = TRUNC (&MAN1TO11,9)
  &MAN9TO11 = .TRAIL
  VER (&MAN1TO9,NB,PICT,'AC99-9999')
  VER (&MAN9TO11,PICT,'-9')
  VER (&MAN12,PICT,9)
)END
