PROC 1 ZCMD
CONTROL NOMSG NOFLUSH
ISPEXEC LIBDEF ISPTLIB DATASET ID('&SYSUID..ISPF.ISPPROF'  +
                                  'SYS1.DFQTLIB'  +
                                  'SYS1.DGTTLIB')  UNCOND
ISPEXEC LIBDEF ISPSLIB DATASET ID('SYS1.DGTSLIB')  UNCOND
ISPEXEC LIBDEF ISPPLIB DATASET ID('SYS1.DFQPLIB'  +
                                  'SYS1.DGTPLIB')  UNCOND
ISPEXEC LIBDEF ISPMLIB DATASET ID('SYS1.DFQMLIB'  +
                                  'SYS1.DGTMLIB')  UNCOND
ALTLIB ACT APPL(CLIST) DATASET('SYS1.DGTCLIB') UNCOND

ISPEXEC SELECT PGM(DGTFMD01) PARM(&ZCMD) NEWAPPL(DGT) NOCHECK PASSLIB

ALTLIB DEACT APPL(CLIST)
ISPEXEC LIBDEF ISPTLIB
ISPEXEC LIBDEF ISPSLIB
ISPEXEC LIBDEF ISPPLIB
ISPEXEC LIBDEF ISPMLIB
