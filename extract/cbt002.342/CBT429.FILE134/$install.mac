//$LCVINST JOB CLASS=E,MSGCLASS=S,NOTIFY=SFT24
//*
//* ASSEMBLE AND LINK $LISTC/$SPACE COMMAND PROCESSOR
//*
//$LCASM   PROC MEM=DUMMY
//ASM      EXEC PGM=ASMA90,PARM='OBJECT,NODECK'
//SYSIN     DD DSN=SFT.SFT24.$LISTCVN.ASM(&MEM),DISP=SHR
//SYSLIN    DD DSN=&&OBJ(&MEM),DISP=(MOD,PASS),
//             UNIT=VIO,SPACE=(CYL,(1,1,4))
//SYSLIB    DD DSN=SYS1.MACLIB,DISP=SHR
//          DD DSN=SYS1.MODGEN,DISP=SHR
//          DD DSN=SFT.SFT24.$LISTCVN.ASM,DISP=SHR
//SYSPRINT  DD SYSOUT=*
//SYSUT1    DD UNIT=VIO,SPACE=(CYL,(1,1))
//         PEND
//*
//SKJ$LC00 EXEC $LCASM,MEM=SKJ$LC00
//SKJ$LC01 EXEC $LCASM,MEM=SKJ$LC01
//SKJ$LC02 EXEC $LCASM,MEM=SKJ$LC02
//SKJ$LC03 EXEC $LCASM,MEM=SKJ$LC03
//SKJ$LC04 EXEC $LCASM,MEM=SKJ$LC04
//SKJ$LCMT EXEC $LCASM,MEM=SKJ$LCMT
//*
//LKED     EXEC PGM=IEWL,PARM='MAP,LIST,LET',
//             COND=(8,LT)
//OBJLIB    DD DSN=&&OBJ,DISP=(OLD,DELETE)
//SYSLMOD   DD DSN=SFT.SFT24.LOAD,DISP=SHR
//SYSPRINT  DD SYSOUT=*
//SYSUT1    DD UNIT=SYSDA,SPACE=(CYL,(1,1))
//SYSLIB    DD DSN=OS390.V1R3.ISP.SISPLOAD,DISP=SHR
//SYSLIN    DD *
 INCLUDE OBJLIB(SKJ$LC00,SKJ$LC01,SKJ$LC02)
 INCLUDE OBJLIB(SKJ$LC03,SKJ$LC04,SKJ$LCMT)
 ENTRY SKJ$LC00
 ALIAS $LISTC,$SPACE,$LCSPF
 NAME SKJ$LC00(R)
