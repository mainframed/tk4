//GREGWORM JOB S,G.PRICE,CLASS=S,MSGCLASS=X,MSGLEVEL=(1,1),
//             NOTIFY=GREG,REGION=2048K,COND=(0,NE)
//*
//*  ASSEMBLE AND LINK EDIT THE "WORM" COMMAND/PROGRAM
//*
//ASM     EXEC PGM=IEV90,PARM='NODECK,OBJECT'
//SYSPRINT DD  SYSOUT=*
//SYSLIB   DD  DSN=SYS1.MACLIB,DISP=SHR
//SYSUT1   DD  UNIT=VIO,SPACE=(CYL,(1,1))
//SYSLIN   DD  DSN=&&OBJSET,DISP=(NEW,PASS),UNIT=VIO,
//             DCB=(DSORG=PS,RECFM=FB,LRECL=80,BLKSIZE=800)
//SYSIN    DD  DSN=GREG.XA.DATA(WORM),DISP=SHR   <=== SOURCE FILE
//LKED    EXEC PGM=IEWL,PARM='LIST,MAP,NCAL,AC=1'
//SYSPRINT DD  SYSOUT=*
//SYSLIN   DD  DSN=&&OBJSET,DISP=(OLD,DELETE)
//         DD  *
  ALIAS HALFWORM,HW,QUARTERW,QW,WORMTEST,HALFTEST,QUARTEST
  NAME WORM(R)
