PROC 1 ZCMD
CONTROL MSG NOFLUSH
ALLOC FILE(SMPTABL) DA('&SYSUID..ISPF.ISPPROF') SHR REUSE

ISPEXEC LIBDEF ISPTLIB DATASET ID('SYS1.SGIMTENU') UNCOND
ISPEXEC LIBDEF ISPSLIB DATASET ID('SYS1.SGIMSENU') UNCOND
ISPEXEC LIBDEF ISPPLIB DATASET ID('SYS1.SGIMPENU') UNCOND
ISPEXEC LIBDEF ISPMLIB DATASET ID('SYS1.SGIMMENU') UNCOND
ALTLIB ACT APPL(CLIST) DATASET('SYS1.SGIMCLS0') UNCOND

ISPEXEC SELECT PGM(GIMSTART) PARM(&ZCMD) NEWAPPL(SMPE) NOCHECK PASSLIB

ALTLIB DEACT APPL(CLIST)
ISPEXEC LIBDEF ISPTLIB
ISPEXEC LIBDEF ISPSLIB
ISPEXEC LIBDEF ISPPLIB
ISPEXEC LIBDEF ISPMLIB
FREE FILE(SMPTABL)
