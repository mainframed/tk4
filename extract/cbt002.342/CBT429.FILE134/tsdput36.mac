%-----------------------------------------------------------------------
+%COMMAND ===>_ZCMD
+
+
+       Dsname %==>_DSNB
+       Volume %==>_VLM   +      (Blank for default)
+
+           Blksize  %==>_BLK   +
+           Lrecl    %==>_LRC   +
+           Recfm    %==>_REC+
+
+         Dsorg    %==>_DSO+
+
+         Dir      %==>_DIR   +  (0 for PS)
+         Primary  %==>_PRM   +
+         Secondary%==>_SEC   +
+         Units    %==>_TYP  +   (Cyl or Track)
+
+
+
+
+
+
+HIT ENTER TO EXECUTE OR PRESS%END KEY+TO EXIT.%PRESS PFK1+FOR FURTHER I
)INIT
  .HELP = TSDPUT31
)PROC
 VER (&DSNB,NONBLANK)
 VER (&DSNB,DSNAME)
 VER (&VLM,PICT,'CCCCCC')
 VER (&BLK,NONBLANK)
 VER (&BLK,RANGE,0,32760)
 VER (&LRC,NONBLANK)
 VER (&LRC,RANGE,0,32760)
 VER (&REC,NONBLANK)
 VER (&REC,LIST,F,FB,FBA,FBM,VB,VS,VBS,VBM,U)
 VER (&DSO,NONBLANK)
 VER (&DSO,LIST,DA,DAU,PO,POU,PS,PSU)
 VER (&DIR,NONBLANK)
 VER (&DIR,RANGE,0,99999)
 VER (&PRM,NONBLANK)
 VER (&PRM,RANGE,1,99999)
 VER (&SEC,NONBLANK)
 VER (&SEC,RANGE,0,99999)
 VER (&TYP,NONBLANK)
 VER (&TYP,LIST,TRACK,CYL)
 &RCF = TRANS (&REC F,F FB,'F,B' FBA,'F,B,A' FBM,'F,B,M'
                        VB,'V,B' VBS,'V,B,S' VBM,'V,B,M' U,'U')
)END
