/* EDUNPRT ¿ CONVERT PRINT TRAIN T-11/TN BACK TO 3278T */
   ISREDIT MACRO

     ISREDIT CHANGE   "þ" "×" ALL
     ISREDIT CHANGE X'8B' "{" ALL
     ISREDIT CHANGE X'9B' "}" ALL

     SET &ZEDSMSG = &STR(NOW 3278T)
     SET &ZEDLMSG = &STR(==MSG> NOW 3278T IF PREVIOUSLY T-11/TN)
     ISPEXEC SETMSG MSG(ISRZ000)
   EXIT CODE(1)
