PROC 0 PARM()
CONTROL NOLIST NOMSG NOPROMPT
IF &SYSISPF = ACTIVE   +
THEN DO
     ISPEXEC VGET (ZSCREEN) ASIS
     ISPEXEC VGET (XOLVL&ZSCREEN) PROFILE
     IF &LASTCC = 0    +
     THEN DO
          SET XOVAR = &STR(&&XOLVL&ZSCREEN)
          SET XOLVL&ZSCREEN = &XOVAR + 1
          ISPEXEC VPUT (XOLVL&ZSCREEN) PROFILE
          ISPEXEC SELECT PANEL(ISR@PRIM) OPT(&PARM) NEWAPPL(ISR)
          SET XOLVL&ZSCREEN = &XOVAR - 1
          ISPEXEC VPUT (XOLVL&ZSCREEN) PROFILE
          END
     ELSE ISPEXEC SELECT PANEL(ISR@PRIM) OPT(&PARM) NEWAPPL(ISR)
     END
ELSE PDF
