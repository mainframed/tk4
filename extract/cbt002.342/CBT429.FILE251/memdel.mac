 /********************* MEMDEL   ISPF EDIT MACRO *********************/
 /* MEMDEL WILL BUILD A  TEMPORARY  MEMBER  WHICH CONTAINS A LIST OF */
 /* MEMBER NAMES FROM THE CURRENT  PDS.  IF ANY MEMBERS ARE SELECTED */
 /* FROM THE LIST BY REPLACING THE '_' WITH AN 'X' THEY WILL THEN BE */
 /* DELETED.  AN OPTIONAL  RANGE  OF  MEMBERS  MAY  BE SELECTED WHEN */
 /* ISSUING THE MEMDEL MACRO, THE DEFAULT IS THE WHOLE DIRECTORY.    */
 /*                                                                  */
 /*                                                                  */
 /* SYNTAX: MEMDEL {ABC HJK} <= OPTIONAL                             */
 /*                                                                  */
 /********************************************************************/

 ISREDIT MACRO (FROM,TO) NOPROCESS
 ISREDIT REP Z9999999 1 1
 ISREDIT (WORKDSN) = DATASET
 ISPEXEC VPUT (FROM,TO) SHARED
ISPEXEC  LMINIT DATAID(PDSID) DATASET('&WORKDSN') +
                ENQ(SHRW)                        +
                ORG(PDSORG)
SET LMRC = &LASTCC
ISPEXEC LMOPEN DATAID(&PDSID) OPTION(OUTPUT)
  ISPEXEC  LMMDEL  DATAID(&PDSID) MEMBER(########)
ISPEXEC LMCLOSE DATAID(&PDSID)
/* FREE DATA ID */
ISPEXEC LMFREE  DATAID(&PDSID)
 SET &Z9 = Z9999999
 ISPEXEC EDIT DATASET('&WORKDSN(########)')  MACRO(MEMLSTX)
 ISPEXEC EDIT DATASET('&WORKDSN(########)')  MACRO(MEMLST1)
ISPEXEC  LMINIT DATAID(PDSID) DATASET('&WORKDSN') +
                ENQ(SHRW)                        +
                ORG(PDSORG)
SET LMRC = &LASTCC
ISPEXEC LMOPEN DATAID(&PDSID) OPTION(OUTPUT)
  ISPEXEC  LMMDEL  DATAID(&PDSID) MEMBER(########)
ISPEXEC LMCLOSE DATAID(&PDSID)
/* FREE DATA ID */
ISPEXEC LMFREE  DATAID(&PDSID)
ISREDIT MOVE Z9999999 AFTER .ZLAST
ISREDIT CANCEL
