//KKM00155 JOB (2100,56-D),'KISER 2215 T',CLASS=A,MSGCLASS=T
//*TYPRUN=HOLD
//*
//*  THIS JOB CONVERTS LIBRARYS SUCH AS WDPSC.MACLIB INTO
//*    IEBUPDTE FORMAT AND PUTS THEM INTO THE EXPORT LIBRARY AS
//*    A SINGLE MEMBER. IT USES THE LISTPDS PROGRAM FROM FILE 316
//*    OF THE CBT TAPE TO CONVERT THE PDS TO SEQUENTIAL FORMAT.
//*
//S1   EXEC PGM=LISTPDS,PARM='NOLIST,DECK,UPDTE'
//*
//SYSPRINT DD SYSOUT=*
//SYSLIB DD DISP=SHR,DSN=WDPSC.MACLIB
//SYSPUNCH DD DSN=WDPSC.EXPORT.ALLOFEM(MACLIB),DISP=SHR
//SYSIN DD DUMMY
//*
//S2   EXEC PGM=LISTPDS,PARM='NOLIST,DECK,UPDTE'
//*
//SYSPRINT DD SYSOUT=*
//SYSLIB DD DISP=SHR,DSN=WDPSC.EXPORT.CLIST,DCB=LRECL=80
//SYSPUNCH DD DSN=WDPSC.EXPORT.ALLOFEM(CLISTS),DISP=SHR
//SYSIN DD DUMMY
//*
//S3   EXEC PGM=LISTPDS,PARM='NOLIST,DECK,UPDTE'
//*
//SYSPRINT DD SYSOUT=*
//SYSLIB DD DISP=SHR,DSN=WDPSC.EXPORT.PROCLIB
//SYSPUNCH DD DSN=WDPSC.EXPORT.ALLOFEM(PROCLIB),DISP=SHR
//SYSIN DD DUMMY
//*
//S4   EXEC PGM=LISTPDS,PARM='NOLIST,DECK,UPDTE'
//*
//SYSPRINT DD SYSOUT=*
//SYSLIB DD DISP=SHR,DSN=WDPSC.MASTER.CLIST.PARMS
//SYSPUNCH DD DSN=WDPSC.EXPORT.ALLOFEM(PARMLIB),DISP=SHR
//SYSIN DD DUMMY
//*
