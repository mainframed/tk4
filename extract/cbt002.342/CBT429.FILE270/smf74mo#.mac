//HGX00155 JOB (2100,56-D),'HGLAS 6-1019',TIME=4,
// CLASS=H,COND=(12,LT),MSGLEVEL=(1,1),REGION=999K,MSGCLASS=F
/*TITLE 'DASD ACTIVITY'
/*JOBPARM S=Q1CD
//OUTPUT OUTPUT DEFAULT=YES,CLASS=F,FORMS=CPRO,JESDS=ALL
//*  THE ABOVE CARD PUTS THE PRINT OUT TO MICROFICHE
//*  ROUTE XEQ WSCXA
//*  THIS JOB RESIDES IN SOFTD155.DAILY.CNTL(HGX00150)
//*
//*-----------------------------------------------------------------
//*  THIS JOB IS AN RMF POST PROCESSOR REPORT FOR HOWARD GLASTETTER
//*-----------------------------------------------------------------
//*
//RMFDASD  EXEC PGM=ERBRMFPP,REGION=800K,TIME=4
//MFPINPUT DD DSN=SOFTD155.DAILY.RMF(0),DISP=SHR,DCB=BUFNO=10
//SYSUDUMP DD SYSOUT=*
//ABNLDUMP DD DUMMY
//SYSIN    DD *
SUMMARY(INT,TOT)
DINTV(0830)
RTOD(0845,1715)
STOD(0845,1715)
SYSOUT(F)
REPORTS(DEVICE(DASD,TAPE),CPU,PAGING,PAGESP,CHAN,IOQ)
//*
//*  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *
//* WAS THE BLEND DATASET CREATED TODAY?  RC0=YES RC4=NO RC8=NO FIND
//*  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *
//*
//CHKTODAY EXEC PGM=CHKTODAY,PARM='DPSCD155.BLEND.DATASET'
//STEPLIB  DD DSN=WDPSC.SHARED.LOAD,DISP=SHR
//SYSUDUMP DD SYSOUT=*
//*
//*  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *
//*        DELETE IF OLD, ELSE SKIP THIS STEP
//*  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *  *
//*
//DELSTEP  EXEC PGM=IEFBR14,COND=(4,NE,CHKTODAY)
//DELETE   DD DSN=DPSCD155.BLEND.DATASET,DISP=(MOD,DELETE)
//*
//*  *  *  *  *  *  *  *  RECREATE IF DELETED, ELSE SKIP THIS STEP
//*
//*  *  *  *  *  *  *  *  FORCE A PHONY CPU ID CALLED 'COMB' AND
//*  *  *  *  *  *  *  *  PHONY LCUS BASED OF 1ST 2 BYTES OF DEV ADDRS
//*  *  *  *  *  *  *  *  AND MULTIPLY CERTAIN FIELDS BY THE NUMBER OF
//*  *  *  *  *  *  *  *  CPUS CREATING THE RMF74.  THIS ADJUSTS FOR
//*  *  *  *  *  *  *  *  ITEMS THAT ARE CALCULATED USING INTERVAL
//*  *  *  *  *  *  *  *  COUNTS.  THE CPU NUMBER IS A PARM TO THE
//*  *  *  *  *  *  *  *  PROGRAM BELOW.
//*
//RMFBLEND EXEC PGM=SMF74ALL,COND=(0,EQ,CHKTODAY),PARM=3,TIME=4
//STEPLIB  DD DSN=WDPSC.SHARED.LOAD,DISP=SHR
//PRINTOUT DD SYSOUT=*
//FILEIN   DD DSN=SOFTD155.DAILY.RMF(0),DISP=SHR,DCB=BUFNO=10
//*
//FILEOUT  DD DSN=DPSCD155.BLEND.DATASET,
//            DISP=(,CATLG,DELETE),
//            UNIT=SYSDA,SPACE=(CYL,(30,8),RLSE),
//            DCB=(LRECL=32760,BLKSIZE=23476,BUFNO=6,RECFM=VBS)
//*
//*  *  *  *  *  *  *  *  PRINT CPU BLEND REPORT
//*
//RMFHG    EXEC PGM=ERBRMFPP,REGION=800K,TIME=4
//MFPINPUT DD DSN=DPSCD155.BLEND.DATASET,DISP=SHR,DCB=BUFNO=6
//SYSUDUMP DD SYSOUT=*
//ABNLDUMP DD DUMMY
//SYSIN DD *
SUMMARY(INT,TOT)
DINTV(0830)
RTOD(0845,1715)
STOD(0845,1715)
SYSOUT(F)
REPORTS(DEVICE(DASD))
SYSID(COMB)
//*
//*  *  *  *  *  *  *  *     SPECIAL STEP FOR IBM - (NULLIFIED)
//
//RMFCHAN  EXEC PGM=ERBRMFPP,REGION=800K,TIME=4
//MFPINPUT DD DSN=SOFTD155.DAILY.RMF(0),DISP=SHR,DCB=BUFNO=10
//SYSUDUMP DD SYSOUT=*
//ABNLDUMP DD DUMMY
//SYSIN    DD *
SUMMARY(INT,TOT)
DINTV(0015)
RTOD(1700,2400)
STOD(1700,2400)
SYSOUT(F)
REPORTS(CHAN)
