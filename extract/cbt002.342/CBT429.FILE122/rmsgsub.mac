//RMSGSUB  PROC OPTION=RUN
//*  PROC TO START THE RMSG SUB-SYSTEM
//*  PARM=RUN       ONLY START RMSG IF RMSG IS NOT STARTED
//*  PARM=FORCE     START RMSG EVEN IF RMSG IS STARTED (RESTART)
//RMSGSUB  EXEC PGM=RMSGLOAD,PARM=&OPTION
//SYSUDUMP DD SYSOUT=A
//STEPLIB  DD DSN=SYS6.LINKLIB,DISP=SHR
//LOADLIB  DD DSN=SYS6.LINKLIB,DISP=SHR
//INTRDR   DD SYSOUT=(A,INTRDR),DCB=(LRECL=80,RECFM=F,BLKSIZE=80)
//CMDFILE  DD DSN=SYS1.PARMLIB(CMDRMSG),DISP=SHR
