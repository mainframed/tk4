.***       NON-JCL DYNAMIC ALLOCATION FUNCTIONS - VERB CODE 01     ***
.*********************************************************************
.*
.*                                      ADDED 11/22/91  MST00JB
.TUPASSW   AIF ('&PASSW' EQ '').TUPERMA
&TUPLEN    SETA &TUPLEN+1
           AIF ('&MAP' NE 'ONLY').TUPASW#
&NM.PASKY  DS  XL6
&NM.PASSW  DS  CL8                      PASSWORD
           AGO .TUPERMA
.TUPASW#   ANOP
           AIF (&PASSL EQ 1).PASS1
           AIF (&PASSL EQ 2).PASS2
           AIF (&PASSL EQ 3).PASS3
           AIF (&PASSL EQ 4).PASS4
           AIF (&PASSL EQ 5).PASS5
           AIF (&PASSL EQ 6).PASS6
           AIF (&PASSL EQ 7).PASS7
           AIF (&PASSL EQ 8).PASS8
           MNOTE 8,'PASSL MUST BE SPECIFIED WHEN USING PASSW PARM'
           MNOTE 8,'VALID VALUES FOR PASSL ARE 1 - 8'
           MEXIT
.PASS1     ANOP
&NM.PASKY  DC  X'0050,0001,0001'
&NM.PASSW  DC  CL1'&PASSW'
           AGO .TUPERMA
.PASS2     ANOP
&NM.PASKY  DC  X'0050,0001,0002'
&NM.PASSW  DC  CL2'&PASSW'
           AGO .TUPERMA
.PASS3     ANOP
&NM.PASKY  DC  X'0050,0001,0003'
&NM.PASSW  DC  CL3'&PASSW'
           AGO .TUPERMA
.PASS4     ANOP
&NM.PASKY  DC  X'0050,0001,0004'
&NM.PASSW  DC  CL4'&PASSW'
           AGO .TUPERMA
.PASS5     ANOP
&NM.PASKY  DC  X'0050,0001,0005'
&NM.PASSW  DC  CL5'&PASSW'
           AGO .TUPERMA
.PASS6     ANOP
&NM.PASKY  DC  X'0050,0001,0006'
&NM.PASSW  DC  CL6'&PASSW'
           AGO .TUPERMA
.PASS7     ANOP
&NM.PASKY  DC  X'0050,0001,0007'
&NM.PASSW  DC  CL7'&PASSW'
           AGO .TUPERMA
.PASS8     ANOP
&NM.PASKY  DC  X'0050,0001,0008'
&NM.PASSW  DC  CL8'&PASSW'
.* - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
.TUPERMA   AIF ('&PERMA' EQ '').TUCNVRT
&TUPLEN    SETA &TUPLEN+1
