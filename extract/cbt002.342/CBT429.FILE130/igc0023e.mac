//JOBCARD  JOB  (ACCOUNT),NAME
//ASM     EXEC PGM=IEV90,REGION=2048K,
//        PARM='OBJ,NODECK,TERM,RENT'
//SYSLIB   DD  DSN=SYS1.MACLIB,DISP=SHR
//         DD  DSN=SYS1.AMODGEN,DISP=SHR
//SYSUT1   DD  SPACE=(CYL,(5,5)),
//             UNIT=SYSDA
//SYSUT2   DD  SPACE=(CYL,(5,5)),
//             UNIT=SYSDA
//SYSUT3   DD  SPACE=(CYL,(5,5)),
//             UNIT=SYSDA
//SYSTERM  DD  SYSOUT=*
//SYSPRINT DD  SYSOUT=A
//SYSPUNCH DD  DUMMY
//SYSLIN   DD   DSN=&&OBJSET,UNIT=SYSDA,SPACE=(80,(200,50)),
//             DISP=(MOD,PASS)
//SYSIN    DD  *
IGC0023E CSECT
         USING *,15
         L     2,180(,4)  PICK UP CALLER'S JSCB
AUTHOFF  NI    236(2),X'FE'  SET AUTH OFF
         LTR   0,0           R0 EQ 0 REQUESTS AUTH OFF
         BZR   14
AUTHON   OI    236(2),X'01'  SET AUTH ON
         BR    14
         END
