PROC 0
CONTROL MAIN
IF &DEBUG EQ DEBUG THEN CONTROL CON SYM MSG NOPROMPT LIST
                   ELSE CONTROL NOCON NOSYM NOMSG NOPROMPT NOLIST
ISPEXEC VGET (YSRC YSESS YCLASS) PROFILE
SET STATE = 1
ISPEXEC DISPLAY PANEL(INFOMGTP)
IF &LASTCC GE 8 THEN SET STATE = 0
DO WHILE(&STATE = 1)
  IF &YSRC NE THEN SET DS = SRC(&YSRC)
  ELSE SET DS =
  IF &YCLASS NE THEN SET EC = CLASS(&YCLASS)
  ELSE SET EC =
  IF &YSESS NE THEN SET SS = SESS(&YSESS)
  ELSE SET SS =
  ISPEXEC SELECT PGM(BLGINIT) PARM(&DS &EC &SS)
  ISPEXEC DISPLAY PANEL(YINFOMGT)
  IF &LASTCC GE 8 THEN SET STATE = 0
END
ISPEXEC VPUT (YSRC YSESS YCLASS) PROFILE
EXIT CODE(0)
