%-------------------------  INFORMATION/MANAGEMENT  --------------------
%COMMAND ===>_ZCMD
+
+   SRC EXECUTION NAME  %===>_YSRC    +
+
+   PRIVILEGE CLASS NAME%===>_YCLASS  +
+
+   SESSION PARAMETERS  %===>_Z +
+
)INIT
  .CURSOR = YSRC
  .ZVARS = '(YSESS)'
)PROC
  VER (&YSRC,NAME,MSG=IN000)
  VER (&YCLASS,NAME,MSG=IN001)
  VER (&YSESS,NUM)
)END
