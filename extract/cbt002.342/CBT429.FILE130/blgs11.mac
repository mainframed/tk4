//JOBCARD  JOB  (ACCOUNT),NAME
//*
//*  THIS JOB CREATES SOME SESSION PARAMETERS WHICH INCLUDE THE
//*  CBTMODS INFO/MVS FILE
//*
//*  UPDATE ALL DATA SET NAMES AS APPROPRIATE FOR YOUR INSTALLATION
//*
//STEP1    EXEC ASMHCL
//C.SYSIN   DD *
BLGVDSN  CSECT
         BLGPARMS RPANLDS=RPANELS,                                     X
               DICTDS=DICTDS,                                          X
               RFTDS=RFTS,                                             X
               WPANLDS=WPANELS,                                        X
               LINECNT=100
SYSTEM   BLGCLUST NAME=5,          INFO/MGMT CLUSTER                   X
               PRODUCT=MGMT,       NAME THE PRODUCT                    X
               SDDS=MGTSDDS,       NAME THE SDDS                       X
               SDIDS=MGTSDIDS,     NAME THE SDIDS                      X
               SDLDS=MGTSDLDS      NAME THE SDLDS
MVS      BLGCLUST NAME=0,          INFO/MVS CLUSTER                    X
               PRODUCT=MVS,        NAME THE PRODUCT                    X
               SDDS=OZSDDS,        NAME THE SDDS                       X
               SDIDS=OZSDIDS       NAME THE SDIDS
CBT      BLGCLUST NAME=1,          INFO/MVS CBTMODS CLUSTER            X
               PRODUCT=MVS,        NAME THE PRODUCT                    X
               SDDS=CBTSDDS,       NAME THE SDDS                       X
               SDIDS=CBTSDIDS      NAME THE SDIDS
MGTSDDS  BLGCLDSN DSN=DISKV.SDDS5
MGTSDIDS BLGCLDSN DSN=DISKV.SDIDS5
MGTSDLDS BLGCLDSN DSN=DISKV.SDLDS5
OZSDDS   BLGCLDSN DSN=DISKV.OZ.SDDS
OZSDIDS  BLGCLDSN DSN=DISKV.OZ.SDIDS
CBTSDDS  BLGCLDSN DSN=DISKV.CBTMODS.SDDS
CBTSDIDS BLGCLDSN DSN=DISKV.CBTMODS.SDIDS
RPANELS  BLGCLDSN DSN=DISKV.READ.PANELS
DICTDS   BLGCLDSN DSN=DISKV.DICT
RFTS     BLGCLDSN DSN=DISK.REPORT.FORMATS,FILE=RFTDD
WPANELS  BLGCLDSN DSN=DISKV.WRITE.PANELS
         BLGGEN
         END
/*
//L.SYSLMOD DD DISP=SHR,DSN=SYS1.LINKLIB(BLGSES11)
//
