PROC 0
/* #102ERE *********************************************************/
/***                                                             ***/
/***   # #    1    000  2222  EEEEE RRRR  EEEEE                  ***/
/***  #####  11   0   0     2 E     R   R E                      ***/
/***   # #    1   0   0    2  EEEE  RRRR  EEEE                   ***/
/***  #####   1   0   0   2   E     R  R  E                      ***/
/***   # #   111   000  22222 EEEEE R   R EEEEE                  ***/
/***                                                             ***/
/***                                                             ***/
/***  THE #102ERE CLIST DISPLAYS ALL OF THE ERE'S                ***/
/***                                                             ***/
/*******************************************************************/
   EQ ERE1 ESE+8% AREA REMARK('ERE CONTROL BLOCKS') +
      LENGTH(X'88') NODROP
   EQ ERELAST ESE+C% AREA REMARK('ERE CONTROL BLOCKS') +
      LENGTH(X'88') NODROP
   LIST (ERE1:ERELAST)
   STACK ERE1
EXIT
