)ATTR
  ! TYPE(OUTPUT)  INTENS(HIGH) SKIP(ON)
  + TYPE(TEXT)    INTENS(LOW) SKIP(ON)
  % TYPE(TEXT)    INTENS(HIGH) SKIP(ON)
  _ TYPE(INPUT)   INTENS(HIGH) CAPS(ON)
  @ TYPE(INPUT)   INTENS(NON) CAPS(ON)
)BODY
%----------  BUILDING DEPOSIT LIB/CCF HISTORY MODULE UPDATE SERVICES  --
%COMMAND ===>_ZCMD                                                     +
+
%  HISTORY MEMBER NAME ===>_MEMBER  +
+
%  WORKORDER NUMBER    ===>_WORKORDR+     ( WO...... )
+
%  HISTORY MEMBER TYPE ===>_Z+            ( 1 THRU 6 )
%   1.+COPYBOOK        %2.+JCL        %3.+PROC        %4.+RAMIS
%   5.+SOURCE PROGRAM  %6.+SYSIN MEMBER
+
+
+  *** ( PF3×PF15 = END/CANCEL )     ( ENTER KEY = CONTINUE ) ***
)INIT
  .ZVARS = '(LIB)'
 IF (&ZERRMSG ^= ' ') .MSG = &ZERRMSG
)PROC
  VER (&MEMBER,NONBLANK)
  VER (&WORKORDR,NONBLANK)
  VER (&LIB,NONBLANK)
  VER (&LIB,RANGE,1,6)
)END
