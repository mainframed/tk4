)ATTR
  ! TYPE(OUTPUT)  INTENS(HIGH) SKIP(ON)
  + TYPE(TEXT)    INTENS(LOW) SKIP(ON)
  % TYPE(TEXT)    INTENS(HIGH) SKIP(ON)
  _ TYPE(INPUT)   INTENS(HIGH) CAPS(ON)
)BODY
%----- BUILDING DEPOSIT PRODCNTL DB2/COBOL2 BATCH AND CICS COMPILE SERVI
%COMMAND ===>_ZCMD                                                     +
+
% LIBRARIAN MASTER DATASET:
+   PROJECT ===>_LIBPRJ  +
+   GROUP   ===>_LIBGRP  +
+   TYPE    ===>_LIBTYP  +
+   MEMBER  ===>_MEMBER  +                   ACCOUNT CODE:_ACCTCDE
+
% SELECT COMPILE TYPE ===>_Z+ (1 × 2)
%   1.+ PRE-COMPILE/COMPILE/LINK/PACKAGE/BIND A BATCH COBOL2 PROGRAM
%   2.+ PRE-COMPILE/COMPILE/LINK/PACKAGE/BIND A CICS COBOL2 PROGRAM
+
% SELECT TARGET LOADLIB ===>_Z+   ( 1=QA × 2=PROD × 3=EMERGENCY )   < V2
+                                 ( 4=QA × 5=PROD × 6=EMERGENCY )   < V3
+
% DSN TABLE QUALIFIER   ===>_DB2QUAL +
+
+   ( HIT ENTER TO PROCESS × PF3 OR PF15 TO QUIT/CANCEL )
+
)INIT
  .ZVARS = '(COMPTYP LL)'
 IF (&ZERRMSG ^= ' ') .MSG = &ZERRMSG
  IF (&CURSOR = ' ')
    &CURSOR = MEMBER
    IF (&LIBTYP = ' ')
      &CURSOR = LIBTYP
    IF (&LIBGRP = ' ')
      &CURSOR = LIBGRP
    IF (&LIBPRJ = ' ')
      &CURSOR = LIBPRJ
    .CURSOR = &CURSOR
    &COMPTYP = ' '
)PROC
  VER (&LIBPRJ,DSNAME)
  VER (&LIBGRP,DSNAME)
  VER (&LIBTYP,DSNAME)
  VER (&MEMBER,NAME)
  VER (&ACCTCDE,NONBLANK,PICT,NNNNNNNNNNNNNNNN)
  VER (&COMPTYP,NONBLANK)
  VER (&COMPTYP,RANGE,1,2)
  VER (&LL,NONBLANK)
  VER (&LL,RANGE,1,6)
)END
