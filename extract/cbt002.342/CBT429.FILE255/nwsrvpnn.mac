  % TYPE(TEXT)    INTENS(HIGH) SKIP(ON)
  _ TYPE(INPUT)   INTENS(HIGH) CAPS(ON)
)BODY
%---------- BUILDING DEPOSIT NETWARE SYSTEMS LAN SERVER BACKUP SERVICES
%COMMAND ===>_ZCMD                                                     +
+
% SELECT LAN SERVER FOR BACKUP RESTART ===>_Z + (1 THRU 4)
% 1.+ CS6-POS       ( POS - PTTS31D   )
% 2.+ CS6-MAGIC     ( MAG - PTTS32D   )
% 3.+ CS-SERVER6A   ( C6A - PTTS50D/W )
% 4.+ CS-SERVER6B   ( C6B - PTTS51D/W )
+
+
+ ( ENTER KEY = SELECT/CONTINUE   ***   PF3 × PF15 = QUIT/CANCEL )
)INIT
  .ZVARS = '(LAN)'
 IF (&ZERRMSG ^= ' ') .MSG = &ZERRMSG
)PROC
  VER (&LAN,NONBLANK)
  VER (&LAN,RANGE,1,4)
)END
