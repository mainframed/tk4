+  ===>_ABS9                                                         +×
+  ===>_ABS10                                                        +×
+  ===>_ABS11                                                        +×
+  ===>_ABS12                                                        +×
+  ===>_ABS13                                                        +×
+  ===>_ABS14                                                        +×
+  ===>_ABS15                                                        +×
+  ===>_ABS16                                                        +×
)INIT
  .HELP = LBLOHLP1
 IF (&ZERRMSG ^= ' ') .MSG = &ZERRMSG
 &CURSOR = WORKORDR
