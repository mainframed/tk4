)ATTR
  % TYPE(TEXT)  INTENS(HIGH) SKIP(ON)
  ! TYPE(TEXT)  INTENS(HIGH) SKIP(ON)
  _ TYPE(INPUT) INTENS(HIGH)  CAPS(ON)
  + TYPE(TEXT) INTENS(LOW)  SKIP(ON)
)BODY
!----------------- COPYBOOK CROSS-REFERENCE SERVICE  --------------
%COMMAND ===>_ZCMD                                     +
+
+ENTERING COPYBOOK NAME WILL RESULT IN LIST OF ALL SOURCE MEMBERS +
+  IN WHICH THE COPYBOOK EXISTS +
+
+       COPYBOOK NAME ===>_COPYBOOK +
)INIT
    &CURSOR = COPYBOOK
)PROC
  VER (&COPYBOOK,NONBLANK)
  VPUT (COPYBOOK) SHARED
)END
