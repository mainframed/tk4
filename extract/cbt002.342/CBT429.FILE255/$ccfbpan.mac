)ATTR
  ! TYPE(OUTPUT)  INTENS(HIGH) SKIP(ON)
  + TYPE(TEXT)    INTENS(LOW) SKIP(ON)
  % TYPE(TEXT)    INTENS(HIGH) SKIP(ON)
  _ TYPE(INPUT)   INTENS(HIGH) CAPS(ON)
)BODY
%------ BUILDING DEPOSIT LIB/CCF BATCH ASSEMBLE/COMPILE SERVICES -------
%COMMAND ===>_ZCMD                                                     +
+
% LIBRARIAN MASTER DATASET:
+   PROJECT ===>_LIBPRJ  +
+   GROUP   ===>_LIBGRP  +
+   TYPE    ===>_LIBTYP  +
+   MEMBER  ===>_MEMBER  +                   ACCOUNT CODE:_ACCTCDE
+
% SELECT COMPILE TYPE ===>_Z+ (1 THRU 5)    %TURNOVER COMPILE: _Z+  (Y×N
%   1.+COMPILE/LINKEDT COBOL-II PROGRAM     %COMPILE ONLY: _Z+  (Y×N)
%   2.+COMPILE/LINKEDT DYL280 PROGRAM       %3.+ASSEMBLE/LINKEDT BAL PRO
%   4.+COMPILE/LINKEDT RPGIAUTO PROGRAM     %5.+COMPILE/LINKEDIT RPGII P
+
% COBOL2 AND CA-OPTIMIZER COMPILE OPTIONS:
+ _CCOPT                                                        +×
% LOADLIB ===>_CCFLOAD                                     +×
+
% COPYBOOK LIBRARIAN DATASET NODES:    +( select any 4 nodes )
+   ( 1=TESTCOPY  2=TURNCOPY  3=RJCTCOPY  4=QACOPY  5=PRODCOPY  6=EMRGCO
% SYSLIB DD COPYBOOK SEARCH CHAIN:   %1._Z+   %2._Z+   %3._Z+   %4._Z+
% ( ENTER KEY = SUBMIT - PF3×PF15 = END/CANCEL - PF1×PF13 = HELP )
)INIT
  .ZVARS = '(COMPTYP QACOMP CMPONLY C1 C2 C3 C4)'
  .HELP = $CCFBHP1
 IF (&ZERRMSG ^= ' ') .MSG = &ZERRMSG
  IF (&CURSOR = ' ')
    &CURSOR = MEMBER
    IF (&LIBTYP = ' ')
      &CURSOR = LIBTYP
    IF (&LIBGRP = ' ')
      &CURSOR = LIBGRP
    IF (&LIBPRJ = ' ')
      &CURSOR = LIBPRJ
    .CURSOR = &CURSOR
)PROC
  VER (&LIBPRJ,DSNAME)
  VER (&LIBGRP,DSNAME)
  VER (&LIBTYP,DSNAME)
  VER (&MEMBER,NAME)
  VER (&ACCTCDE,NONBLANK,PICT,NNNNNNNNNNNNNNNN)
  VER (&COMPTYP,NONBLANK)
  VER (&COMPTYP,RANGE,1,5)
  VER (&CMPONLY,LIST,Y,N)
  VER (&QACOMP,LIST,Y,N)
  VER (&CCFLOAD,NONBLANK)
  VER (&C1,NONBLANK)
  VER (&C2,NONBLANK)
  VER (&C3,NONBLANK)
  VER (&C4,NONBLANK)
  VER (&C1,RANGE,1,6)
  VER (&C2,RANGE,1,6)
  VER (&C3,RANGE,1,6)
  VER (&C4,RANGE,1,6)
  VPUT (LIBPRJ,LIBGRP,LIBTYP,ACCTCDE) PROFILE
)END
