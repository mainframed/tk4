)ATTR
  ! TYPE(OUTPUT)  INTENS(HIGH) SKIP(ON)
  + TYPE(TEXT)    INTENS(LOW) SKIP(ON)
  % TYPE(TEXT)    INTENS(HIGH) SKIP(ON)
  _ TYPE(INPUT)   INTENS(HIGH) CAPS(ON)
)BODY
%------ BUILDING DEPOSIT LIB/CCF HISTORY/ABSTRACT CHANGE SERVICES ------
%COMMAND ===>_ZCMD                                                     +
+
+
% SELECT HISTORY DATA MEMBER TYPE+ (1 THRU 6):
%     1.+  UPDATE COPYBOOK MEMBER
%     2.+  UPDATE JCL MEMBER
%     3.+  UPDATE PROC MEMBER
%     4.+  UPDATE RAMIS MEMBER
%     5.+  UPDATE SOURCE PROGRAM MEMBER
%     6.+  UPDATE SYSIN MEMBER
+
% MEMBER TYPE+ ===>_Z+     %MEMBER NAME+ ===>_MEMBER  +
+
)INIT
  .ZVARS = '(HISTLIB)'
 IF (&ZERRMSG ^= ' ') .MSG = &ZERRMSG
)PROC
  VER (&HISTLIB,NONBLANK)
  VER (&HISTLIB,RANGE,1,6)
  VER (&MEMBER,NONBLANK)
)END
