 ISREDIT FIND '//***FILE 001' 1
 ISREDIT (ROW,COL) = CURSOR
 ISREDIT LOCATE &ROW
    SET &ROW = &ROW + 2
 ISREDIT LOCATE &ROW
 /*   FILE SEARCH          */
    SET &ROW = &ROW + 2
 ISREDIT LOCATE &ROW
    SET &NOMB = 2
    SET &NOMBR = &STR(00&NOMB)
GETNOMB: +
    IF &NOMB < 10                       THEN GOTO SET0
    IF &NOMB < 100                      THEN GOTO SET1
    IF &NOMB < 1000                     THEN GOTO SET2
SET0: +
    SET &NOMBR = &STR(00&NOMB)
    GOTO DOFIL
SET1: +
    SET &NOMBR = &STR(0&NOMB)
    GOTO DOFIL
SET2: +
    SET &NOMBR = &STR(&NOMB)
DOFIL: +
 ISREDIT FIND '//***FILE '  1
 ISREDIT (ROW,COL) = CURSOR
 ISREDIT LINE_BEFORE &ROW =  "./ ADD NAME=@FILE&NOMBR"
    SET &ROW = &ROW + 2
 ISREDIT LOCATE &ROW
    SET &NOMB = &NOMB + 1
    IF &NOMB > 500 THEN GOTO MD
    GOTO GETNOMB
MD: +
