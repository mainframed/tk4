string1="The cat sat on the mat,said fred"
string2="The rat sat on the mat,said joe"
string3="The cat sat on the rat,said mike"
 filter="****   ********************"

if COMPUM(filter,string1,string2) then say "string1 is like string 2"
if COMPUM(filter,string2,string3) then say "string2 is like string 3"

exit(0)
