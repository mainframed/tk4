Location=MAlloc(2000)                         /*Allocate 2000 bytes */
A=Storage(C2X(Location),1,"EYECATCHER")       /*Move is data        */
say Storage(C2X(Location),10)                 /*Display Storage     */
A=MFree(Location)                             /*Free Storage        */

exit(0)
