./       ADD   NAME=SLSS1TIM
%------------ SLSS (Initialization Process) ----------------------------
+
+
+ This is the first time that the SLSS process has been invoked.  A cont
+ file is required before any libraries can be established.  This proces
+ build the control file 'SLSSCNTL' in a ISPF output table.  Enter the d
+ set name to be used as for the output DDNAME(ISPTABL).  This process w
+ free the DDNAME(ISPTABL) and re-allocate the DDNAME with the supplied
+ set name.
+
+ ISPF OUTPUT TABLE LIBRARY:
+    PROJECT%===>_SLSSPROJ+
+    LIBRARY%===>_SLSSLIBR+
+    TYPE   %===>_SLSSTYPE+
+
+ OTHER PARTITIONED DATA SET:    (Do not enter the member name)
+    DATA SET NAME %===>_SLSSOTHR                                    +
+
+
+
+
+
+
)INIT
  &A1 = ' '
  &A2 = ' '
  &SLSSDSN = ' '
)PROC
  IF (&SLSSOTHR = ' ')
    VER (&SLSSPROJ,NB,DSNAME)
    VER (&SLSSLIBR,NB,DSNAME)
    VER (&SLSSTYPE,NB,DSNAME)
    &SLSSDSN = '&SLSSPROJ..&SLSSLIBR..&SLSSTYPE'
  IF (&SLSSOTHR ^= ' ')
    VER (&SLSSOTHR,DSNAME)
    &A1 = TRUNC(&SLSSOTHR,1)
    IF (&A1 = '''')
      &A2 = .TRAIL
      &SLSSDSN = TRUNC(&A2,'''')
    IF (&A1 ^= '''')
      &SLSSDSN = '&SLSSOTHR'
  VER (&SLSSDSN,DSNAME)
)END
