)BODY WIDTH(&ZSCREENW) EXPAND(\\)
  # Skill Level # Output Printer Device # Graphics Preview # Help+
×
^\-\@Document Composition Facility - SCRIPT/VS^\-\
%COMMAND ===>_ZCMD
+
×ISPF Library:
×   Project%===>_PRJ1    +       _DEBUG+
×   Group  %===>_LIB1    +
×   Type   %===>_TYP1    +
×   Member %===>_MEM     +             Blank for a member selection list
×
×Other Partitioned or Sequential Data Set:
×   Data Set Name%===>_DSN
×
×
+   Enter@"HELP SCRIPT"+from any TSO COMMAND line to obtain the function
+   syntax, and operands of the SCRIPT command.  A complete list of all
+   publications is also provided.  The SCRIPT DSNAME LEVEL is SYS0.SCRI
×
)INIT
  .HELP   = SCHTP100
  .CURSOR = MEM
  &LIB2   = &Z
  &LIB3   = &Z
  &LIB4   = &Z
  &PSWD   = &Z
  &TYP1   = &SCRP
  &ZMLCSR = '        '           /* Added for member name pattern    */
                                 /* support (wildcard of "*").       */

  IF (&SKILL = &Z)
    &SKILL = 'A'
  IF (&RDEV = &Z)
    &RDEV = '3816'
    IF (&CDPU = &Z)
      &CDPU = 'N'
  IF (&RDEV ^= &Z)
    &CDPU = 'N'
  IF (&DSN ^= ' ')
    &MEM = ' '
    IF (.CURSOR = ' ') .CURSOR = DSN
  IF (.CURSOR = ' ')   .CURSOR = MEM

)REINIT
  &SCHPLMSG = &Z
  IF (&SKILL ^= ' ')
    .CURSOR = MEM
    REFRESH (ZCMD)
  IF (&RDEV ^= ' ')
    .CURSOR = MEM
    REFRESH (ZCMD)
  IF (&CDPU ^= ' ')
    .CURSOR = MEM
    REFRESH (ZCMD)

  IF (&ZMLCSR ^= ' ')
    .CURSOR = MEM

)PROC
  &PCMD = TRUNC(&ZCMD,' ')
  IF (&PCMD = 'PDBAS')
    &SKILL = 'B'
    &ZCMD = ' '
    &SCHPSMSG = 'Basic mode chosen'
    .MSG = SCHP000Z
  ELSE
    IF (&PCMD = 'PDADV')
      &SKILL = 'A'
      &ZCMD = ' '
      &SCHPSMSG = 'Advanced mode chosen'
      .MSG = SCHP000Z

  IF (&PCMD = 'PDTERM')
    &RDEV = 'TERM'
    &SCHPSMSG = 'Device TERM selected'
    &ZCMD = ' '
    .MSG = SCHP000Z
  IF (&PCMD = 'PD3270')
    &RDEV = '3270'
    &SCHPSMSG = 'Device 3270 selected'
    &ZCMD = ' '
    .MSG = SCHP000Z
  IF (&PCMD = 'PD1403')
    &RDEV = '1403'
    &SCHPSMSG = 'Device 1403 selected'
    &ZCMD = ' '
    .MSG = SCHP000Z
  IF (&PCMD = 'PD3800')
    &RDEV = '3800'
    &SCHPSMSG = 'Device 3800 selected'
    &ZCMD = ' '
    .MSG = SCHP000Z
  IF (&PCMD = 'PDPS')
    &RDEV = 'PS'
    &SCHPSMSG = 'PostScript selected'
    &ZCMD = ' '
    .MSG = SCHP000Z
  IF (&PCMD = 'PD3820')
    &RDEV = '3820'
    &SCHPSMSG = 'Device 3820 selected'
    &ZCMD = ' '
    .MSG = SCHP000Z
  IF (&PCMD = 'PD3812')
    &RDEV = '3812'
    &SCHPSMSG = 'Device 3812 selected'
    &ZCMD = ' '
    .MSG = SCHP000Z
  IF (&PCMD = 'PD3816')
    &RDEV = '3816'
    &SCHPSMSG = 'Device 3816 selected'
    &ZCMD = ' '
    .MSG = SCHP000Z
  IF (&PCMD = 'PD4224')
    &RDEV = '4224'
    &SCHPSMSG = 'Device 4224 selected'
    &ZCMD = ' '
    .MSG = SCHP000Z
  IF (&PCMD = 'PD4028')
    &RDEV = '4028'
    &SCHPSMSG = 'Device 4028 selected'
    &ZCMD = ' '
    .MSG = SCHP000Z
  IF (&PCMD = 'PDAFP')
    &RDEV = 'AFP'
    &SCHPSMSG = 'Core Device AFP selected'
    &ZCMD = ' '
    .MSG = SCHP000Z
  IF (&PCMD = 'PDISI')
    &RDEV = 'ISI'
    &SCHPSMSG = 'ISI printer selected'
    &ZCMD = ' '
    .MSG = SCHP000Z

  IF (&PCMD = 'PDNOVIEW')
    &CDPU =  'NO'
    &SCHPSMSG = 'Preview not selected'
    &ZCMD = ' '
    .MSG = SCHP000Z
  ELSE
    IF (&PCMD = 'PDVIEW')
      &CDPU = 'YES'
      &SCHPSMSG = 'Preview mode chosen'
      &ZCMD = ' '
     .MSG = SCHP000Z


                                 /* -------------------------------- */
                                 /*                                  */
  IF (&DSN = ' ')                /* If not "other data set", then    */
    VER (&PRJ1,NB)               /* verify that the library fields   */
    VER (&LIB1,NB)               /* are filled in by the user.       */
    VER (&TYP1,NB)               /*                                  */
                                 /* -------------------------------- */

                                 /* -------------------------------- */
                                 /*                                  */
  IF (&DSN ^= ' ')               /* If the first character of the    */
    &ZFC = TRUNC(&DSN,1)         /* data set name is "'", then       */
    IF (&ZFC = '''')             /* check to see if the last "'"     */
      &ZREM = .TRAIL             /* missing.                         */
      &ZREM2 = TRUNC(&ZREM,'''') /* If the last "'" is missing,      */
      IF (&ZREM2 = &ZREM)        /* then add it to the end of the    */
        &DSN = '&DSN&ZFC'        /* data set name.                   */
    &ZMLCSR = '********'         /*                                  */
