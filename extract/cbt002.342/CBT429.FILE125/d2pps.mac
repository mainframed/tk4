GOPTIONS NOTEXT82;
GOPTIONS DEVICE=GDDMPCGX;
GOPTIONS CBACK=CYAN ;
GOPTIONS COLORS=(WHITE CYAN PINK BLUE GREEN YELLOW RED);
*OPTIONS DEVADDR=(ADMPRINT,.,L02MCE) DEVICE=GDDM87;
GOPTIONS HSIZE=8 VSIZE=7;
TITLE1    J=C H=1 F=NONE C=CYAN  ' PAGES/SECOND SYSTEM 3 (TSO/BATCH) ';
TITLE2    J=C H=1 F=NONE C=CYAN
   ' JAN -MAY 12, 1989   AVERAGE PAGES/SECOND ';
FOOTNOTE1 J=L H=1 F=NONE C=BLACK 'D2PPS   SOURCE:RMF DATA. SYSTEM 3';
FOOTNOTE2 J=L H=1 F=NONE C=CYAN 'TECHNICAL SUPPORT  '
          J=R H=1 F=NONE C=CYAN 'NOON HOUR EXCLUDED ';
*
*    THIS SAS JOB TAKES THE FILE CREATED FROM RMF RECORD 71'S
*       PRODUCES A GRAPH
*
*     TO READ IN MORE THAN ONE MONTH'S WORTH OF DATA
*     CONCATENATE THE DATA AS FOLLOWS:
*          SET SASDB.P0718810
*              SASDB.P0718811
*              SASDB.P0718812
*
*     AND THEN USE MULTIPLE %INCLUDES TO EXCLUDE CERTAIN
*     DATES. FOR EXAMPLE,
*           %INCLUDE DD1(EX8810)
*           %INCLUDE DD1(EX8811)
*           %INCLUDE DD1(EX8812)
*
*     YOU COULD, OF COURSE, COPY THE EXCLUDE INSTRUCTIONS
*     INTO THE SAS PROGRAM BY ISSUING THE
*     INCLUDE COMMAND ON THE COMMAND LINE AND THEN MOVING
*     THOSE LINES OF CODE TO THE APPROPRIATE LOCATION.
*
*
;
  DATA TEST0
       (KEEP=GDAT GHH GPGSEC);
  SET SASDB.P0718901
      SASDB.P0718902
      SASDB.P0718903
      SASDB.P0718904
      SASDB.P071WK01
      SASDB.P071WK02
;
* EXCLUDE 2ND AND 3RD SHIFTS                    *;
   IF S71IST LT 090000 THEN DELETE ;
   IF S71IST GT 155000 THEN DELETE ;
* EXCLUDE LUNCH TIME                            *;
IF S71IST GE 120000 AND S71IST LT 130000
   THEN DELETE;
%INCLUDE DD1(EX718901) ;
%INCLUDE DD1(EX718902) ;
%INCLUDE DD1(EX718903) ;
%INCLUDE DD1(EX718904) ;
%INCLUDE DD1(EX718905) ;
 HH = FLOOR(S71IST/10000);
GPGSEC = PGSEC;
GHH  = HH    ;
GDAT = S71DAT ;
 OUTPUT TEST0;
RETAIN;
RETURN;
 PROC MEANS;
  VAR GPGSEC;
  BY GDAT;
  OUTPUT OUT=TEST1 MEAN=AVGPSEC;
*PROC PRINT DATA=TEST1;
  DATA TEST2
       (KEEP=GDAT GNUM AVGPSEC);
  SET TEST1  ;
 GNUM = _N_ ;
OUTPUT TEST2 ;
 RETURN ;
*PROC PRINT DATA=TEST1;
   PROC GPLOT      DATA = TEST2      /*GOUT=MYCAT.GRAPH*/;
    SYMBOL1 W=1 C=RED   I=JOIN V=DIAMOND ;
    SYMBOL2 W=1 C=CYAN  I=JOIN V=SQUARE  ;
   PLOT AVGPSEC*GNUM      /
                            VAXIS= 0 TO 400 BY 50
  ;
RUN;

