GOPTIONS NOTEXT82;
GOPTIONS DEVICE=GDDMPCGX;
GOPTIONS CBACK=CYAN ;
GOPTIONS COLORS=(WHITE CYAN PINK BLUE GREEN YELLOW RED);
*OPTIONS DEVADDR=(ADMPRINT,.,L02MCE) DEVICE=GDDM87;
GOPTIONS HSIZE=8 VSIZE=7;
TITLE1    J=L H=1 F=NONE C=CYAN  '  ';
TITLE2    J=L H=1 F=NONE C=CYAN
          'JUL-DEC 1988 AVG MAX TSO USERS/DAY     '
 ;
FOOTNOTE1 J=L H=1 F=NONE C=CYAN '  ';
FOOTNOTE2 J=L H=1 F=NONE C=CYAN 'D2TMM  AVG OF MAX TSO USERS';
*
*
*    THIS SAS JOB TAKES THE FILE CREATED FROM RMF RECORD 70'S
*       PRODUCES A GRAPH
*
*     TO READ IN MORE THAN ONE MONTH'S WORTH OF DATA
*     CONCATENATE THE DATA AS FOLLOWS:
*          SET SASDB.P0708810
*              SASDB.P0708811
*              SASDB.P0708812
*
*     AND THEN USE MULTIPLE %INCLUDES TO EXCLUDE CERTAIN
*     DATES. FOR EXAMPLE,
*           %INCLUDE DD1(EX8810)
*           %INCLUDE DD1(EX8811)
*           %INCLUDE DD1(EX8812)
*
*     YOU COULD, OF COURSE, COPY THE EXCLUDE INSTRUCTIONS
*     INTO THE SAS PROGRAM BY ISSUING THE
*     INCLUDE COMMAND ON THE COMMAND LINE AND THEN MOVING
*     THOSE LINES OF CODE TO THE APPROPRIATE LOCATION.
*
*
;
  DATA TMM8701
       (KEEP=GDAT GHH GTMM GNUM);
  SET SASDB.P0708807
      SASDB.P0708808
      SASDB.P0708809
      SASDB.P0708810
      SASDB.P0708811
      SASDB.P0708812
;
   IF S70IST LT 090000 THEN DELETE ;
   IF S70IST GT 150000 THEN DELETE ;
* -LUNCH TIME                                   *;
IF S70IST GE 120000 AND S70IST LT 130000
   THEN DELETE;
%INCLUDE DD1(EX8807) ;
%INCLUDE DD1(EX8808) ;
%INCLUDE DD1(EX8809) ;
%INCLUDE DD1(EX8810) ;
%INCLUDE DD1(EX8811) ;
%INCLUDE DD1(EX8812) ;
 HH = FLOOR(S70IST/10000);
GTMM = S70TMM ;
GHH  = HH ;
GDAT = S70DAT ;
 OUTPUT TMM8701;
RETAIN;
RETURN;
   PROC PRINT ;
 PROC MEANS NOPRINT;
  VAR GTMM;
  BY GDAT;
  OUTPUT OUT=TEST2 MEAN=AVGTMM;
 PROC PRINT DATA=TEST2;
SYMBOL1 W=1 C=GREEN I=JOIN V=DIAMOND ;
SYMBOL2 W=1 C=RED   I=JOIN V=STAR ;
   PROC GPLOT      DATA = TEST2     /*GOUT=MYCAT.GRAPH*/;
        PLOT AVGTMM*GDAT    /
        VAXIS= 0 TO 200 BY 20
  ;
RUN;
