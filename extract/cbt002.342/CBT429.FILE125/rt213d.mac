GOPTIONS NOTEXT82;
GOPTIONS DEVICE=GDDMPCGX;
GOPTIONS CBACK=CYAN ;
GOPTIONS COLORS=(WHITE CYAN PINK BLUE GREEN YELLOW RED);
*OPTIONS DEVADDR=(ADMPRINT,.,L02MCE) DEVICE=GDDM87;
*OPTIONS HSIZE=8 VSIZE=7;
TITLE1    J=L H=1 F=NONE C=CYAN  '  ';
TITLE2    J=C H=1 F=NONE C=CYAN
  'TSO AVERAGE RESPONSE TIME FOR PERIOD 1 BY 1/2 HOUR INTERVALS';
FOOTNOTE1 J=L H=1 F=NONE C=CYAN '  ';
FOOTNOTE2 J=L H=1 F=NONE C=CYAN
 'D2RT2  DEC 88 SOURCE:RMF DATA - SYSTEM 1';
*
*
*    THIS SAS JOB TAKES THE FILE CREATED FROM RMF RECORD 70'S
*       PRODUCES A GRAPH
*
*     TO READ IN MORE THAN ONE MONTH'S WORTH OF DATA
*     CONCATENATE THE DATA AS FOLLOWS:
*          SET SASDB.P0708810
*              SASDB.P0708811
*              SASDB.P0708812
*
*     AND THEN USE MULTIPLE %INCLUDES TO EXCLUDE CERTAIN
*     DATES. FOR EXAMPLE,
*           %INCLUDE DD1(EX8810)
*           %INCLUDE DD1(EX8811)
*           %INCLUDE DD1(EX8812)
*
*     YOU COULD, OF COURSE, COPY THE EXCLUDE INSTRUCTIONS
*     INTO THE SAS PROGRAM BY ISSUING THE
*     INCLUDE COMMAND ON THE COMMAND LINE AND THEN MOVING
*     THOSE LINES OF CODE TO THE APPROPRIATE LOCATION.
*
*
*       NUMBER OF DAYS DEPENDS ON THE INPUT.
*
;
  DATA TEST0
       (KEEP=GDAT GHH GRTP1 GRTP2 GRTP3 GRTTOT) ;
  SET SASDB.P0P28812
  ;
   IF S72IST LT 090000 THEN DELETE ;
   IF S72IST GT 155000 THEN DELETE ;
%INCLUDE DD1(EX8812)        ;
 HH = FLOOR(S72IST/100);
GRTP1 = RTP1;
GRTP2 = RTP2;
GRTP3 = RTP3;
GRTTOT= RTTOT;
GHH  = HH ;
GDAT = S72DAT;
 OUTPUT TEST0;
RETAIN;
RETURN;
*PROC PRINT DATA=TEST0;
  DATA TEST1
       (KEEP=GDAT GNUM GHH GRTP1 GRTP2 GRTP3 GRTTOT);
  SET TEST0
  ;
GNUM = _N_ ;
IF GHH = 1249 THEN GHH = 1230;
RETURN ;
*PROC PRINT DATA=TEST1;
   PROC G3D   DATA = TEST1    /*GOUT=TCCAT.GRAPH*/  ;
        PLOT GDAT*GHH=GRTP1/ CAXIS = CYAN
            CTEXT = CYAN  CTOP = RED    CBOTTOM = BLUE
            XTICKNUM=14
            YTICKNUM=0
            ZTICKNUM=10 ZMIN=0 ZMAX=1.8
            GRID
            TILT=60 90  ROTATE = 60 90
          NAME='XRT213D'
          DES='TSO P1 RT 3D DEC 87'
  ;
RUN;

