GOPTIONS NOTEXT82;
GOPTIONS DEVICE=GDDMPCGX;
GOPTIONS CBACK=CYAN ;
GOPTIONS COLORS=(WHITE CYAN PINK BLUE GREEN YELLOW RED);
*OPTIONS NOCHARACTERS;
*OPTIONS DEVADDR=(ADMPRINT,.,L02MCE) DEVICE=GDDM87;
GOPTIONS HSIZE=8 VSIZE=7;
TITLE1    J=L H=1 F=NONE C=CYAN  '  ';
TITLE2    J=L H=1 F=NONE C=CYAN
  'DEC 1988 TSO RESPONSE TIME 1ST PERIOD';
FOOTNOTE1 J=L H=1 F=NONE C=CYAN '  ';
FOOTNOTE2 J=L H=1 F=NONE C=CYAN
  'GREEN=MAX RED=AVG BLUE=MIN';
FOOTNOTE3 J=L H=1 F=NONE C=CYAN 'D2RT2  SOURCE:RMF DATA SYSTEM 3';
*
*    THIS SAS JOB TAKES THE FILE CREATED FROM RMF RECORD 72'S
*       PRODUCES A GRAPH
*
*     TO READ IN MORE THAN ONE MONTH'S WORTH OF DATA
*     CONCATENATE THE DATA AS FOLLOWS:
*          SET SASDB.P0728810
*              SASDB.P0728811
*              SASDB.P0728812
*
*     AND THEN USE MULTIPLE %INCLUDES TO EXCLUDE CERTAIN
*     DATES. FOR EXAMPLE,
*           %INCLUDE DD1(EX8810)
*           %INCLUDE DD1(EX8811)
*           %INCLUDE DD1(EX8812)
*
*     YOU COULD, OF COURSE, COPY THE EXCLUDE INSTRUCTIONS
*     INTO THE SAS PROGRAM BY ISSUING THE
*     INCLUDE COMMAND ON THE COMMAND LINE AND THEN MOVING
*     THOSE LINES OF CODE TO THE APPROPRIATE LOCATION.
*
*
;
  DATA TEST0
       (KEEP=GDAT GHH GRTP1 GRTP2 GRTP3 GRTTOT) ;
  SET SASDB.P0P28812
;
   IF S72IST LT 090000 THEN DELETE ;
   IF S72IST GT 155000 THEN DELETE ;
* -EXCLUDE LUNCH TIME                           *;
IF S72IST GE 120000 AND S72IST LT 130000
   THEN DELETE;
%INCLUDE DD1(EX8812)  ;
 HH = FLOOR(S72IST/10000);
GRTP1 = RTP1;
GRTP2 = RTP2;
GRTP3 = RTP3;
GRTTOT= RTTOT;
GHH  = HH ;
GDAT = S72DAT;
 OUTPUT TEST0;
RETAIN;
RETURN;
 PROC MEANS   NOPRINT    ;
  VAR GRTP1 GRTP2 GRTP3 GRTTOT;
  BY GDAT;
  OUTPUT OUT=TEST1 MEAN=AVGP1 AVGP2 AVGP3 AVGTOT
                   MIN =MINP1 MINP2 MINP3 MINTOT
                   MAX =MAXP1 MAXP2 MAXP3 MAXTOT
     ;
 PROC PRINT DATA=TEST1;
  DATA TEST2
       (KEEP=GDAT GNUM AVGP1 AVGP2 AVGP3 AVGTOT
                       MINP1 MINP2 MINP3 MINTOT
                       MAXP1 MAXP2 MAXP3 MAXTOT
    );
  SET TEST1
  ;
GNUM = _N_ ;
RETURN ;
SYMBOL1 W=1 C=RED   I=JOIN V=DIAMOND ;
SYMBOL2 W=1 C=CYAN  I=JOIN V=SQUARE  ;
SYMBOL3 W=1 C=GREEN I=JOIN V=STAR    ;
SYMBOL4 W=1 C=RED   I=JOIN V=CARTOG  ;
*ATTERN V=S R=99 ;
   PROC GPLOT      DATA = TEST2  /*GOUT=MYCAT.GRAPH*/ ;
        PLOT AVGP1*GNUM = 1
             MINP1*GNUM = 2
             MAXP1*GNUM = 3   /OVERLAY
        VAXIS= 0 TO 2.0 BY  .2
  ;
RUN;

*       VAXIS= 0 TO 4.0 BY .15;
