//SORTWK01 DD UNIT=DISK,SPACE=(CYL,(15,5))
//SORTWK02 DD UNIT=DISK,SPACE=(CYL,(15,5))
//SORTWK03 DD UNIT=DISK,SPACE=(CYL,(15,5))
//SORTWK04 DD UNIT=DISK,SPACE=(CYL,(15,5))
//SYSOUT   DD SYSOUT=*
//SYSPRINT DD SYSOUT=*
//EXITLIB  DD DSN=SYS1.LINKLIB,DISP=SHR
//SYSIN    DD *
 OPTION EQUALS
 SORT FIELDS=(11,4,CH,A,7,4,CH,A)
 MODS E15=(ERBPPSRT,500,EXITLIB,N),E35=(ERBPPSRT,500,EXITLIB,N)
