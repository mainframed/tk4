GOPTIONS NOTEXT82;
*OPTIONS DEVICE=GDDMPCGX;
GOPTIONS CBACK=CYAN ;
GOPTIONS COLORS=(WHITE CYAN PINK BLUE GREEN YELLOW RED);
*OPTIONS NOCHARACTERS;
*OPTIONS DEVADDR=(ADMPRINT,.,L02MCE) DEVICE=GDDM87 /*USE FOR ADMPRINT*/;
*PTIONS GDEST=U1 GCLASS=S                          /*USE FOR VPS     */;
*OPTIONS DEVADDR=(SASWTR,.,L02MCE)   DEVICE=IBM3287D /*      VPS  */;
*OPTIONS HSIZE=8 VSIZE=7;
TITLE1 J=C H=2 F=SIMPLEX C=WHITE
  'REGRESSION ANALYSIS OF AVERAGE CPU UTILIZATION BY DAY';
TITLE2    J=C H=1.5 F=SIMPLEX C=WHITE
  'JANUARY 1989 THROUGH MAY 26, 1989 : 3090-400E SERIAL # &S70SER';
FOOTNOTE1 J=L H=1 F=SIMPLEX C=WHITE '    RM1CPU: TECHNICAL SUPPORT';
FOOTNOTE2 J=L H=1 F=SIMPLEX C=RED
  '    RED LINE IS REGRESSION LINE ';
FOOTNOTE3 J=R H=1 F=SIMPLEX C=WHITE
  '    NOTE: Noon hour excluded from average';
SYMBOL1 W=1 C=GREEN V=DIAMOND I=JOIN;
SYMBOL2 W=1 C=CYAN V=STAR I=JOIN;
SYMBOL3 W=1 C=RED V=SQUARE I=JOIN;
*
*    THIS SAS JOB TAKES THE FILE CREATED BY CPUTOT.
*       CALCULATES AVERAGE CPU UTILIZATION FOR EACH DAY FOR
*       ONE PHYSICAL PROCESSOR COMPLEX.
*       THE NUMBER OF DAYS DEPENDS ON THE INPUT.
*
*       DOES A REGRESSION ANALYSIS ON THE ABOVE MENTIONED DATA.
*   ---------------------------------------------------------------
*
*    THIS SAS JOB TAKES THE FILE CREATED FROM RMF RECORD 70'S
*       PRODUCES A GRAPH
*
*     TO READ IN MORE THAN ONE MONTH'S WORTH OF DATA
*     CONCATENATE THE DATA AS FOLLOWS:
*          SET SASDB.UTM18810
*              SASDB.UTM18811
*              SASDB.UTM18812
*
*     AND THEN USE MULTIPLE %INCLUDES TO EXCLUDE CERTAIN
*     DATES. FOR EXAMPLE,
*           %INCLUDE DD1(EX8810)
*           %INCLUDE DD1(EX8811)
*           %INCLUDE DD1(EX8812)
*
*     YOU COULD, OF COURSE, COPY THE EXCLUDE INSTRUCTIONS
*     INTO THE SAS PROGRAM BY ISSUING THE
*     INCLUDE COMMAND ON THE COMMAND LINE AND THEN MOVING
*     THOSE LINES OF CODE TO THE APPROPRIATE LOCATION.
*
*
*    UPDATE INFORMATION:
*       WORKS FOR MVS/SP 2.2
*
;
  DATA TEST0
       (KEEP=GDAT GHH GCPU_UT);
  SET SASDB.UTM18901
      SASDB.UTM18902
      SASDB.UTM18903
      SASDB.UTM18904
      SASDB.UTM1WK01
      SASDB.UTM1WK02
      SASDB.UTM1WK03
      SASDB.UTM1WK04
;
   IF S70IST LT 090000 THEN DELETE ;
   IF S70IST GT 155000 THEN DELETE ;
* LUNCH TIME                 ;
IF S70IST GE 120000 AND S70IST LT 130000
   THEN DELETE;
%INCLUDE DD1(EX8901) ;
%INCLUDE DD1(EX8902) ;
%INCLUDE DD1(EX8903) ;
%INCLUDE DD1(EX8904) ;
%INCLUDE DD1(EX8905) ;
 HH = FLOOR(S70IST/10000);
GCPU_UT = CPU_UT;
GHH  = HH;
GDAT = S70DAT;
 OUTPUT TEST0    ;
RETAIN;
RETURN;
 PROC MEANS   NOPRINT;
  VAR GCPU_UT;
  BY GDAT;
  OUTPUT OUT=TEST1 MEAN=AVGCPU;
*PROC PRINT DATA=TEST1;
  DATA TEST2
       (KEEP=GDAT GNUM AVGCPU);
  SET TEST1  ;
GNUM = _N_ ;
RETURN;
SYMBOL1 W=1 C=GREEN I=JOIN V=STAR    L=4;
SYMBOL2 W=1 C=RED   I=JOIN V=DIAMOND ;
  PROC GLM ;
  MODEL AVGCPU= GNUM/P;
  OUTPUT  PREDICTED = PAVGCPU
          RESIDUAL = RESID;
PROC PRINT;
  PROC GPLOT;
   PLOT AVGCPU*GNUM   PAVGCPU*GNUM   / OVERLAY
   VAXIS = 0 TO 100 BY 10
    VREF = 75
   CVREF = RED
   LVREF = 2
  ;
RUN;
