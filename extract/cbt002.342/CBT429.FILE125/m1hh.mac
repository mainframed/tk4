GOPTIONS NOTEXT82;
*OPTIONS DEVICE=GDDMPCGX;
GOPTIONS CBACK=CYAN ;
GOPTIONS COLORS=(WHITE CYAN PINK BLUE GREEN YELLOW RED);
*OPTIONS NOCHARACTERS;
*OPTIONS DEVADDR=(ADMPRINT,.,L02MCE) DEVICE=GDDM87 /*USE FOR ADMPRINT*/;
*OPTIONS HSIZE=8 VSIZE=7;
TITLE1 J=C H=1.5 F=SIMPLEX C=WHITE
  'ANALYSIS OF THE MONTHLY HOURLY AVERAGES';
TITLE2    J=C H=1 F=SIMPLEX C=WHITE
  'JANUARY 1989 THROUGH APRIL 1989 : 3090-400E SERIAL # &S70SER';
FOOTNOTE1 J=L H=1 F=SIMPLEX C=WHITE
  '    M1HH: TECHNICAL SUPPORT-NOTE: NOON HOUR EXCLUDED FROM AVERAGE';
FOOTNOTE2 J=L H=1 F=SIMPLEX C=RED
  '    BREAKDOWN OF UTILIZATION BY HOUR FOR EACH MONTH';
FOOTNOTE3 J=L H=1 F=SIMPLEX C=RED
                  '    THE MAXIMUM UTILIZATION FOR 1 HOUR';
FOOTNOTE4 J=L H=1 F=SIMPLEX C=GREEN
                  '    THE HIGHEST HOURLY AVERAGE';
FOOTNOTE5 J=L H=1 F=SIMPLEX C=CYAN
                  '    MONTHLY AVERAGE OF ALL THE HOURS';
*
*    THIS SAS JOB TAKES THE FILE CREATED BY HHMNTHM.
*       CALCULATES AVERAGE CPU UTILIZATION FOR EACH HOUR FOR
*       ONE PHYSICAL PROCESSOR COMPLEX.
*   ---------------------------------------------------------------
*
*    THIS SAS JOB TAKES THE FILE CREATED FROM RMF RECORD 70'S
*       PRODUCES A GRAPH
*
*     TO READ IN MORE THAN ONE MONTH'S WORTH OF DATA
*     CONCATENATE THE DATA AS FOLLOWS:
*          SET SASDB.UTM18810
*              SASDB.UTM18811
*              SASDB.UTM18812
*
*     YOU COULD, OF COURSE, COPY THE EXCLUDE INSTRUCTIONS
*     INTO THE SAS PROGRAM BY ISSUING THE
*     INCLUDE COMMAND ON THE COMMAND LINE AND THEN MOVING
*     THOSE LINES OF CODE TO THE APPROPRIATE LOCATION.
*
*
*    UPDATE INFORMATION:
*       WORKS FOR MVS/SP 2.2
*
;
  DATA TEST0
       (KEEP=HMAXCPU MAVGCPU MMAXCPU MDATE);
  SET SASDB.M1JAN89
      SASDB.M1FEB89
      SASDB.M1MAR89
      SASDB.M1APR89
;
*PROC PRINT DATA=TEST0;
SYMBOL1 W=1 C=RED   I=JOIN V=STAR    ;
SYMBOL2 W=1 C=CYAN  I=JOIN V=DIAMOND    ;
SYMBOL3 W=1 C=GREEN I=JOIN V=BOX     ;
  PROC GPLOT;
   PLOT HMAXCPU*MDATE=3 MAVGCPU*MDATE=2 MMAXCPU*MDATE=1   / OVERLAY
   VAXIS = 40 TO 100 BY 10
   HAXIS = 'JAN89'  'FEB89'  'MAR89'  'APR89'
   VREF  = 75
   CVREF = RED
   LVREF = 2
   ;
RUN;
