GOPTIONS NOTEXT82;
GOPTIONS DEVICE=GDDMPCGX;
GOPTIONS CBACK=CYAN ;
GOPTIONS COLORS=(WHITE CYAN PINK BLUE GREEN YELLOW RED);
*OPTIONS NOCHARACTERS;
*OPTIONS DEVADDR=(ADMPRINT,.,L02MCE) DEVICE=GDDM87 /*USE FOR ADMPRINT*/;
*PTIONS GDEST=U1 GCLASS=S                          /*USE FOR VPS     */;
*OPTIONS DEVADDR=(SASWTR,.,L02MCE)   DEVICE=IBM3287D /*      VPS  */;
GOPTIONS HSIZE=8 VSIZE=7;
TITLE1    J=L H=1 F=NONE C=CYAN  '  ';
TITLE2    J=L H=1 F=NONE C=CYAN
  'DEC 1988 TSO RESPONSE TIME BY PERIOD';
FOOTNOTE1 J=L H=1 F=NONE C=CYAN '  ';
FOOTNOTE2 J=L H=1 F=NONE C=CYAN 'RED LINE IS AVERAGE RESPONSE TIME';
FOOTNOTE3 J=L H=1 F=NONE C=CYAN 'D2RT2  SOURCE:RMF DATA SYSTEM 1';
*
*    THIS SAS JOB TAKES THE FILE CREATED FROM RMF RECORD 72'S
*       PRODUCES A GRAPH
*
*    USE   SET SASDB.P0P2YYMM FOR PERFORMANCE GROUP 2, AND
*     USE  SET SASDB.P0P3YYMM FOR PERFORMANCE GROUP 3.
*     WHERE YY IS THE YEAR (E.G. 88) AND MM IS THE MONTH (E.G. 10).
*
*     TO READ IN MORE THAN ONE MONTH'S WORTH OF DATA
*     CONCATENATE THE DATA AS FOLLOWS:
*          SET SASDB.P0P28810
*              SASDB.P0P28811
*              SASDB.P0P28812
*
*     AND THEN USE MULTIPLE %INCLUDES TO EXCLUDE CERTAIN
*     DATES. FOR EXAMPLE,
*           %INCLUDE DD1(EX8810)
*           %INCLUDE DD1(EX8811)
*           %INCLUDE DD1(EX8812)
*
*     YOU COULD, OF COURSE, COPY THE EXCLUDE INSTRUCTIONS
*     INTO THE SAS PROGRAM BY ISSUING THE
*     INCLUDE COMMAND ON THE COMMAND LINE AND THEN MOVING
*     THOSE LINES OF CODE TO THE APPROPRIATE LOCATION.
*
*
;
  DATA TEST0
       (KEEP=GDAT GHH GRTP1 GRTP2 GRTP3 GRTTOT) ;
  SET SASDB.P0P28807
      SASDB.P0P28808
      SASDB.P0P28809
      SASDB.P0P28810
      SASDB.P0P28811
      SASDB.P0P28812
  ;
   IF S72IST LT 090000 THEN DELETE ;
   IF S72IST GT 155000 THEN DELETE ;
%INCLUDE DD1(EX728807) ;
%INCLUDE DD1(EX728808) ;
%INCLUDE DD1(EX728809) ;
%INCLUDE DD1(EX728810) ;
%INCLUDE DD1(EX728811) ;
%INCLUDE DD1(EX728812) ;
 HH = FLOOR(S72IST/10000);
GRTP1 = RTP1;
GRTP2 = RTP2;
GRTP3 = RTP3;
GRTTOT= RTTOT;
GHH  = HH ;
GDAT = S72DAT;
 OUTPUT TEST0;
RETAIN;
RETURN;
 PROC PRINT DATA=TEST0;
 PROC MEANS;
  VAR GRTP1 GRTP2 GRTP3 GRTTOT;
  BY GDAT;
  OUTPUT OUT=TEST1 MEAN=ARTP1 ARTP2 ARTP3 ARTTOT;
*PROC PRINT DATA=TEST1;
  DATA TEST2
       (KEEP=GDAT GNUM ARTP1 ARTP2 ARTP3 ARTTOT);
  SET TEST1
  ;
GNUM = _N_ ;
RETURN ;
*PROC PRINT ;
SYMBOL1 W=1 C=YELLOW I=JOIN V=DIAMOND ;
SYMBOL2 W=1 C=CYAN   I=JOIN V=SQUARE  ;
SYMBOL3 W=1 C=GREEN  I=JOIN V=STAR    ;
SYMBOL4 W=1 C=RED    I=JOIN V=CARTOG  ;
*ATTERN V=S R=99 ;
   PROC GPLOT      DATA = TEST2 /*GOUT=MYCAT.GRAPH*/ ;
        PLOT ARTP1*GNUM = 1
             ARTP2*GNUM = 2
             ARTP3*GNUM = 3
             ARTTOT*GNUM= 4  / OVERLAY
  ;
RUN;

*       VAXIS= 0 TO 4.0 BY .15;
