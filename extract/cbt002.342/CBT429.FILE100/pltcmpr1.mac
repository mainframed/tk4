)ATTR
  +  TYPE(TEXT) INTENS(LOW) SKIP(ON)
  %  TYPE(TEXT) INTENS(HIGH) SKIP(ON)
  _  TYPE(INPUT) INTENS(HIGH)
)BODY
%------------------------  FOREGROUND COMPARE  -------------------------
%COMMAND%===>_ZCMD
+ THIS OPTION OF ISPF WILL ALLOW YOU TO OPTIONALLY CHANGE, AND THEN
+ COMPARE TWO SEQUENTIAL (OR SEQUENTIAL-LIKE) DATASETS
+ NOTE:  ALL DATASET NAMES MUST BE FULLY QUALIFIED
%
+   INPUT DATASET NAME 1  %===>_INDSN1
+   INPUT RECORD LENGTH 1 %===>_INLR1+ (REQUIRED IF CHANGING LRECL)
+   OUTPUT RECORD LENGTH 1%===>_OUTLR1+(REQUIRED IF CHANGING LRECL)
+   SORT FIELDS 1         %===>_SRTFLDS1                          +
+
+   INPUT DATASET NAME 2  %===>_INDSN2
+   INPUT RECORD LENGTH 2 %===>_INLR2+ (REQUIRED IF CHANGING LRECL)
+   OUTPUT RECORD LENGTH 2%===>_OUTLR2+(REQUIRED IF CHANGING LRECL)
+   SORT FIELDS 2         %===>_SRTFLDS2                          +
+
% ENTER PARAMETERS DESCRIBING THE CHANGE YOU WISH MADE TO THE INPUT DATA
%      DATASET 1                               DATASET 2
+ FIRST START POSITION  %===>_STPOS11+    FIRST START POSITION  %===>_ST
+ FIRST FIELD LENGTH    %===>_FLNGTH11+   FIRST FIELD LENGTH    %===>_FL
+ SECOND START POSITION %===>_STPOS12+    SECOND START POSITION %===>_ST
+ SECOND FIELD LENGTH   %===>_FLNGTH12+   SECOND FIELD LENGTH   %===>_FL
+ THIRD START POSITION  %===>_STPOS13+    THIRD START POSITION  %===>_ST
+ THIRD FIELD LENGTH    %===>_FLNGTH13+   THIRD FIELD LENGTH    %===>_FL
)INIT
  .HELP = TPLTCMP1
  &ZHTOP = ISR00003   /* TUTORIAL TABLE OF CONTENTS        */
  &ZHINDEX = ISR91000 /* TUTORIAL INDEX - 1ST PAGE         */
)PROC
  IF (&INLR1 ^= ' ')
    VER(&INLR1,NUM,MSG=PLTCM008)
  IF (&OUTLR1 ^= ' ')
    VER(&OUTLR1,NUM,MSG=PLTCM008)
  IF (&INLR2 ^= ' ')
    VER(&INLR2,NUM,MSG=PLTCM008)
  IF (&OUTLR2 ^= ' ')
    VER(&OUTLR2,NUM,MSG=PLTCM008)
  IF (&STPOS11 ^= ' ')
    VER(&STPOS11,NUM,MSG=PLTCM008)
  IF (&FLNGTH11 ^= ' ')
    VER(&FLNGTH11,NUM,MSG=PLTCM008)
  IF (&STPOS12 ^= ' ')
    VER(&STPOS12,NUM,MSG=PLTCM008)
  IF (&FLNGTH12 ^= ' ')
    VER(&FLNGTH12,NUM,MSG=PLTCM008)
  IF (&STPOS13 ^= ' ')
    VER(&STPOS13,NUM,MSG=PLTCM008)
  IF (&FLNGTH13 ^= ' ')
    VER(&FLNGTH13,NUM,MSG=PLTCM008)
  IF (&STPOS21 ^= ' ')
    VER(&STPOS21,NUM,MSG=PLTCM008)
  IF (&FLNGTH21 ^= ' ')
    VER(&FLNGTH21,NUM,MSG=PLTCM008)
  IF (&STPOS22 ^= ' ')
    VER(&STPOS22,NUM,MSG=PLTCM008)
  IF (&FLNGTH22 ^= ' ')
    VER(&FLNGTH22,NUM,MSG=PLTCM008)
  IF (&STPOS23 ^= ' ')
    VER(&STPOS23,NUM,MSG=PLTCM008)
  IF (&FLNGTH23 ^= ' ')
    VER(&FLNGTH23,NUM,MSG=PLTCM008)
)END
