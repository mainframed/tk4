//CAH0B    JOB (GTE4),'ASSEMBLE GTEU',TIME=30,REGION=4096K,
//  CLASS=C,MSGLEVEL=(1,1),MSGCLASS=H,NOTIFY=CAH0
/*JOBPARM  LINES=200
//*
//*  ASSEMBLE THE SOURCE MODULES
//*  LINK THEM INTO A TRANSIENT LINKLIB
//*  LINKLIB NAME IS ON THE SYSLMOD FILE DD STATEMENT
//*
//ASMCLR   PROC NAME=
//*********************************************
//*  ASSEMBLE AND LINK REENTERABLE MODULES  ***
//*********************************************
//ASM      EXEC PGM=IFOX00,PARM='OBJ,NODECK,RENT'
//SYSLIB   DD  DSN=SYS1.MACLIB,DISP=SHR
//         DD  DSN=SYS1.GTEUSRC,DISP=SHR         <==== NOTE
//         DD  DSN=SYS1.AMODGEN,DISP=SHR
//SYSUT1   DD  DSN=&&SYSUT1,UNIT=SYSDA,SPACE=(CYL,(2,1))
//SYSUT2   DD  DSN=&&SYSUT2,UNIT=SYSDA,SPACE=(CYL,(1,1))
//SYSUT3   DD  DSN=&&SYSUT3,UNIT=SYSDA,SPACE=(CYL,(1,1))
//SYSPRINT DD  SYSOUT=*
//SYSPUNCH DD  SYSOUT=B
//SYSGO    DD  DSN=&&OBJSET,UNIT=SYSDA,SPACE=(CYL,(1,1)),DISP=(,PASS)
//SYSIN    DD  DSN=SYS1.GTEUSRC(&NAME),DISP=SHR
//*
//LKED     EXEC PGM=IEWL,COND=(8,LT,ASM),
//             PARM=(XREF,LET,LIST,NCAL,RENT,REUS,REFR)
//SYSLIN   DD  DSN=&&OBJSET,DISP=(OLD,DELETE)
//         DD  DDNAME=SYSIN
//SYSLMOD  DD  DSN=SYS1.GTEULK(&NAME),DISP=SHR
//SYSUT1   DD  DSN=&&SYSUT1,UNIT=SYSDA,SPACE=(CYL,(1,1))
//SYSPRINT DD  SYSOUT=*
//ASMCLR   PEND
//*
//*
//*
//S10R  EXEC ASMCLR,NAME=GTEUA10     SUBROUTINE
//S30R  EXEC ASMCLR,NAME=GTEUA30     SUBROUTINE
//S40R  EXEC ASMCLR,NAME=GTEUA40     SUBROUTINE
//S50R  EXEC ASMCLR,NAME=GTEUA50     SUBROUTINE
//S60R  EXEC ASMCLR,NAME=GTEUA60     SUBROUTINE
//S70R  EXEC ASMCLR,NAME=GTEUA70     SUBROUTINE
//S80R  EXEC ASMCLR,NAME=GTEUA80     SUBROUTINE
//*
//S00R  EXEC ASMCLR,NAME=GTEUA00     DRIVER - UPDATE
//S01R  EXEC ASMCLR,NAME=GTEUA01     DRIVER - RESTORE
//S02R  EXEC ASMCLR,NAME=GTEUA02     DRIVER - DELETE
//S03R  EXEC ASMCLR,NAME=GTEUA03     DRIVER - ALIAS
//S04R  EXEC ASMCLR,NAME=GTEUA04     DRIVER - STATS
//S05R  EXEC ASMCLR,NAME=GTEUA05     DRIVER - BACKUP
//*
//SPUP  EXEC ASMCLR,NAME=GTEUPUP     TSO COMMAND PROCESSOR
//*
//SA0R  EXEC ASMCLR,NAME=GTEUAA0     UTILITY FOR CREATING LIBRARIES
//SB0R  EXEC ASMCLR,NAME=GTEUAB0     UTILITY FOR CREATING LIBRARIES
//*
//
