//CAH0Z    JOB (GTE4),'USERMOD GU20101',TIME=30,REGION=4096K,
//  CLASS=C,MSGLEVEL=(1,1),MSGCLASS=H,NOTIFY=CAH0
/*JOBPARM  LINES=200
//*
//*  --  THIS IS A SAMPLE USERMOD TO THE FUNCTION --
//*
//*  PRIOR TO RUNNING THIS JOBSTREAM, CHANGE THE HELP
//*  MEMBER GTEUPUPH, AND SOURCE MEMBER GTEUPUP, IN SYS1.GTEUSRC.
//*
//*  ASSEMBLE SOURCE MODULE FOR GTEUPUP.
//*  THEN PERFORM SYSMOD TO UPDATE THE LOAD MODULE AND HELP.
//*
//ASMCLR   PROC NAME=
//*********************************************
//*  ASSEMBLE AND LINK REENTERABLE MODULE   ***
//*********************************************
//ASM      EXEC PGM=IFOX00,PARM='OBJ,NODECK,RENT'
//SYSLIB   DD  DSN=SYS1.MACLIB,DISP=SHR
//         DD  DSN=SYS1.GTEUSRC,DISP=SHR         <==== NOTE
//         DD  DSN=SYS1.AMODGEN,DISP=SHR
//SYSUT1   DD  DSN=&&SYSUT1,UNIT=SYSDA,SPACE=(CYL,(2,1))
//SYSUT2   DD  DSN=&&SYSUT2,UNIT=SYSDA,SPACE=(CYL,(1,1))
//SYSUT3   DD  DSN=&&SYSUT3,UNIT=SYSDA,SPACE=(CYL,(1,1))
//SYSPRINT DD  SYSOUT=*
//SYSPUNCH DD  SYSOUT=B
//SYSGO    DD  DSN=&&OBJSET,UNIT=SYSDA,SPACE=(CYL,(1,1)),DISP=(,PASS)
//SYSIN    DD  DSN=SYS1.GTEUSRC(&NAME),DISP=SHR
//*
//LKED     EXEC PGM=IEWL,COND=(8,LT,ASM),
//             PARM=(XREF,LET,LIST,NCAL,RENT,REUS,REFR)
//SYSLIN   DD  DSN=&&OBJSET,DISP=(OLD,DELETE)
//         DD  DDNAME=SYSIN
//SYSLMOD  DD  DSN=SYS1.GTEULK(&NAME),DISP=SHR
//SYSUT1   DD  DSN=&&SYSUT1,UNIT=SYSDA,SPACE=(CYL,(1,1))
//SYSPRINT DD  SYSOUT=*
//ASMCLR   PEND
//*
//ASM  EXEC ASMCLR,NAME=GTEUPUP
//*
//**************************************************************
//*
//*  RECEIVE AND APPLY THE SYSMOD
//*
//SMP  EXEC SMPE,COND=(8,LE)
//SMPCNTL  DD *
SET BDY(GLOBAL).
RECEIVE SELECT(GU20101) LIST.
SET BDY(T------).
APPLY   SELECT(GU20101) REDO  /* .   ACCEPT IS ONLY A COMMENT, AND
SET BDY(D------).                    IS INCLUDED FOR USE AFTER TESTING
ACCEPT  SELECT(GU20101)       */ .
/*
//SMPPTFIN DD *
++USERMOD (GU20101)   /*

    COMMENTS GO HERE.

                      */.
++VER(Z038)  FMID(GGU0200).
++MOD(GTEUPUP)  LKLIB(GTEULK)  DISTLIB(AGTEULIB)
                LEPARM(RENT,REUS,REFR).
++MAC(GTEUPUPH) MALIAS(PUP)
    DISTLIB(AGTEHELP) SYSLIB(GTEHELP) TXLIB(GTEUSRC).
