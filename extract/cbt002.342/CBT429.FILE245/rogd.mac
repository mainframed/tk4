ARG OPT
IF OPT = "RESET" THEN "%ROGR"
LRC = LISTDSI("ISPTLIB" "FILE")
DSN1 = SYSDSNAME
LRC = LISTDSI("ISPPROF" "FILE")
DSN2 = SYSDSNAME
IF DSN1 ^= DSN2 THEN  "CONCAT DDN(ISPTLIB) DSN('" ×× DSN2 ×× "')"
IF RC > 1  THEN DO
                 SAY "CONCAT FAILED"
                 "%ROGR"
                 EXIT
                END
"ISPSTART  CMD(%ROGS " ×× OPT ×× ") NEWAPPL(AAAA) "
"K"
IF OPT ^= "FORCE" & OPT ^= "RESET" & OPT ^= " " THEN OPT
                                                ELSE "ISPF"
