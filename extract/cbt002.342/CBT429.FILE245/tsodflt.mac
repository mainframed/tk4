//* LIB: SYS1.PROCLIB(TSODFLT)
//*
//* NOTE: THIS PROCEDURE ENABLES USERS TO LOG ON TO TSO/E WITH
//*       BASIC SYS1 ISPF/SDSF CONCATINATIONS.  IT ALLOCATES THE
//*       ISPPROF DATASET AS A TEMPORARY DATASET TO ENSURE PROBLEMS
//*       DO NOT ARISE DUE TO ITS UNAVAILABILITY.
//*
//SYS1TSO EXEC PGM=IKJEFT01,
//             DYNAMNBR=300
//ISPPROF  DD  DSN=&&ISPPROF,DSORG=PO,
//             DISP=(NEW,DELETE,DELETE),
//             UNIT=SYSALLDA,
//             SPACE=(TRK,(5,1,12)),
//             DCB=(RECFM=FB,LRECL=80,BLKSIZE=15440)
//SYSPROC  DD  DSN=SYS1.ISRCLIB,
//             DISP=SHR
//ISPPLIB  DD  DSN=SYS1.ISFPLIB,
//             DISP=SHR
//         DD  DSN=SYS1.ISRPENU,
//             DISP=SHR
//         DD  DSN=SYS1.ISPPENU,
//             DISP=SHR
//ISPMLIB  DD  DSN=SYS1.ISFMLIB,
//             DISP=SHR
//         DD  DSN=SYS1.ISPMENU,
//             DISP=SHR
//         DD  DSN=SYS1.ISRMENU,
//             DISP=SHR
//ISPTLIB  DD  DSN=SYS1.ISFTLIB,
//             DISP=SHR
//         DD  DSN=SYS1.ISPTENU,
//             DISP=SHR
//         DD  DSN=SYS1.ISRTLIB,
//             DISP=SHR
//ISPSLIB  DD  DSN=SYS1.ISRSENU,
//             DISP=SHR
//         DD  DSN=SYS1.ISPSLIB,
//             DISP=SHR
//SYSHELP  DD  DSN=SYS1.HELP,
//             DISP=SHR
//SYSPRINT DD  SYSOUT=*,
//             TERM=TS
//SYSTERM  DD  SYSOUT=*,
//             TERM=TS
//SYSIN    DD  TERM=TS
//*
