 INCLUDE OBJECT
 REPLACE COPYFILE
 INCLUDE SYSLMOD(COPYFILE)
 ORDER   COPYSLNL,TLPRINT,MSGWRITE
 SETCODE AC(1)
 ENTRY   COPYSLNL
 NAME    COPYSLNL(R)
