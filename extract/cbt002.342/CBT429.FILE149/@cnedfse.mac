*
* THIS ZAP ADDS THE FOLLOWING TO IKJEBMA9 (EDIT USER SUBCOMMAND TABLE):
*   D - ALIAS FOR DOWN
*   T - ALIAS FOR TOP
*   U - ALIAS FOR UP
*   FULLSCR, FSE - CCNEDFSE
*   MODIFY,  MOD - CCNEDMOD
*   DO - CCNEDDO
*
 NAME IKJEBEMA IKJEBMA9
 EXPAND  IKJEBMA9(84)
 IDRDATA SM01255
*
VER 0000 FF
* FSE
REP 0000 07C6E4D3D3E2C3D9      AL1(7),C'FULLSCR'
REP 0008 03C6E2C5              AL1(3),C'FSE'
REP 000C C3C3D5C5C4C6E2C5      CL8'CCNEDFSE'
* MODIFY
REP 0014 06D4D6C4C9C6E8        AL1(6),C'MODIFY'
REP 001B 03D4D6C4              AL1(3),C'MOD'
REP 001F C3C3D5C5C4D4D6C4      CL8'CCNEDMOD'
* ALIAS FOR DOWN
REP 0027 01C4                  AL1(1),C'D'
REP 0029 00                    AL1(0)
REP 002A C9D2D1C5C2C5C4D6      CL8'IKJEBEDO'
* ALIAS FOR TOP
REP 0032 01E3                  AL1(1),C'T'
REP 0034 00                    AL1(0)
REP 0035 C9D2D1C5C2C5E3D6      CL8'IKJEBETO'
* ALIAS FOR UP
REP 003D 01E4                  AL1(1),C'U'
REP 003F 00                    AL1(0)
REP 0040 C9D2D1C5C2C5E4D7      CL8'IKJEBEUP'
* DO SUBCOMMAND
REP 0048 02C4D6                AL1(2),C'DO'
REP 004B 00                    AL1(0)
REP 004C C3C3D5C5C4C4D640      CL8'CCNEDDO '
*
REP 0054 FF                    END OF TABLE
*
 IDRDATA SM01255
