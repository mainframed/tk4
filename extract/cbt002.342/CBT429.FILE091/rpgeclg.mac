//DEFAULT  PROC RGREGN=512K,RGPARM='NODECK,LOAD,LIST,NOSEQN',
//         RGU3SPC='(600,(100,20))',RGU2SPC='(600,(100,20))',
//         RGU1SPC='(600,(100,20))',RGGOSPC='(80,(200,50))',
//         LKREGN=100K,LKPARM='XREF,LIST,LET',
//         LKMDSPC='(1024,(50,20,1))',LKU1SPC='(1024,(50,20))',
//         GOCOND='((9,LT,RPG),(5,LT,LKED))',GOPARM='DATE=000000'
//*-------------------------------------------------------------------*
//RPG      EXEC PGM=IESRPG,REGION=&RGREGN,PARM=(&RGPARM)
//SYSPRINT DD SYSOUT=*
//SYSPUNCH DD DUMMY
//SYSUT3   DD DSNAME=&&SYSUT3,UNIT=SYSDA,SPACE=&RGU3SPC
//SYSUT2   DD DSNAME=&&SYSUT2,UNIT=SYSDA,SPACE=&RGU2SPC
//SYSUT1   DD DSNAME=&&SYSUT1,UNIT=SYSDA,SPACE=&RGU1SPC
//SYSGO    DD DSNAME=&&LOADSET,UNIT=(SYSDA),
//            DISP=(MOD,PASS,DELETE),SPACE=&RGGOSPC
//*-------------------------------------------------------------------*
//LKED     EXEC PGM=IEWL,REGION=&LKREGN,PARM=(&LKPARM)
//SYSLIN   DD DSNAME=&&LOADSET,DISP=(OLD,DELETE)
//         DD DDNAME=SYSIN
//SYSLMOD  DD DSNAME=&&GOSET(RPG),UNIT=SYSDA,DISP=(NEW,PASS,DELETE),
//            SPACE=&LKMDSPC
//SYSUT1   DD DSNAME=&&SYSUT1,SPACE=&LKU1SPC,
//            UNIT=(SYSDA,SEP=(SYSLIN,SYSLMOD))
//SYSPRINT DD SYSOUT=*
//*-------------------------------------------------------------------*
//GO       EXEC PGM=*.LKED.SYSLMOD,COND=&GOCOND,PARM='&GOPARM'
