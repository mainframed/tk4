/* THIS CLIST SUBMITS A BACKGOUND JOB TO PRINT THE SOFTWARE STATUS  */
/* REPORT. IT IS EXECUTED FROM THE SOFTWARE STATUS MENU PANEL.      */
/* BE SURE TO EDIT THE JOB CARD, JOBLIB CARD, AND ALL DD CARDS.     */
/* (NOTE: THE &SYSUID PARAMETER ON THE JOB CARD GETS REPLACED BY    */
/* THE TSO USERID.)                                                 */
/*                                                                  */
/* DD CARDS STEP1.PRTFILE AND STEP3.PRTFILE REFER TO THE RUNTIME    */
/* MESSAGES FILE AND THE PRINTED REPORT ITSELF RESPECTIVELY.        */
/*                                                                  */
/* THE DD CARDS STEP1.CNTLCARD AND SORT.SYSIN REFER TO PDS          */
/* MEMBERS CONTAINING CONTROL CARDS FOR PROGRAMS SSR010PR AND SORT. */
/*                                                                  */
/* THE FIRST OF THESE MEMBERS, SSRPRNTC, CONTAINS CARDS CONTROLLING */
/* THE ORDER IN WHICH THE SOFTWARE STATUS ENTRIES ARE PRINTED. A    */
/* SAMPLE SSRPRNTC MEMBER, INCLUDED IN THIS LIBRARY, CONTAINS       */
/* INSTRUCTIONS ON FORMATTING THE CONTROL CARDS. YOU MUST CREATE AN */
/* SSRPRNTC MEMBER ON YOUR PARMLIB.                                 */
/*                                                                  */
/* THE SECOND MEMBER, SSRPRNTS, CONTAINS SORT CONTROL CARDS. COPY   */
/* THE SSRPRNTS MEMBER FROM THIS INSTALL LIBRARY ONTO YOUR PARMLIB  */
/* AS IS.                                                           */
/*                                                                  */
/* COPY THIS CLIST ONTO A CLIST LIBRARY ALLOCATED TO YOUR TSO/ISPF  */
/* SESSION.                                                         */
/*                                                                  */
 PROC 0
    SET SLSHASK=&STR(/*)
    SUBMIT *   END($$)
//&SYSUID.1 JOB (ACCT),'NAME',CLASS=P,NOTIFY=&SYSUID
//*JOBLIB   DD  DSN=YOUR.LOADLIB,DISP=SHR
//STEP1    EXEC PGM=SSR010PR
//PRTFILE   DD  SYSOUT=A,DCB=BLKSIZE=133
//SSRINTBL  DD  DSN=SSU1.SOFTSTAT.INDXTBL,DISP=SHR
//SSRPRTBL  DD  DSN=SSU1.SOFTSTAT.PRODTBL,DISP=SHR
//SSRHSTRY  DD  DSN=SSU1.SOFTSTAT.HISTORY,DISP=SHR
//OUTFILE   DD  DSN=&&OUTFILE,UNIT=DISK,DISP=(NEW,PASS),
//              DCB=(LRECL=94,BLKSIZE=9400,RECFM=FB),SPACE=(TRK,(1,1))
//SYSOUT    DD  SYSOUT=*
//SYSUDUMP  DD  SYSOUT=*
//CNTLCARD  DD  DSN=YOUR.PARMLIB(SSRPRNTC),DISP=SHR
&SLSHASK
//SORT     EXEC PGM=SORT,PARM='SIZE=MAX'
//SYSOUT    DD  SYSOUT=*
//SORTIN    DD  DSN=*.STEP1.OUTFILE,DISP=(OLD,DELETE)
//SORTOUT   DD  DSN=&&SORTOUT,DISP=(NEW,PASS,DELETE),UNIT=DISK,
//              DCB=(RECFM=FB,LRECL=94,BLKSIZE=9400),SPACE=(TRK,(1,1))
//SORTWK01  DD  UNIT=DISK,SPACE=(CYL,(1,1))
//SORTWK02  DD  UNIT=DISK,SPACE=(CYL,(1,1))
//SYSIN     DD  DSN=YOUR.PARMLIB(SSRPRNTS),DISP=SHR
&SLSHASK
//STEP3    EXEC PGM=SSR011PR
//PRTFILE   DD  SYSOUT=A,DCB=BLKSIZE=133
//INFILE    DD  DSN=*.SORT.SORTOUT,DISP=(OLD,DELETE,DELETE)
//SYSOUT    DD  SYSOUT=*
//SYSUDUMP  DD  SYSOUT=*
&SLSHASK
$$
