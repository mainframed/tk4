)ATTR
  + TYPE(TEXT) INTENS(LOW) SKIP(ON)
  % TYPE(TEXT) INTENS(HIGH) SKIP(ON)
  { TYPE(INPUT) INTENS(&INTND) JUST(RIGHT)
  _ TYPE(INPUT) INTENS(LOW) JUST(LEFT)
  × TYPE(INPUT) INTENS(&INTN1)
  @ TYPE(INPUT) INTENS(&INTN2)
  # TYPE(INPUT) INTENS(&INTN3)
  $ TYPE(INPUT) INTENS(&INTN4)
  ^ TYPE(INPUT) INTENS(&INTN5)
)BODY
%---------- SOFTWARE STATUS REPORT - ADD PTF/MOD ENTRY -----------------
%COMMAND ===>_ZCMD
+
%       PTF/MOD ===>_PTFMD     % DATE INSTALLED ===>{MM{DD{YY+(MONTH,DAY
+
%  NEW
%PRODUCT?   ACRONYM/PRODUCT NAME:             RELEASE:     FMID:   ORDER
+  ×Z%===> ×ACRNM1          +                _RELS1       _FMID1  _ORDR1
+          ×PRDN11                          +
+          ×PRDN21                          +
+  @Z%===> @ACRNM2          +                _RELS2       _FMID2  _ORDR2
+          @PRDN12                          +
+          @PRDN22                          +
+  #Z%===> #ACRNM3          +                _RELS3       _FMID3  _ORDR3
+          #PRDN13                          +
+          #PRDN23                          +
+  $Z%===> $ACRNM4          +                _RELS4       _FMID4  _ORDR4
+          $PRDN14                          +
+          $PRDN24                          +
+  ^Z%===> ^ACRNM5          +                _RELS5       _FMID5  _ORDR5
+          ^PRDN15                          +
+          ^PRDN25                          +
+                    PRESS ENTER TO PROCESS ADDS; "END" TO CANCEL
)INIT
  .HELP = SSRHLP30
  .ZVARS = '(C1,C2,C3,C4,C5)'
  &INTND = LOW
  &INTN1 = LOW
  &INTN2 = LOW
  &INTN3 = LOW
  &INTN4 = LOW
  &INTN5 = LOW
  &FORM = TRUNC (&STATE,1)
  IF (&ERRT = 'PN')
    .MSG = SSRMS007
  IF (&ERRT = 'DT')
    .MSG = SSRMS008
  IF (&FORM = A)
    .MSG = SSRMS003
  IF (&FORM = C)
    .MSG = SSRMS006
  IF (&ERRL = 01)
    &INTN1 = HIGH
    .CURSOR = ACRNM1
  IF (&ERRL = 02)
    &INTN2 = HIGH
    .CURSOR = ACRNM2
  IF (&ERRL = 03)
    &INTN3 = HIGH
    .CURSOR = ACRNM3
  IF (&ERRL = 04)
    &INTN4 = HIGH
    .CURSOR = ACRNM4
  IF (&ERRL = 05)
    &INTN5 = HIGH
    .CURSOR = ACRNM5
  &ERRT = ' '
  &ERRL = 00
)PROC
  VER (&MM,NONBLANK,MSG=SSRMS004)
  VER (&DD,NONBLANK,MSG=SSRMS004)
  VER (&YY,NONBLANK,MSG=SSRMS004)
  VER (&MM,RANGE,01,12,MSG=SSRMS004)
  VER (&DD,RANGE,01,31,MSG=SSRMS004)
  IF (&MM = 04,06,09,11)
    VER (&DD,RANGE,01,30,MSG=SSRMS004)
  IF (&MM = 02)
    VER (&DD,RANGE,01,28,MSG=SSRMS004)
  VER (&YY,NUM,MSG=SSRMS004)
  IF (.MSG = SSRMS004)
     &INTND = HIGH
  VER (&C1,LIST,Y)
  VER (&C2,LIST,Y)
  VER (&C3,LIST,Y)
  VER (&C4,LIST,Y)
  VER (&C5,LIST,Y)
  IF (&C1 = Y)
    IF (&PRDN11 = ' ')
      &INTN1 = HIGH
      .MSG = SSRMS005
      .CURSOR = PRDN11
  IF (&C2 = Y)
    IF (&PRDN12 = ' ')
      &INTN2 = HIGH
      .MSG = SSRMS005
      .CURSOR = PRDN12
  IF (&C3 = Y)
    IF (&PRDN13 = ' ')
      &INTN3 = HIGH
      .MSG = SSRMS005
      .CURSOR = PRDN13
  IF (&C4 = Y)
    IF (&PRDN14 = ' ')
      &INTN4 = HIGH
      .MSG = SSRMS005
      .CURSOR = PRDN14
  IF (&C5 = Y)
    IF (&PRDN15 = ' ')
      &INTN5 = HIGH
      .MSG = SSRMS005
      .CURSOR = PRDN15
)END
