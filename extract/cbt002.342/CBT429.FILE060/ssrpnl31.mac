)ATTR
   % TYPE(TEXT) INTENS(HIGH) SKIP(ON)
   + TYPE(TEXT) INTENS(LOW) SKIP(ON)
   _ TYPE(INPUT) INTENS(LOW)
)BODY
%------------------ SOFTWARE STATUS REPORT - ADD NEW PRODUCT -----------
%COMMAND ===>_ZCMD
+
+          PTF/MOD ===>&PTFMD     + DATE INSTALLED ===>+&MM&DD&YY+
+
% ENTER NEW PRODUCT INFORMATION:+     (REQUIRED: PRODUCT NAME, VENDOR, C
+
%     ACRONYM ===>_ACRNM           +
+
%PRODUCT NAME ===>_PRDN1                           +
%             ===>_PRDN2                           +
%     RELEASE ===>_RELS6       +
+
%        FMID ===>_FMID6  +
+
%ORDER NUMBER ===>_ORDR6       +
+
%      VENDOR ===>_VENDR                           %  NEW VENDOR? ==>_Z+
+
%    CATEGORY ===>_CTGRY                           %NEW CATEGORY? ==>_Z+
+
+          PRESS ENTER TO PROCESS ADDS; "END" TO RETURN TO ENTRY PANEL
)INIT
  .HELP = SSRHLP31
  .ZVARS = '(C6,C7)'
  IF (&ERRT = DP)
    .MSG = SSRMS011
    .CURSOR = PRDN1
  IF (&ERRT = VN)
    .MSG = SSRMS012
    .CURSOR = VENDR
  IF (&ERRT = CN)
    .MSG = SSRMS013
    .CURSOR = CTGRY
  IF (&RELS6 = ' ')
    .CURSOR = RELS6
  IF (&FMID6 = ' ')
    .CURSOR = FMID6
  IF (&ORDR6 = ' ')
    .CURSOR = ORDR6
  .CURSOR = VENDR
  &ERRT = ' '
)PROC
  VER (&PRDN1,NONBLANK,MSG=SSRMS005)
  VER (&VENDR,NONBLANK,MSG=SSRMS009)
  VER (&CTGRY,NONBLANK,MSG=SSRMS010)
  VER (&C6,LIST,Y)
  VER (&C7,LIST,Y)
)END
