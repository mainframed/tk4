)ATTR
  × TYPE(&TYP1) INTENS(HIGH)
  @ TYPE(&TYP2) INTENS(HIGH)
  # TYPE(&TYP3) INTENS(HIGH)
  $ TYPE(&TYP4) INTENS(HIGH)
  ^ TYPE(&TYP5) INTENS(HIGH)
  * TYPE(&TYP6) INTENS(HIGH)
  | TYPE(&TYP7) INTENS(HIGH)
  ~ TYPE(&TYP8) INTENS(HIGH)
  { TYPE(&ARGTYP) INTENS(LOW)
  } TYPE(&DELTYP) INTENS(LOW)
)BODY
%------------------ SOFTWARE STATUS REPORT - LIST PRODUCT --------------
%COMMAND ===>_ZCMD
+
%           &HD1    %:{SRCHARG1                        %&HD2
% SELECT
%  (S)    ACRONYM:          PRODUCT NAME:
   ×Z+    &ACRNML1         +&PRDL11                           +
                               +&PRDL21                           +
   @Z+    &ACRNML2         +&PRDL12                           +
                               +&PRDL22                           +
   #Z+    &ACRNML3         +&PRDL13                           +
                               +&PRDL23                           +
   $Z+    &ACRNML4         +&PRDL14                           +
                               +&PRDL24                           +
   ^Z+    &ACRNML5         +&PRDL15                           +
                               +&PRDL25                           +
   *Z+    &ACRNML6         +&PRDL16                           +
                               +&PRDL26                           +
   |Z+    &ACRNML7         +&PRDL17                           +
                               +&PRDL27                           +
   ~Z+    &ACRNML8         +&PRDL18                           +
                               +&PRDL28                           +
+
)INIT
  .HELP = SSRHLP32
  .CURSOR = ZCMD
  &SLCT = ' '
  &ZCMD = ' '
  .ZVARS = '(DEL,SL1,SL2,SL3,SL4,SL5,SL6,SL7,SL8)'
  &HD1 = ' '
  &HD2 = ' '
  &ARGTYP = OUTPUT
  &DELTYP = OUTPUT
  &DEL = ' '
  &SRCHTYP = TRUNC (&PARM,1)
  &SRCHTYP = .TRAIL
  &SRCHTYP = TRUNC (&SRCHTYP,1)
  IF (&SRCHTYP = P)
    &HD1 = 'START AT'
    &ARGTYP = INPUT
  IF (&SRCHTYP = F)
    &HD1 = '    FMID'
  IF (&SRCHTYP = T)
    &HD1 = ' PTF/MOD'
  IF (&SRCHTYP = A)
    &HD1 = ' ACRONYM'
  IF (&SRCHTYP = C)
    &HD1 = 'CATEGORY'
    &ARGTYP = INPUT
    &HD2 = '(OVERTYPE TO MODIFY)'
    IF (&ENDL = 00)
      &HD2 = '     DELETE (D) ===>'
      &DELTYP = INPUT
  IF (&SRCHTYP = V)
    &HD1 = '  VENDOR'
    &ARGTYP = INPUT
    &HD2 = '(OVERTYPE TO MODIFY)'
    IF (&ENDL = 00)
      &HD2 = '     DELETE (D) ===>'
      &DELTYP = INPUT
  IF (&ERRT = DI)
    .MSG = SSRMS018
  IF (&ERRT = N)
    .MSG = SSRMS007
  IF (&ERRT = E)
    .MSG = SSRMS017
  &TYP1 = INPUT
  IF (&ENDL = 00)
    &TYP1 = OUTPUT
  &TYP2 = INPUT
  IF (&ENDL = 00,01)
    &TYP2 = OUTPUT
  &TYP3 = INPUT
  IF (&ENDL = 00,01,02)
    &TYP3 = OUTPUT
  &TYP4 = INPUT
  IF (&ENDL = 00,01,02,03)
    &TYP4 = OUTPUT
  &TYP5 = INPUT
  IF (&ENDL = 00,01,02,03,04)
    &TYP5 = OUTPUT
  &TYP6 = INPUT
  IF (&ENDL = 00,01,02,03,04,05)
    &TYP6 = OUTPUT
  &TYP7 = INPUT
  IF (&ENDL = 00,01,02,03,04,05,06)
    &TYP7 = OUTPUT
  &TYP8 = INPUT
  IF (&ENDL = 00,01,02,03,04,05,06,07)
    &TYP8 = OUTPUT
)PROC
  VER (&SL1,LIST,S,MSG=SSRMS014)
  VER (&SL2,LIST,S,MSG=SSRMS014)
  VER (&SL3,LIST,S,MSG=SSRMS014)
  VER (&SL4,LIST,S,MSG=SSRMS014)
  VER (&SL5,LIST,S,MSG=SSRMS014)
  VER (&SL6,LIST,S,MSG=SSRMS014)
  VER (&SL7,LIST,S,MSG=SSRMS014)
  VER (&SL8,LIST,S,MSG=SSRMS014)
  IF (&SL1 = S)
    &SL1 = ' '
    &ERRL = 01
    &SLCT = Y
  IF (&SL2 = S)
    IF (&SLCT = Y)
      .MSG = SSRMS015
    IF (&SLCT ^= Y)
      &SL2 = ' '
      &ERRL = 02
      &SLCT = Y
  IF (&SL3 = S)
    IF (&SLCT = Y)
      .MSG = SSRMS015
    IF (&SLCT ^= Y)
      &SL3 = ' '
      &ERRL = 03
      &SLCT = Y
  IF (&SL4 = S)
    IF (&SLCT = Y)
      .MSG = SSRMS015
    IF (&SLCT ^= Y)
      &SL4 = ' '
      &ERRL = 04
      &SLCT = Y
  IF (&SL5 = S)
    IF (&SLCT = Y)
      .MSG = SSRMS015
    IF (&SLCT ^= Y)
      &SL5 = ' '
      &ERRL = 05
      &SLCT = Y
  IF (&SL6 = S)
    IF (&SLCT = Y)
      .MSG = SSRMS015
    IF (&SLCT ^= Y)
      &SL6 = ' '
      &ERRL = 06
      &SLCT = Y
  IF (&SL7 = S)
    IF (&SLCT = Y)
      .MSG = SSRMS015
    IF (&SLCT ^= Y)
      &SL7 = ' '
      &ERRL = 07
      &SLCT = Y
  IF (&SL8 = S)
    IF (&SLCT = Y)
      .MSG = SSRMS015
    IF (&SLCT ^= Y)
      &SL8 = ' '
      &ERRL = 08
      &SLCT = Y
  VER (&ZCMD,LIST,B,DOWN,F,UP,MSG=SSRMS016)
  IF (&ZCMD = DOWN)
    &ERRT = 'F '
  IF (&ZCMD = UP)
    &ERRT = 'B '
  IF (&ZCMD = F)
    &ERRT = 'F '
  IF (&ZCMD = B)
    &ERRT = 'B '
  IF (&SLCT = Y)
    &ERRT = 'S '
  IF (&DEL = D)
    &ERRT = 'DD'
  &SLCT = ' '
)END
