)ATTR
  % TYPE(TEXT) INTENS(HIGH) SKIP(ON)
  + TYPE(TEXT) INTENS(LOW) SKIP(ON)
  × TYPE(&TYP1) INTENS(&INTN1)
  @ TYPE(&TYP2) INTENS(&INTN2)
  # TYPE(&TYP3) INTENS(&INTN3)
  $ TYPE(&TYP4) INTENS(&INTN4)
  ~ TYPE(&TYP5) INTENS(&INTN5)
  | TYPE(&TYP6) INTENS(&INTN6)
  { TYPE(&TYP7) INTENS(&INTN7)
  } TYPE(&TYP8) INTENS(&INTN8)
)BODY
%-----------------SOFTWARE STATUS REPORT - PRODUCT HISTORY -------------
%COMMAND ===>_ZCMD
%
%                                                    DELETE PRODUCT (D)
%         ACRONYM ===>+_ACRNM           +
%    PRODUCT NAME ===>+_PRDN1                           +
%                 ===>+_PRDN2                           +
%          VENDOR ===>+_VENDR                           +
%        CATEGORY ===>+_CTGRY                           +
+
%DELETE:
%  (D)   ORDER:           RELEASE:         FMID:       PTF/MOD:      INS
+  ×Z+  ×ORDR1       +   ×RELS1       +   ×FMID1  +   ×PTFMDU1   +   ×M1
+  @Z+  @ORDR2       +   @RELS2       +   @FMID2  +   @PTFMDU2   +   @M2
+  #Z+  #ORDR3       +   #RELS3       +   #FMID3  +   #PTFMDU3   +   #M3
+  $Z+  $ORDR4       +   $RELS4       +   $FMID4  +   $PTFMDU4   +   $M4
+  ~Z+  ~ORDR5       +   ~RELS5       +   ~FMID5  +   ~PTFMDU5   +   ~M5
+  |Z+  |ORDR6       +   |RELS6       +   |FMID6  +   |PTFMDU6   +   |M6
+  {Z+  {ORDR7       +   {RELS7       +   {FMID7  +   {PTFMDU7   +   {M7
+  }Z+  }ORDR8       +   }RELS8       +   }FMID8  +   }PTFMDU8   +   }M8
+
)INIT
  .HELP = SSRHLP34
  .ZVARS = '(C9,C1,C2,C3,C4,C5,C6,C7,C8)'
  &ZCMD = ' '
  IF (&ERRT = 'E ')
    .MSG = SSRMS017
  IF (&ERRT = 'DP')
    .MSG = SSRMS011
  IF (&ERRT = 'DT')
    .MSG = SSRMS008
  IF (&ERRT = 'DE')
    .MSG = SSRMS004
  &TYP1 = INPUT
  &INTN1 = LOW
  IF (&ENDL = 00)
    &TYP1 = OUTPUT
  IF (&C1 = 'D')
    &TYP1 = OUTPUT
  IF (&ERRL = 01)
    &INTN1 = HIGH
  &TYP2 = INPUT
  &INTN2 = LOW
  IF (&ENDL = 00,01)
    &TYP2 = OUTPUT
  IF (&C2 = 'D')
    &TYP2 = OUTPUT
  IF (&ERRL = 02)
    &INTN2 = HIGH
  &TYP3 = INPUT
  &INTN3 = LOW
  IF (&ENDL = 00,01,02)
    &TYP3 = OUTPUT
  IF (&C3 = 'D')
    &TYP3 = OUTPUT
  IF (&ERRL = 03)
    &INTN3 = HIGH
  &TYP4 = INPUT
  &INTN4 = LOW
  IF (&ENDL = 00,01,02,03)
    &TYP4 = OUTPUT
  IF (&C4 = 'D')
    &TYP4 = OUTPUT
  IF (&ERRL = 04)
    &INTN4 = HIGH
  &TYP5 = INPUT
  &INTN5 = LOW
  IF (&ENDL = 00,01,02,03,04)
    &TYP5 = OUTPUT
  IF (&ERRL = 05)
    &INTN5 = HIGH
  IF (&C5 = 'D')
    &TYP5 = OUTPUT
  &TYP6 = INPUT
  &INTN6 = LOW
  IF (&ENDL = 00,01,02,03,04,05)
    &TYP6 = OUTPUT
  IF (&C6 = 'D')
    &TYP6 = OUTPUT
  IF (&ERRL = 06)
    &INTN6 = HIGH
  &TYP7 = INPUT
  &INTN7 = LOW
  IF (&ENDL = 00,01,02,03,04,05,06)
    &TYP7 = OUTPUT
  IF (&C7 = 'D')
    &TYP7 = OUTPUT
  IF (&ERRL = 07)
    &INTN7 = HIGH
  &TYP8 = INPUT
  &INTN8 = LOW
  IF (&ENDL = 00,01,02,03,04,05,06,07)
    &TYP8 = OUTPUT
  IF (&C8 = 'D')
    &TYP8 = OUTPUT
  IF (&ERRL = 08)
    &INTN8 = HIGH
  .CURSOR = ZCMD
)PROC
  VER (&PRDN1,NONBLANK)
  VER (&C9,LIST,D)
  VER (&C1,LIST,D)
  VER (&C2,LIST,D)
  VER (&C3,LIST,D)
  VER (&C4,LIST,D)
  VER (&C5,LIST,D)
  VER (&C6,LIST,D)
  VER (&C7,LIST,D)
  VER (&C8,LIST,D)
  IF (&ZCMD = DOWN)
    &ERRT = 'F '
  IF (&ZCMD = UP)
    &ERRT = 'B '
)END
