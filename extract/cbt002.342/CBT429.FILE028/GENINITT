            SET &OUT = &STR(//**************************************************// )
            PUTFILE OUT
            SET &OUT = &STR(//*   INITIALIZE STANDARD LABEL ON A TAPE          *// )
            PUTFILE OUT
            SET &OUT = &STR(//**************************************************// )
            PUTFILE OUT
            SET &OUT = &STR(//*                                                    )
            PUTFILE OUT
            SET &OUT = &STR(//LABEL   EXEC PGM=IEHINITT                            )
            PUTFILE OUT
            SET &OUT = &STR(//SYSPRINT DD  SYSOUT=*                                )
            PUTFILE OUT
            SET &OUT = &STR(//T        DD  UNIT=(TAPE,,DEFER)                      )
            PUTFILE OUT
            SET &OUT = &STR(//SYSIN    DD  *                                       )
            PUTFILE OUT
            SET &OUT = &STR(T INITT SER=&VOL,OWNER='SAMGOLOB',DISP=REWIND          )
            PUTFILE OUT
            SET &OUT = &STR(/*                                                     )
            PUTFILE OUT
