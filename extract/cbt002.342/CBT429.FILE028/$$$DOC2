//SYSPRNT2 DD  SYSOUT=*
//SYSUT1   DD  UNIT=TAPE,DISP=OLD,
//     VOL=(,RETAIN,,,SER=PL8608),
//     LABEL=(1,BLP,EXPDT=98000)
 
 
