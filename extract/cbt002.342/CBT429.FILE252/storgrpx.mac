/* BUILDING DEPOSIT, INC. SMS ACS STORGRP ROUTINE                   */
PROC &STORGRP
FILTLIST &ALLPOOLS INCLUDE(FDR*.**)
FILTLIST &HIPERF   INCLUDE('FASTREAD','FASTWRIT')
FILTLIST &SYSPARTH INCLUDE(PPLAN.**)
FILTLIST &SYSQARTH INCLUDE(QPLAN.**)
FILTLIST &SYSTARTH INCLUDE(TPLAN.**,MERC002.**,MERC101.**,MERC606.**)
FILTLIST &SYSTAPPL INCLUDE(APPL1.**)
FILTLIST &SYSPDB2  INCLUDE(DB2A.**)
FILTLIST &SYSQDB2  INCLUDE(DB2X.**)
FILTLIST &SYSTDB2  INCLUDE(DB2Y.**)
FILTLIST &SYSPSYS  INCLUDE(SYSP.**)
FILTLIST &SYSTSYS  INCLUDE(SYST.**)
FILTLIST &SYSMSYS  INCLUDE(SYSM.**)
FILTLIST &SYSPOOL  INCLUDE(SYS%.**)
FILTLIST &DB2V31   INCLUDE(DB2P.**)
SELECT
/* TRUE TEMPORARY DATASETS                                          */
   WHEN (&DSTYPE = 'TEMP') DO
      SET &STORGRP = 'VIOPOOL','TEMP'
      EXIT CODE(0)
      END
/* HIGH PERF (FASTREAD/FASTWRIT) DATASETS ON SYSP ONLY FOR CICS/SYS */
   WHEN (&STORCLAS = &HIPERF) DO
      SET &STORGRP = 'SYSPPERF','SYSPUSER'
      EXIT CODE(0)
      END
/* SYSTEM DATASETS                                                  */
   WHEN (&DSN = &SYSPSYS) DO
      SET &STORGRP = 'SYSPSYS'
      EXIT CODE(0)
      END
   WHEN (&DSN = &SYSTSYS) DO
      SET &STORGRP = 'SYSTSYS'
      EXIT CODE(0)
      END
   WHEN (&DSN = &SYSMSYS) DO
      SET &STORGRP = 'SYSPSYS'
      EXIT CODE(0)
      END
   WHEN (&DSN = &SYSPOOL) DO
      SET &STORGRP = 'SYSPSYS'
      EXIT CODE(0)
      END
/* MERC PLANNING ARTHUR DATABASE                                    */
   WHEN (&DSN = &SYSPARTH) DO
      SET &STORGRP = 'SYSPARTH'
      EXIT CODE(0)
      END
   WHEN (&DSN = &SYSQARTH) DO
      SET &STORGRP = 'SYSQARTH'
      EXIT CODE(0)
      END
   WHEN (&DSN = &SYSTARTH) DO
      SET &STORGRP = 'SYSTARTH'
      EXIT CODE(0)
      END
/* DB2 APPLICATION DATABASE                                         */
   WHEN (&DSN = &SYSPDB2) DO
      SET &STORGRP = 'SYSPDB2'
      EXIT CODE(0)
      END
   WHEN (&DSN = &SYSQDB2) DO
      SET &STORGRP = 'SYSQDB2'
      EXIT CODE(0)
      END
   WHEN (&DSN = &SYSTDB2) DO
      SET &STORGRP = 'SYSTDB2'
      EXIT CODE(0)
      END
   WHEN (&DSN = &SYSTAPPL) DO
      SET &STORGRP = 'SYSTAPPL'
      EXIT CODE(0)
      END
   WHEN (&DSN = &DB2V31) DO
      SET &STORGRP = 'DB2V31'
      EXIT CODE(0)
      END
/* SYSTEM UTIL,LIKE FDR, FILES TO BE ALLOC ON ANY VOLUME            */
   WHEN (&DSN = &ALLPOOLS) DO
      SET &STORGRP='SYSPUSER','USER',  +
                   'SYSPARTH','SYSQARTH','SYSTARTH',  +
                   'SYSPDB2','SYSQDB2','SYSTDB2',  +
                   'SYSPSYS','SYSTSYS','SYSMSYS',  +
                   'SYSPPERF','SYSTAPPL','DB2V31'
      EXIT CODE(0)
      END
/* ELSE GENERAL POOL                                                */
   OTHERWISE DO
      IF &USER = 'TECH105' THEN
         SET &STORGRP = 'USER','SYSPUSER'
         ELSE
      SET &STORGRP = 'USER'
      EXIT CODE(0)
      END
END
END
