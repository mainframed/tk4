)ATTR
  +  TYPE(TEXT) INTENS(LOW) SKIP(ON)
  %  TYPE(TEXT) INTENS(HIGH) SKIP(ON)
  _  TYPE(INPUT) INTENS(HIGH) CAPS(ON)
)BODY
%----------------------------  SYSLOG UTILITY  ------------------------
% COMMAND ===>_ZCMD
%
%   ENTER THE LOCATION IDENTIFIER FOR THE SYSLOG(S) YOU WISH TO BROWSE.
%
%   VALID IDENTIFIERS ARE 'REGN'      (WHERE 'N' = 1 TO 8).
%   VALID IDENTIFIERS ARE 'ADMN'      (WHERE 'N' = 1 - 3081 OR 2 - 3090)
%
%
%                LOCATION ===>_LOCID+
)INIT
  .HELP = TSYSLOG1
)PROC
 VER (&LOCID,LIST,ADM1,ADM2,REG1,REG2,REG3,REG4,REG5,REG6,REG7,REG8,MSG=
)END
