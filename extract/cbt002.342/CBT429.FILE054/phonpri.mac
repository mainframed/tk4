%------------------------  PHONE LIST PROCESSING  ----------------------
%SELECT OPTION ===>_ZCMD
%
%  1+- Edit the header or list datasets
%  2+- Print phone lists
%  3+- Edit other datasets
%
)INIT
  .HELP = ISR00003
  &ZHTOP = ISR00003   /* TUTORIAL TABLE OF CONTENTS        */
  &ZHINDEX = ISR91000 /* TUTORIAL INDEX - 1ST PAGE         */
)PROC
  &ZSEL = TRANS( TRUNC (&ZCMD,'.')
                1,'PANEL(PHONEDS)'
                2,'PANEL(PHONPRTS)'
                3,'PGM(ISREDIT)'
                G,'PANEL(GAME1)'
              ' ',' '
                *,'?' )
)END
