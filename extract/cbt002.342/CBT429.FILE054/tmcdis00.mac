)ATTR DEFAULT(%+Ö)
  % TYPE(TEXT) INTENS(HIGH) SKIP(ON)
  + TYPE(TEXT) INTENS(LOW) SKIP(ON)
  Ö TYPE(INPUT) INTENS(HIGH) CAPS(ON) JUST(LEFT)
  { TYPE(OUTPUT) INTENS(LOW) CAPS(ON) JUST(ASIS) PAD(' ')
  ^ TYPE(INPUT) INTENS(HIGH) CAPS(ON) JUST(ASIS) PAD('_')
)BODY
%------------------   Deluxe Regional TMC Selection Panel   ------------
%COMMAND ===>ÖZCMD

+  Enter the number of the regional TMC table to be displayed.
+
+      0.  Admin TMC.
+      1.  Region 1 TMC.
+      2.  Region 2 TMC.
+      3.  Region 3 TMC.
+      4.  Region 4 TMC.
+      5.  Region 5 TMC.
+      6.  Region 6 TMC.
+      7.  Region 7 TMC.
+      8.  Region 8 TMC.
+      P.  Pascal TMC.
+
+  Press%end+key to return to main menu
)INIT
  &ZCMD = &Z
  .HELP = TMCTUT00
)PROC
  VER (&ZCMD,LIST,0,1,2,3,4,5,6,7,8,P)
  IF (&ZCMD = 0)
      &TMCTAB = 'TMCTAB$'
      &TMCREG = 'ADMIN'
  IF (&ZCMD = 1)
      &TMCTAB = 'TMCTAB1'
      &TMCREG = 'REG-1'
  IF (&ZCMD = 2)
      &TMCTAB = 'TMCTAB2'
      &TMCREG = 'REG-2'
  IF (&ZCMD = 3)
      &TMCTAB = 'TMCTAB3'
      &TMCREG = 'REG-3'
  IF (&ZCMD = 4)
      &TMCTAB = 'TMCTAB4'
      &TMCREG = 'REG-4'
  IF (&ZCMD = 5)
      &TMCTAB = 'TMCTAB5'
      &TMCREG = 'REG-5'
  IF (&ZCMD = 6)
      &TMCTAB = 'TMCTAB6'
      &TMCREG = 'REG-6'
  IF (&ZCMD = 7)
      &TMCTAB = 'TMCTAB7'
      &TMCREG = 'REG-7'
  IF (&ZCMD = 8)
      &TMCTAB = 'TMCTAB8'
      &TMCREG = 'REG-8'
  IF (&ZCMD = P)
      &TMCTAB = 'TMCTABP'
      &TMCREG = 'REG-P'
)END
