)ATTR DEFAULT(%+Ö)
  % TYPE(TEXT) INTENS(LOW) SKIP(ON)
  + TYPE(TEXT) INTENS(HIGH) SKIP(ON)
  Ö TYPE(INPUT) INTENS(HIGH) CAPS(ON) JUST(LEFT)
  { TYPE(INPUT) INTENS(HIGH) CAPS(ON) JUST(LEFT) PAD('_')
  } TYPE(INPUT) INTENS(HIGH) CAPS(ON) JUST(RIGHT) PAD('_')
  < TYPE(OUTPUT) INTENS(HIGH) CAPS(ON) JUST(LEFT)
)BODY
+---------------   DELUXE User Registration (ADD a TSO User)   ---------
+COMMAND ===>ÖZCMD
%
%User-id:<USERID  %
%
%Attributes:{TSOATT0 %  {TSOATT1 %  {TSOATT2 %  {TSOATT3 %    Dest:{TSOD
%
%Proc:{TSONAM0 %    Size:}TSOSIZ0 %K   Unit:{TSOUNT0 %    Maxsize:}TSOMA
%
%
%Select Region(s) in which to add user
%Reg1{Z%  Reg2{Z%  Reg3{Z%  Reg4{Z%  Reg5{Z%  Reg6{Z%  Reg7{Z%  Reg8{Z%
%
%
%   Press+ENTER%key to add TSO user; press+END%key to cancel request
%
%
%
%
%
%
%
%
)INIT
  .HELP = USRTUT07
  .ZVARS = '(TSO1 TSO2 TSO3 TSO4 TSO5 TSO6 TSO7 TSO8 TSOA)'
  .CURSOR = ZCMD
 IF (&TSOATT0 = &Z)
     &TSOATT0 = 'NOACCT'
 IF (&TSOATT1 = &Z)
     &TSOATT1 = 'NOJCL'
 IF (&TSOATT2 = &Z)
     &TSOATT2 = 'NOMOUNT'
 IF (&TSOATT3 = &Z)
     &TSOATT3 = 'NOOPER'
 IF (&TSODEST = &Z)
     &TSODEST = 'NODEST'
 IF (&TSONAM0 = &Z)
     &TSONAM0 = '$$$DFLT1'
 IF (&TSOSIZ0 = &Z)
     &TSOSIZ0 = '2048'
 IF (&TSOUNT0 = &Z)
     &TSOUNT0 = 'SYSTSO'
 IF (&TSOMAXR = &Z)
     &TSOMAXR = '4096'
 IF (&RACF1 = Y)
     &TSO1 = Y
 IF (&RACF2 = Y)
     &TSO2 = Y
 IF (&RACF3 = Y)
     &TSO3 = Y
 IF (&RACF4 = Y)
     &TSO4 = Y
 IF (&RACF5 = Y)
     &TSO5 = Y
 IF (&RACF6 = Y)
     &TSO6 = Y
 IF (&RACF7 = Y)
     &TSO7 = Y
 IF (&RACF8 = Y)
     &TSO8 = Y
)PROC
  VER (&TSOATT0,LIST,NOACCT,ACCT)
  VER (&TSOATT1,LIST,NOJCL,JCL)
  VER (&TSOATT2,LIST,NOMOUNT,MOUNT)
  VER (&TSOATT3,LIST,NOOPER,OPER)
  VER (&TSOSIZ0,RANGE,0,65535)
  VER (&TSO1,LIST,N,Y)
  VER (&TSO2,LIST,N,Y)
  VER (&TSO3,LIST,N,Y)
  VER (&TSO4,LIST,N,Y)
  VER (&TSO5,LIST,N,Y)
  VER (&TSO6,LIST,N,Y)
  VER (&TSO7,LIST,N,Y)
  VER (&TSO8,LIST,N,Y)
  VER (&TSOA,LIST,N,Y)
  IF (&TSOMAXR = &Z)
      &TSOMAXR = 'NOLIM'
  IF (&TSOMAXR = 'NOLIM')
      &TSOMAXR = 0
  VER (&TSOMAXR,RANGE,0,65535)
  IF (&TSOMAXR = 0)
      &TSOMAXR = 'NOLIM'
  IF (&TSOA ^= &Z)
      &TSO1 = Y
      &TSO2 = Y
      &TSO3 = Y
      &TSO4 = Y
      &TSO5 = Y
      &TSO6 = Y
      &TSO7 = Y
      &TSO8 = Y
  IF (&TSO1 = Y)
      IF (&RACF1 ^= Y)
          &USRM01S = 'Invalid specification'
          &USRM01L = 'User will also be defined to region 1 RACF.'
          .MSG = USER001
          &RACF1 = Y
  IF (&TSO2 = Y)
      IF (&RACF2 ^= Y)
          &USRM02S = 'Invalid specification'
          &USRM02L = 'User will also be defined to region 2 RACF.'
          .MSG = USER002
          &RACF2 = Y
  IF (&TSO3 = Y)
      IF (&RACF3 ^= Y)
          &USRM03S = 'Invalid specification'
          &USRM03L = 'User will also be defined to region 3 RACF.'
          .MSG = USER003
          &RACF3 = Y
  IF (&TSO4 = Y)
      IF (&RACF4 ^= Y)
          &USRM04S = 'Invalid specification'
          &USRM04L = 'User will also be defined to region 4 RACF.'
          .MSG = USER004
          &RACF4 = Y
  IF (&TSO5 = Y)
      IF (&RACF5 ^= Y)
          &USRM05S = 'Invalid specification'
          &USRM05L = 'User will also be defined to region 5 RACF.'
          .MSG = USER005
          &RACF5 = Y
  IF (&TSO6 = Y)
      IF (&RACF6 ^= Y)
          &USRM06S = 'Invalid specification'
          &USRM06L = 'User will also be defined to region 6 RACF.'
          .MSG = USER006
          &RACF6 = Y
  IF (&TSO7 = Y)
      IF (&RACF7 ^= Y)
          &USRM07S = 'Invalid specification'
          &USRM07L = 'User will also be defined to region 7 RACF.'
          .MSG = USER007
          &RACF7 = Y
  IF (&TSO8 = Y)
      IF (&RACF8 ^= Y)
          &USRM08S = 'Invalid specification'
          &USRM08L = 'User will also be defined to region 8 RACF.'
          .MSG = USER008
          &RACF8 = Y
)END
