          &USRM22L = 'RESUME date must be of the form ''&TODAY''.'
          VER (&UPDRESC,PICT,NN/NN/NN,MSG=USER022)
  IF (&UPDREVD ^= &CREVD)
      IF (&UPDREVD ^= &Z)
