          &USRM19L = 'REVOKE date must be of the form ''&TODAY''.'
          VER (&UPDREVD,PICT,NN/NN/NN,MSG=USER019)
  IF (&UPDRESD ^= &CRESD)
      IF (&UPDRESD ^= &Z)
          &USRM22S = 'Invalid RESUME date'
          &USRM22L = 'RESUME date must be of the form ''&TODAY''.'
          VER (&UPDRESD,PICT,NN/NN/NN,MSG=USER022)
  IF (&UPDREVE ^= &CREVE)
      IF (&UPDREVE ^= &Z)
