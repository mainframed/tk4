               PDRES UPDDFLT UPDRAT0 UPDRAT1 UPDRAT2 RACFATT3 UPDDATA U+
               PDEXT)'
 .ZVARS = &ZLIST
 &ZCMD = &Z
 &R$ = &Z
 &R1 = &Z
 &R2 = &Z
 &R3 = &Z
 &R4 = &Z
 &R5 = &Z
 &R6 = &Z
 &R7 = &Z
 &R8 = &Z
 IF (&UPDRACF$ = Y)
     &R$ = '*'
