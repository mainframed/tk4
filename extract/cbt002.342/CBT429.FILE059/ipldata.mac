         BAL   R8,DATECONV
* TIME
         LA    R4,336(,R2)
         L     R1,0(,R4)
         SLR   R0,R0
         D     R0,=F'360000'
         CVD   R1,DOUBLE
         OI    DOUBLE+7,X'0F'
         UNPK  IPLTIME(2),DOUBLE+6(2)
         LR    R1,R0
         SLR   R0,R0
         D     R0,=F'6000'
         CVD   R1,DOUBLE
         OI    DOUBLE+7,X'0F'
         UNPK  IPLTIME+3(2),DOUBLE+6(2)
