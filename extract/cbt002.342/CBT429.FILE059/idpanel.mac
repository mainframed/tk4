)ATTR DEFAULT(%+_)
    % TYPE(TEXT) INTENS(HIGH) COLOR(TURQ)
    ^ TYPE(TEXT) INTENS(HIGH) COLOR(YELLOW)
    + TYPE(TEXT)  INTENS(HIGH) COLOR(PINK)
)BODY
^----------------------- ^ISPF/PDF Basic Variable Display  -------------
%OPTION  ===>_ZCMD
                    %USERID     - &ZUSER
                    %PREFIX     - &ZPREFIX
                    %SYSID      - &SYSID
  ^SCREEN - &ZSCREEN                             ^SCR DEPTH - &ZSCREEND
                    +TIME       - &ZTIME
                    +DATE       - &ZDATE
                    +JULIAN     - &ZJDATE

                    %PROCEDURE  - &ZLOGON
                    %APPLID     - &ZAPPLID

                    +TERMINAL   - &ZTERM
                    +PF KEYS    - &ZKEYS
                    +COLORS     - &ZCOLORS

                %ISPF Environment   -   &ZENVIR
                %Log  Dataset - &ZLOGNAME
                %List Dataset - &ZLSTNAME
)INIT
  VPUT (ZHTOP,ZHINDEX,ZSCLMPRJ) PROFILE /*             @L1C*/
  IF (&SYSID = &Z)
    .RESP = ENTER
    &ZCMD = SYD
)PROC
&ZQ = &Z
  IF (&ZCMD ^= ' ')
    &ZQ = TRUNC(&ZCMD,'.')
    IF (&ZQ = ' ')
      .MSG = ISRU000
  &ZSEL = TRANS( &ZQ
              SYD,'CMD(%SMCASID)'
                0,'PANEL(ISPOPTA)'
                1,'PGM(ISRBRO) PARM(ISRBRO01)'
                2,'PGM(ISREDIT) PARM(P,ISREDM01)'
                3,'PANEL(ISRUTIL)'
                4,'PANEL(ISRFPA)'
                5,'PGM(ISRJB1) PARM(ISRJPA) NOCHECK'
                6,'PGM(ISRPTC)'
                7,'PGM(ISPYXDR) PARM(ISR) NOCHECK'
                8,'PANEL(ISRLPRIM)'
                9,'PANEL(ISRDIIS)'
               10,'PGM(ISRSCLM) NOCHECK'
                F,'PANEL(IFAMU01) NEWAPPL(FAXE)'
                O,'PANEL(NEWPNL) NEWAPPL(NONI)'
                P,'PANEL(PDS@PRIM)'
                C,'PGM(ISPTUTOR) PARM(ISR00005)'
                T,'PGM(ISPTUTOR) PARM(ISR00000)'
              ' ',' '
                X,'EXIT'
                *,'?' )
  &ZTRAIL = .TRAIL
)END
