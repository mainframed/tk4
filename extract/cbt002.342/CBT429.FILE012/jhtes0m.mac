)CM
)CM   ***** RENAME *****
)SEL &O = 5 × &O = 6
  RENAME       DSNAME=&TDSN,                                           X
)SEL &O = 5 && &TMEM ^= &Z
               MEMBER=&TMEM,                                           X
)ENDSEL
               VOL=&TDEV=(&TVOL),                                      X
               NEWNAME=&TNEW
)ENDSEL
)CM
)CM   ***** SCRATCH *****
)SEL &O = 1 × &O = 2
  SCRATCH      DSNAME=&TDSN,                                           X
)SEL &TMEM ^= &Z
               MEMBER=&TMEM,                                           X
)ENDSEL
               VOL=&TDEV=(&TVOL),PURGE
)ENDSEL
)CM
)CM   ***** UNCATLG *****
)SEL &O = 2 × &O = 3 × &O = 6
  UNCATLG      DSNAME=&TDSN
)ENDSEL
)CM
)CM   ***** CATLG *****
)SEL &O = 4
  CATLG        DSNAME=&TDSN,                                           X
)ENDSEL
)SEL &O = 6
  CATLG        DSNAME=&TNEW,                                           X
)ENDSEL
)SEL &O = 4 × &O = 6
               VOL=&TDEV=(&TVOL)
)ENDSEL
