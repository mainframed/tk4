)SET S = &S + 1
//*---------------------------------------------------------------------
//*  DEFINE GDG INDEX FOR &TDSN LIMIT(&LIM) &EMP
//*---------------------------------------------------------------------
//GDG&S    EXEC PGM=IDCAMS,REGION=256K
//SYSPRINT DD  SYSOUT=*
//SYSIN    DD  *
)SEL &GDOWN = &Z
  DEF GDG (NAME(&TDSN) -
           LIMIT(&LIM) SCRATCH &EMP)
)ENDSEL
)SEL &GDOWN ^= &Z
  DEF GDG (NAME(&TDSN) -
           LIMIT(&LIM) SCRATCH &EMP OWNER(&GDOWN))
)ENDSEL
  LISTC ENTRY(&TDSN) ALL
