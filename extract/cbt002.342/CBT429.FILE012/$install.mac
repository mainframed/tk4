//MINSTALL JOB (D72667,A,SMU),'MMURA      ',MSGLEVEL=1,
//             CLASS=L,MSGCLASS=Y,NOTIFY=MC00200
/*JOBPARM R=B04A
//*
//*   SAMPLE INSTALLATION JOB.  CHANGE PARAMETERS WHERE NECESSARY.
//*
//*   NOTE: A MODIFIED COPY OF PDF'S ISR@PRIM IS IN THE
//*         DISTRIBUTION LIBRARY BUT IS NOT COPIED BY THIS JOB.
//*         YOU MUST COPY OR ADAPT IT TO A MENU AT YOUR INSTALLATION
//*         TO USE JH#PDF8.
//*
//STEP1    EXEC PGM=IEBCOPY,REGION=384K
//SYSPRINT DD  SYSOUT=*
//MODS     DD  DISP=SHR,DSN=dist.library                 <===
//PANELS   DD  DISP=SHR,DSN=panel.library                <===
//SKELS    DD  DISP=SHR,DSN=skel.library                 <===
//MSGS     DD  DISP=SHR,DSN=msg.library                  <===
//CLISTS   DD  DISP=SHR,DSN=clist.library                <===
//SYSUT3   DD  UNIT=DISK,SPACE=(TRK,(10,10))
//SYSUT4   DD  UNIT=DISK,SPACE=(TRK,(10,10))
//SYSIN    DD  *
 C O=PANELS,I=((MODS,R))
 S M=JHABH@0,JHABH@1                SAMPLE JH#PDF8 TUTORIAL PANELS
 S M=JHABH@2,JHABH@3                SAMPLE JH#PDF8 TUTORIAL PANELS
 S M=JHABH01,JHABH02                SAMPLE JH#PDF8...
 S M=JHABH03,JHABH03A,JHABH03B         SUBOPTION TUTORIAL PANELS
 S M=JHABH05,JHABH07,JHABH07A       SAMPLE JH#PDF8...
 S M=JHABH09,JHABH09A,JHABH10          SUBOPTION TUTORIAL PANELS
 S M=JHABP@A,JHABP@B                SAMPLE JH#PDF8 PESUDO-SEL. PANELS
 S M=JHABP0A,JHABP0G                SAMPLE JH#PDF8 SUBOPTION PANELS
 S M=JHABP0Z                        SAMPLE JH#PDF8 SUBOPTION PANELS
 S M=JHABP01,JHABP02,JHABP03        SAMPLE JH#PDF8 SUBOPTION PANELS
 S M=JHABP05,JHABP07,JHABP09        SAMPLE JH#PDF8 SUBOPTION PANELS
 S M=JHABP10,JHABP14                SAMPLE JH#PDF8 SUBOPTION PANELS
 S M=JHABP18,JHABP19,JHABP19V       SAMPLE JH#PDF8 SUBOPTION PANELS
 S M=JHABROB                        JH BROWSE PANEL
 S M=JHAEFR01                       EDIT PANEL FOR JH#PDF8/JH#EDTMP
 S M=JHAYP110,JHAYP111              PANELS FOR JH#YDTBL
 S M=JHAYP112,JHAYP113              PANELS FOR JH#YDTBL
 S M=JHAYP12,JHAYP12B               PANELS FOR JH#YCTBL
 S M=JHAYP14@,JHAYP141,JHAYP142     PANELS FOR FG SEL. PANEL UPDATE
 S M=JHTEPE01                       JH EDIT ENTRY PANEL
 S M=JHTEPE31,JHTEPE32              PANELS FOR JH#TEDPR
 S M=JHTEPE41,JHTEPE42              PANELS FOR JH#TEDRT
 S M=JHTEP0M,JHTEP0M2               PANELS FOR JH#TPRGM
 S M=PANELID                        SAMPLE PANEL TO TURN PANELID ON
 S M=XALTK                          HELP PANEL FOR SECONDARY PFKS
 S M=ZALTKA,ZALTKB,ZALTKC           PANELS FOR SECONDARY PFKS
 S M=ZCMDTBL                        TABLE DISPLAY PANEL FOR TSCMDTB
 C O=SKELS,I=((MODS,R))
 S M=JHABS@C,JHABS@I                SAMPLE JH#PDF8 INIT AND TERM SKELS
 S M=JHABS0A,JHABS0G,JHABS0Z        SAMPLE JH#PDF8 SUBOPTION SKELS
 S M=JHABS01,JHABS02,JHABS03        SAMPLE JH#PDF8 SUBOPTION SKELS
 S M=JHABS05,JHABS07,JHABS09        SAMPLE JH#PDF8 SUBOPTION SKELS
 S M=JHABS10,JHABS14                SAMPLE JH#PDF8 SUBOPTION SKELS
 S M=JHABS18,JHABS19                SAMPLE JH#PDF8 SUBOPTION SKELS
 S M=JHTES0M                        SKELETON FOR JH#TPRGM
 C O=MSGS,I=((MODS,R))
 S M=JHA@M00,JHA@M01,JHA@M02        GENERAL PURPOSE MESSAGES
 S M=JHA@M03,JHA@M04                GENERAL PURPOSE MESSAGES
 S M=JHABM00                        SAMPLE JH#PDF8 MESSAGES
 S M=JHABM01,JHABM02,JHABM03        SAMPLE JH#PDF8 SUBOPTION MESSAGES
 S M=JHABM05,JHABM07,JHABM09        SAMPLE JH#PDF8 SUBOPTION MESSAGES
 S M=JHABM10,JHABM14                SAMPLE JH#PDF8 SUBOPTION MESSAGES
 S M=JHABM18,JHABM19                SAMPLE JH#PDF8 SUBOPTION MESSAGES
 S M=JHAYM11,JHAYM12                MSGS FOR JH#YCTBL, JH#YDTBL
 S M=JHAYM14                        MESSAGES FOR FG SEL. PANEL UPDATE
 S M=JHTEM01                        JH EDIT ENTRY PANEL MSGS
 C O=CLISTS,I=((MODS,R))
 S M=ISRALTK                        CLIST FOR SECONDARY PFKS
 S M=ISRUOLJH                       JH PDF 3.8 (ISRUOL) CLIST
 S M=JH#TEDPR                       CLIST TO DISPLAY EDIT PROFILE
 S M=JH#TEDRT                       CLIST TO DISPL/MODIFY EDIT RECOV TB
 S M=JH#TPRGM                       CLIST TO INVOKE IEHPROGM FOREGROUND
 S M=JH#YCTBL                       CLIST FOR TABLE RECONSTRUCTION
 S M=JH#YDTBL                       CLIST TO DISPLAY TABLE CONTENTS
 S M=JH#YPUP,JH#YPUPG               CLISTS FOR FG SEL. PANEL UPDATE
 S M=TSCMDTB                        CLIST TO DISPLAY COMMAND TABLE
/*
//*
//STEP2   EXEC ASMHCL,CLASS='*',
//    PARM.LKED='RENT,REFR,XREF,LIST,LET'
//ASM.SYSLIB DD DISP=SHR,DSN=SYS1.MACLIB
//           DD DISP=SHR,DSN=dist.library                <===
//ASM.SYSIN  DD DISP=SHR,DSN=dist.library(JH#PDF8)       <===
//LKED.SYSLMOD DD DISP=SHR,DSN=target.loadlib            <===
//LKED.SYSIN DD *
   NAME JH#PDF8(R)
/*
//*
//STEP3   EXEC ASMHCL,CLASS='*',
//    PARM.LKED='RENT,REFR,XREF,LIST,LET'
//ASM.SYSLIB DD DISP=SHR,DSN=SYS1.MACLIB
//           DD DISP=SHR,DSN=dist.library                <===
//ASM.SYSIN  DD DISP=SHR,DSN=dist.library(JH#GTDSN)      <===
//LKED.SYSLMOD DD DISP=SHR,DSN=target.loadlib            <===
//LKED.SYSIN DD *
   NAME JH#GTDSN(R)
/*
//*
//STEP4   EXEC ASMHCL,CLASS='*',
//    PARM.LKED='RENT,REFR,XREF,LIST,LET'
//ASM.SYSLIB DD DISP=SHR,DSN=SYS1.MACLIB
//           DD DISP=SHR,DSN=dist.library                <===
//ASM.SYSIN  DD DISP=SHR,DSN=dist.library(JH#EDTMP)      <===
//LKED.SYSLMOD DD DISP=SHR,DSN=target.loadlib            <===
//LKED.SYSIN DD *
   NAME JH#EDTMP(R)
/*
