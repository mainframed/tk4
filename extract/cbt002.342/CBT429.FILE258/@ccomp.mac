PROC 1 PGM OMD
  CONTROL ASIS NOMSG NOLIST NOSYMLIST
  FREE FI(INC)
  ALLOC FI(INC) DA('SASDDF.A.SOURCE') SHR
  DELETE TEMP.LIST
  LC370 'SASDDF.A.SOURCE(&PGM)' +
    OBJECT('''SASDDF.A.OBJ(&PGM)''') +
    DBGLIB('''SASDDF.A.DBGLIB(&PGM)''') PRINT(TEMP) &OMD
  IF &LASTCC <= 4 THEN +
    CLK370 'SASDDF.A.OBJ(&PGM)' +
      LOAD('''SASDDF.A.LOADLIB(&PGM)''')
  FREE FI(INC)
  ISPEXEC BROWSE DATASET(TEMP.LIST)
EXIT
