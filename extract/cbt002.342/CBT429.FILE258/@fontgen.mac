PROC 2 INSYM OUTARRAY
IF &INSYM ^= &OUTARRAY THEN DO
  /* PROCESSES A FONT DEFINITION TO A C-LANGUAGE STRUCTURE */
  ALLOC F(IN) DA('SASDDF.A.SOURCE(&INSYM)') SHR
  ALLOC F(OUT) DA('SASDDF.A.SOURCE(&OUTARRAY)') SHR
  CALL 'SASDDF.A.LOADLIB(FONT)' '&OUTARRAY'
  FREE F(IN)
  FREE F(OUT)
  END
ELSE +
  WRITE ERROR: PARAMETERS CAN NOT BE THE SAME.
EXIT
