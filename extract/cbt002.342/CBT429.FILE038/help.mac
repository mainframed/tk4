      DATA IST010 /'FREE','ZE  ','    '/
      DATA IST011 /'GRAP','HICS','    '/
      DATA IST012 /'HINT','S   ','    '/
      DATA IST013 /'HELP','    ','    '/
      DATA IST014 /'IMPU','LSE ','    '/
