CFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFF
C
      FUNCTION ISGN (IVAL)
C
C     SET FUNCTION ACCORDING TO SIGN  1 = POSITIVE
C                                     0 = ZERO
C                                    -1 = NEGATIVE
C
CFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFF
      IF (IVAL) 10 , 20 , 30
10    ISGN = -1
      GO TO 40
20    ISGN = 0
      GO TO 40
30    ISGN = +1
40    RETURN
      END
