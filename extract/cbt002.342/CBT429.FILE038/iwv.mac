      FUNCTION IWV(ILEN)
C
C  RETURN WORD LENGTH GIVEN A CERTAIN VARIABLE LENGTH
C
C  *IBM*
C
CFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFF
      COMMON  /CWORD/ IHOLD
      IWV = (ILEN+3)/4
      IHOLD = ILEN
      RETURN
      END
