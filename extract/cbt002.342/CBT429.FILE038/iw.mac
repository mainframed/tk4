CFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFF
C
      FUNCTION IW(ILEN)
C
C  RETURN THE NUMBER OF WORDS NEEDED FOR A CERTAIN STRING
C
C  *IBM* DEPENDANT MODULE
C
CFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFFF
      COMMON  /CWORD/ IHOLD
      IW = (ILEN+3)/4
      IHOLD = ILEN
      RETURN
      END
