CCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCC
C
      SUBROUTINE PRMSG (IMSG,IDIM)
C
C  DISPLAY A CHARACTER MSG TO OUT UNIT
C
CCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCC
C
      COMMON /OUTUNT/ IOUTT
      DIMENSION IMSG(IDIM)
      DIMENSION IFORM (4)
      DATA     IFORM /'(1X,',-1,-1,')' /
      I = IWI(IDUMMY)
      CALL SETFOR (I,IFORM(2),IFORM(3))
      WRITE (IOUTT,IFORM) IMSG
      RETURN
      END
