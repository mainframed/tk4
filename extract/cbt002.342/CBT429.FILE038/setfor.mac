CCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCC
C
      SUBROUTINE SETFOR (ILEN,IPOS1C,IPOS2C)
C
C  SET FORMAT STATEMENT FOR A GIVEN LENGTH STRING
C
CCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCC
      DIMENSION IXA04(25),IAX(4)
      DATA IXA04 / '1A4','2A4','3A4','4A4','5A4','6A4',
     *     '7A4','8A4','9A4','10A4','11A4','12A4','13A4','14A4',
     *     '15A4','16A4','17A4','18A4','19A4','20A4','21A4','22A4',
     *     '23A4','24A4','25A4' /
      DATA IBLK / ' '/,IHOLDR /'1X'/
      DATA IAX / ' ',',A1 ',',A2 ',',A3 '/
      IL = ILEN
      IF (IL.LT.1) IL = 1
      IPOS1 = IHOLDR
      IPOS2 = IBLK
      IF (IL.LE.0) GO TO 20
      IREMAN = MOD (IL,4)
      IL = IL - IREMAN
      IF (IL.LE.0) GO TO 10
      IPOS1 = IXA04(IL/4)
10    CONTINUE
      IPOS2 = IAX(IREMAN+1)
20    CONTINUE
      IPOS1C = IPOS1
      IPOS2C = IPOS2
      RETURN
      END
