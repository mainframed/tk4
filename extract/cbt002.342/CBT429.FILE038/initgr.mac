CCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCC
C
      SUBROUTINE INITGR
C
C INIT GRAFICS VARIABLES AND SETUP WORK FILE 97
C
CCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCC
C
      COMMON /WORKQ/ IGCNT, INQUE(10)
C
      COMMON /GRAPH/ IOK
C
      DIMENSION I(20)
      IGCNT = 0
      REWIND 25
      REWIND 97
5     READ (25,1001,END=1000) I
      WRITE (97,1001) I
      GO TO 5
1000  CONTINUE
      CALL SZTERM
      CALL ADDQUE ('INTR')
      RETURN
1001  FORMAT (20A4)
      END
