CCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCC
C
      SUBROUTINE PRMSG1 (IMSG,IDIM)
C
C  DISPLAY AN ARRAY IN A1 FORMAT
C
CCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCCC
C
      COMMON /OUTUNT/ IOUTT
      DIMENSION IMSG(IDIM)
      I = IDIM
      IF (I.GT.80) I = 80
      WRITE (IOUTT,50000) (IMSG(N),N=1,I)
      RETURN
50000 FORMAT (' ',80A1)
      END
