//*--------------------------------------------------------*
//* *  INSTALL THE 'REVIEW' TSO COMMAND                    *
//*--------------------------------------------------------*
//MACLIB   EXEC PGM=IEBUPDTE,PARM=NEW
//SYSPRINT DD DUMMY
//SYSIN    DD DSN=SHARE.TSOSRC(MACROS),DISP=SHR
//SYSUT2   DD DSN=&&MACLIB,UNIT=SYSDA,DISP=(NEW,PASS),
//            SPACE=(CYL,(5,5,25),RLSE),
//            DCB=(RECFM=FB,LRECL=80,BLKSIZE=3120)
//*--------------------------------------------------------*
//REVIEW   EXEC ASMHCL,COND=(0,NE),REGION=1024K,
//         PARM.ASM='OBJECT,NODECK,TERM,NOLIST',
//         PARM.LKED='LIST,MAP,RENT,REUS,REFR'
//SYSLIB   DD DSN=SYS1.MACLIB,DISP=SHR
//         DD DSN=&&MACLIB,DISP=(OLD,DELETE)
//SYSTERM  DD SYSOUT=*
//SYSIN    DD DSN=SHARE.TSOSRC(REVIEW),DISP=SHR
//         DD DSN=SHARE.TSOSRC(REVSMF),DISP=SHR
//LKED.SYSLMOD DD DSN=SYS1.SHARE.LOAD,UNIT=,DISP=SHR,SPACE=
//LKED.SYSIN DD *
 ALIAS REV
 NAME REVIEW(R)
/*
