//*----------------------------------------------------------------*
//*      INSTALL PROCESS IS USING THE BATCH FACILITY OF ASM-H. IF
//*      ONLY HAVE ASM-XF YOU WILL HAVE TO DO SOME CHANGES.
//*      NOTE:  THIS NEEDS SOME CHECKOUT ON NEW VTOCS, ETC. LAST
//*             USED IT ON MVS/SP 1.3.4 PRE-DFP (DID HAVE DFDS).
//*----------------------------------------------------------------*
//MAKEPDS  EXEC PGM=PDSLOAD,PARM='UPDTE(><)'
//SYSPRINT DD DUMMY
//SYSUT2   DD DSN=&&VTOCSRC,UNIT=VIO,DISP=(NEW,PASS),
//            SPACE=(CYL,(5,5,12),RLSE),
//            DCB=(RECFM=FB,LRECL=80,BLKSIZE=3120,BUFNO=1)
//SYSIN    DD DSN=SHARE.TSOSRC(VTOC),DISP=SHR
//*---------------------------------------------------------------*
//ASM      EXEC PGM=IEV90,REGION=1024K,COND=(0,NE),
//         PARM='TERM,ALIGN,NODECK,OBJECT,BATCH,NOLIST'
//SYSLIB   DD DSN=&&VTOCSRC,DISP=(OLD,DELETE)
//         DD DSN=SYS1.AMODGEN,DISP=SHR           NEED SU60 MACROS
//         DD DSN=SYS1.MACLIB,DISP=SHR            NEED SU60 MACROS
//SYSUT1   DD UNIT=VIO,SPACE=(1024,(140,140),,,ROUND),
//            DCB=BUFNO=1
//SYSPRINT DD SYSOUT=*
//SYSTERM  DD SYSOUT=*
//SYSLIN   DD DSN=&&OBJ,UNIT=VIO,SPACE=(CYL,(5,2)),
//            DCB=(BLKSIZE=3040,LRECL=80,RECFM=FBS,BUFNO=1),
//            DISP=(NEW,PASS)
//SYSIN    DD DSN=&&VTOCSRC(VTOCCHEK),DISP=(OLD,DELETE),
//            UNIT=AFF=SYSLIB,VOL=REF=*.SYSLIB
//         DD DSN=&&VTOCSRC(VTOCEXCP),DISP=(OLD,DELETE),
//            UNIT=AFF=SYSLIB,VOL=REF=*.SYSLIB
//         DD DSN=&&VTOCSRC(VTOCFORM),DISP=(OLD,DELETE),
//            UNIT=AFF=SYSLIB,VOL=REF=*.SYSLIB
//         DD DSN=&&VTOCSRC(VTOCMSGX),DISP=(OLD,DELETE),
//            UNIT=AFF=SYSLIB,VOL=REF=*.SYSLIB
//         DD DSN=&&VTOCSRC(VTOCPRNT),DISP=(OLD,DELETE),
//            UNIT=AFF=SYSLIB,VOL=REF=*.SYSLIB
//         DD DSN=&&VTOCSRC(VTOCSORT),DISP=(OLD,DELETE),
//            UNIT=AFF=SYSLIB,VOL=REF=*.SYSLIB
//         DD DSN=&&VTOCSRC(VTOC),DISP=(OLD,DELETE),
//            UNIT=AFF=SYSLIB,VOL=REF=*.SYSLIB
//*-----------------------------------------------------------------*
//LKED     EXEC PGM=IEWL,REGION=1024K,
//         PARM='XREF,LET,LIST,AC=0,SIZE=(140K,6400)',
//         COND=(0,NE)
//SYSLIB   DD DSN=SYS1.SHARE.LOAD,DISP=SHR
//SYSLIN   DD DSN=&&OBJ,DISP=(OLD,DELETE)
//         DD DDNAME=SYSIN
//SYSLMOD  DD DSN=SYS1.SHARE.LOAD,DISP=SHR,UNIT=,SPACE=
//SYSUT1   DD UNIT=VIO,SPACE=(CYL,(15)),DCB=BUFNO=1
//SYSPRINT DD SYSOUT=*
//SYSIN    DD *
 ENTRY VTOCCMD
 NAME  VTOC(R)
