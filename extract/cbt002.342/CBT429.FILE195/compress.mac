PROC 1 DSN
/*   THIS "COMPRESS" CLIST WILL NOT WORK OUTSIDE OF ISPF ....  */
/*      PLEASE NOTE THAT THE COMPRESS IS DONE DISP=SHR   ....  */
CONTROL NOFLUSH
ALLOC FI($$IOFILE) REUSE SHR DA(&DSN)
ALLOC FI(SYSPRINT) REUSE DA(*)
ALLOC FI(SYSIN) REUSE NEW DELETE TRACKS SPACE(1) +
    RECFM(F B) BLKSIZE(80) LRECL(80)
OPENFILE SYSIN OUTPUT
SET &SYSIN = &STR( COPY  INDD=$$IOFILE,OUTDD=$$IOFILE)
PUTFILE SYSIN
CLOSFILE SYSIN
ISPEXEC SELECT PGM(IEBCOPY)
ALLOC FI(SYSIN) REUSE DA(*)
ALLOC FI(SYSPRINT) REUSE DA(*)
FREE FI($$IOFILE)
