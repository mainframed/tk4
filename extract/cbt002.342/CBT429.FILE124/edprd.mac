PROC 0
CONTROL PROMPT
ISREDIT MACRO
ISREDIT (DSNAME) = DATASET
ISREDIT (MEMBER) = MEMBER
IF &MEMBER EQ /*NULL*/ -
   THEN SET &PRDSN = &STR('&DSNAME')
   ELSE SET &PRDSN = &STR('&DSNAME(&MEMBER)')
ISPEXEC DISPLAY PANEL(PDPANL)
ISPEXEC VPUT (PDOPTS) PROFILE
IF &RESPONSE EQ /* NULL */ THEN -
   DO
 /*WRITE PRINTDS DA(&PRDSN) &PDOPTS*/
 /*WRITE*/
   PRINTDS DA(&PRDSN) &PDOPTS
   SET &CCODE = &LASTCC
   SET &ZEDSMSG = &STR(PRINTDS ENDED RC=&CCODE)
   SET &ZEDLMSG = &STR(PRINTDS ENDED WITH A RETURN CODE OF &CCODE)
   END
ELSE DO
   SET &ZEDSMSG = &STR(REQUEST CANCELLED)
   SET &ZEDLMSG = &STR(EDPRD TERMINATED BY AN END OR RETURN COMMAND)
   END
ISPEXEC SETMSG MSG(ISRZ000)
ISPEXEC CONTROL DISPLAY REFRESH
