 PUNCH '//           NOTIFY=&NOTIFY'
         AGO  .OVER0
.SKIPNO0  ANOP
 PUNCH '//           CLASS=&JOBCLASS,MSGCLASS=&JOBMSGCL.&TYPRN'
.OVER0   ANOP
 PUNCH '//*---------------------------------------------------------* '
 PUNCH '//* JOB: &JOBNAME.0                                           '
 PUNCH '//* USE: TO INITALIZE THE VOLUME=&VOLUME ON UNIT=&UNITADR     '
 PUNCH '//*                                                           '
 PUNCH '//*     _STEPS_   __________ FUNCTION _____________________   '
