/*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*/
/* DOC:  THIS IS AN ISPF V2.2 ISREDIT MACRO                          */
/* REQ:  USER MUST BE IN ISPF EDIT MODE.                             */
/*                                                                   */
/* FUNCTION: ADD JCL ONTO ASSEMBLER SOURCE CODE FOR ASSEM/LINK       */
/*                                                                   */
/*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*/
PROC 0 P( )
CONTROL MAIN
ISREDIT MACRO (NAME)
SET &ID=&SYSUID
SET &RC=0
ISREDIT (DSN) =  DATASET
ISREDIT (MEM) =  MEMBER
IF &NAME^= THEN DO
   SET &P=&SYSPREF
   PROFILE PREFIX(&NAME)
  END

LISTC

IF &P^=  THEN PROFILE PREFIX(&P)

EXIT CODE(&RC)
