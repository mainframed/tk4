//IDMSLOGS   JOB
//*-----------------------------------------------------------------*
//*   DOC: ASSEM, LINK, AND EXEC IDMSLOGS                           *
//*-----------------------------------------------------------------*
//ASMHCL  EXEC ASMHCL
//C.SYSLIB  DD  DSN=SYS1.MACLIB,DISP=SHR,DCB=BLKSIZE=32000
//          DD  DSN=SYS1.AMODGEN,DISP=SHR
//SYSIN     DD  DSN=THIS.LIBRARY(IDMSLOGS)
//*
//L.SYSLMOD DD  DSN=LINKLIB,DISP=SHR
//SYSIN     DD  *
  INCLUDE SYSLMOD(JULSUB)
  NAME IDMSLOGS(R)
/*
//*
//PRTPLOG EXEC PGM=RHDCPRLG,REGION=4M
//SYSUDUMP DD  SYSOUT=(,)
//SYSLST   DD  DSN=&&LOG,DISP=(,PASS),UNIT=SYSDA,
// DCB=(LRECL=133,BLKSIZE=23408,RECFM=FBA),SPACE=(CYL,(30,1),RLSE)
//DMSGDB   DD  DSN=DMSGDB,DISP=SHR
//DICTDB   DD  DSN=DICTDB,DISP=SHR
//DLOGDB   DD  DSN=DLOGDB,DISP=SHR
//SYS002   DD  DUMMY
//*
//* GET ALOT OF 'M
//*
//SYS001   DD  DSN=ARCHIVE.LOG.DAILY(-00),DISP=SHR,UNIT=TAPE
//         DD  DSN=ARCHIVE.LOG.DAILY(-01),DISP=SHR,UNIT=AFF=SYS001
//         DD  DSN=ARCHIVE.LOG.DAILY(-02),DISP=SHR,UNIT=AFF=SYS001
//         DD  DSN=ARCHIVE.LOG.DAILY(-03),DISP=SHR,UNIT=AFF=SYS001
//         DD  DSN=ARCHIVE.LOG.DAILY(-04),DISP=SHR,UNIT=AFF=SYS001
//         DD  DSN=ARCHIVE.LOG.DAILY(-05),DISP=SHR,UNIT=AFF=SYS001
//         DD  DSN=ARCHIVE.LOG.DAILY(-06),DISP=SHR,UNIT=AFF=SYS001
//         DD  DSN=ARCHIVE.LOG.DAILY(-07),DISP=SHR,UNIT=AFF=SYS001
//J1JRNL   DD  DUMMY
//J2JRNL   DD  DUMMY
//J3JRNL   DD  DUMMY
//J4JRNL   DD  DUMMY
//SYSJRNL  DD  DUMMY
//SYSIPT   DD  *
 INPUT ARCHIVE
    PRINT (STAT)
