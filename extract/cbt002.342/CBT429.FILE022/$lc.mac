PROC 0 NOALL TEST
CONTROL MAIN
ISREDIT MACRO (TYPE,L)
ATTR A LRECL(125) BLKSIZE(629) RECFM(V B A) DSORG(PS)
DDD OFILE '$' SP(1,1) CYL USING(A) MOD
OPENFILE OFILE OUTPUT
SET OFILE=&STR(   LISTC &T ALL OFILE(OFILE))
PUTFILE OFILE
CLOSFILE OFILE
IF &TYPE= OR &TYPE=ALL THEN DO
   SET &P=&SYSPREF
   IF &P=   THEN PROF PREF(&SYSUID)
   LISTCAT &TYPE OFILE(OFILE)
   SET &RC=&LASTCC
   IF &P=   THEN PROF NOPREFIX
   IF &RC^=0 THEN GOTO FREEM
   SET &DDNAME=OFILE
   IF &TEST^=TEST THEN GOTO REVIEWM
NEWSTUFF: DDD OFILE 'HEDGESM.IDCAMS'
DDD SYSOUT '$' NEW LRECL(121) RECFM(F B A) DSORG(PS) BLKSIZE(23474)
OPENFILE SYSOUT   OUTPUT
OPENFILE OFILE INPUT
ERROR GOTO CLOSE
CONTROL LIST SYM CON NOFLUSH MSG
LOOP: GETFILE OFILE
      IF &SUBSTR(1:1,&OFILE)=1 THEN GOTO LOOP
      IF &SUBSTR(1:8,&OFILE)=&STR(0NONVSAM) THEN DO
         SET &DSN=&SUBSTR(18:&LENGTH(&OFILE),&OFILE)
         SET &A=3
         DO WHILE (&A^=0)
            LOOP2: GETFILE OFILE
                   IF &SUBSTR(1:1,&OFILE)=1 THEN GOTO LOOP2
                   SET &A=&A-1
           END
         IF &SUBSTR(38:45,&OFILE)=&STR(CREATION) THEN DO
            SET &YY=&SUBSTR(56:57,&OFILE)
            SET &DDD=&SUBSTR(59:61,&OFILE)
          END
         SET &A=3
         DO WHILE (&A^=0)
            LOOP3: GETFILE OFILE
                   IF &SUBSTR(1:1,&OFILE)=1 THEN GOTO LOOP3
                   SET &A=&A-1
           END
         IF &SUBSTR(9:14,&OFILE)=&STR(VOLSER) THEN +
            SET &VOL=&SUBSTR(27:33,&OFILE)
      END
      IF &DSN^=   THEN DO
         SET &DD=&STR(&DDD)
         SET &X=&YY/4
         SET &Y=&X*4
         IF &Y=&YY THEN SET &L=1   /* LEAP YEAR  */
                   ELSE SET &L=0
         SET &MON=01
         SET &DDD=&DDD-31
         IF &DDD<0 THEN DO
            SET &DDD=&DDD+31
            GOTO WRITE
           END
         SET &MON=02
         SET &DDD=&DDD-28-&L
         IF &DDD<0 THEN DO
            SET &DDD=&DDD+28+&L
            GOTO WRITE
           END
         SET &MON=03
         SET &DDD=&DDD-31
         IF &DDD<0 THEN DO
            SET &DDD=&DDD+31
            GOTO WRITE
           END
         SET &MON=04
         SET &DDD=&DDD-30
         IF &DDD<0 THEN DO
            SET &DDD=&DDD+30
            GOTO WRITE
           END
         SET &MON=05
         SET &DDD=&DDD-31
         IF &DDD<0 THEN DO
            SET &DDD=&DDD+31
            GOTO WRITE
           END
         SET &MON=06
         SET &DDD=&DDD-30
         IF &DDD<0 THEN DO
            SET &DDD=&DDD+30
            GOTO WRITE
           END
         SET &MON=07
         SET &DDD=&DDD-31
         IF &DDD<0 THEN DO
            SET &DDD=&DDD+31
            GOTO WRITE
           END
         SET &MON=08
         SET &DDD=&DDD-31
         IF &DDD<0 THEN DO
            SET &DDD=&DDD+31
            GOTO WRITE
           END
         SET &MON=09
         SET &DDD=&DDD-30
         IF &DDD<0 THEN DO
            SET &DDD=&DDD+30
            GOTO WRITE
           END
         SET &MON=10
         SET &DDD=&DDD-31
         IF &DDD<0 THEN DO
            SET &DDD=&DDD+31
            GOTO WRITE
           END
         SET &MON=11
         SET &DDD=&DDD-30
         IF &DDD<0 THEN DO
            SET &DDD=&DDD+30
            GOTO WRITE
           END
         SET &MON=12
         SET &DDD=&DDD-31
         IF &DDD<0 THEN DO
            SET &DDD=&DDD+31
            GOTO WRITE
           END
         WRITE: +
         SET SYSOUT=&STR( &DSN.  &VOL  &YY.&DD  OR  &MON/&DDD/&YY)
         PUTFILE SYSOUT
        END
      SET &DSN=

      GOTO LOOP
CLOSE: ERROR OFF
       CLOSFILE OFILE
       CLOSFILE SYSOUT
       FREE DD(OFILE)
       SET &DDNAME=SYSOUT
       GOTO REVIEWM
   EXIT
  END

IF &L=L THEN SET &T=&STR(LVL(&TYPE))
        ELSE SET &T=&STR(ENT('&TYPE'))
LISTC &T ALL OFILE(OFILE)

IF &LASTCC^=0 THEN GOTO FREEM
SET &DDNAME=OFILE

REVIEWM: +
ISPEXEC  LMINIT  DATAID(ID1) DDNAME(&DDNAME)
ISPEXEC  BROWSE  DATAID(&ID1)
ISPEXEC  LMFREE  DATAID(&ID1)

WRITENR TO PRINT ENTER (Y/N):&STR( )
READ &A
IF &A=Y THEN PRINTODD &DDNAME DEST(MISPRT1) ASIS
FREEM: +
FREE DD(OFILE,A)
IF &DDNAME=SYSOUT THEN  FREE DD(SYSOUT)
EXIT
