  DATA _NULL_;
     INFILE SMPELIST;
     INPUT @1 CC   $1.
           @2 DATA $120.;
   * DROP ALL UNNECESSARY RECORDS ;
     IF SUBSTR(CC,1,1)='1' × SUBSTR(DATA,1,6)='  NAME' ×
        SUBSTR(DATA,1,5)=' NAME' × SUBSTR(DATA,1,4)='NAME' THEN DELETE;

    * NAMES OF YOUR GLOBAL, TARGET, AND DLIB ZONES ... ;

      IF SUBSTR(DATA,1,6)='GLOBAL' ×  SUBSTR(DATA,1,6)='MVSTGT' ×
         SUBSTR(DATA,1,6)='MVSDLB'  THEN DELETE;
      IF SUBSTR(DATA,1,20)='                     ' THEN DELETE;
      DROP CC;
      FILE SMPOUT LS=120 NOPRINT NOTITLE;
           PUT @1 DATA;

