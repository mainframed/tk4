00010000PROC 0
00020004  ISPEXEC VGET (DEBUG SLSS CIBSRTKY CIBNUMBR LIBADMIN CIBOWNER)
00030000  IF &DEBUG = &STR(DEBUG) THEN DO
00040000      CONTROL MSG LIST CONLIST SYMLIST
00050000  END DO
00060000  ELSE DO
00070000      CONTROL NOMSG
00080000  END DO
00090000OPENPROF: +
00100003  ISPEXEC CONTROL ERRORS RETURN
00110000  ISPEXEC TBEND  SLSSPROF
00120001  ISPEXEC TBOPEN SLSSPROF
00130000  SET RC = &LASTCC
00140000  IF &RC = 8 THEN DO
00150000    %SLSSPROF &SYSUID &DEBUG
00160000    SET PROFRC = &LASTCC
00170000    IF &PROFRC ^= 0 THEN DO
00180000      SET SLSSERR1 = UNABLE TO CREATE YOUR SLSS PROFILE.
00190000      SET SLSSERR2 = BE SURE JOB CARDS ARE CORRECT BEFORE SUBMITTING JOBS.
00200000      SET SLSSERR3 = CONTACT TECHNICAL SERVICES FOR FURTHER INFORMATION
00210000      SET SLSSERR4 = REGARDING THIS ERROR.
00220000      SET SLSSERR5 =
00230000      SET SLSSERRX = PRESS ENTER TO CONTINUE
00240000      ISPEXEC DISPLAY PANEL(SLSS@ERR)
00250000      GOTO DISPLAY
00260000    END DO
00270000    SET RC = 0
00280000  END DO
00290000  IF &RC ^= 0 THEN DO
00300000    SET SLSSERR1 = UNABLE TO OPEN YOUR SLSS PROFILE.
00310000    SET SLSSERR2 = RETURN CODE FOR 'ISPEXEC TBOPEN' WAS &RC..
00320000    SET SLSSERR3 = CONTACT TECHNICAL SERVICES FOR FURTHER INFORMATION
00330000    SET SLSSERR4 = REGARDING THIS ERROR.
00340000    SET SLSSERR5 =
00350000    SET SLSSERRX = PRESS ENTER TO CONTINUE
00360000    ISPEXEC DISPLAY PANEL(SLSS@ERR)
00370000    GOTO DISPLAY
00380000  END DO
00390000  ISPEXEC TBTOP SLSSPROF
00400000  ISPEXEC TBVCLEAR SLSSPROF
00410000  SET SLSSUSR = &STR(&SYSUID)
00420000  ISPEXEC TBSKIP SLSSPROF
00430000  SET SKIPRC = &LASTCC
00440000  IF &SKIPRC ^= 0 THEN DO
00450000    SET SLSSERR1 = UNABLE TO OBTAIN INFORMATION FROM YOUR SLSS PROFILE.
00460000    SET SLSSERR2 = RETURN CODE FOR 'ISPEXEC TBSKIP' WAS &SKIPRC..
00470000    SET SLSSERR3 = CONTACT TECHNICAL SERVICES FOR FURTHER INFORMATION
00480000    SET SLSSERR4 = REGARDING THIS ERROR.
00490000    SET SLSSERR5 =
00500000    SET SLSSERRX = PRESS ENTER TO CONTINUE
00510000    ISPEXEC DISPLAY PANEL(SLSS@ERR)
00520000  END DO
00530000DISPLAY: +
00540000  IF &STR(&SLSSJOB1) = &STR() THEN DO
00550000    ISPEXEC VGET (ZLLGJOB1 ZLLGJOB2 ZLLGJOB3 ZLLGJOB4)
00560000    SET SLSSJOB1 = &STR(&ZLLGJOB1)
00570000    SET SLSSJOB2 = &STR(&ZLLGJOB2)
00580000    SET SLSSJOB3 = &STR(&ZLLGJOB3)
00590000    SET SLSSJOB4 = &STR(&ZLLGJOB4)
00600000  END DO
00610000  ISPEXEC DISPLAY PANEL(SLSSJOBC)
00620000  SET RC = &LASTCC
00630000  IF &RC = 8 THEN DO
00640000    ISPEXEC TBEND SLSSPROF
00650000    EXIT
00660000  END DO
00670000  IF &RC ^= 0 THEN DO
00680000    SET SLSSERR1 = UNABLE TO DISPLAY SLSS JOB CARD SETUP PANEL.
00690000    SET SLSSERR2 = RETURN CODE FOR 'ISPEXEC DISPLAY' WAS &RC..
00700000    SET SLSSERR3 = CONTACT TECHNICAL SERVICES FOR FURTHER INFORMATION
00710000    SET SLSSERR4 = REGARDING THIS ERROR.
00720000    SET SLSSERR5 =
00730000    SET SLSSERRX = PRESS ENTER TO CONTINUE
00740000    ISPEXEC DISPLAY PANEL(SLSS@ERR)
00750000    ISPEXEC TBEND SLSSPROF
00760000    EXIT
00770000  END DO
00780000  ISPEXEC TBPUT SLSSPROF
00790001  ISPEXEC TBCLOSE SLSSPROF
00800000  EXIT
