03520000      SET SLSSWRN1 = UNABLE TO COMPLETE REQUEST TO UPDATE ENTRY.
03530000      SET SLSSWRN2 = RETURN CODE FROM 'ISPEXEC TBMOD' WAS &RC.
03540000      SET SLSSWRN3 =
03550000      SET SLSSWRN4 =
