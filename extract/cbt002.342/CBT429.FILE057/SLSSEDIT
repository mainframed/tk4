08380000      SET SLSSERR2 = RETURN CODE FOR 'ISPEXEC TBGET' WAS &RC..
08390000      SET SLSSERR3 = CONTACT TECHNICAL SERVICES FOR FURTHER INFORMATION
08400000      SET SLSSERR4 = REGARDING THIS ERROR.
08410000      SET SLSSERR5 =
