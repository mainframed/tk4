03660000          SET SLSSERR1 = ERROR OPENING LIBRARY MAINLOCS
03670000          SET SLSSERR2 = RETURN CODE FOR 'ISPEXEC TBOPEN' WAS &OPENRC.
03680000          SET SLSSERR3 = CONTACT TECHNICAL SERVICE FOR FURTHER INFORMATION
