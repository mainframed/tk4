00010000PROC 0 DEBUG
00020000CONTROL NOLIST MSG NOFLUSH PROMPT
00030000IF &DEBUG NE &STR() THEN +
00040000  CONTROL LIST MSG NOFLUSH PROMPT CONLIST SYMLIST
00050000SET &SAVEPREF = &STR(&SYSPREF)
00060000PROFILE PREFIX(&SYSUID)
00070000RECEIVE
00080000IF &SAVEPREF EQ &STR() THEN +
00090000  PROFILE NOPREFIX
00100000ELSE +
00110000  IF &SAVEPREF EQ &STR( ) THEN +
00120000    PROFILE NOPREFIX
00130000  ELSE +
00140000    PROFILE PREFIX(&SAVEPREF)
