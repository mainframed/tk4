16210000SUBMIT2: +
16220003  SET SLSSINDX = &SLSSINDX + 1
16230003  IF &SLSSINDX = 27 THEN SET SLSSINDX = 1
16240020  SET TEMPNAME = &STR(//&SLSSUSR.&SUBSTR(&SLSSINDX:&SLSSINDX,&STR(ABCDEFGHIJKLMNOPQRSTUVWXYZ&FS)))
