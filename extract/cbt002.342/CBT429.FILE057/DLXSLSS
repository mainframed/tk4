00010026PROC 0  DEBUG
00020026CONTROL NOLIST NOMSG NOFLUSH PROMPT
00030026IF &DEBUG NE &STR() THEN +
00040026  CONTROL LIST MSG NOFLUSH PROMPT CONLIST SYMLIST
00050026FREE F(SLSSTLIB)
00060035ALLOCATE DDNAME(SLSSTLIB) DATASET('TSO.SLSS.ISPTABL') SHR REUSE
00070028SLSSMAIN: +
00080027SET SLSS = &STR(MAIN)
00090027ISPEXEC VPUT (SLSS DEBUG) SHARED
00100036%SLSSENQ ENQTALL
00110027IF &LASTCC ^= 0 THEN GOTO DUMMTLIB
00120027ISPEXEC SELECT PGM(SLSSMAIN) PARM(&DEBUG)
00130027DUMMTLIB: +
00140034ISPEXEC TBOPEN SLSSPROF NOWRITE LIBRARY(ISPTABL)
00150034IF &LASTCC ^= 0 THEN DO
00160034  %SLSSPROF &SYSUID &DEBUG
00170034END DO
00180034ISPEXEC TBEND SLSSPROF
00190026FREE F(SLSSTLIB)
00200023EXIT
