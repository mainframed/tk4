00010000PROC 1 SLSSUSER DEBUG
00020000  IF &DEBUG = &STR(DEBUG) THEN DO
00030000      CONTROL MSG NOFLUSH LIST CONLIST SYMLIST PROMPT
00040000      ISPEXEC  CONTROL DISPLAY  LINE SM
00050000  END DO
00060000  ELSE DO
00070000      CONTROL NOMSG NOFLUSH NOLIST NOCONLIST NOSYMLIST
00080000  END DO
00090000  ISPEXEC TBCREATE SLSSPROF -
00100000  KEYS(SLSSUSR) -
00110000  NAMES(SLSSINDX SLSSJOB1 SLSSJOB2 SLSSJOB3 SLSSJOB4) -
00120000  WRITE -
00130000  REPLACE
00140000  SET RC = &LASTCC
00150000  IF &RC ^= 0 THEN DO
00160000    SET SLSSERR1 = UNABLE TO CREATE SLSS PROFILE TABLE.
00170000    SET SLSSERR2 = RETURN CODE FOR 'ISPEXEC TBCREATE' WAS &RC..
00180000    SET SLSSERR3 = CONTACT TECHNICAL SERVICES FOR FURTHER INFORMATION
00190000    SET SLSSERR4 = REGARDING THIS ERROR.
00200000    SET SLSSERR5 =
00210000    SET SLSSERRX = PRESS ENTER TO CONTINUE
00220000    ISPEXEC DISPLAY PANEL(SLSS@ERR)
00230000    ISPEXEC TBEND SLSSPROF
00240001    EXIT CODE(20)
00250000  END DO
00260000  SET SLSSUSR = &STR(&SLSSUSER)
00270000  SET SLSSINDX = &STR(0)
00280000  SET SLSSJOB1 = &STR(&ZLLGJOB1)
00290000  SET SLSSJOB2 = &STR(&ZLLGJOB2)
00300000  SET SLSSJOB3 = &STR(&ZLLGJOB3)
00310000  SET SLSSJOB4 = &STR(&ZLLGJOB4)
00320000  ISPEXEC TBADD SLSSPROF
00330000  SET RC = &LASTCC
00340000  IF &RC ^= 0 THEN DO
00350000    SET SLSSERR1 = UNABLE TO ADD USER TO SLSS PROFILE TABLE.
00360000    SET SLSSERR2 = RETURN CODE FOR 'ISPEXEC TBADD' WAS &RC..
00370000    SET SLSSERR3 = CONTACT TECHNICAL SERVICES FOR FURTHER INFORMATION
00380000    SET SLSSERR4 = REGARDING THIS ERROR.
00390000    SET SLSSERR5 =
00400000    SET SLSSERRX = PRESS ENTER TO CONTINUE
00410000    ISPEXEC DISPLAY PANEL(SLSS@ERR)
00420000    ISPEXEC TBEND SLSSPROF
00430001    EXIT CODE(20)
00440000  END DO
00450002  ISPEXEC TBCLOSE SLSSPROF
00460001  EXIT CODE(00)
