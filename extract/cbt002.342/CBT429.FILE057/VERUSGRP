00010000PROC 2 OWNER DFLTGRP DEBUG
00020000CONTROL NOLIST NOMSG NOFLUSH PROMPT
00030000IF &DEBUG NE &STR() THEN +
00040000  CONTROL LIST MSG NOFLUSH PROMPT CONLIST SYMLIST
00050000SET &OWNVER = &STR(VALID)
00060000SET &GRPVER = &STR(VALID)
00070000SET SYSOUTTRAP = 100
00080000LU &OWNER
00090000SET &RETURN = &LASTCC
00100000IF &RETURN ^= 0 THEN +
00110000    DO
00120000        SET SYSOUTTRAP = 100
00130000        LG &OWNER
00140000        SET &RETURN = &LASTCC
00150000        IF &RETURN ^= 0 THEN +
00160000            SET &OWNVER = &STR(INVALID)
00170000    END
00180000SET SYSOUTTRAP = 100
00190000LG &DFLTGRP
00200000SET &RETURN = &LASTCC
00210000IF &RETURN ^= 0 THEN +
00220000    SET &GRPVER = &STR(INVALID)
00230000ISPEXEC  VPUT (OWNVER GRPVER)
