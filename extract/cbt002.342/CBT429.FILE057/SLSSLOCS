00010002PROC 0                        N O T   U S E D
00020000    ISPEXEC VGET (DEBUG SLSS SLSID) SHARED
00030000    IF &DEBUG = DEBUG THEN DO
00040000        CONTROL MSG LIST CONLIST SYMLIST
00050000    END DO
00060000    ELSE DO
00070000        CONTROL NOMSG
00080000    END DO
00090000PROCESS: +
00100001    ISPEXEC TBOPEN &SLSS.LOCS LIBRARY(SLSSTLIB)
00110000    SET &RC = &LASTCC
00120000    IF &RC = 8 THEN DO
00130000      GOTO BLDTABL
00140000    END DO
00150000    IF &RC ^= 0 THEN DO
00160000      SET &SLSSERR1 = ERROR OPENING LIBRARY &SLSS.LOCS
00170000      SET &SLSSERR2 = RETURN CODE FOR 'ISPEXEC TBOPEN' WAS &RC.
00180000      SET &SLSSERR3 = CONTACT TECHNICAL SERVICE FOR FURTHER INFORMATION
00190000      SET &SLSSERR4 = CONCERNING THIS ERROR.
00200000      SET &SLSSERR5 =
00210000      SET &SLSSERRX = PRESS ENTER TO CONTINUE
00220000      ISPEXEC DISPLAY PANEL(SLSS@ERR)
00230000      ISPEXEC TBEND &SLSS.LOCS
00240000      EXIT
00250000    END DO
00260000BLDTABL: +
00270000    SET &CREATE = Y
00280001    ISPEXEC TBCREATE &SLSS.LOCS LIBRARY(SLSSTLIB) -
00290000    KEYS(LOCID) -
00300000    NAMES(LOC1  LOC2  LOC3  LOC4  LOC5  LOC6  LOC7  LOC8  LOC9  LOC10 -
00310000          LOC11 LOC12 LOC13 LOC14 LOC15 LOC16 LOC17 LOC18 LOC19 LOC20 -
00320000          LOC21 LOC22 LOC23 LOC24 LOC25 LOC26 LOC27 LOC28 LOC29 LOC30 -
00330000          LOC31 LOC32 LOC33 LOC34 LOC35 LOC36 LOC37 LOC38 LOC39 LOC40 -
00340000          LOC41 LOC42 LOC43 LOC44 LOC45 LOC46 LOC47 LOC48 LOC49 LOC50 -
00350000          LOC51 LOC52 LOC53 LOC54 LOC55 LOC56 LOC57 LOC58 LOC59 LOC60)
00360000    SET &RC = &LASTCC
00370000    IF &RC ^= 0 THEN DO
00380000      SET &SLSSERR1 = UNABLE TO CREATE THE REQUESTED LIBRARY &SLSS.LOCS
00390000      SET &SLSSERR2 = RETURN CODE FROM 'ISPEXEC TBCREATE' WAS &RC..
00400000      SET &SLSSERR3 = PLEASE CONTACT TECHNICAL SERVICES FOR FURTHER
00410000      SET &SLSSERR4 = INFORMATION REGARDING THIS ERROR.
00420000      SET &SLSSERR5 =
00430000      SET &SLSSERRX = PRESS ENTER TO CONTINUE
00440000      ISPEXEC DISPLAY PANEL(SLSS@ERR)
00450000      ISPEXEC TBEND &SLSS.LOCS
00460000      EXIT
00470000    END DO
00480000SHOWIT: +
00490000    ISPEXEC DISPLAY PANEL(&SLSS.LOCS)
00500000    IF &LASTCC = 8 THEN DO
00510001      ISPEXEC TBCLOSE &SLSS.LOCS LIBRARY(SLSSTLIB)
00520000      EXIT
00530000    END DO
00540000    SET &LOCID = &STR(&SLSS.&MANID)
00550000    ISPEXEC TBMOD &SLSS.LOCS
00560000    SET &RC = &LASTCC
00570000    IF &RC > 12 THEN DO
00580000      SET &SLSSERR1 = UNABLE TO ADD/UPDATE ENTRY.
00590000      SET &SLSSERR2 = RETURN CODE FOR 'ISPEXEC TBMOD' WAS &RC.
00600000      SET &SLSSERR3 =
00610000      SET &SLSSERR4 =
00620000      SET &SLSSERR5 =
00630000      SET &SLSSERRX = PRESS ENTER TO CONTINUE
00640000      ISPEXEC DISPLAY PANEL(SLSS@ERR)
00650000    END DO
00660000    GOTO SHOWIT
