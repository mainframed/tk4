00010000PROC 0 DEBUG
00020000CONTROL NOLIST MSG NOFLUSH PROMPT
00030000IF &DEBUG NE &STR() THEN +
00040000  CONTROL LIST MSG NOFLUSH PROMPT CONLIST SYMLIST
00090000SET &SAVEPREF = &STR(&SYSPREF)
00100000PROFILE PREFIX(&SYSUID)
00101001ISPEXEC CONTROL ERRORS RETURN
00110001ISPEXEC DISPLAY PANEL(XMITRCV3)
00115000IF &LASTCC = 8 THEN +
00116000  DO
00116100    IF &SAVEPREF EQ &STR() THEN +
00116200      PROFILE NOPREFIX
00116300    ELSE +
00116400      IF &SAVEPREF EQ &STR( ) THEN +
00116500        PROFILE NOPREFIX
00116600      ELSE +
00116700        PROFILE PREFIX(&SAVEPREF)
00116800     EXIT
00117000  END
00118000IF &LOGDEL EQ &STR(YES) THEN +
00119000  DO
00120000    CONTROL NOMSG
00121003    SET SYSOUTTRAP = 20
00130000    PDS LOG.MISC FIX RESET
00130103    SET SYSOUTTRAP = 0
00131002    ISPEXEC CONTROL DISPLAY REFRESH
00140000    CONTROL MSG
00150000  END
00160000ELSE +
00170000  DO
00180000    ISPEXEC BROWSE DATASET(LOG.MISC)
00190000  END
00200000IF &SAVEPREF EQ &STR() THEN +
00210000  PROFILE NOPREFIX
00220000ELSE +
00230000  IF &SAVEPREF EQ &STR( ) THEN +
00240000    PROFILE NOPREFIX
00250000  ELSE +
00260000    PROFILE PREFIX(&SAVEPREF)
