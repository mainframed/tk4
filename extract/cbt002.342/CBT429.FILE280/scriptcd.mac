.CM ---------- SCRIPT REFERENCE CARD GENERATOR -----------------------
.LL 72
.UR .AD (75-&SYSLL)/2
.PL 132
.WD
.HS 0;.HM 0;.TM 1
.BM 27
.OB /SCRIPT REFERENCE CARD//%/
.EB /%//UNIVERSITY OF WATERLOO COMPUTING CENTRE/
.CM ---------- PAGE CONTROL FOR THE REFERENCE CARD -------------------
.CR PA XX
.RM PA SAVE NOSAVE
.CM
.RM
.CM ---------- THE OUTPUT PAGE ---------------------------------------
.FN SET 1
.FN SET 1 ////
.FN ON
.CE;.UR &SYSVERSION
.SP
.UR .BX 1 &SYSLL
.SP;.CE;THE OUTPUT PAGE
.SP;.BX OFF;.SP
.IM APENDIXD . SCRIPTCD
.FN OFF
.XX
.CM ---------- CONTROL WORDS BY FUNCTION -----------------------------
.SP 3;.CC BEGIN
.UR .BX 1 &SYSLL
.SP;.CE;CONTROL WORD SUMMARY, BY FUNCTION
.SP;.BX OFF;.CC END 10;.SP
.IM REFALPH . SCRIPTCD
.CM ---------- OPTIONS -----------------------------------------------
.SP 3;.CP BEGIN
.UR .BX 1 &SYSLL
.SP;.CE;OPTIONS SUMMARY
.SP;.BX OFF
.CP END 10;.SP
..RM IN SAVE NOSAVE
.UR .IF &0 EQ 0;.TH ..IN
.EL .UR .IF &1+0 LT 0;..TH ..IN -2
.EL ..IN +2
.SR 0 = 0
.RM
.SR 0 = 0
.IM OPTIONS . SCRIPTCD
.RM IN DELETE
.CM ---------- SYSTEM REFERENCE VARIABLES AND REMOTES ---------------
.SP 3;.CP BEGIN
.UR .BX 1 &SYSLL
.SP;.CE;SUMMARY OF SYSTEM REFERENCE VARIABLES AND REMOTES
.SP;.BX OFF
.CP END 10;.SP
.IM APENDIXG . SCRIPTCD
.CM ---------- CONTROL WORDS IN ALPHABETIC ORDER ---------------------
.SP 3
.UR .BX 1 &SYSLL
.SP
.CE;.UP CONTROL WORD SUMMARY, IN ALPHABETICAL ORDER
.SP;.BX OFF
.LI )
))RM IN SAVE NOSAVE
)IF &0 NE 0;)TH ))IN 3;)EL ))IN
)SR 0 = 0
)RM
)SR HRGLASS=0
)UR )TB &SYSLL.RIGHT
)SR BRKIND='*'
)SU ON
)IM APENDIXI ) SCRIPTCD
.RM IN DELETE
.CM ---------- SYSPUB COMMAND SUMMARY --------------------------------
.SP 3;.CP BEGIN
.UR .BX 1 &SYSLL
.SP;.CE;SYSPUB COMMAND SUMMARY
.SP;.BX OFF
.CP END 10;.SP
.IM APENDIXX . SCRIPTCD
.CM ---------- END OF SCRIPT REFERENCE CARD GENERATOR ----------------
