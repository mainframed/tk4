)ATTR
 + TYPE(TEXT) SKIP(ON) INTENS(LOW)
)BODY
%-------------------------  ISPF Command Table Edit  -------------------
%COMMAND ===>_ZCMD


%Specify command table to be edited:

+   Table Name     %===>_MEMF+
+   Dataset Name   %===>_DSNFROM                                       +
                         (default dataset name is TSO.ISPTLIB)
)PROC
 VER(&MEMF,NB,NAME)
 VER(&DSNFROM,DSNAME)
 IF (&DSNFROM = ' ')
   &DSNFROM = '''TSO.ISPTLIB'''
)END
