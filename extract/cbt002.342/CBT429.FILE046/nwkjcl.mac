//******************************************************************//
//*      ASSEMBLE AND LINKEDIT THE PACKRAT PROGRAM                 *//
//******************************************************************//
//*01ASM  EXEC ASMHCL,PARM.C='ALIGN',   NOT USING THE SLAC ASSEMBLER H
//*       PARM.L='LIST,LET,XREF,NCAL'   NOT USING THE SLAC ASSEMBLER H
//S01ASML EXEC ASMHSLCL,PARM.C='ALIGN,NRLIST,OPCXA,DMAP',
//        PARM.L='LIST,LET,XREF,NCAL'
//C.SYSLIB DD DISP=SHR,DSN=TSY2.MACLIB,DCB=BLKSIZE=32760
//       DD DSN=TSY2.ATSOMAC,DISP=SHR
//       DD DSN=TSY2.AMODGEN,DISP=SHR
//C.SYSIN DD DSN=TST2SSG.PACKRAT.SHIP(PACKRATN),DISP=SHR
//L.SYSLMOD DD DSN=TST.TSO.CMDLIB,DISP=SHR
//L.SYSIN DD *
 SETCODE AC(1)
 SETSSI CB298046
 NAME PACKRAT(R)
/*
//S02ASML EXEC ASMHSLCL,PARM.C='ALIGN,NRLIST,OPCXA,DMAP',
//        PARM.L='LIST,LET,XREF,NCAL'
//C.SYSLIB DD DISP=SHR,DSN=TSY2.MACLIB,DCB=BLKSIZE=32760
//       DD DSN=TSY2.ATSOMAC,DISP=SHR
//       DD DSN=TSY2.AMODGEN,DISP=SHR
//C.SYSIN DD DSN=TST2SSG.PACKRAT.SHIP(BACKENDN),DISP=SHR
//L.SYSLMOD DD DSN=TST.TSO.CMDLIB,DISP=SHR
//L.SYSIN DD *
 SETCODE AC(1)
 SETSSI CB298046
 NAME BACKEND(R)
/*
//LKED   EXEC  PGM=IEWL,
//       PARM='NCAL,MAP,LIST,LET'
//SYSUT1   DD  UNIT=SYSDA,SPACE=(1024,(200,20))
//SYSPRINT DD  SYSOUT=*
//SYSLIB   DD  DISP=SHR,DSN=TST.TSO.CMDLIB
//SYSLMOD  DD  DISP=SHR,DSN=TST.TSO.CMDLIB
//SYSLIN   DD  *
 INCLUDE SYSLIB(PACKRAT)
 INCLUDE SYSLIB(BACKEND)
 ORDER   PACKRAT,BACKEND
 SETCODE AC(1)
 SETSSI CC298046
 ENTRY   PACKRAT
 NAME    PACKRAT(R)
