./ ADD NAME=SNAPPC   0100-94267-94267-1200-00024-00024-00000-CONYERS
//MS01@SNP JOB (MS01,228),'CONYERS - SNAPPC ',MSGLEVEL=(1,1),
//         NOTIFY=MS01VC1,MSGCLASS=T,TIME=(,10),CLASS=A
//ASSEM    EXEC ASMHCLG,REGION=1000K
//C.SYSIN    DD *
SNAP     CSECT
         STM   14,12,12(13)
         LR    12,15
         USING SNAP,12
BEGIN    ST    13,SAVE+4
         LR    2,13
         LA    13,SAVE
         ST    13,8(2)
         OPEN  (PRTOUT,OUTPUT)
         SNAP  DCB=PRTOUT,SDATA=(PCDATA)
         CLOSE PRTOUT
         L     13,SAVE+4
         RETURN (14,12),RC=0
PRTOUT   DCB   DSORG=PS,RECFM=VBA,LRECL=125,BLKSIZE=1632,MACRF=(W),    X
               DDNAME=SNAPRINT
*
SAVE     DS    18F'0'
         END
