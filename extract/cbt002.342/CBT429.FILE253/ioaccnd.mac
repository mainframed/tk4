PROC 0 DEBUG                +
       DATEFMT(USA)   +
       DBPREFA(SYSS.IOA)   +
       DBVERA(V410)     +
       LANG()         +
       LOADLIB(SYS1.IOA.V410.LOAD)   +
       DBG(DUMMY)           +
       NOALC
CONTROL MAIN NOLIST NOCONLIST NOSYMLIST NOMSG
ALLOC F(PRTDBG) &DBG SYSOUT(A)  HOLD
IF &DEBUG=DEBUG THEN CONTROL MAIN LIST CONLIST SYMLIST MSG
REDISP:+
IF &DATEFMT=JPN  +
   THEN  ISPEXEC DISPLAY PANEL(CTMPCNDJ)
   ELSE +
IF &DATEFMT=USA  +
   THEN  ISPEXEC DISPLAY PANEL(CTMPCNDA)
   ELSE  ISPEXEC DISPLAY PANEL(CTMPCNDW)
IF &RESP = END THEN EXIT
ALLOC DD(DALOG)    DA('&DBPREFA..&DBVERA..LOG') SH
ALLOC DD(DASINC)   DA('&DBPREFA..&DBVERA..SNC') SH
ALLOC DD(DARESF)   DA('&DBPREFA..&DBVERA..RES') SH
ALLOC DD(DAPRINT)  DS(*)
IF &DATEFMT=JPN  +
   THEN CALL  '&LOADLIB&LANG(IOACND)' '&CTMFUN COND &CTMCONDN &MM.&DD'
   ELSE +
IF &DATEFMT=USA  +
   THEN CALL  '&LOADLIB&LANG(IOACND)' '&CTMFUN COND &CTMCONDN &MM.&DD'
   ELSE CALL  '&LOADLIB&LANG(IOACND)' '&CTMFUN COND &CTMCONDN &DD.&MM'
GOTO  REDISP
END
