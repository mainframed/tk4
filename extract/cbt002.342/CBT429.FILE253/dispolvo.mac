CONTROL MAIN NOMSG
ERROR DO
      IF &LASTCC LT 0  THEN +
          DO
          WRITE
          WRITE + END OF CAI/UCF/TSO SESSION FOR USER &SYSUID.
          WRITE + CA-DISPATCH ONLINE NOT ACTIVE OR IS BEING QUIESCED.
          WRITE
          FREE  F(CAICTL)
          EXIT QUIT
          END
      RETURN
      END
FREE  F(CAICTL)
ALLOC F(CAICTL) DA('SYS1.DISPATCH.V5R0M0.OLVO2.SYSCTL') SHR
CALL 'SYS1.DISPATCH.V5R0M0.CAILIB(CAITSODC)'
FREE  F(CAICTL)
WRITE
WRITE + END OF CAI/UCF/TSO SESSION FOR USER &SYSUID.
WRITE
EXIT QUIT
