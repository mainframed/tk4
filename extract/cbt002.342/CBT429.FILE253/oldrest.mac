SUBMIT_RESTORE_JOB:
ADDRESS TSO
"SUBMIT ('"SUBPDS"("RMEMB")')"
SRC = RC
IF SRC ^= 0 THEN DO
   SAY '*** SUBMIT ERROR: MEMBER ( 'SUBPDS'('RMEMB'). ***'
   SAY '*** NWRESTOR EXEC TASK CANCELLED - RETURN CODE: ('SRC'). ***'
   EXIT SRC
END
RETURN
