/* REXX BACKUP STATUS BROWSE  */
/*                     */
/* START QMF INTERFACE */
/*                     */
/* TRACE ?I */
CALL DSQCIX "EXIT";
CALL DSQCIX "START (DSQSMODE=INTERACTIVE DSQSSUBS=DSNA";
IF dsq_return_code ^=0 then do;
    "VMFCLEAR";
    say dsq_message_id dsq_message_text;
    say dsq_q_message_id dsq_q_message_text;
    signal TERM;
    end;
/*                          */
/* DISPLAY SELECTION PANNEL */
/*                          */
datec = DATE('O');
datec = OVERLAY('-',datec,3,1);
datec = OVERLAY('-',datec,6,1);
datec = INSERT('19',datec,0,2);
cdate = datec;
CALL LEAPRTN;
IF TIME(H) < 12 THEN;
   DO;
     CALL PREVDATE;
     datec = cdate;
   END;
CALL PREVDATE;
datep = cdate;
cdate = datec;
CALL NEXTDATE;
daten = cdate;
"VMFCLEAR";
DO FOREVER;
   address "ISPEXEC" "DISPLAY PANEL(PBKSTAT)";
   IF rc^=0 THEN SIGNAL TERM;
   CALL SCREDIT;
   SELECT;
      WHEN wordn = 1 THEN;
         op="('"op1"')"
      WHEN wordn = 2 THEN;
         op="('"op1"','"op2"')"
      WHEN wordn = 3 THEN;
         op="('"op1"','"op2 "','"op3"')"
      OTHERWISE;
          ITERATE;
          END;
   ope = "(" op ")"
   CALL DSQCIX ,
    " RUN QUERY TECH107.QBSTAT (FORM=TECH107.FBSTAT"  ×× ,
    " &DATEF='"datef"' &DATET='"datet"' &OP=" ope ;
   if dsq_return_code ^=0 then do;
       "VMFCLEAR";
       say dsq_message_id dsq_message_text;
       say dsq_q_message_id dsq_q_message_text;
       signal TERM;
       end;
END;
TERM:
CALL DSQCIX "EXIT";
EXIT 0;
/*************/
/*************/
/*************/
PREVDATE:
cyy =SUBSTR(cdate,3,2);cmm=SUBSTR(cdate,6,2);cdd=SUBSTR(cdate,9,2);
cdd = cdd - 1; cdd=RIGHT(cdd,2,0);
IF cdd = 00 then;
   DO;
    cmm = cmm - 1; cmm=RIGHT(cmm,2,0);
    SELECT;
       WHEN cmm=01 THEN;
          DO;
            cyy=cyy - 1; cyy=RIGHT(cyy,2,0); cmm=12; cdd=31;
          END;
       WHEN cmm=01 × cmm=03 × cmm=05 × cmm=07 × ,
            cmm=08 × cmm=10 THEN;
            cdd = 31;
       WHEN cmm=02 THEN;
            cdd = febmdd;
       WHEN cmm=04 × cmm=06 × cmm=09 × cmm=01  THEN;
            cdd = 30;
       OTHERWISE  END;
  END;
cdate =  '19'  ×× cyy ×× '-' ×× cmm ×× '-' ×× cdd;
RETURN;
/*************/
/*************/
/*************/
NEXTDATE:
cyy =SUBSTR(cdate,3,2);cmm=SUBSTR(cdate,6,2);cdd=SUBSTR(cdate,9,2);
cdd = cdd+1; cdd=RIGHT(cdd,2,0);
SELECT;
   WHEN cmm=12 & cdd>31 THEN;
      DO;
        cyy=cyy+1; cyy=RIGHT(cyy,2,0); cmm=01; cdd=01;
      END;
   WHEN cmm=02 & cdd>febmdd THEN;
      DO;
        cmm=03; cdd=01;
      END;
   WHEN (cmm=01 × cmm=03 × cmm=05 × cmm=07 × ,
         cmm=08 × cmm=10) & cdd>31  THEN;
      DO;
        cmm=cmm+1; cmm=RIGHT(cmm,2,0); cdd=01;
      END;
   WHEN (cmm=04 × cmm=06 × cmm=09 × cmm=01) & cdd>30  THEN;
      DO;
        cmm=cmm+1; cmm=RIGHT(cmm,2,0); cdd=01;
      END;
   OTHERWISE  END;
cdate =  '19'  ×× cyy ×× '-' ×× cmm ×× '-' ×× cdd;
RETURN;
/*************/
/*************/
/*************/
SCREDIT:
/*                              */
/* FORMAT DATE AND SELECT QUERY */
/*                              */
SELECT;
     WHEN s='2' THEN;
       DO;
         datef=datep; datet=datec;
       END;
     WHEN s='1' THEN;
       DO;
         datef=datec; datet=daten;
       END;
     OTHERWISE END;
/*                              */
wordn = 0
IF   f = 'Y' THEN;
     DO;
       op1   = "FAILURE','ACTIVE"; wordn = wordn + 1;
     END;
IF   c = 'Y' THEN;
     DO;
       SELECT;
           WHEN wordn = 0 THEN;
              op1 = 'COMPLETE';
           WHEN wordn = 1 THEN;
              op2 = 'COMPLETE';
           OTHERWISE END;
       wordn = wordn + 1;
     END;
IF   p = 'Y' THEN;
     DO;
       SELECT;
           WHEN wordn = 0 THEN;
              op1 = 'PENDING';
           WHEN wordn = 1 THEN;
              op2 = 'PENDING';
           WHEN wordn = 2 THEN;
              op3 = 'PENDING';
           OTHERWISE END;
       wordn = wordn + 1;
     END;
RETURN;
/*************/
/*************/
/*************/
LEAPRTN:
cyy =SUBSTR(cdate,3,2);
febmdd = 28;
IF cyy//4 = 0 THEN;
   febmdd=29;
RETURN;
/*************/
/*************/
/*************/
