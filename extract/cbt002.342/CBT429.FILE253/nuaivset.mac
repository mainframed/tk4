PROC 0 DEBUG N1() V1() N2() V2() N3() V3() N4() V4() N5() V5()
/**
/**  REFER: NUA.V2R10M12.CLIST(NUAIVSET)
/**  COMP:  IBM/MVS USER-ACCESS (H213)
/**  DOC:   ISPF MENU SYSTEM - SET ONE TO FIVE TSO/E VARIABLES
/**
CONTROL NOFLUSH NOLIST NOMSG NOCONLIST NOPROMPT
IF &DEBUG EQ DEBUG THEN CONTROL LIST MSG CONLIST
/**
IF &SYSISPF NE ACTIVE THEN DO
    WRITE %NUAIVSET - ISPF NOT AVAILABLE
    EXIT CODE(20)
    END
ELSE ISPEXEC CONTROL ERRORS RETURN
/**
ERROR EXIT CODE(&LASTCC)
IF &STR(&N1) NE THEN SET &&N1=&STR(&V1)
IF &STR(&N2) NE THEN SET &&N2=&STR(&V2)
IF &STR(&N3) NE THEN SET &&N3=&STR(&V3)
IF &STR(&N4) NE THEN SET &&N4=&STR(&V4)
IF &STR(&N5) NE THEN SET &&N5=&STR(&V5)
IF &STR(&N1&N2&N3&N4&N5) NE THEN +
    ISPEXEC VPUT (&N1 &N2 &N3 &N4 &N5) SHARED
EXIT CODE(0)
