PROC 0 DEBUG  DATEFMT(USA) +
         DBPREFA(SYSS.IOA) +
         DBVERA(V410) +
         STEPLIB(SYS1.IOA.V410.LOAD) +
         LANG() +
         NOALC
CONTROL MAIN NOLIST NOCONLIST NOSYMLIST NOMSG
IF &DEBUG=DEBUG THEN CONTROL MAIN LIST CONLIST SYMLIST MSG
REDISP:+
IF &DATEFMT=JPN  +
   THEN  ISPEXEC DISPLAY PANEL(CTDPCNDJ)
   ELSE  +
  IF &DATEFMT=USA  +
     THEN  ISPEXEC DISPLAY PANEL(CTDPCNDA)
     ELSE  ISPEXEC DISPLAY PANEL(CTDPCNDW)
IF &RESP = END THEN EXIT
ALLOC DD(DALOG)    DA('&DBPREFA..&DBVERA..LOG') SH
ALLOC DD(DASINC)   DA('&DBPREFA..&DBVERA..SNC') SH
ALLOC DD(DARESF)   DA('&DBPREFA..&DBVERA..RES') SH
ALLOC DD(DAPRINT)  DS(*)
IF &DATEFMT=JPN  +
 THEN CALL  '&STEPLIB&LANG(CTDCND)' '&CTMFUN COND &CTMCONDN &MM.&DD'
  ELSE +
  IF &DATEFMT=USA  +
 THEN CALL  '&STEPLIB&LANG(CTDCND)' '&CTMFUN COND &CTMCONDN &MM.&DD'
 ELSE CALL  '&STEPLIB&LANG(CTDCND)' '&CTMFUN COND &CTMCONDN &DD.&MD'
GOTO  REDISP
END
