)PROC
&ZQ = &Z
   IF (&ZCMD ^= ' ')
      &ZQ = TRUNC(&ZCMD,'.')
      IF (&ZQ = ' ')
         .MSG = ISRU000
  &ZSEL = TRANS( &ZQ
                A,'PANEL(ASMH#P)'
                B,'PANEL(COBOL#P)'
                C,'CMD(%VFORT#C)'
                E,'PANEL(COB2#P)'
                F,'PANEL(ADM#P)'
                I,'PANEL(IDT#P)'
                K,'PANEL(COUNT#P)'
                L,'PGM(ISPTUTOR) PARM(USER#PT1)'
                P,'PANEL(PRINT#P)'
                S,'PANEL(SORT#P)'
                U,'CMD(%ABND#C)'
                V,'PANEL(COMPR#P)'
                W,'PANEL(COPYS#P)'
                Y,'CMD(DUSER)'
                Z,'PANEL(JUL#P)'
                0,'CMD(LASTIPL)'
                1,'CMD(LISTAX)'
                4,'PANEL(SEND#P)'
                7,'CMD(%COMS#C)'
                9,'CMD(DA)'
              ' ',' '
                X,'EXIT'
                *,'?' )
