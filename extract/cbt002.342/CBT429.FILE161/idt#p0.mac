)ATTR
  +  TYPE(TEXT) INTENS(LOW) SKIP(ON)
  %  TYPE(TEXT) INTENS(HIGH) SKIP(ON)
  _  TYPE(INPUT) INTENS(HIGH)
)BODY
+
+     % TSO/E  TRANSMIT  MEMBERS  OF  A  PDS  USING  SELECT
+
+ TO SYSTEM==>_SY+  +1A - HRCC-1A ESA           3A - PRCC-3A OS/390
+                    1B - HRCC-1B ESA           3B - PRCC-3B ESA
+                    1E - HRCC-1E OS/390
+
+                    2A - ARCC-2A ESA
+                    2B - ARCC-2B OS/390
+                    2C - ARCC-2C ESA
+
+
+ TO USERID===>_TUSER  +RECEIVING USER ID
+    NOTIFY===>_NTF+   +YES OR NO  - NOTIFY USER OF ARRIVAL OF THE FILE.
+   MESSAGE===>_MES+   +YES OR NO  - SEND A MSG IN ADDITION TO THE FILE.
+  PDS TO
+  TRANSFER===>_IFILE                                                  +
+
+  NOTE: THE FILE NAME MUST BE PLACED IN QUOTES FULLY QUALIFIED
+
+  COMMANDS AVAILABLE WHEN THE MEMBER LIST IS DISPLAYED:
+
+  %S+- SELECT FOR TRANSMISSION     %B+- BROWSWE    %E+- EDIT
)INIT
  .HELP = IDT#PT2
)PROC
 VER (&SY,NONBLANK)
 VER (&SY,LIST,1A,1B,1C,1E,2A,2B,2C,3A,3B)
 VER (&TUSER,NONBLANK)
 VER (&TUSER,NAME)
 VER (&NTF,LIST,YES,NO)
 VER (&MES,LIST,YES,NO)
 VER (&IFILE,NONBLANK)
 VER (&IFILE,DSNAME)
)END
