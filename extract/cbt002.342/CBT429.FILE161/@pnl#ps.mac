)ATTR
  % TYPE(TEXT)  INTENS(HIGH)  COLOR(PINK)
  + TYPE(TEXT)  INTENS(LOW)   COLOR(GREEN)
  _ TYPE(INPUT) INTENS(HIGH)
)BODY
+
+                SYSPROGs  Internal  Systems
+
%OPTION  ===>_ZCMD
%
% A+ACCOUNT    - Perform Account Fuctions
% B+avail      - available for use
% I+Info/MVS   - Information System        %4+SYSLOG   - Browse Yesterda
% O+OMEGAMON   - OMEGAMON                  %5+BMANUAL  - Browse Manual L
% R+RMFMON     - RMF Monitor (Z to term)   %6+UMANUAL  - Update Manual L
% T+Tool Box   - Sys Prog Tool Box
%
% X +EXIT
%
)INIT
  .HELP = ISR00003
  &ZPRIM = NO         /* ALWAYS A PRIMARY OPTION MENU      */
  &ZHTOP = ISR00003   /* TUTORIAL TABLE OF CONTENTS        */
  &ZHINDEX = ISR91000 /* TUTORIAL INDEX - 1ST PAGE         */
)PROC
&ZQ = &Z
   IF (&ZCMD ^= ' ')
      &ZQ = TRUNC(&ZCMD,'.')
      IF (&ZQ = ' ')
         .MSG = ISRU000
  &ZSEL = TRANS( &ZQ
                4,'CMD(%LOG#C)'
                5,'CMD(%LIB#CB)'
                6,'CMD(%LIB#CE)'
                A,'PANEL(@PNL#PSA)'
                I,'CMD(%INF#C)'
                O,'CMD(%OMEGAMON)'
                R,'CMD(%RMFMON#C)'
                T,'PANEL(@PNL#PX)'
              ' ',' '
                X,'EXIT'
                *,'?' )
  &ZTRAIL = .TRAIL
)END
