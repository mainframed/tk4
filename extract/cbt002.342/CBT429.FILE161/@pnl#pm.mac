)ATTR
  % TYPE(TEXT)  INTENS(HIGH)  COLOR(WHITE)
  + TYPE(TEXT)  INTENS(LOW)   COLOR(GREEN)
  _ TYPE(INPUT) INTENS(HIGH)
)BODY
%                     ISPF/PDF PRIMARY MENU
%OPTION  ===>_ZCMD                                                  +
%
%    0 +ISPF PARMS  - Terminal & user parameters              +USERID -%
%    1 +BROWSE      - Display source data                     +PREFIX -%
%    2 +EDIT        - Create or change source data            +DATE   -%
%    3 +UTILITIES   - Perform utility functions               +JULIAN -%
%    4 +FOREGROUND  - Compile, Assem, LinkEdit, or Debug      +TIME   -%
%    5 +BATCH       - Compile, Assemble, or Link Edit         +PROC   -%
%    6 +COMMAND     - Enter TSO command or CLIST              +TERM   -%
%    7 +DIALOG TEST - Perform dialog testing                  +KEYS   -%
%    C +CHANGES     - summary of changes, this release
%    S +SDSF        - Spool Display and Search Facility
%    T +TUTORIAL    - Display information about ISPF/PDF
%    X +EXIT        - Exit ISPF Ver 2 Rel 3
%
%B+BDT       - Bulk Data Transfer      %P+PHOENIX    +- Computer Based E
%D+DB2       - DB2 Products Menu       %U+USER SYSTEM+- User Developed S
%I+Systems   - Systems Integration     %V+Vendor Sys +- ISSO Vendor Soft
%N+In-House  - NSA Developed Software
%
+    Report all ISPF problems to the %HELP DESK+- Tele%555-2345+
)INIT
  .HELP = ISR00003
  &ZPRIM = YES        /* ALWAYS A PRIMARY OPTION MENU      */
  &ZHTOP = ISR00003   /* TUTORIAL TABLE OF CONTENTS        */
  &ZHINDEX = ISR91000 /* TUTORIAL INDEX - 1ST PAGE         */
  VPUT (ZHTOP,ZHINDEX) PROFILE
)PROC
&ZQ = &Z
    IF (&ZCMD ^= ' ')
       &ZQ =  TRUNC (&ZCMD,'.')
       IF (&ZQ = ' ')
          .MSG = ISRU000
    &ZSEL = TRANS( &ZQ
                0,'PANEL(ISPOPTA)'
                1,'PGM(ISRBRO) PARM(ISRBRO01)'
                2,'PGM(ISREDIT) PARM(P,NSAEDM01)'
                3,'PANEL(ISRUTIL)'
                4,'PANEL(ISRFPA)'
                5,'PGM(ISRJB1) PARM(ISRJPA) NOCHECK'
                6,'PGM(ISRPTC)'
                7,'PGM(ISRYXDR) NOCHECK'
                8,'PANEL(ISRLPRIM)'
                B,'CMD(%BDTINIT)'
                C,'PGM(ISPTUTOR) PARM(ISR00005)'
                D,'CMD(%DBPNINIT)'
                I,'CMD(%@PNL#CI)'
                N,'PANEL(@PNL#PN)'
                P,'CMD(%PHOENIX)'
                S,'PANEL(ZSDSFOP2) NEWAPPL(ISF)'
                T,'PGM(ISPTUTOR) PARM(ISR00000)'
                U,'CMD(%USERCMD)'
                V,'PANEL(@PNL#PV)'
              ' ',' '
                X,'EXIT'
                *,'?' )
   IF (&ZCMD = 'S')
       &ZSEL = 'PGM(ISFISP) NOCHECK NEWAPPL(ISF)'
  &ZTRAIL = .TRAIL
)END
