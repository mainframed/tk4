%             G D D M   Presentation  Graphics  Feature   Rel 4.0
%               (This is only available for IBM 3279 terminals)
%
%SELECT OPTION ===>_ZCMD
%                                                          +USERID   -
%   0 +ALLOCATE  - Allocate data sets %(Note 1)            +TIME     -
%   1 +IMD       - Invoke Interactive Map Definition       +TERMINAL -
%   2 +ICU       - Invoke the Interactive Chart Utility
%   3 +ISE       - Invoke the Image Symbol Editor
%   4 +VSE       - Invoke the Vector Symbol Editor
%   5 +DEMO      - Invoke GDDM Demonstration %(Note 2)+
%   6 +DEMO      - Demonstration - Space Shuttle %(Note 2)+
%   7 +DEMO      - Demonstration - Sabrebat Fighter Plane (X-29A) %(Note
%   8 +DEMO      - Demonstration - 2nd ISG LOGO %(Note 2)+
%   9 +DEMO      - Demonstration - Night Flite %(Note 2)+
%   C +Compress  - Compress your GDDM-PGF files.
%
%                           %----> N O T E S <----+
%    1. +The ALLOCATE procedure (Option 0) for graphics files should onl
%       +done once - before your%INITIAL+invocation of the IMD, ICU, ISE
%       +VSE.  The files remain available for any subsequent TSO session
%    2. +Selection of this option%DOES NOT+require file allocation.
%
+PRESS%END KEY+TO TERMINATE GDDM/ISPF+
)INIT
  .HELP = ADM#PT0
  &ZHTOP = ISR00003                    /* TUTORIAL TABLE OF CONTENTS */
  &ZHINDEX = ISR91000                  /* TUTORIAL INDEX - 1ST PAGE  */
)PROC
  &ZSEL = TRANS( TRUNC (&ZCMD,'.')
                 0,'CMD(%ADM#C0)'
                 1,'CMD(%ADM#C1)'
                 2,'CMD(%ADM#C2)'
                 3,'CMD(%ADM#C3)'
                 4,'CMD(%ADM#C4)'
                 5,'CMD(%ADM#C5)'
                 6,'CMD(%ADM#C6)'
                 7,'CMD(%ADM#C7)'
                 8,'CMD(%ADM#C8)'
                 9,'CMD(%ADM#C9)'
                 C,'CMD(%ADM#CC)'
               ' ',' '
                 *,'?' )
)END
