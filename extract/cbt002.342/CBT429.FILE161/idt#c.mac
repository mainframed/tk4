PROC 0 DEBUG
/* ----------------------------------------------------------- */
/* THIS CLIST CAN BE USED TO INVOKE THE TOOL BOX               */
/*                                                             */
/*                    JIM MARSHALL                             */
/* ----------------------------------------------------------- */
CONTROL NOLIST NOMSG  NOFLUSH  PROMPT
IF  &DEBUG ^= &STR() THEN  +
    CONTROL  MSG  LIST  CONLIST  SYMLIST
ISPEXEC ADDPOP COLUMN(1) ROW(1)
ISPEXEC SELECT PANEL(IDT#P)
ISPEXEC REMPOP
EXIT CODE(0)
