+
+               Systems Programmers  %T O O L+  Box
+
%OPTION  ===>_ZCMD
%
% A+DSMF        - Display SMF                %1+$DI      - Display Initi
% B+DSVCTAB     - Display SVC Table          %2+DUDASD   - D U,DASD
% C+ future                                  %3+DUTAPE   - D U,TAPE
% D+ future                                  %4+SHOWSS   - Show SubSyste
% E+ future                                  %5+avail    - available
% F+ZAP         - Full Screen Superzap       %6+avail    - available
% I+avail       - available
% M+MEMBER      - Member Search
% N+POF         - Poor Man's%NCCF+
% O+avail       - available
% P+avail       - available
% R+avail       - available
% S+LISTSU      - List Installed SU's
% U+DLINK       - Display Linklist
% V+DAPF        - Display APF List
%
% X +EXIT
%
)INIT
  .HELP = ISR00003
  &ZPRIM = NO         /* ALWAYS A PRIMARY OPTION MENU      */
  &ZHTOP = ISR00003   /* TUTORIAL TABLE OF CONTENTS        */
  &ZHINDEX = ISR91000 /* TUTORIAL INDEX - 1ST PAGE         */
)PROC
&ZQ = &Z
   IF (&ZCMD ^= ' ')
      &ZQ = TRUNC(&ZCMD,'.')
      IF (&ZQ = ' ')
         .MSG = ISRU000
  &ZSEL = TRANS( &ZQ
                1,'CMD($DI)'
                2,'CMD(DUDASD)'
                3,'CMD(DUTAPE)'
                4,'CMD(SHOWSS)'
                A,'CMD(SMFDS)'
                B,'CMD(SVCTAB)'
                F,'CMD(%ZAP#C)'
                L,'PANEL(SLSS#P)'
                M,'CMD(%MEMB#C)'
                N,'CMD(POF)'
                S,'CMD(LISTSU)'
                U,'CMD(DLINK)'
                V,'CMD(DAPF)'
              ' ',' '
                X,'EXIT'
                *,'?' )
  &ZTRAIL = .TRAIL
)END
