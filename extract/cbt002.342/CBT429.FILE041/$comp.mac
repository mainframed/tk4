//TSMASCOM JOB D439,SHEEDY,CLASS=A,MSGCLASS=X,NOTIFY=TSMAS
/*JOBPARM ROOM=0350
//* SEE SYS2.JES2136.EXITSRC($COMP)
//*
//* THIS JOB COMPRESSES SYS2.JES2136.EXITSRC.
//*
//* BACKUP - CREATE A COPY OF THE GUY WE WANT TO COMPRESS.
//*
//BACKUP   EXEC PGM=IEBCOPY
//SYSPRINT DD  SYSOUT=*
//SYSUT3   DD  UNIT=SYSDA,SPACE=(CYL,(10,10))
//SYSUT4   DD  UNIT=SYSDA,SPACE=(CYL,(10,10))
//IN1      DD  DISP=SHR,DSN=SYS2.JES2136.EXITSRC    <-------
//OUT1     DD  DISP=(NEW,CATLG,DELETE),DSN=SYS2.JES2136.TEMPSRC,
//             UNIT=SYSDA,DCB=(*.IN1),SPACE=(CYL,(5,5,100))
//SYSIN    DD    *
   COPY O=OUT1,I=IN1
/*
//*
//* COMPR - COMPRESS THE DATA SET.  BYPASS THIS STEP IF ZERO IS LESS
//*         THAN RETURN CODE FOR THE BACKUP STEP.
//*
//COMPR    EXEC PGM=IEBCOPY,COND=(0,LT)
//SYSPRINT DD  SYSOUT=*
//SYSUT3   DD  UNIT=SYSDA,SPACE=(CYL,(1,1))
//SYSUT4   DD  UNIT=SYSDA,SPACE=(CYL,(1,1))
//IN1      DD  DISP=SHR,DSN=SYS2.JES2136.EXITSRC    <-------
   COPY OUTDD=IN1,INDD=IN1
/*
//*
//* DELET - DELETE THE BACKUP DATA SET.  BYPASS THIS STEP IF ZERO IS
//*         LESS THAN RETURN CODE OF ANY PRECEDING STEP.
//*
//DLET     EXEC PGM=IEFBR14,COND=(0,LT)
//OUT3     DD  DISP=(OLD,DELETE),DSN=SYS2.JES2136.TEMPSRC
