//TSMASPRE JOB (D439),
//             '*FORMS CONTROL PREP ',
//             CLASS=A,
//             MSGCLASS=X,
//             NOTIFY=TSMAS
/*JOBPARM  ROOM=0350
//* SEE SYS2.JES2136.EXITSRC($PREP1)
//*
//* THIS JOB PREPARES LIBRARIES FOR TURNING OVER THE XEROX FORMS
//* CONTROL JES2 EXIT1MIT AND TEC021 INTO PRODUCTION.  ONLY APPLICABLE
//* TO BENJ.  FRANKLIN.
//*
//* THIS STEP SCRATCHES THE PREVIOUS BACKUP MEMBERS FROM PRODUCTION.
//*
//SCRATCH  EXEC PGM=IEHPROGM,PARM='LINECNT=76'
//SYSPRINT DD  SYSOUT=*
//MVSPG2   DD  UNIT=3380,VOL=SER=MVSPG2,DISP=OLD
//MVSPG4   DD  UNIT=3380,VOL=SER=MVSPG4,DISP=OLD
//SYSIN    DD  *
  SCRATCH  DSNAME=SYS1.PLINKLIB,VOL=3380=MVSPG2,MEMBER=EXIT1MIB
  SCRATCH  DSNAME=SYS2.TECH.LINKLIB,VOL=3380=MVSPG4,MEMBER=TEC021B
//*
//* THIS STEP COMPRESSES THE PRODUCTION LIBRARIES AND COPIES OVER THE
//* NEW MEMBERS RENAMED TO TEMPORARY NEW NAMES PRIOR TO TURNOVER.
//*
//COPY     EXEC PGM=IEBCOPY,COND=(8,LT)
//SYSPRINT DD  SYSOUT=*
//SYSUT3   DD  UNIT=SYSDA,SPACE=(CYL,(1,1))
//SYSUT4   DD  UNIT=SYSDA,SPACE=(CYL,(1,1))
//EXITLOAD DD  DSN=SYS2.JES2136.EXITLOAD,DISP=SHR  NEW EXITS
//PLINKLIB DD  DSN=SYS1.PLINKLIB,DISP=SHR          PRODUCTION EXITS
//TECHLINK DD  DSN=SYS2.TECH.LINKLIB,DISP=SHR      PRODUCTION TEC021
COMP1    COPY OUTDD=PLINKLIB,INDD=PLINKLIB   COMPRESS SYS1.PLINKLIB
COPY1    COPY OUTDD=PLINKLIB,INDD=EXITLOAD   COPY AND RENAME EXIT1MIT
EXIT1MIT SELECT MEMBER=((EXIT1MIT,EXIT1MIN,R))
COMP2    COPY OUTDD=TECHLINK,INDD=TECHLINK   COMPRESS SYS2.TECH.LINKLIB
COPY2    COPY OUTDD=TECHLINK,INDD=EXITLOAD   COPY AND RENAME TEC021
TEC021   SELECT MEMBER=((TEC021,TEC021N,R))
//*
//* TEST FOR BAD RETURN CODES.
//*
//STEP97   EXEC PGM=IEFBR14,   DON'T RUN IF BAD RETURN CODES
//             COND=((8,LT,SCRATCH),(0,LT,COPY))
//*
//STEP98   EXEC PGM=TEC064,    DON'T RUN IF STEP97 RUNS (IE: RUN IF ANY
//             COND=(0,EQ,STEP97),  STEP IS NON-ZERO)
//         PARM='PREPARATION FAILED - NOTIFY SUBMITTER OR MIKE SHEEDY'
//STEPLIB  DD  DSN=SYS2.TECH.LINKLIB,
//             DISP=SHR
//*
//* TEST FOR ABEND.
//*
//STEP99   EXEC PGM=TEC064,    DON'T RUN IF NO STEP ABENDS
//             COND=ONLY,
//         PARM='PREPARATION FAILED - NOTIFY SUBMITTER OR MIKE SHEEDY'
//STEPLIB  DD  DSN=SYS2.TECH.LINKLIB,
//             DISP=SHR
