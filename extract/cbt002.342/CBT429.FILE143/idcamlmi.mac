/******* IDCAMS FROM ISPF - EXAMINE PARMS
CONTROL  MSG
ISPEXEC  VGET (UCMMD UENTITY UNPARM UNPARM2 UNPARM3)
SET      SP1 = &NRSTR(&UCMMD)
SET      SP2 = &NRSTR(&UENTITY)
SET      SP3 = &NRSTR(&UNPARM)
SET      SP4 = &NRSTR(&UNPARM2)
SET      SP5 = &NRSTR(&UNPARM3)
SET      UCMMD = &STR(DIAGNOSE)
SET      UENTITY = &STR(ICFCATALOG  INDATASET(YOUR.CATALOG   )
SET      UNPARM  = &STR(INCLUDE CATALOG(OPTIONAL.CAT)
SET      UNPARM2 = &STR(DUMP)
SET      UNPARM3 = &STR(ERRORLIMIT(10)
ISPEXEC VPUT (SP1 SP2 SP3 SP4 SP5 UCMMD UENTITY UNPARM UNPARM2 UNPARM3)
END
