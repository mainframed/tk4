++USERMOD (A$$W003)   /*
      THIS USERMOD IS TO INSTALL THE
      SVC MODULES FOR CICS MODULES BEGIN WITH IGC INSTEAD OF WCSVC

      SVC = 227 - TYPE 6 (CICS PROD)
      SVC = 228 - TYPE 2 (CICS PROD)
      SVC = 229 - TYPE 6 (CICS TEST)
      SVC = 230 - TYPE 2 (CICS TEST)
      SVC = 231 - TYPE 6 (CICS DEVL)
      SVC = 232 - TYPE 2 (CICS DEVL)

      THIS USERMOD REQUIRES THAT A UCLIN
      BE DONE TO ADD THE FMID TO BE VER'D
      AND TO ADD THE SVC TO THE NUCLEUS.

      SET BOUNDARY(GLOBAL) .
          UCLIN .
             ADD GLOBALZONE FMID(A$$SVCS) .
          ENDUCL .
      SET BOUNDARY(TARGETZONE) .
          UCLIN .
             ADD SYSMOD(A$$SVCS) FUNCTION
                 RECDATE(YYDDD) INSDATE(YYDDD) .
             ADD MOD(IGC227) LMOD(IEANUC01)
                 DISTLIB(ASRCLIB)  FMID(A$$SVCS) .
             ADD MOD(IGC228) LMOD(IEANUC01)
                 DISTLIB(ASRCLIB)  FMID(A$$SVCS) .
             ADD MOD(IGC229) LMOD(IEANUC01)
                 DISTLIB(ASRCLIB)  FMID(A$$SVCS) .
             ADD MOD(IGC230) LMOD(IEANUC01)
                 DISTLIB(ASRCLIB)  FMID(A$$SVCS) .
             ADD MOD(IGC231) LMOD(IEANUC01)
                 DISTLIB(ASRCLIB)  FMID(A$$SVCS) .
             ADD MOD(IGC232) LMOD(IEANUC01)
                 DISTLIB(ASRCLIB)  FMID(A$$SVCS) .
          ENDUCL .
      SET BOUNDARY(DLIBZONE) .
          UCLIN .
             ADD SYSMOD(A$$SVCS) FUNCTION
                 RECDATE(YYDDD) INSDATE(YYDDD) .
             ADD MOD(IGC227) FMID(A$$SVCS)
                 DISTLIB(ASRCLIB)  .
             ADD MOD(IGC228) FMID(A$$SVCS)
                 DISTLIB(ASRCLIB)  .
             ADD MOD(IGC229) FMID(A$$SVCS)
                 DISTLIB(ASRCLIB)  .
             ADD MOD(IGC230) FMID(A$$SVCS)
                 DISTLIB(ASRCLIB)  .
             ADD MOD(IGC231) FMID(A$$SVCS)
                 DISTLIB(ASRCLIB)  .
             ADD MOD(IGC232) FMID(A$$SVCS)
                 DISTLIB(ASRCLIB)  .
          ENDUCL .

                                                     07/26/88 BRM  */.
++VER (Z038) FMID(A$$SVCS) .
++MOD (IGC227) LKLIB(INCLUDE) .
++MOD (IGC228) LKLIB(INCLUDE) .
++MOD (IGC229) LKLIB(INCLUDE) .
++MOD (IGC230) LKLIB(INCLUDE) .
++MOD (IGC231) LKLIB(INCLUDE) .
++MOD (IGC232) LKLIB(INCLUDE) .
