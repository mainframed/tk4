++USERMOD (A$$W001)   /*
      THIS USERMOD IS TO INSTALL THE
      SVC MODULES FOR CAD/CAM (MODEL204)

      SVC = 200 - TYPE 4 (DEVL)
      SVC = 203 - TYPE 3 (DEVL) *** THESE TYPE 3'S NEED ** RMODE ANY ***
      SVC = 222 - TYPE 3 (PROD) ***                     ** AMODE 31  ***
      SVC = 240 - TYPE 2 (DEVL)
      SVC = 241 - TYPE 4 (PROD)
      SVC = 253 - TYPE 2 (PROD)

      THIS USERMOD REQUIRES THAT A UCLIN
      BE DONE TO ADD THE FMID TO BE VER'D
      AND TO ADD THE SVC TO THE NUCLEUS.

      SET BOUNDARY(GLOBAL) .
          UCLIN .
             ADD GLOBALZONE FMID(A$$SVCS) .
          ENDUCL .
      SET BOUNDARY(TARGETZONE) .
          UCLIN .
             ADD SYSMOD(A$$SVCS) FUNCTION
                 RECDATE(YYDDD) INSDATE(YYDDD) .
             ADD MOD(IGC240) LMOD(IEANUC01)
                 DISTLIB(ASRCLIB)  FMID(A$$SVCS) .
             ADD MOD(IGC253) LMOD(IEANUC01)
                 DISTLIB(ASRCLIB)  FMID(A$$SVCS) .
          ENDUCL .
      SET BOUNDARY(DLIBZONE) .
          UCLIN .
             ADD SYSMOD(A$$SVCS) FUNCTION
                 RECDATE(YYDDD) INSDATE(YYDDD) .
             ADD MOD(IGC240) FMID(A$$SVCS)
                 DISTLIB(ASRCLIB)  .
             ADD MOD(IGC253) FMID(A$$SVCS)
                 DISTLIB(ASRCLIB)  .
          ENDUCL .

                                                     09/14/88 BRM  */.
++VER (Z038) FMID(A$$SVCS) .
++JCLIN .
