++USERMOD (A$$W004)   /*
      THIS USERMOD IS TO INSTALL THE
      SVC MODULES FOR IDMS

      SVC = 206 - TYPE 1 (IDMS PROD)
      SVC = 239 - TYPE 1 (IDMS PROD)

      THIS USERMOD REQUIRES THAT A UCLIN
      BE DONE TO ADD THE FMID TO BE VER'D
      AND TO ADD THE SVC TO THE NUCLEUS.

      SET BOUNDARY(GLOBAL) .
          UCLIN .
             ADD GLOBALZONE FMID(A$$SVCS) .
          ENDUCL .
      SET BOUNDARY(TARGETZONE) .
          UCLIN .
             ADD SYSMOD(A$$SVCS) FUNCTION
                 RECDATE(YYDDD) INSDATE(YYDDD) .
             ADD MOD(IGC206) LMOD(IEANUC01)
                 DISTLIB(ASRCLIB)  FMID(A$$SVCS) .
             ADD MOD(IGC239) LMOD(IEANUC01)
                 DISTLIB(ASRCLIB)  FMID(A$$SVCS) .
          ENDUCL .
      SET BOUNDARY(DLIBZONE) .
          UCLIN .
             ADD SYSMOD(A$$SVCS) FUNCTION
                 RECDATE(YYDDD) INSDATE(YYDDD) .
             ADD MOD(IGC206) FMID(A$$SVCS)
                 DISTLIB(ASRCLIB)  .
             ADD MOD(IGC239) FMID(A$$SVCS)
                 DISTLIB(ASRCLIB)  .
          ENDUCL .

                                                     07/26/88 BRM  */.
++VER (Z038) FMID(A$$SVCS) .
++HOLD(A$$W004) SYSTEM FMID(A$$SVCS) REASON(UCLIN) DATE(88208)
                COMMENT(RUN UCLIN AS SHOWN IN USERMOD COMMENTS) .
++MOD (IGC206) LKLIB(INCLUDE) .
++MOD (IGC239) LKLIB(INCLUDE) .
