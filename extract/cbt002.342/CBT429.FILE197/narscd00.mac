//AIMS31SC JOB 'IMSGROUP  SH28205610*110A044         999  3000007037',
//  REGION=999K,TIME=5,MSGLEVEL=1,MSGCLASS=H,NOTIFY=$A1016
//*MAIN ORG=LOCAL,CLASS=L5
//NARSCD00  EXEC IMSUPDTE,MEMBER=NARSCD00,
//             P=IMA,DISPR=SHR,
//  LOAD='PVT.MODS',
//  SYMI=SOURCETM,SYMO=SOURCETM,ACCT=1,SOUT='*'
//C.SYSIN  DD *
  TITLE ' THE ORIGINAL MACRO=NARSCD '
         COPY  NARSCD
         COPY  NARPST
         TITLE ' THE REAL    CSECTS=(NARSCDID,NARSCD10) '
         PUNCH ' REPLACE NARSTEST '
         PUNCH ' ENTRY   NARSCD00 '
NARSCDID CSECT
         DC     0D'0',16C'ROCKWELL '
         DC     CL16'&SYSDATE-&SYSTIME'
         PRINT NOGEN
         REQUATE
         PRINT   GEN
NARSECT  NARSCD  SECT=CSECT
TAILBACK DC     0D'0',CL4'RIS0',A(NARSECT)
         TITLE ' THE SAMPLE   DSECT=NARSCD '
QQ       DSECT
TESTDSEC NARSCD P=Q,PP=N
         TITLE ' SOME TEST CASES OF VARIOUS EXPANSIONS OF NARSCDF '
NARSTEST CSECT
         PROLOGUE
     NARSCDF R6,NARSCFA#
         SPACE 3
     NARSCDF R6,NARSCFA0
         SPACE 3
     NARSCDF R6,NARSCFA3
         SPACE 3
     NARSCDF R6,NARSCFA#,KNOWN=SCD
         SPACE 3
     NARSCDF R6,NARSCFA0,KNOWN=SCD
         SPACE 3
     NARSCDF R6,NARSCFA3,KNOWN=PST
         SPACE 3
     NARSCDF R6,NARSCD10,KNOWN=SCD
         SPACE 3
     NARSCDF R6,NARSTIM0,KNOWN=NARSCD
         LTORG
         PRINT NOGEN
         ISCD  SCDBASE=0
         IDLI  PSTBASE=0
         END
