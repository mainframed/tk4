//$SPBHW   JOB  (BHWEST,NOBOX),'SYSTEMS*BRIAN',CLASS=7,
//     MSGCLASS=W,NOTIFY=$SPBHW,REGION=0M,TIME=1440
//*
//*
//*   REFER   $SPBHW.PDS.DATA(EX16$)
//*
//ASMFCL PROC  CLASS='*',      -   SYSOUT CLASS
//             MEM=,
//             DSN='$SPBHW.PDS.DATA',
//             LOAD='SYS1.LINKLIB',
//             WORK1=SYSDA,        DASD OUTPUT TYPE
//             WORK2=SYSDA,        DASD WORK TYPE
//             PRI=1,              PRIMARY ALLOCATION
//             SEC=1,              SECONDARY ALLOCATION
//             DIR=1,              NUMBER OF DIRECTORY BLOCKS
//             ALLOC=TRK           ALLOC INCREMENT
//ASM      EXEC  PGM=ASMA90,REGION=5M,COND=EVEN,
//         PARM='OBJECT,NODECK'
//SYSPRINT DD  SYSOUT=&CLASS
//SYSLIB   DD  DSN=SYS1.HASPSRC,DISP=SHR
//         DD  DSN=SYS1.MACLIB,DISP=SHR
//         DD  DSN=SYS1.MODGEN,DISP=SHR
//         DD  DSN=SYS1.AMODGEN,DISP=SHR
//SYSUT1   DD  SPACE=(CYL,(10,10)),UNIT=&WORK2
//SYSUT2   DD  SPACE=(CYL,(10,10)),UNIT=&WORK2
//SYSUT3   DD  SPACE=(CYL,(10,10)),UNIT=&WORK2
//SYSPUNCH DD  DUMMY
//SYSLIN   DD  DSN=&&INDXOBJ,DISP=(NEW,PASS),
//             UNIT=&WORK1.,FREE=CLOSE,
//             SPACE=(&ALLOC.,(&PRI.,&SEC.),RLSE)
//SYSIN    DD  DSN=&DSN.(&MEM),DISP=SHR
//LKED     EXEC  PGM=IEWL,REGION=2M,COND=(4,LT,ASM),
//         PARM='LIST,MAP,LET,AC=1,RENT,REUS,REFR'
//SYSPRINT DD  SYSOUT=&CLASS
//SYSUT1   DD  SPACE=(CYL,(10,10)),UNIT=&WORK2
//SYSLIB   DD  DSN=$SPBHW.VTOC.LOAD,DISP=SHR
//SYSLMOD  DD  DSN=&LOAD,DISP=SHR,
//             SPACE=(&ALLOC.,(&PRI.,&SEC.,&DIR.))
//SYSLIN   DD  DSN=&&INDXOBJ,DISP=(OLD,PASS)
//         DD  DDNAME=SYSIN
//       PEND
//ASMFCL      EXEC ASMFCL,MEM=EX16
//LKED.SYSIN   DD   *
   NAME JESXIT16(R)
