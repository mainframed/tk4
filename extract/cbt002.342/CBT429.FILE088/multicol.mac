//P0010    EXEC SAS,
	//             SORT=50
	//SASLIST  DD  SYSOUT=Q
	//SCRATCH  DD  DSN=&&SCRATCH,
	//             DISP=(NEW,PASS,DELETE),
	//             SPACE=(TRK,(5,5)),UNIT=SYSDA
	//VMF      DD  DSN=SYS3.CA.TLMS.VMF,
	//             DISP=SHR,
	//             DCB=BUFNO=255
	//SYSIN    DD  *
	
	   TITLE1 'Scratch List' ;
	   TITLE2 'Merrill Lynch Europe' ;
	
	  options errors=0 ;
	
	 DATA _null_ ;
	   INFILE VMF;
	   FILE SCRATCH NOPRINT ;
	 INPUT
	  @1    TYPE        #CHAR1.
	  @3    VOLUME      #CHAR6.
	  @130  STATUS           1.
	  @273  DSN         #CHAR44.
	 ;
	 IF STATUS  =  2  AND
	    TYPE EQ 'B'   ;
	  PUT @1 VOLUME
	      @10 dsn ;
	 RUN ;
	
	 DATA  _NULL_ ;
	   INFILE scratch ;
	   FILE PRINT N = PS ;
	   RETAIN I J 1 ;
	   I = 1 ;
	   J = 5 ;
	   DO I = 1 TO 80 BY 10 ;
	   DO WHILE (J LE 55) ;
	      INPUT  @1 VOLUME #CHAR6.
	             @10 dsn    #char44.
	       ;
	        PUT #J @I VOLUME  ;
	             J = J + 1 ;
	    END;
	             J = 5 ;
	    END;
	        PUT _PAGE_;
	  RUN;
