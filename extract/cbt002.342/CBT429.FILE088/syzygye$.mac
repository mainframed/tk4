//TMSOPTNS DD  *
REPORT49 LOGO=NO
//OUTPUT   DD DUMMY
//SYSOUT   DD DUMMY
//SORTLIB  DD DSN=SYS1.SORTLIB,DISP=SHR
//SORTWK01 DD UNIT=SYSDA,SPACE=(CYL,(2))
//SORTWK02 DD UNIT=SYSDA,SPACE=(CYL,(2))
//SORTWK03 DD UNIT=SYSDA,SPACE=(CYL,(2))
//SORTWK04 DD UNIT=SYSDA,SPACE=(CYL,(2))
//SORTWK05 DD UNIT=SYSDA,SPACE=(CYL,(2))
//SORTWK06 DD UNIT=SYSDA,SPACE=(CYL,(2))
//SYSIN    DD *
CONTROL-SECTION
   SORT VOLSER
   TITLE 'TAPES AVAILABLE FOR SCRATCH THAT ARE NOT NEW'
   DEFINE FIRST     CH 'H00000'
   DEFINE ENDTAPE   CH 'H05000'
   DEFINE BDAY      PD '99001'
   DEFINE SCRT      HX '04'
   DEFINE DELT      HX '02'
   DEFINE STK       HX '02'
   DEFINE SILO      HX '01'
   DEFINE Z         BI '0000'
PROCESS-SECTION
  WHEN VOLSER  LT FIRST
       REJECT
  WHEN VOLSER  GT ENDTAPE
       REJECT
  WHEN BTHDATE GT BDAY
       REJECT
  WHEN FLAG1   ON DELT
       REJECT
  WHEN FLAG1 ON SCRT AND ROBTY EQ STK
       LIST
REPORT-SECTION
  PRINT VOLSER
  PRINT ROBTY
  PRINT CRTDT
  PRINT EXPDT
  PRINT CJOB
  PRINT DSN
  PRINT VENDOR
