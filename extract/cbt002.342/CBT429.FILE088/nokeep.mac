//$SPBHW      JOB (BHWEST,NOBOX),'$SPBHW',
//         CLASS=7,MSGCLASS=W,TIME=1440,
//         NOTIFY=$SPBHW,REGION=0M
//*-----------------------------------------------*
//*   LIB: $SPBHW.PDS.DATA(NOKEEP)
//*   DOC: ASSEMBLE AND LINK USING ASMBHWL
//*-----------------------------------------------*
//ASMBHWL  EXEC ASMBHWL,
//  PARM.C='RENT,NOALIGN',PARM.L='RENT,REUS'
//C.SYSLIB  DD  DSN=SYS1.MACLIB,DISP=SHR,DCB=BLKSIZE=32000
//          DD  DSN=SYS1.AMODGEN,DISP=SHR
//          DD  DSN=$SPBHW.PDS.DATA,DISP=SHR
//SYSIN    DD  *
NOKEEP   TITLE 'NOKEEP -- WTOR EXIT TO SUPPRESS MESSAGES'
*        USE THE FOLLOWING IN MPFLSTXX TO COMPLETELY SUPPRESS
*        A MESSAGE EVERYWHERE.  BE CAREFUL, THE MESSGE WILL NOT
*        BE ABLE TO BE FOUND.  IT WILL BE AS IF IT NEVER HAPPENED.
*
*  MSGID,SUP(YES),RETAIN(NO),USEREXIT(NOKEEP) <-- THIS MODULE IS NOKEEP
*
*BRIAN WESTERMAN 1/98
*
NOKEEP   CSECT
NOKEEP   AMODE 31
NOKEEP   RMODE ANY
         USING *,R12
         STM   R14,R12,12(R13)
         LR    R12,R15
         B     START
         DC    C'PROGRAM=NOKEEP'
         DC    C'PROPERTY OF SYZYGY INCORPORATED'
         DC    C'AND BRIAN WESTERMAN '
         DC    C'@1998 ALL RIGHTS RESERVED'
         DC    C'&SYSDATE',C' &SYSTIME'
         PRINT NOGEN
START    L     R11,0(R1)      PICK UP ADDRESS OF CTXT
         USING CTXT,R11
         OI    CTXTRFB2,CTXTRDTM
RETURN   LM    R14,R12,12(R13)
         SR    R15,R15
         BR    R14
         SPACE 1
         LTORG
         SPACE 3
         PRINT   GEN
         IEZVX100
         PRINT NOGEN
         REGEQU
         END   NOKEEP
//L.SYSLMOD DD  DSN=IPO1.LINKLIB(NOKEEP),DISP=SHR
//REFRESH   EXEC PGM=LINKLLA
//SYSLMOD   DD   DISP=SHR,DSN=IPO1.LINKLIB
//SYSPRINT  DD   SYSOUT=*
