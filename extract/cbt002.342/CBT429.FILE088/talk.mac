PROC 0
CONTROL PROMPT NOFLUSH
     SET &S1 = Y
     GOTO FIRST
DISIT: +
     SET &S1 = N
FIRST: +
     ISPEXEC DISPLAY PANEL(TALKP)
     IF &LASTCC = 8 THEN GOTO BYE
     SET &T = &SUBSTR(1:5,&SYSTIME)
     SE '&STR(&MSG1) (&T)' U(&STR(&UID)),LOGON
     SET &MSG1 = &STR()
     IF &STR(&MSG2) EQ &STR() THEN GOTO DISIT
     SE '&STR(&MSG2) (&T)' U(&STR(&UID)),LOGON
     SET &MSG2 = &STR()
     GOTO DISIT
BYE: +
     ISPEXEC VPUT (UID) PROFILE
     END
