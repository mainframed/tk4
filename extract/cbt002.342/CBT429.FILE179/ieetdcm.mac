* @SPACE(1);
*/********************************************************************/
*/*                                                                  */
*/*           ADDRESSES OF PARTS OF THE SCREEN IMAGE BUFFER  @G811PSS*/
*/*           WHEN IN STATUS DISPLAY MODE                    @G811PSS*/
*/*                                                                  */
*/********************************************************************/
*   2 DCMDLLA   PTR(31),            /* LAST LINE IN MSG AREA @G811PSS*/
*   2 DCMDLL1A  PTR(31),            /* LAST LINE IN MSG AREA + 1
*                                                            @G811PSS*/
*   2 DCMDLSCT  PTR(31),            /* SCT FOR LAST LINE IN MSG AREA
*                                                            @G811PSS*/
*   2 DCMDSCT1  PTR(31),            /* SCT FOR LAST LINE IN MSG AREA
*                                      + 1                   @G811PSS*/
*   2 DCMDSSCT  PTR(31),            /* SSCT FOR LAST LINE IN MSG AREA
*                                      + 1                   @G811PSS*/
*   2 *(2)      PTR(31),            /* RESERVED                  @L1A*/
* @SPACE(1);
*/********************************************************************/
*/*                                                                  */
*/*           ADDRESSES TO BE RESOLVED DURING OPEN               @L1A*/
*/*                                                                  */
*/********************************************************************/
*   2 DCMLSSCT  PTR(31),            /* ADDRESS OF LAST SSCT      @L1A*/
*   2 DCMAEORD  PTR(31),            /* ADDR OF ADJUNCT EXTENDED ORDERS
*                                                            @G860PSS*/
*   2 DCMQAPTR  PTR(31),            /* ADDR OF DATA OBTAINED FROM READ
                                       PARTITION QUERY           @D1A*/
*   2 *(6)      PTR(31),            /* RESERVED                  @D1A*/
*   2 *         PTR(31),            /* RESERVED                  @L1A*/
*    3 DCMLADCN PTR(31),            /* LAST ADCON IN LIST        @L1A*/
* @EJECT;
*/********************************************************************/
*/*                                                                  */
*/*           NUMBER OF LINES IN MESSAGE AREA AND ENTRY      @G811PSS*/
*/*           AREA VALUES                                    @G811PSS*/
*/*                                                                  */
*/********************************************************************/
*   2 DCMFNLMA  FIXED(8),           /* MAX NUMBER LINES IN MSG AREA
*                                      WHEN IN FULL CAPABILITY MODE
*                                                            @G811PSS*/
*   2 DCMMNLMA  FIXED(8),           /* MAX NUMBER LINES IN MSG AREA
*                                      WHEN IN MESSAGE STREAM MODE
*                                                            @G811PSS*/
*   2 DCMDNLMA  FIXED(8),           /* MAX NUMBER LINES IN MSG AREA
*                                      WHEN IN STATUS DISPLAY MODE
*                                                            @G811PSS*/
*   2 DCMENTL1  FIXED(8),           /* LINE NUMBER -1 OF 1ST LINE
*                                      IN ENTRY AREA         @G811PSS*/
*   2 DCMENTL2  FIXED(8),           /* LINE NUMBER -1 OF 2ND LINE
*                                      IN ENTRY AREA         @G811PSS*/
*   2 DCMINLNM  FIXED(8),           /* LINE NUMBER -1 OF INSTRUCTION
*                                      LINE                  @G860PSS*/
*   2 *         FIXED(15),          /* RESERVED              @G860PSS*/
*   2 DCMFENRC  FIXED(15),          /* ADDR OF 2ND LINE IN ENTRY AREA
*                                      IN ROW-COLUMN FORMAT  @G811PSS*/
*   2 DCMENTPO  FIXED(15),          /* OFFSET OF 1ST CHAR IN ENTRY AREA
*                                                            @G811PSS*/
*   2 DCMSCRW   FIXED(15),          /* WIDTH OF SCREEN       @G860PSS*/
*   2 DCMLENTY  FIXED(15),          /* LENGTH OF ENTRY AREA      @D1C*/
*   2 *         FIXED(31),          /* RESERVED              @G860PSS*/
* @EJECT;                           /*                       @G811PSS*/
*/********************************************************************/
*/*                                                                  */
*/*           COMMAND BUFFER AREA FOR THE LAST COMMAND       @G811PSS*/
*/*           ENTERED                                        @G811PSS*/
*/*                                                                  */
*/********************************************************************/
*   2 DCMCBUFA  CHAR(128),          /* COMMAND BUFFER AREA FOR LAST
*                                      COMMAND ENTERED       @G811PSS*/
* @SPACE(1);                             /*                  @G811PSS*/
*/********************************************************************/
*/*                                                                  */
*/*           SAVE AREAS                                     @G811PSS*/
*/*                                                                  */
*/********************************************************************/
*   2 DCMTLEN   FIXED(15),          /* TDCM LENGTH               @D1A*/
*   2 *         FIXED(15),          /* PADDING FOR LENGTH        @D1A*/
*   2 DCMAIDSV  CHAR(6),            /* SAVE AREA FOR AID FROM RMI
*                                                            @G811PSS*/
*   2 *         FIXED(15),          /* RESERVED              @G811PSS*/
*   2 DCMTRDCM  PTR(31),            /* POINTER TO RDCM       @G860P2G*/
*   2 DCMNXTOR  PTR(31),            /* ADDR OF NEXT ADJUNCT EXTENDED
*                                      ORDER                 @G860PSS*/
*   2 DCMCROW   FIXED(8),           /* CURRENT ROW NUMBER -1 @G860PSS*/
*   2 DCMCCOL   FIXED(8),           /* CURRENT COL NUMBER -1 @G860PSS*/
*   2 DCMMFRMF  BIT(8),             /* FULL CAPABILITY MFORM SAVEAREA
*                                                                @L5C*/
*     3 DCMMFRFT BIT(1),            /* DISPLAY TIME STAMP        @L5A*/
*     3 DCMMFRFJ BIT(1),            /* DISPLAY JOB NAME          @L5A*/
*     3 DCMMFRFS BIT(1),            /* DISPLAY SYSTEM NAME       @L5A*/
*     3 *        BIT(5),            /* RESERVED                  @L5A*/
*   2 DCMDELFC  CHAR(2),            /* FULL CAPABILITY DEL SAVE  @L2A*/
*   2 DCMCONFC  CHAR(1),            /* FULL CAPABILITY CON SAVE  @L2A*/
*   2 DCMSEGFC  FIXED(8),           /* FULL CAPABILITY SEG SAVE  @L2A*/
*   2 DCMRNUMF  FIXED(8),           /* FULL CAPABILITY RNUM SAVE @L2A*/
*   2 DCMRTMEF  FIXED(16),          /* FULL CAPABILITY RTME SAVE @L2A*/
*   2 DCMMFRMM  BIT(8),             /* MESSAGE STREAM MFORM SAVEAREA
*                                                                @L5C*/
*     3 DCMMFRMT BIT(1),            /* DISPLAY TIME STAMP        @L5A*/
*     3 DCMMFRMJ BIT(1),            /* DISPLAY JOB NAME          @L5A*/
*     3 DCMMFRMS BIT(1),            /* DISPLAY SYSTEM NAME       @L5A*/
*     3 *        BIT(5),            /* RESERVED                  @L5A*/
*   2 DCMDELMS  CHAR(2),            /* MESSAGE STREAM DEL SAVE   @L2A*/
*   2 DCMCONMS  CHAR(1),            /* MESSAGE STREAM CON SAVE   @L2A*/
*   2 DCMSEGMS  FIXED(8),           /* MESSAGE STREAM SEG SAVE   @L2A*/
*   2 DCMRNUMM  FIXED(8),           /* MESSAGE STREAM RNUM SAVE  @L2A*/
*   2 DCMRTMEM  FIXED(16),          /* MESSAGE STREAM RTME SAVE  @L2A*/
*   2 DCMQALEN  FIXED(15),          /* LENGTH OF AREA TO HOLD READ
*                                      PARTITION QUERY           @D1A*/
*   2 *(3)      FIXED(31),          /* RESERVED                  @L3C*/
* @SPACE(1);                        /*                           @L3A*/
*/********************************************************************/
*/*                                                                  */
*/* THE DCMBUFER WORK AREA IS TO BE USED ON A MODULE-BY-MODULE BASIS */
*/* ONLY.  THIS AREA MUST NOT BE USED AS A COMMUNICATION MEDIUM.     */
*/* DOING SO WOULD DEFEAT THE PURPOSE OF GIVING THE DIDOCS           */
*/* MODULES A SAFE WORK AREA.                                    @L3A*/
*/*                                                                  */
*/********************************************************************/
*   2 DCMBUFER CHAR(168);           /* MODULE WORK AREA          @L3A*/
*@EJECT;
*/********************************************************************/
*/*   CONTROL BLOCK VERSIONS - POSSIBLE VALUES OF DCMTVERN   @G860P2G*/
*/********************************************************************/
* DECLARE
*   DCMTSP21    FIXED(8) CONSTANT(1), /* OS/VS2 HBB2102 LEVEL
*                                                            @G860PSS*/
*   DCMTS212    FIXED(8) CONSTANT(2), /* OS/VS2 JBB2125 LEVEL    @D1A*/
*   DCMTS220    FIXED(8) CONSTANT(3); /* OS/VS2 JBB2220 LEVEL    @L3A*/
*/********************************************************************/
*/*                                                                  */
*/*           MAPPING OF A SCREEN CONTROL TABLE (SCT) ENTRY  @G811PSS*/
*/*                                                                  */
*/*           FIRST BYTE OF AN SCT                           @G811PSS*/
*/*                                                                  */
*/********************************************************************/
*DECLARE
*  1 DCMSCTA CHAR(4) BASED(DCMSCTB),                      /*     @L3C*/
*   2 DCMSCTA1 BIT(8),             /* FIRST BYTE OF AN SCT       @L3A*/
*    3 DCMMSGOR BIT(1),            /* WTOR MESSAGE DISPLAYED     @L3C*/
*    3 DCMMSGIN BIT(1),            /* MESSAGE DISPLAYED IN LINE  @L3C*/
*    3 DCMMSGCN BIT(1),            /* MESSAGE CONTINUED ON NEXT
*                                     NEXT LINE                  @L3C*/
*    3 DCMMSGJK BIT(1),            /* TO WRITE O-O-L DISPLAY FROM.
*                                     MAY CONTAIN JUNK (SDS INTERFACE
*                                     2)                         @L3C*/
*    3 DCMMSGAD BIT(1),            /* MESSAGE CAN BE AUTO
*                                     DELETED                    @L3C*/
*    3 DCMMSGRD BIT(1),            /* REQUEST HAS SPECIFIED
*                                     MESSAGE BE REMOVED         @L3C*/
*    3 DCMMSGIF BIT(1),            /* INFORMATIONAL MESSAGE
*                                     IN LINE                    @L3C*/
*    3 DCMMSGST BIT(1),            /* END OF TABLE INDICATOR     @L3C*/
* @SPACE(1);                             /*                  @G811PSS*/
*/********************************************************************/
*/*                                                                  */
*/*           SECOND BYTE OF AN SCT                          @G811PSS*/
*/*                                                                  */
*/********************************************************************/
*   2 DCMSCTA2 BIT(8),             /* SECOND BYTE OF AN SCT      @L3A*/
*    3 DCMMSGAC BIT(1),            /* ACTION MESSAGE             @L3C*/
*    3 DCMMSGC7 BIT(1),            /* DESCRIPTOR CODE 7 MESSAGE  @L3C*/
*    3 DCMMSGR2 BIT(1),            /* RESERVED - WAS DCMMSGDM
*                                                                @L3C*/
*    3 DCMMSGUA BIT(1),            /* URGENT ATTENTION MESSAGE
*                                     DISPLAYED IN LINE          @L3C*/
*    3 DCMMSGEA BIT(1),            /* EVENTUAL ACTION MESSAGE
*                                     DISPLAYED IN LINE - WAS
*                                     DCMMSGIR                   @L3C*/
*    3 DCMMSGCT BIT(1),            /* THIS IS A CONTINUED LINE   @L3C*/
*    3 DCMMSGPP BIT(1),            /* ISSUED BY PROBLEM PROGRAM  @L3C*/
*    3 DCMMSGCL BIT(1),            /* CONTROL LINE OF AN
*                                     IN-LINE MLWTO              @L3C*/
* @SPACE(1);                             /*                      @L3A*/
*/********************************************************************/
*/*                                                                  */
*/*           THIRD BYTE OF AN SCT                               @L3A*/
*/*                                                                  */
*/********************************************************************/
*   2 DCMSCTA3 BIT(8),             /* THIRD BYTE OF AN SCT       @L3A*/
*     3 DCMEJNUM BIT(1),           /* NUMBER IS IN THE LINE. THIS IS
*                                     A COMMUNICATION BETWEEN
*                                     IEECVETJ AND IEECVFT2.
*                                     (IEECVETJ WILL LET IEECVFT2 KNOW
*                                     FIRST TWO POSITIONS IN THE LINE
*                                     CONTAIN THE NUMBER)        @P1A*/
* @SPACE(1);                             /*                      @L3A*/
*/********************************************************************/
*/*                                                                  */
*/*           FOURTH BYTE OF AN SCT                              @L3A*/
*/*                                                                  */
*/********************************************************************/
*   2 DCMSCTA4 BIT(8);             /* FOURTH BYTE OF AN SCT      @L3A*/
* @SPACE(1);                       /*                            @L3A*/
*/********************************************************************/
*/*                                                                  */
*/* MAPPING OF A SECONDARY SCREEN CONTROL TABLE (SSCT) ENTRY @G811PSS*/
*/* FIRST BYTE OF AN SSCT                                        @L3A*/
*/*                                                                  */
*/********************************************************************/
*DECLARE
*  1 DCMSCTC CHAR(2) BASED(DCMSCTD),
*   2 DCMSCTC1 BIT(8),             /* FIRST BYTE OF AN SSCT      @L3A*/
*    3 DCMSECCL BIT(1),            /* CONTROL LINE OF O-O-L
*                                     DISPLAY                    @L3C*/
*    3 DCMSECLL BIT(1),            /* LABEL LINE OF O-O-L
*                                     DISPLAY                    @L3C*/
*    3 DCMSECDL BIT(1),            /* DATA LINE OF O-O-L DISPLAY @L3C*/
*    3 DCMSECBL BIT(1),            /* THIS LINE IS BLANKED       @L3C*/
*    3 *        BIT(1),            /* RESERVED - WAS DCMSECXX
*                                                                @L3C*/
*    3 *        BIT(1),            /* RESERVED - WAS DCMSECYY
*                                                                @L3C*/
*    3 DCMSECDD BIT(1),            /* LINE RESERVED FOR DYNAMIC DISPLAY
*                                                                @L3C*/
*    3 DCMSECST BIT(1),            /* END OF TABLE INDICATOR         */
* @SPACE (1);                            /*                      @L3A*/
*/********************************************************************/
*/*                                                                  */
*/* SECOND BYTE OF AN SSCT ENTRY                                 @L3A*/
*/*                                                                  */
*/********************************************************************/
*   2 DCMSCTC2 BIT(8);             /* SECOND BYTE OF AN SSCT     @L3A*/
* @EJECT;                           /*                           @L3A*/
*/********************************************************************/
*/*                                                                  */
*/* MAP OF THE EXTENDED START EXTENDED FIELD ORDERS FOR 3270     @D1A*/
*/* DEVICES                                                      @D1A*/
*/*                                                                  */
*/********************************************************************/
*DECLARE
*  1 DCMORDER BASED(DCMORDPT),    /* EXTENDED ORDERS           @L4C*/
*    2 DCMSBAO  CHAR(3),            /* SET BUFFER ADDRESS FIELD
*                                                            @G860PSS*/
*      3 DCMSBA   FIXED(8),         /* SET BUFFER ADDRESS ORDER
*                                                            @G860PSS*/
*      3 DCMSBAA1 FIXED(8),         /* SET BUFFER ADDRESS ROW ADDR
*                                                            @G860PSS*/
*      3 DCMSBAA2 FIXED(8),         /* SET BUFFER ADDRESS COL ADDR
*                                                            @G860PSS*/
*    2 DCMSFEO  CHAR(8),            /* START FIELD EXTENDED FIELD
*                                                            @G860PSS*/
*      3 DCMSFE   FIXED(8),         /* START FIELD EXTENDED ORDER
*                                                            @G860PSS*/
*      3 DCMSFEN  FIXED(8),         /* NUMBER OF TYPE/VALUE PAIRS WHICH
*                                      FOLLOW                @G860PSS*/
*      3 DCMSFEFT FIXED(8),         /* FIELD ATTRIBUTE TYPE  @G860PSS*/
*      3 DCMSFEFA FIXED(8),         /* FIELD ATTRIBUTE VALUE @G860PSS*/
*      3 DCMSFECT FIXED(8),         /* COLOR ATTRIBUTE TYPE  @G860PSS*/
*      3 DCMSFECA FIXED(8),         /* COLOR ATTRIBUTE VALUE @G860PSS*/
*      3 DCMSFEHT FIXED(8),         /* HIGHLIGHTING ATTRIBUTE TYPE
*                                                            @G860PSS*/
*      3 DCMSFEHA FIXED(8);         /* HIGHLIGHTING ATTRIBUTE VALUE
*                                                            @G860PSS*/
* @EJECT;                                /*                      @D1C*/
*/********************************************************************/
*/*                                                                  */
*/* MAP OF THE EXTENDED SET ATTRIBUTE ORDERS FOR 3270 DEVICES    @D1A*/
*/*                                                                  */
*/********************************************************************/
* DCL 1 DCMSAO DEF(DCMSFEO),        /* SET ATTRIBUTE FIELD       @D1A*/
*       2 DCMSA1  FIXED(8),         /* SET ATTRIBUTE ORDER       @D1A*/
*       2 DCMSAHT FIXED(8),         /* HIGHLIGHTING ATTRIBUTE TYPE
*                                                                @D1A*/
*       2 DCMSAHA FIXED(8),         /* HIGHLIGHTING ATTRIBUTE VALUE
*                                                                @D1A*/
*       2 DCMSADAT CHAR(1),         /* DATA TO HIGHLIGHT         @D1A*/
*       2 DCMSA2  FIXED(8),         /* SET ATTRIBUTE ORDER       @D1A*/
*       2 DCMSARST FIXED(16) BDY(BYTE); /* CHARACTER
*                                                ATTRIBUTE RESET @D1A*/
* @SPACE(1);                             /*                      @L3A*/
*/********************************************************************/
*/*                                                                  */
*/* MAP OF THE DOM INFORMATION                                   @L3A*/
*/*                                                                  */
*/********************************************************************/
* DCL 1 DCMDOMEN BASED,             /* MAP OF A DOM ENTRY        @L3A*/
*       2 DCMDTCB  PTR(31),         /* JOBSTEP TCB ADDRESS       @L3A*/
*       2 DCMDOMID,                 /* FOUR BYTE DOM ID          @L3A*/
*         3 DCMDSYID FIXED(8),      /* SYSTEM ID                 @L3A*/
*         3 DCMDMID  CHAR(3),       /* DOM ID                    @L3A*/
*       2 DCMDTKAS,                 /* TOKEN AND ASID            @L3A*/
*         3 DCMDTOKN CHAR(4),       /* DOM TOKEN                 @L3A*/
*         3 DCMDASID FIXED(16),     /* ASID                      @L3A*/
*       2 DCMDRSVD CHAR(4);         /* RESERVED                  @L3A*/
* @SPACE(1);                             /*                  @G811PSS*/
*DECLARE DCMEND  (*) CHAR(1)   BASED
*  (ADDR(DCMSTRT )+LENGTH(DCMSTRT ));
