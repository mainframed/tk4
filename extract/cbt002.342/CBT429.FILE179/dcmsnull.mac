         MACRO
&NAME    DCMSNULL &IMAGE=,&FORMAT=,&BLOCK=
.*$MACRO=DCMSNULL   DATE=10/23/79       SOURCE=DCMS (DLH@SDC)
         COPY  DCMSGBLS
         AIF   ('&BLOCK' EQ '').IFA
         AIF   ('&BLOCK' EQ 'IOCB').IOCB
         AIF   ('&BLOCK' EQ 'TDCB').TDCB
         MNOTE 9,'BLOCK= OPERAND MUST BE IOCB OR TDCB'
         MEXIT
.TDCB    ANOP
         MNOTE 9,'TCDB GENERATION NO WORKY'
         MEXIT
.IOCB    ANOP
&NAME    LM    R0,R1,IOCB
         AGO   .INVOK
.IFA     ANOP
&NAME    LA    R0,&FORMAT
         LA    R1,&IMAGE
.INVOK   ANOP
         BAL   R14,@CLEAR
*
**
*
&DCMSCLR SETB  1
         MEND
