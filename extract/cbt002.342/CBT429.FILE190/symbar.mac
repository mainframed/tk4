         MACRO
         SYMBAR &MODE=DEC
R0       EQU   0  PARAMETER PASSING REG./UTILITY
R1       EQU   1  PARAMETER PASSING REG./UTILITY
R2       EQU   2  UTILITY
R3       EQU   3  UTILITY
R4       EQU   4  UTILITY
R5       EQU   5  UTILITY
R6       EQU   6  UTILITY
R7       EQU   7  UTILITY
R8       EQU   8 UTILITY
R9       EQU   9  UTILITY
         AIF   (K'&MODE NE 3).MHEX
         AIF   ('&MODE'(1,3) EQ 'HEX').HEX
         AIF   ('&MODE'(1,3) EQ 'DEC').DEC
.MHEX    MNOTE 4,'INVALID KEYWORD OPERAND, ASSUME ''HEX'''
.HEX     ANOP
RA       EQU   10 UTILITY
RB       EQU   11 UTILITY
RC       EQU   12 UTILITY
RD       EQU   13 NORMALLY CONTAINS SAVE AREA ADDRESS
RE       EQU   14 NORMALLY CONTAINS RETURN ADDRESS
RF       EQU   15 NORMALLY CONTAINS ENTRY POINT ADDRESS
         AGO   .END
.DEC     ANOP
R10      EQU   10 UTILITY
R11      EQU   11 UTILITY
R12      EQU   12 UTILITY
R13      EQU   13 NORMALLY CONTAINS SAVE AREA ADDRESS
R14      EQU   14 NORMALLY CONTAINS RETURN ADDRESS
R15      EQU   15 NORMALLY CONTAINS ENTRY POINT ADDRESS
.END     MEND
