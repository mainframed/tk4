//ASMVCL  PROC MAC='SYS1.MACLIB',MAC1='SYS1.AMODGEN',
//        MEMBER=
//*
//* DOC: THIS PROCEDURE EXECUTES THE ASSEMBLER AND THE LINKAGE EDITOR.
//*
//ASM     EXEC PGM=IEV90,PARM=OBJ,REGION=2048K
//SYSLIB   DD  DSN=&MAC,DISP=SHR
//         DD  DSN=&MAC1,DISP=SHR
//SYSUT1   DD  DSN=&&SYSUT1,UNIT=3380,SPACE=(1700,(600,600))
//SYSUT2   DD  DSN=&&SYSUT2,UNIT=3380,SPACE=(1700,(300,300))
//SYSUT3   DD  DSN=&&SYSUT3,UNIT=3380,SPACE=(1700,(300,300))
//SYSPRINT DD  SYSOUT=*,DCB=BLKSIZE=1089
//SYSPUNCH DD  DUMMY
//SYSLIN   DD  DSN=&OBJSET,SPACE=(3120,(40,40),,,ROUND),UNIT=DISK,
//             DISP=(MOD,PASS),
//             DCB=(BLKSIZE=3120,LRECL=80,RECFM=FBS,BUFNO=1)
//SYSIN    DD  DSN=&&TEMP(&MEMBER),DISP=(OLD,PASS)
//*
//LKED    EXEC PGM=IEWL,PARM=(XREF,LET,LIST,NCAL),REGION=728K,
//             COND=(4,LT,ASM)
//SYSPRINT DD  SYSOUT=*,DCB=(RECFM=FB,LRECL=121,BLKSIZE=1210)
//SYSLMOD  DD  SPACE=(CYL,(1,1,1)),DSN=&LOD(X),DISP=(,PASS),
//             UNIT=DISK,DCB=BUFNO=1
//*YSLMOD  DD  DSN=&&GOSET(GO),UNIT=3380,SPACE=(1024,(300,90,1)),
//*            DISP=(MOD,PASS)
//SYSUT1   DD  DSN=&&SYSUT1,UNIT=3380,SPACE=(1024,(300,90))
//SYSLIN   DD  DSN=&&OBJSET,DISP=(OLD,PASS),DCB=BLKSIZE=3120
//         DD  DDNAME=SYSIN
//******************************************************************
