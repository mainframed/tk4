//DUMP$XFR JOB (U4757,U4757,000000),'MINISTRY OF SYSTEMS',
//     CLASS=C,
//     MSGCLASS=A
/*NOTIFY $UHMD
/*MAIL=HMD
/*JOBPARM L=999,ROOM=A539
//*********************************************************************
//***                       M V X / X A                              **
//***                     X F E R D U M P                            **
//***                                                                **
//***  THIS PROCEDURE WILL TRANSFER ALL THE SYS1.DUMPXX DATASETS     **
//***  ON A PARTICULAR SYSTEM TO TAPE. THE DUMP TITLES AND TAPE      **
//***  VOLUME WILL BE RECORED IN AN INDEX DATA SET FOR LATER         **
//***  PERUSAL UNDER ISPF. THE TITLES ARE STORED IN DATASET          **
//***  'SYS3.XFER.DATA' AND THE ENTRIES CAN BE BROWSED USING         **
//***  THE 'XIX' CLIST UNDER TSO.                                    **
//***                                                                **
//***  THE FOLLOWING SERVICES ARE PROVIDED:                          **
//***                                                                **
//***  (1) TRANSFER 'SYS1.DUMPXX' DATASETS TO TAPE                   **
//***  (2) UPDATE DUMP TRANFER INDEX ('SYS3.XFER.DATA')              **
//***  (3) WRITE TAPE VOLUME SERIAL TO OPERATOR/PROGRAMMER           **
//***  (4) WRITE DUMP TITLES TO OPERATOR/PROGRAMMER                  **
//***  (5) PRINT THE MASTER TRACE TABLE AND DUMP SUMMARY             **
//***                                                                **
//***  UPDATED FOR MVS/XA VERSION 2.1.2                              **
//***  INSTALLED 11/13/85 - HOWARD M. DEAN                           **
//*********************************************************************
//XFERDMP0 EXEC PGM=IKJEFT01,PARM=AMDPRDMP,REGION=512M
//TAPE      DD   DSNAME=SYS1.DUMP00,DISP=SHR
//SYSUT1    DD   UNIT=SYSDA,SPACE=(4104,(5000,500))
//SYSTSPRT  DD   SYSOUT=*
//SYSTSIN   DD   DUMMY
//SYSIN     DD   DSN=SYS3.PARMLIB(DUMPXFR1),DISP=SHR
//SYSPRINT  DD   SYSOUT=*
//PRINTER   DD   SYSOUT=*
//SERIAL    DD   DSN=SYS3.XFER.DATA,DISP=OLD
//*
//XFERDMP1 EXEC PGM=IKJEFT01,PARM=AMDPRDMP,REGION=512M
//TAPE      DD   DSNAME=SYS1.DUMP01,DISP=SHR
//SYSUT1    DD   UNIT=SYSDA,SPACE=(4104,(5000,500))
//SYSTSPRT  DD   SYSOUT=*
//SYSTSIN   DD   DUMMY
//SYSIN     DD   DSN=SYS3.PARMLIB(DUMPXFR1),DISP=SHR
//SYSPRINT  DD   SYSOUT=*
//PRINTER   DD   SYSOUT=*
//SERIAL    DD   DSN=SYS3.XFER.DATA,DISP=OLD
//*
//XFERDMP2 EXEC PGM=IKJEFT01,PARM=AMDPRDMP,REGION=512M
//TAPE      DD   DSNAME=SYS1.DUMP02,DISP=SHR
//SYSUT1    DD   UNIT=SYSDA,SPACE=(4104,(5000,500))
//SYSTSPRT  DD   SYSOUT=*
//SYSTSIN   DD   DUMMY
//SYSIN     DD   DSN=SYS3.PARMLIB(DUMPXFR1),DISP=SHR
//SYSPRINT  DD   SYSOUT=*
//PRINTER   DD   SYSOUT=*
//SERIAL    DD   DSN=SYS3.XFER.DATA,DISP=OLD
//*
//XFERDMP3 EXEC PGM=IKJEFT01,PARM=AMDPRDMP,REGION=512M
//TAPE      DD   DSNAME=SYS1.DUMP03,DISP=SHR
//SYSUT1    DD   UNIT=SYSDA,SPACE=(4104,(5000,500))
//SYSTSPRT  DD   SYSOUT=*
//SYSTSIN   DD   DUMMY
//SYSIN     DD   DSN=SYS3.PARMLIB(DUMPXFR1),DISP=SHR
//SYSPRINT  DD   SYSOUT=*
//PRINTER   DD   SYSOUT=*
//SERIAL    DD   DSN=SYS3.XFER.DATA,DISP=OLD
//*
//INDEX1    EXEC PGM=IKJEFT01,PARM='%XFER1',REGION=512M
//SYSPROC   DD   DSNAME=SYS3.CLIST,DISP=SHR
//SYSTSPRT  DD   DSNAME=SYS3.XFER.TEMP,DISP=(,CATLG,DELETE),
//   UNIT=SYSDA,
//   SPACE=(TRK,(15,5),RLSE),
//   DCB=(BLKSIZE=800,LRECL=80,RECFM=FB)
//SYSTSIN   DD   DUMMY
//SYSUT2    DD   DSNAME=SYS.TAPE00,
//   DISP=(,PASS,DELETE),
//   VOL=(,RETAIN),
//   DCB=(BLKSIZE=4104,LRECL=4104,RECFM=F),
//   UNIT=3480,LABEL=(1,SL,RETPD=0)
//SERIAL    DD   DSN=SYS3.XFER.DATA,DISP=OLD
//*
//INDEX2   EXEC PGM=IKJEFT01,PARM='%XFER2',REGION=1024K
//SYSPROC  DD   DSNAME=SYS3.CLIST,DISP=SHR
//SYSTSPRT DD   SYSOUT=*
//SYSTSIN  DD   DUMMY
//TEMP     DD   DSN=SYS3.XFER.TEMP,DISP=(OLD,DELETE)
//INDEX    DD   DSN=SYS3.XFER.DATA,DISP=OLD
//*
//*
//TRANDMP0  EXEC PGM=IKJEFT01,PARM=AMDPRDMP,REGION=512M
//TAPE      DD   DSNAME=SYS1.DUMP00,DISP=SHR
//SYSUT2    DD   DSNAME=SYS.TAPE00,DISP=(NEW,PASS),
//    UNIT=3480,LABEL=(1,SL,RETPD=15),
//    VOL=(,RETAIN,REF=*.INDEX1.SYSUT2)
//SYSTSPRT  DD   SYSOUT=*
//SYSTSIN   DD   DUMMY
//SYSPRINT  DD   SYSOUT=*
//PRINTER   DD   SYSOUT=*
//SYSUDUMP  DD   SYSOUT=*
//SYSIN     DD   DSNAME=SYS3.PARMLIB(DUMPXFR2),DISP=SHR
//*
//TRANDMP1  EXEC PGM=IKJEFT01,PARM=AMDPRDMP,REGION=512M
//TAPE      DD   DSNAME=SYS1.DUMP01,DISP=SHR
//SYSUT2    DD   DSNAME=SYS.TAPE01,DISP=(NEW,PASS),
//    UNIT=3480,LABEL=(2,SL,RETPD=15),
//    VOL=(,RETAIN,REF=*.TRANDMP0.SYSUT2)
//SYSTSPRT  DD   SYSOUT=*
//SYSTSIN   DD   DUMMY
//SYSPRINT  DD   SYSOUT=*
//PRINTER   DD   SYSOUT=*
//SYSUDUMP  DD   SYSOUT=*
//SYSIN     DD   DSNAME=SYS3.PARMLIB(DUMPXFR2),DISP=SHR
//*
//TRANDMP2  EXEC PGM=IKJEFT01,PARM=AMDPRDMP,REGION=512M
//TAPE      DD   DSNAME=SYS1.DUMP02,DISP=SHR
//SYSUT2    DD   DSNAME=SYS.TAPE02,DISP=(NEW,PASS),
//    UNIT=3480,LABEL=(3,SL,RETPD=15),
//    VOL=(,RETAIN,REF=*.TRANDMP0.SYSUT2)
//SYSTSIN   DD   DUMMY
//SYSTSPRT  DD   SYSOUT=*
//SYSPRINT  DD   SYSOUT=*
//PRINTER   DD   SYSOUT=*
//SYSUDUMP  DD   SYSOUT=*
//SYSIN     DD   DSNAME=SYS3.PARMLIB(DUMPXFR2),DISP=SHR
//*
//TRANDMP3  EXEC PGM=IKJEFT01,PARM=AMDPRDMP,REGION=512M
//TAPE      DD   DSNAME=SYS1.DUMP03,DISP=SHR
//SYSUT2    DD   DSNAME=SYS.TAPE03,DISP=(NEW,PASS),
//    UNIT=3480,LABEL=(4,SL,RETPD=15),
//    VOL=(,RETAIN,REF=*.TRANDMP0.SYSUT2)
//SYSTSIN   DD   DUMMY
//SYSTSPRT  DD   SYSOUT=*
//SYSPRINT  DD   SYSOUT=*
//PRINTER   DD   SYSOUT=*
//SYSUDUMP  DD   SYSOUT=*
//SYSIN     DD   DSNAME=SYS3.PARMLIB(DUMPXFR2),DISP=SHR
