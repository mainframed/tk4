//C.SYSLIB   DD  DISP=SHR,DSN=TSD.TSDDC.VSAMANAL
//           DD  DISP=SHR,DSN=SYS1.MACLIB
//C.SYSTERM  DD  SYSOUT=*
//C.SYSIN    DD  DISP=SHR,DSN=TSD.TSDDC.VSAMANAL(VSAMANAL)
/*
//L.SYSLMOD   DD  DISP=SHR,DSN=TSD.TSDDC.LOADLIB
