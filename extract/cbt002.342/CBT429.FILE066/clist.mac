./ ADD NAME=DSYS
PROC 0
/* DSYS - DISPLAY VARIOUS DATA ABOUT SYSTEM
CLRSCRN
WRITE                 =-=-=-=-=-=-= SYSTEM STATISTICS =-=-=-=-=-=-=
WRITE
TIMD
WRITE
CPUID
WRITE
CALL 'FIELD.TEST.LOAD(LASTCLPA)'
WRITE
LASTIPL
WRITE
CALL 'FIELD.TEST.LOAD(SMFDS)'
WRITE
WRITE RACF IS &SYSRACF, VER/REL/MOD = &SYSLRACF
IF &SYSHSM  ^= THEN WRITE DFHSM IS AVAILABLE, VER/REL/MOD = &SYSHSM
ELSE                WRITE DFHSM IS NOT AVAILABLE
IF &SYSTSOE ^= THEN WRITE TSO/E IS AVAILABLE, VER/REL/MOD = &SYSTSOE
CALL 'FIELD.TEST.LOAD(DSSLVL)'
WRITE
WRITE LINKLIST LIBRARIES:
CALL 'FIELD.TEST.LOAD(LNKLST)'
WRITE
WRITE RACF DATASET:
CALL 'FIELD.TEST.LOAD(RACFDS)'
WRITE
SYSDSCAN 00:02
./ ADD NAME=VSAMNAME
PROC 0
/* CPROC(VSAMNAME)
CALL 'FIELD.TEST.LOAD(VSAMNAME)' 'SYS1.STGINDEX'
CALL 'FIELD.TEST.LOAD(VSAMNAME)' 'SYS1.MAN1'
CALL 'FIELD.TEST.LOAD(VSAMNAME)' 'SYS1.MAN2'
CALL 'FIELD.TEST.LOAD(VSAMNAME)' 'SYS1.MAN3'
CALL 'FIELD.TEST.LOAD(VSAMNAME)' 'PAGE.WETTERAU.PLPA'
CALL 'FIELD.TEST.LOAD(VSAMNAME)' 'PAGE.WETTERAU.COMMON'
CALL 'FIELD.TEST.LOAD(VSAMNAME)' 'PAGE.WETTERAU.LOCAL1'
CALL 'FIELD.TEST.LOAD(VSAMNAME)' 'PAGE.WETTERAU.LOCAL2'
