//HPZ$F   JOB ,ZBIEGIEN,MSGCLASS=W,CLASS=D
/*JOBPARM ROOM=3426
//*  UNLOAD THE FONTLIB BUT SELECT THE X1.... MEMBERS ONLY
//*  PRODUCE A CROSS REFERENCE REPORT OF THE CHARACTER SETS TO
//*  TRANSLATE TABLES TO FONT NAMES.
//A EXEC SAS@
//OUT1 DD UNIT=DISK,SPACE=(CYL,(5,5),RLSE),
//   DSN=&&TEMP,DISP=(,PASS,DELETE),
//  DCB=(LRECL=80,RECFM=FB,BLKSIZE=11440)
//PROCLIB DD DSN=SYS1.FONTLIB,DISP=SHR
OPTIONS GEN=0;
PROC SOURCE INDD=PROCLIB NOPRINT OUTDD=OUT1 NOALIAS;
SELECT X1:; /* SELECT MEMBERS STARTING WITH X1 ONLY */;
DATA ABC(KEEP=FONT CHARSET TRANSTBL);
INFILE OUT1;
RETAIN SW 0;
LENGTH FONT $ 8;
RETAIN FONT;
INPUT @16 NAMEX1 $CHAR7. @;
IF NAMEX1 = 'NAME=X1' THEN DO; /* PICK UP THE FONT NAME */;
   SW=1;INPUT @21 FONT $CHAR8. ;DELETE;END;
IF SW = 0 THEN DELETE; /* DELETE REST OF THE RECORDS */;
INPUT @46 CHARSET $CHAR8. TRANSTBL $CHAR8.; /*GET REST */;
SW=0;
OUTPUT ABC;

PROC PRINT DATA=ABC;
TITLE1 'FONTS AND THEIR CORRESPONDING CHARACTER SETS AND';
TITLE2 'TRANSLATE TABLES';
TITLE3 'SORTED BY FONT NAME';

PROC SORT DATA=ABC;
BY CHARSET FONT;

PROC PRINT DATA=ABC;
TITLE1 'FONTS AND THEIR CORRESPONDING CHARACTER SETS AND';
TITLE2 'TRANSLATE TABLES';
TITLE3 'SORTED BY CHARACTER SET NAME AND FONT NAME';

PROC SORT DATA=ABC;
BY TRANSTBL FONT;

PROC PRINT DATA=ABC;
TITLE1 'FONTS AND THEIR CORRESPONDING CHARACTER SETS AND';
TITLE2 'TRANSLATE TABLES';
TITLE3 'SORTED BY TRANSLATE TABLE AND FONT NAME';
