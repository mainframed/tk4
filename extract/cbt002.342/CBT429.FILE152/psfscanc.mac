//HPZ$C   JOB ,ZBIEGIEN,MSGCLASS=W,CLASS=D
/*JOBPARM ROOM=3426
//* TAKE THE SAS RASTER FILE AND DELETE CHARACTERS
//* THE DD NAME OF THE SAS FILE IS CHAR
//A EXEC SAS@
//ABC DD *   NAMES OF CHARACTERS TO DELETE, ONE PER CARD
ABCDEF00
//CHAR  DD DSN=HPZ.SAS.CHARSET,DISP=OLD
OPTIONS GEN=0;
DATA ABC  (KEEP= CHARID);
INFILE ABC;
INPUT CHARID $CHAR8.;

PROC SORT DATA=ABC;
 BY CHARID;

DATA CHAR.RASTER(DROP=INRAS INABC);
MERGE CHAR.RASTER(IN=INRAS)
      ABC(IN=INABC);
 BY CHARID;
IF INRAS AND INABC THEN RETURN;
IF INRAS AND NOT INABC THEN OUTPUT CHAR.RASTER;
IF NOT INRAS AND INABC THEN
  PUT ' THE CHARACTER IS NOT IN THE CHARACTER SET ,CHAR=' CHARID;

DATA CHAR.CHARDAT1(DROP=INRAS INABC);
MERGE CHAR.CHARDAT1(IN=INCHAR)
      ABC(IN=INABC);
 BY CHARID;
IF INCHAR AND INABC THEN RETURN;
IF INCHAR AND NOT INABC THEN OUTPUT CHAR.CHARDAT1;
IF NOT INCHAR AND INABC THEN
  PUT ' THE CHARACTER IS NOT IN THE CHARACTER SET ,CHAR=' CHARID;
