//HPZ$P   JOB ,ZBIEGIEN,MSGCLASS=W,CLASS=D
/*JOBPARM ROOM=3426
//* COMPARE THE SAS CHARACTER AND TRANSLATE FILES FOR DIFFERENCES
//* MANY TIMES THERE ARE DIFFERENCES, MATCH BY CHARACTER NAME
//A EXEC SAS@
//OUT DD SYSOUT=W
//CHAR  DD DSN=HPZ.SAS.CHARSET,DISP=OLD
//SYSIN DD *
OPTIONS GEN=0;
DATA ABC(KEEP= CHARID CDPOINT);
MERGE CHAR.TRANSCHR(IN=INTRAN RENAME=(CINCR=XINCR))
      CHAR.CHARDAT1(IN=INCHAR);
 BY CHARID;
IF INCHAR AND NOT INTRAN THEN
 PUT CHARID 'IN CHARACTER ONLY';
IF INTRAN AND NOT INCHAR THEN
 PUT CHARID 'IN TRANSLATE ONLY';
IF INTRAN AND INCHAR THEN OUTPUT ABC;

PROC SORT DATA=ABC;
 BY CDPOINT;

PROC PRINT DATA=ABC;
FORMAT CDPOINT $HEX2.;
