)ATTR
Ö TYPE(TEXT) SKIP(ON) COLOR(YELLOW)
! TYPE(TEXT) SKIP(ON) COLOR(GREEN)
{ TYPE(TEXT) SKIP(ON) COLOR(RED)
@ TYPE(TEXT) SKIP(ON) COLOR(PINK)
)BODY
Ö----------------------- Generation Data Group Utility -----------------
% Command ===>_ZCMD
+
% GDG DSNAME  ===>_GDGPREF                              Ö(35 CHARS MAXIM
Ö IN QUOTES
+
%  SELECTION ===>_G+
+
+   %I@-@LIST THE GDG INDEX ENTRY     %LÖ-ÖLIST THE GDG INDEX WITH
+       @INFORMATION (SHORT VERSION)      ÖVOLSER INFORMATION AND
+       @WITH IDCAMS LIMIT DISPLAY        ÖIDCAMS LIMIT DISPLAY (LONG VE
+
+
+   %C{-CREATE A GDG INDEX.           %M -!MODIFY THE NUMBER OF GDG DATA
+     { SPECIFY THE # OF ENTRIES          !ENTRIES TO BE CHANGED:  GDG E
+     { TO BE CREATEDÖON NEXT LINE        !TO BE CHANGED/EXPANDED/REDUCE
+                                         !    @(1-255) ===>_GNM+
+
+   %D@-@DELETE THE GDG INDEX.  {ALL DATA SETS WILL BE UNCTLGD
+       {BUT NOT 'DELETED' IF THERE ARE EXISTING DATA ENTRIES.
+       {BE SURE TO DELETE ALL GNNNVXX@BEFORE DELETING INDEX.
+
% LIST Data Set Disposition  ===>_K+ (K-KEEP×D-DELETE×M-MOD) +
+      Ö&ZUSER..GDGPRT.LIST'
+
+Press the %ENTER KEY+to proceede or the%END KEY+to terminate this panel

)INIT
 .CURSOR = GDGPREF
 &G = I
 &GNM = &Z
 IF (&K = &Z)
   &K = D
)PROC
  VER (&GDGPREF,NONBLANK)
  VER (&GDGPREF,DSNAME)
  VER (&K,NONBLANK)
  VER (&K,LIST,K,D,M)
  VER (&G,NONBLANK)
  VER (&G,LIST,C,D,I,L,M)
  IF (&G = 'M')
     VER (&GNM,NONBLANK)
     VER (&GNM,NUM)
     VER (&GNM,RANGE,1,255)
  IF (&G = 'C')
     VER (&GNM,NONBLANK)
     VER (&GNM,NUM)
     VER (&GNM,RANGE,1,255)
  IF (&G = 'D')
     &GNM = 'DLX'
 VPUT (GDGPREF G K GNM DEBUG) SHARED
)END
