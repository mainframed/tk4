//USRADD6  JOB (GEB1,0107),'SYSTEMS',MSGCLASS=A,CLASS=S,
//             MSGLEVEL=(1,1),USER=SYSPGM,PASSWORD=????????
/*ROUTE    XEQ   REG6
/*ROUTE    PRINT ADMIN.FETCH
//STEP1    EXEC TMPBATCH
//SYSPRINT DD  SYSOUT=*
//SYSIN    DD  *
PROFILE NOPREFIX
ALLOC FILE(SYSUADS) DSN(SYS1.UADS) SHR
ALLOC FILE(SYSLBC) DSN(SYS1.BRODCAST) SHR
)DOT USRADD
)SEL &RACF6 = Y
)SET SUB = Y
AU &USERID NAME('&NAME') OWNER(&OWNER) +
DFLTGRP(&DFLTGRP) AUTHORITY(&AUTHORTY) UACC(&UACC) +
&RACFATT0 &RACFATT1 &RACFATT2 +
DATA('&USERDATA')
PW USER(&USERID)
LU &USERID
)ENDSEL
)SEL &TSO6 = Y
)SET SUB = Y
ACCOUNT
ADD (&USERID * * &TSONAM0) SIZE(&TSOSIZ0) UNIT(SYSDA) &TSODEST0 +
&TSOMAXR0 &TSOATT0 &TSOATT1 &TSOATT2 &TSOATT3
L (&USERID)
END
AD &USERID..* GENERIC OWNER(&OWNER) UACC(NONE)
LD ID(&USERID)
DEFINE ALIAS (NAME (&USERID) RELATE(CATALOG.VTSO1))
LISTC ENT(&USERID) ALL
)ENDSEL
)ENDDOT
FREE FILE(SYSUADS)
FREE FILE(SYSLBC)
