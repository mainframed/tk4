//C.SYSIN DD *
MSGQSSSM TITLE 'MESSAGE QUEUEING SUBSYSTEM'
MSGQSSSM CSECT ,
MSGQSSSM AMODE ANY
MSGQSSSM RMODE ANY
R0       EQU   0
R1       EQU   1
R2       EQU   2
R3       EQU   3
R4       EQU   4
R5       EQU   5
R6       EQU   6
R7       EQU   7
R8       EQU   8
R9       EQU   9
RA       EQU   10
RB       EQU   11
RC       EQU   12
RD       EQU   13
RE       EQU   14
RF       EQU   15
AR0      EQU   0
AR1      EQU   1
AR2      EQU   2
AR3      EQU   3
AR4      EQU   4
AR5      EQU   5
AR6      EQU   6
AR7      EQU   7
AR8      EQU   8
AR9      EQU   9
ARA      EQU   10
ARB      EQU   11
ARC      EQU   12
ARD      EQU   13
ARE      EQU   14
ARF      EQU   15
         SAVE  (14,12),,MSGQSSSM*&SYSDATE*&SYSTIME
         USING MSGQSSSM,RB,RC
         LR    RB,RF ADDRESS
         LA    RC,4095(,RB)
         LA    RC,1(,RC)
         LR    R9,R0 MAP SSCVT
         USING SSCT,R9
         LR    RA,R1 MAP SSOB
         USING SSOB,RA MAP SSOB INPUT
         CLC   SSOBFUNC,=AL2(SSOBCONV)
         BE    MSGQCONV CONVERTER/INTERP PROCESSING
         CLC   SSOBFUNC,=AL2(SSOBAGRP)
         BE    MSGQAGRP GROUP ALLOCATION PROCESSING
         CLC   SSOBFUNC,=AL2(SSOBOPEN)
         BE    MSGQOPEN OPEN PROCESSING
         CLC   SSOBFUNC,=AL2(SSOBCLOS)
         BE    MSGQCLOS CLOSE PROCESSING
         CLC   SSOBFUNC,=AL2(SSOBUNAL)
         BE    MSGQUNAL UNALLOCATION PROCESSING
         MVC   SSOBRETN,=AL2(0)
         B     NORMEXIT ????
MSGQCONV EQU   * *
         MVC   SSOBRETN,=AL2(SSCIRTOK)
         B     NORMEXIT
MSGQAGRP EQU   * *
         MVC   SSOBRETN,=AL2(SSAGRQOK)
         B     NORMEXIT
MSGQOPEN EQU   * *
         MVC   SSOBRETN,=AL2(SSDMOK)
         B     NORMEXIT
MSGQCLOS EQU   * *
         MVC   SSOBRETN,=AL2(SSDMOK)
         B     NORMEXIT
MSGQUNAL EQU   * *
         MVC   SSOBRETN,=AL2(SSALRTOK)
         B     NORMEXIT
MSGQAM   EQU   * *
NORMEXIT EQU   * *
         LM    RE,RC,12(RD) RESTORE
         SLR   RF,RF
         BSM   R0,RE SSI RETURN
         LTORG
         IHAPSA
         CVT   DSECT=YES
JESCT    IEFJESCT
         IEFJSCVT
         IEFVTSPL
         IEFJSBVT
         IEFJSSOB (CI,AL,DA,AG),CONTIG=NO
         IKJTCB
         IHARB
         IEFSSARB
         END
