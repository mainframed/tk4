/* REXX */
"ALLOC F(IN) DA('TECH102.NOTERM.DATA') SHR";
"ALLOC F(OUT) DA('TECH110.USER.CNTLLIB(SCNTRMOT)') SHR";
"EXECIO * DISKR IN (FINIS STEM IN.";
OUTCTR=1;
LIST="";
DO I = 1 TO IN.0;
   PARSE VALUE STRIP(SUBSTR(IN.I,2)) WITH W1 W2 W3;
   IF W2="" & W3="" THEN DO;
       IF WORDPOS(W1,LIST)=0 THEN ,
           LIST=LIST W1;
       END;
END;
X=WORDS(LIST);
DO I = 1 TO X;
   OUT.OUTCTR=WORD(LIST,I);
   OUTCTR=OUTCTR+1;
END;
"EXECIO * DISKW OUT (FINIS STEM OUT.";
EXIT 0;
