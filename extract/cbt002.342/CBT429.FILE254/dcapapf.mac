--DROP TABLESPACE SCAP02;
--COMMIT;
  CREATE TABLESPACE SCAP02
  IN DCAP01
  USING VCAT DB2A
  PCTFREE 0
  FREEPAGE 0
  BUFFERPOOL BP0
  SEGSIZE 16
  LOCKSIZE ANY
  CLOSE NO;
  COMMIT;

  CREATE TABLE BUILD.TCAPAPAF
  (DATE               DATE     NOT NULL WITH DEFAULT,
   WEEKDAY            CHAR(03) NOT NULL WITH DEFAULT,
   DOMAIN             CHAR(08) NOT NULL WITH DEFAULT,
   MEAN_PROC_UTIL     DEC(6,2) NOT NULL WITH DEFAULT,
   VAR_PROC_UTIL      DEC(6,2) NOT NULL WITH DEFAULT,
   MAX_PROC_UTIL      DEC(6,2) NOT NULL WITH DEFAULT,
   FREQ0_PROC_UTIL    DEC(6,2) NOT NULL WITH DEFAULT,
   FREQ1_PROC_UTIL    DEC(6,2) NOT NULL WITH DEFAULT,
   FREQ2_PROC_UTIL    DEC(6,2) NOT NULL WITH DEFAULT,
   FREQ3_PROC_UTIL    DEC(6,2) NOT NULL WITH DEFAULT,
   FREQ4_PROC_UTIL    DEC(6,2) NOT NULL WITH DEFAULT,
   FREQ5_PROC_UTIL    DEC(6,2) NOT NULL WITH DEFAULT,
   FREQ6_PROC_UTIL    DEC(6,2) NOT NULL WITH DEFAULT,
   FREQ7_PROC_UTIL    DEC(6,2) NOT NULL WITH DEFAULT,
   FREQ8_PROC_UTIL    DEC(6,2) NOT NULL WITH DEFAULT,
   FREQ9_PROC_UTIL    DEC(6,2) NOT NULL WITH DEFAULT)
   IN DCAP01.SCAP02;
  COMMIT;

  GRANT SELECT, UPDATE, INSERT, DELETE ON TABLE
    BUILD.TCAPAPAF
    TO TECH110;
  COMMIT;
  GRANT SELECT ON TABLE
    BUILD.TCAPAPAF
    TO PUBLIC AT ALL LOCATIONS;
  COMMIT;
