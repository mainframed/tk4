/* REXX EXEC TO MOVE ARTHUR SYSW FILES */
"ALLOC F(INPUT) DA('TECH110.USER.CNTLLIB(ARTOUT)') SHR";
"ALLOC F(OTPUT) DA('TECH110.USER.CNTLLIB(ARTRESTO)') SHR";
"EXECIO * DISKR INPUT (FINIS STEM IN.";
OUTCTR = 1;
DO INDEX = 1 TO IN.0;
     DSN=STRIP(WORD(IN.INDEX,1));
     OUT.OUTCTR=" S DSN="DSN",";
     OUTCTR=OUTCTR+1;
     PARSE VAR DSN Q1 "." Q2 "." Q3 "." QL;
     IF Q2="ARTHUR" THEN NDSN="SYS1.ARTH.V2R6M0."QL;
            ELSE NDSN="SYS1.SYSW.V2R11M7."QL;
     OUT.OUTCTR="   NEWNAME="NDSN",NVOL=SMSU03,";
     OUTCTR=OUTCTR+1;
     OUT.OUTCTR="   MGMTCLAS=NOMIG,STORCLAS=SCSTNRD,DATACLAS=DATAPS";
     OUTCTR=OUTCTR+1;
END;
"EXECIO * DISKW OTPUT (FINIS STEM OUT.";
EXIT 0;
