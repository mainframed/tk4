/* REXX EXEC TO MOVE ARTHUR SYSW FILES */
"ALLOC F(OUTPUT) DA('JEFFK.USER.CNTLLIB(ARTOUT)') SHR";
DUMMY = OUTTRAP("DSNLIST.");
"LISTC LVL('SYS3.ARTHUR.V260') NAMES";
"LISTC LVL('SYS3.SYSTW.V2117') NAMES";
DUMMY = OUTTRAP("OFF");
OUTCTR=1;
DO INDEX = 1 TO DSNLIST.0;
   PARSE VAR DSNLIST.INDEX W1 W2 W3 . ;
   IF W1^="NONVSAM" THEN ITERATE;
   OUT.OUTCTR=" SELECT DSN="W3;
   OUTCTR=OUTCTR+1;
END;
"EXECIO * DISKW OUTPUT (FINIS STEM OUT.";
EXIT 0;
