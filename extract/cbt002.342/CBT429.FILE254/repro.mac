PROC 0 DEBUG

ALLOCATE:  ALLOCATE DSNAME('TECH110.N.TEMP.TRACK.FILE') +
                    DDNAME(DD1) +
                    OLD

           IF &LASTCC NE 0 THEN +
           GOTO ERROR

           ALLOCATE DSNAME('TECH110.N.CTN.TRACK.FILE') +
                    DDNAME(DD2) +
                    SPACE(2,2) CYLINDERS +
                    UNIT(SYSDA) +
                    DSORG(PS) +
                    LRECL(80) +
                    BLKSIZE(23680) +
                    RECFM(F,B) +
                    MOD

           IF &LASTCC NE 0 THEN +
           GOTO ERROR

REPRO:     REPRO INFILE(DD1) OUTFILE(DD2)

           IF &LASTCC NE 0 THEN +
           GOTO ERROR

           FREE DD(DD1) +
           DELETE

           FREE DD(DD2)

           GOTO EXIT

ERROR:     WRITE
           WRITE REPRO "ERROR" !!!!
           WRITE
           WRITE CHECK ERROR MESSAGES
           GOTO END

EXIT:      WRITE
           WRITE REPRO "SUCCESSFUL" !!!!
           WRITE

END:     EXIT
