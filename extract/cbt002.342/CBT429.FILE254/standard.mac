//  SET PLAN='PLAN'          /* PLANNING                             */
//  SET REAL='REAL'          /* REAL ESTATE DEVELOPMENT              */
//  SET SALES='SALES'        /* SALES                                */
//  SET SHIP='SHIP'          /* SHIPPING                             */
//********************************************************************/
