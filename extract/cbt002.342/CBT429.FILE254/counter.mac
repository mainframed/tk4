/* REXX */
"ALLOC F(MLBIN) DA('TECH110.MLB.REPORT11') SHR";
"EXECIO * DISKR MLBIN (FINIS STEM MLB.";
"FREE F(MLBIN)";
LIST = "";
DO I = 1 TO MLB.0;
   ZZ = STRIP(MLB.I);
   IF WORDPOS(ZZ,LIST) = 0 THEN DO;
        LIST = LIST ZZ;
        COUNT.ZZ = 1;
        END;
     ELSE DO;
        COUNT.ZZ = COUNT.ZZ + 1;
       END;
END;
X = WORDS(LIST);
"ALLOC F(OUTPUT) DA('TECH110.USER.CNTLLIB(OUTCOUNT)') SHR";
OUTP = 1;
DO I = 1 TO X;
   PTR = WORD(LIST,I);
   OUTDATA.OUTP = LEFT(PTR,8)RIGHT(COUNT.PTR,8,"0");
   OUTP=OUTP+1;
END;
"EXECIO * DISKW OUTPUT (FINIS STEM OUTDATA.";
