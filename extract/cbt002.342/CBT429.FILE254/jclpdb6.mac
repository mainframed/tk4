//TECH110Z JOB (530000000000),'TECH.JEFF.KAPLAN',
// CLASS=S,MSGCLASS=G,MSGLEVEL=(1,1),NOTIFY=TECH110
//MXGSAS   PROC ENTRY=SASXA1,
//    WORK='30,10',             WORK SPACE, CYLINDERS
//    SORT=10,                  SORT WORK SPACE, CYLINDERS
//    MXGHLQ='SYSS.MXG.V12R03MA',    LEVEL QUALIFIER FOR MXG LIBRARIES
//    SASHLQ='SYSS.SAS.V6R8M0', HIGH LEVEL QUALIFIER FOR SAS LIBRARIES
//    CONFIG=NULLFILE,
//    LOAD='*.NULLPDS,VOL=REF=*.NULLPDS',
//    SASAUTO='*.NULLPDS,VOL=REF=*.NULLPDS',
//    OPTIONS=
//*********************************************************************
//*  JCL PROCEDURE "MXGSAS" TO EXECUTE MXG PROGRAMS UNDER SAS        **
//*                                                                  **
//*  USES "PRODUCTION" MXG DATA SET NAMES OF                         **
//*           MXG.USERID.SOURCLIB      -  INSTALLATION TAILORING     **
//*           MXG.MXG.SOURCLIB         -  MXG MASTER SOURCE LIBRARY  **
//*           MXG.MXG.FORMATS          -  MXG FORMAT LIBRARY         **
//*                                                                  **
//*  VENDOR:  MERRILL CONSULTANT'S MXG SOFTWARE  214-351-1966        **
//*                                                                  **
//*  THE DEFAULT SYMBOLICS DEFINED ABOVE ASSUME YOUR HIGH LEVEL      **
//*   QUALIFIER FOR SAS LIBRARIES IS "SAS.SAS608" AND YOUR HIGH      **
//*   LEVEL QUALIFIER FOR MXG LIBRARIES IS "MXG".                    **
//*                                                                  **
//*********************************************************************
//MXGSAS   EXEC PGM=&ENTRY,PARM='SORT=&SORT &OPTIONS'
//NULLPDS   DD DISP=(MOD,PASS),DSN=&&NULLPDS,UNIT=SYSDA,
//    SPACE=(TRK,(1,1,1)),DCB=BLKSIZE=6160
//STEPLIB   DD DISP=(SHR,PASS),DSN=&LOAD
//          DD DISP=SHR,DSN=&SASHLQ..LIBRARY
//CONFIG    DD DISP=SHR,DSN=&SASHLQ..CNTLLIB(BATCHXA)
//          DD DISP=SHR,DSN=&MXGHLQ..SRCELIB(CONFIG)
//          DD DISP=SHR,DSN=&CONFIG
//SASAUTOS  DD DISP=(SHR,PASS),DSN=&SASAUTO
//          DD DISP=SHR,DSN=&SASHLQ..AUTOLIB
//SASHELP   DD DISP=SHR,DSN=&SASHLQ..SASHELP
//SASMSG    DD DISP=SHR,DSN=&SASHLQ..SASMSG
//WORK      DD UNIT=SYSDA,SPACE=(CYL,(&WORK),,,ROUND)
//SASLOG    DD SYSOUT=*
//SASLIST   DD SYSOUT=*
//SASPARM   DD UNIT=SYSDA,SPACE=(400,(100,300)),
//    DCB=(RECFM=FB,LRECL=80,BLKSIZE=400,BUFNO=1)
//SORTWK01  DD UNIT=SYSDA,SPACE=(CYL,(&SORT))
//SORTWK02  DD UNIT=SYSDA,SPACE=(CYL,(&SORT))
//SORTWK03  DD UNIT=SYSDA,SPACE=(CYL,(&SORT))
//INSTREAM  DD UNIT=SYSDA,SPACE=(CYL,(1,1))
//LIBRARY   DD DISP=SHR,DSN=&MXGHLQ..FORMATS
//SOURCLIB  DD DISP=SHR,DSN=&MXGHLQ..CNTLLIB
//          DD DISP=SHR,DSN=&MXGHLQ..SRCELIB
// PEND
//S1 EXEC MXGSAS,TIME=15,WORK=200
//PDB      DD DSN=TECH110.MXG.PDB,DISP=(,CATLG),
//            DCB=RECFM=FS,
//            SPACE=(CYL,(5,3))
//*
//SPIN     DD DSN=TECH110.MXG.SPIN,DISP=(,CATLG),
//            DCB=RECFM=FS,
//            SPACE=(CYL,(5,3))
//*
//*   BY DEFAULT, MXG SENDS CICSTRAN DATA TO THE CICSTRAN DDNAME, WHICH
//*    MOST SITES PUT ON TAPE BECAUSE OF ITS VOLUME, AND WHICH IS THEN
//*    READ BACK IN WITH ASUMCICS TO PUT THE PDB.CICS SUMMARIZED DATASET
//*    IN YOUR DAILY PDB.  YOU CONTROL THE DESTINATION IN IMACCICS.
//*   IF YOU DO NOT HAVE TYPE 110 SMF RECORDS TO PROCESS, YOU CAN REMOVE
//*    THIS CICSTRAN DDNAME, PROVIDED YOU FIRST EDIT MEMBER IMACCICS AND
//*    CHANGE ALL DDNAMES THEREIN ALL TO "WORK".
//CICSTRAN DD DSN=TECH110.MXG.CICSTRAN,DISP=(,CATLG),  CICS 110 TRANS
//            DCB=RECFM=FS
//*
//*   BY DEFAULT, MXG SENDS DB2ACCT TO THE PDB, BECAUSE IT IS NOT TOO
//*    LARGE AT MOST MXG SITES.  HOWEVER, LARGE DB2 SITES NEED TO BE
//*    ABLE TO PUT DB2ACCT TO TAPE.  BY CHANGING THE DEFAULT _LDB2ACC
//*    FROM "DB2ACCT" TO "DB2ACCT.DB2ACCT" IN MEMBER IMACDB2, AND THEN
//*    ADDING THE //DB2ACCT DD, YOU TOO CAN STORE YOUR DETAIL DB2ACCT
//*    DATA SET IN THE DB2ACCT DATA LIBRARY ON TAPE.
//DB2ACCT  DD DSN=TECH110.MXG.DB2ACCT,DISP=(,CATLG),    DB2 101 ACCT
//            DCB=RECFM=FS
//*
//SMF      DD DSN=TECH110.SMF.EXTRACT,DISP=SHR
//*
//SOURCLIB DD DSN=SYSS.MXG.V12R03MA.CNTLLIB,DISP=SHR
//         DD DSN=SYSS.MXG.V12R03MA.SRCELIB,DISP=SHR
//*
//SYSIN    DD *

  %INCLUDE SOURCLIB(BUILDPDB); /*BUILD PDB                      */

     /* ASUM MEMBERS SUMMARIZE DETAIL DATA INTO THE PDB */

  %INCLUDE SOURCLIB(ASUMCICS); /*BUILD PDB.CICS FROM CICSTRAN     */
  %INCLUDE SOURCLIB(ASUMDB2A); /*BUILD PDB.ASUMDB2A FROM DB2ACCT  */
  %INCLUDE SOURCLIB(ASUMJOBS); /*BUILD PDB.JOBSKED FROM JOBS      */
  %INCLUDE SOURCLIB(ASUM70PR); /*BUILD PDB.ASUM70PR FROM TYPE70PR */
  %INCLUDE SOURCLIB(ASUMTMNT); /*BUILD PDB.TAPEMNTS FROM TYPETMNT */

  /* ANAL MEMBERS ANALYZE AND PRODUCE SAMPLE REPORTS */

  %INCLUDE SOURCLIB(ANALCONT);
  %INCLUDE SOURCLIB(ANALDALY);   /* NOTE:  IT IS NOT NORMAL TO    */
  %INCLUDE SOURCLIB(ANALPROG);   /*        CONCATENATE ALL OF     */
  %INCLUDE SOURCLIB(ANALESV);    /*        THESE MXG ANALYSIS     */
  %INCLUDE SOURCLIB(ANALMPL);    /*        AND REPORTING PROGRAMS */
  %INCLUDE SOURCLIB(ANALAVAL);   /*        IN YOUR DAILY PDB RUN. */
  %INCLUDE SOURCLIB(ANALPRNT);   /*        THEY ARE INCLUDED HERE */
  %INCLUDE SOURCLIB(ANALTAPE);   /*        SO THAT YOU WILL GET   */
  %INCLUDE SOURCLIB(ANALTSOR);   /*        SOME SAMPLE REPORTS ON */
  %INCLUDE SOURCLIB(ANALTURN);   /*        YOUR FIRST EXECUTION OF*/
  %INCLUDE SOURCLIB(ANALCICS);   /*        BUILDPDB.              */
