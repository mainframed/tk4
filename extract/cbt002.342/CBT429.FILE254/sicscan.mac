/* REXX SICSCAN */
"ALLOC F(IN) DATASET('TECH110.SIC') SHR";
"ALLOC F(OUT) DATASET('TECH110.SICMINOR') SHR";
OUTPTR=1;
"EXECIO * DISKR IN (FINIS STEM SIC.";
DO I = 1 TO SIC.0;
    SIC = SUBSTR(SIC.I,1,4);
    TEXT = SUBSTR(SIC.I,5);
    IF SUBSTR(SIC,3,1)="0" × SUBSTR(SIC,4,1)="0" THEN ITERATE I;
    OUTPUT.OUTPTR = SIC TEXT;
    OUTPTR = OUTPTR+1;
END;
"EXECIO * DISKW OUT (FINIS STEM OUTPUT.";
