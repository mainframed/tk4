/* REXX */
"ALLOC F(INDD) DA('TECH110.TEMP.DATA') OLD";
"EXECIO * DISKR INDD (FINIS STEM T.";
"ALLOC F(OUTDD) DA('TECH110.TEMP.DATA3') OLD";
X=0;
S1=0;S2=0;
DO I = 1 TO T.0;
  PARSE VAR T.I PD PS;
  IF PD^="$" THEN DO;
  S1=PD;
  S2=S2+PS;
  END;
  ELSE DO;
    X=X+1;
    OUTX.X=S1 RIGHT(S2,9,"0");
S1=0;S2=0;
  END;
END;
"EXECIO * DISKW OUTDD (FINIS STEM OUTX.";
EXIT 0;
