/* REXX CREATE JOB TO RESET TSO ALIAS */
"ALLOC F(IN) DA('TECH110.USER.CNTLLIB(TSORMVXL)') SHR";
"ALLOC F(OUTPUT) DA('TECH110.USER.CNTLLIB(TSUALDEF)') SHR";
"EXECIO * DISKR IN (FINIS STEM IN.";
DO I =1 TO IN.0;
   IN.I=STRIP(IN.I);
END;
OUTCTR=1;
DO I = 1 TO IN.0;
   OUT.OUTCTR=" DEL" IN.I "ALIAS";
   OUTCTR=OUTCTR+1;
   OUT.OUTCTR=" DEF ALIAS (NAME("IN.I") REL(SYS1.UCATTPDP.CLUSTER))";
   OUTCTR=OUTCTR+1;
END;
"EXECIO * DISKW OUTPUT (FINIS STEM OUT.";
