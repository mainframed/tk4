/* REXX */
"ALLOC F(INPUT) DSNAME('TECH102.ALLCSD.DATA(JEFF)') SHR";
"EXECIO * DISKR INPUT (STEM IN. FINIS";
TRANSID="";
OUTPTR=1;
DO I = 1 TO IN.0;
    PARSE VAR IN.I . . NEWTRANS . ;
    PARSE VAR NEWTRANS "(" NEWTRANS ")";
    IF NEWTRANS=TRANSID THEN DO;
         OUTPUT.OUTPTR=IN.I;
         OUTPTR=OUTPTR+1;
         END;
    ELSE DO;
         TRANSID=NEWTRANS;
         OUTPUT.OUTPTR=" ";
         OUTPTR=OUTPTR+1;
         OUTPUT.OUTPTR=IN.I;
         OUTPTR=OUTPTR+1;
         END;
END;
"ALLOC F(OUT) DSNAME('TECH102.ALLCSD.DATA(FINISHED)') SHR";
"EXECIO * DISKW OUT (STEM OUTPUT. FINIS";
"FREE F(INPUT)";
"FREE F(OUT)";
EXIT 0;
