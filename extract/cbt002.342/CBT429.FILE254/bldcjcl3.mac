/* REXX BUILD A COPY JOB                                     */
ADDRESS "TSO";
"ALLOC F(INPUT) DA('TECH110.USER.CNTLLIB(COPINPUT)') SHR";
"EXECIO * DISKR INPUT (FINIS STEM INPUT.";
"FREE F(INPUT)";
OP = 1;
OUT.OP="//COPYSYS JOB 5304010530000000,'M MCCARTY',NOTIFY=TECH110,";
OP=OP+1;
OUT.OP="// CLASS=S,MSGCLASS=G,REGION=8M";
OP=OP+1;
OUT.OP="//COPY EXEC PGM=IDCAMS";
OP=OP+1;
OUT.OP="//SYSPRINT DD SYSOUT=*";
OP=OP+1;
OUT.OP="//SYSIN DD *";
OP=OP+1;
DO I = 1 TO INPUT.0 BY 1;
  PARSE VAR INPUT.I IDSN;
  IDSN=STRIP(IDSN);
  PARSE VAR IDSN "SYS1." REMDSN;
  CDSN="SYSC."REMDSN;
  ADSN="SYSA."REMDSN;
  OUT.OP=" DELETE" IDSN "NVSAM NOSCRATCH -";
  OP=OP+1;
  OUT.OP=" CAT(SYS1.MCATSB.CLUSTER)";
  OP=OP+1;
  OUT.OP=" ALTER" IDSN "-";
  OP=OP+1;
  OUT.OP=" NEWNAME("CDSN") -";
  OP=OP+1;
  OUT.OP=" CAT(SYS1.MCATSA.CLUSTER)";
  OP=OP+1;
END;
"ALLOC F(OUTPT) DA('TECH110.USER.CNTLLIB(FIXOUT)') SHR";
"EXECIO * DISKW OUTPT (FINIS STEM OUT.";
"FREE F(OUTPT)";
EXIT 0;
