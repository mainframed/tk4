/* REXX EXTRACT CURRENT ACTIVE STORE COUNT */
"EXECIO * DISKR STORCNTR (FINIS STEM ST.";
DO I = 1 TO ST.0;
    PARSE VAR ST.I MSGNO RCDLIT . . . RCDOUT;
    IF MSGNO^="WER054I" × RCDLIT ^="RCD" THEN ITERATE;
    SAY "THE CURRENT ACTIVE STORE COUNT IS:" STRIP(RCDOUT);
    EXIT 0;
END;
"EXECIO * DISKR DISTCNTR (FINIS STEM DI.";
DO I = 1 TO DI.0;
    PARSE VAR DI.I MSGNO RCDLIT . . . RCDOUT;
    IF MSGNO^="WER054I" × RCDLIT ^="RCD" THEN ITERATE;
    SAY "THE CURRENT DISTRO COUNT IS:" STRIP(RCDOUT);
    EXIT 0;
END;
