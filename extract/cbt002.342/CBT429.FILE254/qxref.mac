EXPLAIN PLAN SET QUERYNO=1 FOR
SELECT JOBNAME,JOBCLASS,
       PROCJSTP,PROCSTEP,PROCNAME,
       EXECPSTP,EXECSTEP,EXECPGM,
       FILEDDNM,FILEACC,FILEDSN,FILEDISP,FILEMEM
FROM BUILD.TTECHJOB,
     BUILD.TTECHPROC,
     BUILD.TTECHEXEC,
     BUILD.TTECHFILE
WHERE FILEJOBN=PROCJOBN AND
      FILEPROC=PROCNAME AND
      FILEJSTP=PROCJSTP AND
      FILEPROC=EXECPROC AND
      FILEPSTP=EXECPSTP AND
      FILEJOBN=JOBNAME AND
      FILEDSN LIKE 'PAA%' AND
      EXECPGM LIKE '%' AND
      FILEMEM LIKE '%' AND
      PROCNAME LIKE '%' AND
      JOBNAME LIKE '%'
ORDER BY FILEDSN
