/* REXX EXEC TO CREATE A LIST OF TSO HLQ */
"ALLOC F(OUTPUT) DA('TECH110.USER.CNTLLIB(TIDOUTO)') SHR";
DUMMY = OUTTRAP("DSNLIST.");
"LISTC ENTRY('SYS1.UCATTPDP.CLUSTER') ALL";
DUMMY = OUTTRAP("OFF");
OUTCTR=1;
DO INDEX = 1 TO DSNLIST.0;
   LINE = STRIP(DSNLIST.INDEX);
   IF SUBSTR(LINE,1,9)^="ALIAS----" THEN ITERATE;
   ALIAS = SUBSTR(LINE,10);
   OUT.OUTCTR=ALIAS;
   OUTCTR=OUTCTR+1;
END;
"EXECIO * DISKW OUTPUT (FINIS STEM OUT.";
EXIT 0;
