/* REXX BUILD A COPY JOB                                     */
ADDRESS "TSO";
"ALLOC F(INPUT) DA('TECH110.USER.CNTLLIB(COPINPUT)') SHR";
"EXECIO * DISKR INPUT (FINIS STEM INPUT.";
"FREE F(INPUT)";
OP = 1;
DO I = 1 TO INPUT.0 BY 1;
  PARSE VAR INPUT.I IDSN;
  IDSN=STRIP(IDSN);
  PARSE VAR IDSN "SYS1." REMDSN;
  TDSN="SYST."REMDSN;
  SDSN="SYSS."REMDSN;
  TDSN="'"TDSN"'";
  SDSN="'"SDSN"'";
  OUT.OP='"ALTER' TDSN'"' "' '×× ,";
  OP=OP+1;
  OUT.OP='"NEWNAME('SDSN') CAT(SYS1.UCATSS.CLUSTER)"';
  OP=OP+1;
END;
"ALLOC F(OUTPT) DA('TECH110.USER.CNTLLIB(FIXOUTA)') SHR";
"EXECIO * DISKW OUTPT (FINIS STEM OUT.";
"FREE F(OUTPT)";
EXIT 0;
