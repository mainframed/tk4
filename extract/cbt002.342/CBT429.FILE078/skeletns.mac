./ ADD NAME=LOGLIST  0103-94182-94278-0635-00036-00036-00000-JCL
)CM
)CM *****     BUILD REXX EXEC FOR ISPF LOG/LIST SYSOUT   *****
)CM *****     ALLOCATION USING AN OUTPUT DESCRIPTOR.     *****
)CM
)CM *****     EXECUTED IN THE INITIAL LOGON CLIST IF     *****
)CM *****     PRESENT IN THE ISPFILE LIBRARY AS MEMBER   *****
)CM *****     "@LOGLIST".                                *****
)CM
/* REXX */
TRACE
lltitle = "&LLTITLE"
llname  = "&LLNAME"
llroom  = "&LLROOM"
llbldg  = "&LLBLDG"
lldept  = "&LLDEPT"
lladdr1 = "&LLADDR1"
lladdr2 = "&LLADDR2"
lladdr3 = "&LLADDR3"
lladdr4 = "&LLADDR4"
"OUTDES LOGLIST REUSE",
  "TITLE('"lltitle"')",
  "NAME('"llname"')",
  "ROOM('"llroom"')",
  "BUILDING('"llbldg"')",
  "DEPT('"lldept"')",
  "ADDRESS('"lladdr1"',",
          "'"lladdr2"',",
          "'"lladdr3"',",
          "'"lladdr4"')"
"ALLOC F(ISPLOG)  SYSOUT(R) HOLD OUTDES(LOGLIST) REUSE",
"  LRECL(125) BLKSIZE(129)  RECFM(V A)"
"ALLOC F(ISPLIST) SYSOUT(R) HOLD OUTDES(LOGLIST) REUSE",
"  LRECL(121) BLKSIZE(1210) RECFM(F B A)"
EXIT
/* EXEC TO ALLOCATE ISPLOG AND ISPLIST SYSOUT DATA SETS WITH A   */
/* DYNAMIC OUTPUT DESCRIPTOR                                     */
