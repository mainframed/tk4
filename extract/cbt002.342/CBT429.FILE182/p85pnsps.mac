)ATTR DEFAULT(%+_)
    % TYPE(TEXT) INTENS(HIGH) COLOR(WHITE) SKIP(ON)
    Ö TYPE(TEXT) INTENS(HIGH) COLOR(YELLOW)
    + TYPE(TEXT) INTENS(LOW)  COLOR(TURQ) SKIP(ON)
    ^ TYPE(OUTPUT) INTENS(LOW)  COLOR(TURQ) SKIP(ON)
    _ TYPE(INPUT) INTENS(HIGH) CAPS(ON) JUST(LEFT) COLOR(RED)
    | TYPE(INPUT) INTENS(HIGH) CAPS(OFF)
    @ TYPE(OUTPUT) INTENS(LOW) COLOR(BLUE)
    } TYPE(TEXT) INTENS(HIGH) COLOR(YELLOW) SKIP(ON) HILITE(USCORE)
    $ TYPE(PS)
)BODY EXPAND(\\)
+-\-\-ÖMEMLIST Source Member List 3+-\-\-
%COMMAND ===>|ZCMD                                            %SCROLL ==
Enter an ISPF command, a PDS subcommand or a special control code:
 % 8+View log  %1+Suspend ISPF   %6+MEMLIST all   %F+Find    % R+Recall
 %LA+Lista     %4+Sublist =      %7+Output table  %L+Locate  %SO+Sort
 %LV+Listv     %5+Check aliases  %9+Swap panels   %O+Options  %X+Above/B
%&PDSMSG  --------------------------------------------------------------
}CMD $NAME    }DATA    $VER.MOD$   CREATED$   LAST MODIFIED$ SIZE+ INIT
)MODEL
_Z   ^Z       _Z       +@Z     @Z         @Z         @Z    @Z    @Z
)INIT
   .HELP  = P85PN5@
   .ZVARS = '(ZSCBR MEMLOPT  MEMLNAME MEMLDATA MEMLVMOD +
            MEMLCDAX MEMLMDAX MEMLMTIM MEMLCSIZ MEMLISIZ MEMLMID)'
)PROC
IF (&ZCMD = 'NAME')
  IF (&ZPSMLS1 = '')
    &ZCMD = 'SORT NAME ASCENDING'
    &ZPSMLS1 = ''     /*      NAME          */
    &ZPSMLS2 = ''     /*      VERSION       */
    &ZPSMLS3 = ''     /*      CREATED       */
    &ZPSMLS4 = ''     /*      MODIFIED      */
    &ZPSMLS5 = ''     /*      SIZE          */
    &ZPSMLS6 = ''     /*      ID            */
    &ZPSMLS7 = ''     /*      TTR           */
    &ZPSMLS1 = 1
  ELSE
    &ZCMD = 'SORT NAME DESCENDING'
    &ZPSMLS1 = ''
IF (&ZCMD = 'VERSION')
  IF (&ZPSMLS2 = '')
    &ZCMD = 'SORT VERSION DESCENDING'
    &ZPSMLS1 = ''     /*      NAME          */
    &ZPSMLS2 = ''     /*      VERSION       */
    &ZPSMLS3 = ''     /*      CREATED       */
    &ZPSMLS4 = ''     /*      MODIFIED      */
    &ZPSMLS5 = ''     /*      SIZE          */
    &ZPSMLS6 = ''     /*      ID            */
    &ZPSMLS7 = ''     /*      TTR           */
    &ZPSMLS2 = 1
  ELSE
    &ZCMD = 'SORT VERSION ASCENDING'
    &ZPSMLS2 = ''
IF (&ZCMD = 'CREATED')
  IF (&ZPSMLS3 = '')
    &ZCMD = 'SORT CREATED DESCENDING'
    &ZPSMLS1 = ''     /*      NAME          */
    &ZPSMLS2 = ''     /*      VERSION       */
    &ZPSMLS3 = ''     /*      CREATED       */
    &ZPSMLS4 = ''     /*      MODIFIED      */
    &ZPSMLS5 = ''     /*      SIZE          */
    &ZPSMLS6 = ''     /*      ID            */
    &ZPSMLS7 = ''     /*      TTR           */
    &ZPSMLS3 = 1
  ELSE
    &ZCMD = 'SORT CREATED ASCENDING'
    &ZPSMLS3 = ''
IF (&ZCMD = 'MODIFIED')
  IF (&ZPSMLS4 = '')
    &ZCMD = 'SORT MODIFIED DESCENDING'
    &ZPSMLS1 = ''     /*      NAME          */
    &ZPSMLS2 = ''     /*      VERSION       */
    &ZPSMLS3 = ''     /*      CREATED       */
    &ZPSMLS4 = ''     /*      MODIFIED      */
    &ZPSMLS5 = ''     /*      SIZE          */
    &ZPSMLS6 = ''     /*      ID            */
    &ZPSMLS7 = ''     /*      TTR           */
    &ZPSMLS4 = 1
  ELSE
    &ZCMD = 'SORT MODIFIED ASCENDING'
    &ZPSMLS4 = ''
IF (&ZCMD = 'SIZE')
  IF (&ZPSMLS5 = '')
    &ZCMD = 'SORT SIZE DESCENDING'
    &ZPSMLS1 = ''     /*      NAME          */
    &ZPSMLS2 = ''     /*      VERSION       */
    &ZPSMLS3 = ''     /*      CREATED       */
    &ZPSMLS4 = ''     /*      MODIFIED      */
    &ZPSMLS5 = ''     /*      SIZE          */
    &ZPSMLS6 = ''     /*      ID            */
    &ZPSMLS7 = ''     /*      TTR           */
    &ZPSMLS5 = 1
  ELSE
    &ZCMD = 'SORT SIZE ASCENDING'
    &ZPSMLS5 = ''
IF (&ZCMD = 'ID')
  IF (&ZPSMLS6 = '')
    &ZCMD = 'SORT ID ASCENDING'
    &ZPSMLS1 = ''     /*      NAME          */
    &ZPSMLS2 = ''     /*      VERSION       */
    &ZPSMLS3 = ''     /*      CREATED       */
    &ZPSMLS4 = ''     /*      MODIFIED      */
    &ZPSMLS5 = ''     /*      SIZE          */
    &ZPSMLS6 = ''     /*      ID            */
    &ZPSMLS7 = ''     /*      TTR           */
    &ZPSMLS6 = 1
  ELSE
    &ZCMD = 'SORT ID DESCENDING'
    &ZPSMLS6 = ''

&PPRE = TRUNC(&ZCMD,3)
IF (&PPRE = 'c ''' OR           /* cHANGE ' command?     */
    &PPRE = 'C ''')             /* Change ' command?     */
  &PDSN = TRUNC(&ZCMD,2)
  &PDSN = .TRAIL
  &PFC = TRUNC(&PDSN,1)         /* If first character    */
  IF (&PFC = '''')              /*   of DSN is "'" check */
    &PREM = .TRAIL              /*   to see if last "'"  */
    &PREM2 = TRUNC(&PREM,'''')  /*   is missing.         */
    IF (&PREM2 = &PREM)         /* If last "'" missing   */
      &ZCMD = '&ZCMD&PFC'       /*   add it to the end.  */
)PNTS
FIELD(ZPS00001) VAR(ZCMD) VAL(NAME)
FIELD(ZPS00002) VAR(ZCMD) VAL(VERSION)
FIELD(ZPS00003) VAR(ZCMD) VAL(CREATED)
FIELD(ZPS00004) VAR(ZCMD) VAL(MODIFIED)
FIELD(ZPS00005) VAR(ZCMD) VAL(SIZE)
FIELD(ZPS00006) VAR(ZCMD) VAL(ID)
)END
