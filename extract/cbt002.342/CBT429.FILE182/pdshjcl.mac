//*
//*  THIS MEMBER GENERATES AND EXECUTES THE HELPLIST PROGRAM FOR PDS
//*
//HELPGEN PROC MEMBER=MISSING,
//             INSTLIB='PDS.INSTALL.LIBRARY'        <<=== INSTALL LIB
//ONE    EXEC  PGM=IEV90,REGION=2048K,
//             PARM=(DECK,NOOBJECT,NORLD,TERM,'XREF(SHORT)')
//SYSUT1   DD  UNIT=VIO,SPACE=(CYL,(10,5))
//SYSUT2   DD  UNIT=VIO,SPACE=(CYL,(10,5))
//SYSUT3   DD  UNIT=VIO,SPACE=(CYL,(10,5))
//SYSPUNCH DD  UNIT=VIO,SPACE=(TRK,(5,5)),DISP=(MOD,PASS),DSN=&&X,
//             DCB=BLKSIZE=3120
//SYSPRINT DD  SYSOUT=*
//SYSTERM  DD  SYSOUT=*
//SYSIN    DD  DISP=SHR,DSN=&INSTLIB.(&MEMBER)
//SYSLIB   DD  DSN=SYS1.AMODGEN,DISP=SHR,DCB=BLKSIZE=16000
//         DD  DSN=SYS1.MACLIB,DISP=SHR
//         DD  DSN=&INSTLIB,DISP=SHR
//       PEND
//*
//*
//HELPLIST EXEC HELPGEN,MEMBER=HELPLIST
//*
//LK1    EXEC  PGM=IEWL,PARM='MAP,LET,NCAL',COND=(0,LT)
//SYSPRINT DD  SYSOUT=*
//IN       DD  DSN=&&X,DISP=(OLD,DELETE)
//SYSLMOD  DD  DISP=SHR,DSN=LINKLIST.LIB            <<=== LINK TARGET
//SYSUT1   DD  UNIT=VIO,SPACE=(TRK,(5,5))
//SYSLIN   DD  *
  INCLUDE  IN
  NAME     HELPLIST(R)                              <<=== LOADMOD NAME
//*
//ONE   EXEC  PGM=HELPLIST                          <<=== LOADMOD NAME
//SYSPRINT DD  SYSOUT=(*,,DX00)
//SYSUT1   DD  DSN=SYS2.HELP,DISP=SHR               <<=== HELP LIBRARY
//SYSIN    DD  *
PDS85                                               <<=== HELP MEMBER
