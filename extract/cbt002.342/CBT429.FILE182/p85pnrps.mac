)ATTR DEFAULT(%+_)
    % TYPE(TEXT) INTENS(HIGH) COLOR(WHITE) SKIP(ON)
    Ö TYPE(TEXT) INTENS(HIGH) COLOR(YELLOW)
    + TYPE(TEXT) INTENS(LOW)  COLOR(TURQ) SKIP(ON)
    ^ TYPE(OUTPUT) INTENS(LOW)  COLOR(TURQ) SKIP(ON)
    _ TYPE(INPUT) INTENS(HIGH) CAPS(ON) JUST(LEFT) COLOR(RED)
    | TYPE(INPUT) INTENS(HIGH) CAPS(OFF)
    @ TYPE(OUTPUT) INTENS(LOW)
    ! TYPE(OUTPUT) INTENS(LOW) JUST(RIGHT)
    } TYPE(TEXT) INTENS(HIGH) COLOR(YELLOW) SKIP(ON) HILITE(USCORE)
    $ TYPE(PS)
)BODY EXPAND(\\)
+-\-\-ÖList Volumes+-\-\-
%COMMAND ===>|ZCMD                                            %SCROLL ==
Enter an ISPF command or a special control code:
 %LA+Lista    %7+Output table  %9+Swap panels  %O+Options
 %ML+Memlist  %8+View log      %F+Find         %S+Sort     %X+Above/Belo
+  ---------------------------------------------------------------------
}CMD $VOLUME}  DATA  $DEV $DEV  +MOUNT$USE$SPACE+ PRI  ALT   F0    FREE
+----$ NAME +--------$ADDR$TYPE  +ATTR$CNT$ USED+ CYLS CYLS  DSCB   VIR
)MODEL
_Z   ^Z     _Z       @Z   @Z     @Z   @Z   @Z   !Z    !Z   !Z    !Z    +
)INIT
   .HELP  = P85PNI@
   .ZVARS = '(ZSCBR PDSVOPT  PDSVVOL  PDSVDATA PDSVADDR PDSVTYPE +
            PDSVATTR PDSVCNT PDSVSPCU PDSVDCYL PDSVACYL +
            PDSVFMT0 PDSVVIRS PDSVFRAG)'
)PROC
IF (&ZCMD = 'VOLUME')
  IF (&ZPSLVX1 = '')
    &ZCMD = 'SORT VOLUME ASCENDING'
    &ZPSLVX1 = ''     /*      VOLUME        */
    &ZPSLVX2 = ''     /*      ADDRESS       */
    &ZPSLVX3 = ''     /*      DEVTYPE       */
    &ZPSLVX4 = ''     /*      COUNT         */
    &ZPSLVX5 = ''     /*      TOTAL         */
    &ZPSLVX6 = ''     /*      LARGEST       */
    &ZPSLVX7 = ''     /*      USED          */
    &ZPSLVX1 = 1
  ELSE
    &ZCMD = 'SORT VOLUME DESCENDING'
    &ZPSLVX1 = ''
IF (&ZCMD = 'ADDRESS')
  IF (&ZPSLVX2 = '')
    &ZCMD = 'SORT ADDRESS ASCENDING'
    &ZPSLVX1 = ''     /*      VOLUME        */
    &ZPSLVX2 = ''     /*      ADDRESS       */
    &ZPSLVX3 = ''     /*      DEVTYPE       */
    &ZPSLVX4 = ''     /*      COUNT         */
    &ZPSLVX5 = ''     /*      TOTAL         */
    &ZPSLVX6 = ''     /*      LARGEST       */
    &ZPSLVX7 = ''     /*      USED          */
    &ZPSLVX2 = 1
  ELSE
    &ZCMD = 'SORT ADDRESS DESCENDING'
    &ZPSLVX2 = ''
IF (&ZCMD = 'DEVTYPE')
  IF (&ZPSLVX3 = '')
    &ZCMD = 'SORT DEVTYPE ASCENDING'
    &ZPSLVX1 = ''     /*      VOLUME        */
    &ZPSLVX2 = ''     /*      ADDRESS       */
    &ZPSLVX3 = ''     /*      DEVTYPE       */
    &ZPSLVX4 = ''     /*      COUNT         */
    &ZPSLVX5 = ''     /*      TOTAL         */
    &ZPSLVX6 = ''     /*      LARGEST       */
    &ZPSLVX7 = ''     /*      USED          */
    &ZPSLVX3 = 1
  ELSE
    &ZCMD = 'SORT DEVTYPE DESCENDING'
    &ZPSLVX3 = ''
IF (&ZCMD = 'COUNT')
  IF (&ZPSLVX4 = '')
    &ZCMD = 'SORT COUNT ASCENDING'
    &ZPSLVX1 = ''     /*      VOLUME        */
    &ZPSLVX2 = ''     /*      ADDRESS       */
    &ZPSLVX3 = ''     /*      DEVTYPE       */
    &ZPSLVX4 = ''     /*      COUNT         */
    &ZPSLVX5 = ''     /*      TOTAL         */
    &ZPSLVX6 = ''     /*      LARGEST       */
    &ZPSLVX7 = ''     /*      USED          */
    &ZPSLVX4 = 1
  ELSE
    &ZCMD = 'SORT COUNT DESCENDING'
    &ZPSLVX4 = ''
IF (&ZCMD = 'USED')
  IF (&ZPSLVX7 = '')
    &ZCMD = 'SORT USED ASCENDING'
    &ZPSLVX1 = ''     /*      VOLUME        */
    &ZPSLVX2 = ''     /*      ADDRESS       */
    &ZPSLVX3 = ''     /*      DEVTYPE       */
    &ZPSLVX4 = ''     /*      COUNT         */
    &ZPSLVX5 = ''     /*      TOTAL         */
    &ZPSLVX6 = ''     /*      LARGEST       */
    &ZPSLVX7 = ''     /*      USED          */
    &ZPSLVX7 = 1
  ELSE
    &ZCMD = 'SORT USED DESCENDING'
    &ZPSLVX7 = ''
)PNTS
FIELD(ZPS00001) VAR(ZCMD) VAL(VOLUME)
FIELD(ZPS00002) VAR(ZCMD) VAL(ADDRESS)
FIELD(ZPS00003) VAR(ZCMD) VAL(DEVTYPE)
FIELD(ZPS00004) VAR(ZCMD) VAL(COUNT)
FIELD(ZPS00005) VAR(ZCMD) VAL(USED )
FIELD(ZPS00006) VAR(ZCMD) VAL(VOLUME)
FIELD(ZPS00007) VAR(ZCMD) VAL(ADDRESS)
FIELD(ZPS00008) VAR(ZCMD) VAL(DEVTYPE)
