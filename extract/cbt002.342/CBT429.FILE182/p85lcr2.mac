)ATTR DEFAULT(%+_)
    % TYPE(TEXT) INTENS(HIGH) COLOR(WHITE) SKIP(ON)
    Ö TYPE(TEXT) INTENS(HIGH) COLOR(YELLOW)
    + TYPE(TEXT) INTENS(LOW)  COLOR(TURQ) SKIP(ON)
    _ TYPE(INPUT) INTENS(HIGH) CAPS(ON) JUST(LEFT) COLOR(RED)
    ^ TYPE(INPUT) INTENS(HIGH) CAPS(ON) JUST(LEFT) COLOR(RED) PAD(_)
    | TYPE(INPUT) INTENS(HIGH) CAPS(OFF)
)BODY EXPAND(\\)
+-\-\-ÖDataset Rename Verification+-\-\-
%OPTION ===>_ZCMD
+
+   Rename of DSN: &PDSCDSN
+       on VOLUME: &PDSCVOL
+   This dataset is not cataloged
+
+   NEW DS NAME   %===>_DSN1                                          +
+
+   Catalog NEW   %===>_CNEW+    (Yes/No to catalog new dataset)
+
+
)INIT
  .HELP   = P85LC@PH
  &CNEW=&PDSCCNEW
  IF (&PDSCCNTL = 'YES')
      &DSN1='''&PDSCDSN'''
  .CURSOR = DSN1
)PROC
  &CNEW = TRANS(TRUNC(&CNEW,1),Y,YES,N,NO,*,*)
  VER (&DSN1,NB,DSNAME)
  VER (&CNEW,NB,LIST,YES,NO)
  &PDSCNEW=&CNEW
     &ENT1=TRUNC(&DSN1,'(')
     IF (&ENT1 ^= &DSN1) /* MEMBER NAME GIVEN */
       VER (&DSN1,PICT,N)
     IF (&ENT1 = &DSN1) /* MEMBER NAME NOT GIVEN */
       &T=TRUNC(&DSN1,1)
       IF (&T='''')
         &ENT1=.TRAIL
         &ENT1=TRUNC(&ENT1,'''')
       IF (&T^='''')
         &ENT1= '&ZPREFIX..&DSN1'
         IF (&ZPREFIX='') &ENT1=&DSN1
  &PDSZSEL = &ENT1
)END
