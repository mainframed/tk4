)ATTR DEFAULT(%+_)
    % TYPE(TEXT) INTENS(HIGH) COLOR(WHITE) SKIP(ON)
    Ö TYPE(TEXT) INTENS(HIGH) COLOR(YELLOW)
    + TYPE(TEXT) INTENS(LOW)  COLOR(TURQ) SKIP(ON)
    _ TYPE(INPUT) INTENS(HIGH) CAPS(ON) JUST(LEFT) COLOR(RED)
    ^ TYPE(INPUT) INTENS(HIGH) CAPS(ON) JUST(LEFT) COLOR(RED) PAD(_)
    | TYPE(INPUT) INTENS(HIGH) CAPS(OFF)
)BODY EXPAND(\\)
+-\-\-ÖO.M    MAP Subcommand+-\-\-
%OPTION  ===>|ZCMD
%
+Enter the member group name and any operands below for%MAP:
%===>|LOPER

+
+Operands:% memgroup         -+(e.g., *; start:end; first*pat; range*; p
%           FULL             -+provide a full map
%           SHORT            -+map only CSECT symbols
%           ENTRY            -+map only the entry point
%           MODULE(Partname) -+restrict the output to specific external
%           JCL/RELINK       -+generate JCL and linkage-editor inputs
+
+Defaults:  memgroup, FULL
)INIT
  &LOPER  = '&OPERS'
  .CURSOR = LOPER
  .HELP   = P85OPHH
  IF(&LOPER  = &Z) &LOPER=&P85#MA1
)PROC
 &P85#MA1=&LOPER
 &PDSZSEL='Map  &LOPER'
)END
