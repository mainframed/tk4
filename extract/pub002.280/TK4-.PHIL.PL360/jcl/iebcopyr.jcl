//IEBCOPY JOB  DUMMY,DUMMY,CLASS=A,MSGCLASS=A,MSGLEVEL=(1,1)
//* ***************************************************************** *
//* RELOAD PL360 LOAD MODULE, SOURCE AND PROCEDURES                   *
//* ***************************************************************** *
//RELOAD  EXEC PGM=IEBCOPY,REGION=1024K
//SYSPRINT DD  SYSOUT=A
//TAPEIN1  DD  DSN=PL360.SOURCE,UNIT=TAPE,DISP=(OLD,KEEP),
//             VOL=(PRIVATE,RETAIN,SER=APL360),LABEL=(1,SL)
//TAPEIN2  DD  DSN=PL360.LOADMOD,UNIT=AFF=TAPEIN1,DISP=(OLD,KEEP),
//             VOL=(PRIVATE,RETAIN,SER=APL360),LABEL=(2,SL)
//TAPEIN3  DD  DSN=PL360.PROC,UNIT=AFF=TAPEIN1,DISP=(OLD,KEEP),
//             VOL=(PRIVATE,RETAIN,SER=APL360),LABEL=(3,SL)
//SOURCE   DD  DSN=SYS1.PL360.SOURCE,DISP=(NEW,CATLG,DELETE),
//             UNIT=SYSDA,SPACE=(CYL,(2,1,15)),
//             DCB=(BLKSIZE=6160,LRECL=80,RECFM=FB)
//PL360LIB DD  DSN=SYS1.PL360LIB,DISP=(NEW,CATLG,DELETE),
//             UNIT=SYSDA,SPACE=(CYL,(5,1,20)),
//             DCB=(BLKSIZE=1024,LRECL=0,RECFM=U)
//PROCLIB  DD  DSN=SYS1.PROCLIB,DISP=SHR
//SYSUT3   DD  UNIT=SYSDA,SPACE=(80,(60,45)),DISP=(NEW,DELETE)
//SYSIN    DD  *
  COPY INDD=((TAPEIN1,R)),OUTDD=SOURCE
  COPY INDD=((TAPEIN2,R)),OUTDD=PL360LIB
  COPY INDD=((TAPEIN3,R)),OUTDD=PROCLIB
/*
//
