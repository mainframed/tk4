.cm Start of REF$
.se c=0
.rm REF$ save nosave
.su on
.sp 2
.cp 10
.se c=&c+1
.se p(&c)=%
.if &&p(&c) le 9;.se p(&c)=' &&p(&c)'
.se t(&c)='&1'
.if &0 >= 2;.se t(&c)='&&t(&c) &2'
.if &0 ge 3;.se t(&c)='&&t(&c) &3'
.if &0 ge 4;.se t(&c)='&&t(&c) &4'
.if &0 ge 5;.se t(&c)='&&t(&c) &5'
.if &0 ge 6;.se t(&c)='&&t(&c) &6'
.us &&t(&c)
.sp
.su off
.rm
.cm End of REF$
