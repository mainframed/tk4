//C7BSS   JOB (M096,0616,99,9999),'SYSTEMS*BRIAN',NOTIFY=HERC01,
//       MSGCLASS=X
//ASMFCL PROC  CLASS='*',      -   SYSOUT CLASS
//             MEM=,
//*            DSN='SYS2.ASM',
//*            LOAD='SYS1.APFLIB',
//             LOAD='SYS2.CMDLIB',
//             WORK1=SYSDA,        DASD OUTPUT TYPE
//             WORK2=SYSDA,        DASD WORK TYPE
//             PRI=1,              PRIMARY ALLOCATION
//             SEC=1,              SECONDARY ALLOCATION
//             ALLOC=TRK           ALLOC INCREMENT
//ASM      EXEC  PGM=IFOX00,REGION=500K,COND=EVEN,
//         PARM='LOAD,NODECK,XREF'
//SYSPRINT DD  SYSOUT=&CLASS
//SYSLIB   DD  DSN=SYS1.MACLIB,DISP=SHR
//         DD  DSN=SYS1.AMODGEN,DISP=SHR
//SYSUT1   DD  SPACE=(CYL,(10,10)),UNIT=&WORK2
//SYSUT2   DD  SPACE=(CYL,(10,10)),UNIT=&WORK2
//SYSUT3   DD  SPACE=(CYL,(10,10)),UNIT=&WORK2
//SYSGO    DD  DSN=&&INDXOBJ,DISP=(NEW,PASS),
//             UNIT=&WORK1.,FREE=CLOSE,
//             SPACE=(&ALLOC.,(&PRI.,&SEC.),RLSE)
//SYSIN    DD  DISP=(OLD,DELETE),DSN=&&SHOWSS
//LKED     EXEC  PGM=IEWL,REGION=256K,COND=(4,LT,ASM),
//         PARM='LIST,MAP,LET,AC=1,RENT,REUS,REFR'
//SYSPRINT DD  SYSOUT=&CLASS
//SYSUT1   DD  SPACE=(CYL,(10,10)),UNIT=&WORK2
//SYSLIB   DD  DSN=SYS2.LINKLIB,DISP=SHR
//SYSLMOD  DD  DSN=&LOAD.(&MEM),DISP=SHR
//SYSLIN   DD  DSN=&&INDXOBJ,DISP=(OLD,PASS)
//         DD  DDNAME=SYSIN
//       PEND
//RACUPDT EXEC PGM=IEBUPDTE
//SYSUT1   DD  DISP=SHR,DSN=SYS2.ASM
//SYSUT2   DD  DISP=(,PASS),UNIT=VIO,DSN=&&SHOWSS,
//             SPACE=(CYL,(1,1)),DCB=(LRECL=80,RECFM=FB,BLKSIZE=3120)
//SYSPRINT DD  SYSOUT=*
//SYSIN    DD  DISP=SHR,DSN=SYS2.ASM(SHSSUPD)
//ASMFCL      EXEC ASMFCL,MEM=SHOWSS
//LKED.SYSIN  DD   *
  NAME SHOWSS(R)
