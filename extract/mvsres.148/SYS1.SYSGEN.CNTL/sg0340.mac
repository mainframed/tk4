//*
//*********************************************************************
//*
//HMASMP  EXEC PGM=HMASMP,PARM='DATE=U'
//SYSUT1   DD  UNIT=3350,SPACE=(1700,(600,100))
//SYSUT2   DD  UNIT=3350,SPACE=(1700,(600,100))
//SYSUT3   DD  UNIT=3350,SPACE=(1700,(600,100))
//SYSUT4   DD  UNIT=3350,SPACE=(80,(2,2))
//SMPWRK1  DD  UNIT=3350,SPACE=(CYL,(5,10,84)),DCB=(BLKSIZE=3120,
//             LRECL=80)
//SMPWRK2  DD  UNIT=3350,SPACE=(CYL,(5,10,84)),DCB=(BLKSIZE=3120,
//             LRECL=80)
//SMPWRK3  DD  UNIT=3350,SPACE=(CYL,(5,10,84)),DCB=(BLKSIZE=3120,
//             LRECL=80)
//SMPWRK4  DD  UNIT=3350,SPACE=(CYL,(1,10,84)),DCB=(BLKSIZE=3120,
//             LRECL=80)
//SMPWRK5  DD  UNIT=3350,SPACE=(CYL,(30,10,250))
//SMPOUT   DD  SYSOUT=*
//SYSPRINT DD  SYSOUT=*
//ASMPRINT DD  SYSOUT=*
//CMPPRINT DD  SYSOUT=*
//COPPRINT DD  SYSOUT=*
//LKDPRINT DD  SYSOUT=*
//E37PRINT DD  SYSOUT=*
//UPDPRINT DD  SYSOUT=*
//ZAPPRINT DD  SYSOUT=*
//SMPLOG   DD  DUMMY
//SMPPTS   DD  DSN=SYS1.SMPPTS,DISP=OLD
//SMPCDS   DD  DSN=SYS1.SMPCDS,DISP=OLD
//SMPCRQ   DD  DSN=SYS1.SMPCRQ,DISP=OLD
//SMPSCDS  DD  DSN=SYS1.SMPSCDS,DISP=OLD
//SMPJCLIN DD  DISP=SHR,DSN=SYS1.STAGE1.OUTPUT,DCB=BLKSIZE=32720
//         DD  DATA,DLM='><'
//JES2    EXEC PGM=IEWL,PARM='XREF,LET,LIST,NCAL'
//SYSPRINT DD  SYSOUT=*
//SYSUT1   DD  DSN=&&SYSUT1,UNIT=SYSDA,SPACE=(1024,(50,20))
//SYSLMOD  DD  DSN=SYS1.LINKLIB,DISP=SHR,UNIT=3350,VOL=SER=MVSRES
//AOSH3    DD  DSN=SYS1.AOSH3,DISP=SHR,UNIT=3350,VOL=SER=MVSDLB
//SYSLIN   DD  *
       ORDER HASPNUC
       ORDER HASPRDR
       ORDER HASPRDRO
       ORDER HASPRSCN
       ORDER HASPXEQ
       ORDER HASPPRPU
       ORDER HASPACCT
       ORDER HASPMISC
       ORDER HASPCON
       ORDER HASPRTAM
       ORDER HASPCOMM
       ORDER HASPCOMA
       ORDER HASPINIT(P)
       INCLUDE AOSH3(HASPNUC)
       INCLUDE AOSH3(HASPRDR)
       INCLUDE AOSH3(HASPXEQ)
       INCLUDE AOSH3(HASPPRPU)
       INCLUDE AOSH3(HASPACCT)
       INCLUDE AOSH3(HASPMISC)
       INCLUDE AOSH3(HASPCON)
       INCLUDE AOSH3(HASPRTAM)
       INCLUDE AOSH3(HASPCOMM)
       INCLUDE AOSH3(HASPINIT)
       PAGE    HASPINIT
       NAME    HASJES20(R)
       INCLUDE AOSH3(HASPBLKS)
       NAME    HASPBLKS(R)
       INCLUDE AOSH3(HASPFMT0)
       NAME    HASPFMT0(R)
       INCLUDE AOSH3(HASPFMT1)
       NAME    HASPFMT1(R)
       INCLUDE AOSH3(HASPFMT2)
       NAME    HASPFMT2(R)
       INCLUDE AOSH3(HASPFMT3)
       NAME    HASPFMT3(R)
       INCLUDE AOSH3(HASPFMT4)
       NAME    HASPFMT4(R)
       INCLUDE AOSH3(HASPFMT5)
       NAME    HASPFMT5(R)
//SSSM     EXEC PGM=IEWL,PARM='XREF,LIST,LET,NCAL'
//SYSPRINT DD  SYSOUT=*
//SYSUT1   DD  DSN=&&SYSUT1,UNIT=SYSDA,SPACE=(1024,(50,20))
//SYSLMOD  DD  DSN=SYS1.LPALIB,DISP=SHR,UNIT=3350,VOL=SER=MVSRES
//AOSH3    DD  DSN=SYS1.AOSH3,DISP=SHR,UNIT=3350,VOL=SER=MVSDLB
//SYSLIN   DD  *
       INCLUDE AOSH3(HASPSSSM)
       NAME    HASPSSSM(R)
><
//SMPCNTL  DD  *
 JCLIN.
