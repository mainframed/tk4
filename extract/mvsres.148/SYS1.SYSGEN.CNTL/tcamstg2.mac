//TCAMGEN  JOB CLASS=A,MSGCLASS=X,MSGLEVEL=(1,1)
//*********************************************************************
//*
//*                   MVS 3.8 TCAM Generation
//*                   =======================
//*
//* DESC: Run TCAM Generation Stage 2 only
//*
//*       This job uses the stage 2 deck as created by running the
//*       first 9 steps of job TCAMGEN (stage 1 assembly + editing
//*       of the resulting stage 2 deck). It is meant to be used
//*       instead of job TCAMGEN on systems on which the editing
//*       prerequisites for TCAMGEN are not given.
//*
//*       Note that this job generates TCAM only. It doesn't create
//*       the NCP RRTs. If these aren't yet installed on the system
//*       the respective steps of job TCAMGEN can be used to create
//*       them.
//*
//*********************************************************************
//*
//* Stage 2 assembly
//*
//STAGE2   EXEC PGM=IEUASM,PARM=LOAD,REGION=512K
//SYSLIB   DD DSNAME=SYS1.MACLIB,DISP=SHR
//SYSUT1   DD DSNAME=&SYSUT1,UNIT=SYSDA,SPACE=(3400,(400,50)),
//            SEP=(SYSLIB)
//SYSUT2   DD DSNAME=&SYSUT2,UNIT=SYSDA,SPACE=(3400,(400,50))
//SYSUT3   DD DSNAME=&SYSUT3,SPACE=(3400,(400,50)),
//            UNIT=(SYSDA)
//SYSPRINT DD SYSOUT=*
//SYSPUNCH DD DUMMY
//SYSGO    DD DSNAME=&LOADSET,UNIT=SYSDA,SPACE=(80,(200,50)),
//            DISP=(MOD,PASS)
//SYSIN    DD *
TCAM CSECT
         INTRO ENVIRON=TSO,KEYLEN=252,LNUNITS=750,CIB=10, ..............
               ABEFMT=(SIB,RVT,DCT,LGB,DCB,DEB,SCB,PLCB,LCB,      ......
               QCB,OPT,TTE),                           @OY16449   ......
               FEATURE=(,,,,MIXD3705),BTRACE=0,PLCBNO=12, ..............
               BRACKET=NO,                                  ............
               CROSSRF=0,TRACE=0,LINETYP=BOTH,                 .........
               SIBCNT=70,SUBAREA=3,MAXSUBA=31,                  ........
               DTRACE=0,OLTEST=0,PROGID=TCAM,         ..................
               STARTUP=C,THRESH=(255,1,1,1)
         LTR   15,15
         BNZ   IEDAYBYE
         LA    3,DCBL1
         LA    4,DCBL2-DCBL1
         LA    5,DCBLAST
OPENLOOP LR    1,3
         OPEN  MF=(E,(1))
         USING IHADCB,6
         L     6,0(,3)
         TM    DCBOFLGS,X'10'
         BO    OPENNEXT
         MVC   OPENWTO+34(8),DCBDDNAM
OPENWTO  WTO   'IKJ403I LINE GROUP FOR DD          NOT OPENED',DESC=6, -
               ROUTCDE=2
         B     OPENNEXT
OPENNEXT BXLE  3,4,OPENLOOP
         DROP  6
         B     OPENDONE
DCBL1    DS    0F
         OPEN  (DCB1,(INOUT)),MF=L
DCBL2    EQU   *
         OPEN  (DCB2,(INOUT)),MF=L
         OPEN  (DCB3,(INOUT)),MF=L
         OPEN  (DCB4,(INOUT)),MF=L
         OPEN  (DCB5,(INOUT)),MF=L
         OPEN  (DCB6,(INOUT)),MF=L
         OPEN  (DCB7,(INOUT)),MF=L
DCBLAST  OPEN  (DCB8,(INOUT)),MF=L
OPENDONE DS    0H
         READY
         CLOSE (DCB1)
         CLOSE (DCB2)
         CLOSE (DCB3)
         CLOSE (DCB4)
         CLOSE (DCB5)
         CLOSE (DCB6)
         CLOSE (DCB7)
         CLOSE (DCB8)
IEDAYBYE L     13,4(,13)
         RETURN (14,12)
