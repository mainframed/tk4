        6 JCTJDPCD CHAR(2),  /* JOB CONDITION CODE */
        6 JCTJDPOP CHAR(1),  /* JOB CONDITION OPERATOR */
        6    *     CHAR(1),  /* RESERVED FOR FUTURE USE */
