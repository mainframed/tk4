N15P6    RRTENT RMTP1=78,RMTP2=20
N15L7    RRTENT RMTP1=B8,RMTP2=20
N15P7    RRTENT RMTP1=78,RMTP2=20
N15L8    RRTENT RMTP1=B8,RMTP2=20
N15P8    RRTENT RMTP1=78,RMTP2=20
N15L9    RRTENT RMTP1=B8,RMTP2=20
N15P9    RRTENT RMTP1=78,RMTP2=20
N15LA    RRTENT RMTP1=B8,RMTP2=20
N15PA    RRTENT RMTP1=78,RMTP2=20
$LUPOOL  RRTENT RMTP1=38,RMTP2=20
$LUPOOL  RRTENT RMTP1=38,RMTP2=20
$LUPOOL  RRTENT RMTP1=38,RMTP2=20
$LUPOOL  RRTENT RMTP1=38,RMTP2=20
