//MVTCOBOL JOB (001),'RELOAD MVT COBOL',CLASS=A,MSGCLASS=A
//*
//* ***************************************************************** *
//* RELOAD MVT COBOL FROM TAPE                                        *
//* ***************************************************************** *
//*
//RELOAD  EXEC PGM=IEBCOPY,REGION=1024K
//SYSPRINT DD  SYSOUT=A
//TAPEIN1  DD  DSN=COBOL.LOAD,UNIT=TAPE,DISP=(OLD,KEEP),
//             VOL=(PRIVATE,RETAIN,SER=000901),LABEL=(1,SL)
//TAPEIN2  DD  DSN=COBOL.LIB,UNIT=AFF=TAPEIN1,DISP=(OLD,KEEP),
//             VOL=(PRIVATE,RETAIN,SER=000901),LABEL=(2,SL)
//TAPEIN3  DD  DSN=COBOL.PROCS,UNIT=AFF=TAPEIN1,DISP=(OLD,KEEP),
//             VOL=(PRIVATE,RETAIN,SER=000901),LABEL=(3,SL)
//LIBOUT1  DD  DSN=&&COBLOAD,DISP=(NEW,PASS),
//             UNIT=SYSDA,SPACE=(CYL,(1,1,5)),
//             DCB=(RECFM=U,BLKSIZE=13030)
//LIBOUT2  DD  DSN=&&COBLIB,DISP=(NEW,PASS),
//             UNIT=SYSDA,SPACE=(CYL,(1,1,15)),
//             DCB=(RECFM=U,BLKSIZE=13030)
//PROCLIB  DD  DSN=SYS2.PROCLIB,DISP=SHR
//SYSUT3   DD  UNIT=SYSDA,SPACE=(80,(60,45)),DISP=(NEW,DELETE)
//SYSIN    DD  *
  COPY INDD=TAPEIN1,OUTDD=LIBOUT1
  COPY INDD=TAPEIN2,OUTDD=LIBOUT2
  COPY INDD=((TAPEIN3,R)),OUTDD=PROCLIB
