//MVS0190 JOB  (SETUP),
//             'CLIP DASD',
//             CLASS=A,
//             COND=(0,NE),
//             MSGCLASS=A,
//             MSGLEVEL=(1,1)
//*********************************************************************
//*
//* NAME: SYS1.SETUP.CNTL(MVS0190)
//*
//* DESC: ADD A FEW PROCEDURES ETC SRC VOLUMES
//*
//*********************************************************************
//*
//UPDATE1 EXEC PGM=IEBUPDTE,PARM=NEW
//SYSPRINT DD  SYSOUT=*
//SYSUT2   DD  DISP=SHR,DSN=SYS2.PROCLIB
//SYSIN    DD  DATA,DLM='><'
.@ ADD NAME=CLIP
//CLIP    PROC CUA=000,        <=== ADDRESS OF OFFLINE UNIT TO RENAME
//             VOLID=VVVVVVVV  <=== NEW NAME OF DASD DEVICE
//CMD1    EXEC PGM=BSPOSCMD,PARM=' V &CUA.,OFFLINE'
//DELAY1  EXEC PGM=DELAY,PARM='2'
//CMD2    EXEC PGM=BSPOSCMD,PARM=' S DEALLOC'
//DELAY1  EXEC PGM=DELAY,PARM='2'
//CMD3    EXEC PGM=BSPPA2SI,
//    PARM=' REFORMAT UNITADDRESS(&CUA.) VOLID(&VOLID.) NOVERIFY'
//SYSUT1   DD  DISP=(,PASS),UNIT=VIO,SPACE=(TRK,(1,1))
//ICKDSF  EXEC PGM=ICKDSF
//SYSPRINT DD  SYSOUT=*
//SYSIN    DD  DISP=(OLD,DELETE),DSN=*.CMD3.SYSUT1
//CMD4    EXEC PGM=BSPOSCMD,PARM='V &CUA.,ONLINE'
//CMD5    EXEC PGM=BSPOSCMD,PARM='M &CUA.,VOL=(SL,&VOLID),USE=PRIVATE'
><
//UPDATE2 EXEC PGM=IEBUPDTE,PARM=NEW
//SYSPRINT DD  SYSOUT=*
//SYSUT2   DD  DISP=SHR,DSN=SYS2.JCLLIB
//SYSIN    DD  DATA,DLM='><'
.@ ADD NAME=CLIPDASD
//CLIPDASD JOB (SETUP),
//             'CLIP DASD',
//             CLASS=A,
//             COND=(0,NE),
//             MSGCLASS=A,
//             MSGLEVEL=(1,1)
//*********************************************************************
//*
//* NAME: SYS2.JCLLIB(CLIPDASD)
//*
//* DESC: RENAME AN OFFLINE VOLUME
//*
//*********************************************************************
//*
//SRC000  EXEC CLIP,CUA=348,VOLID=SRC000
//SRC001  EXEC CLIP,CUA=349,VOLID=SRC001
//SRC002  EXEC CLIP,CUA=34A,VOLID=SRC002
//SRCCAT  EXEC CLIP,CUA=34B,VOLID=SRCCAT
><
//SUBMIT EXEC PGM=IEFBR14 OR IEBGENER
//SYSPRINT DD  DUMMY
//SYSIN    DD  DUMMY
//SYSUT1   DD  DISP=SHR,DSN=SYS2.JCLLIB(CLIPDASD)
//SYSUT2   DD  SYSOUT=(A,INTRDR)
