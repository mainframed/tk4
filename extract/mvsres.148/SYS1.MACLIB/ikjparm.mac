         MACRO
&CSECNM  IKJPARM &DUMMY,&DSECT=IKJPARMD
         GBLC  &IKJCSNM,&IKJDSLN,&IKJCSND,&IKJDSNM
.* RELEASE 20 SUPPORT CODE -- 20035
&IKJCSNM SETC  '&CSECNM'               SAVE CSECT NAME
&IKJDSNM SETC  '&DSECT'
&IKJDSLN SETC  'IKJ@&SYSNDX'           CREATE AND SAVE NAME FOR DSECT L*
                                          ENGTH
&IKJCSND SETC  'IKJ$&SYSNDX'
         AIF   (N'&SYSLIST(1) EQ 0).GOOD                     @ZA65509
         MNOTE 4,'IKJPARM SHOULD HAVE NO POSITIONAL PARAMETERS'
.GOOD    ANOP
         AIF   ('&CSECNM' EQ '').ERROR1
&IKJCSNM CSECT ,                       ENTER PCL CSECT
         DC    AL2(&IKJCSND-&IKJCSNM)  LENGTH OF CSECT
         DC    AL2(&IKJDSLN)           LENGTH OF DSECT
         IKJSUBF NOCSECT               START A SUBFIELD
&IKJDSNM DSECT
         DS    2A                      FOR STORAGE CHAINS
&SYSECT  CSECT ,                       RESUME USERS CSECT
         MEXIT
.ERROR1  MNOTE 8,'STATEMENT LABEL MISSING BUT REQUIRED'
         MEND
