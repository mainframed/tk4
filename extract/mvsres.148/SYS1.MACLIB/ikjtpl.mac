* %      GOTO TPLBSL; /*
         MACRO
         IKJTPL
.* A 0-999999                                                    S20031
***********************************************************************
*    TEST PARAMETER LIST (TPL) IS GENERATED BY THE TMP FOR USE        *
*    WITH TEST PROGRAMS.                                              *
***********************************************************************
TPL      DSECT
TPLCBUF  DS    A        PTR TO COMMAND BUFFER
TPLUPT   DS    A        PTR TO UPT
TPLPSCB  DS    A        PTR TO PSCB
TPLECT   DS    A        PTR TO ECT
TPLTBUF  DS    A        PTR TO TEST COMMAND BUFFER
TPLCTCB  DS    A        PTR TO ATTACHED CP TCB
TPLSTAI  DS    A        PTR TO TMP STAI EXIT ROUTINE
TPLSPLS  DS    A        PTR TO STAI PARAMETER LIST
TPLNECB  DS    A        PTR TO ECB FOR ABENDING CP
TPLNTCB  DS    A        PTR TO TCB FOR ABENDING CP
TPLMECB  DS    A        PTR TO STOP/MODIFY ECB
TPLECBL  EQU   *        TMP WAIT ECB LIST
TPLCECB  DS    A        PTR TO ATTACHED CP ECB
TPLIECB  DS    A        PTR TO TMP STAI ECB
TPLAECB  DS    A        PTR TO TMP ATTN ECB - HIGH ORDER BIT ON
TPLRESV  DS    A        RESERVED
**/
         MEND
* %TPLBSL:
* /* THE TPL MAPPING CAN BE PLACED AT THE BEGINNING, MIDDLE,   */
* /* OR END OF ANY BSL STRUCTURE THROUGH THE USE OF THE        */
* /* COMPILE TIME OPTIONS 'TPLSLEV' AND/OR 'TPLOPT1'.          */
* /*   TPLSLEV - THIS IS THE CHARACTER ('2' TO '7') TO BE      */
* /*             ASSIGNED TO THE HIGHEST LEVEL IF THE TPL IS   */
* /*             NOT TO BE LOCATED AT THE BEGINNING OF THE     */
* /*             STRUCTURE.                                    */
* /*   TPLOPT1 - IF A CHARACTER STRING 'CONTINUE' IS ASSIGNED  */
* /*             TO THIS OPTION, A SEMI-COLON IS NOT PLACED AT */
* /*             THE END OF THE STRUCTURE SO THAT ADDITIONAL   */
* /*             STATEMENTS MAY BE ADDED TO THE STRUCTURE.     */
* /* IF NEITHER OPTION IS SPECIFIED, THE TPL MAPPING WILL      */
* /* DEFAULT TO A STRUCTURE OF ITS OWN.                        */ ;
* %       DECLARE (X,Y,Z) FIXED      /* COMPILE TIME VARIABLES */ ;
* %       IF TPLSLEV='2'
* %         THEN
* %           X=2              /* SET FIRST LEVEL AS SPECIFIED */ ;
* %           GOTO TPLBSL2;
* %       IF TPLSLEV='3'
* %         THEN
* %           X=3              /* SET FIRST LEVEL AS SPECIFIED */ ;
* %           GOTO TPLBSL2;
* %       IF TPLSLEV='4'
* %         THEN
* %           X=4              /* SET FIRST LEVEL AS SPECIFIED */ ;
* %           GOTO TPLBSL2;
* %       IF TPLSLEV='5'
* %         THEN
* %           X=5              /* SET FIRST LEVEL AS SPECIFIED */ ;
* %           GOTO TPLBSL2;
* %       IF TPLSLEV='6'
* %         THEN
* %           X=6              /* SET FIRST LEVEL AS SPECIFIED */ ;
* %           GOTO TPLBSL2;
* %       IF TPLSLEV='7'
* %         THEN
* %           X=7              /* SET FIRST LEVEL AS SPECIFIED */ ;
* %           GOTO TPLBSL2;
* %TPLBSL1: /* BEGIN A BSL STRUCTURE - 'TPLSLEV' NOT SPECIFIED */ ;
* %       Y=2                      /* USE THESE DEFAULT VALUES */ ;
* %       Z=3                      /* FOR THE LEVEL NUMBERS    */ ;
* /*******************************************************************/
* /* TEST PARAMETER LIST (TPL) IS GENERATED BY THE TMP FOR USE       */
* /* WITH TEST PROGRAMS.                                             */
* /*******************************************************************/
* DECLARE
*   1 TPL BASED(TPLPTR),
* %       GOTO TPLBSL3;
* %TPLBSL2: /* CONTINUE A BSL STRUCTURE USING LEVEL SPECIFIED  */ ;
* %       Y=X+1                /* SET SECOND LEVEL ONE HIGHER  */ ;
* %       Z=X+2                /* SET THIRD LEVEL TWO HIGHER   */ ;
*     X TPL,
* %TPLBSL3: /* THIS IS THE MAIN TPL STRUCTURE                  */ ;
*       Y TPLCBUF  PTR(31),          /* PTR TO COMMAND BUFFER        */
*       Y TPLUPT   PTR(31),          /* PTR TO UPT                   */
*       Y TPLPSCB  PTR(31),          /* PTR TO PSCB                  */
*       Y TPLECT   PTR(31),          /* PTR TO ECT                   */
*       Y TPLTBUF  PTR(31),          /* PTR TO TEST COMMAND BUFFER   */
*       Y TPLCTCB  PTR(31),          /* PTR TO ATTACHED CP TCB       */
*       Y TPLSTAI  PTR(31),          /* PTR TO TMP STAI EXIT RTN     */
*       Y TPLSPLS  PTR(31),          /* PTR TO STAI PARAMETER LIST   */
*       Y TPLNECB  PTR(31),          /* PTR TO ECB FOR ABENDING CP   */
*       Y TPLNTCB  PTR(31),          /* PTR TO TCB FOR ABENDING CP   */
*       Y TPLMECB  PTR(31),          /* PTR TO STOP/MODIFY ECB       */
*       Y TPLECBL,                   /* TMP WAIT ECB LIST            */
*         Z TPLCECB  PTR(31),        /* PTR TO ATTACHED CP ECB       */
*         Z TPLIECB  PTR(31),        /* PTR TO TMP STAI ECB          */
*         Z TPLLEND  CHAR(1),        /* HIGH ORDER BIT ON            */
*         Z TPLAECB  PTR(24),        /* PTR TO TMP ATTN ECB          */
* %       IF TPLOPT1='CONTINUE'
* %         THEN
* %           GOTO TPLBSL4;
* %         ELSE
* %           GOTO TPLBSL5;
* %TPLBSL4: /* STRUCTURE IS TO BE CONTINUED - DO NOT CLOSE     */ ;
*       Y TPLRESV  PTR(31),          /* RESERVED                     */
* %       GOTO TPLBSL6;
* %TPLBSL5: /* STRUCTURE IS COMPLETED - USE SEMI-COLON         */ ;
*       Y TPLRESV  PTR(31);          /* RESERVED                     */
*
* %TPLBSL6: /* DEACTIVATE THE COMPILE TIME VARIABLES           */ ;
* %       DEACTIVATE X,Y,Z;
