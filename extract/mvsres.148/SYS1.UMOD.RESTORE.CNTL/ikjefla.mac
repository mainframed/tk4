//IKJEFLA  JOB  (RESTORE),
//          'Relink IKJEFLA',
//          CLASS=A,
//          MSGCLASS=X,
//          MSGLEVEL=(1,1)
//*********************************************************************
//*
//* Name: SYS1.UMOD.RESTORE.CNTL(IKJEFLA)
//*
//* Desc: Relink IKJEFLA after restoring usermods
//*       ZUM0007, ZJW0004 and ZJW0005
//*
//*       IKJEFLA elements IKJEFLD, IKJEFLPA and IKJEFLPB are not
//*       restored by SMP correctly. This job restores IKJEFLPB to
//*       the state retained from last sysgen in SYS1.UMOD.RESTORE.LIB,
//*       IKJEFLPA to the state currently accepted from SYS1.AOST5
//*       and removes IKJEFLD.
//*
//*********************************************************************
//*
//LKED    EXEC PGM=IEWL,PARM=(XREF,LET,LIST,NCAL,RENT,REUS)
//LPALIB   DD DSN=SYS1.LPALIB,DISP=SHR
//AOST4    DD DSN=SYS1.AOST4,DISP=SHR
//SYSGEN   DD DSN=SYS1.UMOD.RESTORE.LIB,DISP=SHR
//SYSLMOD  DD DSN=SYS1.LPALIB,DISP=SHR
//SYSUT1   DD DSN=&&SYSUT1,UNIT=SYSDA,
//         SPACE=(1024,(50,20))
//SYSPRINT DD SYSOUT=*
//SYSLIN   DD *
 ORDER   IKJEFLA(P),IKJEFLB
 ORDER   IKTLOGFF,IKTIIOM
 ORDER   IKTXINIT,IKTRPLXT
 ORDER   IKJEFLEA
 ORDER   IKTXLOG,IKTLOGR
 ORDER   IKJEFLC,IKJEFLCM
 ORDER   IKJEFLE,IKJEFLG,IKJEFLGB
 ORDER   IKJEFLGH,IKJEFLGN,IKJEFLH,IKJEFLI
 ORDER   IKJEFLJ,IKJEFLK,IKJEFLL,IKJEFLLM
 ORDER   IKJEFLPA,IKJEFLS,IKJEFLP0
 ORDER   IKJEFLGM,IKJEFLIM,IKJEFLE1
 ORDER   IKJEFLE2,IKJEFLE3,IKJEFLE4,IKJEFLPB
 ENTRY   IKJEFLA
 ALIAS   IKJEFLC,IKJLB1,IKJLJ1,IKJLM1,IKJLK1
 ALIAS   IKJEFLES
 INCLUDE AOST4(IKJEFLPA)
 INCLUDE SYSGEN(IKJEFLPB)
 REPLACE IKJEFLD
 INCLUDE LPALIB(IKJEFLA)
 NAME IKJEFLA(R)
/*
//
