//IPCSCOPY PROC DUMP=,    NO DEFAULT DUMP NUMBER 00 THRU 09
//             D=RM146,    SYSPRINT DESTINATION
//             C='A',      DEFAULT SYSOUT CLASS = A
//             EMPTY=YES,  EMPTY=NO WILL NOT CLEAR DUMP DS
//             OUT=AVPPUA  DEFAULT OUTPUT VOLSER FOR COPY
//*****************************************************************
//IEFPROC  EXEC PGM=DUMPCOPY,PARM=&OUT
//STEPLIB  DD  DSN=SYS1.MVSSERV,DISP=SHR
//DUMPIN   DD  DDNAME=IEFRDER
//IEFRDER  DD  DSN=SYS1.DUMP&DUMP,DISP=SHR,UNIT=SYSALLDA
//SYSPRINT DD  SYSOUT=&C,DEST=&D
//SYSUDUMP DD  SYSOUT=&C,DEST=&D
//DUMPLOG  DD  DSN=#A1000.DUMPLOG,DISP=(MOD,KEEP)
//*****************************************************************
//CLEAR    EXEC PGM=IEBGENER
//SYSPRINT DD  DUMMY
//SYSIN    DD  DUMMY
//SYSUT1   DD  DUMMY,DCB=BLKSIZE=4104
//SYSUT2   DD  DDNAME=&EMPTY
//NO       DD  DUMMY
//YES      DD  DSN=*.IEFPROC.DUMPIN,VOL=REF=*.IEFPROC.DUMPIN,
//             DISP=SHR,DCB=(RECFM=F,BLKSIZE=4104)
