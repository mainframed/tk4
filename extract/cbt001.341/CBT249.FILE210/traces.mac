 /*   BASIC DRIVER PROGRAM FOR TRACE FORMATTING                      */
 TRACES:       %INCLUDE(DRIVER);
  ANAL: PROCEDURE OPTIONS(REENTRANT);
      DECLARE SWITCH FIXED BINARY STATIC INIT(0);
     IF SWITCH = 0 THEN DO;
    SWITCH = 1;
     PUT PAGE ;
   PUT SKIP(10) EDIT('**********     ',
     'BASIC TRACE FORMATTER -- NO ANALYSIS ROUTINE')
     (COL(4),A,A);
    PUT PAGE;
    END;
    RETURN;
   END;
  END;
