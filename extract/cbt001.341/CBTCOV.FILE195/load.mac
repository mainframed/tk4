//CPLOAD JOB 750-CP-FM-P,'LOAD SCHEDULER',TIME=2,MSGCLASS=Q
/*JOBPARM L=2
//IDCAMS EXEC PGM=IDCAMS
//SYSPRINT DD SYSOUT=*
//SYSIN DD *
 DEFINE NONVSAM(NAME(SYS1.MLPALIB) DEVICETYPES(3350) VOLUMES(XXXXXX)) -
 CATALOG(MASTER.CATALOG/PASSWORD)
 DEFINE ALIAS(NAME(CP) RELATE(SYSCTLG.VCVOLXX)) -
 CATALOG(MASTER.CATALOG/PASSWORD)
//S1 EXEC PGM=IEBCOPY
//SYSPRINT DD SYSOUT=*
//SYSUT3 DD UNIT=SYSWK,SPACE=(CYL,(10))
//SYSUT4 DD UNIT=SYSWK,SPACE=(CYL,(10))
//TAPEJCL DD UNIT=TAPE,DISP=SHR,DSN=CP.JSC50JCL,
// VOL=(,RETAIN,SER=SENATE),
// LABEL=(2,SL,EXPDT=98000)
//TAPESRC DD UNIT=AFF=TAPEJCL,DISP=SHR,DSN=CP.JSC50SRC,
// LABEL=(3,SL,EXPDT=98000),
// VOL=(,RETAIN,SER=SENATE)
//DISKJCL DD UNIT=3350,DISP=(,CATLG),VOL=SER=XXXXXX,
// DSN=CP.JSC50JCL,SPACE=(6160,(50,,5))
//DISKSRC DD UNIT=3350,DISP=(,CATLG),VOL=SER=XXXXXX,
// DSN=CP.JSC50SRC,SPACE=(6160,(400,,10))
//SYSIN DD *
 C O=DISKJCL,I=TAPEJCL
 C O=DISKSRC,I=TAPESRC
/*
//LOADMOD EXEC PGM=IEFBR14
//LINK DD UNIT=3350,DISP=(,CATLG),VOL=SER=XXXXXX,
// DSN=CP.JSC50LNK,SPACE=(TRK,(15,,10))
//MLPALIB DD UNIT=3350,DISP=(,KEEP,DELETE),VOL=SER=XXXXXX,
// SPACE=(TRK,(20,,5)),DSN=SYS1.MLPALIB
//DIAGFILE DD UNIT=3350,VOL=SER=XXXXXX,DSN=CP.DIAGFILE,
// SPACE=(CYL,(1))
//REQFILE  DD UNIT=3350,VOL=SER=XXXXXX,DSN=CP.REQFILE,
// SPACE=(TRK,(1))
//IEBPTPCH EXEC PGM=IEBPTPCH
//SYSPRINT DD SYSOUT=*
//SYSUT1 DD DISP=SHR,DSN=CP.JSC50SRC
//SYSUT2 DD SYSOUT=*  ****THIS REQUIRES AN UPPER/LOWER CASE PRINTER****
//SYSIN DD *
 PRINT PREFORM=A,MAXNAME=10,MAXFLDS=10,TYPORG=PO
 MEMBER NAME=DOCUMENT
 RECORD FIELD=(80)
