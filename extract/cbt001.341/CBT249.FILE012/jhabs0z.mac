)CM 08/02/84 MURA - ADD VOLUME SERIAL TO COMMENTS
)SET S = &S + 1
//*---------------------------------------------------------------------
)SEL &ZCMD ^= M
//*  JH BG - SUPERZAP &TZFUNC OF &TLNKLB &TVOL
//*                            &JHNAME &JHCSECT
)ENDSEL
)SEL &ZCMD = M
//*  JH BG - SUPERZAP TO &TLNKLB &TVOL
//*                      &JHNAME &JHCSECT
<//*          FIX ID: &FIXNO×>
)ENDSEL
//*---------------------------------------------------------------------
//ZAP&S    EXEC PGM=IMASPZAP,REGION=128K
//SYSPRINT DD  SYSOUT=*
//SYSLIB   DD  DSN=&TLNKLB,
//             <VOL=SER=&TVOL,UNIT=&TUNIT,×>DISP=SHR
//SYSIN    DD  *
     &TZFUNC &JHNAME &JHCSECT
)SEL &ZCMD = M
     <VER &DISPL1 &VER1 ×>
     <VER &DISPL2 &VER2 ×>
     <VER &DISPL3 &VER3 ×>
     <VER &DISPL4 &VER4 ×>
     <VER &DISPL5 &VER5 ×>
)SEL &DISPL1 ^= &Z && &REP1 ^= &Z
     REP &DISPL1 &REP1
)ENDSEL
)SEL &DISPL2 ^= &Z && &REP2 ^= &Z
     REP &DISPL2 &REP2
)ENDSEL
)SEL &DISPL3 ^= &Z && &REP3 ^= &Z
     REP &DISPL3 &REP3
)ENDSEL
)SEL &DISPL4 ^= &Z && &REP4 ^= &Z
     REP &DISPL4 &REP4
)ENDSEL
)SEL &DISPL5 ^= &Z && &REP5 ^= &Z
     REP &DISPL5 &REP5
)ENDSEL
     <IDRDATA &FIXNO ×>
)ENDSEL
