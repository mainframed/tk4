)ATTR
 /* 01/17/84 Mura - panel, msg id, skel names changed                */
  %  TYPE(TEXT)  INTENS(HIGH)  SKIP(ON)
  +  TYPE(TEXT)  INTENS(LOW)   SKIP(ON)
)BODY
%---------------------------  BATCH LINKAGE EDIT  ----------------------
%COMMAND ===>_ZCMD
%
+INPUT LOAD LIBRARIES (Referenced by control cards below):
+            DD1%===>_LNKDD1                                 +
+            DD2%===>_LNKDD2                                 +
+
+LINK TO        %===>_LNKLB2                                 +
+MEMBER NAME    %===>_TLMEM   +
+ENTRY POINT    %===>_TENTRY  +
+LINK EDIT PARMS%===>_BLNK
+LINK SYSLIBS   %===>_BSL1                                        +
+               %===>_BSL2                                        +
+
+LINK CONTROL CARDS:
+               %===>_LNKC1                                       +
+               %===>_LNKC2                                       +
+               %===>_LNKC3                                       +
+
%                Note: "NAME" and "ENTRY" cards will be generated automa
)INIT
  &TLMEM = &Z   &TENTRY = &Z
  .HELP = JHABH07
)PROC
  IF (&ZCMD ^= &Z)  .MSG = ISPZ001
  &LOGMSG = JHABM079

  &TLNKDD1 = &Z
  VER(&LNKDD1,DSNAME)
  IF (&LNKDD1 ^= &Z)
     &T1=TRUNC(&LNKDD1,'(')
     IF (.TRAIL ^= &Z)  .MSG = JHA@M009 /* MEMBER NAME NOT ALLOWED  */
     &T1=TRUNC(&LNKDD1,1)
     IF (&T1 ^= '''')
        &TLNKDD1 = '&ZPREFIX..&LNKDD1' /* ADD USER'S TSO PREFIX     */
     &T2=.TRAIL
     IF (&T1 = '''')
        &TLNKDD1 = TRUNC(&T2,'''')   /* STRIP OFF QUOTES FOR JCL    */
     VER(&TLNKDD1,DSNAME)

  &TLNKDD2 = &Z
  VER(&LNKDD2,DSNAME)
  IF (&LNKDD2 ^= &Z)
     &T1=TRUNC(&LNKDD2,'(')
     IF (.TRAIL ^= &Z)  .MSG = JHA@M009 /* MEMBER NAME NOT ALLOWED  */
     &T1=TRUNC(&LNKDD2,1)
     IF (&T1 ^= '''')
        &TLNKDD2 = '&ZPREFIX..&LNKDD2' /* ADD USER'S TSO PREFIX     */
     &T2=.TRAIL
     IF (&T1 = '''')
        &TLNKDD2 = TRUNC(&T2,'''')   /* STRIP OFF QUOTES FOR JCL    */
     VER(&TLNKDD2,DSNAME)

  VER(&LNKLB2,NB,DSNAME)
  IF (&LNKLB2 ^= &Z)
     &T1=TRUNC(&LNKLB2,1)
     IF (&T1 ^= '''')
        &TLNKLB2 = '&ZPREFIX..&LNKLB2' /* ADD USER'S TSO PREFIX     */
     &T2=.TRAIL
     IF (&T1 = '''')
        &TLNKLB2 = TRUNC(&T2,'''')   /* STRIP OFF QUOTES FOR JCL    */
     VER(&TLNKLB2,DSNAME)

  VER(&TLMEM,NB,NAME)

  VER(&TENTRY,NAME)

  &TBSL1 = &Z
  VER(&BSL1,DSNAME)
  IF (&BSL1 ^= &Z)
     &T1=TRUNC(&BSL1,'(')
     IF (.TRAIL ^= &Z)  .MSG = JHA@M009 /* MEMBER NAME NOT ALLOWED  */
     &T1=TRUNC(&BSL1,1)
     IF (&T1 ^= '''')
        &TBSL1 = '&ZPREFIX..&BSL1'   /* ADD USER'S TSO PREFIX       */
     &T2=.TRAIL
     IF (&T1 = '''')
        &TBSL1 = TRUNC(&T2,'''')     /* STRIP OFF QUOTES FOR JCL    */
     VER(&TBSL1,DSNAME)

  &TBSL2 = &Z
  VER(&BSL2,DSNAME)
  IF (&BSL2 ^= &Z)
     &T1=TRUNC(&BSL2,'(')
     IF (.TRAIL ^= &Z)  .MSG = JHA@M009 /* MEMBER NAME NOT ALLOWED  */
     &T1=TRUNC(&BSL2,1)
     IF (&T1 ^= '''')
        &TBSL2 = '&ZPREFIX..&BSL2'   /* ADD USER'S TSO PREFIX       */
     &T2=.TRAIL
     IF (&T1 = '''')
        &TBSL2 = TRUNC(&T2,'''')     /* STRIP OFF QUOTES FOR JCL    */
     VER(&TBSL2,DSNAME)

  VPUT (LNKDD1 LNKDD2 LNKLB2 LNKC1 LNKC2 LNKC3
        BLNK BSL1 BSL2) PROFILE
)END
