)ATTR
  @  TYPE(INPUT)  CAPS(OFF)
  %  TYPE(TEXT)   INTENS(HIGH)   SKIP(ON)
  +  TYPE(TEXT)   INTENS(LOW)    SKIP(ON)
 /* 10/03/83 MURA - don't vput variables not set after uz61079       */
)BODY
%----------------  JH PF KEY DEFINITION - PRIMARY KEYS -----------------
%COMMAND ===>_ZCMD
+
+NUMBER OF PF KEYS:%24+
+SET OF PF KEYS IN EFFECT%===>_Z+(A or B - verify before proceeding)
+
+PF13%===>@ZPF13
+PF14%===>@ZPF14
+PF15%===>@ZPF15
+PF16%===>@ZPF16
+PF17%===>@ZPF17
+PF18%===>@ZPF18
+PF19%===>@ZPF19
+PF20%===>@ZPF20
+PF21%===>@ZPF21
+PF22%===>@ZPF22
+PF23%===>@ZPF23
+PF24%===>@ZPF24
+
+Press%ENTER+key to process changes and display alternate keys (PF1-PF12
+Use%HELP+if you have not entered this option previously.
)INIT
  .ZVARS = '(JHPFKG)'
  .CURSOR = JHPFKG
  .HELP = XALTK
  IF (&JHPFKG = &Z)                  /* if option never used before  */
     &JHPFKG = A                     /* then initialize PFK set to A */
  &TPFKG = &JHPFKG                   /* save old value for CLIST     */
  IF (&PPFKG ^= &Z)                  /* if parm provided with cmd    */
     .RESP = END                     /* then don't display panel     */
     &JHPFKG = &PPFKG                /* set panel field to parm value*/
     &PPFKG = &Z                     /* reset parm value to blanks   */

)PROC
 IF (&ZCMD ^= ' ') .MSG = ISPZ001    /* invalid command              */
 VER (&JHPFKG NB LIST A,B)           /*                              */

 IF (.MSG = &Z)                      /* continue only if no error    */
    IF (&TPFKG ^= &JHPFKG) /* if set has been changed via panel/parm */
       &TPF13=&ZPF13   &TPF14=&ZPF14   &TPF15=&ZPF15   &TPF16=&ZPF16
       &TPF17=&ZPF17   &TPF18=&ZPF18   &TPF19=&ZPF19   &TPF20=&ZPF20
       &TPF21=&ZPF21   &TPF22=&ZPF22   &TPF23=&ZPF23   &TPF24=&ZPF24
       &ZPF13=&JHPF13  &ZPF14=&JHPF14  &ZPF15=&JHPF15  &ZPF16=&JHPF16
       &ZPF17=&JHPF17  &ZPF18=&JHPF18  &ZPF19=&JHPF19  &ZPF20=&JHPF20
       &ZPF21=&JHPF21  &ZPF22=&JHPF22  &ZPF23=&JHPF23  &ZPF24=&JHPF24
       &JHPF13=&TPF13  &JHPF14=&TPF14  &JHPF15=&TPF15  &JHPF16=&TPF16
       &JHPF17=&TPF17  &JHPF18=&TPF18  &JHPF19=&TPF19  &JHPF20=&TPF20
       &JHPF21=&TPF21  &JHPF22=&TPF22  &JHPF23=&TPF23  &JHPF24=&TPF24
       &TPF01=&ZPF01   &TPF02=&ZPF02   &TPF03=&ZPF03   &TPF04=&ZPF04
       &TPF05=&ZPF05   &TPF06=&ZPF06   &TPF07=&ZPF07   &TPF08=&ZPF08
       &TPF09=&ZPF09   &TPF10=&ZPF10   &TPF11=&ZPF11   &TPF12=&ZPF12
       &ZPF01=&JHPF01  &ZPF02=&JHPF02  &ZPF03=&JHPF03  &ZPF04=&JHPF04
       &ZPF05=&JHPF05  &ZPF06=&JHPF06  &ZPF07=&JHPF07  &ZPF08=&JHPF08
       &ZPF09=&JHPF09  &ZPF10=&JHPF10  &ZPF11=&JHPF11  &ZPF12=&JHPF12
       &JHPF01=&TPF01  &JHPF02=&TPF02  &JHPF03=&TPF03  &JHPF04=&TPF04
       &JHPF05=&TPF05  &JHPF06=&TPF06  &JHPF07=&TPF07  &JHPF08=&TPF08
       &JHPF09=&TPF09  &JHPF10=&TPF10  &JHPF11=&TPF11  &JHPF12=&TPF12

       IF (&ZPF13 = ' ')
         &ZPF13 = HELP
       IF (&ZPF14 = ' ')
         &ZPF14 = SPLIT
       IF (&ZPF15 = ' ')
         &ZPF15 = END
       IF (&ZPF16 = ' ')
         &ZPF16 = RETURN
       IF (&ZPF17 = ' ')
         &ZPF17 = RFIND
       IF (&ZPF18 = ' ')
         &ZPF18 = RCHANGE
       IF (&ZPF19 = ' ')
         &ZPF19 = UP
       IF (&ZPF20 = ' ')
         &ZPF20 = DOWN
       IF (&ZPF21 = ' ')
         &ZPF21 = SWAP
       IF (&ZPF22 = ' ')
         &ZPF22 = LEFT
       IF (&ZPF23 = ' ')
         &ZPF23 = RIGHT
       IF (&ZPF24 = ' ')
         &ZPF24 = CURSOR
       IF (&ZPF01 = ' ')
         &ZPF01 = HELP
       IF (&ZPF02 = ' ')
         &ZPF02 = SPLIT
       IF (&ZPF03 = ' ')
         &ZPF03 = END
       IF (&ZPF04 = ' ')
         &ZPF04 = RETURN
       IF (&ZPF05 = ' ')
         &ZPF05 = RFIND
       IF (&ZPF06 = ' ')
         &ZPF06 = RCHANGE
       IF (&ZPF07 = ' ')
         &ZPF07 = UP
       IF (&ZPF08 = ' ')
         &ZPF08 = DOWN
       IF (&ZPF09 = ' ')
         &ZPF09 = SWAP
       IF (&ZPF10 = ' ')
         &ZPF10 = LEFT
       IF (&ZPF11 = ' ')
         &ZPF11 = RIGHT
       IF (&ZPF12 = ' ')
         &ZPF12 = CURSOR

       VPUT ( ZPF13 ZPF14 ZPF15 ZPF16 ZPF17 ZPF18 ) PROFILE
       VPUT ( ZPF19 ZPF20 ZPF21 ZPF22 ZPF23 ZPF24 ) PROFILE
       VPUT ( JHPF13 JHPF14 JHPF15 JHPF16 JHPF17 JHPF18 )       PROFILE
       VPUT ( JHPF19 JHPF20 JHPF21 JHPF22 JHPF23 JHPF24 JHPFKG) PROFILE
       VPUT ( ZPF01 ZPF02 ZPF03 ZPF04 ZPF05 ZPF06 ) PROFILE
       VPUT ( ZPF07 ZPF08 ZPF09 ZPF10 ZPF11 ZPF12 ) PROFILE
       VPUT ( JHPF01 JHPF02 JHPF03 JHPF04 JHPF05 JHPF06 ) PROFILE
       VPUT ( JHPF07 JHPF08 JHPF09 JHPF10 JHPF11 JHPF12 ) PROFILE

 IF (.MSG ^= ' ')
   .RESP = ENTER
)END
