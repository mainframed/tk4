)CM CHK PRIORSKL TO DETERMINE IF LAST SEL. WAS IEHPROGM - MURA 3/22/83
)CM 01/17/84 SKELETON NAME CHANGED
)SEL &PRIORSKL ^= JHABS19 × &PREVVOL ^= &TVOL
)SET S = &S + 1
//*
//*---------------------------------------------------------------------
//*  JH BG - IEHPROGM &TFUNC OF &TDSN &TMEM
//*---------------------------------------------------------------------
//IEHP&S   EXEC PGM=IEHPROGM,REGION=128K,PARM='LINECNT=60'
//SYSPRINT DD  SYSOUT=*
)SEL &O ^= 3 && &O ^= 4
//DD1      DD  UNIT=&TDEV,DISP=OLD,
//             VOL=SER=(&TVOL)
)ENDSEL
//SYSIN    DD  *
)ENDSEL
)CM
)CM   ***** RENAME *****
)SEL &O = 5 × &O = 6
  RENAME       DSNAME=&TDSN,                                           X
)SEL &O = 5 && &TMEM ^= &Z
               MEMBER=&TMEM,                                           X
)ENDSEL
               VOL=&TDEV=(&TVOL),                                      X
               NEWNAME=&TNEW
)ENDSEL
)CM
)CM   ***** SCRATCH *****
)SEL &O = 1 × &O = 2
  SCRATCH      DSNAME=&TDSN,<MEMBER=&TMEM,×>                           X
               VOL=&TDEV=(&TVOL),PURGE
)ENDSEL
)CM
)CM   ***** UNCATLG *****
)SEL &O = 2 × &O = 3 × &O = 6
  UNCATLG      DSNAME=&TDSN
)ENDSEL
)CM
)CM   ***** CATLG *****
)SEL &O = 4
  CATLG        DSNAME=&TDSN,                                           X
)ENDSEL
)SEL &O = 6
  CATLG        DSNAME=&TNEW,                                           X
)ENDSEL
)SEL &O = 4 × &O = 6
               VOL=&TDEV=(&TVOL)
)ENDSEL
)SET PREVVOL = &TVOL
