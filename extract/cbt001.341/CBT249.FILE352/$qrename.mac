PROC 0 LIB('TECH.INTEL.MODS.SOURCE') NEW OLD MACROS ONLY
 /*                                                                 */
 /* CLIST: $QRENAME                                                 */
 /*                                                                 */
 /* PURPOSE: CONVERT TO/FROM QUEUE SP3 NAMES TO UNIQUE NAMES        */
 /*          FOR STORING ON PANVALET.                               */
 /*                                                                 */
 /* OPTIONS :   NEW - CONVERT FROM OLD TO NEW NAMES                 */
 /*             OLD - CONVERT FROM NEW TO OLD NAMES                 */
 /*             MACROS - INCLUDE MACROS IN THE NEW OR OLD RENAME    */
 /*             ONLY - ONLY RENAME THE MACROS TO NEW OR OLD MODE    */
 /*                                                                 */
 /*  KEN TRUE - INTEL CORP - 14JUL 83                               */
 /*                                                                 */
CONTROL LIST MSG END(ENDD)
IF &NEW^=  && &OLD^= THEN +
  DO
    WRITE ..BOTH 'NEW' AND 'OLD' SPECIFIED..PICK ONE ONLY
    EXIT QUIT
  ENDD
LPDS '&LIB.'
IF &NEW^=  && &ONLY= THEN +
  DO
    WRITE ..CONVERTING FROM 'OLD' NAMES TO NEW Q3... NAMES
    RENAME ACTIVE   Q3ACTIVE
    RENAME ALLOCATE Q3ALLOC
    RENAME CDN      Q3CDN
    RENAME CHCT     Q3CHCT
    RENAME CJCT     Q3CJCT
    RENAME CJHIST   Q3CJHIST
    RENAME CJOE     Q3CJOE
    RENAME CJQE     Q3CJQE
    RENAME CKPT     Q3CKPT
    RENAME CPDDB    Q3CPDDB
    RENAME CPFK     Q3CPFK
    RENAME CSPIN    Q3CSPIN
    RENAME CTGPS    Q3CTGPS
    RENAME CTSO     Q3CTSO
    RENAME DDNAME   Q3DDNAME
    RENAME DISPLAY  Q3DISPLY
    RENAME FINDJOB  Q3FNDJOB
    RENAME FINDPDDB Q3FDPDDB
    RENAME FORMAT   Q3FORMAT
    RENAME HELP     Q3HELP
    RENAME HEXBLK   Q3HEXBLK
    RENAME HEXDUMP  Q3HEXDMP
    RENAME INIT     Q3INIT
    RENAME INITS    Q3INITS
    RENAME JCL      Q3JCL
    RENAME JLOG     Q3JLOG
    RENAME JMSG     Q3JMSG
    RENAME LIST     Q3LIST
    RENAME LISTDS   Q3LISTDS
    RENAME PARSE    Q3PARSE
    RENAME PRINT    Q3PRINT
    RENAME QUEUE    Q3QUEUE
    RENAME Q0       Q3Q0
    RENAME READSPC  Q3READSP
    RENAME REPOS    Q3REPOS
    RENAME SAVE     Q3SAVE
    RENAME SEARCH   Q3SEARCH
    RENAME SYSLOG   Q3SYSLOG
    RENAME SYSOUT   Q3SYSOUT
    RENAME XDS      Q3XDS
 ENDD
IF &NEW^=  && &MACROS^= THEN +
    DO
      RENAME $JQT     Q3QJQT   /*   M A C R O          */
      RENAME MODESET  Q3MODEST /*   M A C R O          */
      RENAME QCOMMON  Q3QCOMMN /*   M A C R O          */
      RENAME QDEBUG   Q3QDEBUG /*   M A C R O          */
      RENAME QSTART   Q3QSTART /*   M A C R O          */
      RENAME QSTOP    Q3QSTOP  /*   M A C R O          */
      RENAME QTILT    Q3QTILT  /*   M A C R O          */
    ENDD
 /*                                                            */
IF &OLD^=  && &ONLY= THEN +
  DO
    WRITE ..CONVERTING FROM NEW Q3... NAMES TO 'OLD' NAMES
    RENAME Q3ACTIVE ACTIVE
    RENAME Q3ALLOC  ALLOCATE
    RENAME Q3CDN    CDN
    RENAME Q3CHCT   CHCT
    RENAME Q3CJCT   CJCT
    RENAME Q3CJHIST CJHIST
    RENAME Q3CJOE   CJOE
    RENAME Q3CJQE   CJQE
    RENAME Q3CKPT   CKPT
    RENAME Q3CPDDB  CPDDB
    RENAME Q3CPFK   CPFK
    RENAME Q3CSPIN  CSPIN
    RENAME Q3CTGPS  CTGPS
    RENAME Q3CTSO   CTSO
    RENAME Q3DDNAME DDNAME
    RENAME Q3DISPLY DISPLAY
    RENAME Q3FNDJOB FINDJOB
    RENAME Q3FDPDDB FINDPDDB
    RENAME Q3FORMAT FORMAT
    RENAME Q3HELP   HELP
    RENAME Q3HEXBLK HEXBLK
    RENAME Q3HEXDMP HEXDUMP
    RENAME Q3INIT   INIT
    RENAME Q3INITS  INITS
    RENAME Q3JCL    JCL
    RENAME Q3JLOG   JLOG
    RENAME Q3JMSG   JMSG
    RENAME Q3LIST   LIST
    RENAME Q3LISTDS LISTDS
    RENAME Q3PARSE  PARSE
    RENAME Q3PRINT  PRINT
    RENAME Q3QUEUE  QUEUE
    RENAME Q3Q0     Q0
    RENAME Q3READSP READSPC
    RENAME Q3REPOS  REPOS
    RENAME Q3SAVE   SAVE
    RENAME Q3SEARCH SEARCH
    RENAME Q3SYSLOG SYSLOG
    RENAME Q3SYSOUT SYSOUT
    RENAME Q3XDS    XDS
  ENDD
IF &OLD^=  && &MACROS^= THEN +
    DO
      RENAME Q3QJQT   $JQT     /*   M A C R O          */
      RENAME Q3MODEST MODESET  /*   M A C R O          */
      RENAME Q3QCOMMN QCOMMON  /*   M A C R O          */
      RENAME Q3QDEBUG QDEBUG   /*   M A C R O          */
      RENAME Q3QSTART QSTART   /*   M A C R O          */
      RENAME Q3QSTOP  QSTOP    /*   M A C R O          */
      RENAME Q3QTILT  QTILT    /*   M A C R O          */
    ENDD
  WRITE .. CURRENT MEMBER LISTING :
  D
  WRITE .. YOU ARE IN LPDS COMMAND .. TYPE 'END' TO END
