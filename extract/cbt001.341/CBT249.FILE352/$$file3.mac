//TRCANLZD JOB (65003,B29),'BILL WILLIAMS',NOTIFY=$6TSBW,
//             CLASS=H,MSGCLASS=A,MSGLEVEL=(1,1)
//*LOGONID SYS
//*******************************************************************
//*
//*  JOB READS FORMATTED MSS TRACE DATA (3 GEN) TO PROVIDE 24 HOURS
//*  ANALYSIS OF MSS ACTIVITY.  SAS PRINTS THE FOLLOWING REPORTS:
//*
//*    - MSS SYMMARY REPORT
//*    - AVERAGE CARTRIDGE LOADS ON MASS STORAGE - PER HOUR
//*    - AVERAGE PAGES STAGED/DESTAGED ON MASS STORAGE - PER HOUR
//*    - AVERAGE CYLINDERS FAULTS ON MASS STORAGE - PER HOUR
//*    - AVERAGE FORCED LRU ON MASS STORAGE - PER HOUR
//*    - ANALYSIS ON MSVGP'S
//*    - ANALYSIS ON MSVGP'S - DESCENDING CARTRIDGE LOADS
//*
//*******************************************************************
//STEP1   EXEC PGM=IDCAMS
//SYSPRINT DD SYSOUT=*
//DD1      DD DSN=&&MSVGP,DISP=(,PASS),UNIT=SYSDA,SPACE=(CYL,(1,1)),
//            DCB=(RECFM=VBS,LRECL=125,BLKSIZE=629)
//SYSIN    DD *
  LISTMSVI ALLVOLUMES REPORT(STATUS) OFILE(DD1)
//*
//STEP2   EXEC PGM=SAS,PARM='SORT=10',REGION=192K
//LIBRARY  DD DSN=&LIBRARY,UNIT=SYSDA,SPACE=(CYL,(1,,20)),
//            DISP=(MOD,PASS)
//STEPLIB  DD DSN=SAS.LIBRARY,DISP=SHR     <==  SAS793
//         DD DSN=*.LIBRARY,DISP=(OLD,PASS),VOL=REF=*.LIBRARY
//WORK     DD UNIT=SYSDA,SPACE=(6160,(500,200),,,ROUND)
//FT11F001 DD SYSOUT=*,DCB=(BLKSIZE=141,LRECL=137,RECFM=VBA) LOG
//FT12F001 DD SYSOUT=A,DCB=(BLKSIZE=141,LRECL=137,RECFM=VBA),
//            COPIES=1                                       PRINT
//FT13F001 DD SYSOUT=B,DCB=(RECFM=F,BLKSIZE=80)              PUNCH
//FT15F001 DD UNIT=SYSDA,SPACE=(400,(100,300)),         PARMCARDS
//            DCB=(RECFM=FB,LRECL=80,BLKSIZE=400,BUFNO=1)
//INMSVGP  DD DSN=&&MSVGP,DISP=(OLD,DELETE)
//INDSN    DD DSN=FCI1.FORMAT.MSSTRACE(0),DISP=SHR
//         DD DSN=FCI1.FORMAT.MSSTRACE(-1),DISP=SHR
//         DD DSN=FCI1.FORMAT.MSSTRACE(-2),DISP=SHR
//         DD DSN=FCI1.FORMAT.MSSTRACE(-3),DISP=SHR
//SYSOUT   DD SYSOUT=*
//SORTLIB  DD DSNAME=SYS1.SORTLIB,DISP=SHR
//SORTWK01 DD SPACE=(CYL,(10)),UNIT=SYSDA
//SORTWK02 DD SPACE=(CYL,(10)),UNIT=(SYSDA,,SEP=(SORTWK01))
//SORTWK03 DD SPACE=(CYL,(10)),UNIT=(SYSDA,,SEP=(SORTWK01,SORTWK02))
//SYSIN    DD *
**  SAS JOB TO ANALYSIS MSS REFORMATED TRACE DATA FOR THE LAST 24   **
**  HOURS OF PROCESSING.  NOTE: JOB TRCANLZ (S SYS,JOB=TRCANLZ)     **
**  ANALYSIS TRACE DATA FOR THE LAST 30 DAYS OF PROCESSING.  JOB    **
**  PROVIDES THE FOLLOWING REPORTS:                                 **
**   -MSS SUMMARY REPORT                                            **
**   -AVERAGE NUMBER OF CARTRIDGE LOADS ON MASS STORAGE - PER HOUR  **
**   -AVERAGE PAGES STAGED/DESTAGED ON MASS STORAGE - PER HOUR      **
**   -AVERAGE CYLINDERS FAULTED ON MASS STORAGE - PER HOUR          **
**   -AVERAGE FORCED LRU ON MASS STORAGE - PER HOUR                 **
**   -ANALYSIS OF MASS STORAGE VOLUME GROUP ACTIVITY                **
**   -ANALYSIS OF MSVGP ACTIVITY - DESCENDING CARTRIDGE LOADS       **;
 DATA INGP;
 INFILE INMSVGP;
 INPUT  @2   VOLID       $6.
             FILLA       $2.
             MSVGP       $8. ;
  DROP  FILLA ;
  IF (VOLID = '      ') ×
     (VOLID = 'IDCAMS') ×
     (VOLID = 'BASE V') ×
     (VOLID = 'COPIES') ×
     (VOLID = 'DUPLIC') ×
     (VOLID = 'VOLUME') THEN DELETE;
  PROC SORT; BY VOLID;
**   REFORMATED TRACE DATA CREATED BY ICGTRINT                    ****;
 DATA INDATA TIME;
 INFILE INDSN END=ENDA;
 INPUT  @1   STDG     PIB4.
             CFAULT   PIB4.
             SYSSGTM  PIB4.
             DEVSGTM  PIB4.
             DSTGD    PIB4.
             SALLOC   PIB4.
             SYSDSTM  PIB4.
             DEVDSTM  PIB4.
             BUSYTIME PIB4.
             LOADTIME PIB4.
             LOADNUM  PIB1.
             SVEYEAR  PIB1.
             SVEJUL   PIB2.
             SVETME   PIB4.
             PAGES    PIB1.
             MININUM  PIB1.
             HITNUM   PIB1.
             STGDNUM  PIB1.
             DSTGDNUM PIB1.
             VTOCSTG  PIB1.
             VTOCDSTG PIB1.
             LIBRARY  PIB1.
             DEPORT   PIB1.
             DSCU     PIB1.
             CELLZ    PIB1.
             CELLY    PIB1.
             CELLX    PIB1.
             CARTPAIR PIB1.
             READWRIT PIB1.
             VOLID    $6.
             CHARDRC  PIB1.;
** FORMAT DATE (DDMMMYY), TIME (HH:MM:SS), AND DELETE ANY RECORD  **
** CREATED PRIOR TO DESIRED MONITORING DATE/TIME.                 **
** VARIABLE NAMES:  S=SUM, A=AVERAGE, M=MAXIMUM                   **;
 SVEDATE = (SVEYEAR * 1000) + (SVEJUL);
 TRCDATE = DATEJUL(SVEDATE);
 IF SVETME >0 THEN DO;
 HOUR    = SVETME / 3600;
 MINUTE  = (SVETME / 60) - (HOUR * 60);
 SECOND  = (SVETME) - ((HOUR * 3600) + (MINUTE * 60));
                   END;
                   ELSE DO;
 HOUR=00; MINUTE=00; SECOND=00;
                   END;
 TRCTIME = HMS(HOUR,MINUTE,SECOND);
 CURRENT = TODAY( );
 T1 = TIME( );
 H1 = HOUR(T1);
 IF ((CURRENT = (TRCDATE + 1)) & (H1 > HOUR)) ×
    ( CURRENT > (TRCDATE + 1))  THEN DO;
    IF ^ENDA THEN DELETE;
             ELSE DO;
             OUTPUT TIME;
             DELETE;
                 END;
                                  END;
  IF (TRCDATE ×× TRCTIME) > (HIDATE ×× HITIME) THEN DO;
     HIDATE = TRCDATE;        HITIME = TRCTIME;
                                                   END;
  IF (TRCDATE ×× TRCTIME) < (LODATE ×× LOTIME) THEN DO;
     LODATE = TRCDATE;        LOTIME = TRCTIME;
                                                   END;
  RETAIN HIDATE 0 HITIME 0 LODATE 999999 LOTIME 9;
 IF CARTPAIR = 1 THEN SEQ = 'SEQ2-';
                 ELSE DO;
 IF VTOCSTG  = 1 ×
    VTOCDSTG = 1 THEN SEQ = 'SEQ1+';
                 ELSE SEQ = 'SEQ1-';
                 END;
 IF STGDNUM  > 0 THEN DO;
      ACTION = '  STAGE';
      T1_T5  = SYSSGTM + DEVSGTM;         ** SYSSGTM IS T1-T4 TIME **;
      T4_T5  = DEVSGTM  ;
                     END;
                 ELSE DO;
      ACTION = 'DESTAGE';
      T1_T5  = SYSDSTM  ;
      T4_T5  = DEVDSTM  ;
                     END;
  IF (ENDA) THEN OUTPUT TIME INDATA;
            ELSE OUTPUT INDATA;
  FORMAT HITIME  TIME8.       LOTIME TIME8.
         HIDATE  DATE7.       LODATE DATE7.
         TRCTIME TIME8.       T1_T5  5.1
         TRCDATE DATE7.       T4_T5  5.1
         HOUR    2.;
 DROP SVETME MINUTE SECOND STGDNUM DSTGDNUM SYSSGTM
      DEVSGTM SYSDSTM DEVDSTM LIBRARY DEPORT SVEJUL SVEYEAR
      CARTPAIR CELLZ CELLY CELLX CHARDRC;
  PROC SORT DATA=INDATA;  BY VOLID;
  DATA INDATA;
  MERGE INGP(IN=INT)
        INDATA(IN=INS);
  BY VOLID;
  IF INS=1;
  PROC SORT DATA=INDATA; BY READWRIT SEQ DSCU ACTION;
  PROC MEANS MAXDEC=2 NOPRINT;
  BY READWRIT SEQ DSCU ACTION;
  VAR T1_T5 T4_T5 BUSYTIME LOADTIME CFAULT SALLOC LOADNUM PAGES
                  MININUM HITNUM VTOCSTG VTOCDSTG;
  OUTPUT OUT=FIRST
           N=COUNT
         SUM=T1_T5 T4_T5 BUSYTIME LOADTIME CFAULT SALLOC LOADNUM
             PAGES MININUM HITNUM VTOCSTG VTOCDSTG
         MAX=MT1_5 MT4_5 MT2_5 MT2_3;
  PROC SORT DATA=FIRST ;  BY ACTION ;
  PROC MEANS MAXDEC=2 NOPRINT;
  BY ACTION;  VAR MT1_5 MT4_5 MT2_5 MT2_3
                  T1_T5 T4_T5 BUSYTIME LOADTIME
                  CFAULT SALLOC LOADNUM PAGES MININUM HITNUM
                  VTOCSTG VTOCDSTG COUNT;
  OUTPUT OUT=ACTION
         SUM=FILL15 FILL45 FILL25 FILL23
             ST1_5 ST4_5 ST2_5 ST2_3 TFAULT TLRU TCARTL TPAGES TMINIH
             THITL TVSTG TVDSTG COUNT
         MAX=MT1_5 MT4_5 MT2_5 MT2_3;
  PROC SORT DATA=FIRST;  BY READWRIT;
  PROC MEANS MAXDEC=2 NOPRINT;
  BY READWRIT;    VAR MT4_5 MT2_5 MT2_3
                  T4_T5 BUSYTIME LOADTIME
                  LOADNUM PAGES COUNT;
  OUTPUT OUT=DRD
         SUM=FILL45 FILL25 FILL23
             ST4_5 ST2_5 ST2_3 TCARTL TPAGES COUNT
         MAX=MT4_5 MT2_5 MT2_3;
  PROC SORT DATA=FIRST;  BY DSCU;
  PROC MEANS MAXDEC=2 NOPRINT;
  BY DSCU;   VAR MT4_5 T4_T5 PAGES COUNT;
  OUTPUT OUT=SA
         SUM=FILL45 ST4_5 TPAGES COUNT
         MAX=MT4_5;
  PROC SORT DATA=FIRST;  BY SEQ ;
  PROC MEANS MAXDEC=2 NOPRINT;
  BY SEQ;     VAR HITNUM VTOCSTG VTOCDSTG COUNT;
  OUTPUT OUT=SEQ
         SUM=THITL TVSTG TVDSTG COUNT;
  DATA LAST;
  SET TIME(IN=IN1) ACTION DRD SA SEQ;
  FILE PRINT HEADER=HHH NOTITLES;
  IF (ST1_5 > 0)  &  (COUNT > 0) THEN DO;
      ST1_5 = ST1_5 / 4;
      AT1_5 = ST1_5 / COUNT;
                                     END;
  IF (ST4_5 > 0)  &  (COUNT > 0) THEN DO;
      ST4_5 = ST4_5 / 4;
      AT4_5 = ST4_5 / COUNT;
                                     END;
  IF (ST2_5 > 0)  &  (COUNT > 0) THEN DO;
      ST2_5 = ST2_5 / 4;
      AT2_5 = ST2_5 / COUNT;
                                     END;
  IF (ST2_3 > 0)  &  (COUNT > 0) THEN
      AT2_3 = ((ST2_3 / 4) / COUNT);
  IF (MT1_5 > 0)  THEN MT1_5 = MT1_5 / 4;
  IF (MT4_5 > 0)  THEN MT4_5 = MT4_5 / 4;
  IF (MT2_5 > 0)  THEN MT2_5 = MT2_5 / 4;
  IF (MT2_3 > 0)  THEN MT2_3 = MT2_3 / 4;
  IF READWRIT = 0 THEN DRD = '200';
  IF READWRIT = 1 THEN DRD = '201';
  IF READWRIT = 2 THEN DRD = '202';
  IF READWRIT = 3 THEN DRD = '203';
* IF READWRIT = 4 THEN DRD = '204'; ** AJUSTED TO FAIRCHILD A2 MOD ;
* IF READWRIT = 5 THEN DRD = '205';
* IF READWRIT = 6 THEN DRD = '206';
* IF READWRIT = 7 THEN DRD = '207';
  IF DSCU = 0 THEN SA = '800';
  IF DSCU = 1 THEN SA = '810';
* IF DSCU = 2 THEN SA = '820';      ** AJUSTED TO FAIRCHILD A2 MOD;
* IF DSCU = 3 THEN SA = '830';
  IF IN1 THEN DO;
  PUT / @13 'DATA GATHERED FROM REFORMATTED TRACE DATA - ANALYSIS FROM '
             LODATE  + 1 LOTIME ' TO ' HIDATE + 1 HITIME //;
             DELETE;
             END;
  IF ACTION = '  STAGE'  THEN DO;
      PUT / @1 'ACTION' @11 'TOTAL' @18 'TOTAL' @24 'TOTAL'
      @30 ' CART ' @38 ' TOTAL' @46 ' MINI' @52 'ALREADY'
      @60 'VTOC' @66 ' VTOC' @73 '  TOTAL' @83 '   AVE' @92 '   AVE'
      @101 '   AVE' @110 '   AVE';
  PUT @10 'RECORDS' @18 'FAULT' @24 ' LRU' @30 ' LOADS' @38 ' PAGES'
      @45 'HEADERS' @53 'LOADED' @60 'STGE' @66 'DSTGE' @73 '  T1-T5'
      @83 '  T1-T5' @92 '  T4-T5' @101 '  T2-T5' @110 '  T2-T3';
  PUT @83 34*'-';
  PUT @83 '   MAX' @92 '   MAX'   @101 '   MAX' @110 '   MAX' ;
  PUT @83 '  T1-T5' @92 '  T4-T5' @101 '  T2-T5' @110 '  T2-T3' /;
      END;
  IF ACTION ^= '  '  THEN DO;
  PUT / @1 ACTION $7.  @11 COUNT 5.    @18 TFAULT 5.  @24 TLRU  5.
