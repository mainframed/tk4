PROC 0 PRINTER() REMOTE() SYSOUT(A) HOLD DEBUG SENDTO(MVS#KMT)
/*                                                                   */
/* TECHINFO CLIST: RUN INFO/MVS RETRIEVAL PROGRAM (BLGOZD) ON THE    */
/*    INTEL USER DATABASE.                                           */
/* KEN TRUE - INTEL CORP - 4 NOV 81                                  */
/*                                                                   */
CONTROL NOMSG END(ENDD)
IF &DEBUG ^=  THEN CONTROL LIST MSG

FREE F(VSAM,VSAM9,OZPRINT) ATTR(OZATTR)
CONTROL MSG
SET DEST=&STR(        )
SET ROWS=&STR(        )
SET COLS=&STR(        )
SET ENVIRON=&STR(        )
GETINFO  DEST(DEST) ROWS(ROWS) COLS(COLS) ENV(ENVIRON)
IF &ENVIRON = TSO   THEN ERAS3270
IF &ENVIRON = TONE4 THEN $$CLEAR
ATTR OZATTR RECFM(F B A) LRECL(80) BLKSIZE(3120)
WRITE .. WELCOME TO THE INTEL TECHNICAL INFORMATION SYSTEM
WRITE
WRITE    THIS SYSTEM IS BASED ON THE IBM INFO/MVS DATA RETRIEVAL PROGRAM
WRITE
WRITE    WHEN YOU ENTER THE SYSTEM ENTRY G3 WILL BE PRESENTED.
WRITE
WRITE    FOR 'WHATS NEW' SEE ENTRY G2.
WRITE
WRITE    FIRST TIME USERS SHOULD TYPE 'HELP' TO SEE HOW TO OPERATE THE
WRITE    INQUIRY SYSTEM.
WRITE
WRITE    THIS SYSTEM REPLACES THE 'TECHNEWS' COMMAND IN ITS ENTIRETY,
WRITE    AND 'PSANEWS' ARCHIVAL HINT/TIP INFORMATION MAY BE FOUND HERE.
WRITE
WRITE    PLEASE SEND COMMENTS/SUGGESTIONS TO KEN TRUE (SJ1-417 X7-5004)
WRITE
IF &PRINTER ^=  THEN +
  DO
     %##CHKPRT &PRINTER LIST /* CHECK OUT VALIDITY OF DSPRINT PRINTER*/
     SET &RC = &LASTCC
     DO WHILE(&RC ^= 0)
        WRITE ..INVALID LOCAL PRINTER NAME '&PRINTER'
        WRITENR ENTER LOCAL PRINTER NAME===>
        READ &PRINTER
        %##CHKPRT &PRINTER LIST
        SET &RC = &LASTCC
     ENDD
     WRITE .. LOCAL PRINTER '&PRINTER' WILL BE USED FOR HARDCOPY ..
     CONTROL MSG
     ALLOC F(OZPRINT) NEW +
        SP(10,5) TR CAT REL USI(OZATTR)
     CONTROL NOMSG
     IF &DEBUG ^=  THEN CONTROL LIST MSG
  ENDD
ELSE +
  DO
    SET &D = &REMOTE
    IF  &D =  THEN SET &D = &DEST
    IF  &D =  THEN SET &DNAME = &STR()
              ELSE SET &DNAME = &STR(DEST(&D))
     CONTROL MSG
     ALLOC F(OZPRINT) SYSOUT(&SYSOUT) &DNAME &HOLD
    WRITE .. OUTPUT TO SYSTEM PRINTER CLASS=&SYSOUT &DNAME &HOLD
  ENDD
IF &DEBUG ^=  THEN CONTROL LIST MSG
ALLOC F(VSAM)        DS('NTL1.TECHINFO.CLUSTER') SHR
ALLOC F(VSAM9)       DS('IBM.BLGOZS.VSAM')       SHR
TERM SCR(24,80)
%##LOGUSE TECHINFO
$ OZD 'D=BGNU'
TERM SCR(&ROWS,&COLS)
IF &PRINTER ^=  THEN +
  DO
    CONTROL MSG
    WRITE .. NOW ROUTING YOUR LISTING TO &PRINTER ..
    DSPRINT 'TECHINFO.OZPRINT.OUTPUT' &PRINTER DDN(OZPRINT) +
          CC TMAR(2) BMAR(2) NONUM
    CONTROL NOMSG
    FREEALL
  ENDD
FREE F(VSAM,VSAM9,OZPRINT) ATTR(OZATTR)
