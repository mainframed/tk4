./       ADD   NAME=CLEANJCL
PROC 2 INDSNAME OUTDSNAME   DEBUG
  /*                                   */
  /* CLIST TO REFORMAT JCL INTO A NEAT */
  /*  FORM USING PGM=JCLFORMT          */
  /* ADAPTED FROM 'MODIFY' BY P. NUNES */
  /*                                   */
CONTROL NOFLUSH NOMSG END(ENDD)
IF (&SYSPCMD = EDIT × &SYSPCMD = E ) THEN /* OOPS..NOT UNDER EDIT */+
  DO
    WRITE ****RETURN TO TSO COMMAND LEVEL TO EXECUTE CLEANJCL****
    EXIT QUIT
  ENDD
IF &DEBUG ^=  THEN CONTROL LIST CONLIST
DEL &OUTDSNAME
ATTN +
  DO
    ATTN OFF
    WRITE ***ATTENTION ACCEPTED***
    FREE ATTR(MY8080)
    FREE F(SYSUT1 SYSUT2 SYSPRNT1 SYSPRNT2 SYSIN SYSOUT)
    ALLOC FI(SYSUT1) NEW DELETE SPACE(10,2) CYL UNIT(SYSVIO)
    ALLOC FI(SYSUT2) NEW DELETE SPACE(10,5) TRACKS UNIT(SYSVIO)
    ALLOC FI(SYSIN)  DA(*)
    EXIT QUIT
  ENDD
FREE ATTR(MY8080) +
     F(SYSUT1 SYSUT2 SYSPRNT1 SYSPRNT2 SYSIN SYSOUT)
ALLOC F(SYSUT1) DA(&INDSNAME.) SHR
SET &RC = &LASTCC
IF &RC ^= 0 THEN +
  DO
    WRITE **UNABLE TO ALLOCATE INPUT DATASET &INDSNAME.**
    ALLOC FI(SYSUT1) NEW DELETE SPACE(10,2) CYL UNIT(SYSVIO)
    ALLOC FI(SYSUT2) NEW DELETE SPACE(10,5) TRACKS UNIT(SYSVIO)
    ALLOC FI(SYSIN)  DA(*)
    EXIT QUIT
  ENDD
ATTR MY8080 LRECL(80) BLKSIZE(80) RECFM(F,B)
ALLOC F(SYSUT2) DA(&OUTDSNAME) NEW SP(1,5) TR USING(MY8080) CAT
SET &RC = &LASTCC
IF &RC ^= 0 THEN +
  DO
    WRITE **UNABLE TO ALLOCATE OUTPUT DATASET &OUTDSNAME.**
    FREE F(SYSUT1) ATTR(MY8080)
    ALLOC FI(SYSUT1) NEW DELETE SPACE(10,2) CYL UNIT(SYSVIO)
    ALLOC FI(SYSUT2) NEW DELETE SPACE(10,5) TRACKS UNIT(SYSVIO)
    ALLOC FI(SYSIN)  DA(*)
    EXIT QUIT
  ENDD
ALLOC F(SYSOUT) DA(*)
ALLOC F(SYSPRNT1) DUMMY
ALLOC F(SYSPRNT2) SYSOUT(A)
DDSTAR DDNAME(SYSIN)     /* SPOOL CONTROL CARDS FOR JCLFORMAT PGM */
01004DSN=
01007DSNAME=
01006 DATA
01007SYSOUT=
01005DUMMY
01007DDNAME=
02005DISP=
03005UNIT=
04006SPACE=
04006SPLIT=
04009SUBALLOC=
05006LABEL=
06004VOL=
06007VOLUME=
07004AMP=
25004DCB=
!EOD                     /* END OF DATA FOR DDSTAR SPOOLING       */
