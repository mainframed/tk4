  PUT   @2 SEQ    $5.  @11 COUNT 5.    @18 THITL  3.  @26 TVSTG   5.
        @32 TVDSTG  5.;
      END;
  IF SEQ = 'SEQ2-' THEN DO;
  PUT /// @2 'T1-T2 (TIME IN SEC.) = WAITING TRANSFER TO ACTIVE SCHEDULE
 QUEUE & DRD AVAILABILITY';
