//SYSIN   DD   DSN=SYS5.MSSMODS.JOBS.CNTL(MOUNTMSS),DISP=SHR
//*
./       ADD   NAME=UCBZAP
//CLEANIT PROC  A=A
//CLEANIT EXEC PGM=UCBZAP
//SYSPRINT DD  SYSOUT=&A
./       ADD   NAME=VTAMDOC
//VTAMDOC  JOB (MVS19200,0194),'KEN TRUE',NOTIFY=MVS#KMT,
//         CLASS=A,MSGCLASS=M,MSGLEVEL=(1,1)
//*********************************************************************
//*
//* REFER:  TECH.MVS.INSTJCL(VTAMDOC)
//* COMPID: ACF/VTAM,ACF/NCP
//* DOC:    THIS JOB ASSEMBLE THE CURRENT PEP GEN AND VTAM LOCAL
//*         TERMINAL DEFINITIONS TO DOCUMENT THE CURRENT VTAM
//*         TERMINAL CONFIGURATION.  IT IS PART OF THE SERIES OF JOBS
//*         WHICH GENERATES NETWORK DOCUMENTATION FILES FOR TECHINFO.
//*
//* WARNING: BE SURE TO CHANGE PEP=,LOCAL1=,LOCAL2= ON THE
//*         'EXEC VTAMASM' CARD TO REFLECT THE CURRENT PEP AND LOCALS.
//*
//* VERSION 3   06/11/82   KEN TRUE        CHANGE PEP# TO PEP AND
//*    REQUIRE FULL PEP MEMBER NAME.
//* VERSION 2   06/08/82   SHELBY SIEGEL   CHANGE SOURCE DATASET FOR
//*    PEP TO SYS1.VTAMLST.   UPDATE DOCUMENTATION.
//* VERSION 1   02/11/82   KEN TRUE
//*
//*********************************************************************
//*
//VTAMASM PROC PEP=,LOCAL1=,LOCAL2=
//ASM    EXEC  PGM=IEV90,PARM='OBJECT,NODECK'
//SYSLIB   DD  DSN=TECH.INTEL.MODS.NETINFO,DISP=SHR,
//             DCB=BLKSIZE=19040
//         DD  DSN=SYS1.MACLIB,DISP=SHR           SYSTEM MACRO LIBRARY
//SYSUT1   DD  SPACE=(CYL,(5,5)),UNIT=WORK
//SYSUT2   DD  SPACE=(CYL,(5,5)),UNIT=WORK
//SYSUT3   DD  SPACE=(CYL,(5,5)),UNIT=WORK
//SYSPRINT DD  SYSOUT=*
//SYSIN    DD  DSN=TECH.INTEL.MODS.NETINFO(NOGEN),DISP=SHR,
//             DCB=BLKSIZE=6160
//         DD  DSN=SYS1.VTAMLST(&LOCAL1),DISP=SHR
//         DD  DSN=SYS1.VTAMLST(&LOCAL2),DISP=SHR
//         DD  DSN=SYS1.VTAMLST(&PEP),DISP=SHR
//SYSGO    DD  DSN=TECH.NETINFO.TABLES(&PEP),DISP=OLD
//*                                                                   *
//       PEND
//*
//*
//* GENERATE THE DOCUMENTATION FOR THE VTAM CONFIGURATION
//P010209 EXEC VTAMASM,
//             PEP=P010209,     <====CURRENT PEP (FULL MEMBER NAME)
//             LOCAL1=L01CU700, <====LOCAL TERMINALS IN VTAMLST
//             LOCAL2=L01CU720  <====LOCAL TERMINALS IN VTAMLST
//*
//AFTER   EXEC PGM=IEFBR14
//
./       ADD   NAME=WHOGOT
//WHOGOT EXEC PGM=WHOGOT
//STEPLIB DD  DSN=SYS5.MSSMODS.LOAD,DISP=SHR
