./       ADD   NAME=##LOGUSE
PROC 1 CLIST NOTIFY(MVS#KMT) DEBUG
/*                                                                   */
/* LOGUSE   CLIST: SEND INFO TO DESIRED USER RE USAGE OF A CLIST     */
/* KEN TRUE - INTEL CORP - 4 NOV 81                                  */
/*                                                                   */
CONTROL NOMSG END(ENDD)
IF &DEBUG ^=  THEN CONTROL LIST MSG

SET PGMR = &STR(                    )
SET PHONE= &STR(            )
SET LINE= &STR(        )
SET DAY=&STR(         )
SET DATE=&STR(             )
SET ENVIRON = &STR(        )
GETINFO  PGM(PGMR) +
   PHONE(PHONE) DAY(DAY) DATE(DATE) +
   LINE(LINE) ENV(ENVIRON)
IF &ENVIRON = TONE4 THEN SET &WAIT = &STR()
IF &ENVIRON = TSO   THEN SET &WAIT = WAIT
SE '**&CLIST &DAY, &DATE &SYSTIME FROM &LINE **' +
   USER(&NOTIFY) LOG &WAIT
SE '**&CLIST &PGMR &PHONE UNDER &ENVIRON.**' +
   USER(&NOTIFY) LOG &WAIT
END
