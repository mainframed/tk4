CALL 'SYS5.MSSMODS.LOAD(JCLFORMT)'
SET &RC = &LASTCC
IF &RC = 0 THEN +
  WRITE ..&INDSNAME FORMATTED..SAVED ON &OUTDSNAME
 ELSE +
  WRITE **JCL FORMAT FAILED FOR &INDSNAME. .. RC = &RC.**
FREE ATTR(MY8080) +
     F(SYSUT1 SYSUT2 SYSPRNT1 SYSPRNT2 SYSIN SYSOUT)
ALLOC FI(SYSUT1) NEW DELETE SPACE(10,2) CYL UNIT(SYSVIO)
ALLOC FI(SYSUT2) NEW DELETE SPACE(10,5) TRACKS UNIT(SYSVIO)
ALLOC FI(SYSIN)     DA(*)
