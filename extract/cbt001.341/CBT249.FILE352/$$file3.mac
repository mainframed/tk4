  IF READWRIT = 3 THEN DRD = '203';
* IF READWRIT = 4 THEN DRD = '204'; ** AJUSTED TO FAIRCHILD A2 MOD ;
* IF READWRIT = 5 THEN DRD = '205';
* IF READWRIT = 6 THEN DRD = '206';
* IF READWRIT = 7 THEN DRD = '207';
  IF DSCU = 0 THEN SA = '800';
  IF DSCU = 1 THEN SA = '810';
* IF DSCU = 2 THEN SA = '820';      ** AJUSTED TO FAIRCHILD A2 MOD;
* IF DSCU = 3 THEN SA = '830';
  IF IN1 THEN DO;
  PUT / @13 'DATA GATHERED FROM REFORMATTED TRACE DATA - ANALYSIS FROM '
             LODATE  + 1 LOTIME ' TO ' HIDATE + 1 HITIME //;
             DELETE;
             END;
  IF ACTION = '  STAGE'  THEN DO;
      PUT / @1 'ACTION' @11 'TOTAL' @18 'TOTAL' @24 'TOTAL'
      @30 ' CART ' @38 ' TOTAL' @46 ' MINI' @52 'ALREADY'
      @60 'VTOC' @66 ' VTOC' @73 '  TOTAL' @83 '   AVE' @92 '   AVE'
      @101 '   AVE' @110 '   AVE';
  PUT @10 'RECORDS' @18 'FAULT' @24 ' LRU' @30 ' LOADS' @38 ' PAGES'
      @45 'HEADERS' @53 'LOADED' @60 'STGE' @66 'DSTGE' @73 '  T1-T5'
      @83 '  T1-T5' @92 '  T4-T5' @101 '  T2-T5' @110 '  T2-T3';
  PUT @83 34*'-';
  PUT @83 '   MAX' @92 '   MAX'   @101 '   MAX' @110 '   MAX' ;
  PUT @83 '  T1-T5' @92 '  T4-T5' @101 '  T2-T5' @110 '  T2-T3' /;
      END;
  IF ACTION ^= '  '  THEN DO;
  PUT / @1 ACTION $7.  @11 COUNT 5.    @18 TFAULT 5.  @24 TLRU  5.
      @30 TCARTL 6.  @38 TPAGES  6.  @46 TMINIH 6.  @54 THITL 3.
      @59 TVSTG  5.  @66 TVDSTG  5.  @73 ST1_5  8.  @83 AT1_5 7.1
      @92 AT4_5  7.1 @101 AT2_5   7.1 @110 AT2_3 7.1 ;
  PUT @83 34*'-';
  PUT @83 MT1_5  7.1 @92 MT4_5   7.1 @101 MT2_5   7.1 @110 MT2_3 7.1;
      END;
  IF DRD = '200' THEN DO;
      PUT /// @4 'DRD' @11 'TOTAL' @19 'CART' @26 'TOTAL' @34 '  TOTAL'
      @44 ' AVE' @51 ' AVE' @58 ' AVE'
      @65 '   MAX' @74 '   MAX' @83 '   MAX';
      PUT @10 'RECORDS' @18 'LOADS' @26 'PAGES' @34 '  T2_T5'
      @44 'T4-T5'  @51 'T2-T5'  @58 'T2-T3'
      @65 '  T4-T5'  @74 '  T2-T5'  @83 '  T2-T3'/;
      END;
  IF DRD ^= '  ' THEN DO;
  PUT   @4 DRD    $3.  @11 COUNT 5.    @18 TCARTL  6.  @26 TPAGES  6.
        @34 ST2_5  8.  @44 AT4_5 5.1   @51 AT2_5  5.1  @58 AT2_3   5.1
        @65 MT4_5  7.1 @74 MT2_5 7.1   @83 MT2_3 7.1;
      END;
  IF SA  = '800' THEN DO;
      PUT /// @4 'SA  ' @11 'TOTAL' @18 'TOTAL' @26 '   TOTAL'
      @36 ' AVE' @43 '   MAX';
      PUT @10 'RECORDS' @18 'PAGES' @26 '   T4_T5'
      @36 'T4-T5'  @43 '  T4-T5'/;
      END;
  IF SA ^= '  ' THEN DO;
  PUT   @4 SA     $3.  @11 COUNT 5.    @18 TPAGES  6.  @26 ST4_5   8.
        @36 AT4_5 5.1  @43 MT4_5  7.1  ;
      END;
  IF SEQ = 'SEQ1+' THEN DO;
     PUT /// @4 'CART' @11 'TOTAL' @18 'ALREADY' @26 'VTOC' @32 'VTOC';
      PUT @10 'RECORDS' @18 'LOADED' @26 'STGE' @32 'DSTGE'/;
      END ;
  IF SEQ ^= '  ' THEN DO;
  PUT   @2 SEQ    $5.  @11 COUNT 5.    @18 THITL  3.  @26 TVSTG   5.
        @32 TVDSTG  5.;
      END;
  IF SEQ = 'SEQ2-' THEN DO;
  PUT /// @2 'T1-T2 (TIME IN SEC.) = WAITING TRANSFER TO ACTIVE SCHEDULE
 QUEUE & DRD AVAILABILITY';
  PUT     @2 'T2-T3 (TIME IN SEC.) = WAITING ON PICKER, MOVE TIME, LOAD
TIME, WAITING FOR PATH & DUFFER TO READ CTOC, READ CTOC';
  PUT     @2 'T3-T4 (TIME IN SEC.) = WAITING ON SA';
  PUT     @2 'T4-T5 (TIME IN SEC.) = WAITING ON PATH, DATA TRANSFER, SEA
RCH';
