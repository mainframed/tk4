SET &MAXCC = 0
ALLOC F(XFERPAN)  DA('INTRO.XFERPAN') SHR
ALLOC F(PRODPAN)  DA('FCI1.PANSORC')  SHR
ALLOC F(PANSPOOL) DA('SYS2.PANSPOOL') SHR
CONTROL NOMSG
IF &MAXCC ^= 0 THEN +
  DO
