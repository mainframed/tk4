 TITLE FAIRCHILD CAMERA & INSTRUMENT CORP. - HOST SYSTEM SOFTWARE;
 TITLE2 AVERAGE FORCED LRU ON MASS STORAGE - PER HOUR;
  DATA ENDIT;
  SET SECOND;
  IF MSVGP = '      ' THEN MSVGP = 'NONGROUP' ;
  PROC SORT; BY MSVGP;
  PROC MEANS MAXDEC=2 NOPRINT;
  BY MSVGP;  VAR PAGES VTOCSTG VTOCDSTG SALLOC CFAULT LOADNUM MININUM;
  OUTPUT OUT=GROUP
             SUM=PAGES VTOCSTG VTOCDSTG SALLOC CFAULT LOADNUM MININUM;
  PROC PRINT N;
  ID  MSVGP ;
  VAR MININUM LOADNUM VTOCSTG VTOCDSTG PAGES CFAULT SALLOC;
 TITLE FAIRCHILD CAMERA & INSTRUMENT CORP. - HOST SYSTEM SOFTWARE;
 TITLE2 ANALYSIS OF MASS STORAGE VOLUME GROUP ACTIVITY ;
