              ELSE SET &V2 = &STR()
 IF &UNIT1 ^=  THEN SET &U1 = &STR(UNIT(&UNIT1))
               ELSE SET &U1 = &STR()
 IF &UNIT2 ^=  THEN SET &U2 = &STR(UNIT(&UNIT2))
               ELSE SET &U2 = &STR()
 SET &MAXCC = 0
 ALLOC F(SYSLIB1) DA(&LIB1) SHR &V1 &U1
 ALLOC F(SYSLIB2) DA(&LIB2) SHR &V2 &U2
 IF &MAXCC ^= 0 THEN +
   DO
     WRITE ..UNABLE TO ALLOC..EXITING
     ALLOC F(SYSIN) DA(*)
     ALLOC F(SYSPRINT) DA(*)
     FREEAX
     EXIT QUIT
   ENDD
 ATTR FB LRECL(80) BL(6160) RECFM(F B)
 ALLOC F(UT1) NEW SP(5,2) TR USI(FB)
 ALLOC F(UT2) NEW SP(5,2) TR USI(FB)
 IF &OUTPUT ^=  THEN +
   DO
     IF &NEW ^=  THEN +
       DO
         DELETE &OUTPUT
         ALLOC F(OUTPUT) DA(&OUTPUT) NEW SP(5,2) TR REL USI(FB)
       ENDD
      ELSE +
         ALLOC F(OUTPUT) DA(&OUTPUT) OLD
   ENDD
 ELSE +
   ALLOC F(OUTPUT) DUMMY BL(80)
 FREE ATTR(FB)
 IF &SYSOUT ^=  +
   THEN  ALLOC F(SYSPRINT) SYSOUT(&CLASS) &HOLD &D
   ELSE  ALLOC F(SYSPRINT) DA(*)
 ALLOC F(SYSIN) DA(*)
 WRITE ENTER COMMANDS
 WRITE (LIB1P,LIB1S,LIB1C,LIB2P,LIB2S,LIB2C,BOTHP,BOTHS,BOTHC)
 WRITE (....P=PREFIX ....S=SUFFIX ....C=COMMENT)
 WRITE  END WITH A SEMICOLON.  (UPPERCASE IN STRINGS!!!)
 CALL 'SYS5.MSSMODS.LOAD(COMPMEMB)' 'NR'
 IF &DEBUG =  THEN FREEAX
 IF &SYSOUT ^=  THEN +
  DO
    FREE F(SYSPRINT) &HOLD
    ALLOC F(SYSPRINT) DA(*)
  ENDD
./       ADD   NAME=C963A001
//C963A001 PROC MAPOUT=Z,
//             EOM=NO,                  SET EOM=YES FOR HISTORY RPT
//             SYSOUT='*',
//             DUMPOUT=A,
//             DEBUG=,                  SET DEBUG=DEBUG FOR 963P00
//             PDS=NOPDS,               SET PDS=PDS FOR DIR BLK (*1*)
//             FCI1=FCI1,
//             FCI2=FCI2,
//             F016=F016,               SET F016=F916 FOR WEEKLY
//             F017=F017,               SET F017=F917 FOR WEEKLY
//             A00963=A00963,
//             MSSVOL=MSSVOLB1,
//             VOLSER=PRODX1,
//             CTLCARD='SYS5.MSSMODS.JOBS.CNTL',
//             MSVCCAT=MSVCCAT,
//             CONTROL=S963DCTL,
//             PRTJOB=J963PRNT,
//             WSPACE=3,
//             SCRCOPY=3,
//             M4COPY=1,
//             M4LIB='SYS2.M4USER'
//*********************************************************************
//* NOTE: SETTING PDS=PDS WILL CAUSE MAPPING OF PDS DIR BLK ALLOC..   *
//*       AND SIGNIF INCREASE IN RUN TIME FOR VOLUMES WITH PDS'S      *
//*       DUE TO CYLINDERFAULT OF PDS DIRECTORIES ON MSS.    KT       *
//*********************************************************************
//*        STEP000 - DELETE TEMPORARY DATASETS (IF NEED BE)           *
//*                  AND RESIDENT DATASETS: F011,F020, L002           *
//*********************************************************************
//STEP000 EXEC PGM=IEFBR14
//DD1A     DD  DSN=&FCI2..&A00963..F0963.F010,
//             DISP=(MOD,DELETE,DELETE),
//             UNIT=SYSDA,SPACE=(TRK,(0))
//DD1B     DD  DSN=&FCI2..&A00963..F0963.F010,
//             DISP=(MOD,UNCATLG,DELETE),
//             UNIT=SYSDA,SPACE=(TRK,(0)),VOL=REF=*.DD1A
//DD2A     DD  DSN=&FCI1..&A00963..F0963.F011,
//             DISP=(MOD,DELETE,DELETE),
//             UNIT=SYSDAV,SPACE=(CYL,(1)),MSVGP=PROD9
//DD2B     DD  DSN=&FCI1..&A00963..F0963.F011,
//             DISP=(MOD,UNCATLG,DELETE),
//             UNIT=SYSDAV,SPACE=(CYL,(1)),VOL=REF=*.DD2A
//DD3A     DD  DSN=&FCI2..&A00963..F0963.F000,
//             DISP=(MOD,DELETE,DELETE),
//             UNIT=SYSDA,SPACE=(TRK,(0))
//DD3B     DD  DSN=&FCI2..&A00963..F0963.F000,
//             DISP=(MOD,UNCATLG,DELETE),
//             UNIT=SYSDA,SPACE=(TRK,(0)),VOL=REF=*.DD3A
//DD4A     DD  DSN=&FCI1..&A00963..F0963.F020,
//             DISP=(MOD,DELETE,DELETE),
//             UNIT=SYSDA,SPACE=(TRK,(0)),VOL=SER=&VOLSER
//DD4B     DD  DSN=&FCI1..&A00963..F0963.F020,
//             DISP=(MOD,UNCATLG,DELETE),
//             UNIT=SYSDA,SPACE=(TRK,(0)),VOL=SER=&VOLSER
//DD5A     DD  DSN=&FCI1..&A00963..L0963.L002,
//             DISP=(MOD,DELETE,DELETE),
//             UNIT=SYSDA,SPACE=(TRK,(0)),VOL=SER=&VOLSER
//DD5B     DD  DSN=&FCI1..&A00963..L0963.L002,
//             DISP=(MOD,UNCATLG,DELETE),
//             UNIT=SYSDA,SPACE=(TRK,(0)),VOL=SER=&VOLSER
//DD6A     DD  DSN=&FCI1..&A00963..F0963.F005,
//             DISP=(MOD,DELETE,DELETE),
//             UNIT=SYSDA,SPACE=(TRK,(0))
//DD6B     DD  DSN=&FCI1..&A00963..F0963.F005,
//             DISP=(MOD,UNCATLG,DELETE),
//             UNIT=SYSDA,SPACE=(TRK,(0)),VOL=REF=*.DD6A
//DD7A     DD  DSN=&FCI2..&A00963..F0963.F005,
//             DISP=(MOD,DELETE,DELETE),
//             UNIT=SYSDA,SPACE=(TRK,(0))
//DD7B     DD  DSN=&FCI2..&A00963..F0963.F005,
//             DISP=(MOD,UNCATLG,DELETE),
//             UNIT=SYSDA,SPACE=(TRK,(0)),VOL=REF=*.DD7A
//DD8A     DD  DSN=&FCI2..&A00963..F0963.F015,
//             DISP=(MOD,DELETE,DELETE),
//             UNIT=SYSDA,SPACE=(TRK,(0))
//DD8B     DD  DSN=&FCI2..&A00963..F0963.F015,
//             DISP=(MOD,UNCATLG,DELETE),
//             UNIT=SYSDA,SPACE=(TRK,(0)),VOL=REF=*.DD8A
//*********************************************************************
//*        STEP010 - READ MSVI FOR MSS VOLUMES TO BE MONITORED        *
//*                  PRODUCE F005 FILE (MONVOLS)                      *
//*********************************************************************
//STEP010 EXEC PGM=P963P30,
//             PARM='NR'
//SYSPRINT DD  SYSOUT=&SYSOUT
//REPORT   DD  SYSOUT=&SYSOUT
//MSVI     DD  DSN=&MSVCCAT..MSVI,DISP=SHR
//MONVOLS  DD  DSN=&FCI2..&A00963..F0963.F005,DISP=(NEW,CATLG,DELETE),
//             UNIT=SYSDA,
//             SPACE=(6160,(3,1),RLSE),
//             DCB=(RECFM=FB,LRECL=80,BLKSIZE=6160)
//SYSIN    DD  DSN=&FCI1..&A00963..F0963.F001(&MSSVOL),DISP=SHR
//*********************************************************************
//*        STEP015 - MERGE IN REAL DASD TO BE MONITORED AND ELIMINATE *
//*                  DUPLICATE VOLUMES ..PRODUCE F005 (MONVOLS) FILE  *
//*********************************************************************
//STEP015 EXEC PGM=P963P35,
//             PARM='NR,NOSTAE,NOSPIE'
//SYSPRINT DD  SYSOUT=&SYSOUT
//REPORT   DD  SYSOUT=&SYSOUT
//SYSOUT   DD  SYSOUT=&SYSOUT
//SORTLIB  DD  DSN=SYS1.SORTLIB,DISP=SHR
//SORTWK01 DD  UNIT=SYSDA,SPACE=(CYL,(1,1)),DISP=(,DELETE)
//SORTWK02 DD  UNIT=SYSDA,SPACE=(CYL,(1,1)),DISP=(,DELETE)
//SORTWK03 DD  UNIT=SYSDA,SPACE=(CYL,(1,1)),DISP=(,DELETE)
//$ORTPARM DD  DSN=SYS2.CTLCARDS(U963SP01),DISP=SHR
//OUTVOLS  DD  DSN=&FCI1..&A00963..F0963.F005,DISP=(NEW,CATLG,DELETE),
//             UNIT=SYSDA,SPACE=(6160,(6,6),RLSE),
//             DCB=(RECFM=FB,LRECL=80,BLKSIZE=6160)
//INVOLS   DD  DSN=&FCI2..&A00963..F0963.F005,DISP=(OLD,DELETE,KEEP)
//         DD  DSN=&FCI1..&A00963..F0963.F001(REALDASD),DISP=SHR
//*********************************************************************
//*        STEP018 - SCRATCH/UNCATALOG DATASETS                       *
//*********************************************************************
//STEP018 EXEC PGM=P963P02,
//             PARM='NR,NOSTAE,NOSPIE'                    MF041580
//INSCR    DD  DSN=FCI1.A00963.F0963.F817,DISP=SHR
//REPORT   DD  SYSOUT=&SYSOUT
//SYSUDUMP DD  SYSOUT=&SYSOUT
//SYSPRINT DD  SYSOUT=&SYSOUT
//SYSOUT   DD  SYSOUT=&SYSOUT
//SORTLIB  DD  DSN=SYS1.SORTLIB,DISP=SHR
//SORTWK01 DD  UNIT=SYSDA,SPACE=(CYL,(1,1)),DISP=(,DELETE)
//SORTWK02 DD  UNIT=SYSDA,SPACE=(CYL,(1,1)),DISP=(,DELETE)
//SORTWK03 DD  UNIT=SYSDA,SPACE=(CYL,(1,1)),DISP=(,DELETE)
//$ORTPARM DD  DSN=SYS2.CTLCARDS(U963SP01),DISP=SHR
//*********************************************************************
//*        STEP020 - READ VTOCS OF VOLUMES TO BE MONITORED            *
//*                  PRODUCE F010 FILE (DSNLOG)                       *
//*********************************************************************
//STEP020 EXEC PGM=MAPDISK,
//             PARM='EXT,&PDS,LOG,MSS'
//SYSPRINT DD  SYSOUT=&MAPOUT
//SYSUDUMP DD  SYSOUT=&DUMPOUT
//DSNLOG   DD  DSN=&FCI2..&A00963..F0963.F010,
//             DISP=(NEW,CATLG,DELETE),
//             UNIT=SYSDA,
//             SPACE=(6156,(80,6),RLSE),
//             DCB=(RECFM=FB,LRECL=114,BLKSIZE=6156)
//MSSVOLS  DD  DSN=&FCI1..&A00963..F0963.F005,DISP=(OLD,DELETE,KEEP)
//*********************************************************************
//*        STEP030 - SORT THE F010(DSNLOG) FILE BY DSN,MSVGP, AND VOL *
//*                  SORT CARD IS:                                    *
//*                    SORT FIELDS=(1,44,CH,A,45,8,CH,A,53,6,CH,A)    *
//*********************************************************************
//STEP030 EXEC PGM=SORT
//SORTIN   DD  DSN=&FCI2..&A00963..F0963.F010,
//             DISP=(OLD,DELETE,KEEP)
//SORTOUT  DD  DSN=&FCI1..&A00963..F0963.F011,
//             DISP=(NEW,CATLG,DELETE),
//             UNIT=SYSDAV,MSVGP=PROD9,
//             SPACE=(6156,(30,3),RLSE,,ROUND),
//             DCB=(RECFM=FB,LRECL=114,BLKSIZE=6156)
//SYSIN    DD  DSN=&CTLCARD(S963SLOG),DISP=SHR
//SYSOUT   DD  SYSOUT=&SYSOUT
//SORTLIB  DD  DSN=SYS1.SORTLIB,DISP=SHR
//SORTWK01 DD  UNIT=SYSDA,SPACE=(CYL,&WSPACE)
//SORTWK02 DD  UNIT=SYSDA,SPACE=(CYL,&WSPACE)
//SORTWK03 DD  UNIT=SYSDA,SPACE=(CYL,&WSPACE)
//*********************************************************************
//*        STEP040 - SORT THE F000 (GRANT) FILE BY DSN,MSVGP, AND VOL *
//*                  SORT CARD IS:                                    *
//*                    SORT FIELDS=(1,44,CH,A,73,8,CH,A,45,6,CH,A)    *
//*********************************************************************
//STEP040 EXEC PGM=SORT
//SORTIN   DD  DSN=&FCI1..&A00963..F0963.F000,
//             DISP=SHR
//SORTOUT  DD  DSN=&FCI2..&A00963..F0963.F000,
//             DISP=(NEW,CATLG,DELETE),
//             UNIT=SYSDA,
//             SPACE=(6160,(15,5),RLSE),
//             DCB=(RECFM=FB,LRECL=80,BLKSIZE=6160)
//SYSIN    DD  DSN=&CTLCARD(S963SGNT),DISP=SHR
//SYSOUT   DD  SYSOUT=&SYSOUT
//SORTLIB  DD  DSN=SYS1.SORTLIB,DISP=SHR
//SORTWK01 DD  UNIT=SYSDA,SPACE=(CYL,&WSPACE)
//SORTWK02 DD  UNIT=SYSDA,SPACE=(CYL,&WSPACE)
//SORTWK03 DD  UNIT=SYSDA,SPACE=(CYL,&WSPACE)
//*********************************************************************
//*        STEP050 - RUN THE DASD MONITOR PROGRAM                     *
//*                  MATCH/MERGE SORTED DSNLOG TO SORTED GRANT FILES  *
//*                  PRODUCE MERGED FILE AND SCRLOG. RPT=001,002      *
//*********************************************************************
//STEP050 EXEC PGM=P963P00,
//             REGION=764K,
//             PARM='NR/&DEBUG.'
//SYSPRINT DD  SYSOUT=&SYSOUT
//REPORT   DD  SYSOUT=&SYSOUT
//SCRRPT   DD  DSN=&FCI1..&A00963..L0963.L002,
//             DISP=(NEW,CATLG,DELETE),
//             UNIT=SYSDA,VOL=SER=&VOLSER,
//             SPACE=(6233,(10,5),RLSE),
//             DCB=(RECFM=VBA,LRECL=135,BLKSIZE=6233)
//CONTROL  DD  DSN=&CTLCARD(&CONTROL),
//             DISP=SHR
//INLOG    DD  DSN=&FCI1..&A00963..F0963.F011,
//             DISP=(OLD,KEEP,KEEP)
//INGRANT  DD  DSN=&FCI2..&A00963..F0963.F000,
//             DISP=(OLD,DELETE,KEEP)
//OUTLOG   DD  DSN=&FCI1..&A00963..F0963.F020,
//             DISP=(NEW,CATLG,DELETE),
//             UNIT=SYSDA,VOL=SER=&VOLSER,
//             SPACE=(6106,(150,15),RLSE),
//             DCB=(RECFM=FB,LRECL=142,BLKSIZE=6106)
//OUTSCR   DD  DSN=&FCI2..&A00963..F0963.F015,
//             DISP=(NEW,CATLG,DELETE),
//             UNIT=SYSDA,
//             SPACE=(6160,(10,5),RLSE),
//             DCB=(RECFM=FB,LRECL=80,BLKSIZE=6160)
//VOLSUM   DD  DSN=&FCI1..&A00963..F0963.F025,
//             DISP=(MOD,KEEP,KEEP)
//******************************************************************
//*    STEP051 - AUTO SCRATCH DASDMON DATASETS   -  STEP RESTARTABLE
//******************************************************************
//STEP051 EXEC PGM=P963P27,COND=(1000,NE,STEP050)
//INSCR    DD  DSN=&FCI2..&A00963..F0963.F015,
//             DISP=(OLD,KEEP,KEEP)
//INLOG    DD  DSN=&FCI1..&A00963..F0963.F020,
//             DISP=SHR
//NEWSCR   DD  DSN=&FCI1..&A00963..F0963.&F016.(+1),
//             DISP=(,CATLG,DELETE),
//             UNIT=SYSDA,SPACE=(6160,(12,1),RLSE),
//             VOL=SER=&VOLSER,
//             DCB=(FCIMODEL,LRECL=80,BLKSIZE=6160,RECFM=FB)
//OLDSCR   DD  DSN=&FCI1..&A00963..F0963.&F017.(+1),
//             DISP=(,CATLG,DELETE),
//             UNIT=SYSDA,SPACE=(6160,(12,3),RLSE),
//             VOL=SER=&VOLSER,
//             DCB=(FCIMODEL,LRECL=80,BLKSIZE=6160,RECFM=FB)
//INHIST   DD  DSN=&FCI1..&A00963..F0963.F120(+0),
//             DISP=SHR
//OUTHIST  DD  DSN=&FCI1..&A00963..F0963.F120(+1),
//             DISP=(NEW,CATLG,DELETE),
//             UNIT=3330V,SPACE=(CYL,(4,1),RLSE),
//             MSVGP=PROD9,
//             DCB=(FCIMODEL,LRECL=296,BLKSIZE=6216,RECFM=FB)
//SYSPRINT DD  SYSOUT=&SYSOUT
//*********************************************************************
//*        STEP052 - SCRATCH/UNCATALOG DATASETS - AUTO SCRATCH        *
//*                - CHANGE GDG +1 TO 0 THEN STEP RESTART             *
//*********************************************************************
//STEP052 EXEC PGM=P963P02,
//             PARM='NR,NOSTAE,NOSPIE',                MF041580
//             COND=(1000,NE,STEP050)
//INSCR    DD  DSN=&FCI1..&A00963..F0963.&F016.(+1),DISP=SHR
//REPORT   DD  SYSOUT=&SYSOUT
//SYSUDUMP DD  SYSOUT=&SYSOUT
//SYSPRINT DD  SYSOUT=&SYSOUT
//SYSOUT   DD  SYSOUT=&SYSOUT
//SORTLIB  DD  DSN=SYS1.SORTLIB,DISP=SHR
//SORTWK01 DD  UNIT=SYSDA,SPACE=(CYL,(1,1)),DISP=(,DELETE)
//SORTWK02 DD  UNIT=SYSDA,SPACE=(CYL,(1,1)),DISP=(,DELETE)
//SORTWK03 DD  UNIT=SYSDA,SPACE=(CYL,(1,1)),DISP=(,DELETE)
//$ORTPARM DD  DSN=SYS2.CTLCARDS(U963SP01),DISP=SHR
//*********************************************************************
//** STEP053 - PUNCH THE SCRLOG CARDS - DELETED BY BILL WILLIAMS ON
//**           JAN 23, 80.... NO NEED FOR SCRATCH CARD OUTPUT.
//**EP053 EXEC PGM=IEBGENER,
//**           COND=(1000,NE,STEP050)
//**SPRINT DD  SYSOUT=&SYSOUT
//**SUT1   DD  DSN=&FCI1..&A00963..F0963.&F017.(+1),
//**           DISP=(OLD,KEEP,KEEP)
//**SUT2   DD  SYSOUT=B,DCB=(RECFM=F,BLKSIZE=80)
//**SIN    DD  DUMMY,
//**           DCB=BLKSIZE=80
//*********************************************************************
//*        STEP055 - PRINT THE SCRATCH REPORT                         *
//*********************************************************************
//STEP055 EXEC PGM=IEBGENER,
//             COND=(1000,NE,STEP050)
//SYSPRINT DD  SYSOUT=&SYSOUT
//SYSUT1   DD  DSN=&FCI1..&A00963..L0963.L002,
//             DISP=(OLD,KEEP,KEEP)
//SYSUT2   DD  SYSOUT=&SYSOUT,COPIES=&SCRCOPY,
//             DCB=(RECFM=VBA,LRECL=135,BLKSIZE=6233)
//SYSIN    DD  DUMMY,
//             DCB=BLKSIZE=80
//*********************************************************************
//*        STEP058 - TRANSMIT THE SCRATCH REPORT                      *
//*********************************************************************
//*TEP058 EXEC PGM=P963P03, DELETED BY KT
//*            PARM='NR',
//*            COND=(1000,NE,STEP050)
//*YSPRINT DD  SYSOUT=&SYSOUT
//*NLIST   DD  DSN=&FCI1..&A00963..L0963.L002,
//*            DISP=SHR
//*XRDR    DD  SYSOUT=(A,INTRDR)
//*XDSK    DD  DSN=&FCI2..&A00963..X0963.X002,
//*            DISP=(NEW,KEEP,DELETE),
//*            UNIT=SYSDAV,MSVGP=XMIT,
//*            SPACE=(6118,(40,4),RLSE,,ROUND)
//* PT14   DD  SYSOUT=(A,,1411),DEST=RMT14
//*XPT71   DD  SYSOUT=(A,,1411),DEST=STECH
//* PT00   DD  SYSOUT=(Y,,1414)
//*XPT09   DD  SYSOUT=(A,,1411),DEST=RMT9
//* PT00   DD  SYSOUT=(Y,,1411)
//* PT00   DD  SYSOUT=(Y,,1411)
//*********************************************************************
//*        STEP059 - SUBMIT JOB TO PRINT LOCAL SCRATCH RPT FOR DISTRIB*
//*********************************************************************
//STEP059 EXEC PGM=IEBGENER,
//             COND=(1000,NE,STEP050)
//SYSPRINT DD  SYSOUT=&SYSOUT
//SYSUT1   DD  DSN=&CTLCARD(&PRTJOB),
//             DISP=SHR
//         DD  DSN=&CTLCARD(EOF),
//             DISP=SHR
//SYSUT2   DD  SYSOUT=(A,INTRDR)
//SYSIN    DD  DUMMY,
//             DCB=BLKSIZE=80
//*********************************************************************
//*        STEP060 - RUN MARK IV EXTRACTOR FOR ADDITIONAL REPORTS     *
//*********************************************************************
//STEP060 EXEC PGM=MARKIV,
//             COND=(1000,NE,STEP050)
//M4INPUT  DD  DSN=&CTLCARD(S963MRPT),DISP=SHR
//M4OLD    DD  DSN=&FCI1..&A00963..F0963.F020,
//             DISP=(OLD,KEEP,KEEP)
//M4LIST   DD  SYSOUT=&SYSOUT
//M4REPO   DD  DISP=(NEW,PASS),UNIT=SYSDA,
//             SPACE=(CYL,(&WSPACE,&WSPACE),RLSE)
//M4SORT   DD  DISP=(NEW,PASS),UNIT=SYSDA,SPACE=(TRK,1)
//M4LIB    DD  DSN=&M4LIB,DISP=SHR
//*********************************************************************
//*        STEP070 - SORT THE MARK IV OUTPUT                          *
//*********************************************************************
//STEP070 EXEC PGM=SORT,
//             COND=((4,LE,STEP060),(1000,NE,STEP050)),
//             PARM='CORE=100000'
//SORTIN   DD  DSN=*.STEP060.M4REPO,DISP=(OLD,DELETE)
//SORTLIB  DD  DSN=SYS1.SORTLIB,DISP=SHR
//SORTOUT  DD  DISP=(NEW,PASS),UNIT=SYSDA,
//             SPACE=(CYL,(&WSPACE,&WSPACE),RLSE)
//SORTWK01 DD  UNIT=SYSDA,SPACE=(CYL,&WSPACE)
//SORTWK02 DD  UNIT=SYSDA,SPACE=(CYL,&WSPACE)
//SORTWK03 DD  UNIT=SYSDA,SPACE=(CYL,&WSPACE)
//SYSIN    DD  DSN=*.STEP060.M4SORT,DISP=(OLD,DELETE)
//SYSOUT   DD  SYSOUT=&SYSOUT
//*********************************************************************
//*        STEP080 - INVOKE MARK IV TO PRINT THE REPORTS              *
//*********************************************************************
//STEP080 EXEC PGM=MARKIV,
//             COND=((4,LE,STEP060),(1000,NE,STEP050))
//M4INPUT  DD  DSN=&CTLCARD(S963M4RC),DISP=SHR
//M4LIST   DD  SYSOUT=&SYSOUT,COPIES=&M4COPY
//M4REPI   DD  DSN=*.STEP070.SORTOUT,DISP=(OLD,DELETE)
//*********************************************************************
//*  IF STEP090 'EOM=NO' THEN STEP095, 100, AND 110 DOES NOT EXECUTE. *
//*********************************************************************
//STEP090  EXEC PGM=RTNCODE,PARM=&EOM
//*********************************************************************
//*        STEP095 - RUN MARK IV EXTRACTOR FOR HISTORY REPORT         *
//*        STEP095, 100, AND 110 EXECUTE ON WEEK END RUN ONLY         *
//*********************************************************************
//STEP095 EXEC PGM=MARKIV,
//             COND=((1,NE,STEP090),(1000,NE,STEP050)),
//             REGION=764K
//M4INPUT  DD  DSN=&CTLCARD(S963MHIS),DISP=SHR
//M4OLD    DD  DSN=&FCI1..&A00963..F0963.F120(+1),DISP=SHR
//         DD  DSN=&FCI1..&A00963..F0963.F121(+0),DISP=SHR
//         DD  DSN=&FCI1..&A00963..F0963.F122(-1),DISP=SHR
//M4LIST   DD  SYSOUT=&SYSOUT
//M4REPO   DD  DISP=(NEW,PASS),UNIT=SYSDA,
//             SPACE=(CYL,(&WSPACE,&WSPACE),RLSE)
//M4SORT   DD  DISP=(NEW,PASS),UNIT=SYSDA,SPACE=(TRK,1)
//M4LIB    DD  DSN=&M4LIB,DISP=SHR
//*********************************************************************
//*        STEP100 - SORT THE MARK IV OUTPUT                          *
//*********************************************************************
//STEP100 EXEC PGM=SORT,
//             COND=((1,NE,STEP090),(1000,NE,STEP050)),
//             PARM='CORE=100000'
//SORTIN   DD  DSN=*.STEP095.M4REPO,DISP=(OLD,DELETE)
//SORTLIB  DD  DSN=SYS1.SORTLIB,DISP=SHR
//SORTOUT  DD  DISP=(NEW,PASS),UNIT=SYSDA,
//             SPACE=(CYL,(&WSPACE,&WSPACE),RLSE)
//SORTWK01 DD  UNIT=SYSDA,SPACE=(CYL,&WSPACE)
//SORTWK02 DD  UNIT=SYSDA,SPACE=(CYL,&WSPACE)
//SORTWK03 DD  UNIT=SYSDA,SPACE=(CYL,&WSPACE)
//SYSIN    DD  DSN=*.STEP095.M4SORT,DISP=(OLD,DELETE)
//SYSOUT   DD  SYSOUT=&SYSOUT
//*********************************************************************
//*        STEP110 - INVOKE MARK IV TO PRINT THE REPORTS              *
//*********************************************************************
//STEP110 EXEC PGM=MARKIV,
//             COND=((1,NE,STEP090),(1000,NE,STEP050)),
//             REGION=764K
//M4INPUT  DD  DSN=&CTLCARD(S963M4RC),DISP=SHR
//M4LIST   DD  SYSOUT=&SYSOUT,COPIES=&M4COPY
//M4REPI   DD  DSN=*.STEP100.SORTOUT,DISP=(OLD,DELETE)
//*********************************************************************
//*        STEP120 - SORT ALL HISTORY FILES FOR CLIST - SHOWHIST      *
//*********************************************************************
//STEP120  EXEC PGM=FCISORT
