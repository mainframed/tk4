*          DATA SET FSF        AT LEVEL 001 AS OF 03/13/81
*          DATA SET SR308000   AT LEVEL 005 AS OF 10/15/78
*          DATA SET SR308000   AT LEVEL 004 AS OF 09/21/78
*          DATA SET SR308000   AT LEVEL 003 AS OF 04/26/78
*          DATA SET SR308000   AT LEVEL 001 AS OF 04/21/78
         TITLE 'SR308000 - TSO FULL SCREEN FACILITY INTERFACE'
*
*              AUTHOR. DON HIGGINS.
*              DATE.   04/21/78.
*              REMARKS.THIS PROGRAM DEFINES PROVIDES CALL INTERFACE
*                      TO THE DYNAMICALLY LOADED COMMON SUPPORT
*                      PROGRAM FCIMOD07. SEE FCIMOD07 FOR DOCUMENTATION
*
*              MAINT.
*                      REV1 09/21/78 ADD FSACF
*                      REV2 10/15/78 ADD FSMODE
         TITLE 'LOCAL MACROS'
         MACRO
&N       ROUTE &CODE
         USING *,R15
&N       LA    R0,&CODE
         B     START
         ENTRY &N
         MEND
         TITLE 'MAINLINE'
SR308000 CSECT
FSR      ROUTE 1        FS READ
FSW      ROUTE 2        FS WRITE
FSARF    ROUTE 3        FS AREA READ FIELD
FSAWF    ROUTE 4        FS AREA WRITE FIELD
FSAM     ROUTE 5        FS AREA MOVE
FST      ROUTE 6        FS TITLE
FSRL     ROUTE 7        FS READ LINE
FSWL     ROUTE 8        FS WRITE LINE
FSACF    ROUTE 9        FS AREA COMPARE FIELD
FSMODE   ROUTE 10       FS SET MODE SWITCHES
FSEND    ROUTE 11       FS MODE END - CLEAR SCREEN                 FCI*
START    BALR  R15,0
         USING *,R15
         L     R15,ASUB
         BR    R15
ASUB     DCWV  FCIMOD07 DYNAMIC CALL INTERFACE SUPPORT SUBROUTINE
         EQUAL
         END
