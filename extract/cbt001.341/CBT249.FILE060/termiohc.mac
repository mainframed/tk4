TERMIOHC TITLE 'HARDCOPY TERMIO SUPPORT FOR TSO '
         DCMSREGS
         EJECT
TERMIOHC CSECT
         USING TERMIOHC,R11
         LR    R11,R15
*        TERMIO# SAVED THE REGISTERS
         LR    R10,R1              POINT TO IOCB
         USING IOCB,R10
         GETMAIN R,LV=WORKLEN,SP=2
         ST    R13,4(R1)           CHAIN
         ST    R1,8(R13)           THE
         LR    R13,R1              SAVE AREAS
         USING DCMSWORK,R13
         SPACE 3
         OI    IOCBFLGT,IOCBTHCY
         TM    IOCBFLGS,IOCBFNWR+IOCBFNRD
         BO    QUITLOG
         TM    IOCBFLGS,IOCBFNWR
         BO    NOWRITE
         CLI   IOCBTERM,X'7F'
         BE    NOCHECK
         GTSIZE
         STM   0,1,IOCBTERM
         MVI   IOCBTERM,X'7F'
NOCHECK  EQU   *
         LH    R2,IOCBTERM+6
         CH    R2,=H'80'
         BNL   CONTINU
         TPUT  CANTDO,L'CANTDO
         LA    R15,16
         MVI   IOCBAID,AIDPA1
         B     QUITLOG
CONTINU  EQU   *
         SR    R0,R0           .
         SR    R15,R15         .
         SR    R1,R1           .
         LA    R14,LOGBUF      .
         ICM   R1,8,=C' '      .
         L     R15,=A(28*80) . .
         MVCL  R14,R0 .        .   CLEAR BUFFER OUT
         L     R9,IOCBFMT
         L     R8,IOCBIMG
         LH    R7,0(R9)
         LH    R6,2(R9)
         AR    R6,R9
         LA    R9,6(R9)
         LA    R1,LOGBUF
FORMLOOP EQU   *
         LH    R2,2(R9)            TO OFFSET
         AR    R2,R1
         LH    R4,4(R9)            FROM OFFSET
         AR    R4,R8
         LH    R3,6(R9)            GET LENGTH
         LR    R5,R3               IN = OUT (LENGTH
         LA    R5,0(R5)            PADCHAR
*
         MVCL  R2,R4
*
         LA    R9,8(R9)            NEXT FORMAT ENTRY
         CR    R9,R6
         BL    FORMLOOP
*
*        LOGOUT VERSION OF SCREEN COMPLETED HERE
*
         LA    R2,LOGBUF
         LA    R3,23
         LA    R4,80
         TPUT  SLASH,80
LOGLOOP  EQU   *
         MVI   0(R2),C'>'
         TPUT  (R2),(R4)
         AR    R2,R4
         BCT   R3,LOGLOOP
         LA    R1,IOCBTIME+1
         LA    R0,STAMPLEN
         TPUT  (1),(0)
         TPUT  SLASH,80
         TPUT  SPACEZ,1
NOWRITE  EQU   *
         MVI   IOCBAID,AIDENT
         TM    IOCBFLGS,IOCBFNRD
*        BO    QUITLOG
         LA    R1,OUTPUTL
         L     R0,=A(136)
         TGET  (1),(0)
         LTR   R15,R15
         BZ    GETLGTH
         CH    R15,=H'8'
         BE    DUMATTN
         BL    GETLGTH
         EX    0,*
GETLGTH  LA    R1,0(R1)
         LR    R3,R1
         BCTR  R3,0
         EX    R3,TRANS          TRANSLATE INPUT TO UPPER CASE
*        LR    R3,R1
         MVC   IOCBEND(L'IOCBEND),OUTPUTL
         CH    R1,=H'1'
         BE    DUMPFK
         CH    R1,=H'2'
         BNE   INPUT
         CLI   OUTPUTL,C'1'
         BNE   INPUT
         CLI   OUTPUTL+1,C'0'
         BNE   KEY11
         MVI   IOCBAID,AIDPF10
         B     QUITLOG
TRANS    TR    OUTPUTL(0),UPCTBL      FORCE UPPERCASE ON INPUT
KEY11    CLI   OUTPUTL+1,C'1'
         BNE   KEY12
         MVI   IOCBAID,AIDPF11
         B     QUITLOG
KEY12    CLI   OUTPUTL+1,C'2'
         BNE   QUITLOG
         MVI   IOCBAID,AIDPF12
         B     QUITLOG
DUMPFK   EQU   *
         CLI   OUTPUTL,C'!'
         BNE   DUMPFK2
         MVI   OUTPUTL,AIDPA1
DUMPFK2  CLI   OUTPUTL,AIDPF1
         BL    PFK02
         CLI   OUTPUTL,AIDPF9
         BH    QUITLOG
         MVC   IOCBAID(1),OUTPUTL
         B     QUITLOG
PFK02    CLI   OUTPUTL,AIDPA3
         BL    QUITLOG
         CLI   OUTPUTL,AIDPF12
         BH    QUITLOG
         MVC   IOCBAID(1),OUTPUTL
         B     QUITLOG
DUMATTN  EQU   *
         MVI   IOCBAID,AIDPA1
         B     QUITLOG
*
INPUT    EQU   *
         L     R9,IOCBFMT
         L     R8,IOCBIMG
         LH    R7,0(R9)
         LH    R6,2(R9)
         AR    R6,R9
         LA    R9,6(R9)
LOOKLOOP EQU   *
         TM    0(R9),X'80'  CURSOR IN FIELD?
         BO    GOTCUR
         LA    R9,8(R9)
         CR    R9,R6
         BNH   LOOKLOOP
         B     QUITLOG
GOTCUR   EQU   *
         LH    R4,4(R9)   GET OFFSET
         AR    R4,R8      OFFSET + START
         AR    R1,R4
         CR    R1,R9       ? END
         BNH   MOVEIN
         LR    R1,R9
         SR    R1,R4
         LR    R3,R1
MOVEIN   EX    R3,MOVEXX
         B     QUITLOG
MOVEXX   MVC   0(0,R4),OUTPUTL
         EJECT
SLASH    DC    80C'-'
SPACEZ   DC    CL160' '
         SPACE 2
UPCTBL   DC    256C'?'                 ALL INVALIDS GO TO QUESTION
         ORG   UPCTBL+C' '
         DC    C' '
         ORG   UPCTBL+C'Ö'
         DC    C'Ö.<(+×&&'
         ORG   UPCTBL+C'!'
         DC    C'!$*);^-/'
         ORG   UPCTBL+C','
         DC    C',%_>?'
         ORG   UPCTBL+C':'
         DC    C':#@''="'
         ORG   UPCTBL+X'81'
         DC    C'ABCDEFGHI'
         ORG   UPCTBL+X'91'
         DC    C'JKLMNOPQR'
         ORG   UPCTBL+X'A2'
         DC    C'STUVWXYZ'
         ORG   UPCTBL+C'A'
         DC    C'ABCDEFGHI'
         ORG   UPCTBL+C'J'
         DC    C'JKLMNOPQR'
         ORG   UPCTBL+C'S'
         DC    C'STUVWXYZ'
         ORG   UPCTBL+C'0'
         DC    C'0123456789'
         ORG
         SPACE 4
CANTDO   DC    C'LINESIZE OF TERMINAL MUST BE 80 OR GREATER'
         DS    0F .
QUITLOG  DS     0H .
         SR    R15,R15
DCMSRCD  EQU   *
         LR    R2,R13
         L     R1,4(R13)
         ST    R15,16(R1)
         LR    R13,R1
         FREEMAIN R,LV=WORKLEN,A=(R2),SP=2
         LM    14,12,12(13)
         BR    R14
         LTORG
         EJECT
IOCB DSECT
         IOCB
         EJECT
DCMSWORK DSECT
DCMSAVE  DS    18F .
PARMADDR DS    F
OUTPUTL  DS    3F .
LOGBUF   DS    28CL80 .
WORKLEN  EQU   *-DCMSWORK
         END
