//SMP001R0 JOB 527TSO000S0008,TEC,CLASS=J,MSGCLASS=1
//*
//* THIS IS A COPY OF THE IMS SVC THAT IS REQUIRED
//* THE CALL FOR THIS SVC IS IN THE NUCLEUS
//* IF YOU FORGET TO PUT THIS INTO LPALIB YOU
//* WILL GET ALL 806-4 FOR MODULE DFSMRCL0 ON
//* EVERYTHING
//*
// EXEC IEBCOPY,SO=1
//IN DD DSN=SYS1.LPALIB,DISP=SHR,UNIT=DASD,VOL=SER=DAGENP
//O1 DD DSN=SYS1.LPALIB,DISP=SHR,UNIT=DASD,VOL=SER=DAGENT
//O2 DD DSN=SYS1.LPALIB,DISP=SHR,UNIT=DASD,VOL=SER=DAGEN6
//O3 DD DSN=SYS1.LPALIB,DISP=SHR,UNIT=DASD,VOL=SER=NEWRES
 COPY INDD=IN,OUTDD=O1
  S M=DFSMRCL0
 COPY INDD=IN,OUTDD=O2
  S M=DFSMRCL0
 COPY INDD=IN,OUTDD=O3
  S M=DFSMRCL0

