//SMP001R0 JOB 527TSO000S0006,TEC,CLASS=J,MSGCLASS=1
// EXEC SMP8106
//SMPPTFIN DD *
++USERMOD (CB00015).
++VER (Z038) FMID(JBB1122)                /*
*
* THIS ZAP RESULTS IN SELECTION OF THE DEVICE WITH LOWEST UCB
* ADDRESS, AND HENCE TENDS TO REUSE THE SAME DEVICE REPEATEDLY
* AS IN MVT. IT SUPRESS THE SAVING OF THE MOST RECENTLY ALLOCATED
* UCB SO THAT EACH CALL THEN ALLOCATES THE UCB CLOSEST TO ZERO
*
*              */.
++ZAP  (IRARMIOM).
 NAME   IRARMIOM
 VER 090A 47E0,C8F2             BNO    @RF000389
 VER 090E 5080,22C8             ST     @08,RCVTAPAD(,RMCTPTR)
 VER 0912 4880,4000  @RF000389  LH     @08,IDXPTR(,IDX)
 VER 0916 5860,CC54             L      DEVPTR,DEVSTL
 REP 090E 4700                  NOP    @08,RCVTAPAD(,RMCTPTR)
 DUMP IEANUC01 IRARMIOM
/*
//SMPCNTL DD *
  REJECT   S(CB00015) .
  RECEIVE  S(CB00015).
  APPLY    S(CB00015) BYPASS(ID) .
//
