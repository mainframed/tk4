//STEP01 EXEC IEBCOPY,SO=1
//IN     DD   DSN=SYS1.LPALIB,DISP=SHR,UNIT=DASD,VOL=SER=AAAAAA
//OT1    DD   DSN=SYS1.LPALIB,DISP=SHR,UNIT=DASD,VOL=SER=BBBBBB
