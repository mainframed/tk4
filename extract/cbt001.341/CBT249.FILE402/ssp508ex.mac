//SSP508X  JOB (145011,SMVS,TN,SYSTEMS),
//            DEAN.DAVIS,
//            NOTIFY=SSPXDD,
//            CLASS=A,
//            MSGCLASS=Z
//STEP010  EXEC PGM=SORT
//STEPLIB  DD  DSN=SSP.CASORT61.LINKLIB,
//             DISP=SHR
//SORTLIB  DD  DSN=SSP.CASORT61.SORTLIB,
//             DISP=SHR
//SYSOUT   DD  SYSOUT=*
//SORTIN   DD  DSN=SSP.MOA.DMSEXT,
//             DISP=(OLD,PASS)
//SORTOUT  DD  DSN=&&DMSSORT,
//             UNIT=SYSDA,
//             DISP=(NEW,PASS,DELETE),
//             DCB=(RECFM=FB,LRECL=105,BLKSIZE=19005),
//             SPACE=(TRK,(20,20),RLSE)
//SORTWK01 DD  UNIT=SYSDA,
//             SPACE=(TRK,(10,10))
//SORTWK02 DD  UNIT=SYSDA,
//             SPACE=(TRK,(10,10))
//SORTWK03 DD  UNIT=SYSDA,
//             SPACE=(TRK,(10,10))
//SYSIN    DD  *
 SORT FIELDS=(1,44,A),FORMAT=BI
 END
//STEP020  EXEC PGM=SSP508
//STEPLIB  DD  DSN=SYS1.COBLIB,
//             DISP=SHR
//SYSUDUMP DD  SYSOUT=*
//SYSOUT   DD  SYSOUT=*
//DMSEXT   DD  DSN=&&DMSSORT,
//             DISP=(OLD,PASS)
//DMSVSAM  DD  DSN=SSPXDD.FB300,
//             DISP=SHR
//STEP030  EXEC PGM=IDCAMS
//SYSPRINT DD  SYSOUT=*
//FILEIN   DD  DSN=SSPXDD.FB300,DISP=(OLD,KEEP)
//FILEOUT  DD  DSN=SSPV.ONLINE.SSPDSM.KSDS,DISP=SHR
//SYSPRINT DD  SYSOUT=*
   VERIFY FILE(FILEOUT)
   REPRO -
         INFILE(FILEIN) -
         OUTFILE(FILEOUT) -
         REPLACE -
         REUSE
   PRINT -
         INFILE(FILEOUT)
//
