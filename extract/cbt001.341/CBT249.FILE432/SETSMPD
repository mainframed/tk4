00011005PROC 0 EXIST NOM
00020002  IF &EXIST = EXIST THEN GOTO RECALL
00030000  ISPEXEC VGET (SUBSYS CDSDSN CDSVOL CDSUNIT PTSDSN PTSUNIT PTSVOL EXITOPT ENQDSN -
00040010                LPREFIX MSVGP SPRIM SSEC CCMCDSN)
00050000  ISPEXEC VPUT (SUBSYS CDSDSN CDSVOL CDSUNIT PTSDSN PTSUNIT PTSVOL EXITOPT ENQDSN -
00060010                LPREFIX MSVGP SPRIM SSEC CCMCDSN) PROFILE
00070001  GOTO EXIT
00080000RECALL:  +
00081009  ISPEXEC VGET ZAPPLID
00082009  IF &ZAPPLID ^= THEN GOTO EXIT
00090000  ISPEXEC VGET (SUBSYS CDSDSN CDSVOL CDSUNIT PTSDSN PTSUNIT PTSVOL EXITOPT ENQDSN -
00100010                LPREFIX MSVGP SPRIM SSEC CCMCDSN) PROFILE
00110000  ISPEXEC VPUT (SUBSYS CDSDSN CDSVOL CDSUNIT PTSDSN PTSUNIT PTSVOL EXITOPT ENQDSN -
00120010                LPREFIX MSVGP SPRIM SSEC CCMCDSN) SHARED
00130001EXIT:  +
00130105  IF &NOM = NOM THEN EXIT
00131004  WRITE SMP VARIABLES INITIALIZED
