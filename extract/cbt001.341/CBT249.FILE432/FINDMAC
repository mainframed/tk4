00010000PROC 0
00020000CONTROL NOMSG
00030000FREE F(IN MACOUT)
00040001DEL FINDMAC.REPORT
00050000CONTROL MSG
00060003    ISPEXEC VGET (MACIN FRDISP)
00070000    ALLOC F(IN) DS(&MACIN) SHR
00080003    IF &FRDISP = T THEN ALLOC F(MACOUT) DS(*)
00090001       ELSE ALLOC F(MACOUT) DS(FINDMAC.REPORT) NEW SPA(1,1) TR BLOCK(6160)
00100000    $P FINDMAC
00110000FREE F(IN MACOUT)
00120002    %GENPRINT DSN(FINDMAC.REPORT)
