00010001PROC 0 DEBUG
00020000IF &DEBUG = DEBUG THEN CONTROL LIST CONLIST SYMLIST PROMPT NOFLUSH
00030000IF &DEBUG = THEN CONTROL NOMSG
00040002FREE F(PDS SYSPRINT)
00050000CONTROL MSG
00060003ISPEXEC VGET (EDSN DIR)
00070003ALLOC F(PDS) DS(&EDSN) SHR
00071002ALLOC F(SYSPRINT) DS(*)
00080000IF &DEBUG = THEN CONTROL NOMSG
00090000$P EXPDIR '&DIR'
00100000FREE F(PDS)
