00010002PROC 0 DSN(IPCSBRWS.LIST)
00011002IF &DSN = THEN GOTO OTHER
00020002IF &DSN = IPCSBRWS.LIST THEN GOTO SETIT
00030002GOTO REV
00070002SETIT: SET &DSN = &SYSPREF..IPCSBRWS.LIST
00080001GOTO REV
00090001OTHER: WRITENR ENTER BROWSE DSN ===>
00100000READ &DSN
00110000REV: +
00120003ISPEXEC BROWSE DATASET('&DSN')
