00010000PROC 0 DEBUG
00020000 IF &DEBUG = DEBUG THEN CONTROL MSG LIST CONLIST SYMLIST PROMPT NOFLUSH
00030000 ISPEXEC VGET (TXTDSN TXTMEM)
00040000 SET &STXTDSN = &TXTDSN
00050000 ISPEXEC VPUT (STXTDSN)
00060000 ISPEXEC SELECT PGM(FIXUPDSN) PARM(STXTDSN)
00070000 ISPEXEC VGET (STXTDSN)
00071002 SET &STXTDSN = &STXTDSN
00080000 CONTROL NOMSG
00090000 IF &TXTMEM = THEN ISPEXEC EDIT DATASET('&STXTDSN')
00100000              ELSE ISPEXEC EDIT DATASET('&STXTDSN(&TXTMEM)')
