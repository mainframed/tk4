00010001PROC 0 DEBUG
00011001IF &DEBUG = DEBUG THEN CONTROL LIST CONLIST SYMLIST PROMPT NOFLUSH
00012001                  ELSE CONTROL PROMPT
00020006ISPEXEC VGET (DPARM)
00021000CONTROL NOMSG
00030005FREE F(SYSPRINT)
00030101DEL DISKMAP.REPORT
00030201ALLOC F(SYSPRINT) DS(DISKMAP.REPORT) NEW SPA(10,10) TR
00031000CONTROL MSG
00050004ISPEXEC SELECT PGM(DISKMAP) PARM('&DPARM')
00060000%GENPRINT DSN(DISKMAP.REPORT)
