-NUM: PROCEDURE(ST,LN) RETURNS(CHAR(3) VARYING) REORDER;
    DECLARE
      ST                 CHAR(*) VARYING,
      LN                 FIXED BIN(15,0),
      NM                 FIXED BIN(31,0) INIT(0),
      STR                CHAR(3) VARYING INIT('');
    NM = ST;
    STR = SUBSTR('   ',1,LN);
    UNSPEC(STR) = UNSPEC(LN) ×× SUBSTR(UNSPEC(NM),33-8*LN);
    RETURN(STR);
 END NUM;
