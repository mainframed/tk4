//FORTSCL PROC PDS='EN.TERM.COMPRESS'
//*
//* SYSUT1 AND SYSUT2 ADDED 4-16-80 JRD
//*
//*
//*
//*
//FORT    EXEC PGM=DASOURCE,REGION=1024K
//STEPLIB  DD  DSN=SYS2.SP.CMPRSRCE,DISP=SHR
//         DD  DSN=SYS2.FORTRANX.COMPILER,DISP=SHR
//WORK04   DD  SYSOUT=A
//SYSPRINT DD  SYSOUT=A
//SYSLIN   DD  DSNAME=&LOADSET,DISP=(MOD,PASS),UNIT=SYSDA,             X
//         SPACE=(CYL,(1,1),RLSE),DCB=BLKSIZE=3200
//INPUT    DD  UNIT=DISK,SPACE=(CYL,(2,2))
//WORKVOL  DD  UNIT=SYSDA,VOL=REF=REF.WORKVOL,DISP=SHR
//SYSUT1   DD  UNIT=DISK,SPACE=(TRK,(10,5))
//SYSUT2   DD  UNIT=DISK,SPACE=(TRK,(10,5))
//SYSUT3   DD  UNIT=DISK,SPACE=(TRK,(10,5))
//SYSUT4   DD  UNIT=DISK,SPACE=(TRK,(10,5))
//WORK01   DD  UNIT=DISK,SPACE=(CYL,(1,1,1))
//WORK02   DD  UNIT=DISK,SPACE=(CYL,(1,1,1)),
//             DCB=(RECFM=FB,LRECL=80,BLKSIZE=400)
//WORK03   DD  UNIT=DISK,SPACE=(CYL,(2,2))
//PDS      DD  DSN=&PDS,DISP=SHR
//SYSUDUMP DD  SYSOUT=A
//LKED     EXEC PGM=IEWL,REGION=1024K,COND=(4,LT,FORT),
//             PARM='LIST,LET,XREF,SIZE=(500K,100K)'
//SYSUT1 DD UNIT=DISK,SPACE=(CYL,(1,1)),DCB=BLKSIZE=1024
//SYSPRINT DD  SYSOUT=A,DCB=BLKSIZE=4840
//SYSLMOD  DD  DSNAME=&GOSET(MAIN),DISP=(NEW,PASS),UNIT=SYSDA,         X
//             SPACE=(CYL,(1,,1))
//SYSLIN   DD  DSNAME=&LOADSET,DISP=(OLD,DELETE)
//         DD  DDNAME=SYSIN
//SYSLIB   DD  DSN=SYS2.FORTRANX.LINKLIB,DISP=SHR,DCB=BLKSIZE=19069
//         DD  DSN=EN.SSP.FORT.LOADLIB,DISP=SHR
