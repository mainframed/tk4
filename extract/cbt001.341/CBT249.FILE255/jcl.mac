//MARTYA  JOB (0000DMJS00,DK1D),SPRINZEN,CLASS=B,TIME=2,NOTIFY=MARTY,
//    MSGCLASS=A
//*********************************************************************
//*                                                                   *
//*                           R M F R P T                             *
//*                                                                   *
//*                      COMPILE RMFPRT MODULES AND RUN               *
//*   NOTE: CHANGE "XXXXXXXX" TO VALID USERID IN JCL                  *
//*********************************************************************
//PLIXCL    PROC  PROG=,USERID=XXXXXXXX
//PLI       EXEC  PGM=IEL0AA,REGION=512K,
//          PARM='NAG,NSTG,NOF,NOESD,NOMAP,STMT,GN,A,XREF'
//SYSIN     DD    DSN=&USERID..RMFRPT.PLI(&PROG),DISP=SHR
//SYSPRINT  DD    SYSOUT=A
//SYSLIN    DD    DSN=&&LOADSET,DISP=(MOD,PASS),UNIT=SYSDA,
//          SPACE=(80,(250,100))
//SYSUT1    DD    DSN=&&SYSUT1,UNIT=SYSDA,
//          SPACE=(1024,(200,50),,CONTIG,ROUND),DCB=BLKSIZE=1024
//LKED      EXEC  PGM=IEWL,PARM='LIST,LET,MAP',REGION=100K
//SYSLIB    DD    DSN=SYS1.PLIBASE,DISP=SHR
//          DD    DSN=&USERID..RMFRPT.LOAD,DISP=SHR
//SYSLMOD   DD    DSN=&USERID..RMFRPT.LOAD(&PROG),DISP=SHR
//SYSUT1    DD    DSN=&&SYSUT1,UNIT=SYSDA,
//          SPACE=(1024,(200,50),,CONTIG,ROUND),DCB=BLKSIZE=1024
//SYSPRINT  DD    SYSOUT=*
//SYSLIN    DD    DSN=&&LOADSET,DISP=(OLD,DELETE)
//          DD    DDNAME=SYSIN
//          PEND
//LOAD      EXEC PGM=IEFBR14
//CREATE    DD DSN=XXXXXXXX.RMFRPT.LOAD,DISP=(,CATLG),UNIT=SYSDA,
//          SPACE=(TRK,(24,10,5),RLSE)
//REC70     EXEC PLIXCL,PROG=REC70
//REC71     EXEC PLIXCL,PROG=REC71
//REC72     EXEC PLIXCL,PROG=REC72
//REC73     EXEC PLIXCL,PROG=REC73
//REC74     EXEC PLIXCL,PROG=REC74
//REC75     EXEC PLIXCL,PROG=REC75
//REC76     EXEC PLIXCL,PROG=REC76
//REC76A    EXEC PLIXCL,PROG=REC76A
//WEEKDAY   EXEC PLIXCL,PROG=WEEKDAY
//REPORT    EXEC PLIXCL,PROG=REPORT
//RMFRPT    EXEC PLIXCL,PROG=RMFRPT
//RUN       EXEC PGM=RMFRPT,REGION=1152K,COND=(4,LT)
//STEPLIB  DD DSN=XXXXXXXX.RMFRPT.LOAD,DISP=SHR
//SYSPRINT DD SYSOUT=A
//SYSUT1   DD DSN=SYS1.MANX,DISP=SHR,
//         DCB=(RECFM=VBS,LRECL=32000,BLKSIZE=4000)
//SYSIN    DD DSN=XXXXXXXX.RMFRPT.PLI(SAMPDATA),DISP=SHR
//
