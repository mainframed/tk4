         COMMAND PLI,CDL=PLICDL,LAST=YES
PLICDL   CDL   PGM=IEL0AC,ADL=PLIADL,OPTLIST=PLIOPT,                   X
               SDL=PLISDL,TASKDSN='SYS2.PLILIB'
PLIOPT   OPTION QUAL=PLI,DAIRPB=SYSINPB,                               X
               PROMPT='ENTER DATA SET NAME',                           X
               HELP='NAME OF DATA SET CONTAINING PLI SOURCE',          X
               ALLOC=OLD
         OPTION NAME=PRINT,DAIRPB=PRTPB,INSERT=SOURCE,QUAL=LIST,       X
               DEFAULT=NOPRINT,DCB=PRTDCB
         OPTION NAME=LIB,INSERT=INCLUDE,ALLOC=(OLD,PDS),               X
               DAIRPB=LIBPB,QUAL=PLI
         OPTION NAME=OBJ,QUAL=OBJ,DEFAULT='OBJ(TSO.OBJ)',DCB=OBJDCB,   X
               LAST=YES,DAIRPB=OBJPB,NOINSRT=NOOBJECT
SYSINPB  DAIRPB DDNAME=SYSIN,DISP=(SHR,KEEP,KEEP)
PLIUT1   DAIRPB DSN='TSO.SYSUT1',DISP=(SHR,KEEP,KEEP),                 X
               CONTROL=(ABKL,UID),SPACE=(3200,100,100),DDNAME=SYSUT1,  X
               VOLUME=WORK
PRTPB    DAIRPB DDNAME=SYSPRINT,DISP=(SHR,KEEP,KEEP),                  X
               SPACE=(3200,100,100),CONTROL=ABKL,VOLUME=WORK
OBJPB    DAIRPB DDNAME=SYSLIN,DISP=(SHR,KEEP,KEEP),                    X
               SPACE=(3200,100,100),CONTROL=ABKL,VOLUME=WORK
LIBPB    DAIRPB DDNAME=SYSLIB,DISP=(SHR,KEEP,KEEP)
PLIADL   ADL   SYSLIN,0,0,SYSLIB,SYSIN,SYSPRINT,SYSPUNCH,SYSUT1
PRTDCB   DCB   DSORG=PS,RECFM=FBM,BLKSIZE=6050,                        X
               DDNAME=SYSPRINT,MACRF=W,LRECL=121
OBJDCB   DCB   DSORG=PS,RECFM=FB,BLKSIZE=3200,                         X
               DDNAME=SYSLIN,MACRF=W,LRECL=80
PLISDL   SDL   PLIUT1
         GENSTR
         END
