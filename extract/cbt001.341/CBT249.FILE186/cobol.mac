         COMMAND COBOL,CDL=COBCDL,LAST=YES
COBCDL   CDL   PGM=IKFCBL00,ADL=COBADL,OPTLIST=COBOPT,                 X
               SDL=COBSDL
COBOPT   OPTION QUAL=COBOL,DAIRPB=SYSINPB,                             X
               PROMPT='ENTER DATA SET NAME',                           X
               HELP='NAME OF DATA SET CONTAINING COBOL SOURCE',        X
               ALLOC=OLD
         OPTION NAME=PRINT,DAIRPB=PRTPB,QUAL=LIST,DCB=PRTDCB,          X
               DEFAULT=NOPRINT,DFINSRT=NOSOURCE,NOINSRT=NOSOURCE
         OPTION NAME=LIB,ALLOC=(OLD,PDS),                              X
               DAIRPB=LIBPB,QUAL=COBOL,NOINSRT=NOLIB,                  X
               DEFAULT='LIB(''SYS2.COBOL.COPYLIB'')'
         OPTION NAME=TERM,DEFAULT=TERM,INSERT=TERM,DFINSRT=TERM
         OPTION NAME=OBJ,QUAL=OBJ,DEFAULT='OBJ(TSO.OBJ)',DCB=OBJDCB,   X
               LAST=YES,DAIRPB=OBJPB,NOINSRT=NOOBJECT
SYSINPB  DAIRPB DDNAME=SYSIN,DISP=(SHR,KEEP,KEEP)
COBUT1   DAIRPB DSN='TSO.SYSUT1',DISP=(SHR,KEEP,KEEP),                 X
               CONTROL=(ABKL,UID),SPACE=(3200,100,100),DDNAME=SYSUT1,  X
               VOLUME=WORK
PRTDCB   DCB   DSORG=PS,RECFM=FBM,BLKSIZE=6050,                        X
               DDNAME=SYSPRINT,MACRF=W,LRECL=121
OBJDCB   DCB   DSORG=PS,RECFM=FB,BLKSIZE=3200,                         X
               DDNAME=SYSLIN,MACRF=W,LRECL=80
COBUT2   DAIRPB DSN='TSO.SYSUT2',DISP=(SHR,KEEP,KEEP),                 X
               CONTROL=(ABKL,UID),SPACE=(3200,100,100),DDNAME=SYSUT2,  X
               VOLUME=WORK
COBUT3   DAIRPB DSN='TSO.SYSUT3',DISP=(SHR,KEEP,KEEP),                 X
               CONTROL=(ABKL,UID),SPACE=(3200,100,100),DDNAME=SYSUT3,  X
               VOLUME=WORK
COBUT4   DAIRPB DSN='TSO.SYSUT4',DISP=(SHR,KEEP,KEEP),                 X
               CONTROL=(ABKL,UID),SPACE=(3200,100,100),DDNAME=SYSUT4,  X
               VOLUME=WORK
PRTPB    DAIRPB DDNAME=SYSPRINT,DISP=(SHR,KEEP,KEEP),                  X
               SPACE=(3200,100,100),CONTROL=ABKL,VOLUME=WORK
OBJPB    DAIRPB DDNAME=SYSLIN,DISP=(SHR,KEEP,KEEP),                    X
               SPACE=(3200,100,100),CONTROL=ABKL,VOLUME=WORK
LIBPB    DAIRPB DDNAME=SYSLIB,DISP=(SHR,KEEP,KEEP)
COBADL   ADL   SYSLIN,0,0,SYSLIB,SYSIN,SYSPRINT,SYSPUNCH,SYSUT1,       X
               SYSUT2,SYSUT3,SYSUT4
COBSDL   SDL   COBUT1,COBUT2,COBUT3,COBUT4
         GENSTR
         END
