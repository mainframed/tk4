         COMMAND ASMF,CDL=ASMCDL,LAST=YES
ASMCDL   CDL   PGM=IFOX00,ADL=ASMADL,OPTLIST=ASMOPT,                   X
               SDL=ASMSDL
ASMOPT   OPTION QUAL=ASM,DAIRPB=SYSINPB,                               X
               PROMPT='ENTER DATA SET NAME',                           X
               HELP='NAME OF DATA SET CONTAINING ASM SOURCE',          X
               ALLOC=OLD
         OPTION NAME=PRINT,DAIRPB=PRTPB,QUAL=LIST,DCB=PRTDCB,          X
               DEFAULT=NOPRINT,DFINSRT=NOLIST,NOINSRT=NOLIST
         OPTION NAME=LIB,CONCAT='''SYS1.MACLIB''',ALLOC=(OLD,PDS),     X
               DAIRPB=LIBPB,QUAL=ASM,DEFAULT='LIB(''SYS1.MACLIB'')'
         OPTION NAME=TERM,DEFAULT=TERM,INSERT=TERM,DFINSRT=TERM
         OPTION NAME=STMT,DEFAULT=NOSTMT,INSERT='STMT,NONUM',          X
               DFINSRT=NOSTMT,NOINSRT=NOSTMT
         OPTION NAME=DECK,DEFAULT=NODECK,DFINSRT=NODECK,INSERT=DECK,   X
               NOINSRT=NODECK
         OPTION NAME=OBJ,QUAL=OBJ,DEFAULT='OBJ(TSO.OBJ)',DCB=OBJDCB,   X
               LAST=YES,DAIRPB=OBJPB,INSERT=OBJECT,DFINSRT=OBJECT
SYSINPB  DAIRPB DDNAME=SYSIN,DISP=(SHR,KEEP,KEEP)
ASMUT1   DAIRPB DSN='TSO.SYSUT1',DISP=(SHR,KEEP,KEEP),                 X
               CONTROL=(ABKL,UID),SPACE=(3200,100,100),DDNAME=SYSUT1,  X
               VOLUME=WORK
ASMUT2   DAIRPB DSN='TSO.SYSUT2',DISP=(SHR,KEEP,KEEP),                 X
               CONTROL=(ABKL,UID),SPACE=(3200,100,100),DDNAME=SYSUT2,  X
               VOLUME=WORK
ASMUT3   DAIRPB DSN='TSO.SYSUT3',DISP=(SHR,KEEP,KEEP),                 X
               CONTROL=(ABKL,UID),SPACE=(3200,100,100),DDNAME=SYSUT3,  X
               VOLUME=WORK
PRTPB    DAIRPB DDNAME=SYSPRINT,DISP=(SHR,KEEP,KEEP),                  X
               SPACE=(3200,100,100),CONTROL=ABKL,VOLUME=WORK
OBJPB    DAIRPB DDNAME=SYSLIN,DISP=(SHR,KEEP,KEEP),                    X
               SPACE=(3200,100,100),CONTROL=ABKL,VOLUME=WORK
LIBPB    DAIRPB DDNAME=SYSLIB,DISP=(SHR,KEEP,KEEP)
ASMADL   ADL   0,0,0,SYSLIB,SYSIN,SYSPRINT,SYSPUNCH,SYSUT1,            X
               SYSUT2,SYSUT3,SYSLIN
PRTDCB   DCB   DSORG=PS,RECFM=FBM,BLKSIZE=6050,                        X
               DDNAME=SYSPRINT,MACRF=W,LRECL=121
OBJDCB   DCB   DSORG=PS,RECFM=FB,BLKSIZE=3200,                         X
               DDNAME=SYSLIN,MACRF=W,LRECL=80
ASMSDL   SDL   ASMUT1,ASMUT2,ASMUT3
         GENSTR
         END
