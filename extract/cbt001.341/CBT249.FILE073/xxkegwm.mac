*              /* LEVEL=0000                                         */
*PROCESS X(S),A(S),AG,S,STG,LIST,NEST,F(I),LMSG,MAP,ESD,OPT(TIME),OF;
 XXKEGWM: /* GET KEGWORTH DEVICE INFO */
   PROC OPTIONS(REENTRANT) REORDER RETURNS(POINTER);
 %SKIP(2);
 /********************************************************************/
 /*  XXKEGWM CONTAINS THE DEVICE INFORMATION FOR KEGWORTH.           */
 /*  THERE IS ONE ENTRY FOR EACH POSSIBLE DEVICE ADDRESS (IN THE     */
 /* RANGE X000-FFF OR 0-4095 DECIMAL) WHICH IS EITHER:               */
 /*                                                                  */
 /*  0 - THE DEVICE AT THIS ADDRESS IS NOT A DIRECT ACCESS DEVICE    */
 /*      OR THIS ADDRESS DOES NOT HAVE A DEVICE ATTACHED TO IT.      */
 /*                                                                  */
 /*  1 - DEVICE IS 3330 MODEL I.                                     */
 /*  2 - DEVICE IS 3330 MODEL II.                                    */
 /*  3 - DEVICE IS 3350.                                             */
 /*  4 - DEVICE IS 3350-FIXED HEAD.                                  */
 /********************************************************************/
 /*  NEGATIVE NUMBERS MEAN THAT THIS ADDRESS IS THE ALTERNATE PATH   */
 /*  FOR THE DEVICE ADDRESS -(ENTRY).                                */
 /*                                                                  */
 /*  THIS ROUTINE ALLOCATES A BASED VARIABLE AND ASSIGNS TO IT       */
 /*  THE VALUE OF THE KEGWORTH INFORMATION WHICH MAY SUBSEQUENTLY    */
 /*  BE MODIFIED BY THE PARSE ROUTINE.                               */
 /********************************************************************/
 /*   LEVL(0000)  26 JANUARY 1979.                                   */
 /********************************************************************/
 /* WRITTEN BY    JOHN H LISTER,                                     */
 /*               TECHNICAL SERVICES GROUP,                          */
 /*               DATA PROCESSING DEPARTMENT,                        */
 /*               NATIONAL WESTMINSTER BANK,                         */
 /*               GOODMANS FIELDS COMPUTER CENTRE,                   */
 /*               74 ALIE STREET, LONDON   E1 8HL.                   */
 /*                                                                  */
 /*               TELEPHONE: 01-488 0707 EXT 2766.                   */
 /********************************************************************/
 %SKIP(2);
 /* CONSTANTS */
 %CONTROL(NOFORMAT);
 DCL KEGWORTH(0:4095) FIXED BIN(15) STATIC INIT(
 /********************************************************************/
 /*  ENTRIES FOR CHANNEL 0.                                          */
 /********************************************************************/
 (256) 0,    /* X000-0FF */
 /********************************************************************/
 /*  ENTRIES FOR CHANNEL 1.                                          */
 /********************************************************************/
 (48)  0,    /* X100-12F */
 (8)   2,    /* X130-137 */       (8)   0,    /* X138-13F */
 (16)  0,    /* X140-14F */
 (8)   4,    /* X150-157 */       (8)   3,    /* X158-15F */
 (128) 0,    /* X160-1DF */
 (8)   4,    /* X1E0-1E7 */       (8)   3,    /* X1E8-1EF */
 (16)  0,    /* X1F0-1FF */
 /********************************************************************/
 /*  ENTRIES FOR CHANNEL 2.                                          */
 /********************************************************************/
 (64)  0,    /* X200-23F */
 (8)   2,    /* X240-247  */      (8)   0,    /* X248-24F */
  -336,  -337,  -338,  -339,  -340,  -341,  -342,  -343, /* X250-257 */
  -344,  -345,  -346,  -347,  -348,  -349,  -350,  -351, /* X258-25F */
 (128) 0,    /* X260-2DF */
  -480,  -481,  -482,  -483,  -484,  -485,  -486,  -487, /* X2E0-2E7 */
  -488,  -489,  -490,  -491,  -492,  -493,  -494,  -495, /* X2E8-2EF */
 (16)  0,    /* X2F0-2FF */
 /********************************************************************/
 /*  ENTRIES FOR CHANNEL 3.                                          */
 /********************************************************************/
 (64)  0,    /* X300-33F */
 (8)   4,    /* X340-347 */       (8)   0,    /* X348-34F */
 (176) 0,    /* X350-3FF */
 /********************************************************************/
 /*  ENTRIES FOR CHANNEL 4.                                          */
 /********************************************************************/
 (256) 0,    /* X400-4FF */
 /********************************************************************/
 /*  ENTRIES FOR CHANNEL 5.                                          */
 /********************************************************************/
     2,     2,     1,     1,     1,     1,     0,     0, /* X500-507 */
     2,     2,     1,     1,     1,     1,     0,     0, /* X508-50F */
 (8)   2,    /* X510-517 */      (8)    0,    /* X518-51F */
 (8)   2,    /* X520-527 */      (8)    0,    /* X528-52F */
 (16)  0,    /* X530-53F */
     2,     2,     1,     1,     1,     1,     1,     1, /* X540-547 */
     2,     2,     1,     1,     1,     1,     1,     1, /* X548-54F */
 (48)  0,    /* X550-57F */
     2,     2,     1,    +1,     1,     1,     1,     1, /* X580-587 */
     2,     2,     1,    +1,     1,     1,     1,     1, /* X588-58F */
 (48)  0, /* X590-59F */
 (8)   2, /* X5C0-5C7 */
 (8)   0, /* X5C8-5CF */
 (16)  0, /* X5D0-5DF */
 (8)   4, /* X5E0-5E7 */
 (8)   3, /* X5E8-5EF */
 (8)   4, /* X5F0-5F7 */
 (8)   3, /* X5F8-5FF */
 /********************************************************************/
 /*  ENTRIES FOR CHANNEL 6.                                          */
 /********************************************************************/
 -1280, -1281, /* X600-601 */
 -1282, -1283, -1284, -1285, /* X602-605 */
 (2)   0, /* X606-607 */
 -1288, -1289, /* X608-609 */
 -1290, -1291, -1292, -1293, /* X60A-60D */
 (2)   0, /* X60E-60F */
 (48)  0, /* X610-63F */
 -1344, -1345, /* X640-641 */
 -1346, -1347, -1348, -1349, -1350, -1351, /* X642-647 */
 -1352, -1353, /* X648-649 */
 -1354, -1355, -1356, -1357, -1358, -1359, /* X64A-64F */
 (48)  0, /* X650-67F */
 -1408, -1409, /* X680-681 */
 -1410, -1411, -1412, -1413, -1414, -1415, /* X682-687 */
 -1416, -1417, /* X688-689 */
 -1418, -1419, -1420, -1421, -1422, -1423,  /* X68A-68F */
 (48)  0, /* X690-6BF */
 (8)   2, /* X6C0-6C7 */
 (8)   0, /* X6C8-6CF */
 (8)   2, /* X6D0-6D7 */
 (8)   0, /* X6D8-6DF */
 -1504, -1505, -1506, -1507, -1508, -1509, -1510, -1511, /* X6E0-6E7 */
 -1512, -1513, -1514, -1515, -1516, -1517, -1518, -1519, /* X6E8-6EF */
 -1520, -1521, -1522, -1523, -1524, -1525, -1526, -1527, /* X6F0-6F7 */
 -1528, -1529, -1530, -1531, -1532, -1533, -1534, -1535, /* X6F7-6FF */
 /********************************************************************/
 /*  ENTRIES FOR CHANNELS 7 - F ARE 0 AS KEGWORTH 168S DO            */
 /*  NOT HAVE THESE CHANNELS.                                        */
 /********************************************************************/
 (2304) 0);
 %CONTROL(FORMAT);
 %SKIP(2);
 /* BASED VARIABLE TEMPLATES */
   DCL
      DEVICE_ARRAY(0:4095) FIXED BIN(15) BASED(OUTPTR);
 %SKIP(2);
 /* LOCAL VARIABLES */
   DCL
      OUTPTR POINTER;                  /* POINTER VALUE RETURNED IS
                                       THE ADDRESS OF A COPY OF
                                       KEGWORTH */
 %SKIP(2);
      ALLOCATE DEVICE_ARRAY;
      DEVICE_ARRAY=KEGWORTH;
      RETURN(OUTPTR);
   END;
