)ATTR
 ^ TYPE(TEXT) INTENS(LOW) SKIP(ON)
)BODY
%---------- ACCESS METHOD SERVICES (AMS) -- BATCH MODE JCL -------------
+OPTION%===>_Z     +
+
+  ENTER%CANCEL  +TO TERMINATE PROCESSING
+  ENTER%SAVE    +TO SAVE JCL IN DATA SET
+  PRESS%END KEY +TO SUBMIT JOB
+
+  AMS%&AMSFUNC +FUNCTION SELECTED+
+
+
+  SYSOUT CLASS%===>_Z             +
+
+  JOB STATEMENT INFORMATION:  (VERIFY BEFORE PROCEEDING)
+
%  ===>_Z
%  ===>_Z
%  ===>_Z
%  ===>_Z
+
+
+
+
+
)INIT
  &AMSOPT = ' '
  &AMSSOUT = 'A'
  .ZVARS = '(AMSOPT AMSSOUT AMSJ1 AMSJ2 AMSJ3 AMSJ4)'
)PROC
  IF (.RESP = ENTER)
    IF (&AMSOPT = '  ')
      .MSG = AMS013
  IF (.RESP = END)
    IF (&AMSOPT = '  ')
      &AMSOPT = 'END'
  VER (&AMSOPT,NONBLANK)
  VER (&AMSOPT,LIST,CANCEL,SAVE,ENTER)
  VER (&AMSSOUT,NB)
  VER (&AMSJ1,NONBLANK)
  VER (&AMSJ2,NONBLANK)
  VER (&AMSJ3,NONBLANK)
  VER (&AMSJ4,NONBLANK)
)END
