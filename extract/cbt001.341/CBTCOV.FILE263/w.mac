//X75826P1 JOB 10-337-2340-PRO-A0,
// 'SMITH-TCC',CLASS=E,MSGCLASS=1,NOTIFY=X75826
//KILL EXEC SAS  EMPTIES OBJ LIBRARY
//OBJ DD DSN=X75826.ABE.OBJ,DISP=OLD
PROC PDS DDNAME=OBJ NOLIST KILL;
