+      %===>_SP           +
+
+           (STATUS/A, PROCESS/P OR ALL/A)
+
+      %===>_VSB              +
+
+           (VIOLATIONS/V, SUCCESSES/S OR BOTH/B)
+
+DATE OF RACF EVENT:
+
+  FROM%===>_FDATE+  (YYDDD - JULIAN DATE)
+  TO  %===>_TDATE+  (YYDDD - JULIAN DATE)
+
+TIME OF RACF EVENT:
+
+  FROM%===>_FTIME + (HHMMSS - HOURS, MINUTES, SECONDS, INCLUSIVE)
+  TO  %===>_TTIME + (HHMMSS - HOURS, MINUTES, SECONDS, INCLUSIVE)
+
)INIT

 .CURSOR= SP
 &SP  = PROCESS
 &VSB = BOTH
 &FDATE = ' '
 &FTIME = ' '
 &TDATE = ' '
 &TTIME = ' '
 &DT = ' '
 &TM = ' '
 &OP = ' '
 &SORP = ' '

)PROC

 VER (&FDATE,RANGE,81001,99365)
 VER (&TDATE,RANGE,81001,99365)
 VER (&FTIME,RANGE,000000,240000)
 VER (&TTIME,RANGE,000000,240000)

 &VSBT = TRUNC(&VSB,1)
 VER (&VSBT,LIST,V,S,B,MSG=HNBRA611)
 &SPT  = TRUNC(&SP,1)
 VER (&SPT,LIST,S,P,A,MSG=HNBRA612)

 IF  (&FDATE ^= ' ')
     IF  (&TDATE ^= ' ')
         &DT = 'DATE(&FDATE:&TDATE)'
     IF  (&TDATE = ' ')
         &DT = 'DATE(&FDATE)'
 IF  (&FTIME ^= ' ')
     IF  (&TTIME ^= ' ')
         &TM = 'TIME(&FTIME:&TTIME)'
     IF  (&TTIME = ' ')
         &TM = 'TIME(&FTIME)'

 &SORP = TRANS (&SPT A,' ' S,STATUS P,PROCESS)

 &OP = TRANS(&VSBT B,' ' V,VIOLATIONS S,SUCCESSES)

)END
./       ADD   NAME=HNBRA61A
%--------------  RACF REPORT WRITER - RECORD SELECTION (2 OF 2)  -------
%ENTER SELECTION DATA BELOW, ALL FIELDS ARE OPTIONAL+(%PF3+FOR NO SELECT
+
+ENTER SPECIFIC VALUES FOR THE LISTED ITEMS, THE KEYWORD%NONE+MAY BE SPE
+      FOR%GROUP+AND%USER+TO INDICATE THAT RECORDS WHICH ARE MISSING THE
+      FIELDS SHOULD BE SELECTED:
+
+      RACF GROUP NAME  %===>_GLIST
+      RACF USER ID     %===>_ULIST
+      MVS JOB NAME     %===>_JLIST
+      MVS STEP NAME    %===>_SLIST
+      MVS SYSTEM ID    %===>_ILIST
+      VTAM TERMINAL ID %===>_TLIST
+
+LIST SPECIFIC%AUTHORITY LEVELS+TO BE SELECTED:
+      ===>_AUTHS
+          (SPECIAL, OPERATIONS, AUDITOR, EXIT, NORMAL)
+          (SEPARATED BY COMMAS OR BLANKS)
+
+LIST SPECIFIC%LOG REASONS+WHICH SHOULD CAUSE A RECORD TO BE SELECTED:
+      ===>_LOGREAS
+          (CLASS, USER, SPECIAL, RESOURCE, RACINIT, COMMAND, CMDVIOL)
+          (SEPARATED BY COMMAS OR BLANKS)
)INIT

 .CURSOR= GLIST
 &GLIST = ' '
 &ULIST = ' '
 &JLIST = ' '
 &SLIST = ' '
 &ILIST = ' '
 &TLIST = ' '
 &AUTHS = ' '
 &LOGREAS = ' '
 &GR = ' '
 &US = ' '
 &JB = ' '
 &ST = ' '
 &AU = ' '
 &RE = ' '
 &TR = ' '

)PROC

 IF  (&GLIST ^= ' ')
     IF  (&GLIST ^= NONE)
         &GR = 'GROUP(&GLIST)'
     IF  (&GLIST = NONE)
         &GR = ' '
 IF  (&ULIST ^= ' ')
     IF  (&ULIST ^= NONE)
         &US = 'USER(&ULIST)'
     IF  (&ULIST = NONE)
         &US = 'NOUSER'
 IF  (&JLIST ^= ' ')
     IF  (&JLIST ^= NONE)
         &JB = 'JOB(&JLIST)'
     IF  (&JLIST = NONE)
         &JB = 'NOJOB'
 IF  (&SLIST ^= ' ')
     IF  (&SLIST ^= NONE)
         &ST = 'STEP(&SLIST)'
     IF  (&SLIST = NONE)
         &ST = ' '
 IF  (&TLIST ^= ' ')
     IF  (&TLIST ^= NONE)
         &TR = 'TERMINAL(&TLIST)'
     IF  (&TLIST = NONE)
         &TR = ' '
 IF  (&AUTHS ^= ' ')
     &AU = 'AUTHORITY(&AUTHS)'
 IF  (&LOGREAS ^= ' ')
     &RE = 'REASON(&LOGREAS)'

 &SELCMD = 'SELECT &SORP &OP &DT &TM &GR &US &JB &ST &AU &RE &TR'

)END
./       ADD   NAME=HNBRA62
%--------------  RACF REPORT WRITER - EVENT SELECTION (1 OF 2)  --------
%PF1+FOR A MATRIX OF VALID COMBINATIONS,%PF3+TO END SELECT FUNCTION
+
+RACF EVENT NAME %===>_EVENT     +
+
+     VALID EVENTS ARE: LOGON, ACCESS, ADDVOL, RENAME, DELETE, DELVOL, D
+                       ALLSVC, ADDSD, ADDGROUP, ADDUSER, ALTDSD, ALTGRO
+                       ALTUSER, CONNECT, DELDSD, DELGROUP, DELUSER, PAS
+                       PERMIT, RALTER, RDEFINE, RDELETE, REMOVE, RVARY,
+                       SETROPTS, ALLCOMMAND
+
+EVENT QUALIFIERS%===>_EVEQUAL+    (0,1,2,3,4,5 OR BLANK)
+
+
+CLASSES         %===>_CLASSES
+
+    VALID CLASSES ARE: DATASET, DASDVOL, TAPEVOL, JOBS
+
+RESOURCE NAME   %===>_RESNAME
+                      (IE. DATASET NAME, VOLUME SERIAL)
+
+NEW NAME        %===>_NEWRES
+                      (VALID ONLY FOR 'RENAME' EVENT)
)INIT

 .CURSOR  = EVENT
 .HELP    = HNBRA62M
 &EVENT   = ' '
 &EVEQUAL = ' '
 &CLASSES = ' '
 &RESNAME = ' '
 &NEWRES  = ' '

)PROC

 VER (&EVENT,LIST,LOGON,ACCESS,ADDVOL,RENAME,DELETE,DELVOL,DEFINE,
 ALLSVC,ADDSD,ADDGROUP,ADDUSER,ALTDSD,ALTGROUP,ALTUSER,CONNECT,DELDSD,
 DELGROUP,DELUSER,PASSWORD,PERMIT,RALTER,RDEFINE,RDELETE,REMOVE,RVARY,
 SETROPTS,ALLCOMMAND,MSG=HNBRA626)

 VER (&RESNAME,DSNAME)
 IF  (&RESNAME ^= ' ')
     VER (&NEWRES,DSNAME)
 IF  (&NEWRES ^= ' ')
     IF (&EVENT ^= RENAME)
        .MSG = HNBRA621

 IF (&EVEQUAL ^= ' ')
    &EQ = 'EVQUAL(&EVEQUAL)'

 IF (&CLASSES ^= ' ')
    &CL = 'CLASS(&CLASSES)'

 IF (&RESNAME ^= ' ')
    &NM = 'NAME(&RESNAME)'

 IF (&NEWRES ^= ' ')
    &NN = 'NEWNAME(&NEWRES)'

)END
./       ADD   NAME=HNBRA62A
%--------------  RACF REPORT WRITER - EVENT SELECTION (2 OF 2)  --------
%PF1+FOR A MATRIX OF VALID COMBINATIONS
+
+DSNAME LEVEL    %===>_DSQUAL
+                      (FIRST LEVEL DSNAME QUALIFIER OR GROUP NAME)
+
+
+NEW LEVEL       %===>_NEWQUAL
+                      (VALID ONLY WITH%RENAME+EVENT)
+
+
+ACCESS INTENT   %===>_INTENT
+                      (ALTER, CONTROL, UPDATE, READ, NONE)
+                      (VALID ONLY WITH%ACCESS+EVENT)
+
+ACCESS ALLOWED  %===>_ALLOW
+                      (ALTER, CONTROL, UPDATE, READ, NONE)
+                      (VALID ONLY WITH%ACCESS+AND%ADDVOL+EVENTS)
+
+RESOURCE LEVEL  %===>_LEVEL
+      OR RANGE
+                      (ASCENDING LIST OF RESOURCE LEVEL NUMBERS OR)
+                      (A RANGE OF NUMBERS %START:END+             )
+
)INIT

 .CURSOR  = DSQUAL
 .HELP    = HNBRA62M
 &DSQUAL  = ' '
 &NEWQUAL = ' '
 &ACCESS  = ' '
 &ALLOW   = ' '
 &LEVEL   = ' '

)PROC

 VER (&DSQUAL,DSNAME)
 IF  (&NEWQUAL ^= ' ')
     VER (&NEWQUAL,DSNAME)
     IF  (&EVENT ^= RENAME)
     .MSG = HNBRA625

 IF (&INTENT ^= ' ')
    VER (&INTENT,LIST,ALTER,CONTROL,UPDATE,READ,NONE,MSG=HNBRA622)
    IF  (&EVENT ^= ACCESS)
        .MSG = HNBRA623

 IF (&ALLOW ^= ' ')
    VER (&ALLOW,LIST,ALTER,CONTROL,UPDATE,READ,NONE,MSG=HNBRA622)
    IF  (&EVENT ^= ACCESS)
        IF  (&EVENT ^= ADDVOL)
            .MSG = HNBRA624

 IF (&DSQUAL ^= ' ')
    &DS = 'DSQUAL(&DSQUAL)'

 IF (&NEWQUAL ^= ' ')
    &NQ = 'NEWDSQUAL(&NEWQUAL)'

 IF (&INTENT ^= ' ')
    &IN = 'INTENT(&INTENT)'

 IF (&ALLOW ^= ' ')
    &AL = 'ALLOWED(&ALLOW)'

 IF (&LEVEL ^= ' ')
    &LV = 'LEVEL(&LEVEL)'

 &EVECMD = 'EVENT &EVENT &EQ &CL &NM &DS &IN &AL &NN &NQ &LV'

)END
