./       ADD   NAME=HNBRA11
%-----------------------------  ADD A NEW RACF USERID  -----------------
+
+
%ENTER/VERIFY PARAMETERS BELOW:
+
+    BATCH OR TSO?                              %===>_BORT    %
+
+    THE USERID TO BE ADDED                     %===>_USER    %(REQUIRED
+    THE USERID OF THE OWNER OF THIS USERID     %===>_OWNER   %
+    THE USERID'S DEFAULT CONNECT GROUP         %===>_DEFGRP  %
+    THE USERID'S GROUP AUTHORITY LEVEL         %===>_GRPAUTH %
+    UNIVERSAL ACCESS AUTHORITY                 %===>_UACC    %
+    CLASS AUTHORIZATION                        %===>_CLAUTH  %
+
+    THE FOLLOWING FIELDS REQUIRE A%YES+OR%NO+RESPONSE:
+
+    GROUP ACCESS       %===>_GRPACC  +AUTOMATIC PROTECTION     %===>_AD
+    SPECIAL AUTHORITY  %===>_SPEC    +OPERATIONS AUTHORITY     %===>_OP
+    AUDITOR AUTHORITY  %===>_AUDIT   +OIDCARD REQUIRED         %===>_OI
+    USERID MODEL       %===>_MDL+
+
+
+    DEPRESS%ENTER+TO CONTINUE OR%PF3+TO EXIT
+
)INIT

 .CURSOR = BORT

 &BORT     = 'BATCH'
 &PSWD     = 'YES'
 &GRPAUTH  = 'USE'
 &DEFGRP   = 'PREFIX'
 &OWNER    = &ZUSER
 &UACC     = 'NONE'
 &CLAUTH   = 'NONE'
 &GRPACC   = 'NO'
 &ADSP     = 'NO'
 &SPEC     = 'NO'
 &OPS      = 'NO'
 &AUDIT    = 'NO'
 &OID      = 'NO'
 &MDL      = 'NO'

)PROC

 VER (&BORT,NONBLANK)
 VER (&USER,NONBLANK)
 VER (&PSWD,NONBLANK)
 VER (&GRPAUTH,NONBLANK)
 VER (&UACC,NONBLANK)
 VER (&CLAUTH,NONBLANK)
 VER (&GRPACC,NONBLANK)
 VER (&ADSP,NONBLANK)
 VER (&SPEC,NONBLANK)
 VER (&OPS,NONBLANK)
 VER (&AUDIT,NONBLANK)
 VER (&OID,NONBLANK)
 VER (&MDL,NONBLANK)

 &OPT = TRUNC(&BORT,1)
 VER (&OPT,LIST,B,T,S,MSG=HNBRA115)
 IF  (&DEFGRP = 'PREFIX')
     &DEFGRP = TRUNC(&USER,3)
 VER (&GRPAUTH,LIST,USE,CREATE,CONNECT,JOIN,MSG=HNBRA112)
 VER (&UACC,LIST,NONE,READ,UPDATE,CONTROL,ALTER,MSG=HNBRA113)
 VER (&CLAUTH,NAME)
 VER (&GRPACC,LIST,YES,NO,MSG=HNBRA114)
 &GRPACC = TRANS (&GRPACC YES,GRPACC NO,NOGRPACC)
 VER (&ADSP,LIST,YES,NO,MSG=HNBRA114)
 &ADSP = TRANS (&ADSP YES,ADSP NO,NOADSP)
 VER (&SPEC,LIST,YES,NO,MSG=HNBRA114)
 &SPEC = TRANS (&SPEC YES,SPECIAL NO,NOSPECIAL)
 VER (&OPS,LIST,YES,NO,MSG=HNBRA114)
 &OPS  = TRANS (&OPS YES,OPERATIONS NO,NOOPERATIONS)
 VER (&AUDIT,LIST,YES,NO,MSG=HNBRA114)
 &AUDIT= TRANS (&AUDIT YES,AUDITOR NO,NOAUDITOR)
 VER (&OID,LIST,YES,NO,MSG=HNBRA114)
 &OID  = TRANS (&OID YES,OIDCARD NO,NOOIDCARD)
 VER (&MDL,LIST,YES,NO,MSG=HNBRA114)
 &MODEL = TRANS (&MDL YES,'MODEL(''MODEL'')' NO,' ')

)END
