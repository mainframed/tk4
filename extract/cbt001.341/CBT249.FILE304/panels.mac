./       ADD   NAME=HNBRA63
%-----------------  RACF REPORT WRITER - LIST DETAIL RECORDS  ----------
%ENTER/VERIFY PARAMETERS BELOW (%PFK3+TO PROCEED TO NEXT OPTION)
+
+
+    SORT FIELDS      %===>_S1      %===>_S2      %===>_S3      %===>_S4
+
+
+    ASCENDING OR DESCENDING SORT?
+
+                     %===>_AOD              +
+
+
+    PAGE BREAK?      %===>_PAG+   (YES/NO)
+
+    TITLE            %===>_TITLE
+
+
+
+
+    SORT FIELDS ARE: DATE, TIME, SYSID, USER, GROUP, EVENT,
+                     EVQUAL, TYPE, NAME, CLASS, TERMINAL, JOBID
+
+
+
)INIT

 .CURSOR = S1
 &S1     = ' '
 &S2     = ' '
 &S3     = ' '
 &S4     = ' '
 &AOD    = ASCENDING
 &PAG    = NO
 &TITLE  = ' '

)PROC

 VER (&S1,NONBLANK)
 VER (&S1,LIST,DATE,TIME,SYSID,USER,GROUP,EVENT,EVQUAL,TYPE,
                    NAME,CLASS,TERMINAL,JOBID,MSG=HNBRA631)
 VER (&S2,LIST,DATE,TIME,SYSID,USER,GROUP,EVENT,EVQUAL,TYPE,
                    NAME,CLASS,TERMINAL,JOBID,MSG=HNBRA631)
 VER (&S3,LIST,DATE,TIME,SYSID,USER,GROUP,EVENT,EVQUAL,TYPE,
                    NAME,CLASS,TERMINAL,JOBID,MSG=HNBRA631)
 VER (&S4,LIST,DATE,TIME,SYSID,USER,GROUP,EVENT,EVQUAL,TYPE,
                    NAME,CLASS,TERMINAL,JOBID,MSG=HNBRA631)
 &AODT = TRUNC(&AOD,1)
 VER (&AODT,LIST,A,D,MSG=HNBRA632)
 &AO = TRANS(&AODT A,ASCEND D,DESCEND)
 VER (&PAG,LIST,YES,NO,MSG=HNBRA633)

 &SORT = 'SORT(&S1 &S2 &S3 &S4)'

 &PAGE   = TRANS(&PAG YES,NEWPAGE NO,' ')

 IF  (&TITLE ^= ' ')
     &TTL = 'TITLE(''&TITLE'')'

 &LSTCMD = 'LIST &SORT &AO &PAGE &TTL'

)END
./       ADD   NAME=HNBRA64
%------------------  RACF REPORT WRITER - SUMMARY REPORTS  -------------
%ENTER/VERIFY PARAMETERS BELOW (%PFK3+TO PROCEED TO CONSTRUCT REPORTS)
+
+
+    MAJOR FIELD     %===>_NAME1                                       +
+
+    'BY' FIELD      %===>_NAME2
+
+    VIOLATIONS/V, SUCCESSES/S, OR BOTH/B:
+
+                    %===>_VSB              +
+
+    PAGE BREAK?     %===>_PAG+   (YES/NO)
+
+    TITLE           %===>_TITLE
+
+
+
+    FIELD NAMES ARE: GROUP, USER, RESOURCE, EVENT AND COMMAND
+
+
+
+
)INIT

 .CURSOR= NAME1
 &NAME1 = ' '
 &NAME2 = ' '
 &VSB   = BOTH
 &PAG   = NO
 &TITLE = ' '

)PROC

 VER (&NAME1,NONBLANK)
 VER (&NAME1,LIST,GROUP,USER,RESOURCE,EVENT,COMMAND,MSG=HNBRA641)
 IF  (&NAME2 ^= ' ')
     VER (&NAME2,LIST,GROUP,USER,RESOURCE,EVENT,COMMAND,MSG=HNBRA641)
 &VSBT = TRUNC(&VSB,1)
 VER (&VSBT,LIST,V,S,B,BOTH,MSG=HNBRA642)
 VER (&PAG,LIST,YES,NO,MSG=HNBRA643)

 IF  (&NAME2 ^= ' ')
     IF (&NAME1 = GROUP)
        VER (&NAME2,LIST,RESOURCE,MSG=HNBRA644)
     IF (&NAME1 = USER)
        VER (&NAME2,LIST,RESOURCE,MSG=HNBRA644)
     IF (&NAME1 = RESOURCE)
        VER (&NAME2,LIST,USER,GROUP,EVENT,MSG=HNBRA645)
     IF (&NAME1 = EVENT)
        VER (&NAME2,LIST,RESOURCE,MSG=HNBRA644)
     IF (&NAME1 = COMMAND)
        VER (&NAME2,LIST,USER,GROUP,RESOURCE,MSG=HNBRA646)
     &BYFIELD = 'BY(&NAME2)'

 &OPTION = TRANS(&VSB B,' ' V,VIOLATIONS S,SUCCESSES)

 &PAGE   = TRANS(&PAG YES,NEWPAGE NO,' ')

 IF  (&TITLE ^= ' ')
     &TTL = 'TITLE(''&TITLE'')'

 &SUMCMD = 'SUMMARY &NAME1 &BYFIELD &OPTION &PAGE &TTL'

)END
