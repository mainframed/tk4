 /* SVC99 INTERFACE - WRITTEN 10/25/80 BY PAUL DIXON */;
 %DCL (@9DSORG_TR,@9DSORG_VSAM,@9DSORG_TQ,
       @9DSORG_TX,@9DSORG_GS,@9DSORG_PO,@9DSORG_POU,
       @9DSORG_MQ,@9DSORG_CQ,@9DSORG_CX,@9DSORG_DA,@9DSORG_DAU,
       @9DSORG_PS,@9DSORG_PSU) CHAR;
 %@9DSORG_TR    = '''0000000000000100''B';
 %@9DSORG_VSAM  = '''0000000000001000''B';
 %@9DSORG_TQ    = '''0000000000100000''B';
 %@9DSORG_TX    = '''0000000001000000''B';
 %@9DSORG_GS    = '''0000000010000000''B';
 %@9DSORG_PO    = '''0000001000000000''B';
 %@9DSORG_POU   = '''0000001100000000''B';
 %@9DSORG_MQ    = '''0000010000000000''B';
 %@9DSORG_CQ    = '''0000100000000000''B';
 %@9DSORG_CX    = '''0001000000000000''B';
 %@9DSORG_DA    = '''0010000000000000''B';
 %@9DSORG_DAU   = '''0010000100000000''B';
 %@9DSORG_PS    = '''0100000000000000''B';
 %@9DSORG_PSU   = '''0100000100000000''B';
 %@9DSORG: PROC(DSORG) RETURNS(CHAR) STATEMENT;
 DCL (DSORG,R1) CHAR;
 IF SUBSTR(DSORG,1,1) = ''''
    THEN
       DO;
          R1 = @9DSORG_PS;
          IF      DSORG  = '''TR'''   THEN R1 = @9DSORG_TR;
          ELSE IF DSORG  = '''VSAM''' THEN R1 = @9DSORG_VSAM;
          ELSE IF DSORG  = '''TQ'''   THEN R1 = @9DSORG_TQ;
          ELSE IF DSORG  = '''TX'''   THEN R1 = @9DSORG_TX;
          ELSE IF DSORG  = '''GS'''   THEN R1 = @9DSORG_GS;
          ELSE IF DSORG  = '''PO'''   THEN R1 = @9DSORG_PO;
          ELSE IF DSORG  = '''POU'''  THEN R1 = @9DSORG_POU;
          ELSE IF DSORG  = '''MQ'''   THEN R1 = @9DSORG_MQ;
          ELSE IF DSORG  = '''CQ'''   THEN R1 = @9DSORG_CQ;
          ELSE IF DSORG  = '''CX'''   THEN R1 = @9DSORG_CX;
          ELSE IF DSORG  = '''DA'''   THEN R1 = @9DSORG_DA;
          ELSE IF DSORG  = '''DAU'''  THEN R1 = @9DSORG_DAU;
          ELSE IF DSORG  = '''PSU'''  THEN R1 = @9DSORG_PSU;
          ELSE IF DSORG ^= '''PS'''   THEN
                  NOTE('INVALID DSORG ' ×× DSORG,8);
          RETURN ('DO; @9_DSORG.@9P  = ' ×× R1 ××
             '; @9I = @9I + 1; @9TUPL(@9I) = ADDR(@9_DSORG); END;');
       END;
    ELSE
       RETURN('DO; @9_DSORG.@9P  = ' ×× DSORG ×× '; @9I = @9I + 1; @9TUP
 L(@9I) = ADDR(@9_DSORG); END;');
 %END @9DSORG;
 %ACTIVATE @9DSORG;
 DCL 01 @9_DSORG UNALIGNED, 05 @9K FIXED BIN(15) INIT(60), 05 @9# FIXED
     BIN(15) INIT(1), 05 @9L FIXED BIN(15) INIT(2), 05 @9P BIT(16);
