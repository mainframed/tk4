 DENTTS:  PROC (RHPPTR,PART,SUBENT) OPTIONS(REENTRANT);
 /* THIS SUBROUTINE IS CALLED TO RESET THE LOGICAL TABS. */
 % DCL @PROCNAME CHAR;
 % @PROCNAME = '';
 % INCLUDE DENTSUB;
   DCL RHPPTR PTR;
   DCL PART(16) CHAR(256) VAR;
   IF PART(1) = 'TABK'
     THEN TABK = PART(2);
     ELSE DO;
       IF TABK = ''
         THEN TABK = 'Ö';
       DO XI = 2 TO 16;
         TABA(XI-1)=PART(XI);
         END;
       END;
   CALL #TABDISP(RHPPTR); /* DENTTD - TABDISP */
   END DENTTS;
