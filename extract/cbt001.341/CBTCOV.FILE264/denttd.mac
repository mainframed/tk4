 DENTTD:  PROC (RHPPTR) OPTIONS(REENTRANT);
 /* THIS SUBROUTINE IS CALLED TO RESET THE LOGICAL TABS. */
 % DCL @PROCNAME CHAR;
 % @PROCNAME = 'MAIN';
 % INCLUDE DENTSUB;
 % INCLUDE DENTPNL;
   DCL RHPPTR PTR;
   DYNPTR=RHPDYNPT;
   OUTFLAG(TABNUM) = '1'B;
   DFLDPTR = TABPTR;
   K=RHPMAXLN+1;
   DATA=REPEAT(' ',LEN-1);
   DO I=1 TO 15;
     IF TABA(I)^=0
       THEN IF TABA(I)+K <= LENGTH(DATA)
         THEN SUBSTR(DATA,TABA(I)+K,1)=TABK;
     END;
   IF RH_RB+K <= LENGTH(DATA)
     THEN SUBSTR(DATA,RH_RB+K,1)='R';
   IF RH_LB+K <= LENGTH(DATA)
     THEN SUBSTR(DATA,RH_LB+K,1)='L';
   IF RHPFILL+K <= LENGTH(DATA)
     THEN SUBSTR(DATA,RHPFILL+K,1)='F';
   END DENTTD;
