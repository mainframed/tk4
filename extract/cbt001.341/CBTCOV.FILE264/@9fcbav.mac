 /* SVC99 INTERFACE - WRITTEN 10/25/80 BY PAUL DIXON */;
 %DCL (@9FCBAV_VERIFY,@9FCBAV_ALIGN) CHAR;
 %FCBAV_VERIFY = '''00000100''B';
 %FCBAV_ALIGN  = '''00001000''B';
 %@9FCBAV: PROC(FCBAV) RETURNS(CHAR) STATEMENT;
 DCL (FCBAV,R1) CHAR;
 IF SUBSTR(FCBAV,1,1) = ''''
    THEN
       DO;
          R1 = FCBAV_ALIGN;
          IF      FCBAV  = '''VERIFY''' THEN R1 = FCBAV_VERIFY;
          ELSE IF FCBAV ^= '''ALIGN''   THEN
                  NOTE('INVALID FCBAV ' ×× FCBAV,8);
          RETURN ('DO; @9_FCBAV.@9P  = ' ×× R1 ××
             '; @9I = @9I + 1; @9TUPL(@9I) = ADDR(@9_FCBAV); END;');
       END;
    ELSE
       RETURN('DO; @9_FCBAV.@9P  = ' ×× FCBAV ×× '; @9I = @9I + 1; @9TUP
 L(@9I) = ADDR(@9_FCBAV); END;');
 %END @9FCBAV;
 %ACTIVATE @9FCBAV;
 DCL 01 @9_FCBAV UNALIGNED, 05 @9K FIXED BIN(15) INIT(38), 05 @9# FIXED
     BIN(15) INIT(1), 05 @9L FIXED BIN(15) INIT(1), 05 @9P  BIT(8);
