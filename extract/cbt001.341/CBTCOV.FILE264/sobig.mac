 /* SOBIG */
         % INCLUDE CPINIT;
         DCL 1 CVT BASED(CVTPTR) UNALIGNED,
              2 PAD CHAR(312),
              2 CVTEORM FIXED BIN(31,0);
         DCL CVTPTR PTR BASED(CVTPTRPTR);
         DCL CVTPTRPTR PTR,
             CVTADJ FIXED BIN(31,0) DEF CVTPTRPTR;
         CVTADJ = 16;
         @SETL(&CPUT = CVTEORM/1024);
 @WRITE('DATE=&SYSDATE, TIME=&SYSTIME, REAL CORE =  &CPUT.K');
         END;
