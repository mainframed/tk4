 /*NEWDSN*/
         % INCLUDE CPINIT1;
         % @CP = 'NEWDSN';
         % INCLUDE CPINIT2;
         @PROC(1,DSNAME,PGMR(#(NUMERIC)),DIV(#(NUMERIC)),
           DEPT(#(NUMERIC)),CHANGE,DELETE,SIGN(S),TRK(#(NUMERIC)),
           NEWVOLS(),VOLUME(),
   FREQ#(OFFLINE,RESIDENT,DAILY,WEEKLY,MONTHLY,REQUEST,LOCAL,REMOTE),
           COMMENTS,GDG(#(NUMERIC)));
         % INCLUDE TGSINITT;
         IF SYMA('&DEPT')=0 × SYMA('&DIV')=0
              THEN IF(^(SYMA('&DEPT')=0 & SYMA('&DIV')=0 &
                SYMA('&PGMR')=0 & SYMA('&TRK')=0))
                   THEN DO;
                        CALL MSGPROC(20);
                        RETURN;
                        END;
         % INCLUDE TGSINIT;
         % INCLUDE TGSINITQ;
         DCL PLIXOPT CHAR(15) VAR INIT('ISA(30K)') STATIC EXT;
         DCL DATE BUILTIN;
         DCL RECSTR CHAR(110);
         DSNRPTR = ADDR(RECSTR);
         RECSTR = LOW(110);
         DCOMPT = 0;
         DNDUPPT,DPDUPPT = 0;
         DCL NVSTR CHAR(6) INIT(' ');
         DCL MYDTE CHAR(6);
         DCL MYSIGN CHAR(1);
         DCL WRKF FIXED BIN(15,0) BASED(WRKPTR);
         DSN=SYMC('&DSNAME');
         DPGMRNO=SYMC('&PGMR');
         DDIV=SYMC('&DIV');
         DDEP=SYMC('&DEPT');
         DTRKS=SYMC('&TRK');
         DGDGCNT=SYMC('&GDG');
         IF DGDGCNT ^= 0
              THEN DO;
                   SUBSTR(DFLGS,11,1) = '1'B;
                   DTRKS = DTRKS * DGDGCNT;
                   END;
              ELSE DGDGCNT = 0;
         IF FREQ > 0
              THEN SUBSTR(DFLGS,FREQ,1) = '1'B;
         IF VOLUME ^= 0
              THEN DO;
                   NEWVOL=SYMC('&VOLUME');
                   END;
              ELSE NEWVOL = LOW(6);
         IF NEWVOLS ^= 0
              THEN DO;
                   NVSTR=SYMC('&NEWVOLS');
                   IF CHANGE ^= 0
                        THEN SUBSTR(DFLGS,10,1) = '1'B;
                   END;
         IF COMMENTS ^= 0
              THEN SUBSTR(DFLGS,9,1) = '1'B;
         IF DELETE ^= 0
              THEN DO;
                   CALL DSNDEL(DSN,NEWVOL);
                   GO TO WRITQCR;
                   END;
         IF CHANGE ^= 0
              THEN DO;
                   MYSIGN = SYMC('&SIGN');
                   CALL DSNCHG(DSNRPTR,NVSTR,MYSIGN);
                   GO TO WRITQCR;
                   END;
         MYDTE = DATE;
         MYDTE = SUBSTR(MYDTE,3,4) ×× SUBSTR(MYDTE,1,2);
         CALL GREGJUL(MYDTE,DDTE);
         DPCAL,DPCUS=0;
         DRFDY='0'B;
         CALL DSNADD(DSNRPTR);
         GO TO WRITQCR;
         END;
