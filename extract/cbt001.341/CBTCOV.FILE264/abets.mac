 /**********************************************************************
 *                                                                     *
 * ABETS IS CALLED TO RESET THE LOGICAL TABS.  ALIAS IS #TABSET.       *
 *                                                                     *
 **********************************************************************/
   %INCLUDE ABESUB;
 ABESUB(ABETS) PARMS(RHPPTR,PART,RHHDRPT);
   %INCLUDE ABESUBA;
   DCL PART(16) CHAR(256) VAR;
   IF PART(1) = 'TABK'
     THEN TABK = PART(2);
     ELSE DO;
       IF TABK = ''
         THEN TABK = 'Ö';
       DO XI = 2 TO 16;
         TABA(XI-1)=PART(XI);
         END;
       END;
   CALL #TABDISP(RHPPTR); /* ABETD - TABDISP */
   END ABETS;
