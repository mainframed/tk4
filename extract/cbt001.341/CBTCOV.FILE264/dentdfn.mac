 /* DENTDFN - SUBENT(36) */
 /* THIS SUBROUTINE IS CALLED TO ASSIGN A DEFAULT SYMBOLIC NAME TO
 THE DATA SET POINTED TO BY RHPTR */
 DENTDFN:  PROC(RHPTR,RHHDRPT) OPTIONS(REENTRANT);
   % DCL @PROCNAME CHAR;
   % @PROCNAME = 'MAIN';
   % INCLUDE DENTSUB;
   DCL NULL BUILTIN;
   DCL PIC PIC'9999999';
   IF OUTDDN=NULL
     THEN DO;
       PIC=RHUNQNO;
       RHUNQNO=RHUNQNO+1;
       RHNAME='T'××PIC;
       END;
     ELSE DO;
       IF OUTDDN->ALCMEM^=''
         THEN RHNAME=OUTDDN->ALCMEM;
         ELSE DO;
           K,N=0;
           DO M=K REPEAT K;
             K=INDEX(SUBSTR(OUTDDN->ALCDSN,M+1),'.')
               +M;
             IF K^=M
               THEN N=M;
               ELSE LEAVE;
             END;
           IF K=0 /* NAME IS UNQUALIFIED */
             THEN RHNAME=OUTDDN->ALCDSN;
             ELSE IF N=0 /* NAME HAS ONLY ONE QUALIFIER */
               THEN RHNAME=SUBSTR(
                 OUTDDN->ALCDSN,K+1);
               ELSE RHNAME=SUBSTR(
                 OUTDDN->ALCDSN,N+1,
                 K-N-1); /* USE 2D LAST QUALIFIER */
           END;
       END;
   END DENTDFN;
