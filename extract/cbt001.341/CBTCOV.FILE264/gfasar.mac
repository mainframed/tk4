  GFASAR:  PROC(PRMPTR,I,S) RETURNS(PTR);
    /* WILL RETURN PTR TO FIRST ASARELS IN DIMENSION.  S IS BIT(1), AND
    IS SET OFF BY THE CALLER IF THE FIRST ELEMENT NOT AN INDEX ELEMENT
    IS DESIRED. */
    DCL PRMPTR PTR; /* PTR TO ASARHDR */
    DCL I FIXED BIN(15,0); /* DESIRED DIMENSION */
    DCL S BIT(1); /* TYPE OF ASARELS WANTED INDICATOR */
    % INCLUDE ASARDCL;
    ASARHP=PRMPTR;
    DCL TPT PTR;
    IF I = 1
      THEN TPT=ASARFIRST;
      ELSE TPT=ASARDPT(I-1)->ASAREL; /* GET 1ST ASARELS IN THIS DIM */
    IF ^S
      THEN DO TPT=TPT REPEAT TPT->ASAREL
        WHILE(TPT^=NULL) UNTIL(^SUBSTR(TPT->ASARELFL,1,1));
        END;
    RETURN(TPT);
    END GFASAR;
