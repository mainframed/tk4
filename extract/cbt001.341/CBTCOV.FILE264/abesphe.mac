   %INCLUDE ABESUB;
   ABESUB(ABESPHE);
   %INCLUDE ABESUBA;
   %INCLUDE ABE79ENT;
   DCL (DP,DT,EF(61,120),PF,PH,T(3,3),TF,TH) FLOAT;

 AS: PROC(JARG,IARG) RETURNS(FLOAT);
   DCL (JARG,IARG) FIXED BIN;
   RETURN(0.1*FLOAT(MOD(IARG+JARG,2)));
   END AS;

   NT,MT=31;
   NP,MP=60;
   EF(*)=0;
   T(*)=0;
   CALL #PLTEU(T(1,1),FLOAT(143.13),FLOAT(65.91),FLOAT(39.23));
   DT=3.14159/FLOAT(NT-1);
   DP=6.28318/FLOAT(NP);
   TH=0;
   DO I=1 TO NT;
     PH=0.45;
     DO J=1 TO NP;
       TF=.33*(2-COS(2*TH));
       PF=.85+.15*SIN(7*PH);
       EF(I,J)=0.90*TF*PF+0.5*AS(J,I);
       PH=PH+DP;
       END;
     TH=TH+DT;
     END;
   CALL #PLTSQ;
   CALL #HIDSV(EF,MP,MT,NP,NT,FLOAT(1),T(1,1),#VISSP,#PL2PO);
   END ABESPHE;
