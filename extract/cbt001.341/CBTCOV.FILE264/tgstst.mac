  /*TGSTST*/
 TGSTST: PROC OPTIONS(MAIN);
         DCL PLIRETC ENTRY (FIXED BIN(31,0));
         % INCLUDE TGSQCR;
         % INCLUDE TGSBDAM;
         DCL BDAM PTR;
         DCL QCRMAIN CHAR(110) EXT;
         QC = ADDR(QCRMAIN);
         OPEN FILE(BDAMFLE) INPUT DIRECT KEYED;
         BDAMKEYA = 1;
         READ FILE(BDAMFLE) INTO(QCRMAIN) KEY(BDAMKEY);
         IF SUBSTR(QCRFLGS,1,1)
              THEN DO;
                   PUT EDIT(
 'CATASTROPHIC ERROR - RESTORE PROCEDURE MUST BE RUN')(A) SKIP(1);
                   CALL PLIRETC(16);
                   END;
         END;
