 /* TITLECP */
   %INCLUDE CPINIT;
   @PROC(1,LIBRARY#(DSNAME));
   @SETDSN(LIBRARY,,PROMPT);
   @ALLOC(DA,OLD);

   DCL (PREFIX,SUFFIX) ENTRY(CHAR(*),CHAR(*))
     RETURNS(FIXED BIN(15,0));
   DCL GETDIR ENTRY(CHAR(8))
     RETURNS(CHAR(8));
   DCL PDS CHAR(44) VAR;
   DCL MEM CHAR(8) VAR;

   PDS=SUBSTR(TMPDSN@.DSNSTR,1,SUFFIX(TMPDSN@.DSNSTR,' '));
   @SETL(&PDS = PDS);
   @CMD('FREE FI(IN)');
   DO MEM=GETDIR(DA08DDN) REPEAT GETDIR(DA08DDN)
     WHILE(MEM^=HIGH(8));
     MEM=SUBSTR(MEM,1,SUFFIX(MEM,' '));
     @SETL(&MEM = MEM);
     @WRITE('PROCESSING &PDS(&MEM)');
     @CMD('ALLOC FI(OUT) DA(''&PDS(&MEM)'') REUSE');
     @CMD('CALL ''X.IS.X000.LOADLIB(TITLE)''');
     END;
   @WRITE('TITLE PROCESSING FINISHED');
   END;
