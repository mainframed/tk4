 /* SVC99 INTERFACE - WRITTEN 10/25/80 BY PAUL DIXON */;
 %@9SYSOUT: PROC(SYSOUT) RETURNS(CHAR) STATEMENT;
 DCL SYSOUT CHAR;
 IF SYSOUT = '*'
    THEN RETURN('DO; @9I = @9I + 1; @9TUPL(@9I) = ADDR(@9_SYSOUD); END;'
                );
    ELSE RETURN('DO; @9_SYSOUT.@9P  = ' ×× SYSOUT ××
        '; @9I = @9I + 1; @9TUPL(@9I) = ADDR(@9_SYSOUT); END;');
 %END @9SYSOUT;
 %ACTIVATE @9SYSOUT;
 DCL 01 @9_SYSOUT UNALIGNED, 05 @9K FIXED BIN(15) INIT(24), 05 @9# FIXED
     BIN(15) INIT(1), 05 @9L FIXED BIN(15) INIT(1), 05 @9P CHAR(1);
 DCL 01 @9_SYSOUD UNALIGNED, 05 @9K FIXED BIN(15) INIT(24), 05 @9# FIXED
     BIN(15) INIT(0);
