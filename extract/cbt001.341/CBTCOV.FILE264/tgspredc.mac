 /* COMMON LOGIC FOR ENQUEUING ON A DOUBLY LINKED LIST
 PRENPT - NEXT ON Q FIELD OF RECORD TO BE ADDED
 PREPPT - PREVIOUS ON Q FIELD OF RECORD TO BE ADDED
 PRERUT - START OF Q PTR FIELD
 PREDAM - NAME OF FIELD CONTAINING BLK ADDR OF CURRENT RECORD
 PREKEY - NAME OF KEY FIELD
 PRENQD - LABEL TO BRANCH TO ON A DUPLICATE ADD
 */
         % DCL (PRENPT,PREPPT,PRERUT,PREDAM,PREKEY,PRENQD) CHAR;
         %DCL PRELNO FIXED;
         % PRELNO = 0;
         % DCL (PREQCR,PREFNDSL,PRENXT,PREENSL,PREGTSL,PREWREN) CHAR;
         % DCL PRECHA CHAR;
         % PRENQD = '';
