 NEWVOLP: PROC(CPPL1,CPPL2,CPPL3,CPPL4) OPTIONS(MAIN);
         % INCLUDE TGSINIT;
         DCL VOLRECS CHAR(110);
         VOLRPTR = ADDR(VOLRECS);
         VOLRECS = LOW(110);
         SUBSTR(VCOMFLG,1,1) = '1'B;
         DCL BDAMRECT CHAR(110);
         DSNRPTR = ADDR(BDAMRECT);
         DCL CHANGEB FIXED BIN(15,0) BASED(CPTR);
         CPTR = ADDR(CHANGE);
         DCL NEWFLGS BIT(6);
         DCL 1 DEVDATA(3),
              2 DEVID CHAR(6) INIT('2314','3330','3330-1'),
              2 TDNTRK FIXED BIN(15,0) INIT(4060,7676,15352);
                   /* # TRKS AVAILABLE */
         PDEPTR = ADDR(PNEWVL);
         WRKPTR = PDEIKPT;
         VOLSER = SUBSTR(WRKSTR,1,PDEIKL);
         IF   CHANGEB = 3 /* 3 = DELETE */
              THEN DO;
                   CALL DELVOL(VOLSER);
                   GO TO WRITQCR;
                   END;
         ISAMKEY = VOLSER;
         FUNCTION = 'READ';
         CALL ISAMIO(ISAMWRK,ISAMIN);
         IF ERRCODE = '0'B
              THEN DO;
                   IF CHANGEB = 2 /* 2=ADD */
                        THEN GO TO WRITQCR;
                   BDAMKEYA = ISAM.DAMASTER;
                   READ FILE(BDAMFLE) INTO(VOLRECS) KEY(BDAMKEY);
                   GO TO DOCHNGE;
                   END;
              ELSE DO;
                   IF CHANGEB = 1 /* 1=CHANGE */
                        THEN DO;
                             CALL MSGPROC(7);
                        GO TO WRITQCR;
                             END;
                   IF OLDVOL ^= LOW(2)
                        THEN DO;
                             PDEPTR = ADDR(OLDVOLI);
                             WRKPTR = PDEIKPT;
                             ISAMKEY = SUBSTR(WRKSTR,1,PDEIKL);
                             FUNCTION = 'READ';
                             CALL ISAMIO(ISAMWRK,ISAMIN);
                             IF ERRCODE ^= '0'B
                                  THEN DO;
                                       CALL MSGPROC(7);
                                       GO TO WRITQCR;
                                       END;
                             BDAMKEYA = ISAM.DAMASTER;
                             FUNCTION = 'DELET';
                             CALL ISAMIO(ISAMWRK,ISAMIN);
                             READ FILE(BDAMFLE) INTO(VOLRECS)
                               KEY(BDAMKEY);
         % INCLUDE TGSPREDC;
         % PRENPT = 'VOLNPT';
         % PREPPT = 'VOLPPT';
         % PRERUT = 'DAFSTVL';
         % INCLUDE TGSDQC;
                             PDEPTR = ADDR(PNEWVL);
                             WRKPTR = PDEIKPT;
                             VOLSER = SUBSTR(WRKSTR,1,PDEIKL);
                             BDAMKEYA = DSNFPT;
                             DO WHILE(BDAMKEYA ^= 0);
                                  READ FILE(BDAMFLE) INTO(BDAMRECT)
                                    KEY(BDAMKEY);
                                       IF DNDUPPT ^= 0 ×
                                    DPDUPPT ^= 0
                                       THEN DO;
         % PRENPT = 'DNDUPPT';
         % PREPPT = 'DPDUPPT';
         %PRERUT = 'ISAM.DAMASTER';
         % PREKEY = 'DSN';
         % INCLUDE TGSDQC;
                                       END;
                                       NEWVOL = VOLSER;
                                       IF DNDUPPT ^= 0 ×
                                    DPDUPPT ^= 0
                                       THEN DO;
                                                 FUNCTION = 'READ';
                                                 ISAMKEY = DSN;
                                                 CALL ISAMIO(ISAMWRK,
                                                   ISAMIN);
         % PREDAM = 'DSNREC.DAMASTER';
         % PREKEY = 'NEWVOL';
         % INCLUDE TGSNQC;
                                                 END;
                                  BDAMKEYA = DSNREC.DAMASTER;
                                  WRITE FILE(BDAMFLE) FROM(BDAMRECT)
                                    KEYFROM(BDAMKEY);
                                  END;
                             GO TO DOCHNGE;
                             END;
                   END;
         VOLREC.DAMASTER = DALFREE;
         RETURN = GETRET;
         GO TO GETFREE;
 GETRET:
         VOLPPT,VOLNPT,DSNFPT,TTRKS,FTRKS = 0;
         VOLFLGS = '0'B;
         PDEPTR = ADDR(TYPESUBI);
         WRKPTR = PDEIKPT;
         DO I = 1 TO 3;
              IF DEVID(I) = SUBSTR(WRKSTR,1,PDEIKL)
                   THEN GO TO GOTID;
              END;
         CALL MSGPROC(12);
         I = 2;
 GOTID:
         SUBSTR(VOLFLGS,8+I,1) = '1'B;
         IF TRK = LOW(2)
              THEN TTRKS,FTRKS = TDNTRK(I);
              ELSE DO;
                   PDEPTR = ADDR(TRKSUBI);
                   WRKPTR = PDEIKPT;
                   TTRKS = SUBSTR(WRKSTR,1,PDEIKL);
                   FTRKS = TTRKS;
                   END;
 DOFREQ:
         NEWFLGS = '0'B;
         IF SPECIFIC ^= LOW(2)
              THEN SUBSTR(NEWFLGS,1,1) = '1'B;
         IF RESIDENT ^= LOW(2)
              THEN SUBSTR(NEWFLGS,2,1) = '1'B;
         IF DAILY ^= LOW(2)
              THEN SUBSTR(NEWFLGS,3,1) = '1'B;
         IF WEEKLY ^= LOW(2)
              THEN SUBSTR(NEWFLGS,4,1) = '1'B;
         IF MONTHLY ^= LOW(2)
              THEN SUBSTR(NEWFLGS,5,1) = '1'B;
         IF REQUEST ^= LOW(2)
              THEN SUBSTR(NEWFLGS,6,1) = '1'B;
         IF NEWFLGS ^= '0'B
              THEN SUBSTR(VOLFLGS,1,6) = NEWFLGS;
         BDAMKEYA = VOLREC.DAMASTER;
         IF CHANGEB=2 /*2=ADD*/ OR OLDVOL ^= LOW(2)
              THEN CALL ADDVOL(VOLRPTR);
              ELSE WRITE FILE(BDAMFLE) FROM(VOLRECS) KEYFROM(BDAMKEY);
         GO TO WRITQCR;
 DOCHNGE:
         IF TRK ^= LOW(2)
              THEN DO;
                   PDEPTR = ADDR(TRKSUBI);
                   WRKPTR = PDEIKPT;
                   I = SUBSTR(WRKSTR,1,PDEIKL);
                   J = FTRKS + I - TTRKS;
              IF J < 0
                        THEN DO;
                             CALL MSGPROC(13);
                             GO TO DOFREQ;
                             END;
                   FTRKS = J;
                   TTRKS = I;
                   END;
         GO TO DOFREQ;
         END;
