 DENTSPF: PROC(RHPTR,SUBENT,RHHDRPT,CPPLCBUF,CPPLUPT,CPPLPSCB,CPPLECT)
   OPTIONS(REENTRANT);
   DCL (CPPLCBUF,CPPLUPT,CPPLPSCB,CPPLECT) PTR;
   % DCL @PROCNAME CHAR;
   % @PROCNAME='';
   % INCLUDE DENTSUB;
   DCL 1 CBUF BASED(CPPLCBUF),
      2 BUFLEN FIXED BIN(15,0),
      2 OFFSET FIXED BIN(15,0),
      2 BUFFER CHAR(4);
   DCL PLIXOPT CHAR(80) VAR INIT('ISA(1500K) ') STATIC EXT;
   DCL (DENTA,DENTIN) ENTRY;
   DCL PLILOAD ENTRY(CHAR(8),ENTRY) RETURNS(FIXED BIN(15,0));
   I=PLILOAD('ISPLINK',SUBENT(50));
   CBUF.BUFFER='SSE ';
   CALL DENTIN(RHPTR,SUBENT,RHHDRPT,CPPLCBUF,CPPLUPT,CPPLPSCB,CPPLECT);
   CALL DENTA(SUBENT,RHHDRPT,CPPLECT);
   CALL #ISPLINK('CONTROL ','DISPLAY ','REFRESH ');
   END;
