 /*TGSNQA*/
 TGSNQ:  PROC(DSNRPTR,VOLRPTR,ISAMIN,PRMCODE);
         % INCLUDE TGSINITA;
         DCL PRMCODE FIXED BIN(31,0);
         PRMCODE = 0B;
         % INCLUDE TGSPREDC;
         % PRENPT = 'DNDSNPT';
         % PREPPT = 'DPDSNPT';
         % PRERUT = 'DSNFPT';
         % PREDAM = 'DSNREC.DAMASTER';
         % PREKEY = 'DSN';
         % PRENQD = 'DUPADD';
         % INCLUDE TGSNQC;
         FTRKS = FTRKS - DTRKS;
         BDAMKEYA = VOLREC.DAMASTER;
         WRITE FILE(BDAMFLE) FROM(VOLREC) KEYFROM(BDAMKEY);
         DNDUPPT,DPDUPPT = 0;
         IF ISAM.DAMASTER = DSNREC.DAMASTER
              THEN RETURN; /* IT IS FIRST FOR DSNAME */
         DCL SAVKEY FIXED DEC(9,0);
         SAVKEY = ISAM.DAMASTER;
         % PRENPT = 'DNDUPPT';
         % PREPPT = 'DPDUPPT';
         % PRERUT = 'ISAM.DAMASTER';
         % PREDAM = 'DSNREC.DAMASTER';
         % PREKEY = 'NEWVOL';
         % INCLUDE TGSNQC;
         RETURN;
 DUPADD:
         PRMCODE = 4;
         CALL MSGPROC(1); /* DUPLICATE ADD MESSAGE */
         END;
