//EUP027SR JOB (10-096-MISC-027-B4,1),
// 'SMITH-NORTH BLDG',CLASS=E
/*ROUTE  PRINT REMOTE15
// EXEC PL1LFCLG
//PL1L.SYSIN DD *
 STRIP:  PROC(PARM) OPTIONS(MAIN);
         DCL PARM CHAR(100) VARYING;
         I = LENGTH(PARM);
         ON ENDFILE(SYSIN) GO TO EOJ;
         DCL SYSOUT FILE RECORD OUTPUT
           ENV(CONSECUTIVE F(800,80));
         DCL CARD CHAR(80);
 NXT:
         READ FILE(SYSIN) INTO(CARD);
         DO J = 1 TO 20 WHILE(SUBSTR(CARD,J,1) = ' ');
              END;
         IF J = 21
              THEN GO TO NXT;
         IF SUBSTR(CARD,J,I) ^= PARM
              THEN GO TO NXT;
         SUBSTR(CARD,1,72) = SUBSTR(CARD,J+I,72-(J+I));
         WRITE FILE(SYSOUT) FROM(CARD);
         GO TO NXT;
 EOJ:
         END;
//LKED.SYSLMOD DD DSN=SYS1.COMLIB(STRIPDOC),DISP=SHR
