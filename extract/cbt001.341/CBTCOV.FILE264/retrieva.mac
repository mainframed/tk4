* RETRIEVA
PCL      IKJPARM
DSN      IKJPOSIT DSNAME,PROMPT=' THE DATA SET TO BE RETRIEVED'
SPCE     IKJKEYWD DEFAULT='SPACE'
         IKJNAME 'SPACE',SUBFLD=SPACESUB
JOBCRD   IKJKEYWD DEFAULT='JOBCARD'
         IKJNAME 'JOBCARD',SUBFLD=JOBSUB
TMPY     IKJKEYWD DEFAULT='TEMPNAME'
         IKJNAME 'TEMPNAME',SUBFLD=TMPSUB
ATKEY    IKJKEYWD DEFAULT='AT'
         IKJNAME 'AT',SUBFLD=ATSUB
VOL      IKJKEYWD
         IKJNAME 'VOLUME',SUBFLD=VOLSUB
SPACESUB IKJSUBF
SPACE    IKJIDENT 'SPACE',MAXLNTH=5,FIRST=NUMERIC,OTHER=NUMERIC,       C
               DEFAULT='1'
JOBSUB   IKJSUBF
JOBCARD  IKJPOSIT DSNAME,DEFAULT='JOBCARD.CNTL'
TMPSUB   IKJSUBF
TEMPNAME IKJPOSIT DSNAME,DEFAULT='BPRT'
ATSUB    IKJSUBF
AT       IKJIDENT 'AT',MAXLNTH=8,FIRST=ANY,OTHER=ANY,                  C
               DEFAULT='LOCAL'
VOLSUB   IKJSUBF
VOLSUBF  IKJIDENT 'VOLUME',MAXLNTH=6,FIRST=ANY,OTHER=ANY,              C
               PROMPT=' ENTER VOL TO RESTORE TO'
         IKJENDP
         END
