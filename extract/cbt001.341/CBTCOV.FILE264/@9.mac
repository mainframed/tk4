 /* SVC99 INTERFACE - WRITTEN 10/25/80 BY PAUL DIXON */;
 DCL @9RBPTR POINTER;
 DCL 01 @9RB UNALIGNED, 05 @9RBLNVRB FIXED BIN(15), 05 @9FLAG1 BIT(16)
     INIT(''), 05 @9ERR FIXED BIN(15), 05 @9INF FIXED BIN(15), 05 @9TXPP
     POINTER, 05 @9FILL0 BIT(32) INIT(''), 05 @9FLAG2 BIT(32) INIT('');
 DCL @9TUPL(99) POINTER;
 DCL @9I FIXED BIN(15) INIT(0);
 DCL @9FB31 FIXED BIN(31);
 DCL @9BIT0 BIT(32) INIT('1'B);
 DCL (@9R0,@9R1) POINTER;                                    /*
 DCL SYSSVC EXTERNAL ENTRY(FIXED BINARY(15),POINTER,POINTER)
                     RETURNS(POINTER)                        */;
 @9RB.@9TXPP = ADDR(@9TUPL);
 @9RBPTR = ADDR(@9RB);
 UNSPEC(@9RBPTR) = UNSPEC(@9RBPTR) × @9BIT0;
 @9R0,@9R1 = ADDR(@9RBPTR);
 %DCL (@9@_DDNRET,@9@_DSNRET,@9@_DSORET,@9@_RETCDI,@9@_RETDAA,
       @9@_RETDDN,@9@_RETDSN,@9@_RETDSO,@9@_RETLIM,@9@_RETLST,
       @9@_RETMBR,@9@_RETNDI,@9@_RETSTA,@9@_RETTYP,@9@_VOLRET) CHAR;
