 /* SVC99 INTERFACE - WRITTEN 10/25/80 BY PAUL DIXON */;
 %DCL (@9STATUS_OLD,@9STATUS_MOD,@9STATUS_NEW,@9STATUS_SHR) CHAR;
 %@9STATUS_OLD = '''00000001''B';
 %@9STATUS_MOD = '''00000010''B';
 %@9STATUS_NEW = '''00000100''B';
 %@9STATUS_SHR = '''00001000''B';
 %@9STATUS: PROC(STATUS) RETURNS(CHAR) STATEMENT;
 DCL (STATUS,R1) CHAR;
 IF SUBSTR(STATUS,1,1) = ''''
    THEN
       DO;
          R1 = @9STATUS_SHR;
          IF      STATUS  = '''OLD''' THEN R1 = @9STATUS_OLD;
          ELSE IF STATUS  = '''MOD''' THEN R1 = @9STATUS_MOD;
          ELSE IF STATUS  = '''NEW''' THEN R1 = @9STATUS_NEW;
          ELSE IF STATUS ^= '''SHR''' THEN
                  NOTE('INVALID STATUS ' ×× STATUS,8);
          RETURN ('DO; @9_STATUS.@9P  = ' ×× R1 ××
             '; @9I = @9I + 1; @9TUPL(@9I) = ADDR(@9_STATUS); END;');
       END;
    ELSE
       RETURN('DO; @9_STATUS.@9P  = ' ×× STATUS ×× '; @9I = @9I + 1; @9T
 UPL(@9I) = ADDR(@9_STATUS); END;');
 %END @9STATUS;
 %ACTIVATE @9STATUS;
 DCL 01 @9_STATUS UNALIGNED, 05 @9K FIXED BIN(15) INIT(4), 05 @9# FIXED
     BIN(15) INIT(1), 05 @9L FIXED BIN(15) INIT(1), 05 @9P  BIT(8);
