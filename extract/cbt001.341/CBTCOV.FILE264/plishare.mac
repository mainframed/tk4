//             LABEL=(4,SL), EXPDT=98000),
//             VOL=(PRIVATE,RETAIN,REF=*.FILE01.TAPE),
//             DCB=(DEN=4,LRECL=80,BLKSIZE=32720,RECFM=FB)
PROC SOURCE INDD=SOURCE OUTDD=TAPE;
