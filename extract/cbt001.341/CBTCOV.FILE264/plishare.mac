//* DEN=4=6250BPI, DEN=3=1600BPI
//FILE01   EXEC PGM=IEBCOPY
//*LOAD LIBRARY CONTAINING ABE
//SYSPRINT DD  SYSOUT=*
//SYSUT3   DD  UNIT=DISK,SPACE=(CYL,(1)),DISP=(NEW,DELETE)
//DISK     DD  DSN=SYS3.NOBLDL,DISP=SHR
//TAPE     DD  DSN=X75826.ABELOAD,DISP=(NEW,PASS),UNIT=TAPE,
//             LABEL=(1,SL), EXPDT=98000),
//             VOL=(PRIVATE,RETAIN) ,SER=MNEBTP)
 COPY INDD=DISK,OUTDD=TAPE
//FILE02   EXEC PGM=IEBCOPY
//*ABE AID LIBRARY
