 COPY INDD=DISK,OUTDD=TAPE
//FILE03   EXEC SAS,LOG='A',PRINT='A'
//*TEXT FOR ABE MANUAL, INCLUDING INSTALLATION INSTRUCTIONS
//SOURCE   DD  DSN=X75826.ABE.TEXT,DISP=SHR
//TAPE     DD  DSN=X75826.ABETEXT,DISP=(NEW,PASS),UNIT=TAPE,
//             LABEL=(3,SL), EXPDT=98000),
//             VOL=(PRIVATE,RETAIN,REF=*.FILE01.TAPE),
//             DCB=(DEN=4,LRECL=80,BLKSIZE=32720,RECFM=FB)
PROC SOURCE INDD=SOURCE OUTDD=TAPE;
