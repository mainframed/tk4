 /* SVC99 INTERFACE - WRITTEN 10/25/80 BY PAUL DIXON */;
 %DCL (@9LABEL_NL,@9LABEL_NSL,@9LABEL_SUL,@9LABEL_BLP,@9LABEL_LTM,
       @9LABEL_AL,@9LABEL_AUL,@9LABEL_SL) CHAR;
 %@9LABEL_NL  = '''00000001''B';
 %@9LABEL_SL  = '''00000010''B';
 %@9LABEL_NSL = '''00000100''B';
 %@9LABEL_SUL = '''00001000''B';
 %@9LABEL_BLP = '''00010000''B';
 %@9LABEL_LTM = '''00100001''B';
 %@9LABEL_AL  = '''01000000''B';
 %@9LABEL_AUL = '''01001000''B';
 %@9LABEL: PROC(LABEL) RETURNS(CHAR) STATEMENT;
 DCL (LABEL,R1) CHAR;
 IF SUBSTR(LABEL,1,1) = ''''
    THEN
       DO;
          R1 = @9LABEL_SL;
          IF      LABEL  = '''NL'''  THEN R1 = @9LABEL_NL;
          ELSE IF LABEL  = '''NSL''' THEN R1 = @9LABEL_NSL;
          ELSE IF LABEL  = '''SUL''' THEN R1 = @9LABEL_SUL;
          ELSE IF LABEL  = '''BLP''' THEN R1 = @9LABEL_BLP;
          ELSE IF LABEL  = '''LTM''' THEN R1 = @9LABEL_LTM;
          ELSE IF LABEL  = '''AL'''  THEN R1 = @9LABEL_AL;
          ELSE IF LABEL  = '''AUL''' THEN R1 = @9LABEL_AUL;
          ELSE IF LABEL ^= '''SL'''  THEN
                  NOTE('INVALID LABEL ' ×× LABEL,8);
          RETURN ('DO; @9_LABEL.@9P  = ' ×× R1 ××
             '; @9I = @9I + 1; @9TUPL(@9I) = ADDR(@9_LABEL); END;');
       END;
    ELSE
       RETURN('DO; @9_LABEL.@9P  = ' ×× LABEL ×× '; @9I = @9I + 1; @9TUP
 L(@9I) = ADDR(@9_LABEL); END;');
 %END @9LABEL;
 %ACTIVATE @9LABEL;
 DCL 01 @9_LABEL UNALIGNED, 05 @9K FIXED BIN(15) INIT(30), 05 @9# FIXED
     BIN(15) INIT(1), 05 @9L FIXED BIN(15) INIT(1), 05 @9P  BIT(8);
