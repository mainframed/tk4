     THEN CALL SETCAPS(CMDBUF);
     ELSE IF ^(LOFLAG × CMDNAME='APL' × CMDNAME='API')
       THEN CALL SETCAPS(CMDBUF);
  RETVAL = 0;
  IF ^RHDEBUGSW
    THEN GO TO BYERR;
  ON ERROR BEGIN;
    RETVAL = 16;
    GO TO EOR;
    END;
  ON CONVERSION BEGIN;
    RETVAL = 16;
    GO TO EOR;
    END;
