 /* SVC99 INTERFACE - WRITTEN 10/25/80 BY PAUL DIXON */;
 %DCL (@9INOUT_IN,@9INOUT_OUT) CHAR;
 %@9INOUT_IN  = '''10000000''B';
 %@9INOUT_OUT = '''01000000''B';
 %@9INOUT: PROC(INOUT) RETURNS(CHAR) STATEMENT;
 DCL (INOUT,R1) CHAR;
 IF SUBSTR(INOUT,1,1) = ''''
    THEN
       DO;
          R1 = @9INOUT_IN;
          IF      INOUT  = '''OUT''' THEN R1 = @9INOUT_OUT;
          ELSE IF INOUT ^= '''IN'''  THEN
                  NOTE('INVALID INOUT ' ×× INOUT,8);
          RETURN ('DO; @9_INOUT.@9P  = ' ×× R1 ××
             '; @9I = @9I + 1; @9TUPL(@9I) = ADDR(@9_INOUT); END;');
       END;
    ELSE
       RETURN('DO; @9_INOUT.@9P  = ' ×× INOUT ×× '; @9I = @9I + 1; @9TUP
 L(@9I) = ADDR(@9_INOUT); END;');
 %END @9INOUT;
 %ACTIVATE @9INOUT;
 DCL 01 @9_INOUT UNALIGNED, 05 @9K FIXED BIN(15) INIT(33), 05 @9# FIXED
     BIN(15) INIT(1), 05 @9L FIXED BIN(15) INIT(1), 05 @9P  BIT(8);
