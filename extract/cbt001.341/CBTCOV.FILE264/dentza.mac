 DENTZA:  PROC (RHPPTR) OPTIONS(REENTRANT);
 /* THIS SUBROUTINE IS CALLED TO RESET THE LOGICAL TABS. */
 % DCL @PROCNAME CHAR;
 % @PROCNAME = 'MAIN';
 % INCLUDE DENTSUB;
   DCL RHPPTR UNALIGNED PTR;
   DCL CRTZAP ENTRY;
   IF HARDCOPY
     THEN RETURN;
   STFSMODE='0'B;
   @CURPNL = 0; /* MAYBE BUG - WHAT ABOUT CURPNL? */
   CALL CRTZAP;
 END DENTZA;
