   %INCLUDE ABESUB;
 ABESUB(ABETEST) PARS(0) CHECK(YES);
   %INCLUDE ABESUBA;

   DCL SYSGETM ENTRY(CHAR(2),FIXED BIN(31,0),FIXED BIN(31,0),
     FIXED BIN(15,0),FIXED BIN(31,0));
   DCL TROUT ENTRY(CHAR(4)) RETURNS(CHAR(8));
   DCL A FIXED BIN(31,0),
       AC CHAR(4) BASED(ADDR(A));

   CALL SYSGETM('RU',1000000,A,99,RETCODE);
   PUT STRING(MSGDATA) EDIT('GETMAINED STORAGE AT ',TROUT(AC))(A,A);
   END ABETEST;
