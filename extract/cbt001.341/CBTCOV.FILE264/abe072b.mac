 /**********************************************************************
 *                                                                     *
 * ABE072B IS CALLED AT INITIALIZATION TIME TO FILL IN STRUCTURE       *
 * ABE79ENT WITH THE ENTRY POINTS TO SOME PLOT79 SUBROUTINES.  ONLY A  *
 * BARE SKELETON OF SUBROUTINES IS INITIALIZED; ENOUGH TO USE GSAS     *
 * AND GED. THIS IS TO SAVE MEMORY.                                    *
 *                                                                     *
 **********************************************************************/
 ABE072B: PROC(RHHDRPT);
   DCL RHHDRPT PTR;
   %INCLUDE ABERHHDR;
   %INCLUDE ABE79ENT;

   DCL (
     CLPL3, CLPL4, ERRAR, ERRAT, ERRCK, ERRIN, ERRMS, ERRTR, LINA3,
     MATMM, MATPM, MATTV, MATUN, MOVA3, PLTCL, PLTDL, PLTIO, PLTLS,
     PLTML, PLT00, SETSZ, SETTR, SET00, SYMSF, SYMSS, SYM00, UTITTY,
     UTRXP
     ) ENTRY OPTIONS(FORTRAN);
   #CLPL3    = CLPL3    ;
   #CLPL4    = CLPL4    ;
   #ERRAR    = ERRAR    ;
   #ERRAT    = ERRAT    ;
   #ERRCK    = ERRCK    ;
   #ERRIN    = ERRIN    ;
   #ERRMS    = ERRMS    ;
   #ERRTR    = ERRTR    ;
   #LINA3    = LINA3    ;
   #MATMM    = MATMM    ;
   #MATPM    = MATPM    ;
   #MATTV    = MATTV    ;
   #MATUN    = MATUN    ;
   #MOVA3    = MOVA3    ;
   #PLTCL    = PLTCL    ;
   #PLTDL    = PLTDL    ;
   #PLTIO    = PLTIO    ;
   #PLTLS    = PLTLS    ;
   #PLTML    = PLTML    ;
   #PLT00    = PLT00    ;
   #SETSZ    = SETSZ    ;
   #SETTR    = SETTR    ;
   #SET00    = SET00    ;
   #SYMSF    = SYMSF    ;
   #SYMSS    = SYMSS    ;
   #SYM00    = SYM00    ;
   #UTITTY   = UTITTY   ;
   #UTRXP    = UTRXP    ;
   END ABE072B;
