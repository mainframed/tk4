 /* THIS IS A DESCRIPTION OF ELEMENTS IN AN AVL BALANCED BINARY TREE
  KEYED ON SYMBOLIC VARIABLE NAMES CONTAINING REPLACEMENT VALUES. */
   DCL 1 SYMREC BASED(SRPTR),
     2 SRLCHILD PTR, /* LEFT CHILD POINTER */
     2 SRRCHILD PTR, /* RIGHT CHILD POINTER */
     2 SRNAME CHAR(8), /* KEY */
     2 SRBF FIXED BIN(15), /* AVL TREE BALANCE FACTOR */
     2 SRTYPE FIXED BIN(15), /* 1=SRFL IS VALID
                                2=SRCH IS VALID
                                3=BOTH ARE VALID */
     2 SRFL FLOAT DEC(16),
     2 SRMAXL FIXED BIN(15,0),
     2 SRCURL FIXED BIN(15,0),
     2 SRCH CHAR(I REFER(SRMAXL));
