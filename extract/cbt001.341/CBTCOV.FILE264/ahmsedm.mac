   /* AHMSEDM - HARDWARE MANAGEMENT SYSTEM EDIT INITIALIZATION */
   %INCLUDE ABESUB;
 ABESUB(AHMSEDM) CHECK(YES);
   %INCLUDE ABESUBA;
   DCL SAVPTR PTR;
   DCL PLILOAD ENTRY(CHAR(8),ENTRY,FIXED BIN(15,0));
   DCL I FIXED BIN(15,0);
   DCL NAME CHAR(8); /* TO BYPASS BUG IN COMPILER */

   CALL PLILOAD('DSNLINK ',#ISPLINK,I);
   SAVPTR=RHPTR;
   I=#CMD('PGM AHMSEDR',RHPTR,RHHDRPT);
   I=I+#CMD('SSE '''' NAME(FEATSEL) INP(USE FEATURES)',RHPTR,RHHDRPT);
   IF I^=0
     THEN DO;
       IF MSGDATA=''
         THEN MSGDATA='ERROR IN AHMSEDR='××I;
       RHRC=8;
       RETURN;
       END;
   RHPTR=SAVPTR;
   CMDDATA='PGM AHMSEDT';
   CALL #STACK('END');
   END AHMSEDM;
