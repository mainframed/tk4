 /* DENTPS (PUTSLOT)
 THIS SUBROUTINE IS INVOKED WHEN A SLOT IS TO BE REMOVED FROM THE
 ARRAY OF RECORDS AND RETURNED TO THE POOL OF FREE SLOTS. */
 DENTPS:  PROC(RHPTR,SUBENT,PRMPTR) OPTIONS(REENTRANT);
   % DCL @PROCNAME CHAR;
   % @PROCNAME = '';
   % INCLUDE DENTSUB;
   DCL PRMPTR UNALIGNED PTR;
   DCL TMPPTR UNALIGNED PTR;
   TMPPTR = PRMPTR;
   IF RECPTR = TMPPTR
     THEN CALL #NC(RHPTR,SUBENT,TMPPTR->NXTPTR);
       /* CALL NEWCUR */
   TMPPTR->LSTPTR->NXTPTR = TMPPTR->NXTPTR;
   TMPPTR->NXTPTR->LSTPTR = TMPPTR->LSTPTR;
   FREE TMPPTR->REC;
   RHCOUNT=RHCOUNT-1;
   SAVEFLAG = '1'B;
   END DENTPS;
