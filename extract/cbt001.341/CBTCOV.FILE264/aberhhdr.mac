       /* PTR TO BPAM DCB FOR SYSRUN OR NULL */
     2 RHPNLDCB PTR,
       /* PTR TO BPAM DCB FOR SYSPNL OR NULL */
     2 RHPNLHDR PTR,
       /* PTR TO LIST OF IN-MEMORY PANELS */
