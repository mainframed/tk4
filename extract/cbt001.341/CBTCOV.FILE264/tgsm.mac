         DCL 1 CODEM BASED (M),
              2 CODE CHAR (1),
                   /* CODE M = IDENTIFY NO-WORK DAY */
              2 BLNK CHAR (2),
              2 PRJNUM CHAR (6),
                   /* PROJECT NUMBER */
              2 MDATE,
                   /* DATE OF HOLIDAY - MMDDYY */
               3 MM PIC '99',
               3 DD PIC '99',
               3 YY PIC '99',
              2 COMMENTS CHAR (65),.
