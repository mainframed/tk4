  /*TGSRESTI*/
 ISAMRST: PROC OPTIONS (MAIN);
         DCL ISAMFLE FILE KEYED ENV(INDEXED);
         DCL 1 ISAMREC,
              2 PAD CHAR (5),
              2 KEY CHAR(44);
         ON ENDFILE (BACKUP) GO TO EOJ;
         OPEN FILE (ISAMFLE) OUTPUT SEQUENTIAL;
         DCL PUTFLAG BIT (1) STATIC INIT ('0'B);
 NEXT:
         READ FILE (BACKUP) INTO (ISAMREC);
         WRITE FILE (ISAMFLE) FROM (ISAMREC) KEYFROM (KEY);
         PUTFLAG = '1'B;
         GO TO NEXT;
 EOJ:
         IF PUTFLAG = '0'B THEN DO;
              PAD = ' ';
         KEY = '000000';
         WRITE FILE (ISAMFLE) FROM (ISAMREC) KEYFROM (KEY);
              END;
         END ISAMRST;
