 /* LSUASAR */
 /* RETURNS THE LOWEST NUMERIC ADDRESS IN THE SPECIFIED DIMENSION */
 LSUASAR:  PROC(PRMPTR,PRMCNT) RETURNS(FIXED BIN(31,0));
         DCL PRMPTR PTR;
         DCL PRMCNT FIXED BIN(15,0);
         % INCLUDE ASARDCL;
         ASARHP = PRMPTR;
         DCL I FIXED BIN(15,0);
         I = REFASAR(PRMPTR); /* GO FIND ELEMENT IF IT EXISTS */
         IF I <= ASARDCT
           THEN RETURN(1);
           ELSE RETURN(UNSPEC(ASARDPT(PRMCNT)->ASAREL));
         END;
