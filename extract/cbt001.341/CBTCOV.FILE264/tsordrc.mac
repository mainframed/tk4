 TSORDRC:  PROC(PARM) OPTIONS(MAIN);
 DCL PARM CHAR(100) VARYING;
 DCL 1 CTLREC, 2 ARY(40) FIXED BIN(15,0) INIT((40)0B);
  DCL CTL FILE ENV(F RECSIZE(80) BLKSIZE(80) REGIONAL(1)) RECORD OUTPUT
  SEQUENTIAL KEYED;
 I = PARM;
 ARY(1) = I;
  DO J = 0 TO I;
  WRITE FILE(CTL) FROM(CTLREC) KEYFROM(J);
  ARY(1) = 0B;
  END;
  END;
