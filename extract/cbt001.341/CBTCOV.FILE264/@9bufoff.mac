 %DCL @9BUFOFF_L CHAR;
 %@9BUFOFF = '''10000000''B';
 %@9BUFOFF: PROC(BUFOFF) RETURNS(CHAR) STATEMENT;
 DCL BUFOFF CHAR;
 IF BUFOFF = '''L'''
    THEN
       RETURN('DO; @9_BUFOFF.@9P = @9BUFOFF_L; @9I = @9I + 1; ' ××
              ' @9TUPL(@9I) = ADDR(@9_BUFOFF); END;');
    ELSE
       RETURN('DO; @9FB31 = ' ×× BUFOFF ××
              '; @9_BUFOFF.@9P  = SUBSTR(UNSPEC(@FB31),25);' ××
              '@9I = @9I + 1; @9TUPL(@9I) = ADDR(@9_BUFOFF); END;');
 %END @9BUFOFF;
 %ACTIVATE @9BUFOFF;
 DCL 01 @9_BUFOFF UNALIGNED, 05 @9K FIXED BIN(15) INIT(53), 05 @9# FIXED
     BIN(15) INIT(1), 05 @9L FIXED BIN(15) INIT(1), 05 @9P  BIT(8);
