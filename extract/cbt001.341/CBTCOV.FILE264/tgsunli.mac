  /*TGSUNLI*/
 ISAMUNL: PROC OPTIONS (MAIN);
         DCL ISAMFLE FILE ENV(INDEXED);
         DCL 1 ISAMREC CHAR(49);
         ON ENDFILE (ISAMFLE) GO TO EOJ;
 NEXT:
         READ FILE (ISAMFLE) INTO (ISAMREC);
         WRITE FILE (BACKUP) FROM (ISAMREC);
         GO TO NEXT;
 EOJ:
         END ISAMUNL;
