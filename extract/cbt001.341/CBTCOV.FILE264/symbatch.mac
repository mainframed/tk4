 /* SYMBATCH */
 /*
0*$.HE GENERALIZED SUBROUTINES - SYMBATCH
0*$.HE PLI PREPROCESSOR TEXT
0*$.HE USE SYMBOLIC REPLACEMENT IN BATCH
0*$.PL 55
0*$.PN 0
0*$.PA
0*$.SS
0*$.NF
0*$11/22/78
0*$AUTHOR:  T. SMITH
0*$SOURCE MODULE NAME:  SYMBATCH
0*$DOCUMENTATION ID:  0*$
0*$.FI L
0*$.SP 5
0*$ADD A "%INCLUDE SYMBATCH" STATEMENT TO YOUR PROGRAM IF YOU WANT
0*$TO USE SYMBOLIC VARIABLES (SEE DOCUMENTATION ON @REP, @SETL,
0*$AND SYM@) IN A PROGRAM WHICH IS NOT A PLIST (SEE DOCUMENTATION
0*$ON PLIST).  FOR EXAMPLE:
0*$.SP 1
0*$.NF
0*$.IN 5
0*$TEST:  PROC OPTIONS(MAIN);
0*$% INCLUDE SYMBATCH;
0*$@SETL(&COLOR = 'BLUE');
0*$DCL STR CHAR(100) VAR;
0*$@REP(STR = 'THAT COW IS &COLOR');
0*$PUT LIST(SYMC('&COLOR'),STR);
0*$*** PRINTS 'BLUE' 'THAT COW IS BLUE' ***
0*$END;
 */
         % DCL @SETLI CHAR;
         % INCLUDE @SETL;
         % INCLUDE @REP;
         DCL (LCLC,SYMC) ENTRY RETURNS(CHAR(100) VARYING);
         DCL (LCLA,SYMA) ENTRY(CHAR(*))
           RETURNS(FIXED BIN(31,0));
         DCL SET@ ENTRY(BIT(1),CHAR(8),CHAR(100) VARYING);
         DCL 1 BUFFER@,
              2 BUFLEN@ FIXED BIN(15,0),
              2 BUFOFF@ FIXED BIN(15,0) INIT(0),
              2 BUFSTR@ CHAR(256);
         DCL NULL BUILTIN;
         DCL SYMHD@ PTR EXT;
         SYMHD@ = NULL;
         DCL 1 DUMECT UNALIGNED,
              2 DUMECTPAD CHAR(32),
              2 DUMECTUSER CHAR(4),
              2 DUMECTPT PTR;
         DUMECTPT = NULL;
         DCL 1 CPPL EXT,
              2 CPPLCBUF PTR,
              2 CPPLUPT PTR,
              2 CPPLPSCB PTR,
              2 CPPLECT PTR;
         CPPLECT = ADDR(DUMECT);
 /* END SYMBATCH */
