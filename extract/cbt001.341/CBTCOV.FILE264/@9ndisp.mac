 /* SVC99 INTERFACE - WRITTEN 10/25/80 BY PAUL DIXON */;
 %DCL (@9NDISP_UNCATLG,@9NDISP_CATLG,@9NDISP_DELETE,@9NDISP_KEEP) CHAR;
 %@9NDISP_UNCATLG = '''00000001''B';
 %@9NDISP_CATLG   = '''00000010''B';
 %@9NDISP_DELETE  = '''00000100''B';
 %@9NDISP_KEEP    = '''00001000''B';
 %@9NDISP: PROC(NDISP) RETURNS(CHAR) STATEMENT;
 DCL (NDISP,R1) CHAR;
 IF SUBSTR(NDISP,1,1) = ''''
    THEN
       DO;
          R1 = @9NDISP_KEEP;
          IF      NDISP  = '''UNCATLG''' THEN R1 = @9NDISP_UNCATLG;
          ELSE IF NDISP  = '''CATLG'''   THEN R1 = @9NDISP_CATLG;
          ELSE IF NDISP  = '''DELETE'''  THEN R1 = @9NDISP_DELETE;
          ELSE IF NDISP ^= '''KEEP'''    THEN
                  NOTE('INVALID NDISP ' ×× NDISP,8);
          RETURN ('DO; @9_NDISP.@9P  = ' ×× R1 ××
             '; @9I = @9I + 1; @9TUPL(@9I) = ADDR(@9_NDISP); END;');
       END;
    ELSE
       RETURN('DO; @9_NDISP.@9P  = ' ×× NDISP ×× '; @9I = @9I + 1; @9TUP
 L(@9I) = ADDR(@9_NDISP); END;');
 %END @9NDISP;
 %ACTIVATE @9NDISP;
 DCL 01 @9_NDISP UNALIGNED, 05 @9K FIXED BIN(15) INIT(5), 05 @9# FIXED
     BIN(15) INIT(1), 05 @9L FIXED BIN(15) INIT(1), 05 @9P  BIT(8);
