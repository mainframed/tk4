 /* PERF003 */
         % INCLUDE PERFA;
         @TYPE(71);
         SMF71PTRA = SMFPTR;
         SMF71ADJA = SMF71ADJA + 48 + SMF71SPC;
         M = ROUND(DIVIDE((SMF71PIN+SMF71POT),3600,8,2),0);
         @PLOT(21,'NO-VIO, NO-SWAP PAGE-INS & PAGE-OUTS/SEC',M);
         M = SMF71PIN+SMF71POT+SMF71SIN+SMF71SOT+SMF71VIN+SMF71VOT;
         M = ROUND(DIVIDE(M,3600,8,2),0);
         @PLOT(22,'TOTAL PAGE-INS & PAGE-OUTS/SEC',M);
         M = ROUND(DIVIDE((SMF71PIN+SMF71PRC),3600,8,2),0);
         @PLOT(23,'PAGE FAULTS (N-VIO N-SWAP PAGEINS + RECL/SEC)',M);
         % INCLUDE PERFB;
