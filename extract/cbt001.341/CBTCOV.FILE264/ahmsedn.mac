   /* AHMSEDN - HARDWARE MANAGEMENT SYSTEM EDIT INITIALIZATION */
   %INCLUDE ABESUB;
 ABESUB(AHMSEDN) CHECK(YES);
   %INCLUDE ABESUBA;
   DCL SAVPTR PTR;
   DCL PLILOAD ENTRY(CHAR(8),ENTRY,FIXED BIN(15,0));
   DCL I FIXED BIN(15,0);
   DCL NAME CHAR(8); /* TO BYPASS BUG IN COMPILER */

   CALL PLILOAD('DSNLINK ',#ISPLINK,I);
   SAVPTR=RHPTR;
   I=  #CMD('ABE X75826.FEATDESC NAME(FEATDESC) DB2 INTERNAL',
     RHPTR,RHHDRPT);
   I=I+#CMD('ABE X75826.FEATURES NAME(FEATURES) DB2 INTERNAL',
     RHPTR,RHHDRPT);
   I=I+#CMD('ABE X75826.LOCNAMES NAME(LOCNAMES) DB2 INTERNAL',
     RHPTR,RHHDRPT);
   I=I+#CMD('ABE X75826.THINGTAB NAME(THINGTAB) DB2 INTERNAL',
     RHPTR,RHHDRPT);
   I=I+#CMD('ABE X75826.VENDORS  NAME(VENDORS)  DB2 INTERNAL',
     RHPTR,RHHDRPT);
   I=I+#CMD('ABE '''' NAME(FEATSEL) INP(USE FEATURES)',RHPTR,RHHDRPT);
   RHPTR=SAVPTR;
   IF I^=0
     THEN DO;
       IF MSGDATA=''
         THEN MSGDATA='ERROR IN AHMSEDN='××I;
       RHRC=8;
       RETURN;
       END;
   CMDDATA='PGM AHMSEDT';
   CALL #STACK('END');
   END AHMSEDN;
