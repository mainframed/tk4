   /* AHMSEDS - HARDWARE MANAGEMENT SYSTEM EDIT SAVE  */
   %INCLUDE ABESUB;
 ABESUB(AHMSEDS) CHECK(YES);
   %INCLUDE ABESUBA;

   I=  #CMD('A FEATDESC SAVE',RHPTR,RHHDRPT);
   I=I+#CMD('A FEATURES SAVE',RHPTR,RHHDRPT);
   I=I+#CMD('A LOCNAMES SAVE',RHPTR,RHHDRPT);
   I=I+#CMD('A THINGTAB SAVE',RHPTR,RHHDRPT);
   I=I+#CMD('A VENDORS  SAVE',RHPTR,RHHDRPT);
   IF I^=0
     THEN DO;
       IF MSGDATA=''
         THEN MSGDATA='ERROR IN SAVE='××I;
       RHRC=8;
       END;
   END AHMSEDS;
