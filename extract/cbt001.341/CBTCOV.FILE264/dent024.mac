 /* COPYRIGHT 1983 BY THOMAS GLEN SMITH */
   /* DENT024 - PFK,DEFINE,DEF,NAME,RUN,DO,LOG */
   % DCL @PROCNAME CHAR;
   % DCL @RENTNAME CHAR;
   % @PROCNAME ='RENT';
   % @RENTNAME ='DENT024';
   % INCLUDE DENTSUB;
   DCL SUFFIX ENTRY(CHAR(*),CHAR(*))RETURNS(FIXED BIN(15,0));
   DCL SYMSW BIT(1) INIT('1'B);
   DCL SUPSW BIT(1) INIT('0'B);
   DCL LISTSW BIT(1) INIT('0'B);
   DCL HITSW BIT(1) INIT('0'B);
   DCL NOSYM CHAR(5) STATIC INIT('NOSYM');
   DCL LIST  CHAR(4) STATIC INIT('LIST');
   DCL SYMCK CHAR(80) VAR;
   DCL VAL CHAR(256) VAR;
   DCL KEY CHAR(8);
   DCL GBLSW BIT(1) STATIC INIT('0'B);
   DCL 1 REPBUF,
     2 BUFLEN FIXED BIN(15,0),
     2 OFFSET FIXED BIN(15,0),
     2 BF CHAR(1024);
   DCL (CPDATA,TPDATA) CHAR(1024) VAR;
   DCL SETCAPS ENTRY;
   DCL (TRH,CP) PTR;
   DCL 1 PFKSTR,
     2 CON CHAR(3) INIT('PFK'),
     2 PFKNUM PIC'99';
   DCL PFKNAME CHAR(5) DEF PFKSTR;
   DCL SURNAME CHAR(8);
   DCL PFKDFT(24) CHAR(15) VAR STATIC INIT(
     /* PFK  1 = */ 'LAST',
     /* PFK  2 = */ 'FILL 50',
     /* PFK  3 = */ 'END',
     /* PFK  4 = */ 'LAST',
     /* PFK  5 = */ 'SPFF',
     /* PFK  6 = */ 'SPFC',
     /* PFK  7 = */ 'PB',
     /* PFK  8 = */ 'PF',
     /* PFK  9 = */ 'TOP',
     /* PFK 10 = */ 'COL L',
     /* PFK 11 = */ 'COL R',
     /* PFK 12 = */ 'BOTTOM',
     /* PFK 13 = */ '',
     /* PFK 14 = */ 'MDEL 1 2',
     /* PFK 15 = */ 'MV 4',
     /* PFK 16 = */ 'MC 1 2 3',
     /* PFK 17 = */ 'MM 1 2 3',
     /* PFK 18 = */ 'TS M0',
     /* PFK 19 = */ 'HALFB',
     /* PFK 20 = */ 'HALFF',
     /* PFK 21 = */ 'LINES',
     /* PFK 22 = */ 'HEX',
     /* PFK 23 = */ 'CHAR',
     /* PFK 24 = */ 'ALTCASE');
   DCL CMODESTR CHAR(15) VAR STATIC INIT('CMODE');
   DCL CMDLAB(8) LABEL;
   GO TO CMDLAB(CMDINDX);
  CMDLAB(8): /* LOG */
   IF ^TERMFLAG
     THEN DO;
       MSGDATA='ERROR - LOG SUBCMD ONLY FOR VIRTUAL TERMINAL';
       RHRC = 8;
       RETURN;
       END;
   IF PARY(2) =''
     THEN DO;
       MSGDATA='MISSING OPERAND';
       RHRC = 8;
       RETURN;
       END;
   L1 = PARY(2);
   IF PARY(3) =''
     THEN DO;
       IX = 3;
       L2 = L1;
       END;
     ELSE DO;
       L2 = PARY(3);
       IX = 4;
       END;
   IF PARY(4) ^=''
     THEN DO;
       SURNAME=PARY(4);
       IX = 5;
       GO TO ENDCASE;
       END;
   SURNAME=RHNAME;
   TRH=RHPTR;
   GO TO COMMON;
  CMDLAB(7):
   /* DO */
   IF PARY(2)=''
     THEN DO;
       IX = 2;
       L1 = 0;
       L2 = 99999999;
       END;
     ELSE DO;
       L1 = PARY(2);
       IF PARY(3)=''
         THEN DO;
           IX = 3;
           L2=L1;
           END;
         ELSE DO;
           L2=PARY(3);
           IX = 4;
           END;
       END;
   SURNAME=RHNAME;
   TRH=RHPTR;
   RHPTR=RHLST;
   GO TO COMMON;
  CMDLAB(1): /* DENT024 */
  CMDLAB(2): /* PFK */
   PFKNUM = PARY(2);
   SURNAME = PFKNAME;
   IX = 3;
   L1 = 0;
   L2 = 99999999;
   GO TO ENDCASE;
  CMDLAB(6): /* 1   2    3  4  5        */
             /* RUN NAME L1 L2 KEYWORDS */
   SURNAME = PARY(2);
   IF PARY(3)=''
     THEN DO;
       IX = 3;
       L1 = 0;
       L2 = 99999999;
       END;
     ELSE DO;
       L1 = PARY(3);
       IF PARY(4) =''
         THEN DO;
           IX = 4;
           L2 = L1;
           END;
         ELSE DO;
           L2=PARY(4);
           IX = 5;
           END;
       END;
   HIX=IX; /* SAVE ORIGINAL IX VALUE */
   DO UNTIL(^HITSW × (HIX+2=IX));
     HITSW='0'B;
     IF PARY(IX)^=''
       THEN DO;
         SYMCK=PARY(IX);
         CALL SETCAPS(SYMCK);
         IF SYMCK=SUBSTR(NOSYM,1,MIN(5,LENGTH(SYMCK)))
           THEN DO;
             SYMSW='0'B;
             HITSW='1'B;
             END;
         IF SYMCK=SUBSTR(LIST,1,MIN(4,LENGTH(SYMCK)))
           THEN LISTSW,HITSW='1'B;
         END;
     IF HITSW
       THEN IX=IX+1;
     END;
  ENDCASE:
   IF SURNAME=''
     THEN TRH=RHPTR; /* USE CURRENT DATA SET */
     ELSE DO;
       CALL SETCAPS(SURNAME);
       DO TRH=NXTRHEL REPEAT TRH->RHNXT WHILE(TRH^=NULL)
         UNTIL(TRH->RHNAME=SURNAME);
         END;
       END;
  COMMON:
   IF TRH=NULL
     THEN DO;
       IF CMDINDX=6
         THEN DO;
           RHRC = 8;
           MSGDATA='NAME NOT FOUND';
           END;
         ELSE DO;
           RHRC = 0;
           I = PARY(2);
           IF I < 25
             THEN IF PFKDFT(I) ^=''

                             THEN CALL #DENTCP(RHPTR,SUBENT,RHHDRPT,
               PFKDFT(I)); /* CALL DENTCP */
               ELSE DO;
                 RHRC = 8;
                 MSGDATA ='PFK NOT DEFINED';
                 END;
           END;
       RETURN;
       END;
   IC=IX;
   IF SYMSW
     THEN DO IX = IX TO 15;
       I = INDEX(PARY(IX),'(');
       IF I < 2 × I > 8 × SUBSTR(PARQCK,IX,1)
         THEN DO; /* IT MUST BE POSITIONAL */
           IK=IX-IC+1; /* IK=POSITIONAL PARAMETER NUMBER */
           IF IK<10
             THEN PUT STRING(KEY) EDIT('&P',IK)(A(2),P'9');
             ELSE PUT STRING(KEY) EDIT('&P',IK)(A(2),P'99');
           VAL=PARY(IX);
           END;
         ELSE DO;
           KEY ='&'××SUBSTR(PARY(IX),1,I-1);
           CALL SETCAPS(KEY);
           VAL = SUBSTR(PARY(IX),I+1,LENGTH(PARY(IX))-I-1);
           END;
       CALL #SET(GBLSW,KEY,VAL); /* CALL SET@ */
       END;
   CPDATA='';
   DO CP=TRH->NXTHDR REPEAT CP->NXTPTR WHILE(CP^=TRH->HDPTR);
     IF CP->RECSEQ>=L1
       THEN IF CP->RECSEQ>L2
         THEN LEAVE;
         ELSE DO;
           IF ^SYMSW
             THEN TPDATA=CP->RECSTR;
             ELSE DO;
               BUFLEN = MIN(1022,LENGTH(CP->RECSTR)+4);
               OFFSET = 0;
               BF = CP->RECSTR;
               CALL #REP(REPBUF); /* CALL REP@ */
               TPDATA = SUBSTR(BF,1,BUFLEN-4);
               END;
           IF LENGTH(TPDATA)>0
             THEN TPDATA=SUBSTR(TPDATA,1,SUFFIX(TPDATA,' '));
           IF LENGTH(CPDATA)=0
             THEN CPDATA=TPDATA;
             ELSE CPDATA=SUBSTR(CPDATA,1,LENGTH(CPDATA)-1)
               ××' '××TPDATA;
           IF LENGTH(CPDATA)>0
             THEN IF SUBSTR(CPDATA,LENGTH(CPDATA),1)^='+'
               THEN DO;
                 IF CPDATA^=''
                   THEN DO;
                     IF LISTSW
                       THEN CALL #TERM(CPDATA);
                     CALL #DENTCP(RHPTR,SUBENT,
                       RHHDRPT,CPDATA);
                     END;
                 CPDATA='';
                 END;
           END;
     END;
   IF CPDATA^=''
     THEN DO;
       IF LISTSW
         THEN CALL #TERM(CPDATA);
       CALL #DENTCP(RHPTR,SUBENT,RHHDRPT,CPDATA);
       END;
   IF CMDINDX=8 /* IS IT LOG? */
     THEN CALL #DENTCP(RHPTR,SUBENT,RHHDRPT,CMODESTR);
   /* CALL DENTCP - PUT HIM IN CMODE */
   RHRC = 0;
   RETURN;
  CMDLAB(3): /* DEFINE */
  CMDLAB(4): /* DEF */
  CMDLAB(5): /* NAME */
   IF PARY(3) ^=''
     THEN DO;
       CALL SETCAPS(PARY(3));
       IF PARY(3)^=SUBSTR('SUPPORT',1,MIN(7,LENGTH(PARY(3))))
         THEN DO;
           MSGDATA='INVALID OPERAND ='××PARY(3);
           RHRC=16;
           RETURN;
           END;
         ELSE SUPSW='1'B;
       END;
   IF PARY(2) ^=''
     THEN DO;
       CALL SETCAPS(PARY(2));
       DO TRH=NXTRHEL REPEAT TRH->RHNXT WHILE(TRH^=NULL)UNTIL(TRH->
       RHNAME=PARY(2));
         END;
       IF TRH^=NULL
         THEN DO;
           MSGDATA ='NAME ALREADY IN USE';
           RHRC = 8;
           RETURN;
           END;
       SURNAME = PARY(2);
       END;
     ELSE SURNAME ='====>';
   IF CMDINDX = 5 /* IS IT NAME? */
     THEN DO;
       RHNAME = PARY(2);
       RHSUPSW=SUPSW;
       RHRC = 0;
       RETURN;
       END;
   PARY(*) ='';
   PARY(4) ='SUBCALL';
   CALL #DENT(CMDINDX,PARPTR,RHPTR,SUBENT,RHHDRPT); /* CALL DENTSI */
   RHNAME = SURNAME;
   RHSUPSW=SUPSW;
   RHRC = 0;
   END;
