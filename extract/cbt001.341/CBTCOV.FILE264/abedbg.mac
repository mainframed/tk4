 /**********************************************************************
 *                                                                     *
 * ABEDBG CONTAINS ENTRY POINTS FOR #DB2GET, #DB2PUT, AND #DB2COM,     *
 * WHICH INTERFACE TO DB2 TO RESPECTIVELY RETRIEVE AND STORE A DB2     *
 * TABLE, AND TO EXECUTE ANY SQL COMMAND IN IMMEDIATE MODE.            *
 *                                                                     *
 **********************************************************************/
   %INCLUDE ABESUB;
 ABESUB(ABEDBG) PARMS(NAME,SUBRTNE,TERM) RETURNS(FIXED BIN(31,0));
   %INCLUDE ABESUBA;

   DCL NAME CHAR(*);
   DCL (SUBRTNE,TERM) ENTRY;
   DCL (DB2GET,DB2PUT) ENTRY(CHAR(*),ENTRY,ENTRY)
     RETURNS(FIXED BIN(31,0));
   DCL DB2COM ENTRY(CHAR(*),ENTRY)
     RETURNS(FIXED BIN(31,0));

   FETCH DB2GET;
   RETURN(DB2GET(NAME,SUBRTNE,TERM));

 ABEDBP: ENTRY(NAME,SUBRTNE,TERM) RETURNS(FIXED BIN(31,0))
   OPTIONS(REENTRANT);
   FETCH DB2PUT;
   RETURN(DB2PUT(NAME,SUBRTNE,TERM));

 ABEDBC: ENTRY(NAME,TERM) RETURNS(FIXED BIN(31,0))
   OPTIONS(REENTRANT);
   FETCH DB2COM;
   RETURN(DB2COM(NAME,TERM));

   END ABEDBG;
