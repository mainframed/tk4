 /* SVC99 INTERFACE - WRITTEN 10/25/80 BY PAUL DIXON */;
 %@9SUBPRM: PROC(V1,V2,V3,V4,V5) RETURNS(CHAR) STATEMENT;
 DCL (V1,V2,V3,V4,V5) CHAR, VN FIXED;
 IF PARMSET(V1) THEN
    DO;
       V1 = '@9_SUBPRM.@9P(1) = ' ×× V1 ×× ';';
       VN = 1;
       IF PARMSET(V2) THEN
          DO;
             V2 = '@9_SUBPRM.@9P(2) = ' ×× V2 ×× ';';
             VN = 2;
             IF PARMSET(V3) THEN
                DO;
                   V3 = '@9_SUBPRM.@9P(3) = ' ×× V3 ×× ';';
                   VN = 3;
                   IF PARMSET(V4) THEN
                      DO;
                         V4 = '@9_SUBPRM.@9P(4) = ' ×× V4 ×× ';';
                         VN = 4;
                         IF PARMSET(V5) THEN
                            DO;
                               V5 = '@9_SUBPRM.@9P(5) = ' ×× V5 ×× ';';
                               VN = 5;
                            END;
                      END;
                END;
          END;
    END;
 RETURN('DO; @9_SUBPRM.@9# = ' ×× VN ×× ';' ××
         V1 ×× V2 ×× V3 ×× V4 ×× V5 ××
        '@9I = @9I + 1; @9TUPL(@9I) = ADDR(@9_SUBPRM); END;');
 %END @9SUBPRM;
 %ACTIVATE @9SUBPRM;
 DCL 01 @9_SUBPRM UNALIGNED, 05 @9K FIXED BIN(15) INIT(96), 05 @9# FIXED
     BIN(15), 05 FILL#01(5), 10 @9L FIXED BIN(15) INIT(67),
     10 @9P CHAR(67);
