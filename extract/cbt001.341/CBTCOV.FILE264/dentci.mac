 DENTCI: PROC(RHHDRPT,SUBENT) OPTIONS(REENTRANT);
 /* THIS SUBROUTINE IS CALLED AT STARTUP TIME BY DENTIN TO INITIALIZE
 INFORMATION ABOUT THE TERMINAL SCREEN FORMAT */
 % DCL @PROCNAME CHAR;
 % @PROCNAME = '';
 % INCLUDE DENTSUB;
   DCL TABAINIT(16) CHAR(256) VAR STATIC INIT(
     'TAB','5','10','15','20','25','30','35',
     '40','45','50','55','60','65','','');
   DCL PLILOAD ENTRY(CHAR(8),ENTRY,FIXED BIN(15,0));
   DCL LINKRET FIXED BIN(15,0);
   DCL ENTRYPT ENTRY(PTR,PTR) VARIABLE;
   DCL DENTZA ENTRY;
   SUBENT(6) = DENTZA; /* CRTZAPS */
   DCL DENTCT ENTRY;
   SUBENT(9) = DENTCT; /* COLTR */
   DCL DENTTS ENTRY;
   SUBENT(14) = DENTTS; /* TABSET */
   DCL DENTTD ENTRY;
   SUBENT(18) = DENTTD; /* TABDISP */
   DCL DENTDIS ENTRY;
   SUBENT(34) = DENTDIS;
   ALLOCATE RHPNL SET(RHPPTR);
   RH_LB=1;
   RH_RB=32760;
   RHPSKHD=NULL;
   STRING(PNLFLAGS)='0'B;
   SCANFLAG='1'B;
   TRUNCMODE='0'B;
   RHPFILL=0;
   CURSBASE = -1;
   RHPNLQ = NULL;
   @ARYPTR = ADDR(RHPNLARYS);
   RHPARYCNT=1;
   RHPMAXLN=5;
   RHPMAXSQ=99999;
   PSCREENLEN,PLINESIZE,RHPFRACT=0;
   RHPAREAP=NULL;
   CALL DENTDIS(RHPPTR);
   CALL #TABSET(RHPPTR,TABAINIT,SUBENT); /* TABSET */
   END DENTCI;
