 /* ABE054 - ISPLINK */
   %INCLUDE ABESUB;
 ABESUB(ABE054) PARS(00000) CHECK(YES);
   %INCLUDE ABESUBA;

   DCL SETCAPS ENTRY;
   DCL PLIRETV BUILTIN;
   DCL A(6) CHAR(80);
   DO I=1 TO HBOUND(PARY,1) WHILE(PARY(I)^='');
     CALL SETCAPS(PARY(I));
     A(I)=PARY(I);
     END;
   SELECT(I);
     WHEN(1) DO;
       MSGDATA='ISPLINK NEEDS AT LEAST ONE PARAMETER';
       RHRC=8;
       RETURN;
       END;
     WHEN(2) CALL #ISPLINK(A(2));
     WHEN(3) CALL #ISPLINK(A(2),A(3));
     WHEN(4) CALL #ISPLINK(A(2),A(3),A(4));
     WHEN(5) CALL #ISPLINK(A(2),A(3),A(4),A(5));
     WHEN(6) CALL #ISPLINK(A(2),A(3),A(4),A(5),A(6));
     OTHERWISE DO;
       MSGDATA='ISPLINK WON''T TAKE MORE THAN 5 PARAMETERS';
       RHRC=8;
       RETURN;
       END;
     END; /* SELECT */
   RHRC=PLIRETV;
   IF RHRC^=0
     THEN PUT STRING(MSGDATA) LIST('NONZERO RETURN CODE FROM ISPLINK =',
       RHRC);
   END ABE054;
