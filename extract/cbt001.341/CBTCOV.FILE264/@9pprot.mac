 /* SVC99 INTERFACE - WRITTEN 10/25/80 BY PAUL DIXON */;
 %DCL (@9PPROT_PWREAD,@9PPROT_PWWRITE) CHAR;
 %@9PPROT_PWREAD  = '''00010000''B';
 %@9PPROT_PWWRITE = '''00110000''B';
 %@9PPROT: PROC(PPROT) RETURNS(CHAR) STATEMENT;
 DCL (PPROT,R1) CHAR;
 IF SUBSTR(PPROT,1,1) = ''''
    THEN
       DO;
          R1 = @9PPROT_PWWRITE;
          IF      PPROT  = '''PWREAD''' THEN R1 = @9PPROT_PWREAD;
          ELSE IF PPROT ^= '''PWWRITE''' THEN
                  NOTE('INVALID PPROT ' ×× PPROT,8);
          RETURN ('DO; @9_PPROT.@9P  = ''' ×× R1 ××
             '''B; @9I = @9I + 1; @9TUPL(@9I) = ADDR(@9_PPROT); END;');
       END;
    ELSE
       RETURN('DO; @9_PPROT.@9P  = ' ×× PPROT ×× '; @9I = @9I + 1; @9TUP
 L(@9I) = ADDR(@9_PPROT); END;');
 %END @9PPROT;
 %ACTIVATE @9PPROT;
 DCL 01 @9_PPROT UNALIGNED, 05 @9K FIXED BIN(15) INIT(32), 05 @9# FIXED
     BIN(15) INIT(1), 05 @9L FIXED BIN(15) INIT(1), 05 @9P  BIT(8);
