         PRENPT,PREPPT = 0;
   /* GENERALIZED CODE TO ENQUEUE A NEW MEMBER
      ON A DOUBLY LINKED LIST */
         % PRELNO = PRELNO + 1;
         % PRECHA = SUBSTR(PRELNO,8,1);
         % PREQCR = 'SETQCR' ×× PRECHA;
         %PREFNDSL = 'FINDSLOT' ×× PRECHA;
         %PREENSL = 'ENDSLOT' ×× PRECHA;
         %PRENXT = 'NXT' ×× PRECHA;
         % PREGTSL = 'GOTSLOT' ×× PRECHA;
         % PREWREN = 'WRITDAM' ×× PRECHA;
         % IF PRENQD = ''
              % THEN %PRENQD = PREWREN;
         IF PRERUT ^= 0
              THEN GO TO PREFNDSL;
 PREQCR:
         PRERUT = PREDAM;
         %IF PRERUT = 'ISAM.DAMASTER'
              %THEN %DO;
         FUNCTION = 'REPL';
         ISAMKEY = DSN;
         CALL ISAMIO(ISAMWRK,ISAMIN);
              %END;
         GO TO PREWREN;
 PREFNDSL:
         BDAMKEYA = PRERUT;
 PRENXT:
         READ FILE(BDAMFLE) INTO(BDAMREC) KEY(BDAMKEY);
         IF BDAM -> PREKEY = PREKEY
              THEN GO TO PRENQD;
         IF BDAM -> PREKEY > PREKEY
              THEN GO TO PREGTSL;
         PREPPT = BDAM -> PREDAM;
         IF BDAM -> PRENPT = 0
              THEN GO TO PREENSL;
         BDAMKEYA = BDAM -> PRENPT;
         GO TO PRENXT;
 PREENSL:
         BDAM -> PRENPT = PREDAM;
         BDAMKEYA = BDAM -> PREDAM;
         WRITE FILE(BDAMFLE) FROM(BDAMREC) KEYFROM(BDAMKEY);
         GO TO PREWREN;
 PREGTSL:
         PRENPT = BDAM -> PREDAM;
         BDAM -> PREPPT = PREDAM;
         BDAMKEYA = BDAM -> PREDAM;
         WRITE FILE(BDAMFLE) FROM(BDAMREC) KEYFROM(BDAMKEY);
         IF PREPPT = 0
              THEN GO TO PREQCR;
         BDAMKEYA = PREPPT;
         READ FILE(BDAMFLE) INTO(BDAMREC) KEY(BDAMKEY);
         BDAM -> PRENPT = PREDAM;
         WRITE FILE(BDAMFLE) FROM(BDAMREC) KEYFROM(BDAMKEY);
 PREWREN:
