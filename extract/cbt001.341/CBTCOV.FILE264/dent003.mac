 /* DENT003 - X,XF,XB,HELP,H,EXEC,EX,XT SUBCOMMANDS */
         % DCL @PROCNAME CHAR;
         % DCL @RENTNAME CHAR;
         % @PROCNAME = 'RENT';
         % @RENTNAME = 'DENT003';
         % INCLUDE DENTSUB;
         % INCLUDE IKJECT;
         DCL SETCAPS ENTRY;
         DCL PREFIX ENTRY(CHAR(*),CHAR(*))
           RETURNS(FIXED BIN(15,0));
         DCL EXNAME CHAR(8) VAR;
         DCL HELPSTR CHAR(4) VAR INIT('');
         DCL CMDSTR CHAR(240) VAR INIT('');
         DCL PROMPT CHAR(80) VAR INIT('SSEX');
         DCL KEYTABLE(3) CHAR(80) VAR STATIC INIT(
           'PROMPT','NEW','CONTINUE');
         DCL INDX FIXED BIN(15,0);
         DCL OPER CHAR(80) VAR;
         DCL KEYLAB(0:3) LABEL;
         DCL (SAVPTR,TRH) PTR;
         DCL (NSW,LOGSW) BIT(1) INIT('0'B);
         DCL TERMSW BIT(1);
         DCL STELP PTR BASED(STPTR);
         DCL STEL BIT(1) BASED(STELP);
         DCL CMDLAB(9) LABEL;
         CALL #CRTZAP(RHPPTR); /* CALL CRTZAPS - CLEAR SCREEN */
         SAVPTR = RECPTR;
         GO TO CMDLAB(CMDINDX);
 CMDLAB(5):  /* HELP */
 CMDLAB(6):  /* H */
         HELPSTR,PROMPT = 'HELP';
         IF PARY(2)=''
           THEN CMDSTR='HELP';
           ELSE DO;
             CMDSTR='HELP '××
               PARY(2)××' '××
               PARY(3)××' '××
               PARY(4)××' ';
             END;
         GO TO X;
 CMDLAB(9):  /* XT */
         CALL #STACK('');
 CMDLAB(1):  /* DENT003 */
 CMDLAB(2):  /* X */
 CMDLAB(3):  /* XF */
 CMDLAB(4):  /* XB */
         IF PARY(2) ^= ''
           THEN DO;
             LOGSW = '1'B;
             NSW = '0'B; /* SET DEFAULT VALUE BEFORE PARSE */
             DO I = 2 TO 16 WHILE(PARY(I)^='');
               CALL SETCAPS(PARY(I));
               CALL #SPARS(PARY(I),KEYTABLE,OPER,INDX);
                 /* CALL SPARS */
               GO TO KEYLAB(INDX);
 KEYLAB(0):  /* INVALID OPERAND */
               LOGSW = '0'B;
               CMDSTR = PARY(2);
               GO TO X;
 KEYLAB(1):  /* PROMPT */
               PROMPT = OPER;
               GO TO ENDCASE;
 KEYLAB(2):  /* NEW */
               NSW = '1'B;
               GO TO ENDCASE;
 KEYLAB(3):  /* CONTINUE */
               NSW = '0'B;
 ENDCASE:
               END;
             IF NSW
               THEN DO;
                 PARY(*) = '';
                 PARY(4) = 'SUBCALL';
                 CALL #DENT(CMDINDX,PARPTR,RHPTR,SUBENT,RHHDRPT);
                   /* CALL DENTSI */
                 SAVPTR = NULL;
                 END;
               ELSE CALL #NC(RHPTR,SUBENT,LSTHDR);
                 /* CALL DENTNC */
             END;
 X:
         IF CMDINDX^=9
           THEN STFSMODE='0'B;
         DO FOREVER=1 REPEAT FOREVER;
           TERMSW=RHECT->ECTIOWA->STELP->STEL;
             /* TERMSW = 0 IF CLIST IS ACTIVE */
           IF CMDSTR=''
             THEN CMDSTR=#PROMPT(PROMPT); /* CALL DOPRM */
           IF CMDSTR=''
             THEN DO;
               IF SAVPTR=NULL
                 THEN SAVPTR=NXTHDR;
               CALL #NC(RHPTR,SUBENT,SAVPTR);
                   /* CALL DENTNC */
               RHRC = 0;
               RETURN;
               END;
           CMDSTR=SUBSTR(CMDSTR,PREFIX(CMDSTR,' '));
             /* ELIMINATE LEADING BLANKS */
           IF TERMSW & LOGSW
             THEN DO;
               IF LSTHDR = HDPTR
                 THEN ADDSEQ=1;
                 ELSE ADDSEQ=LSTHDR->RECSEQ+1;
               CALL #ADD(RHPTR,SUBENT,RECPTR,RECPTR,
                 ADDSEQ,CMDSTR,RHPPTR);  /* CALL ADDREC */
               END;
           CALL #DOCMD(CMDSTR,HELPSTR); /* CALL DOCMD */
           CMDSTR = '';
           END;
         RHRC = 0;
         RETURN;
 CMDLAB(7):  /* EXEC */
 CMDLAB(8): /* EX */
         L1 = PARY(2);
         IF PARY(3)=''
           THEN DO;
             EXNAME='';
             L2 = L1;
             END;
           ELSE IF PARY(4)=''
             THEN DO;
               IF VERIFY(PARY(3),'0123456789')=0
                 THEN DO;
                   EXNAME='';
                   L2 = PARY(3);
                   END;
                 ELSE DO;
                   EXNAME=PARY(3);
                   L2 = L1;
                   END;
               END;
             ELSE DO;
               EXNAME=PARY(4);
               L2 = PARY(3);
               END;
         IF EXNAME=''
           THEN TRH=HDPTR;
           ELSE DO;
             DO TRH=NXTRHEL REPEAT TRH->RHNXT WHILE(TRH^=NULL)
               UNTIL(TRH->RHNAME=EXNAME);
               END;
             IF TRH=NULL
               THEN DO;
                 MSGDATA='NAME '××EXNAME××' NOT FOUND';
                 RHRC = 8;
                 RETURN;
                 END;
             END;
         SAVPTR = RECPTR;
         CALL #MD(TRH,SUBENT,L2); /* CALL MDPROC */
         IF TRH->RECPTR^=TRH
           THEN DO RECPTR=TRH->RECPTR REPEAT LSTPTR WHILE(RECSEQ>=L1)
             UNTIL(LSTPTR=TRH);
             CALL #STACK(RECSTR);
             END;
         CALL #NC(RHPTR,SUBENT,LSTHDR);
                 /* CALL DENTNC */
         CMDSTR = '';
         LOGSW = '1'B;
         GO TO X;
         END;
