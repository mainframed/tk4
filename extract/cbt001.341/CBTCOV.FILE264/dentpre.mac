 /* DENTPRE */
 /* THIS MODULE CONTAINS THE CODE FOR #PRED AND #SUCC. */
 DENTPRE:  PROC(RHHDRPT,RHPTR,CURPTR) RETURNS(PTR UNALIGNED)
   OPTIONS(REENTRANT);
   % DCL @PROCNAME CHAR;
   % @PROCNAME='MAIN';
   % INCLUDE DENTSUB;
   DCL CURPTR PTR UNALIGNED;
   DCL NULL BUILTIN;
   DCL DUM(2) ENTRY;
   IF RHFAMSW
     THEN RETURN(RHFAMEP(RHHDRPT,RHPTR,DUM,CURPTR,2));
     ELSE IF CURPTR=NULL
       THEN RETURN(RHPTR->LSTHDR);
       ELSE RETURN(CURPTR->LSTPTR);
 DENTSUC: ENTRY(RHHDRPT,RHPTR,CURPTR) RETURNS(PTR) OPTIONS(REENTRANT);
   IF RHFAMSW
     THEN RETURN(RHFAMEP(RHHDRPT,RHPTR,DUM,CURPTR,1));
     ELSE IF CURPTR=NULL
       THEN RETURN(RHPTR->NXTHDR);
       ELSE RETURN(CURPTR->NXTPTR);
   END DENTPRE;
