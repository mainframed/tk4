 /* SVC99 INTERFACE - WRITTEN 10/25/80 BY PAUL DIXON */;
 %DCL (@9CDISP_UNCATLG,@9CDISP_CATLG,@9CDISP_DELETE,@9CDISP_KEEP) CHAR;
 %@9CDISP_UNCATLG = '''00000001''B';
 %@9CDISP_CATLG   = '''00000010''B';
 %@9CDISP_DELETE  = '''00000100''B';
 %@9CDISP_KEEP    = '''00001000''B';
 %@9CDISP: PROC(CDISP) RETURNS(CHAR) STATEMENT;
 DCL (CDISP,R1) CHAR;
 IF SUBSTR(CDISP,1,1) = ''''
    THEN
       DO;
          R1 = @9CDISP_KEEP;
          IF      CDISP  = '''UNCATLG''' THEN R1 = @9CDISP_UNCATLG;
          ELSE IF CDISP  = '''CATLG'''   THEN R1 = @9CDISP_CATLG;
          ELSE IF CDISP  = '''DELETE'''  THEN R1 = @9CDISP_DELETE;
          ELSE IF CDISP ^= '''KEEP'''    THEN
                  NOTE('INVALID CDISP ' ×× CDISP,8);
          RETURN ('DO; @9_CDISP.@9P  = ' ×× R1 ××
             '; @9I = @9I + 1; @9TUPL(@9I) = ADDR(@9_CDISP); END;');
       END;
    ELSE
       RETURN('DO; @9_CDISP.@9P  = ' ×× CDISP ×× '; @9I = @9I + 1; @9TUP
 L(@9I) = ADDR(@9_CDISP); END;');
 %END @9CDISP;
 %ACTIVATE @9CDISP;
 DCL 01 @9_CDISP UNALIGNED, 05 @9K FIXED BIN(15) INIT(6), 05 @9# FIXED
     BIN(15) INIT(1), 05 @9L FIXED BIN(15) INIT(1), 05 @9P  BIT(8);
