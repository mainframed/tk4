//TT222X21 JOB 2218,'13F/AAA/ASM$DUMY',CLASS=S,TIME=5,
//             MSGLEVEL=(1,1),MSGCLASS=M,NOTIFY=TS13AAA,PERFORM=27,
//             RESTART=*
/*JOBPARM      LINES=020,COPIES=007,S=(381A,383B)
/*ROUTE PRINT  TECH
//*************************************************************
//*                                                           *
//*         THIS JOB ASSEMBLES AND LINK-EDITS THE             *
//*         $ORTDUMY DUMMY MODULE FOR THE SYNCSORT PARMEXIT   *
//*         FACILITY.  AS A FINAL STEP, THE EXIT IS COPIED    *
//*         TO SYS1.XAPPLINK AND RENAMED TO $ORTPARM TO ALLOW *
//*         DISABLING THE EXIT FOR A SINGLE SYSTEM.           *
//*                                                           *
//*         TO REENABLE THE EXIT FOR THAT SYSTEM, RENAME      *
//*         $ORTPARM IN SYS1.XAPPLINK TO $ORTDUMY; THIS       *
//*         WILL REESTABLISH ACCESS TO $ORTPARM IN            *
//*         SYS1.XAPPLINK. IF $ORTPARM IS LISTED IN           *
//*         IEABLD00, AN IPL IS REQUIRED TO REESTABLISH       *
//*         ACCESS.                                           *
//*                                                           *
//*                                               07/06/81 TJA*
//*                                                           *
//*         JCL MODIFIED: VOL=SER=DLBX21,UNIT=ALLDA           *
//*                       LAST STEP: COPY TO SYS1.XAPPLINK    *
//*                                               12/01/83 AAA*
//*************************************************************
//ASM$DUMY EXEC PGM=IFOX00,REGION=4096K,
//             PARM='DECK,NOOBJ,BUFSIZE(MAX),WORKSIZE(3584K)'
//*
//*            ASSEMBLE  $ORTDUMY FROM SYS1.CUSRC
//*
//SYSIN    DD  DISP=SHR,UNIT=ALLDA,
//             VOL=SER=DLBX21,DSN=SYS1.CUSRC($ORTDUMY)
//SYSLIB   DD  DISP=SHR,UNIT=ALLDA,VOL=SER=DLBX21,DSN=SYS1.AMACLIB
//         DD  DISP=SHR,UNIT=ALLDA,VOL=SER=DLBX21,DSN=SYS1.AMODGEN
//         DD  DISP=SHR,UNIT=ALLDA,VOL=SER=DLBX21,DSN=SYS1.ATSOMAC
//SYSPRINT DD  SYSOUT=*
//SYSPUNCH DD  DISP=(NEW,PASS),DSN=&&LOADSET,
//             UNIT=SYSDA,SPACE=(CYL,(002,001)),
//             DCB=(RECFM=FB,LRECL=80,BLKSIZE=2960)
//SYSUT1   DD  UNIT=VSYSDA,SPACE=(CYL,(5,1))
//SYSUT2   DD  UNIT=VSYSDA,SPACE=(CYL,(5,1))
//SYSUT3   DD  UNIT=VSYSDA,SPACE=(CYL,(5,1))
//LKEDDMY EXEC PGM=IEWL,REGION=1536K,COND=(4,LT,ASM$DUMY),
//             PARM='LIST,XREF,NCAL,SIZE=(1024K,128K)'
//*
//*            LINK EDIT $ORTDUMY TO SYS1.CULIB
//*
//SYSLIN   DD  DISP=(OLD,DELETE),DSN=&&LOADSET
//SYSLMOD  DD  DISP=SHR,UNIT=ALLDA,
//             VOL=SER=DLBX21,DSN=SYS1.CULIB($ORTDUMY)
//SYSPRINT DD  SYSOUT=*
//SYSUT1   DD  UNIT=SYSDA,SPACE=(CYL,(2,1))
//COMPRS  EXEC PGM=IEBCOPY,REGION=4096K
//*
//*            COMPRESS LIBRARIES WHEN FINISHED.
//*
//CUSRC    DD  DISP=OLD,UNIT=ALLDA,VOL=SER=DLBX21,DSN=SYS1.CUSRC
//CULIB    DD  DISP=OLD,UNIT=ALLDA,VOL=SER=DLBX21,DSN=SYS1.CULIB
//SYSUT3   DD  UNIT=SYSDA,SPACE=(CYL,(5,1))
//SYSUT4   DD  UNIT=SYSDA,SPACE=(CYL,(5,1))
//SYSPRINT DD  SYSOUT=*
//SYSIN    DD  *
         COPY  OUTDD=CUSRC,INDD=CUSRC       COMPRESS SYS1.CUSRC
         COPY  OUTDD=CULIB,INDD=CULIB       COMPRESS SYS1.CULIB
/*
//
//TOLINK   EXEC PGM=IEBCOPY,REGION=4096K,                              X
//             COND=((0,LT,ASM$DUMY),(0,LT,LKEDDMY))
//*
//*            COPY $ORTDUMY TO SYS1.XAPPLINK
//*            AND RENAME IT TO $ORTPARM
//*
//SYSUT1   DD  DISP=OLD,UNIT=ALLDA,VOL=SER=DLBX21,DSN=SYS1.CULIB
//SYSUT2   DD  DISP=OLD,UNIT=ALLDA,VOL=SER=SYSPP1,DSN=SYS1.XAPPLINK
//SYSUT3   DD  UNIT=SYSDA,SPACE=(CYL,(5,1))
//SYSUT4   DD  UNIT=SYSDA,SPACE=(CYL,(5,1))
//SYSPRINT DD  SYSOUT=*
//SYSIN    DD  *
         COPY  OUTDD=SYSUT2,INDD=SYSUT1     COPY $ORTDUMY
         S     M=(($ORTDUMY,$ORTPARM,R))    AND RENAME IT TO $ORTPARM
/*
//
