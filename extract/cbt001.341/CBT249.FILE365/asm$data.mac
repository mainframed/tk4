//TT222X21 JOB 2213,'13F/TJA/ASM$DATA',CLASS=S,TIME=5,
//             MSGLEVEL=(1,1),MSGCLASS=M,NOTIFY=TS13TJA,PERFORM=27,
//             RESTART=*
/*JOBPARM      COPIES=007,LINES=020,S=(381A,383B)
/*ROUTE PRINT  TECH
//*************************************************************
//*                                                           *
//*         THIS JOB ASSEMBLES AND LINK-EDITS THE             *
//*         $ORTDATA TABLE MODULE FOR THE SYNCSORT PARMEXIT   *
//*         FACILITY.                                         *
//*                                                           *
//*                                               03/20/84 TJA*
//*************************************************************
//ASM$DATA EXEC PGM=IEV90,REGION=4096K,PARM='DECK,NOOBJECT'
//*
//*            ASSEMBLE  $ORTDATA FROM SYS1.CUSRC
//*
//SYSIN    DD  DISP=SHR,UNIT=ALLDA,
//             VOL=SER=DLBX21,DSN=SYS1.CUSRC($ORTDATA)
//SYSLIB   DD  DISP=SHR,UNIT=ALLDA,VOL=SER=DLBX21,DSN=SYS1.CUMAC,
//             DCB=BLKSIZE=19040
//         DD  DISP=SHR,UNIT=ALLDA,VOL=SER=DLBX21,DSN=SYS1.AMACLIB
//         DD  DISP=SHR,UNIT=ALLDA,VOL=SER=DLBX21,DSN=SYS1.AMODGEN
//         DD  DISP=SHR,UNIT=ALLDA,VOL=SER=DLBX21,DSN=SYS1.ATSOMAC
//SYSPRINT DD  SYSOUT=*
//SYSPUNCH DD  DISP=(NEW,PASS),DSN=&&LOADSET,
//             UNIT=SYSDA,SPACE=(CYL,(002,001)),
//             DCB=(RECFM=FB,LRECL=80,BLKSIZE=2960)
//SYSUT1   DD  UNIT=VSYSDA,SPACE=(CYL,(5,1))
//SYSUT2   DD  UNIT=VSYSDA,SPACE=(CYL,(5,1))
//SYSUT3   DD  UNIT=VSYSDA,SPACE=(CYL,(5,1))
//*
//LKDDATA EXEC PGM=IEWL,REGION=1536K,COND=((4,LT,ASM$DATA)),
//             PARM='LIST,XREF,NCAL,RENT,REFR,OL,SIZE=(1024K,128K)'
//*
//*            LINK EDIT $ORTDATA TO SYS1.CULIB
//*
//STEPLIB  DD  DISP=SHR,UNIT=ALLDA,VOL=SER=SYSPKZ,DSN=SYS1.LINKLIB
//SYSLIN   DD  DISP=(OLD,DELETE),DSN=&&LOADSET
//SYSLMOD  DD  DISP=SHR,UNIT=ALLDA,
//             VOL=SER=DLBX21,DSN=SYS1.CULIB($ORTDATA)
//SYSPRINT DD  SYSOUT=*
//SYSUT1   DD  UNIT=SYSDA,SPACE=(CYL,(2,1))
//COMPRS  EXEC PGM=IEBCOPY,REGION=4096K
//*
//*            COMPRESS LIBRARIES WHEN FINISHED.
//*
//CUMAC    DD  DISP=SHR,UNIT=ALLDA,VOL=SER=DLBX21,DSN=SYS1.CUMAC
//CUSRC    DD  DISP=SHR,UNIT=ALLDA,VOL=SER=DLBX21,DSN=SYS1.CUSRC
//CULIB    DD  DISP=SHR,UNIT=ALLDA,VOL=SER=DLBX21,DSN=SYS1.CULIB
//SYSUT3   DD  UNIT=SYSDA,SPACE=(CYL,(5,1))
//SYSUT4   DD  UNIT=SYSDA,SPACE=(CYL,(5,1))
//SYSPRINT DD  SYSOUT=*
//SYSIN    DD  *
         COPY  OUTDD=CUMAC,INDD=CUMAC       COMPRESS SYS1.CUMAC
         COPY  OUTDD=CUSRC,INDD=CUSRC       COMPRESS SYS1.CUSRC
         COPY  OUTDD=CULIB,INDD=CULIB       COMPRESS SYS1.CULIB
/*
//
