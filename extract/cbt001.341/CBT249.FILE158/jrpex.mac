         MACRO
&TAG     JRPEX   &REG,&INSTR
         LCLB  &ERROR
.CKREG   AIF   (T'&REG NE 'O').CKINSTR
         MNOTE 8,'REGISTER OPERAND OMITTED; REQUIRED'
&ERROR   SETB  1
.CKINSTR AIF   (T'&INSTR NE 'O').EXPAND
         MNOTE 8,'INSTRUCTION OPERAND OMITTED; REQUIRED'
&ERROR   SETB  1
.EXPAND  AIF   (&ERROR).MEND
         SPACE 1
*        WARNING:  REGISTER 2 WILL BE DESTROYED
*                  &REG OPERAND WILL BE SHIFTED LEFT 24 BITS
         SPACE 1
&TAG     LA    2,&INSTR                     LOAD INST ADDR
         SLL   &REG,24                      SHIFT MASK TO HIGH ORDER
         OR    2,&REG                       COMBINE WITH ADDR
         SVC   238                          ISSUE SUPEREX SVC
         SPACE 1
.MEND    MEND
