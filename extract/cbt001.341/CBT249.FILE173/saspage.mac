TITLE1 STUDY OF PAGING FROM OMF 3 RECORDS;
DATA OUT.PGS (DROP=ID TIME  );
INFILE IN;
INPUT @1 ID $4. @;
IF ID^='OMF3' THEN GO TO DONE;
INPUT @5 DATE PD4.
  TIME PD4. @;
HOUR=FLOOR(TIME/10000)+FLOOR(MOD(TIME,10000)/100)/60;
INPUT @25
 NPIN PIB4.
 NPOUT PIB4.
 VAMI PIB4.
 VAMO PIB4.
 VAMR PIB4.
 SPIN PIB4.
 SPOUT PIB4.
 NPREC PIB4.
 NSWPS PIB4.
 CAIN PIB4.
 CAOUT PIB4.
 CAREC PIB4.
 SPREC PIB4.
 TOSC PIB4.
 TISC PIB4.
 LWSC PIB4.
 XSSC PIB4.
 RSSC PIB4.
 DWSC PIB4.
 RQSC PIB4.
 NQSC PIB4.
 EXSC PIB4.
 USSC PIB4.
 @;
NPAGING =( NPIN + NPOUT)/900;
VPAGING =( VAMI + VAMO)/900;
SPAGING =( SPIN + SPOUT)/900;
CPAGING=( CAIN + CAOUT)/900;
SWAPPING=(NSWPS)/900;
PTR = (NPIN + NPREC + CAIN + CAREC )/900;
LABEL NPIN=PAGEINS EXCLUDING SWAPS & VIO
      NPOUT=PAGEOUTS EXLUDING SWAPS & VIO
      VAMI=VIO PAGE INS
      VAMO=VIO PAGE OUTS
      VAMR=VIO RECLAIMS
      SPIN=PAGES SWAPPED IN
      SPOUT=PAGES SWAPPED OUT
      NPREC=RECLAIMS EXCLUDING SWAP & VIO
      CAIN=COMMON AREA PAGE INS
      CAOUT=COMMON AREA PAGE OUTS
      CAREC=NUMBER OF RECLAIMS OF COMMON AREA PAGES
      SPREC=PAGES RECLAIMED ACROSS SWAPS
      TOSC=TERMINAL OUTPUT SWAP COUNT
      TISC=TERMINAL INPUT SWAP COUNT
      LWSC=LONG WAIT SWAP COUNT
      XSSC=AUX STOR SHORTAGE SWAP COUNT
      RSSC=REAL STOR SHORTAGE SWAP COUNT
      DWSC=DETECTED WAIT SWAP COUNT
      RQSC=REQSWAP SWAP COUNT
      NQSC=CAP ENQ EXCHANGE SWAP COUNT
      EXSC=EXCHANGES ON RECOMM VALUE SWAP COUNT
      USSC=UNILATERAL SWAP OUT COUNT;
LABEL NPAGING=PAGEINS+PAGEOUTS EXCLUDING SWAP&VIO
      VPAGING=VIO PAGEINS+PAGEOUTS
      SPAGING=SWAP PAGEINS+PAGEOUTS
      PTR=PAGEINS+RECLAIMS(PAGE FAULTS)
      CPAGING=COMMON AREA PAGEINS + PAGEOUTS;
OUTPUT OUT.PGS;
DONE: INPUT; DELETE;
RUN; PAGE;
PROC PRINT; BY DATE;
PROC SCATTER; BY DATE;
PLOT HOUR*NPAGING='N'
 HOUR*VPAGING='V'
 HOUR*SPAGING='S'
 HOUR*PTR='P'
 HOUR*CPAGING='C' / OVERLAY HMIN=0 HMAX=24 HINC=1;
