         PRINT NOGEN
         DMSMACRO TYPE=DSECT
         PRINT ON,GEN
DAT0     TITLE 'D M S D A T 0  ADDRESS EXCEPTION DATA SAVE ROUTINE'
         SPACE 3
* NOTE THAT THIS ROUTINE DOES NOT NEED A BASE REGISTER ASSIGNMENT
         SPACE 3
DMSDAT0  CSECT
         USING *,R15
         USING DMSCONST,DMSREG
         USING PSA,PSAREG
         LA    R0,4                     LENGTH OF DATA
         LA    R1,FLCTEA(R0)            ADDRESS OF TRANSLATION EXCEPT.
         BALR  R12,R13                  SAVE IT
         SR    R15,R15                  INDICATE RECORD TO BE SAVED
         BR    R14                      RETURN
         CSECTID DMSDAT0
         END
