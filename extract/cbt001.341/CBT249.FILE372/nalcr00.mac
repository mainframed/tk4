//NALCR00 JOB ,'ALLOC RECOVERY MSGS',CLASS=9,TYPRUN=HOLD
/*PROCLIB NER.SP1IPO.CNTL
/*JOBPARM Q=F,I
//*
//ASM     EXEC PGM=IFOX00,REGION=256K,PARM='NODECK,NOLOAD,NOALIGN'
//SYSLIB   DD  DSN=SYS1.SMPMTS,DISP=SHR,DCB=BLKSIZE=6400,
//             VOL=SER=IPORES,UNIT=3330-1
//         DD  DSN=SYS1.AMODGEN,DISP=SHR,
//             VOL=SER=IPODLB,UNIT=3330-1
//         DD  DSN=SYS1.MACLIB,DISP=SHR,
//             VOL=SER=IPORES,UNIT=3330-1
//SYSUT1   DD  UNIT=SYSDA,SPACE=(CYL,(1,1)),DCB=BLKSIZE=1700
//SYSUT2   DD  UNIT=SYSDA,SPACE=(CYL,(1,1)),DCB=BLKSIZE=1700
//SYSUT3   DD  UNIT=SYSDA,SPACE=(CYL,(1,1)),DCB=BLKSIZE=1700
//SYSPRINT DD  DSN=&&LIST,DISP=(,PASS),UNIT=SYSDA,SPACE=(TRK,(20,20))
//SYSIN    DD  *
*ZAP CARD ++USERMOD(NALCR00)     /*
*ZAP CARD   CHANGE THE TEXT OF MESSAGES ISSUED DURING ALLOCATION
*ZAP CARD   RECOVERY TO HELP REDUCE OPERATOR SCREW UPS.
*ZAP CARD
*ZAP CARD   ORIGINAL MESSAGE:
*ZAP CARD      "IEF433D JOBNAMEX - WAIT REQUESTED --
*ZAP CARD               REPLY 'HOLD' OR 'NOHOLD'."
*ZAP CARD   NEW MESSAGE:
*ZAP CARD      "IEF433D JOBNAMEX - WAIT REQUESTED --
*ZAP CARD               REPLY 'NOHOLD'. (NALCR00)"
*ZAP CARD
*ZAP CARD
*ZAP CARD   ORIGINAL MESSAGE:
*ZAP CARD      "IEF434D JOBNAMEX - INVALID REPLY.
*ZAP CARD               REPLY 'HOLD' OR 'NOHOLD'."
*ZAP CARD   NEW MESSAGE:
*ZAP CARD      "IEF434D JOBNAMEX - INVALID REPLY.
*ZAP CARD               REPLY 'NOHOLD'. (NALCR00)"
*ZAP CARD
*ZAP CARD                                         */   .
*ZAP CARD ++VER(Z038) FMID(EBB1102) .
*ZAP CARD ++ZAP(IEFAB4M9) .
*ZAP CARD  NAME IEFAB4M9
         EJECT
IEFAB4M9 CSECT
         SPACE 4
IEF433D  EQU   IEFAB4M9+X'0264'        IEF443D MESSAGE TEXT LOCATION
IEF434D  EQU   IEFAB4M9+X'02A4'        IEF434D MESSAGE TEXT LOCATION
         SPACE 5
         PRINT DATA
         EJECT
*ZAP CARD * IEF433D ALLOCATION MESSAGE
*ZAP START VER
         ORG   IEF433D
         DC    CL37'IEF433D JOBNAMEX - WAIT REQUESTED -- '
         DC    CL25'REPLY ''HOLD'' OR ''NOHOLD''.'
*ZAP START REP
         ORG   IEF433D
         DC    CL37'IEF433D JOBNAMEX - WAIT REQUESTED -- '
         DC    CL25'REPLY ''NOHOLD''. (NALCR00)'
         EJECT
*ZAP CARD * IEF434D ALLOCATION MESSAGE
*ZAP START VER
         ORG   IEF434D
         DC    CL33'IEF434D JOBNAMEX - INVALID REPLY.'
         DC    CL25'REPLY ''HOLD'' OR ''NOHOLD''.'
*ZAP START REP
         ORG   IEF434D
         DC    CL33'IEF434D JOBNAMEX - INVALID REPLY.'
         DC    CL25'REPLY ''NOHOLD''. (NALCR00)'
         EJECT
*ZAP CARD  IDRDATA NALCR00
*ZAP END
         END
//*
//LIST1   EXEC PGM=IEBGENER
//SYSPRINT DD  DUMMY
//SYSIN    DD  DUMMY
//SYSUT1   DD  DSN=&&LIST,DISP=(OLD,PASS)
//SYSUT2   DD  SYSOUT=A
//*
//ZAPPER  EXEC PL1XCG,TEST=
//PL1.SYSIN DD DSN=NER.SP1IPO.CNTL(ASMTOZAP),DISP=SHR
//GO.IN    DD  DSN=&&LIST,DISP=(OLD,DELETE)
//OUT      DD  DSN=&&PTFIN,DISP=(NEW,PASS),
//             SPACE=(TRK,(1,1)),UNIT=SYSDA,
//             DCB=(RECFM=FB,LRECL=80,BLKSIZE=3120)
//*
//LIST2   EXEC PGM=IEBGENER
//SYSPRINT DD  DUMMY
//SYSIN    DD  DUMMY
//SYSUT1   DD  DSN=&&PTFIN,DISP=(OLD,PASS)
//SYSUT2   DD  SYSOUT=A
//*
//ZAP     EXEC SMP4APLY,COND=(0,LT,ASM)
//SMPCNTL  DD *
 RESTORE S(NALCR00) .
 RESETRC .
 REJECT  S(NALCR00) .
 RESETRC .
 RECEIVE .
 APPLY   S(NALCR00) DIS(NO) .
//SMPPTFIN DD  DSN=&&PTFIN,DISP=(OLD,DELETE)
