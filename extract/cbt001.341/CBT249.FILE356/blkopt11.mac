)ATTR
  @ TYPE(OUTPUT) INTENS(HIGH)
)BODY
%------------------------- FILE ALLOCATION DATA ------------------------
%
+OPTIMUM BLOCKING FACTOR ===>@BLKSIZE
+
+RECORDS PER BLOCK       ===>@RBLK
+RECORDS PER TRACK       ===>@RTRK
+RECORDS PER CYLINDER    ===>@RCYL
+
+
+                                BLKS           TRKS           CYLS
+RECORD CAPACITY 1ST EXTENT ===>@BLKEXT1  +===>@TRKEXT1  +===>@CYLEXT1
+TOTAL RECORD CAPACITY      ===>@BLKTOT   +===>@TRKTOT   +===>@CYLTOT
+
+DO YOU WANT YOUR SPACE IN TRACKS, CYLINDERS, OR BLOCKS ??
+(BLOCKS ARE DEFAULT, ENTER C,T, OR B) ===>_TYPE+
+
)INIT
  .HELP = TBLK11
  .CURSOR = TYPE
  &EXIT = NO
  &FLAG = 1
  &TYPE = B
)PROC
    IF (.RESP = END)
       &EXIT = YES
    VER (&TYPE,LIST,CYL,TRK,BLK,C,B,T)
    &TYPE = TRUNC (&TYPE,1)
)END
