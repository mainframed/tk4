PROC 0 DEBUG
/*--------------------------------------------------------*/
/*      ISPF/PDF CLIST FOR SADSC PANEL TO INVOKE          */
/*      OMEGAMON.                                         */
/*--------------------------------------------------------*/
CONTROL NOMSG PROMPT NOFLUSH NOLIST
IF &DEBUG NE THEN CONTROL LIST MSG CONLIST SYMLIST
ISPEXEC CONTROL ERRORS RETURN
SET &PF      = &STR(09)
SET &RW      = &STR(24)
SET &LR      = &STR(24)
SET &CL      = &STR(80)
SET &PREF    = &STR(S354)
SET &MAXCC   = 0
SET &LASTCC  = 0
ISPEXEC DISPLAY PANEL(OMEG#P)
IF &LASTCC NE 0 THEN  EXIT
ISPEXEC VGET (&PF &RW &LR &CL &PREF)
SET &LASTCC = 0
SET &MAXCC  = 0
FREE FI(OMHELP)
FREE FI(OMPROC)
FREE FI(OMPROCSV)
FREE FI(OMTUTR)
FREE DA('&PREF..XM.LOAD')
ALLOC FI(OMHELP)    DA('&PREF..XM.HELP')  SHR
ALLOC FI(OMPROC)    DA('&PREF..XM.PROC')  SHR
ALLOC FI(OMPROCSV)  DA('&PREF..XM.PROCNEW')  SHR
ALLOC FI(OMTUTR)    DA('&PREF..XM.TUTR')  SHR
ISPEXEC  FTOPEN   TEMP
ISPEXEC  FTINCL   OMEG#S
ISPEXEC  FTCLOSE
ISPEXEC  VGET     ZTEMPF
CONTROL NOMSG
EX    '&ZTEMPF'
CONTROL MSG
FREE FI(OMHELP)
FREE FI(OMPROC)
FREE FI(OMPROCSV)
FREE FI(OMTUTR)
FREE DA('&PREF..XM.LOAD')
END
