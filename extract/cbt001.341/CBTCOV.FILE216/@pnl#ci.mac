PROC 0  DEBUG
/*-------------------------------------------------------------*/
/*      OPEN THE TOOL BOX                                      */
/*-------------------------------------------------------------*/
CONTROL NOLIST MSG PROMPT NOFLUSH
IF &DEBUG NE &STR() THEN +
             CONTROL LIST CONLIST SYMLIST
SET &GODS = &STR($354)
SET &ID   = &SUBSTR(1:4,&SYSUID)
IF  &SYSUID EQ &STR(RSWILSO) THEN GOTO TOOLBOX
IF  &SYSUID EQ &STR(MLSKINN) THEN GOTO TOOLBOX
IF  &SYSUID EQ &STR(JDMARSH) THEN GOTO TOOLBOX
IF  &SYSUID EQ &STR(PJWALDO) THEN GOTO TOOLBOX
IF  &SYSUID EQ &STR(ACBARRE) THEN GOTO TOOLBOX
IF  &SYSUID EQ &STR(VJALEXA) THEN GOTO TOOLBOX
IF  &SYSUID EQ &STR(FEMILLE) THEN GOTO TOOLBOX
IF  &SYSUID EQ &STR(DTHUPFE) THEN GOTO TOOLBOX
IF  &SYSUID EQ &STR(BAFOXHA) THEN GOTO TOOLBOX
IF  &SYSUID EQ &STR(BAHORAN) THEN GOTO TOOLBOX
IF  &SYSUID EQ &STR(WJBECKE) THEN GOTO TOOLBOX
IF  &SYSUID EQ &STR(DMGRYGI) THEN GOTO TOOLBOX
IF  &ID     EQ &GODS  THEN GOTO TOOLBOX
   ISPEXEC SELECT  PANEL(@PNL#PI)
EXIT
TOOLBOX: +
   ISPEXEC SELECT  PANEL(@PNL#PX)
EXIT
