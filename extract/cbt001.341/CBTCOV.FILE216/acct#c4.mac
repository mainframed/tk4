PROC 0
/*--------------------------------------------------------*/
/*      ISPF/PDF CLIST FOR SADSC PANELS FOR USE           */
/*               DOING TSO 'ACCOUNT' FUNCTIONS            */
/*--------------------------------------------------------*/
CONTROL NOMSG PROMPT
SET &AUSER = &STR(SS)
SET &USER = &STR(&SUBSTR(1:2,&SYSUID))
IF &USER NE &AUSER THEN GOTO FINIS
   ISPEXEC  FTOPEN   TEMP
   ISPEXEC  FTINCL   ACCT#S4
   ISPEXEC  FTCLOSE
   ISPEXEC  VGET     ZTEMPF
   EX    '&ZTEMPF'
FINIS: +
END
