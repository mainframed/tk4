)ATTR
  +  TYPE(TEXT) INTENS(LOW) SKIP(ON)
  %  TYPE(TEXT) INTENS(HIGH) SKIP(ON)
  _  TYPE(INPUT) INTENS(HIGH)
)BODY
+     % TRANSMIT MEMBERS OF A PDS TO A TSO USER ON MVS USING MEMBER SELE
+
+ TO SYSTEM===>_NSY    +SYS1,SYS2,VM1,VM2
+                      +BLANKS IMPLIES THE USERID IS A DISTRUBTION LIST
+
+ TO USERID===>_TUSER  +RECEIVING USER ID
+
+    NOTIFY===>_NTF+   +YES OR NO  - NOTIFY THE USER OF THE ARRIVAL
+                       OF THE FILE
+   MESSAGE===>_MES+   +YES OR NO  - SEND A MESSAGE IN ADDITION TO THE
+                       FILE
+  PDS TO
+  TRANSFER===>_IFILE                                                  +
+
+  NOTE: IF THE FILE NAME IS NOT PLACED IN QUOTES, THEN YOUR USERID
+        WILL BE USED TO PREFIX THE FILE NAME.
+
+  COMMANDS AVAILABLE WHEN THE MEMBER LIST IS DISPLAYED:
+ %S+-SELECT FOR TRANSMISSION  %B+-BROWSE  %E+-EDIT
+
+        MEMBER: + _M0     +
)INIT
  .HELP = IDT#PT2
)PROC
 IF (&NYS ^= '   ' )
      VER (&NSY,LIST,SYS1,SYS2,VM1,VM2)
 VER (&NTF,LIST,YES,NO)
 VER (&MES,LIST,YES,NO)
 VER (&IFILE,DSNAME)
 VER (&TUSER,NAME)
)END
