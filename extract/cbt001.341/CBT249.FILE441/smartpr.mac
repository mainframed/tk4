%---------------------  SMART Manual Generator Output  -----------------
%COMMAND ===>_ZCMD
%
+  Input data set       %===>_INPUT
+  Output data set      %===>_OUTPUT
+  Output data set disp %===>_DISP
+  Alternate SYSLIB  DD %===>_ADDN    +
+                    DSN%===>_ADSN
+
+Produce following output:
+  Table of contents    %===>_TABLE   +(YES or NO)
+  Index                %===>_INDEX   +(YES or NO)
+  Report               %===>_REPORT  +(YES or NO)
+  Statistics           %===>_STATS   +(YES or NO)
+  Print all index words%===>_ALL     +(YES or NO)
+  Cover page           %===>_COVER   +(YES or NO)
+  Left adjust output   %===>_LEFT    +(YES or NO)
+  Misspelling xref     %===>_MISSPELL+(YES or NO)
)INIT
 &OUTPUT   = TRANS(&OUTPUT ' ','SMART.LIST' *,*)
 &DISP     = TRANS(&DISP   ' ',NEW *,*)
 &TABLE    = TRANS(TRUNC(&TABLE   ,1) Y,YES N,NO ' ',YES *,*)
 &INDEX    = TRANS(TRUNC(&INDEX   ,1) Y,YES N,NO ' ',YES *,*)
 &REPORT   = TRANS(TRUNC(&REPORT  ,1) Y,YES N,NO ' ',YES *,*)
 &STATS    = TRANS(TRUNC(&STATS   ,1) Y,YES N,NO ' ',NO  *,*)
 &ALL      = TRANS(TRUNC(&ALL     ,1) Y,YES N,NO ' ',NO  *,*)
 &COVER    = TRANS(TRUNC(&COVER   ,1) Y,YES N,NO ' ',YES *,*)
 &LEFT     = TRANS(TRUNC(&LEFT    ,1) Y,YES N,NO ' ',YES *,*)
 &MISSPELL = TRANS(TRUNC(&MISSPELL,1) Y,YES N,NO ' ',NO  *,*)
)PROC
 VER (&INPUT   ,NONBLANK)
 VER (&OUTPUT  ,NONBLANK)
 VER (&DISP    ,NONBLANK)
 VER (&TABLE   ,NONBLANK)
 VER (&INDEX   ,NONBLANK)
 VER (&REPORT  ,NONBLANK)
 VER (&STATS   ,NONBLANK)
 VER (&ALL     ,NONBLANK)
 VER (&COVER   ,NONBLANK)
 VER (&LEFT    ,NONBLANK)
 VER (&MISSPELL,NONBLANK)
 &TABLE    = TRANS(TRUNC(&TABLE   ,1) Y,YES N,NO ' ',YES *,*)
 &DISP     = TRANS(TRUNC(&DISP   ,1) N,NEW S,SHR O,OLD *,*)
 &INDEX    = TRANS(TRUNC(&INDEX   ,1) Y,YES N,NO ' ',YES *,*)
 &REPORT   = TRANS(TRUNC(&REPORT  ,1) Y,YES N,NO ' ',YES *,*)
 &STATS    = TRANS(TRUNC(&STATS   ,1) Y,YES N,NO ' ',NO  *,*)
 &ALL      = TRANS(TRUNC(&ALL     ,1) Y,YES N,NO ' ',NO  *,*)
 &COVER    = TRANS(TRUNC(&COVER   ,1) Y,YES N,NO ' ',YES *,*)
 &LEFT     = TRANS(TRUNC(&LEFT    ,1) Y,YES N,NO ' ',YES *,*)
 &MISSPELL = TRANS(TRUNC(&MISSPELL,1) Y,YES N,NO ' ',NO  *,*)
 VER (&INPUT   ,DSNAME)
 IF  (&OUTPUT ^= '*')
     VER (&OUTPUT  ,DSNAME)
 VER (&TABLE   ,LIST,YES,NO)
 VER (&DISP    ,LIST,NEW,SHR,OLD)
 VER (&INDEX   ,LIST,YES,NO)
 VER (&REPORT  ,LIST,YES,NO)
 VER (&STATS   ,LIST,YES,NO)
 VER (&ALL     ,LIST,YES,NO)
 VER (&COVER   ,LIST,YES,NO)
 VER (&LEFT    ,LIST,YES,NO)
 VER (&MISSPELL,LIST,YES,NO)
 &T = TRANS(&TABLE    YES,'' NO,T )
 &I = TRANS(&INDEX    YES,'' NO,I )
 &R = TRANS(&REPORT   YES,'' NO,R )
 &S = TRANS(&STATS    YES,S  NO,'')
 &A = TRANS(&ALL      YES,A  NO,'')
 &C = TRANS(&COVER    YES,'' NO,C )
 &L = TRANS(&LEFT     YES,L  NO,'')
 &M = TRANS(&MISSPELL YES,M  NO,'')
 IF (&ADDN ^= &Z)
    VER (&ADDN,NAME)
    VER (&ADSN,NONBLANK)   VER (&ADSN,DSNAME)
)END
