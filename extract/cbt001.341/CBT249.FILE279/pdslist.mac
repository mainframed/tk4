*          DATA SET PDSLIST    AT LEVEL 002 AS OF 03/10/82
         TITLE 'PDSLIST SAS PROC DEFINITION'
* TOM FOTH, WSRCC, 03/09/82
*
PROC     SASPROC NAME=PDSLIST,LOADMOD=PDSLST2,DATA=ASGIVEN
PARMS    SASLIST DDNAME,1,DD,1,DDIN,1,INDD,1,IN,1,PDS,1,MODE=ALPHANUM
PARMS SASLIST MEMBER,2,TTR,3,USERDATA,4,CFLAG,5,ALIAS,6,MODE=ALPHANUM, X
               TEST=VARCK
PARMS    SASLIST LABEL,7,LENGTH=40,MODE=ALPHANUM
PARMS    SASLIST DSN,12,LENGTH=40,MODE=ALPHANUM
OPTS     SASLIST DSN,1,MODE=ALPHANUM
DATASETS SASLIST OUT,1,MODE=DSOUT
DATASETS SASLIST DATA,1,MODE=DSOUT
DEFAULT  SASDFLT NO=1,NAME=_DATA_,MODE=DSOUT
         BR    14
VARCK    ENTER
         NAME
VARCK    LEAVE
         SASEND
         END
