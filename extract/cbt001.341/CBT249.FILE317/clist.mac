PROC 0 INT P() FILE(SMF)
/* )STARTHELP
/* )F FUNCTION
/*    STATUTL DISPLAYS STATDIE INFORMATION.
/*
/* )X SYNTAX -
/*    STATUTL INT P('PARMS')
/* )O OPERANDS -
/* ))INT  TYPE INT IF INTERNAL STATDIE BUFFERS ARE TO BE USED, ELSE
/*        AN EXTERNAL FILE IS USED (FILE PARM)
/* ))P    ENTER ANY VALID PARMS FOR STATUTL
/* )ENDHELP
/*
CONTROL MSG
FREE FI(SYSIN PL1DUMP)
ALLOC FI(PL1DUMP) DA(*)
IF &INT=INT THEN FREE  FI(SMFIN)
        ELSE DO
             FREE FI(SMFIN)
             ALLOC FI(SMFIN) DA(&FILE) SHR
             END
CLEAR
CALL 'IRLHS.STATDIE.LOAD(STATUTL)' '/&P'
ALLOC FI(SYSIN) DA(*)
