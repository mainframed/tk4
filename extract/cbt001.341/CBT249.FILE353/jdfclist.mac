PROC 0
CONTROL NOMSG
DEL JDF.CNTL
DEL JDF.CNTL NSCR
FREE    FI(HASPCKPT)
FREE    FI(HASPACE)
FREE    FI(OUTLIST)
FREE    FI(JDFCNTL)
FREE  ATTRLIST(FB80)
CONTROL   MSG
ALLOC   FI(HASPCKPT) DA('SYS1.HASPCMS')  SHR UNIT(3350) VOL(SPOOL1)
ALLOC   FI(HASPACE)  DA('SYS1.CMSPACE')  SHR UNIT(3350) VOL(SPOOL1)
ALLOC   FI(OUTLIST) DA(JOB.OUTLIST) SHR
ATTRIB FB80 LRECL(80) BLKSIZE(800) RECFM(F B)
ALLOC   FI(JDFCNTL) DA(JDF.CNTL) NEW SP(1 1) TRACKS USING(FB80)
FREE  ATTRLIST(FB80)
IF &SYSUID EQ BPSCAN THEN DO
ISPEXEC SELECT PANEL(JDFOPT) OPT(H)
END
IF &SYSUID EQ MVSOPER THEN DO
ISPEXEC SELECT PANEL(JDFOPT) OPT(O)
END
ISPEXEC SELECT PANEL(JDFOPT)
