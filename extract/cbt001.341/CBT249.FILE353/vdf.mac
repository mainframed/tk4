         SPACE 2
         PRINT GEN
         TITLE 'COMMUNICATION VECTOR TABLE'
         CVT   DSECT=YES,PREFIX=NO
         TITLE 'UNIT CONTROL BLOCK'
         IEFUCBOB
         TITLE 'DATA EXTENT BLOCK'
         IEZDEB
         TITLE 'FMT4 DSCB'
DSCB4    DSECT
         ORG   DSCB4+44                     GET PAST KEY (44X'04')
         IECSDSL1 (4)
         END
