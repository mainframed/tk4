//PANDD1    DD  DISP=SHR,DSN=PROD.PANSPOOL
//PANDD2    DD  UNIT=TAPE,VOL=PRIVATE,DSN=PANSPOOL.ARCHIVE(+1),
//     DCB=M.D,LABEL=(2,RETPD=730),DISP=(,CATLG,DELETE)
//PANDD3    DD  DUMMY
//TEMP      DD  UNIT=SYSDA,SPACE=(CYL,(10,5)),DSN=TEMP.JCLLIST,
//     DISP=(,KEEP)
//SYSOUT    DD  SYSOUT=*,DCB=(RECFM=FBA,LRECL=121,BLKSIZE=121)
//SORTIN    DD  VOL=REF=*.TEMP,DSN=TEMP.JCLLIST,
//     DISP=(OLD,DELETE,DELETE)
//SORTOUT   DD  UNIT=3350,VOL=SER=PAG004,SPACE=(CYL,(3,2),RLSE),
//     DSN=PANSPOOL.DIRECTRY(+1),
//     DCB=(M.D,RECFM=FB,LRECL=121,BLKSIZE=3146),DISP=(,CATLG,KEEP)
//SORTWK01  DD  UNIT=SYSDA,SPACE=(CYL,(10,5))
//SORTWK02  DD  UNIT=SYSDA,SPACE=(CYL,(10,5))
//SORTWK03  DD  UNIT=SYSDA,SPACE=(CYL,(10,5))
/*
