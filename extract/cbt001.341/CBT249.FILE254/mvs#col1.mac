TITLE2 WHERE BUSY GE 30% OR QUEUE LENGTH GE .1;
TITLE3 PLOT SET 16;


PROC PLOT UNIFORM DATA=S5A1;
     PLOT NO_JOBS*DATE='*'
     / VREF=3000 VAXIS= 0 TO 4000 BY 200;
TITLE1 TOTAL NUMBER OF JOBS RUN;
TITLE2 PLOT SET 17;


PROC PLOT UNIFORM DATA=S5A1;
     PLOT Q_WAIT*DATE='Q'
          EX_TIME*DATE='E'
          LAPSED*DATE='L'
          / OVERLAY;
TITLE1 AVERAGE JOB QUEUE WAIT AND EXECUTION TIME IN MINUTES;
TITLE2 PLOT SET 18;


PROC PLOT UNIFORM DATA=S5A1;
     PLOT SMF5JCPU*DATE='T'
          SMF5SRBT*DATE='S'
          CPU_TIME*DATE='C'
          / OVERLAY
            VAXIS= 0 TO 2 BY .1;
TITLE1 AVERAGE JOB CPU TIME IN MINUTES;
TITLE2 PLOT SET 19;


PROC PLOT UNIFORM DATA=S5B1;
     PLOT SMF5JCPU*DATE='T'
          SMF5SRBT*DATE='S'
          CPU_TIME*DATE='C'
          /OVERLAY;
TITLE1 TOTAL JOB CPU TIME IN HOURS;
TITLE2 PLOT SET 20;
