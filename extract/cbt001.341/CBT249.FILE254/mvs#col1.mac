PROC MEANS NOPRINT DATA=S70X; BY SID DATE;
VARIABLES CPU0_BUS CPU1_BUS;
OUTPUT OUT=S70X1
MEAN=CPU0_BUS CPU1_BUS;
PROC DELETE DATA=S70X;


