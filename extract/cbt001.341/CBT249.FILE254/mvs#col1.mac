TITLE1 SMF REDUCTION;
MACRO _DAY
DAY=MOD(DATE+5,7);
IF DAY=0 THEN DAY=7; * MON=1 TO SUN=7;
IF JULDATE(DATE) LT 77900 THEN DAY=7; *GET RID OF 1977;
%
MACRO _SHIFT
SHIFT=2;
IF TIME LT 830 THEN SHIFT=1;
IF TIME GT 1630 THEN SHIFT=3;
%


