//**********************************************************************
//*     ASSEMBLE AND LINK-EDIT A ROUTINE INTO TEC.LINKLIB
//**********************************************************************
//STEP#1  EXEC PGM=IEV90,REGION=1024K,COND=(0,LT),
//             PARM='LOAD,XREF(SHORT),LINECNT=55,NODECK'
//*
//SYSLIB   DD  DISP=SHR,DSN=SYS1.MACLIB
//         DD  DISP=SHR,DSN=TEC.MACLIB
//         DD  DISP=SHR,DSN=SYS2.MACLIB
//         DD  DISP=SHR,DSN=SYS1.AMODGEN,UNIT=DASD,VOL=SER=SYSTMA
//*        DD  DISP=SHR,DSN=SYS1.APVTMACS,UNIT=DASD,VOL=SER=SYSTMA
//*        DD  DISP=SHR,DSN=TEC.PPI078.R824.MACLIB
//SYSLIN   DD  UNIT=DASD,SPACE=(CYL,(5,5)),DSN=&LOADSET,DISP=(,PASS)
//SYSUT1   DD  UNIT=DASD,SPACE=(CYL,(5,2))
//SYSUT2   DD  UNIT=DASD,SPACE=(CYL,(6,2))
//SYSUT3   DD  UNIT=DASD,SPACE=(CYL,(10,2))
//SYSPUNCH DD  SYSOUT=F,DCB=FUNC=I
//SYSPRINT DD  SYSOUT=O
//SYSUDUMP DD  SYSOUT=1
//SYSIN    DD  DSN=TEC.TECRJK.PDS(SUSEC),DISP=SHR
//*
//STEP#2    EXEC PGM=IEWL,REGION=1024K,COND=(4,LT),
//          PARM='XREF,LET,LIST'
//*
//SYSPRINT  DD   SYSOUT=O
//SYSUT1    DD   DSN=&SYSUT1,UNIT=SYSDA,SPACE=(CYL,(1,1))
//SYSLMOD   DD   DISP=SHR,DSN=TECRJK.SASFMT.LIBRARY
//SYSLIB    DD   DISP=SHR,DSN=CNB.LINKLIB
//          DD   DISP=SHR,DSN=SYS1.TELCMLIB
//          DD   DISP=SHR,DSN=SYS1.COBLIB
//          DD   DISP=SHR,DSN=SYS1.FORTLIB
//          DD   DISP=SHR,DSN=SYS1.PLIBASE
//          DD   DISP=SHR,DSN=SYS1.PLITASK
//SYSLIN    DD   UNIT=SYSDA,DSN=&LOADSET,DISP=(OLD,DELETE)
//          DD   DDNAME=SYSIN
//SYSIN     DD   *
 ENTRY SUSEC
 NAME  SUSEC(R)
//
