         MVI   CNTRL,X'09'
         MVC   PRNTAREA(132),BLANKS
         MVC   PRNTAREA(46),SEQMESS
         PUT   PRINTDCB,PRNTAREA-1
         MVI   CNTRL,X'11'
         MVC   PRNTAREA(46),BLANKS
         UNPK  ERRORE+23(6),COUNTER
         OI    ERRORE+28,X'F0'
         MVC   PRNTAREA(45),ERRORE
         PUT   PRINTDCB,PRNTAREA-1
         MVI   SEQSW,C'N'
         MVI   PREVKEY,X'00'
         MVC   PREVKEY+1(255),PREVKEY
         LR    1,4
         B     UNI
WORK     DS    F
SYSUT2   DC    CL8'SYSUT2'
NULLS    DC    F'0'
BACKUP   DC    CL6'BACKUP'
REORG    DC    CL5'REORG'
PURPOSE  DC    CL7'PURPOSE'
COUNTER  DC    PL5'0'
UNLDC    DC    CL38'IS UNLOADED BUT IS NOT IN KEY SEQUENCE'
SEQMESS  DC    CL46'RECORDS IN INPUT DATA SET NOT IN KEY SEQUENCE.'
SEQSW    DC    CL1'Y'
PREVKEY  DC    256XL1'00'
CNTMESS  DC    CL37'RECORD COUNT = 000000 LOGICAL RECORDS'
UNLDMESS DC    CL34'INDEXED SEQUENTIAL DATA SET NAMED '
UNLDA    DS    CL44
UNLDB    DC    CL30'HAS BEEN SUCCESSFULLY UNLOADED'
LOADA    BCR   0,0
         OPEN  (QSAMDCB,(INPUT))
         RDJFCB LOADDCB
         CLC   JFCB+102(2),NULLS     BLKSIZE
         BNE   LDA
         MVC   LOADDCB+62(2),QSAMDCB+62
LDA      MVC   LOADDCB+82(2),QSAMDCB+82   LRECL
         TM    JFCB+100,X'90'
         BC    7,LDB
         MVC   LOADDCB+36(1),QSAMDCB+36
LDB      OPEN (LOADDCB,(OUTPUT))
LDF      GET   QSAMDCB
         LH    2,LOADDCB+60   RKP
         AR    2,1
         CLI   0(2),X'FF'
         BE    LOADED
         AP    COUNTER,ONEA
         LR    0,1
         PUT   LOADDCB,(0)
         B     LDF
LOADED   CLOSE LOADDCB
         CLOSE QSAMDCB
         MVI   CNTRL,X'11'
         MVC   PRNTAREA(132),BLANKS
         UNPK  CNTMESS+15(6),COUNTER
         OI    CNTMESS+20,X'F0'
         MVC   PRNTAREA(37),CNTMESS
         PUT   PRINTDCB,PRNTAREA-1
         MVI   CNTRL,X'09'
         MVC   PRNTAREA(132),BLANKS
         MVC   PRNTAREA(48),LOADMESS
         PUT   PRINTDCB,PRNTAREA-1
         MVC   LOADMESB(44),JFCB
         MVC   PRNTAREA(57),LOADMESA
         PUT   PRINTDCB,PRNTAREA-1
         B     RESTORE
LOADMESS DC    CL48'INDEXED SEQUENTIAL DATA SET SUCCESSFULLY CREATED'
LOADMESA DC    CL13'AND IS NAMED '
LOADMESB DC    CL44'                                            '
INFOA    OPEN  BISAMDCB
         LH    5,BISAMDCB+206   TRKS REMAINING IN IND OFLOW AREA
         CVD   5,UNPACK
         UNPK  INFOIND(3),UNPACK
         OI    INFOIND+2,X'F0'
         LH    5,BISAMDCB+224         CYL OFLOW AREAS FULL
         CVD   5,UNPACK
         UNPK  INFOCYL(2),UNPACK
         OI    INFOCYL+1,X'F0'
         LH    5,BISAMDCB+214      NO. OF OFLOW RECORES
         CVD   5,UNPACK
         UNPK  OFLOWREC(5),UNPACK
         OI    OFLOWREC+4,X'F0'
         L     5,BISAMDCB+156    PRIME DATA RECORDS
         CVD   5,UNPACK
         UNPK  PRIMEREC(6),UNPACK
         OI    PRIMEREC+5,X'F0'
         MVI   CNTRL,X'09'
         RDJFCB BISAMDCB
         MVC   PRNTAREA(132),BLANKS
         MVC   REORGHD2(44),JFCB
         MVC   PRNTAREA(91),REORGHD1
         PUT   PRINTDCB,PRNTAREA-1
         MVC   PRNTAREA(132),BLANKS
         MVC   PRNTAREA(44),PRIMEREC
         PUT   PRINTDCB,PRNTAREA-1
         MVC   PRNTAREA(132),BLANKS
         MVC   PRNTAREA(40),OFLOWREC
         PUT   PRINTDCB,PRNTAREA-1
         MVC   PRNTAREA(132),BLANKS
         MVC   PRNTAREA(46),INFOIND
         PUT   PRINTDCB,PRNTAREA-1
         MVC   PRNTAREA(132),BLANKS
         MVC   PRNTAREA(35),INFOCYL
         PUT   PRINTDCB,PRNTAREA-1
         CLOSE BISAMDCB
         B     RESTORE
REORGHD1 DC    CL47'** RECORD DISTRIBUTION STATISTICS FOR DATA SET '
REORGHD2 DS    CL44
PRIMEREC DC    CL44'       RECORDS RESIDE IN THE PRIME DATA AREA'
OFLOWREC DC    CL40'      RECORDS RESIDE IN AN OVERFLOW AREA'
INFOIND  DC    CL46'    TRACKS REMAIN IN INDEPENDENT OVERFLOW AREA'
INFOCYL  DC    CL35'   CYLINDER OVERFLOW AREAS ARE FULL'
UNPACK   DS    D
ERROR    MVC   PRNTAREA(132),BLANKS
         MVI   CNTRL,X'09'
         TM    SCANSW,X'C0'   IS THIS A LOAD OPERATION
         BC    7,NOTLOAD   BRANCH IF NOT ZEROS
         CLC   LOADDCB+80(2),NULLS  WAS ERROR ON NEW ISAM DATA SET
         BE    NOTLOAD   NO
         TM    LOADDCB+80,X'20'   WAS NO SPACE AVAILABLE
         BZ    ER01     BRANCH NO
         MVC   PRNTAREA(52),ERRORA
         B     PUTERR
ER01    TM    LOADDCB+81,X'40'   DUPLICATE RECORD
         BZ    ER02     BRANCH NO
        MVC   PRNTAREA(48),ERRORD
         B     PUTERR
ER02     TM    LOADDCB+81,X'80'   WAS SEQUENCE CHECK
         BZ    ER03    BRANCH NO
        MVC   PRNTAREA(50),ERRORC
        B     PUTERR
ER03     TM    LOADDCB+80,X'04'   DID IO ERROR OCCUR
        BZ    NOTLOAD    BRANCH NO
         MVC   PRNTAREA(59),ERRORB
        B     PUTERR
NOTLOAD MVC   PRNTAREA(53),ERRMESS
PUTERR  PUT   PRINTDCB,PRNTAREA-1
        TM    SCANSW,X'C0'
        BC    6,ER04    THIS IS A PRINT
         MVI   PRNTAREA,C' '
         MVC   PRNTAREA+1(131),PRNTAREA
        UNPK  ERRORE+23(6),COUNTER
        OI    ERRORE+28,X'F0'
        MVC   PRNTAREA(45),ERRORE
         PUT   PRINTDCB,PRNTAREA-1   SHOW CURRENT RECORD COUNT
ER04     L     13,SAVEBLK+4
         RETURN (14,12),RC=12
ERRORA DC CL52'NO SPACE REMAINS IN OUTPUT DATA SET. JOB TERMINATED.'
ERRORB DC CL44'UNCORRECTABLE I-O ERROR ON OUTPUT DATA SET. '
         DC     CL15'JOB TERMINATED.'
ERRORC DC CL50'INPUT RECORDS NOT IN KEY SEQUENCE. JOB TERMINATED.'
ERRORD DC CL48'DUPLICATE KEY IN INPUT DATA SET. JOB TERMINATED.'
ERRORE DC CL45'CURRENT RECORD COUNT =        LOGICAL RECORDS'
ERRMESS DC CL53'ERROR HAS OCCURRED DURING PROCESSING. JOB TERMINATED.'
RESTORE  BCR   0,0
         CLOSE PRINTDCB
         CLOSE   CARDDCB
         L     13,SAVEBLK+4
         CLI   SEQSW,C'Y'
         BE    RESTORE1
         RETURN (14,12),RC=8
RESTORE1 RETURN (14,12),RC=0
EXLIST   DS    0F
         DC    XL1'87'
         DC    AL3(JFCB)
JFCB     DS    CL176
SAVEBLK  DS    18F
CARDDCB  DCB   DSORG=PS,MACRF=GL,DDNAME=SYSIN,DEVD=DA,                 C
               LRECL=80,BFTEK=S,BUFNO=2,EODAD=ENDSCAN,                 C
               EROPT=SKP,SYNAD=ERROR
ISAMDCB  DCB   DSORG=IS,MACRF=(GL,SK),DDNAME=SYSUT1,EODAD=EOF,         C
               SYNAD=ERROR,EXLST=EXLIST
PRINTDCB DCB   DSORG=PS,MACRF=(PM),DDNAME=SYSOUT,DEVD=DA,RECFM=FBM,    C
               LRECL=133,BLKSIZE=798,BFTEK=S,BUFNO=2,EROPT=SKP,        C
               SYNAD=ERROR
BISAMDCB DCB   DSORG=IS,MACRF=(RUS,WUA),DDNAME=SYSUT1,BUFNO=1,         C
               EXLST=EXLIST
QSAMDCB  DCB   DSORG=PS,MACRF=(GL,PM),DDNAME=SYSUT1,SYNAD=ERROR,       C
               EODAD=LOADED,EXLST=EXLIST
LOADDCB  DCB   DSORG=IS,MACRF=PM,DDNAME=SYSUT2,SYNAD=ERROR,            C
               EXLST=EXLIST
         END   LIST
