//TSO67   JOB (3225,P136,99,99,9998),'ASSEMBLE TSSO',CLASS=L,
// MSGCLASS=F,NOTIFY=TSO67
/*XEQ N13
/*ROUTE PRINT N13
//*********************************************************************
//* THIS IS THE JCL STREAM NEEDED TO ASSEMBLE AND LINK ALL THE LOAD   *
//* MODULES THAT COMPRISE THE TSSO SUBSYSTEM. THIS PROC IS SET TO     *
//* ASSEMBLE USING ASSEMBLER H. IT IS NOT POSSIBLE TO ASSEMBLE TSSO   *
//* AS DISTRIBUTED USING ASSEMBLER F BECAUSE IT DOES NOT SUPPORT THE  *
//* BATCH OPTION. TO ASSEMBLE USING ASSEMBLER F, THE LINKAGE EDITOR   *
//* CONTROL STATEMENTS THAT EXIST AT THE END OF EACH SOURCE MODULE    *
//* MUST BE MOVED, AND LINKAGE EDITOR CONTROL CARDS MUST BE ADDED TO  *
//* PROPERLY ASSEMBLE TSSO.                                           *
//* NOTE: THE EXPECTED CONDITION CODE FOR ALL STEPS IS ZERO, EXCEPT   *
//* FOR THE ASSEMBLY OF THE REPLYLOA MODULE, WHICH WILL YIELD A COND  *
//* CODE OF FOUR. THIS IS NOT A CONCERN.                              *
//*                                                                   *
//*********************************************************************
//ASMTSSO PROC MEMBER=,RENT=NORENT
//ASM    EXEC  PGM=IEV90,REGION=3500K,
//    PARM=(LOAD,NODECK,'XREF(SHORT)','MSGLEVEL=0',NOTERM,BATCH,&RENT)
//SYSUT1   DD DSN=&&SYSUT1,UNIT=VIO,SPACE=(CYL,(20,20))
//SYSUT2   DD DSN=&&SYSUT2,UNIT=VIO,SPACE=(CYL,(20,20))
//SYSUT3   DD DSN=&&SYSUT3,UNIT=VIO,SPACE=(CYL,(20,20))
//SYSPUNCH DD SYSOUT=B
//SYSLIN   DD DSN=&&LOADSET,DISP=(MOD,PASS,DELETE),UNIT=VIO,
//            SPACE=(3520,(40,40),,,ROUND),DCB=BLKSIZE=400
//SYSLIB   DD DSN=TSSO.VERS41.ASM,DISP=SHR,DCB=(BLKSIZE=9600)
//         DD DSN=SYS1.MACLIB,DISP=SHR,VOL=SER=CSRES1,UNIT=3380
//         DD DSN=SYS1.AMODGEN,DISP=SHR,VOL=SER=CSSMP1,UNIT=3380
//SYSPRINT DD SYSOUT=*
//SYSIN    DD DSN=TSSO.VERS41.ASM(&MEMBER),DISP=SHR
//SYSTERM  DD SYSOUT=*
//LKED     EXEC PGM=IEWL,REGION=2000K,COND=(4,LT,ASM),
//         PARM=(XREF,LIST,MAP,LET,NCAL,&RENT)
//SYSPRINT DD SYSOUT=*
//SYSUT1   DD DSN=&&SYSUT1,UNIT=VIO,SPACE=(CYL,(20,20))
//SYSLIN   DD DSN=&&LOADSET,DISP=(OLD,DELETE)
//         DD DDNAME=SYSIN
//SYSLMOD  DD DSN=TSSO.VERS41.LOAD,DISP=SHR
//SYSIN DD DUMMY
// PEND
//ASM1 EXEC ASMTSSO,MEMBER=JCECVT,RENT=RENT
//ASM2 EXEC ASMTSSO,MEMBER=TSSOPARS,RENT=RENT
//ASM3 EXEC ASMTSSO,MEMBER=TSSO,RENT=RENT
//ASM4 EXEC ASMTSSO,MEMBER=TSSOWTO,RENT=RENT
//ASM5 EXEC ASMTSSO,MEMBER=TSSOSSSM,RENT=RENT
//ASM6 EXEC ASMTSSO,MEMBER=TSSOESTA,RENT=RENT
//ASM7 EXEC ASMTSSO,MEMBER=EF
//ASM8 EXEC ASMTSSO,MEMBER=MULT
//ASM9 EXEC ASMTSSO,MEMBER=SPMON
//ASM10 EXEC ASMTSSO,MEMBER=VALLOC
//ASM11 EXEC ASMTSSO,MEMBER=VOLSER
//ASM13 EXEC ASMTSSO,MEMBER=LINKLIST
//ASM14 EXEC ASMTSSO,MEMBER=REPLY
//ASM15 EXEC ASMTSSO,MEMBER=REPLYLOA,RENT=RENT
//ASM16 EXEC ASMTSSO,MEMBER=CACHE
//ASM17 EXEC ASMTSSO,MEMBER=CPCMD
//ASM18 EXEC ASMTSSO,MEMBER=DISPAOF
