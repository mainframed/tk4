//DAILY    JOB (U00000),SOFTWARE,CLASS=P,MSGCLASS=W,
//         TIME=1440
//JOBLIB       DD  DSN=SYS1.PANLIB,DISP=SHR
/*
/*        *** PURGE PANSPOOL TO RUN DAILY JOB *****
/*
//STEP1       EXEC PGM=SFT8MV99,PARM='P PANSPOOL'
/*
/*        *** PRINT FILE STATS TO CHECK SPACE USAGE ***
/*
//STATS    EXEC PGM=PAN#2
//SYSPRINT  DD  SYSOUT=*
//PANDD1    DD  DISP=SHR,DSN=PAN.SPOOL
//PANDD2    DD  DUMMY
//SYSIN     DD  *
++PRINT 2-UP,COMMENT
/*
/*
/*        *** PRINT FILE FOR SPACE AVAILABILITY CHECK ****
/*
//STEP2       EXEC PGM=PAN#2
//SYSPRINT     DD  SYSOUT=*
//PANDD1       DD  DISP=SHR,DSN=PAN.SPOOL
//PANDD2       DD  DUMMY
//SYSIN        DD  *
++PRINT 2-UP,COMMENT
/*
/*        *** WRITE FILE TO WORK AREA ***
/*
//STEP3       EXEC PGM=PAN#1,REGION=104K
//SYSPRINT     DD  SYSOUT=*
//PANDD1       DD  DSNAME=PAN.SPOOL,DISP=SHR
//PANDD2       DD  DSNAME=SFT.JCLWORK,DISP=(NEW,CATLG,KEEP),
//             DCB=(RECFM=FB,LRECL=80,BLKSIZE=4080),UNIT=SYSDA,
//             SPACE=(CYL,(50,25),RLSE)
//SYSIN        DD  *
++WRITE  WORK,*
/*
/*        *** PRODUCE DAILY FICHE  ****
/*
//STEP4       EXEC PGM=SFT8MVAC,COND=(0,LT)
//SYSPRINT     DD  SYSOUT=*
//SYSUDUMP     DD  SYSOUT=*
//INPUT        DD  DSN=SFT.JCLWORK,DISP=OLD
//MONTH        DD  DSN=PAN.ECGCOUNT,DISP=(MOD,KEEP,KEEP)
//PREFIXTB     DD  DSN=EMMETT.PANSPOOL(PREFIXTB),DISP=SHR
//FICH1        DD  SYSOUT=F*** STANDARD 208 FRAMES PER FICHE
//FICH2        DD  SYSOUT=(F,,F002)*** COMPRESS 360 FRAMES PER FICHE
//FICH3        DD  SUBSYS=(F,,F002)*** COMPRESS 360 FRAMES PER FICHE
/*
/*        *** ARCHIVE THE DAILY DATA TO TAPE ***
/*
//STEP5    EXEC PGM=JCLARCH,PARM='/123199,DAILY',
//          REGION=1024K,COND=(0,LT)
//SYSPRINT  DD  SYSOUT=*
//SYSUDUMP  DD  SYSOUT=*
//PL1DUMP   DD  SYSOUT=*
//PANARCH   DD  SYSOUT=W,DCB=(RECFM=FBA,LRECL=133,BLKSIZE=1330)
//PANERRS   DD  SYSOUT=*,DCB=(RECFM=FBA,LRECL=133,BLKSIZE=1330)
//PANDD1    DD  DISP=SHR,DSN=PAN.SPOOL
//PANDD2    DD  UNIT=(TAPE,,DEFER),VOL=PRIVATE,
//          DSN=PAN.SARCHIVE(+1),LABEL=EXPDT=99003,
//          DISP=(,CATLG,UNCATLG)
//PANDD3    DD  UNIT=(TAPE,,DEFER),DSN=PAN.SARCHIVE(0),DISP=OLD
//TEMPFILE  DD  UNIT=SYSDA,SPACE=(CYL,(2,1)),DSN=PAN.SPOOLTEM,
//          DISP=(,KEEP)
//SYSOUT    DD  SYSOUT=*,DCB=(RECFM=FBA,LRECL=121,BLKSIZE=3146)
//SORTIN    DD  VOL=REF=*.TEMPFILE,DSN=PAN.SPOOLTEM,
//          DISP=(OLD,DELETE)
//          DD  DISP=(OLD,KEEP,KEEP),DSN=PAN.SDIRECT(0)
//SORTOUT   DD  UNIT=(TAPE,,DEFER),DSN=PAN.SDIRECT(+1),
//          LABEL=EXPDT=99003,DISP=(,CATLG,UNCATLG)
//SORTWK01  DD  UNIT=SYSDA,SPACE=(CYL,(7,3))
//SORTWK02  DD  UNIT=SYSDA,SPACE=(CYL,(7,3))
//SORTWK03  DD  UNIT=SYSDA,SPACE=(CYL,(7,3))
/*
/*        *** RESTART PANSPOOL FOR NEXT 24 HOURS *****
/*
//STEP6       EXEC PGM=SFT8MV99,PARM='S PANSPOOL',COND=EVEN
/*
/*        *** CLEAR RETRIEVAL FILE ***
/*
//STEP7    EXEC PGM=PAN#2
//SYSPRINT  DD  SYSOUT=*
//PANDD1    DD  DISP=SHR,DSN=PAN.TAPELIST
//PANDD2    DD  DUMMY
//SYSIN     DD  *
++DELETE DISABLED
++PRINT 2-UP,COMMENT
/*
