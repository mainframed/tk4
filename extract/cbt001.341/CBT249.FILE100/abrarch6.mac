)ATTR
  +  TYPE(TEXT) INTENS(LOW) SKIP(ON)
  %  TYPE(TEXT) INTENS(HIGH) SKIP(ON)
  _  TYPE(INPUT) INTENS(HIGH)
)BODY
%-------------  RESET DELETE INDICATOR ON ARCHIVED DATASETS  -----------
% COMMAND ===>_ZCMD
%
+   DATA SET NAME  %===>_ADSN                                        +
%
+   DATA SET GROUP %===>_ADSG                                        +
%
)INIT
  .HELP = TABRARC8
)PROC
  IF (&ADSN = ' ')
    VER (&ADSG,NONBLANK,MSG=ABR002H)
  IF (&ADSN ^= ' ')
    VER (&ADSG,LIST,' ',MSG=ABR002I)
)END
