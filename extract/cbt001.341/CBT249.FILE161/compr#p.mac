)ATTR
  +  TYPE(TEXT) INTENS(LOW) SKIP(ON)
  %  TYPE(TEXT) INTENS(HIGH) SKIP(ON)
  _  TYPE(INPUT) INTENS(HIGH)
)BODY
+
+------- SADSC - COMPARE TWO SEQUENTIAL FILES USING %COMPARE +---------
+
+        ENTER file1%===>_DSN1
+
+        ENTER file2%===>_DSN2
+
)INIT
 .CURSOR = DSN1
)PROC
 VER (&DSN1,NONBLANK)     VER (&DSN1,DSNAME)
 VER (&DSN2,NONBLANK)     VER (&DSN2,DSNAME)
 &SEL = 'CMD(%COMPR#C)'
)END
