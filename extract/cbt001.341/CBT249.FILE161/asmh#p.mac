)ATTR
  +  TYPE(TEXT) INTENS(LOW) SKIP(ON)
  %  TYPE(TEXT) INTENS(HIGH) SKIP(ON)
  _  TYPE(INPUT) INTENS(HIGH)
)BODY
+
+------% CHEAP   A S S E M B L E R   S Y N T A X   C H E C K  +------
+
+
+ ENTER DATA SET NAME%===>_PODSN
+      USER MACRO LIB%===>_USERLIB
+                    %===>_NORENT    +NORENT/RENT
)INIT
 .CURSOR = PODSN
 &USERLIB = TRANS (&USERLIB ' ',' ',*,*)
 &NORENT = TRANS (&NORENT ' ',NORENT,*,*)
)PROC
 VER (&PODSN,NONBLANK)    VER(&PODSN,DSNAME)
 IF (&USERLIB ^= &Z)
    VER(&USERLIB,NONBLANK)  VER(&USERLIB,DSNAME)
 VER (&NORENT,NONBLANK)  VER(&NORENT,LIST,RENT,NORENT)
 &SEL  =  'CMD(%ASMH#C)'
)END
