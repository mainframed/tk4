***********************************************************************
*                                                                     *
*              INVOCATION PARAMETER OPTIONS                           *
*                                                                     *
***********************************************************************
         SPACE
OPTPSTR  EQU   0                   PARM STRING
OPTPLEN  EQU   OPTPSTR+12          MINIMUM LENGTH MINUS ONE
OPTPADDR EQU   OPTPLEN+0           ROUTINE ADDRESS
OPTPBITS EQU   OPTPADDR+4          SWITCHING BITS
OPTPEND  EQU   OPTPBITS+4          THE END
         SPACE
OPTEST1  DS    0F                  POSITIONAL PARMS
         DC    CL12'ADJUST',AL1(1),AL3(SWTCHEX)
         B     ADJUST
         DC    CL12'NOADJUST',AL1(3),AL3(SWTCHEX)
         B     UNADJUST
         DC    CL12'CENTER',AL1(1),AL3(SWTCHEX)
         B     ADJUST
         DC    CL12'CENTRE',AL1(4),AL3(SWTCHEX)
         B     ADJUST
         DC    CL12'NOCENTER',AL1(3),AL3(SWTCHEX)
         B     UNADJUST
         DC    CL12'NOCENTRE',AL1(6),AL3(SWTCHEX)
         B     SWTCHNNM
         DC    CL12'CONTINUE',AL1(1),AL3(SWTCHEX)
         B     SWCONT
         DC    CL12'NOCONTINUE',AL1(3),AL3(SWTCHEX)
         B     SWCONTNO
         DC    CL12'DEBUG',AL1(4),AL3(SWTCHEX)
        $ON    PARMSWCH,DBSWS
         DC    CL12'NODEBUG',AL1(6),AL3(SWTCHEX)
        $OFF   PARMSWCH,DBSWS
         DC    CL12'FORMATTED',AL1(1),AL3(SWTCHFO)
