         DC    CL12'NOTRANSLATE',AL1(3),AL3(SWTCHEX)
        $OFF   PARMSWCH,TROSWS
         DC    CL12'TWOPASS',AL1(3),AL3(SWTCHEX)
         MVI   DEFPASOF+TAB3DEF+3,X'02'
         DC    CL12'ONEPASS',AL1(3),AL3(SWTCHEX)
         MVI   DEFPASOF+TAB3DEF+3,X'01'
         DC    CL12'2PASS',AL1(1),AL3(SWTCHEX)
         MVI   DEFPASOF+TAB3DEF+3,X'02'
         DC    CL12'1PASS',AL1(1),AL3(SWTCHEX)
         MVI   DEFPASOF+TAB3DEF+3,X'01'
         DC    CL12'UPCASE',AL1(1),AL3(SWTCHEX)
        $ON    PARMSWCH,TROSWS
         DC    CL12'NOUPCASE',AL1(3),AL3(SWTCHEX)
        $OFF   PARMSWCH,TROSWS
         DC    CL12'UPPER',AL1(2),AL3(SWTCHEX)
        $OFF   PARMSWCH,NUPSWS
         DC    CL12'NOUPPER',AL1(4),AL3(SWTCHEX)
        $ON    PARMSWCH,NUPSWS
         DC    CL12'NOWAIT',AL1(2),AL3(SWTCHEX)
        $ON    PARMSWCH,NOSWS
         DC    CL12'WAIT',AL1(1),AL3(SWTCHEX)
        $OFF   PARMSWCH,NOSWS
         SPACE
         DC    CL12'TERMINAL',AL1(1),AL3(SWTCHEX)
        $ON    TTYPE,TONLINE
         DC    CL12'ONLINE',AL1(1),AL3(SWTCHEX)
        $ON    TTYPE,TONLINE
         DC    CL12'PRINTER',AL1(1),AL3(SWTCHEX)
        $OFF   TTYPE,TONLINE
         DC    CL12'PRT',AL1(1),AL3(SWTCHEX)
        $OFF   TTYPE,TONLINE
         DC    CL12'OFFLINE',AL1(1),AL3(SWTCHEX)
        $OFF   TTYPE,TONLINE
         DC    CL12'FILE',AL1(1),AL3(SWTCHEX)
        $ON    TTYPE,TFILE
         DC    CL12'NOFILE',AL1(3),AL3(SWTCHEX)
        $OFF   TTYPE,TFILE
         SPACE
         DC    CL12'X1620',AL1(4),AL3(SWTCHEX)
         MVI   TDEVICE,TX1620
         DC    CL12'MULTI3',AL1(5),AL3(SWTCHEX)
         MVI   TDEVICE,TXASM3
         DC    CL12'QUME',AL1(3),AL3(SWTCHEX)
         MVI   TDEVICE,TQUME
         DC    CL12'ASISTERM',AL1(3),AL3(SWTCHEX)
         MVI   TDEVICE,TXASIS
         DC    CL12'MYTERM',AL1(5),AL3(SWTCHEX)
         MVI   TDEVICE,TMYTE
         DC    4AL1(FF)            THE END
         SPACE
