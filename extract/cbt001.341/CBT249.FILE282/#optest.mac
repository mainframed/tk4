         DC    CL12'SPIE',AL1(3),AL3(SWTCHEX)
        $OFF   PARMSWCH,DBSWS
         DC    CL12'NOSPIE',AL1(5),AL3(SWTCHEX)
        $ON    PARMSWCH,DBSWS
         DC    CL12'STATISTICS',AL1(3),AL3(SWTCHEX)
