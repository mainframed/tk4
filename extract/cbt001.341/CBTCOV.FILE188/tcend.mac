         MACRO
&LABEL   TCEND                     &COMMENT
         GBLA  &LINE,&TERM,&PORT,&TYPE
         AIF   (&PORT EQ 0).NOPORT
         ORG   TCPTFST-TCINDX+TCTABL
         DC    A(PORT1,PORTEND-TCPORT,PORT&PORT)
.NOPORT  ANOP
         AIF   (&LINE EQ 0).NOLINE
         ORG   TCLNFST-TCINDX+TCTABL
         DC    A(LINE1,LINEEND-TCLINE,LINE&LINE)
.NOLINE  ANOP
         AIF   (&TERM EQ 0).NOTERM
         ORG   TCTMFST-TCINDX+TCTABL
         DC    A(TERM1,TERMEND-TCTERM,TERM&TERM)
.NOTERM  ANOP
         AIF   (&TYPE EQ 0).NOTYPE
         ORG   TCTYFST-TCINDX+TCTABL
         DC    A(TYPE1,TYPEEND-TCTYPE,TYPE&TYPE)
.NOTYPE  ANOP
         ORG   TCNUMS-TCINDX+TCTABL
         DC    AL2(&PORT,&LINE,&TERM,&TYPE)
         ORG
         MEND
