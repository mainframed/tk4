.END1    AIF   ('&O(&W1)' GE '80').RS
&NME     LA    &A1,&A2
         AGO   .COMMON
.RS      ANOP
&NME     ICM   &A1,&A2,&A3
.COMMON  ANOP
         ORG   *-4
         DC    X'&O(&W1)'
         ORG   *+3
         MEND
