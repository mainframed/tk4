         MACRO
&LABEL   TCBEGIN  &D,&DSECT=YES,&TABLE=NO
         MNOTE 'TCINDX DSECT=&DSECT'
         TCINDX DSECT=&DSECT
         SPACE 3
         MNOTE 'TCPORT DSECT=&DSECT'
         TCPORT  DSECT=&DSECT
         SPACE 3
         MNOTE 'TCLINE DSECT=&DSECT'
         TCLINE  DSECT=&DSECT
         SPACE 3
         MNOTE 'TCTERM DSECT=&DSECT'
         TCTERM DSECT=&DSECT
         SPACE 3
         MNOTE 'TCTYPE DSECT=&DSECT'
         TCTYPE DSECT=&DSECT
         AIF   ('&TABLE' EQ 'NO').MEND
         SPACE 3
TCTABL   CSECT
&LABEL   DS    0F
         DC    (TCNDXLEN)X'00'     ROOM FOR INDEX
.MEND    MEND
