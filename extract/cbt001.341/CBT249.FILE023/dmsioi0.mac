         PRINT NOGEN
         DMSMACRO TYPE=DSECT
         PRINT ON,GEN
IOI0     TITLE 'D M S I O I 0  I/O INTERRUPT SAVE ROUTINE OF DMS'
         SPACE 3
* NOTE THAT THIS ROUTINE DOES NOT NEED A BASE REGISTER ASSIGNMENT
         SPACE 3
DMSIOI0  CSECT
         USING *,R15
         USING DMSCONST,DMSREG
         USING PSA,PSAREG
         LA    R0,12                    LENGTH OF DATA TO BE SAVED
         LA    R1,FLCCSW(R0)            ADDRESS OF DATA TO BE SAVED
         BALR  R12,R13                  SAVE CSW + CAW
         SR    R15,R15                  INDICATE RECORD TO BE SAVED
         BR    R14                      AND RETURN
         CSECTID DMSIOI0
         END
