//CBTASML  PROC MBR=,APARM=,LPARM=,NODE=CBT,PRINT=NOGEN, PRINT=GEN,ETC.
//         SYSRES=RES000,
//         ALIB1='N9999.NULL.MACLIB',
//         ALIB2='N9999.NULL.MACLIB',
//         LLIB1='N9999.NULL.LOAD',
//         LLIB2='N9999.NULL.LOAD'
//*
//* PROC   - CBTASML
//* REF    - SYS3.PROCLIB
//* USE    - ASSEMBLE USER PROGRAMS THAT DO NOT REQUIRE JES2 MACROS
//*
//A        EXEC PGM=IEUASM,PARM='LOAD,NODECK,LINECNT=55,&APARM'
//SYSLIB   DD  DSN=&ALIB1,DISP=SHR,DCB=BLKSIZE=6160
//         DD  DSN=&ALIB2,DISP=SHR
//         DD  DSN=N9999.&NODE..MACLIB,DISP=SHR   CBT MACRO LIBRARY
//         DD  DSN=SYS3.MACLIB,DISP=SHR           INSTALLATION MACROS
//         DD  DSN=SYS1.MACLIB,DISP=SHR,          SYSTEM MACRO LIBRARY
//         UNIT=SYSDA,VOL=SER=&SYSRES
//         DD  DSN=SYS1.AMODGEN,DISP=SHR,         SYSTEM MACRO LIBRARY
//         UNIT=SYSDA,VOL=SER=&SYSRES
//SYSUT1   DD  SPACE=(CYL,(5,5)),UNIT=SYSDA
//SYSUT2   DD  SPACE=(CYL,(5,5)),UNIT=SYSDA
//SYSUT3   DD  SPACE=(CYL,(5,5)),UNIT=SYSDA
//SYSPRINT DD  SYSOUT=*
//SYSGO    DD  DSN=N9999.&NODE..OBJ(&MBR),DISP=OLD
//SYSIN    DD  DSN=N9999.&NODE..ASM(&PRINT),DISP=SHR PRINT OPTION
//         DD  DSN=N9999.&NODE..ASM(&MBR),DISP=SHR SOURCE LIBRARY
//*
//L         EXEC PGM=IEWL,PARM='XREF,LIST,LET,MAP,&LPARM',
//          COND=(7,LT,A)
//OBJSET    DD  DSN=N9999.&NODE..OBJ,DISP=SHR
//SYSLIB    DD  DSN=&LLIB1,DISP=SHR,DCB=BLKSIZE=19069
//          DD  DSN=&LLIB2,DISP=SHR
//          DD  DSN=N9999.&NODE..LOAD,DISP=SHR
//          DD  DSN=SYS3.TESTLIB,DISP=SHR
//          DD  DSN=SYS3.LINKLIB,DISP=SHR
//SYSUT1    DD  UNIT=SYSDA,SPACE=(CYL,(2,1))
//SYSLMOD   DD  DSN=N9999.&NODE..LOAD(&MBR),DISP=SHR
//SYSPRINT  DD  SYSOUT=*
//SYSLIN    DD  DSN=N9999.&NODE..OBJ(&MBR),DISP=SHR
//          DD  DDNAME=SYSIN
//*
