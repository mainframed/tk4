%------------------------  SAS  SPF  BATCH INTERFACE -------------------
%ENTER SAS PROGRAM DATASET NAME:×%   ***** BATCH JOB INFORMATION *****
%SPF LIBRARY:                  +×LETTER TO IDENTIFY YOUR JOB %==>_JOBCHA
+   PROJECT%==>_PRJ0           +×ESTIMATED CPU TIME:  MINUTES%==>_MIN
+   LIBRARY%==>_LIB0           +×                     SECONDS%==>_SEC
+   TYPE   %==>_TYP0           +×PRINTER DESTINATION NAME    %==>_SASPRN
+   MEMBER %==>_MEM0           +×JOB OUTPUT CLASS:           %==>_SASCL+
+   VERSION %=>_SASVERS       +× (R=TERMINAL; A=PAPER; F=FICHE; 8=ROTATE
%OTHER PARTITIONED OR SEQUENTIAL SAS PROGRAM DATASET:
+DATASET NAME              %==>_DSNP
+SORTSIZE IN BLOCKS        %==>_SORT+    WORK DATASET SIZE IN BLOCKS%==>
+DEFAULT OUTPUT LINESIZE   %==>_LINESIZE+OTHER OPTIONS==>_OPTIONS
+OPTIONAL SAS WORKFILE NAME%==>_WORKFILE                   +
+
+OPTIONAL SAS/GRAPH HARDCOPY PLOTTER DEVICE TYPE%==>_PLOTTER   +
+(ENTER VERSATEC,VERSBIG,VERSMP1,VERSCTY,CALCOMP,IBM3287, OR TEK)
+
%OPTIONAL INFILE/OUTFILE NAMES:            DATASET DISPOSITION:(SHR/MOD/
%ENTER FILE NAME(S) AND DATASET WITH MEMBER NAME(S)
+FILE %==>_FI1     +DATASET %==>_DA1                               +DISP
+FILE %==>_FI2     +DATASET %==>_DA2                               +DISP
+FILE %==>_FI3     +DATASET %==>_DA3                               +DISP
+FILE %==>_FI4     +DATASET %==>_DA4                               +DISP
%                      PRESS PF1 FOR HELP
)INIT
.HELP=SASTU1
  &JOBCHAR = S
  .CURSOR = MEM0
  &MEM0 = ' '
  IF (&SASVERS = ' ')
      &SASVERS= 79
  IF (&MIN = ' ')
      &MIN = 0
  IF (&SEC = ' ')
      &SEC = 15
  IF (&SASCL = ' ')
      &SASCL = A
  IF (&SASCL = '8')
      &SASCL = 8
  IF (&SASCL = 'F')
      &SASCL = F
  IF (&SASCL = 'Y')
      &SASCL = Y
  IF (&SASCL = 'Z')
      &SASCL = Z
  IF (&SASCL = 'R')
      &SASCL = R
  IF (&SASPRNT = ' ')
      &SASPRNT= LOCAL
  IF (&SASPRNT = 'LOCAL')
      &SASPRNT= LOCAL
  IF (&LINESIZE = ' ')
      &LINESIZE=132
  IF (&SORT    = ' ')
      &SORT = 75
  IF (&WORK = ' ')
      &WORK = 75
  IF (&FI1 ^= ' ')
    IF (&D1 = ' ')
     &D1 = SHR
  IF (&FI2 ^= ' ')
    IF (&D2 = ' ')
     &D2 = SHR
  IF (&FI3 ^= ' ')
    IF (&D3 = ' ')
     &D3 = SHR
  IF (&FI4 ^= ' ')
    IF (&D4 = ' ')
     &D4 = SHR
)PROC
VER(&MIN,RANGE,0,240)
VER(&SEC,RANGE,0,59)
VER (&MIN,NONBLANK)
VER (&SEC,NONBLANK)
VER (&JOBCHAR,NONBLANK)
VER (&SASPRNT,NONBLANK)
VER (&SASCL,NONBLANK)
VER (&SASCL,LIST,A,8,F,Y,Z,R)
VER (&JOBCHAR,ALPHA)
VER (&SASPRNT,LIST,LOCAL,RMT1,RMT2,RMT3,RMT4,RMT5,RMT6,RMT7,RMT8,RMT9,
     RMT10,RMT11,RMT12,RMT13,RMT14,RMT22,
     PRINTER1,U1,U10,U11,U12,U13,U14,U15,U16,U17,U18,U19,U20,U21,U22,
     U23,U24,U25,U26)
  IF (&SASPRNT ^= LOCAL)
     IF  (&SASCL ^= R)
          &SASCL = A
  IF (&MEM0 ^= ' ')
    VER (&PRJ0,NONBLANK,MSG=SAS002)
    VER (&LIB0,NONBLANK,MSG=SAS003)
    VER (&TYP0,NONBLANK,MSG=SAS004)
  IF (&MEM0 ^= ' ')
    VER (&DSNP,LIST,' ',MSG=SAS005)
  IF (&DSNP ^= ' ')
    VER (&MEM0,LIST,' ',MSG=SAS005)
    VER (&WORKFILE,DSNAME)
  VER (&LINESIZE,RANGE,60,133)
  VER (&PLOTTER,LIST,CALCOMP,VERSATEC,TEK,IBM3287,
        VERSBIG,VERSCTY,VERSMP1)
  IF (&FI1 ^= ' ')
    VER (&DA1,NONBLANK,MSG=SAS007)
  IF (&FI2 ^= ' ')
    VER (&DA2,NONBLANK,MSG=SAS007)
  IF (&FI3 ^= ' ')
    VER (&DA3,NONBLANK,MSG=SAS007)
  IF (&FI4 ^= ' ')
    VER (&DA4,NONBLANK,MSG=SAS007)
  VER(&SORT,RANGE,75,5000,MSG=SAS008)
  VER(&WORK,RANGE,75,5000,MSG=SAS008)
  VER(&SORT,NONBLANK)
  VER(&WORK,NONBLANK)
  VER (&D1,LIST,SHR,MOD,OLD,NEW,MSG=SAS009A)
  VER (&D2,LIST,SHR,MOD,OLD,NEW,MSG=SAS009A)
  VER (&D3,LIST,SHR,MOD,OLD,NEW,MSG=SAS009A)
  VER (&D4,LIST,SHR,MOD,OLD,NEW,MSG=SAS009A)
)END
