%---------------------- FDR  A DISK PACK -------------------------------
+     PRESS %END KEY (PF3)+ TO TERMINATE
%ENTER/VERIFY PARAMETERS BELOW:
+
+   ENTER%D+FOR DUMP DISK TO TAPE;
+        %R+TO RESTORE TAPE TO DISK %==>_OPTA
+   TAPE VOLSER (1)    %==>_VOLSER1
+               (2)    %==>_VOLSER2
+   TAPE DATASET NAME  %==>_TAPEDSN   +(DEFAULT IS TAPE VOLSER )
+   SYSOUT CLASS       %==>_SYSOUT+
+
+   DISK VOLSER        %==>_VOLSER   +
+
+   JOB CLASS          %==>_JOBCLASS+
+
+   HOLD FOR THIRD SHIFT (YES OR NO) ==> _HOLD +
+
+   JOB NAME CHARACTER %==> _JOBCHAR+
+
+   FOR TAPE->DISK RESTORES: SHOULD DISK VOLSER BE REPLACED? Y/N%==>_RES
)INIT
&VOLSER1=''
&VOLSER2=''
&TAPEDSN=''
&RES=N
&JOBCLASS=F
&SYSOUT=A
&VOLSER=''
&HOLD=NO
&JOBCHAR=1
)PROC
IF  (&OPTA=R)
     VER(&RES,NONBLANK)
     VER(&RES,LIST,Y,N)
VER (&VOLSER1,NONBLANK)
VER (&VOLSER,NONBLANK)
IF  (&VOLSER2=' ')
    &VOLSER2=PRIVAT
VER (&JOBCLASS,LIST,A,B,C,D,E,F,J)
VER (&OPTA,NONBLANK)
VER (&OPTA,LIST,D,R)
VER (&SYSOUT,LIST,R,Y,A,F)
VER (&HOLD,LIST,YES,NO)
&JOBCHAR = TRUNC(&JOBCHAR,1)
&VOLSER  = TRUNC(&VOLSER,6)
&VOLSER1 = TRUNC(&VOLSER1,6)
&VOLSER2 = TRUNC(&VOLSER2,6)
IF (&TAPEDSN= ' ')
    &TAPEDSN=&VOLSER1
)END
