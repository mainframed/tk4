%-------------------   PROJECT2 SPF  OUTPUT INTERFACE  -----------------
+
+ PRESS%PF3+TO END PROJECT/2 EXECUTION
+
%ENTER OUTPUT OPTION%==>_CODE+
+
%    1+ BROWSE OUTPUT
+
%    2+ ROUTE PLOTTER OUTPUT TO PLOTTER
+
%    3+ EDIT INPUT PROGRAM AND RERUN +
+
%    4+ ROUTE OUTPUT TO PRINTER       NUMBER OF COPIES%==>_COPIES+
+
%    5+ PRINT INPUT PROGRAM
+
%    6+ EDIT OUTPUT OF PROJECT/2
+
%    7+ END PROJECT/2 EXECUTION & KEEP THE OUTPUT DATASET,PROJ2OUT.LIST
+      (THE DEFAULT ACTION WITH PF3 IS TO DELETE THE OUTPUT DATASET)
+
)INIT
  .CURSOR = CODE
  &CODE = 1
  &COPIES = 1
)PROC
  VER (&CODE,LIST,1,2,3,4,5,6,7)
  VER (&COPIES,RANGE,1,9)
)END
