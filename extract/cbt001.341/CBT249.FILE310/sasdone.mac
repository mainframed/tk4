%-----------------------  SAS / SPF  OUTPUT INTERFACE ------------------
+
%OUTPUT OPTIONS:                   ENTER PF3 TO IMMEDIATELY END
+   ENTER OPTION NUMBER%(1, 2, 3, 4, 5, 6, 7, OR 8)%==>_OPTION+
+
%   1+ BROWSE SAS OUTPUT DATASET
+
%   2+ ROUTE OUTPUT TO A PRINTER     PRINTER NAME  %==>_SASPRNT  +
+                                NUMBER OF COPIES  %==>_COPIES  +
+      PRINTER NAMES ARE: LOCAL=BERLIN        RMT1 =MP1
+      RMT3=CONVEX        RMT4 =NEPLAN        U20  =CONN YANK
+      U21 =SOUTH BROAD   RMT22=MP1 COMP ROOM U25  =STAMFORD
+      U10 =NU SOUTH      U12  =MP ENV LAB    RMT23=MP MODULAR BLDG
+                                    OUTPUT CLASS  %==>_SASCL+
+      CLASSES: A=HARDCOPY  F=FICHE  8=ROTATED
+      KEEP OUTPUT DATASET AFTER PRINTING?         %==>_KEEP+
+
%   3+ ROUTE VERSATEC PLOT OUTPUT TO PLOTTER
%   4+ EDIT YOUR SAS PROGRAM AND THEN RERUN IT
%   5+ EDIT YOUR SAS PROGRAM  ONLY.
%   6+ RERUN YOUR SAS PROGRAM ONLY.
%      OPTIONS TO PROCESS ANOTHER DATASET:
%   7+ EDIT OTHER DATASET:  DATASET NAME:%==>_EDITDS
%   8+ PRINT OTHER DATASET: DATASET NAME:%==>_PRINTDS
)INIT
.CURSOR= OPTION
IF (&SASPRNT = ' ')
    &SASPRNT=LOCAL
IF (&SASPRNT =ECS)
    &SASPRNT=LOCAL
IF (&SASCL = ' ')
    &SASCL=A
IF (&KEEP = ' ')
    &KEEP=NO
&OPTION=1
&COPIES=1
)PROC
  VER(&OPTION,NONBLANK)
  VER(&OPTION,LIST,1,2,3,4,5,6,7,8)
  IF (&OPTION = 2)
     VER(&SASPRNT,LIST,LOCAL,RMT1,RMT23,RMT3,RMT4,RMT5,RMT6,RMT7,RMT12,
     RMT13,RMT22,U1,U10,U11,U12,U13,U14,U15,U16,U17,U18,
     U19,U20,U21,U22,U23,U24,U25)
     VER(&SASCL,LIST,A,Y,8,9,S,F)
     VER(&SASCL,NONBLANK)
     VER(&SASPRNT,NONBLANK)
     VER (&COPIES,NONBLANK)
     VER (&COPIES,RANGE,1,35)
     VER(&KEEP,NONBLANK)
     VER(&KEEP,LIST,YES,NO)
  IF (&OPTION = 7)
     VER(&EDITDS,NONBLANK)
     VER(&EDITDS,DSNAME)
  IF (&OPTION = 8)
     VER(&PRINTDS,NONBLANK)
     VER(&PRINTDS,DSNAME)
)END
