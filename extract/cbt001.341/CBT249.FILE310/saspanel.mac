%------------------------  SAS  SPF  INTERFACE  ------------------------
%ENTER NAME OF THE SAS PROGRAM DATASET:
%SPF LIBRARY:
+   PROJECT%==>_PRJ0       +   SAS RELEASE %==>_SASVERS+
+   LIBRARY%==>_LIB0       +                  (79 OR 82)
+   TYPE   %==>_TYP0
+   MEMBER %==>_MEM0
+
%OTHER PARTITIONED OR SEQUENTIAL DATASET (ENTER * FOR INTERACTIVE USE):
+   DATASET NAME%==>_DSNP
+SORTSIZE IN BLOCKS%==>_SORT+   WORK DATASET SIZE IN BLOCKS%==>_WORK+
+DEFAULT OUTPUT LINESIZE %===>_LINESIZE+OTHER OPTIONS%==>_OPTIONS
+
+IF YOU USE A SAS WORKFILE, ENTER THE DATASET NAME BELOW:            +
+  OPTIONAL WORKFILE NAME%==>_WORKFILE                   +
%OPTIONAL INFILE/OUTFILE NAMES:            DATASET DISPOSITION:(SHR/MOD/
%ENTER FILE NAME(S) AND DATASET WITH MEMBER NAME(S)
+FILE %==>_FI1     +DATASET %==>_DA1                               +DISP
+FILE %==>_FI2     +DATASET %==>_DA2                               +DISP
+FILE %==>_FI3     +DATASET %==>_DA3                               +DISP
+FILE %==>_FI4     +DATASET %==>_DA4                               +DISP
+
+  OPTIONAL SAS/GRAPH HARDCOPY PLOTTER DEVICE TYPE%==>_PLOTTER   +
+  (ENTER VERSATEC,VERSBIG,VERSMP1,VERSCTY,CALCOMP,IBM3287, OR TEK)
)INIT
  .CURSOR = LIB0
  &MEM0 = ' '
  &WORK = 1
  &SORT = 75
  &WORK = 75
  IF (&SASVERS  = ' ')
      &SASVERS = 82
  IF (&LINESIZE = ' ')
      &LINESIZE=132
  IF (&FI1 ^= ' ')
    IF (&D1 = ' ')
     &D1 = SHR
  IF (&FI2 ^= ' ')
    IF (&D2 = ' ')
     &D2 = SHR
  IF (&FI3 ^= ' ')
    IF (&D3 = ' ')
     &D3 = SHR
  IF (&FI4 ^= ' ')
    IF (&D4 = ' ')
     &D4 = SHR
)PROC
  IF (&MEM0 ^= ' ')
    VER (&PRJ0,NONBLANK,MSG=SAS002)
    VER (&LIB0,NONBLANK,MSG=SAS003)
    VER (&TYP0,NONBLANK,MSG=SAS004)
  IF (&MEM0 ^= ' ')
    VER (&DSNP,LIST,' ',MSG=SAS005)
  IF (&DSNP ^= ' ')
    VER (&MEM0,LIST,' ',MSG=SAS005)
    VER (&WORKFILE,DSNAME)
  VER (&LINESIZE,RANGE,60,133)
  VER (&PLOTTER,LIST,CALCOMP,VERSATEC,TEK,IBM3287,
        VERSBIG,VERSCTY,VERSMP1)
  IF (&FI1 ^= ' ')
    VER (&DA1,NONBLANK,MSG=SAS007)
  IF (&FI2 ^= ' ')
    VER (&DA2,NONBLANK,MSG=SAS007)
  IF (&FI3 ^= ' ')
    VER (&DA3,NONBLANK,MSG=SAS007)
  IF (&FI4 ^= ' ')
    VER (&DA4,NONBLANK,MSG=SAS007)
  VER(&SORT,RANGE,75,5000,MSG=SAS008)
  VER(&WORK,RANGE,75,5000,MSG=SAS008)
  VER(&SASVERS,LIST,79,82)
  VER(&SORT,NONBLANK)
  VER(&WORK,NONBLANK)
  VER (&D1,LIST,SHR,MOD,OLD,NEW,MSG=SAS009A)
  VER (&D2,LIST,SHR,MOD,OLD,NEW,MSG=SAS009A)
  VER (&D3,LIST,SHR,MOD,OLD,NEW,MSG=SAS009A)
  VER (&D4,LIST,SHR,MOD,OLD,NEW,MSG=SAS009A)
)END
