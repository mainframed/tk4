%------------------- PANVALET DIRECTORY LISTING ------------------------
%ENTER/VERIFY PARAMETERS BELOW:
+
+
+   TYPE OF ACCESS (H OR B    %==>_ACTION+
+
+   PANVALET USER NUMBER      %==>_NUMBPAN+
+
+  ACCESS
+   TYPE
+   %B+    BROWSE PANVALET DIRECTORY+
+   %H+    HARDCOPY   +DESTINATION%==>_DEST   +PRINT CLASS%==>_CLASS+
+          VALID DESTINATIONS: LOCAL=BERLIN  RMT1=MP1
+          RMT2=NEPEX RMT3=CONVEX RMT4=NEPLAN RMT20=CONN YANK
+          U21  =SOUTH BROAD      RMT22=MP1
+          U10  =NUSCO SOUTH      U12=MP ENV LAB
+
%   NOTE: THIS PANVALET DIRECTORY IS UPDATED ONLY ON FRIDAY NIGHT; DATAS
%         ADDED TO PANVALET THIS WEEK WILL NOT BE SHOWN ON THIS LISTING.
)INIT
.HELP=PHHELP0
.CURSOR=ACTION
)PROC
  VER (&ACTION,LIST,H,B,MSG=PAN104)
  VER (&ACTION,NONBLANK,MSG=PAN104)
  VER (&NUMBPAN,NONBLANK,MSG=PAN109)
  VER (&NUMBPAN,RANGE,1,9999,MSG=PAN109)
  IF (&ACTION = H)
      VER(&CLASS,NONBLANK,MSG=PAN108)
      VER(&CLASS,LIST,A,C,D,E,F,G,Y,Z,S,MSG=PAN108)
      VER (&DEST,LIST,RMT1,RMT2,RMT3,RMT4,RMT5,RMT6,RMT7,RMT8,RMT9,
      LOCAL,U1,U10,U11,U12,U13,U14,U15,U16,U17,U18,U19,U20,U21,U22,
      U23,U24,U25,
      RMT10,RMT11,RMT12,RMT13,RMT14,RMT15,RMT16,RMT20,RMT22,
      MSG=PAN106)
  IF (&ACTION = D)
     VER (&PRINTER,NONBLANK,MSG=PAN107)
     VER (&PRINTER,LIST,NSPRT01,M1PRT01,CYPRT01,ELPRT01,
          ECSAP01,MSG=PAN105)
)END
