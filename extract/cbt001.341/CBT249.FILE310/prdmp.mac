%---------STAND-ALONE TAPE / DUMP DATASET PRINT/FICHE/CLEAR------------
+
+ENTER %PF3+ TO EXIT
+
%SELECT OPTION  ==>_ANSW1
+
%     TAPE   ==> DUMP STAND-ALONE TAPE
%     DUMPXX ==> DUMP DATASET NUMBER
%     SHOW   ==> SHOW TITLES OF DUMP DATASETS
+
%ENTER SYSOUT CLASS==>_CLASS
+ (DEFAULT CLASS IS%'F+ FOR FICHE; ENTER%X+TO CLEAR DUMP DATASET)
+
+CLEAR DUMP DATASET AFTER PRINTING?%YES/NO ==> _CLEAR+
%
+SPECIAL ADDRESS SPACE TO BE PRINTED? IF SO, ENTER ASID NOS.%==>_ASID
%
+FOR FICHE OUTPUT, ENTER IN YOUR INITIALS %==> _INITS+
+FICHE TITLE1%==>_TITLE1
+FICHE TITLE2%==>_TITLE2
)INIT
    .CURSOR = ANSW1
    &ASID=' '
    &JOBCHAR=D
    &CLEAR=YES
    &CURRENT=' '
    &X = X
    &ANSW1 = ' '
    &TITLE1=' '
    &TITLE2=' '
    &INITS=' '
    &CLASS = 'F'
    &YES = YES
)PROC

VER (&ANSW1,NONBLANK)
VER (&ANSW1,LIST,TAPE,DUMP00,DUMP01,DUMP02,DUMP03,DUMP04,
     DUMP05,DUMP06,DUMP07,DUMP08,DUMP09,SHOW)
VER (&CLASS,LIST,F,A,Y,X,R)
VER (&CLEAR,LIST,YES,NO)
IF (&CLASS = F)
  IF (&ANSW1 ^= SHOW)
    VER(&INITS,NONBLANK)
    VER(&TITLE1,NONBLANK)
    VER(&TITLE2,NONBLANK)
)END
