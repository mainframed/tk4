)ATTR
$ TYPE(OUTPUT) INTENS(HIGH) JUST(LEFT)
)BODY
%------------------- GENERAL PANVALET-SPF INTERFACE MENU ---------------
+
+PRESS %END KEY (PF3)+ TO TERMINATE
+
+CHOOSE ONE PANVALET CODE AND ENTER IN PARAMETERS AS REQUIRED:
+
%ENTER NAME OF PANVALET DATASET ==>_NAMEPAN                  +
+
+PANVALET ACTION CODE:         %==>_PANWORK+
%CODES:
%R+:CHANGE NAME     +NEW NAME  %==>_NEWNAME                  +
%S+:CHANGE STATUS   +NEW STATUS%==>_STATPAN      +(DISABLE OR INACTIVE)
%F+:CHANGE FORMAT   +FORMAT    %==>_PANFORM     +
%C+:CHANGE COMMENT  +COMMENT   %==>_COMMPAN
%U+:CHANGE USER NO. +OLD USER# %==>_NUMBPAN    +NEW USER# %==>_NUMBNEW
%O+:OTHER PANVALET COMMAND     %==>_OTHRPAN
+
+RESULTS× $TEXT1
+       × $TEXT2
+       × $TEXT3
+         $TEXT4
+         $TEXT5
)INIT
&PANWORK = ' '
&NEWNAME = ' '
&PANFORM = ' '
&COMMPAN = ' '
&STATPAN = ' '
&NUMBNEW = ' '
&NUMBPAN = ' '
&NAMEPAN = ' '
&OTHRPAN=' '
.HELP=PGHELP0
)PROC
VER(&PANWORK,LIST,R,S,U,F,O,C,MSG=PAN020)
&PANWORK = TRANS(&PANWORK R,PANREN  S,PANSTAT  U,PANUSER
               F,PANFORM  C,PANCOM  O,PANOTHR, MSG=PAN021)
IF (&PANWORK = PANFORM)
    VER(&PANFORM,NONBLANK,MSG=PAN024)
IF (&PANWORK = PANREN)
    VER(&NEWNAME,NONBLANK,MSG=PAN024)
    VER(&NAMEPAN,NONBLANK,MSG=PAN024)
IF (&PANWORK = PANCOM)
    VER(&COMMPAN,NONBLANK,MSG=PAN025)
    VER(&NAMEPAN,NONBLANK,MSG=PAN024)
IF (&PANWORK = PANSTAT)
    VER(&NAMEPAN,NONBLANK,MSG=PAN024)
    VER(&STATPAN,NONBLANK,MSG=PAN026)
    VER(&STATPAN,LIST,ENABLE,DISABLE,INACTIVE,ACTIVE,PROD,MSG=PAN023)
IF (&PANWORK = PANUSER)
    VER(&NAMEPAN,NONBLANK,MSG=PAN024)
    VER(&NUMBPAN,NONBLANK,MSG=PAN027)
    VER(&NUMBNEW,NONBLANK,MSG=PAN027)
    VER(&NUMBPAN,RANGE,1,9999,MSG=PAN022)
    VER(&NUMBNEW,RANGE,1,9999,MSG=PAN022)
IF (&PANWORK = PANOTHR)
    VER(&OTHRPAN,NONBLANK,MSG=PAN028)
)END
