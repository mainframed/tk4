)ATTR
# TYPE(OUTPUT) INTENS(HIGH) JUST(LEFT)
)BODY
%------------------- RETRIEVE A DATASET FROM PANVALET TO SPF------------
%ENTER/VERIFY PARAMETERS BELOW:+          PRESS %PF3+TO END
+
+PANVALET NAME %==>_NAMEPAN                 +
+                 %   #MSG1
+SPF LIBRARY:
+   PROJECT%==>_PRJ1    +(YOUR USERID)
+   LIBRARY%==>_LIB1    +(NAME)
+   TYPE   %==>_TYP1    +(DATA,CNTL,TEXT)
+   MEMBER %==>_MEMBER  +(PARTITIONED DATASETS ONLY)
+
+OTHER FULLY QUALIFIED PARTITIONED OR SEQUENTIAL DATASET NAME:
+   DATASET NAME %==>_DSN
+   DATASET TYPE %==>_TYPE   +
+   MEMBER NAME  %==>_OTHMEM   +(OPTIONAL - FOR PARTITIONED ONLY)
+   BLOCK SIZE   %==>_BLO   +
+
+MAXIMUM NUMBER OF LINES %==>_SP   + (RANGE: 10 TO 45000)
+
+RESULTS:×#TEXT1
+        ×#TEXT2
+        ×#TEXT3
+        ×#TEXT4
+        ×#TEXT5
)INIT
 &SP = 220
 &BLO = 6080
 &NAMEPAN =' '
 &LIB1    =' '
 &OTHMEM  =' '
 &DSN = ' '
 &TYPE = ' '
.HELP=PRHELP0
)PROC
  VER (&DSN,DSNAME)
  IF (&DSN ^= '')
      VER(&TYPE,NONBLANK,MSG=PAN032)
      VER(&TYPE,LIST,CLIST,VSBASIC,DATA,PLI,PL1,TEXT,FORT,CNTL,ASM,
           MSG=PAN032)
  VER (&MEMBER,NAME,MSG=PAN003)
  VER (&PRJ1,NAME,MSG=PAN003)
  VER (&LIB1,NAME,MSG=PAN003)
  VER (&TYP1,NAME,MSG=PAN003)
  VER (&NAMEPAN,NONBLANK,MSG=PAN005)
  VER (&BLO,RANGE,80,12960,MSG=PAN034)
  VER (&SP,RANGE,10,65000,MSG=PAN035)
)END
