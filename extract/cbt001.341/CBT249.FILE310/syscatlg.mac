//O1003554 JOB (VSP,008),
//         OLEKSIW,MSGCLASS=R,TIME=(,15),CLASS=Q,MSGLEVEL=1
//*
//*  THIS IS A QUICKIE SAS PROGRAM TO WRITE THE NAME OF THE SYSCATLG
//*   IN THE NUCLEUS ... IT GETS THE HEX CODE FOR THE DEVICE TYPE
//*   INTO THE DATASET VERY EASILY.  THIS IS USED ON AN SE-2 SYSTEM;
//*   I GUESS SP-1 WILL OBSOLETE THIS.  JIM OLEKSIW, NORTHEAST UTILITIES
//*
//SAS  EXEC SAS79,OPTIONS='LINESIZE=72'
//FT11F001  DD  SYSOUT=R
//FT12F001  DD  SYSOUT=R
//SYSIN     DD  *
  DATA _NULL_ ;
    FILE OUT ;
    PUT @1 'UPSPOL' @8 '0B'X @11 'SYS1.TIGER' @73 '03268100' ;
//OUT  DD  DSN=SYS1.NUCLEUS(SYSCATLG),VOL=SER=IPORES,
//     UNIT=SYSDA,DISP=SHR,
//     DCB=(LRECL=19069,BLKSIZE=19069,RECFM=U)
