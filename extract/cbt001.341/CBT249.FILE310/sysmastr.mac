%---------------------------E.C.S.  SYSTEMS   MASTER MENU---------------
%SELECT APPLICATION ===>_OPT     +   PRESS %PF3+TO TERMINATE
%  1 +FDR       -BACKUP OR RESTORE A DISK
%  2 +FAVER     -VSAM DATASET BACKUP OR RESTORE
%  3 +ANALYZE   -ANALYZE (INITIALIZE) A PACK
%  4 +CLIP      -CLIP A PACK
%  5 +SMP       -SET UP SMP JCL AND SUBMIT
%  6 +DUMP      -FICHE/PRINT A DUMP DATASET
%  7 +GENER     -FOREGROUND IEBGENER
%  8 +IEBCOPY   -BATCH IEBCOPY
%  9 +FDRDSF    -RESTORE A DATASET FROM AN FDR TAPE
%  K +KOMPACT   -COMPACT A DISK VOLUME WITH COMPACTOR
%  M +MAP       -MAP A DISK VOLUME USING IEHMAP
%  N +NATMOVE   -MOVE A NATURAL PROGRAM FROM TEST TO PRODUCTION
%  F +DSF JOBS  -PERFORM VARIOUS DEVICE SUPPORT FACILITY TASKS
%  C +COMPRESS  -COMPRESS A DISK DATASET
%  D +JOB DELETE-DELETE MEMBERS FROM JOBLIB AND PANPROD
%  J +JOBLIB    -ADD,UPDATE A PROGRAM IN JOBLIB
%  L +CICS      -PUT A PROGRAM UP ON JOBLIB IN CICS PRODUCTION
%  V +VTOC      -INTERFACE TO THE VTOC COMMAND
%  S +SAS       -SAS INTERACTIVE INTERFACE
%  B +SAS       -SAS BATCH INTERFACE
%  A +APL       -VSAPL/TSO/SPF INTERFACE
%  Q +QWIKPRINT -QUICK GENERAL PURPOSE PRINT
%  P +PDSMAN    -PARTITIONED DATASET MANAGEMENT
)INIT
)PROC
  &SEL = TRANS( TRUNC (&OPT,'.')
   1,'CMD(%FDRS)'
   2,'CMD(%FAVER) '
   3,'CMD(%ANALYZE) '
   4,'CMD(%CLIPS) '
   5,'CMD(%SMPS)'
   6,'CMD(%SPFDUMP) '
   7,'CMD(%GENERCMD)'
   8,'CMD(%IEBCOPY)'
   9,'CMD(%FDRDSFS)'
   K,'CMD(%KOMPACT)'
   M,'CMD(%MAPSPF)'
   N,'CMD(%NATMOV)'
   F,'CMD(%DSFSTUF)'
   C,'CMD(%COMPRSYS)'
   D,'CMD(%JOBDEL)'
   J,'CMD(%JOBLIB)'
   L,'CMD(%JOBCICS)'
   V,'CMD(%VTOCCMD1)'
   S,'CMD(%SASMAIN)'
   B,'CMD(%SASBATCH)'
   A,'CMD(%APLSPF) '
   Q,'CMD(%QWIKPRNT)'
   P,'PANEL(PDSMP00)'
            /* ADD OTHER APPLICATIONS HERE */
            /*                             */
                X,'EXIT'
              ' ',' '
                *,'?' )
)END
