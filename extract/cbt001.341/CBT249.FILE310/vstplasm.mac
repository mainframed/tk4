PROC  SASPROC NAME=VSTPLOT,LOADMOD=VSTPLOT2,DEFLIST=1,DEFMODE=NUMERIC
LISTS SASLIST GRAPH,1,VARIABLES,1,VAR,1,SIZE,3,ID,7,MODE=NUMERIC
