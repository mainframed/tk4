OPTS  SASLIST XGRID1,4,XG1,4,GRID1,5,YGRID1,5,YG1,5
OPTS  SASLIST XGRID2,6,XG2,6,YGRID2,7,YG2,7,GRID2,7
OPTS  SASLIST PLOT35,8,P35,8,LABY0,9,LY0,9
PARMS SASLIST XMIN,1,XMAX,2,YMIN,3,YMAX,4,XLEN,5,YLEN,6,MODE=NUMERIC
PARMS SASLIST SYMBOL,7,HBY,8,SYMRATIO,9,SR,9,MODE=NUMERIC
PARMS SASLIST TITLE,10,LENGTH=40,MODE=ALPHANUM
PARMS SASLIST DTX,15,DTY,16,DBY,17,NF,18,MODE=NUMERIC
PARMS SASLIST YINC,19,XINC,20,MODE=NUMERIC
      SASEND
      END
