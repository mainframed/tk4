%--------------------- PANVALET DATASETS DISPLAYS ----------------------
%SELECT OPTION ===>_PANWORK +
%
%                 D+- DISPLAY PANVALET DATASET(S) INFORMATION
%                 C+- DISPLAY PANVALET DATASET(S) COMMENTS
%
+ PRESS %END KEY (PF3)+ TO TERMINATE
%
%ENTER/VERIFY PARAMETERS BELOW:+(ENTER AT LEAST ONE ITEM)
%
+LEADING CHARACTERS OF PANVALET DATASETNAMES TO BE DISPLAYED:
+   PARTIAL NAME %===>_PARTPAN   +
+
+FULL PANVALET DATASETNAME TO BE DISPLAYED:
+   PANVALET DSN %===>_DSNPAN                +
+
+PANVALET USER CODE TO BE DISPLAYED:  (WARNING-THIS IS THE SLOWEST)
+   PANVALET USER %===>_USERPAN+      (FORM OF THE DISPLAY        )
+
+FULLY QUALIFIED PANVALET LIBRARY NAME TO BE DISPLAYED:
+   PANVALET LIBRARY NAME %===>_LIBPAN       +
+
)INIT
  IF (&PANWORK ='X')
      &USERPAN = NOTHING
  IF (&PANWORK ='X')
      &PARTPAN = NOTHING
  IF (&PANWORK ='X')
      &DSNPAN = NOTHING
  IF (&PANWORK ='X')
      &LIBPAN = NOTHING
.HELP=PDHELP0
)PROC
  &PANWORK = TRUNC (&PANWORK,1)
  VER (&PANWORK,NONBLANK,MSG=PAN002)
  VER (&PANWORK,LIST,D,C,X)
  VER (&USERPAN,RANGE,1,9999)
  VER (&LIBPAN,DSNAME)
)END
