//   JOB CARD     ' OLEKSIW ',
//   MSGCLASS=A,TIME=1,CLASS=A,MSGLEVEL=1,
//   NOTIFY=OLEKSIW
//*
//*   SAS PROGRAM TO BREEZE THROUGH THE SMF DATASETS TO FIND OUT
//*   WHO / WHAT HAS BEEN DELETING SPECIFIC DATASETS.
//*   THIS MAY HELP YOU FIND OUT WHO HAS BEEN BLOWING AWAY A
//*   SPECIFIC DATASET.
//*
//*     JIM OLEKSIW, NORTHEAST UTILITIES SERVICE COMPANY
//*     P.O. BOX 270, HARTFORD, CONNECTICUT 06101
//*
//SAS  EXEC SAS79,OPTIONS='LINESIZE=132'
//*SMFIN DD DSN=SYS1.MANA,DISP=SHR
//SMFIN DD DSN=SMFDLY,UNIT=TAPE,DISP=OLD,VOL=SER=SMFDLY
//SYSIN     DD  *
DATA     VSAMS ;
* QUICK AND DIRTY PROGRAM TO FIND OUT WHO HAS BEEN DELETING A
    QSAM DATASET ;
*  IF YOU ARE READING SE-2 MANX DATASETS DIRECTLY, ADD 4 TO
   ALL STARTING DISPLACEMENTS IN THE INPUT STATEMENT ;
LENGTH REC_ID 2 ;
INFILE SMFIN END = ENDB LENGTH = L MISSOVER;
INPUT @2    REC_ID    PIB1.
   @;
   IF REC_ID = 17 THEN GO TO TYPE62 ;
* DONE ;
   INPUT; DELETE; RETURN ;
TYPE62:                             *    TYPE  62 RECRD;
   INPUT @3     STARTIME    SMFSTAMP8.    SID $4.
         @15    JOBNAME    $8.
                TSTAMP  SMFSTAMP8.
                USERID $10.
                DSNAME $44.
         ;
 FORMAT STARTIME TSTAMP DATETIME22. ;
 DROP REC_ID ;
PROC SORT ; BY DSNAME ;
PROC PRINT  UNIFORM ; ID DSNAME ;
TITLE DELETED DATASETS LISTED BY DATASET NAME ;
