//O1003502 JOB (VSP,008),
//             ' JIM OLEKSIW ',CLASS=Q,MSGLEVEL=1,
//       TIME=(,15),MSGCLASS=A
//*
//*    DEMO OF 3800 CAPABILITIES IN ONE JOB
//*    THIS USES THE CHARACTER SET 'SPEC' DEFINED IN MEMBER SPEC3800
//*     LOGO, SPECIAL CHARACTERS, AND FORMAT CHARACTERS ARE SHOWN
//*     SYSOUT=9 IS A SPECIAL CLASS ALLOCATED TO 90 DEGREE PRINT
//*     AN OUTPUT WRITER IS STARTED THAT USES THE 90 DEGREE PRINT PGM
//*     THE FCB'S ARE DEFINED IN MEMBER FCBS3800
//*     T11 & GFC ARE STANDARD CHARACTER SETS
//*
//*     JIM OLEKSIW, NORTHEAST UTILITIES SERVICE COMPANY
//*     P.O. BOX 270, HARTFORD, CONNECTICUT  06101
//*
//DEMO   EXEC SAS79
//FT11F001  DD  SYSOUT=*
//FT12F001  DD  SYSOUT=*,CHARS=SPEC,COPIES=2
//FT31F001  DD  SYSOUT=*,FCB=LP6,COPIES=2
//FT32F001  DD  SYSOUT=*,FCB=LP6,CHARS=T11,COPIES=2
//FT33F001  DD  SYSOUT=*,FCB=LP12,CHARS=GFC,COPIES=2
//FT34F001  DD  SYSOUT=9,COPIES=2
//FT35F001  DD  SYSOUT=*,CHARS=SPEC,COPIES=2,MODIFY=LOGO
//FT36F001  DD  SYSOUT=*,CHARS=SPEC,COPIES=2
//SYSIN  DD  *
*   SAS PROGRAM FOR DEMONSTRATION OF 3800 CAPABILITIES ;
*  1.  CREATE SOME DATA FOR MANIPULATION ;
DATA ;
  DO X = 0 TO 5 BY .20 ;
   Y = X*SIN(2*X) ; Z = .5*(Y-1)+ X ;
   OUTPUT ;
 END ;
*   2. DEMONSTRATE 6 LINES/INCH OUTPUT ;
OPTIONS PAGESIZE=45 LINESIZE=132 ;
TITLE1 NORTHEAST UTILITIES SERVICE COMPANY ;
TITLE2 ENGINEERING COMPUTER SERVICES ;
TITLE3 3800 PRINTER DEMONSTRATION;
TITLE5 SAMPLE 1: DEFAULT PRINT CAPABILITY ;
PROC PRINT ;
PROC PRINTTO NEW UNIT=31 ;
*  JCL STATEMENT:
 //FT31F001  DD  SYSOUT=A,FCB=LP6        ;
*  PROC PRINTTO ROUTES OUTPUT TO THE JCL STATEMENT FT31F001 ;
 PROC PRINT UNIFORM ;
TITLE5 SAMPLE 2: 6 LINES/INCH, DEFAULT CHARACTERS(GF15);
*
*    SAMPLE 2:  OUTPUT AT SIX LINES/INCH IN LARGE PRINT ;
*;
PROC PRINTTO UNIT=32 NEW ;
*  JCL STATEMENT:
 //FT32F001  DD  SYSOUT=A,FCB=LP6,CHARS=T11           ;
TITLE5 Sample 3. T11 Character Set - 6 lines per inch ;
TITLE6 Note that upper and lower case are supported   ;
OPTIONS PAGESIZE=45 LINESIZE=100 ;
PROC PRINT ;
*
*  3. DEMONSTRATE COMPACT PRINT - 12 LINES/INCH, SMALL CHARACTERS
*    ;
PROC PRINTTO UNIT=33  NEW ;
*  JCL STATEMENT:
 //FT33F001  DD  SYSOUT=A,FCB=LP12,CHARS=LP12          ;
OPTIONS LINESIZE=132 PAGESIZE=90 ;
PROC PRINT ;
TITLE5 SAMPLE 4: CONDENSED PRINT - 90 LINES/PAGE ;
TITLE6 ;
*
*  NEXT, HOW ABOUT ROTATED OUTPUT - PRINT SIDEWAYS ON THE PAPER SO
*  THAT YOU CAN MAKE NOTEBOOKS OUT OF YOUR OUTPUT ;
*;
PROC PRINTTO UNIT=34 NEW ;
*  JCL STATEMENT:
 //FT34F001  DD  SYSOUT=9           ;
OPTIONS LINESIZE=100 PAGESIZE=100 ;
TITLE5 Sample 5: Rotated Output: 100 Characters Wide, 100 Lines/Page ;
TITLE6 Note that upper and lower case are supported   ;
PROC PRINT ;
*
*   NEXT: PUT THE LOGO ON PRINTOUTS ;
* ;
PROC PRINTTO UNIT=35 NEW ;
*  JCL STATEMENT:
 //FT35F001  DD  SYSOUT=A,MODIFY=LOGO,CHARS=SPEC       ;
TITLE5 SAMPLE 6: LOGO ON SAS REPORTS: 8 LINES/INCH 132 CHARACTERS WIDE ;
OPTIONS LINESIZE=132 PAGESIZE=60   CENTER; * (THESE ARE STANDARD) ;
TITLE6 ;
PROC PRINT ;
*
*   HOW ABOUT USING FORMAT CHARACTERS TO BOX IN HEADINGS?
*    EXAMPLE 6;
PROC PRINTTO UNIT=36 NEW ;
*  JCL STATEMENT:
OPTIONS LINESIZE=132 PAGESIZE=60 NOCENTER; * (THESE ARE STANDARD) ;
 //FT36F001  DD  SYSOUT=A,CHARS=SPEC       ;
TITLE5 SAMPLE 7:SPECIAL FORMAT CHARACTERS FOR BOXING IN COLUMNS       ;
OPTIONS LINESIZE=132 PAGESIZE=60 NOCENTER; * (THESE ARE STANDARD) ;
DATA _NULL_ ;
     SET DATA1  END=STOPFLAG ;
     FILE FT36F001 PRINT HEADER=NEWPAGE      ;
     PUT  @25 'a' @30 X 5.2  @38 'a' @43 Y 5.2 @49 'a' ;
     IF STOPFLAG THEN
     PUT @25 'ekkkkkkkkkkkk' @38 'ikkkkkkkkkk' @49 'd' ;
     RETURN ;
     NEWPAGE:
     PUT @25 'bkkkkkkkkkkkk' @38 'hkkkkkkkkkk' @49 'c' ;
     PUT @25 'a  X- VALUE  ' @38 'a Y-VALUE  ' @49 'a' ;
     PUT @25 'fkkkkkkkkkkkk' @38 'jkkkkkkkkkk' @49 'g' ;
     PUT @25 'a            ' @38 'a          ' @49 'a' ;
     RETURN ;
*
*   NOW, DEMONSTRATE THE USE OF 3800 SPECIAL CHARACTERS ;
*  ;
PROC PRINTTO UNIT=36 ;
TITLE5 SAMPLE 8: USE OF THE SPECIAL CHARACTERS IN PROC CHART ;
OPTIONS LINESIZE=132 PAGESIZE=60 CENTER ;
PROC CHART ;
     VBAR X /SYMBOL='23'X SUMVAR = Y MIDPOINTS= 0 TO 5 BY .5 ;
     HBAR X /SYMBOL='25'X SUMVAR = Y MIDPOINTS= 0 TO 5 BY .5 ;
     VBAR X /SYMBOL='26'X SUMVAR = Y MIDPOINTS= 0 TO 5 BY .5 ;
     HBAR X /SYMBOL='27'X SUMVAR = Y MIDPOINTS= 0 TO 5 BY .5 ;
PROC PLOT ;
TITLE5 SAMPLE 9: USE OF THE SPECIAL CHARACTERS IN PROC PLOT  ;
     PLOT Y*X='2A'X Z*X='28'X
         /OVERLAY HREF=2.5 HREFCHAR='81'X
                  VREF=0   VREFCHAR='92'X ;
