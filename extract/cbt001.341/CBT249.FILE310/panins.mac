%---------- PANVALET LIBRARY CHANGE MENU -------------------------------
+
+  YOUR STANDARD PANVALET LIBRARY IS %&LIB
+
+  LAST PANVALET LIBRARY ALLOCATED WAS %&PANLIB
+
+  ENTER THE NAME OF THE PANVALET LIBRARY THAT YOU WANT ALLOCATED TO YOU
+
+         %PANMISC+
+         %PANCON +
+         %PANUTDEV+
+         %PANUCLR+
+         %PANPLAN+
+         %PANEPX+
+
%  ENTER NAME ==>_LIBRARY
)INIT
  &LIB = TRANS(&ZLOGON MGRMISC,PANMISC, SPFMISC,PANMISC TSOMISC,PANMISC
    MGRCON,PANCON TSOCON,PANCON TSOUTDEV,PANUTDEV TSONUCLR,PANNUCLR
    AATSYS,PANSYS AATMGR,PANSYS MGRNUCLR,PANUCLR MGRUTDEV,PANUTDEV
    TSOBRLN,PANBRLN MGRBRLN,PANBRLN
    MGRPLAN,PANPLAN TSOPLAN,PANPLAN TSONEPX,PANEPX MGRNEPX,PANEPX
    )
.HELP=PMHELP0
)PROC
   VER (&LIBRARY,NONBLANK,MSG=PAN101)
   VER (&LIBRARY,LIST,PANMISC,PANCON,PANUTDEV,PANUCLR,PANPLAN,PANEPX,
         PANBRLN,
         PANPROD,PANSYS,PANAPL,PANPROC,MSG=PAN102)
)END
