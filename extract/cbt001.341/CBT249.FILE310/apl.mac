%------------------------SPF/TSO APL INTERFACE -------------------------
+
+  PRESS %PF3+ TO TERMINATE
+
+  APL DEFAULT OPERANDS
+        FULL-SCREEN EDITOR      %==>_EDIT+       (ON OR OFF)
+        APL SESSION MANAGER     %==>_SM +        (ON OR OFF)
+           -SESSION PROFILE     %==>_PROFILE+    (TSO DATASET NAME)
+        TERMINAL CODE           %==>_TERM +      (32791 FOR 3278/3279)
+        FREE SPACE (IN K)       %==>_FREE +
+        WORKSPACE SIZE(IN K)    %==>_WS   +
+        SHARED VARIABLE SIZE(K) %==>_SH  +
+        ALT INPUT SIZE          %==>_AI   +
+        HIGHLIGHTING            %==>_HIGH    +
+        QUIET MODE (YES/NO)     %==>_QUIET+
+        INITIAL APL COMMAND     %==>_COMMAND
+
)INIT
.CURSOR= EDIT
IF (&EDIT=' ')
    &EDIT=OFF
IF (&HIGH=' ')
    &HIGH=INPUT
IF (&SM=' ')
    &SM=OFF
IF (&TERM=' ')
    &TERM=32791
IF (&FREE=' ')
    &FREE=50
IF (&WS=' ')
    &WS=''
IF (&SH=' ')
    &SH=4
IF (&AI=' ')
    &AI=512
IF (&QUIET = ' ')
    &QUIET = NO
IF (&COMMAND=' ')
   &COMMAND=')WSSIZE'
)PROC
  VER(&EDIT,NONBLANK,MSG=APL010)
  VER(&EDIT,LIST,ON,OFF,MSG=APL011)
  VER(&SM,NONBLANK,MSG=APL010)
  VER(&SM,LIST,ON,OFF,MSG=APL012)
  VER(&TERM,NONBLANK,MSG=APL013)
  VER(&TERM,LIST,3277,32771,3279,32791,MSG=APL014)
  VER(&FREE,RANGE,10,800,MSG=APL015)
  VER(&WS,RANGE,50,3200,MSG=APL016)
  VER(&SH,RANGE,4,256,MSG=APL017)
  VER(&AI,RANGE,512,3600,MSG=APL018)
  VER(&QUIET,LIST,YES,NO,MSG=APL019)
  VER(&PROFILE,DSNAME)
)END
