//&SYSUID.&JOBCHAR  JOB (TSO,008,300,300),
//   'PRDUMP &SYSUID   ',CLASS=F,MSGLEVEL=1,MSGCLASS=&CLASS,
//   TIME=15
//*
//*   --- JCL TO PRINT OUT A DUMP DATASET ----
)SEL &CLASS = &X
//CLEARDMP  EXEC PGM=IEBGENER
//SYSIN     DD  DUMMY
//SYSUT1    DD  DUMMY,DCB=SYS1.DUMP00
//SYSUT2    DD  DSN=SYS1.&ANSW1,DISP=OLD,DCB=SYS1.DUMP00
//SYSPRINT  DD  SYSOUT=*
)ENDSEL
//*
//FICHE  EXEC PGM=PLIFICH,
// PARM='NOREPORT/&INITS.&TITLE1 * &TITLE2 '
//*  -----  CHANGE TITLE -------
//*
//FT06F001  DD  SYSOUT=*
//STEPLIB  DD  DSN=BE008BE.LOADMOD.LOAD,DISP=SHR
//*
//*
//PRINT  EXEC PGM=AMDPRDMP,REGION=1024K
//*
//*  ----- CHANGE DSNAME -------
)SEL &ANSW1 = TAPE
//TAPE DD UNIT=TAPE,VOL=SER=DTAPE,DSN=DUMP,DISP=OLD,LABEL=(,NL)
)ENDSEL
)SEL &ANSW1 ^= TAPE
//TAPE   DD  DSN=SYS1.&ANSW1,DISP=SHR
)ENDSEL
//*
//SYSUT1 DD  UNIT=SCRATCHA,DISP=NEW,SPACE=(CYL,(10,10))
//PRINTER  DD  SYSOUT=*
//SYSPRINT DD  SYSOUT=*
)SEL &ASID ^= &CURRENT &&  &CLASS ^= &X
 O Q,L,F,SUMMARY,CVTMAP,CPUDATA,VTAMMAP,P N,SQA,CSA,S=ASID(&ASID)
 GO
)ENDSEL
)SEL &ASID = &CURRENT && &CLASS ^= &X
 ONGO Q,L,F,SUMMARY,CPUDATA,CVTMAP,SUMDUMP,LOGDATA,P N,CSA,SQA,CURRENT
 GO
)ENDSEL
 END
)SEL &CLEAR = &YES
//CLEARDMP  EXEC PGM=IEBGENER
//SYSIN     DD  DUMMY
//SYSUT1    DD  DUMMY,DCB=SYS1.DUMP00
//SYSUT2    DD  DSN=SYS1.&ANSW1,DISP=OLD,DCB=SYS1.DUMP00
//SYSPRINT  DD  SYSOUT=*
)ENDSEL
