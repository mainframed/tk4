STATON  TITLE 'STATDIE INITIALIZE INTERFACE FOR DCMS'
STATON  DCMSTART R,LV=4096
        GETIMAGE LOGO,SCREEN
        MVC L2(79),STARTMG
        OI     IOCBFLGC,IOCBCRAO
        TRMIO IOCB,WRITE,FORMAT=FORMAT,IMAGE=SCREEN
        LINK EP=STATDIE
        LTR  R15,R15
        BZ   GOOD
        MVC  L2(79),BADMSG
        B    PUTOFF
GOOD    MVC  L2(79),GOODMSG
PUTOFF  TRMIO IOCB,FORMAT=FORMAT,IMAGE=SCREEN
        B    DONEFOR
STARTMG ZB 'STATDIE INITIALIZATION IN PROGRESS'
GOODMSG ZB 'STATDIE INITIALIZATION SUCESSFUL'
BADMSG  ZS 'STATDIE INITIALIZATION FAILED'
DONEFOR DCMSTOP SCREEN=YES
         END
