STATOFF TITLE 'STATDIE TERMINATE  INTERFACE FOR DCMS'
STATOFF DCMSTART R,LV=4096
        GETIMAGE LOGO,SCREEN
        MVC L2(79),STARTMG
        OI     IOCBFLGC,IOCBCRAO
        TRMIO IOCB,WRITE,FORMAT=FORMAT,IMAGE=SCREEN
        LINK EP=DIEPURG
        LTR  R15,R15
        BZ   GOOD
        MVC  L2(79),BADMSG
        B    PUTOFF
GOOD    MVC  L2(79),GOODMSG
PUTOFF  TRMIO IOCB,FORMAT=FORMAT,IMAGE=SCREEN
        B    DONEFOR
STARTMG ZB 'STATDIE TERMINATION IN PROGRESS'
GOODMSG ZB 'STATDIE TERMINATION SUCESSFUL'
BADMSG  ZS 'STATDIE TERMINATION FAILED'
DONEFOR DCMSTOP SCREEN=YES
         END
