//***  SAMPLE JCL FOR LOAD MODULE XREF PROGRAMS
//*
//S1 EXEC PGM=UTW16,REGION=180K
//SYSIN DD *
STEPLIB NUTW16  CPUSTRIP
/*
//* THAT WILL XREF MODULES NUTW16 AND CPUSTRIP IN THE STEPLIB
//*
//CSECTS DD DISP=(,PASS),DSN=&&CSECTS,UNIT=DISK,
// SPACE=(CYL,(1,1)),DCB=BLKSIZE=3120
//XREFS DD DISP=(,PASS),DSN=&&XREFS,UNIT=DISK,
// SPACE=(CYL,(1,1)),DCB=BLKSIZE=3120
//ENTRIES DD DISP=(,PASS),DSN=&&ENTRIES,UNIT=DISK,
// SPACE=(CYL,(1,1)),DCB=BLKSIZE=3120
//WORKFILE DD UNIT=DISK,SPACE=(CYL,(5,5))
//TEMP DD UNIT=DISK,SPACE=(CYL,(5,5))
//SYSPRINT DD SYSOUT=A
//PLIDUMP  DD SYSOUT=A
// EXEC CSORTD
//SORTIN DD DISP=(SHR,DELETE),DSN=&&CSECTS
//SORTOUT DD DISP=(,PASS),DSN=&&CSECTSS,UNIT=DISK,SPACE=(CYL,(5,5))
 SORT FIELDS=(53,8,CH,A)
/*
// EXEC CSORTD
//SORTIN DD DISP=(SHR,DELETE),DSN=&&XREFS
//SORTOUT DD DISP=(,PASS),DSN=&&XREFSS,UNIT=DISK,SPACE=(CYL,(5,5))
 SORT FIELDS=(1,52,CH,A,61,8,CH,A,53,8,CH,A,69,8,CH,A)
/*
// EXEC CSORTD
//SORTIN DD DISP=SHR,DSN=FUNCTION.TABLE.OR.DUMMY
//SORTOUT DD DISP=(,PASS),DSN=&&FUNCSS,UNIT=DISK,SPACE=(CYL,(5,5)),
// DCB=(RECFM=FB,LRECL=80,BLKSIZE=3120)
 SORT FIELDS=(1,8,CH,A)
/*
//S2 EXEC PGM=UTW17,PARM='/MODULE'
//*** PAGE THROW AT CHANGE OF LOAD MODULE BECAUSE OF ABOVE SORT
//CSECTS DD DISP=(SHR,DELETE),DSN=&&CSECTSS
//FUNCS DD DISP=(SHR,DELETE),DSN=&&FUNCSS
//XREFS DD DISP=(SHR,DELETE),DSN=&&XREFSS
//REPORT DD SYSOUT=A,DCB=BLKSIZE=1210
//PLIDUMP DD SYSOUT=A
//SYSPRINT DD SYSOUT=A
