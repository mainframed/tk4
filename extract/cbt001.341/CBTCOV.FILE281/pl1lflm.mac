//PL1LFLM PROC UCYLS=4,MODCYL=4,OUTTRKS=200,SPCEOPT=',,ROUND',         *
//            MODDEV=SYSWK,DAUTIL=SYSWK,LPART=128K,PART=128K,          *
//            LNKEDIT=IEWLF880,LSIZE='120K,38K',LOUTBLK=3509,          *
//            LOPTION='LET,MAP',MNAME=GO,OPTION=,                      *
//            OUTDCB='RECFM=VBA,BLKSIZE=3303'
//LKED EXEC PGM=&LNKEDIT,PARM='LIST,&LOPTION,SIZE=(&LSIZE)',           *
//            REGION=&LPART
//SYSLIB   DD DSNAME=SYS1.PL1LIB,DISP=SHR
//SYSPRINT DD SYSOUT=A,DCB=(RECFM=FBSA,BLKSIZE=&LOUTBLK),              *
//            SPACE=(3520,(&OUTTRKS,5),&SPCEOPT)
//SYSLIN   DD DDNAME=SYSIN
//SYSLMOD  DD DSNAME=&&GOSET(&MNAME),UNIT=&MODDEV,DISP=(MOD,PASS),     *
//            SPACE=(CYL,(&MODCYL,1,1))
//SYSUT1   DD UNIT=(&DAUTIL,SEP=(SYSLIB,SYSLIN,SYSLMOD)),              *
//            SPACE=(CYL,(&UCYLS,1))
//GO EXEC PGM=PLEATRAP,PARM='PL1/&OPTION',REGION=&PART,                *
//            COND=(5,LT,LKED),ACCT='PL1LFLM'
//STEPLIB  DD DSNAME=SYS1.PLEA,DISP=SHR
//         DD DSNAME=&&GOSET,DISP=(OLD,DELETE)
//         DD DSNAME=SYS1.PL1,DISP=SHR
//TRACE    DD DSN=&&TRACE,DISP=(,PASS),UNIT=SYSWK,SPACE=(CYL,(2,1))
//SYSPRINT DD SYSOUT=A,SPACE=(3520,(&OUTTRKS,5),&SPCEOPT),             *
//            DCB=(&OUTDCB)
//REPORT EXEC PGM=PLOTAB,PARM='NR,ISA(4K)',COND=(5,LT,LKED)
//STEPLIB  DD DSN=SYS1.PLEA,DISP=SHR
//         DD DSN=SYS1.PLI.LINKLIB,DISP=SHR
//SYSPRINT DD SYSOUT=A
//PLIDUMP  DD SYSOUT=A
//TRACE    DD DSN=&&TRACE,DISP=(OLD,DELETE)
//SYSOUT   DD SYSOUT=A
//SORTLIB  DD DSN=SYS1.SORTLIB,DISP=SHR
//SORTIN   DD UNIT=SYSDA,SPACE=(CYL,(1,1)),
//            DCB=(RECFM=FB,LRECL=28,BLKSIZE=560)
//SORTOUT  DD UNIT=SYSDA,SPACE=(CYL,(1,1)),
//            DCB=(RECFM=FB,LRECL=28,BLKSIZE=560)
//SORTWK01 DD UNIT=SYSDA,SPACE=(TRK,(100),,CONTIG)
//SORTWK02 DD UNIT=SYSDA,SPACE=(TRK,(100),,CONTIG)
//SORTWK03 DD UNIT=SYSDA,SPACE=(TRK,(100),,CONTIG)
//SORTWK04 DD UNIT=SYSDA,SPACE=(TRK,(100),,CONTIG)
//SORTWK05 DD UNIT=SYSDA,SPACE=(TRK,(100),,CONTIG)
//SORTWK06 DD UNIT=SYSDA,SPACE=(TRK,(100),,CONTIG)
