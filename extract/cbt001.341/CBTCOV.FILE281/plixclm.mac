//PLIXCLM PROC LKLBDSN='SYS1.PLIBASE',PUNCH=DUMMY
//PLI EXEC PGM=IEL0AA,PARM='OBJECT,NODECK',REGION=128K,                *
//            ACCT='PLIXCLM'
//STEPLIB  DD DSN=SYS1.PLI.LINKLIB,DISP=SHR
//SYSPRINT DD SYSOUT=A
//SYSPUNCH DD &PUNCH
//SYSLIN   DD DSN=&&LOADSET,DISP=(MOD,PASS),UNIT=SYSDA,                *
//            SPACE=(80,(250,100))
//SYSUT1   DD DSN=&&SYSUT1,UNIT=SYSDA,                                 *
//            SPACE=(1024,(200,50),,CONTIG,ROUND),DCB=BLKSIZE=1024
//LKED EXEC PGM=IEWL,PARM='XREF,LIST',COND=(9,LT,PLI),REGION=128K
//SYSLIB   DD DSN=&LKLBDSN,DISP=SHR
//         DD DSN=SYS1.PLIBASE,DISP=SHR
//SYSLMOD  DD DSN=&&GOSET(GO),DISP=(MOD,PASS),UNIT=SYSDA,              *
//            SPACE=(3303,(200,5,1),RLSE)
//SYSUT1   DD DSN=&&SYSUT1,UNIT=SYSDA,SPACE=(1024,(200,20)),           *
//            DCB=BLKSIZE=1024
//SYSPRINT DD SYSOUT=A
//SYSLIN   DD DSN=&&LOADSET,DISP=(OLD,DELETE)
//         DD DDNAME=SYSIN
//GO EXEC PGM=PLEATRAP,PARM='PLI',COND=((9,LT,PLI),(9,LT,LKED)),       *
//            REGION=128K
//STEPLIB  DD DSN=SYS1.PLEA,DISP=SHR
//         DD DSN=&&GOSET,DISP=(OLD,DELETE)
//         DD DSN=SYS1.PLI.LINKLIB,DISP=SHR
//TRACE    DD DSN=&&TRACE,DISP=(,PASS),UNIT=SYSWK,SPACE=(CYL,(2,1))
//SYSPRINT DD SYSOUT=A
//PLIDUMP  DD SYSOUT=A
//REPORT EXEC PGM=PLOTAB,PARM='NR,ISA(4K)',                            *
//            COND=((9,LT,PLI),(9,LT,LKED))
//STEPLIB  DD DSN=SYS1.PLEA,DISP=SHR
//         DD DSN=SYS1.PLI.LINKLIB,DISP=SHR
//SYSPRINT DD SYSOUT=A
//PLIDUMP  DD SYSOUT=A
//TRACE    DD DSN=&&TRACE,DISP=(OLD,DELETE)
//SYSOUT   DD SYSOUT=A
//SORTLIB  DD DSN=SYS1.SORTLIB,DISP=SHR
//SORTIN   DD UNIT=SYSDA,SPACE=(CYL,(1,1)),
//            DCB=(RECFM=FB,LRECL=28,BLKSIZE=560)
//SORTOUT  DD UNIT=SYSDA,SPACE=(CYL,(1,1)),
//            DCB=(RECFM=FB,LRECL=28,BLKSIZE=560)
//SORTWK01 DD UNIT=SYSDA,SPACE=(TRK,(100),,CONTIG)
//SORTWK02 DD UNIT=SYSDA,SPACE=(TRK,(100),,CONTIG)
//SORTWK03 DD UNIT=SYSDA,SPACE=(TRK,(100),,CONTIG)
//SORTWK04 DD UNIT=SYSDA,SPACE=(TRK,(100),,CONTIG)
//SORTWK05 DD UNIT=SYSDA,SPACE=(TRK,(100),,CONTIG)
//SORTWK06 DD UNIT=SYSDA,SPACE=(TRK,(100),,CONTIG)
