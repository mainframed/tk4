      DOUBLE PRECISION MNAME,MPASS
      DEFINE FILE 9(1,30,L,IV)
      DEFINE FILE 3(50,2400,L,IV)
      CALL ERRSET(201,256,-1,1,1,204)
      CALL ERRSET(206,256,-1,1,1,229)
      CALL ERRSET(231,256,-1,1,1,239)
      CALL ERRSET(241,256,-1,1,1,301)
      READ(9'1)MNAME,POINTS,MPASS,IIKK,IOK
      IF(IIKK.EQ.0)GO TO 99
      IF(IOK.EQ.0)GO TO 98
      IF(POINTS.LT.1.)GO TO 98
      POINTS=POINTS-5.
      IF(POINTS.LT.1)POINTS=1.
      READ(3'IIKK)MNAME,X,MPASS,X1,X2,IRST
      IRST=0
      WRITE(3'IIKK)MNAME,POINTS,MPASS,X1,X2,IRST
      IIKK=0
      WRITE(9'1)MNAME,POINTS,MPASS,IIKK
      GO TO 99
98    READ(3'IIKK)MNAME,POINTS,MPASS,X1,X2,IRST
      IRST=0
      WRITE(3'IIKK)MNAME,POINTS,MPASS,X1,X2,IRST
      IIKK=0
      WRITE(9'1)MNAME,POINTS,MPASS,IIKK
99    STOP
      END
