.EPSTAL1 ANOP
         SPACE 1
         AIF   ('&DSECT' EQ 'NO').EPSTAL2
EPST     DSECT
         AGO   .EPSTAL3
.EPSTAL2 ANOP
         DS    0D
EPST     EQU   *
.EPSTAL3 ANOP
         SPACE 1
         IRARMEP   DSECT=NO,EPN=WM3,NXT=000
         IRARMEP   DSECT=NO,EPN=IL3,NXT=000
         IRARMEP   DSECT=NO,EPN=CL3
IRAEPPA  EQU   &IRAEPPV -          BEGIN PART EVAL SCAN
