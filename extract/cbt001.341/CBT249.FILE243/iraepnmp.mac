*      GOTO EPNMPOK;
*    IF EPN(1) = 'RM1' THEN         /* RES MONITORING RTNE   @Z40BPCH*/
*      GOTO EPNMPOK;
*    IF EPN(1) = 'RM2' THEN         /* RES MONITOR ADJ RTNE  @Z40BPCH*/
*      GOTO EPNMPOK;
