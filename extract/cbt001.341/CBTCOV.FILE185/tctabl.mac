         TCBEGIN TABLE=YES
         TITLE 'THE PORT TABLE'
* JCL FOR THIS ASSEMBLY IS IN 'SYSTEM.MISC.CNTL(TCTABL)'
* THIS SOURCE CODE BELONGS IN 'SYSTEM.SOURCLIB.ASM(TCTABL)'
*----------------------------------------------------------------------
*CHANGE HISTORY
*   MOVE DL HARDCOPY FROM 0DC TO 0F0
         PORT  0F0,1,TYP=HARD,SPD=1200
         PORT  0FF,2,TYP=HARD,SPD=1200
         PORT  0F3,3,TYP=HARD,SPD=300
         TITLE 'THE LINE TABLE'
* NOTE: A LINE IS DEFINED BY ITS ENDPOINTS AND GROUP NAME.
*OLD LINES ARE NEVER RE-DEFINED, THEY SIMPLY ARE DISCONTINUED.
     LINE  1,TYP=Y22,HCPY=DL420,LOC='CS1 420 DL',TERM=1,FLGS=(HCPY)
     LINE  2,TYP=P22,HCPY=SHU,LOC='SACRED HEART',TERM=2,FLGS=(HCPY)
     LINE  3,TYP=Y22,HCPY=TEST,LOC='YCC MACH ROOM',TERM=3,FLGS=(HCPY)
         TITLE 'THE TERMINAL TABLE'
         TERM  1,DECWA
         TERM  2,DECWA
         TERM  3,DECWA
         TITLE 'THE TYPE TABLE'
         TYPE  ONTEL,LSIZ=79,PSIZ=19,XLAT=BITP,FLGS=(APL,HT),          !
               EJCT=0C,S1HT=27F0,CAHT=2740,                            C
               DESC='ONTEL TELEVISION-SCREEN TERMINAL'
*        DECWRITER WITHOUT APL OR HORIZONTAL TABS
         TYPE  DECW,LSIZ=132,PSIZ=48,XLAT=TYPP,FLGS=(HCPY),            !
               DESC='DECWRITER WITHOUT APL OR HORIZONTAL TABS'
*        DECWRITER WITH APL BUT NO HORIZONTAL TABS
         TYPE  DECWA,LSIZ=132,PSIZ=48,XLAT=TYPP,FLGS=(APL,HCPY),       !
               EJCT=0C,SAPL=0E,CAPL=0F,                                C
               DESC='DECWRITER WITH APL BUT NO HORIZONTAL TABS'
*        DECWRITER WITH HORIZONTAL TABLS BUT NO APL
         TYPE  DECWT,LSIZ=132,PSIZ=48,XLAT=TYPP,FLGS=(HCPY,HT),        !
               EJCT=0C,S1HT=27F116,CAHT=27F216,                        C
               DESC='DECWRITER WITH HORIZONTAL TABS BUT NO APL'
*        DECWRITER WITH BOTH APL AND HORIZONTAL TABS
         TYPE  DECWAT,LSIZ=132,PSIZ=48,XLAT=TYPP,FLGS=(HCPY,HT,APL),   !
               EJCT=0C,SAPL=0E,CAPL=0F,S1HT=27F116,CAHT=27F216,        C
               DESC='DECWRITER WITH BOTH APL AND HORIZONTAL TABS'
*        THE WESTERN UNION TERMINALS (WITH AND WITHOUT FORM FEED)
         TYPE  GE300,LSIZ=118,PSIZ=66,XLAT=BITP,FLGS=(HT,HCPY),        !
               S1HT=27F1,CAHT=27F2,                                    C
               DESC='WESTERN UNION TERMINAL WITHOUT FORM FEED'
         TYPE  GE300F,LSIZ=118,PSIZ=66,XLAT=BITP,FLGS=(HT,HCPY),       !
               EJCT=0C,S1HT=27F1,CAHT=27F2,                            C
               DESC='WESTERN UNION TERMINAL WITH FORM FEED'
*        TEKTRONIX 4013
         TYPE  TK13,LSIZ=80,PSIZ=28,XLAT=TYPP,                         C
               DESC='TEKTRONIX 4013'
*        DATAMEDIA-BIT PAIRING OR TYPEWRITER PAIRING
         TYPE  DM,LSIZ=80,PSIZ=24,XLAT=TYPP,FLGS=(APL),                !
               EJCT=0C,                                                C
               DESC='DATAMEDIA TERMINAL WITH APL'
         TYPE  DMN,LSIZ=80,PSIZ=24,XLAT=TYPP,                          !
               EJCT=0C,                                                C
               DESC='DATAMEDIA TERMINAL WITHOUT APL'
         TYPE  DMB,LSIZ=80,PSIZ=24,XLAT=BITP,                          !
               EJCT=0C,                                                C
               DESC='DATAMEDIA TERMINAL WITHOUT APL'
*        THE 3270
         TYPE  T3270,LSIZ=80,PSIZ=24,FLGS=(APL),                       !
               EJCT=0C,DESC='3270 TERMINAL'
*        2 TYPES OF 2741-- EBCD / CORRESPONDENCE
         TYPE  IBM1,LSIZ=120,PSIZ=48,XLAT=EBCD,FLGS=(APL,HT,HCPY),     C
               DESC='IBM 2741 WITH NO UPPER CASE PERIOD'
         TYPE  IBM5,LSIZ=120,PSIZ=48,XLAT=CORR,FLGS=(APL,HT,HCPY),     C
               DESC='IBM 2741 WITH UPPER CASE PERIOD'
*        BARBARA TICE'S BEDFORD-- LEAVE IT FOR NOW
         TYPE  BED
*        HOSPITAL CENTRONICS HARDCOPY PRINTER
         TYPE  CENT,LSIZ=80,PSIZ=66,FLGS(HCPY),EJCT=0C,                C
               DESC='CENTRONICS PRINTER'
*         LA180 DECPRINTER I 1200 BAUD HARDCOPY ONLY
         TYPE  LA180,LSIZ=132,PSIZ=48,FLGS=(HCPY,NOIN),EJCT=0C,        X
               DESC='LA180, DECPRINTER I, 1200 BAUD HARDCOPY ONLY'
         TYPE  LS120,LSIZ=132,PSIZ=48,FLGS=(HCPY),                     X
               DESC='LS120 DECWRITER, 1200 BAUD PRINTING TERMINAL'
         TITLE 'THE FINISHUP STUFF'
         TCEND
         TITLE 'MACROS USED IN THIS ASSEMBLY:'
         END
