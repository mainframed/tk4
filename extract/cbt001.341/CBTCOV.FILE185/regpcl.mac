REGPCL   CSECT
*        PARSE TABLE FOR REGION COMMAND
         PRINT NOGEN
REGPCL   IKJPARM
SIZE     IKJKEYWD
         IKJNAME  'SIZE',SUBFLD=SIZVAL
MARGIN   IKJKEYWD
         IKJNAME  'MARGIN',SUBFLD=MARGVAL
SIZVAL   IKJSUBF
SV       IKJIDENT 'REGION SIZE',FIRST=NUMERIC,OTHER=ALPHANUM,MAXLNTH=5,*
               PROMPT='REGION SIZE',VALIDCK=VALI
MARGVAL  IKJSUBF
MV       IKJIDENT 'MARGIN VALUE',FIRST=NUMERIC,OTHER=ALPHANUM,         *
               MAXLNTH=5,PROMPT='MARGIN VALUE',VALIDCK=VALI
         IKJENDP
VALI     PLIVALID  1
         END
