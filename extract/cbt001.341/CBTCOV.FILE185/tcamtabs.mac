         TITLE 'TCAM TRANSLATE TABLES FOR UNSUPPORTED CHARACTER SETS'
*        THIS ASSEMBLY HAS FIVE SECTIONS
*        1) THE CHARACTER SETS ARE DEFINED WITH DEFSET-ENDSET
*        2) TABLE CHARACTERISTICS ARE DEFINED WITH DEFTBL
*        3) THE SIGNIFICANT CHARACTERS ARE FILLED IN BY MAP
*        4) THE OUTPUT TABLE IS FILLED WITH COLONS BY FILLTBL
*        5) THE OBJECT CODE IS GENERATED BY TCAMTABL
         EJECT
ASCII    DEFSET MAXVAL=127
         COPY ASCIICTL
         COPY ASCIICHR
         ENDSET
         EJECT
EBCDIC   DEFSET
         COPY EBCDCTL
         COPY  EBCDCHR
         ENDSET
         EJECT
APLE2741 DEFSET
         COPY  E2741CTL
         COPY  APLE2741
         ENDSET
         EJECT
BITPAIR  DEFSET MAXVAL=127
         COPY  ASCIICTL
         COPY  APLASCBP
         ENDSET
         EJECT
TYPPAIR  DEFSET MAXVAL=127
         COPY  ASCIICTL
         COPY  APLASCTP
         ENDSET
         EJECT
APLEBCD  DEFSET
         COPY  EBCDCTL
         COPY  APLEBCD
         ENDSET
         EJECT
ASCIIIN  DEFTBL PARITY=(LOC,2,7),REVERSE=(LOC,8) EVEN/ODD PARITY
ASCIIOUT DEFTBL PARITY=(CONT,4,7),REVERSE=(CONT,8)
APLEIN   DEFTBL
APLEOUT  DEFTBL
TPAIRIN  DEFTBL PARITY=(LOC,2,7),REVERSE=(LOC,8)
TPAIROUT DEFTBL PARITY=(CONT,4,7),REVERSE=(CONT,8)
BPAIRIN  DEFTBL PARITY=(LOC,2,7),REVERSE=(LOC,8)
BPAIROUT DEFTBL PARITY=(CONT,4,7),REVERSE=(CONT,8)
         EJECT
         MAP   ASCIIIN,FROM=ASCII,TO=EBCDIC
         MAP   ASCIIOUT,FROM=EBCDIC,TO=ASCII
         MAP   APLEIN,FROM=APLE2741,TO=APLEBCD
         MAP   APLEOUT,FROM=APLEBCD,TO=APLE2741
         MAP   TPAIRIN,FROM=TYPPAIR,TO=APLEBCD
         MAP   TPAIROUT,FROM=APLEBCD,TO=TYPPAIR
         MAP   BPAIRIN,FROM=BITPAIR,TO=APLEBCD
         MAP   BPAIROUT,FROM=APLEBCD,TO=BITPAIR
         EJECT
         FILLTBL ASCIIOUT,SET=ASCII,CHAR=COLON
         FILLTBL APLEOUT,SET=APLE2741,CHAR=COLON
         FILLTBL TPAIROUT,SET=TYPPAIR,CHAR=COLON
         FILLTBL BPAIROUT,SET=BITPAIR,CHAR=COLON
         TITLE 'TCAM TRANSLATE TABLE FOR FULL ASCII'
IEDQ21   TCAMTABL                 ASCII
         TITLE 'TCAM TRANSLATE TABLE FOR EBCDIC 2741 WITH APL BALL'
APLE2741 TCAMTABL APLE
         TITLE 'TCAM TRANSLATE TABLE FOR TYPEWRITER PAIRED ASCII APL'
TPAIR    TCAMTABL                 TPAIR
         TITLE 'TCAM TRANSLATE TABLE FOR BIT PAIRED ASCII APL'
BPAIR    TCAMTABL                 BPAIR
         END
