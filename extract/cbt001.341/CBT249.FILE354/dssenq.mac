//JRLDSSEQ JOB T922,CLASS=C,NOTIFY=JRL
/*ROUTE  PRINT REMOTE90
/*ROUTE    XEQ 3031
//S1      EXEC PGM=IMASPZAP
//SYSPRINT  DD SYSOUT=A
//SYSLIB    DD DISP=SHR,DSN=SYS1.LINKLIB
*        TURN OFF DF/DSS ENQUEUE FOR RESTORING DATASETS
*
*        THIS MODIFICATION WILL FORCE DF/DSS NOT TO DO AN ENQUEUE
*        ON THE DATASET NAME IN ORDER TO RESTORE DATASETS THAT ARE
*        ALREADY ALLOCATED TO SOME USER.
*
*+ VER(Z038) FMID(HAE1200) PRE(UP22250) .
*+ ZAP(ADRDYNA) DISTLIB(ADRLIB) .
 NAME FASTDSSY ADRDYNA
 VER 027C 0A38          SVC   56         ENQUEUE SVC
 REP 027C 1BFF          SR    R15,R15    TURN OFF ENQUEUE
 VER 0354 0A38          SVC   56         ENQUEUE SVC
 REP 0354 1BFF          SR    R15,R15    TURN OFF ENQUEUE
 VER 03D0 0A30          SVC   48         DEQUEUE SVC
 REP 03D0 1BFF          SR    R15,R15    TURN OFF DEQUEUE
 VER 0488 0A30          SVC   48         DEQUEUE SVC
 REP 0488 1BFF          SR    R15,R15    TURN OFF DEQUEUE
 VER 067E 0A30          SVC   48         DEQUEUE SVC
 REP 067E 1BFF          SR    R15,R15    TURN OFF DEQUEUE
 VER 12FE 0A38          SVC   56         ENQUEUE SVC
 REP 12FE 1BFF          SR    R15,R15    TURN OFF ENQUEUE
 VER 13EC 0A30          SVC   48         DEQUEUE SVC
 REP 13EC 1BFF          SR    R15,R15    TURN OFF DEQUEUE
 VER 17E8 0A38          SVC   56         ENQUEUE SVC
 REP 17E8 1BFF          SR    R15,R15    TURN OFF ENQUEUE
