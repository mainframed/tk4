//*
//*  JCL TO INSTALL TSODSN
//*
//COM     EXEC PGM=IEV90,REGION=512K,
//             PARM='LOAD,XREF(SHORT),LINECNT=55,NODECK,TEST'
//*
//SYSLIB   DD  DISP=SHR,DSN=SYS1.MACLIB,UNIT=3350,VOL=SER=SYSTMG
//         DD  DISP=SHR,DSN=SYS1.AMODGEN,UNIT=3350,VOL=SER=SYSTMG
//         DD  DISP=SHR,DSN=TEC.SP3.TESTLIB          <=== THIS PDS
//SYSLIN   DD  UNIT=VIO,DSN=&LOADSET,DISP=(MOD,PASS),
//             SPACE=(CYL,(2,2)),DCB=BLKSIZE=1680
//SYSUT1   DD  UNIT=VIO,SPACE=(CYL,(10,2))
//SYSUT2   DD  UNIT=VIO,SPACE=(CYL,(12,2))
//SYSUT3   DD  UNIT=VIO,SPACE=(CYL,(20,2))
//SYSPUNCH DD  SYSOUT=F,DCB=FUNC=I
//SYSPRINT DD  SYSOUT=1
//SYSUDUMP DD  SYSOUT=1
//SYSIN    DD  DSN=TEC.SP3.TESTLIB(TSODSN),DISP=SHR  <=== THIS PDS
//*
//LKED         EXEC PGM=IEWL,REGION=1024K,COND=(4,LT),DPRTY=(11,0),
//             PARM='XREF,LET,LIST,TEST'
//SYSPRINT     DD   SYSOUT=1
//SYSUT1       DD   DSN=&SYSUT1,UNIT=SYSDA,SPACE=(CYL,(1,1))
//SYSLMOD      DD   DISP=SHR,DSN=SYS1.LINKLIB(TSODSN),VOL=SER=SYSTMG,
//             UNIT=3350
//SYSLIN       DD   UNIT=SYSDA,DSN=&LOADSET,DISP=(OLD,DELETE)
//             DD   DDNAME=SYSIN
//SYSLIB       DD   DISP=SHR,DSN=SYS1.TELCMLIB,UNIT=3350,VOL=SER=SYSTMG
//             DD   DISP=SHR,DSN=SYS1.COBLIB
//             DD   DISP=SHR,DSN=SYS1.FORTLIB
//             DD   DISP=SHR,DSN=SYS1.PLIBASE
//             DD   DISP=SHR,DSN=SYS1.PLITASK
//SYSIN        DD   DUMMY
