//JES2ASM2 PROC XREF=SHORT
//ALLOC    EXEC PGM=IEFBR14
//IPODLB   DD  UNIT=3330V,VOL=SER=IPODLB,DISP=SHR
//*
//JES2     EXEC PGM=ASMBLR,PARM='DECK,XREF(&XREF)',REGION=250K
//SYSLIB   DD  DSN=TMS1.BLK12960,DISP=SHR
//         DD  DSN=SYS1.AMACLIB,DISP=SHR,UNIT=3330V,VOL=SER=IPODLB
//         DD  DSN=SYS1.AMODGEN,DISP=SHR,UNIT=3330V,VOL=SER=IPODLB
//         DD  DSN=T0JES2.TUCMODS.ASM,DISP=SHR
//         DD  DSN=T0JES2.SOURCE,DISP=SHR
//         DD  DSN=T0JES2.LOCALMOD.ASM,DISP=SHR
//SYSUT1   DD  DSN=&&SYSUT1,UNIT=SYSDA,SPACE=(CYL,(5,15))
//SYSUT2   DD  DSN=&&SYSUT2,UNIT=SYSDA,SPACE=(CYL,(5,15))
//SYSUT3   DD  DSN=&&SYSUT3,UNIT=SYSDA,SPACE=(CYL,(5,15))
//SYSPRINT DD  DSN=&M.,DISP=(,PASS),UNIT=SYSDA,
//             SPACE=(CYL,(5,5)),DCB=BLKSIZE=12100
//SYSIN    DD  DSN=T0JES2.SOURCE(&M.),DISP=SHR
//SYSPUNCH DD  DSN=T0JES2.OBJ(&M.),DISP=SHR
//*
//*
//JES2LIST EXEC PGM=JES2LIST
//SYSPRINT DD  SYSOUT=*
//PRINTIN  DD  DSN=&M,DISP=(OLD,PASS),UNIT=SYSDA
//PRINTOUT DD  SYSOUT=*
//CHGMEM   DD  DSN=T0JES2.CHANGES.ASM(&M),DISP=SHR
//CHGPDS   DD  DSN=T0JES2.CHANGES.ASM,DISP=SHR
//STEPLIB  DD  DSN=T0MVS.PDS.LOAD,DISP=SHR
//*
//*
//LISTONLY EXEC PGM=USCCOPY,COND=ONLY
//* *****************************************************************
//* * JES2LIST WILL ABEND WITH A S013-18 IF THERE WERE NO CHANGES   *
//* * TO A MODULE AND CONSEQUENTLY NO MEMBER IN T0JES2.CHANGES.ASM. *
//* * IF THAT HAPPENS, THIS JOB STEP WILL PRINT THE ASSEMBLY.       *
//* *****************************************************************
//SYSPRINT DD  SYSOUT=*
//SYSUT01  DD  DSN=&M,DISP=(OLD,DELETE),UNIT=SYSDA
//SYSUT01D DD  SYSOUT=*
