&RJCARD1
//  MSGLEVEL=(1,1),TIME=5,REGION=512K,PRTY=7,
//  MSGCLASS=T,NOTIFY=&ZUSER
//*MAIN SYSTEM=ANY
)SEL &ODISP = NEW
//KILLIT EXEC PGM=IDCAMS
//SYSPRINT  DD SYSOUT=*
//SYSIN     DD *
  DELETE &CODSN
)ENDSEL
)SEL &DSORG = PS
//COPYIT   EXEC PGM=SYSREPRO
//STEPLIB  DD DSN=AOST.LOAD,DISP=SHR
//SYSPRINT DD SYSOUT=*
//SYSUT1   DD DSN=&CIDSN,DISP=&IDISP
)SEL &ODISP ^= NEW
//SYSUT2   DD DSN=&CODSN,DISP=&ODISP
)ENDSEL
)SEL &ODISP = NEW
//SYSUT2 DD DSN=&CODSN,
//      DISP=(&ODISP,CATLG),
//      SPACE=(&ALLUNIT,(&PRIM,&SEC),RLSE),
//      DCB=(RECFM=&RECFM,LRECL=&LRECL,BLKSIZE=&BLKSIZE),
)SEL &VOLSER = &Z
//         VOL=REF=&ZUSER..REFER
)ENDSEL
)SEL &VOLSER ^= &Z
//         UNIT=&DEVTYPE,VOL=SER=&VOLSER
)ENDSEL
)ENDSEL
)ENDSEL
)SEL &DSORG = PO
//COPYIT   EXEC PGM=IEBCOPY
//SYSPRINT DD SYSOUT=*
//SYSUT3   DD UNIT=SYSDA,SPACE=(CYL,(10))
//SYSUT4   DD UNIT=SYSDA,SPACE=(CYL,(10))
//SYSIN    DD *
)SEL &PLIST = YES
  COPY INDD=IN,OUTDD=OUT
)ENDSEL
)SEL &PLIST = NO
  COPY INDD=IN,OUTDD=OUT,LIST=NO
)ENDSEL
)SEL &MEMBER ^= &Z
  SELECT MEMBER=&MEMBER
)ENDSEL
//IN       DD DSN=&CIDSN,DISP=&IDISP
)SEL &ODISP ^= NEW
//OUT      DD DSN=&CODSN,DISP=&ODISP
)ENDSEL
)SEL &ODISP = NEW
//OUT DD DSN=&CODSN,DISP=(&ODISP,CATLG),
//    SPACE=(&ALLUNIT,(&PRIM,&SEC,&DIRALLOC),RLSE),
//    DCB=(RECFM=&RECFM,LRECL=&LRECL,BLKSIZE=&BLKSIZE),
)SEL &VOLSER = &Z
//         VOL=REF=&ZUSER..REFER
)ENDSEL
)SEL &VOLSER ^= &Z
//         UNIT=&DEVTYPE,VOL=SER=&VOLSER
)ENDSEL
)ENDSEL
)ENDSEL
