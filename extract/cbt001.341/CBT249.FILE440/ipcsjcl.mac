&RJCARD1
//  TIME=&JTIME,MSGLEVEL=1,REGION=&JSIZE.K,PRTY=&JPRTY,
//  MSGCLASS=&MSGCLASS,NOTIFY=&ZPREFIX
//*MAIN SYSTEM=&JMAIN,CLASS=&JCLASS
)SEL &PC = YES × &DSC = YES
//*****************************************************************
//DUMP     EXEC PGM=AMDPRDMP,PARM='LINECNT=42'
//STEPLIB  DD  DSN=AOST.LOAD,DISP=SHR
//SYSPRINT DD  SYSOUT=&SOC
//TAPE     DD  DSN=SYS1.DUMP&DUMP,DISP=SHR,UNIT=&DUNIT,VOL=SER=&DVOL
//PRINTER  DD  SYSOUT=&SOC
//SYSUT1   DD  UNIT=SYSDA,SPACE=(4104,(1027,512))
)SEL  &DSC = YES
//SYSUT3   DD  UNIT=SYSDA,SPACE=(CYL,(2,1))
)ENDSEL
//SYSIN    DD  *
)SEL  &PC = YES
PRINT CURRENT
)ENDSEL
)SEL  &DSC = YES
SCAN
)ENDSEL
END
)ENDSEL
//*****************************************************************
//MSSPROC  EXEC PGM=IPCSCOPY,
//         PARM='SPR=&WHOM,CPU=&DCPUID,NODE=&DCNTR,DUMPNO=&DUMP'
//STEPLIB  DD  DSN=AOST.LOAD,DISP=SHR
//DUMPIN   DD  DSN=SYS1.DUMP&DUMP,DISP=SHR,UNIT=&DUNIT,VOL=SER=&DVOL
//SYSPRINT DD  SYSOUT=&SOC
//SYSUDUMP DD  SYSOUT=&SOC
//DUMPLOG  DD  DSN=AOS.DUMPLOG,DISP=(MOD,KEEP)
//*****************************************************************
)SEL &DEMPTY = YES
//CLEAR    EXEC PGM=IEBGENER,COND=(4,LE,MSSPROC)
//SYSPRINT DD  DUMMY
//SYSIN    DD  DUMMY
//SYSUT1   DD  DUMMY,DCB=(RECFM=F,BLKSIZE=4104,LRECL=4104)
//SYSUT2   DD  DSN=*.MSSPROC.DUMPIN,VOL=REF=*.MSSPROC.DUMPIN,
//             DISP=SHR,DCB=(RECFM=F,BLKSIZE=4104)
//*****************************************************************
)ENDSEL
