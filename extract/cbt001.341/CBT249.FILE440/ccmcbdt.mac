)SEL &WCC = YES
//******   TRANSMIT SYSMODS TO WESTERN COMPUTING CENTER ******
//UNLOAD   EXEC PGM=IEBCOPY               *** TRANSPDS ***
//SYSPRINT DD SYSOUT=*
//IN       DD DSN=&TRANPTS,DISP=SHR
//OUT      DD UNIT=SYSDA,DISP=(,PASS),SPACE=(CYL,(5,20),RLSE),DSN=&&WCC
//SYSUT3   DD UNIT=SYSDA,SPACE=(CYL,(2,2))
//SYSUT4   DD UNIT=SYSDA,SPACE=(CYL,(2,2))
//SYSIN    DD DSN=&&COPYCC,DISP=(OLD,PASS)
//XMIT     EXEC PGM=BDTXMIT
//SYSPRINT DD SYSOUT=*
//INTRDR   DD SYSOUT=(A,INTRDR)
//JCL1     DD DATA,DLM='/A'
&RJCARD1
//  TIME=2,MSGLEVEL=1,REGION=1024K,PRTY=&JPRTY,
//  MSGCLASS=0,NOTIFY=&ZUSER
//*ROUTE XEQ   WCC1
//RECEIVE  EXEC PGM=BDTRECV
//SYSPRINT DD SYSOUT=*
//OUT1     DD UNIT=SYSDA,DISP=(,PASS),SPACE=(CYL,(5,20),RLSE)
//INP1     DD *
/A
//INP1     DD DSN=&&WCC,DISP=(OLD,DELETE)
//JCL2     DD DATA,DLM='/B'
//LOAD     EXEC PGM=IEBCOPY
//SYSPRINT DD SYSOUT=*
//IN2      DD DSN=*.RECEIVE.OUT1,DISP=(OLD,DELETE)
//OUT2     DD DSN=&RINDEX..WCC.SYSMODS,DISP=SHR
//SYSUT3   DD UNIT=SYSDA,SPACE=(CYL,(2,2))
//SYSUT4   DD UNIT=SYSDA,SPACE=(CYL,(2,2))
//SYSIN    DD *
 COPY OUTDD=OUT2,INDD=((IN2,R))
/B
//********************************************************************
)ENDSEL
