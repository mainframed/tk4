         MACRO
         UHBBMOD &COM=PARM.LIST,&MF=
.*       THIS MACRO GENERATES ANY INTERLUDE CODE REQUIRED BETWEEN THE
.*       GENERATED FORM OF A CONTROL BLOCK AND THE CODE TO MODIFY
.*       IT OR A COPY THEREOF.
         GBLB  &UHBM,&UHBIM,&UHBM1
         GBLC  &UHBOGL,&UHBML
         GBLC  &UHBLN
         LCLC  &LL
         LCLC  &S
         AIF   (&UHBIM).MEND
&S       SETC  ' .      '
         AIF   ('&MF(1)' EQ 'I').MFI
         AIF   ('&MF(1)' EQ 'M' OR '&MF(1)' EQ 'E').MFM
         AIF   ('&MF(1)' EQ 'L' AND '&MF(2)' EQ '').MEND
&UHBML   UHBADDR 1,&MF(2),COM=ADDRESS.REMOTE.&COM
         MVC   0(&UHBLN,1),&UHBOGL&S.COPY.INLINE.&COM
.MDONE   ANOP
&UHBM1   SETB  (0)
         MEXIT
.MFI     ANOP
&UHBM1   SETB  (1)
         MEXIT
.MFM     ANOP
&UHBML   UHBADDR 1,&MF(2),COM=ADDRESS.REMOTE.&COM
         AGO   .MDONE
.MEND    MEND
