++USERMOD (RCB02A1)
  /* ICSA RESERVED SPACE IN THE $HCT MACRO        */.
++VER (Z038) FMID(EJE1102).
++MACUPD ($HCT) PREFIX(HASP) ASSEM(JTIPSVC,VERFYQEL).
./ CHANGE NAME=$HCT
HCTICSA  DS    XL32          ICSA RESERVED SPACE                RCB02A1
#IHCTEND EQU   *             END OF ICSA SECTION                RCB02A1
         COPY  #ICSAHCT      GET ICSA HCT UPDATES               RCB02A1
++MAC (#ICSAHCT) PREFIX(HASP) ASSEM(JTIPSVC,VERFYQEL)
                DISTLIB(RICEHSRC).
         ORG   HCTICSA                 START AT BEGINNING       RCB02A1
         DS    0CL(1+#IHCTEND-*)       LENGTH CHECK             RCB02A1
         ORG   #IHCTEND                FIX LOCATION             RCB02A1
