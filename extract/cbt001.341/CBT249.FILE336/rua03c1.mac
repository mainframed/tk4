++USERMOD (RUA03C1) /* DSCB STAMPING SUPPORT FOR ISAM ALLOC */.
++VER (Z038) FMID(EDM1102)
             REQ(RUA03A1,RUA03B1,RUA03D1) /*
  THIS USER MOD MODIFIES IGG032I3 (A DADSM ISAM ALLOCATE
  MODULE) TO ISSUE SVC 237 TO STORE THE ACF2 LOGONID INTO THE
  FORMAT 1 DSCB FOR A NEW DATASET. */.
++ZAP (IGG032I3).
 NAME IGG032I3
 VER 0108 D601,D2A4,D2A4
 VER 0488 0000,0000
 REP 0488 4110,D0C0          PASS DSCB ADDR IN R1
 REP 048C 5800,D1F8          UCB ADDR IN R0
 REP 0490 0AED               ISSUE STAMPING SVC
 REP 0492 D601,D2A4,D2A4
 REP 0498 47F0,C10C          RETURN FROM PATCH
 REP 0108 47F0,C486
