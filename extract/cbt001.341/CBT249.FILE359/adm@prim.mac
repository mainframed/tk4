%-----------------------  GDDM/PGF 3.0 SELECTION MENU  -----------------
%SELECT OPTION ===>_ZCMD
%                                                             +USERID
%   1 +ALLOCATE   - Allocate new data sets for graphics       +TIME
%   2 +ICU        - Invoke the Interactive Chart Utility      +TERMINAL
%   3 +ISE        - Invoke the Image Symbol Editor
%   4 +VSE        - Invoke the Vector Symbol Editor
%
%-----------------------  GDDM/PGF 4.0 SELECTION MENU  -----------------
%
%   5 +ALLOCATE   - Allocate new data sets for graphics
%     (REQUIRED !!  Release 4 graphs WILL NOT RUN on release 3)
%   6 +ICU        - Invoke the Interactive Chart Utility
%   7 +ISE        - Invoke the Image Symbol Editor
%   8 +VSE        - Invoke the Vector Symbol Editor
%
%
%NOTE: +The allocation procedure (option 1/5) for graphics files should
%      +done once - before your%INITIAL+invocation of the Interactive Ch
%      +Utility.  The format and data files allocated by the above proce
%      +will remain intact for your use during subsequent TSO sessions.
%
+PRESS%END KEY+TO TERMINATE GDDM/ISPF+
%
)INIT
  .HELP = ADMT0000
  &ZHTOP = ISR00003                    /* TUTORIAL TABLE OF CONTENTS */
  &ZHINDEX = ISR91000                  /* TUTORIAL INDEX - 1ST PAGE  */
)PROC
  &ZSEL = TRANS( TRUNC (&ZCMD,'.')
                 1,'CMD(%ADMALLOC)'
                 2,'CMD(%ADMICU)'
                 3,'CMD(%ADMISE)'
                 4,'CMD(%ADMVSE)'
                 5,'CMD(%ADMALLO4)'
                 6,'CMD(%ADMICU4)'
                 7,'CMD(%ADMISE4)'
                 8,'CMD(%ADMVSE4)'
               ' ',' '
                 *,'?' )
)END
