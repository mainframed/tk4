PROC 0 DEBUG
CONTROL NOMSG NOFLUSH NOPROMPT NOLIST
IF &DEBUG NE &STR() THEN +
  CONTROL MSG PROMPT CONLIST SYMLIST LIST
ISPEXEC CONTROL ERRORS RETURN
ISPEXEC VGET (JOB1,JOB2,JOB3,JOB4,Z,ZTEMPF,ZPREFIX) SHARED
ISPEXEC VGET (PRJ1,LIB1,LNKT,BCLA,MEM,FLNK,FSL1,FSL2) PROFILE
IF &STR(&BCLA) EQ &STR() THEN +
  SET &BCLA EQ &STR(*)
IF &STR(&BCLA) EQ &STR( ) THEN +
  SET &BCLA EQ &STR(*)
SET &OTHDSN EQ &STR()
SET &VSER EQ &STR()
ISPEXEC DISPLAY PANEL(LINK3B)
IF &LASTCC EQ 8 THEN +
  EXIT
ISPEXEC FTOPEN TEMP
SET &FTOPENRC EQ &LASTCC
ISPEXEC FTINCL LINK3B
ISPEXEC VPUT (PRJ1,LIB1,LNKT,BCLA,MEM,FLNK,FSL1,FSL2) PROFILE
END
