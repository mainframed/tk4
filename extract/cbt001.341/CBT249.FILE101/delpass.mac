PROC 0 DEBUG
CONTROL NOMSG PROMPT NOLIST NOFLUSH
IF &DEBUG NE &STR() THEN +
  CONTROL LIST CONLIST SYMLIST MSG
SET &DSNAMEIN EQ &STR()
SET &DELPW EQ &STR()
SET &CPW EQ &STR()
RESTART: +
ISPEXEC DISPLAY PANEL(PWDEL)
IF &LASTCC EQ 8 THEN +
  EXIT
CONTROL MSG
IF &CPW EQ &STR() THEN +
  PROTECT '&DSNAMEIN' DELETE(&DELPW)
ELSE +
  PROTECT '&DSNAMEIN'/&CPW DELETE(&DELPW)
CONTROL NOMSG
ISPEXEC CONTROL DISPLAY REFRESH
GOTO RESTART
END
