 DCL 1 DATASTAT BASED(ADDR(STATICTEXT)), /*                          */
         2 BYTELENGTH FIXED BIN,  /* LENGTH IN BYTES                 */
         2 STRUCT CHAR(8),        /* CONTAINING STRUCTURE            */
         2 OFFSET FIXED BIN,      /* BYTE OFFSET IN STRUCT           */
         2 DIMENSION FIXED BIN,   /* DIMENSION                       */
         2 INIVAL CHAR(8) VAR,    /* INITIAL VALUE (CONSTANT ONLY)   */
         2 POINTSTO CHAR(8);      /* IF PTR, WHAT IT POINTS TO       */
                                  /* IF STRUCTURE, WHAT POINTS TO IT */
                                  /* IF SUBSCRIPT, BASE ARRAY        */
 DCL DSTATLEN FIXED BIN STATIC INIT(44); /* LENGTH OF STATIC DATA    */
 DCL DVARLEN  FIXED BIN STATIC INIT(12); /* LENGTH OF VARIABLE DATA  */
