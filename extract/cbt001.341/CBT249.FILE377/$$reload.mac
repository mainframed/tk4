//RELOAD   EXEC PGM=IEBCOPY
//SYSPRINT DD SYSOUT=A
//SYSUT3   DD UNIT=SYSDA,SPACE=(CYL,(5,5)),DSN=&&SYSUT3,DISP=(,DELETE)
//SYSUT4   DD UNIT=SYSDA,SPACE=(CYL,(5,5)),DSN=&&SYSUT4,DISP=(,DELETE)
//IN1      DD DSN=SYSX.LIBSPOOL.TAPE.DATA,DISP=(OLD,PASS),
