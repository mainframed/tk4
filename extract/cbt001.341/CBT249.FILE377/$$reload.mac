//            LABEL=(1,BLP,EXPDT=98000)
//OT1      DD DSN=SYSX.LIBSPOOL.DATA,DISP=(NEW,CATLG,DELETE),
//            UNIT=SYSDA,SPACE=(TRK,(50,10,10),RLSE)
//*
//IN2      DD DSN=SYSX.LIBSPOOL.TAPE.CLIST,DISP=(OLD,PASS),
//            UNIT=TAPE,VOL=(,RETAIN,SER=LIBSPL),DCB=DEN=4,
//            LABEL=(2,BLP,EXPDT=98000)
