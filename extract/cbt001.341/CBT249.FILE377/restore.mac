//RESTORE  JOB (@SYSUID,@SYSACCT),'RESTORE LIBSPOOL',CLASS=B
/*JOBPARM   I,W,T=5,Q=T,L=5
//*
//* THIS JOBSTREAM CAN BE USED TO RESTORE THE LIBSPOOL LIBRARIAN
//* DATA SET FROM THE BACKUP MADE OF IT IN THE DAILY ARCHIVE JOBSTREAM.
//* THE BACKUP IS TAKEN BEFORE THE MODULES TAKEN OFFLINE ARE DELETED
//* FROM THE ONLINE LIBSPOOL LIBRARIAN DATA  SET.
//* CORRESPONDING DATA SET NAMES, VOLUME SERIAL NUMBERS, AND SPACE
//* ALLOCATION WILL HAVE TO BE CHANGED.
//* THE STEPS ARE AS FOLLOWS:
//*     INIT - INITIALIZES A NEW LIBSPOOL LIBRARIAN DATA SET.
//*            THIS STEP CAN BE SKIPPED IF THE EXISTING LIBRARIAN
//*            DATA SET IS USED.
//*
//*  RESTORE - RESTORE LIBSPOOL LIBRARIAN DATA SET.
//*
//INIT     EXEC PGM=LIBRARY,PARM='NRJS,NJTA'
//SYSPRINT  DD SYSOUT=A
//MASTER    DD DSN=SHSW.LSLIB,                                 CHANGE ?
//            DISP=(NEW,CATLG,DELETE),
//            SPACE=(CYL,30),                                  CHANGE ?
//            DCB=(DSORG=DA,BLKSIZE=2220),
//            VOL=SER=F031L1,                                  CHANGE ?
//            UNIT=3350
//SYSIN    DD  *
-OPT INIT,DISK,SEQ=/81,8,10,10/
-END
//RESTORE EXEC PGM=LIBRARY,PARM='NRJS,NJTA'
//SYSPRINT DD SYSOUT=A
//BACKUP   DD DSN=SHSW.LSLIB.BACKUP,DISP=(OLD,KEEP),           CHANGE ?
//            DCB=(BLKSIZE=32760),
//            LABEL=(2,SL),
//            VOL=SER=103872,                                  CHANGE ?
//            UNIT=TAPE
//MASTER   DD DSN=SHSW.LSLIB,DISP=SHR                          CHANGE ?
//SYSIN    DD  *
-OPT RESTORE
-END
