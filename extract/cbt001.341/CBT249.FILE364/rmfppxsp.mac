//PP      EXEC PGM=IEFBR14,REGION=(&REGN)
//*
//*            THIS STEP DELETES THE WORK DATA SETS
//*
//SRTINPUT DD  DISP=(MOD,DELETE,DELETE),UNIT=SYSDA,                    X
//             SPACE=(TRK,(001,001),RLSE),                             X
//       DSN=TECH.SM.D.RMF&SID..BD&DATE&DATE1..ED&DATE&DATE2..RAWDATA, X
//             DCB=(RECFM=VBS,LRECL=32760,BLKSIZE=23476)
//SORTOUT  DD  DISP=(MOD,DELETE,DELETE),UNIT=SYSDA,                    X
//             SPACE=(TRK,(001,001),RLSE),                             X
//       DSN=TECH.SM.D.RMF&SID..BD&DATE&DATE1..ED&DATE&DATE2..DATA,    X
//             DCB=(RECFM=VBS,LRECL=32760,BLKSIZE=23476)
//RMFDATE  DD  DISP=(MOD,DELETE,DELETE),UNIT=SYSDA,                    X
//             SPACE=(TRK,(001,001),RLSE),                             X
//       DSN=TECH.SM.D.RMF&SID..BD&DATE&DATE1..ED&DATE&DATE2..DATE,    X
//             DCB=(RECFM=FB,LRECL=080,BLKSIZE=9040)
//RMFDAACT DD  DISP=(MOD,DELETE,DELETE),UNIT=SYSDA,                    X
//             SPACE=(TRK,(001,001),RLSE),                             X
//       DSN=TECH.SM.D.RMF&SID..BD&DATE&DATE1..ED&DATE&DATE2..RMFDAACT,X
//             DCB=(RECFM=FB,LRECL=080,BLKSIZE=9040)
//RMFDABSY DD  DISP=(MOD,DELETE,DELETE),UNIT=SYSDA,                    X
//             SPACE=(TRK,(001,001),RLSE),                             X
//       DSN=TECH.SM.D.RMF&SID..BD&DATE&DATE1..ED&DATE&DATE2..RMFDABSY,X
//             DCB=(RECFM=FB,LRECL=080,BLKSIZE=9040)
//RMFDAPLT DD  DISP=(MOD,DELETE,DELETE),UNIT=SYSDA,                    X
//             SPACE=(TRK,(001,001),RLSE),                             X
//       DSN=TECH.SM.D.RMF&SID..BD&DATE&DATE1..ED&DATE&DATE2..RMFDAPLT,X
//             DCB=(RECFM=FB,LRECL=080,BLKSIZE=9040)
//RMFDAQUE DD  DISP=(MOD,DELETE,DELETE),UNIT=SYSDA,                    X
//             SPACE=(TRK,(001,001),RLSE),                             X
//       DSN=TECH.SM.D.RMF&SID..BD&DATE&DATE1..ED&DATE&DATE2..RMFDAQUE,X
//             DCB=(RECFM=FB,LRECL=080,BLKSIZE=9040)
//CTLGCTL1 EXEC PGM=IEFPARAM,REGION=(&REGN),                           X
//       PARM=(' UNCATLG DSNAME=:',                                    X
//        'TECH.SM.D.RMF&SID..BD&DATE&DATE1..ED&DATE&DATE2..DATA')
//*
//*            THIS STEP GENERATES AN IEHPROGM CONTROL CARD
//*            TO UNCATALOG
//*    DSN=TECH.SM.D.RMF&SID..BD&DATE&DATE1..ED&DATE&DATE2..DATA
//*
//SYSPUNCH DD  DISP=(NEW,PASS),UNIT=VSYSDA,DSN=&&CTLGCTL,              X
//             SPACE=(TRK,(1,1)),DCB=(RECFM=FB,LRECL=80,BLKSIZE=800)
//CTLGCTL2 EXEC PGM=IEFPARAM,REGION=(&REGN),                           X
//       PARM=(' UNCATLG DSNAME=:',                                    X
//        'TECH.SM.D.RMF&SID..BD&DATE&DATE1..ED&DATE&DATE2..DATE')
//*
//*            THIS STEP GENERATES AN IEHPROGM CONTROL CARD
//*            TO UNCATALOG
//*    DSN=TECH.SM.D.RMF&SID..BD&DATE&DATE1..ED&DATE&DATE2..DATE
//*
//SYSPUNCH DD  DISP=(MOD,PASS),DSN=&&CTLGCTL
//CTLGCTL3 EXEC PGM=IEFPARAM,REGION=(&REGN),                           X
//       PARM=(' UNCATLG DSNAME=:',                                    X
//        'TECH.SM.D.RMF&SID..BD&DATE&DATE1..ED&DATE&DATE2..RAWDATA')
//*
//*            THIS STEP GENERATES AN IEHPROGM CONTROL CARD
//*            TO UNCATALOG
//*    DSN=TECH.SM.D.RMF&SID..BD&DATE&DATE1..ED&DATE&DATE2..RAWDATA
//*
//SYSPUNCH DD  DISP=(MOD,PASS),DSN=&&CTLGCTL
//CTLGCTL4 EXEC PGM=IEFPARAM,REGION=(&REGN),                           X
//       PARM=(' UNCATLG DSNAME=:',                                    X
//        'TECH.SM.D.RMF&SID..BD&DATE&DATE1..ED&DATE&DATE2..RMFDAACT')
//*
//*            THIS STEP GENERATES AN IEHPROGM CONTROL CARD
//*            TO UNCATALOG
//*    DSN=TECH.SM.D.RMF&SID..BD&DATE&DATE1..ED&DATE&DATE2..RMFDAACT
//*
//SYSPUNCH DD  DISP=(MOD,PASS),DSN=&&CTLGCTL
//CTLGCTL5 EXEC PGM=IEFPARAM,REGION=(&REGN),                           X
//       PARM=(' UNCATLG DSNAME=:',                                    X
//        'TECH.SM.D.RMF&SID..BD&DATE&DATE1..ED&DATE&DATE2..RMFDABSY')
//*
//*            THIS STEP GENERATES AN IEHPROGM CONTROL CARD
//*            TO UNCATALOG
//*    DSN=TECH.SM.D.RMF&SID..BD&DATE&DATE1..ED&DATE&DATE2..RMFDABSY
//*
//SYSPUNCH DD  DISP=(MOD,PASS),DSN=&&CTLGCTL
//CTLGCTL6 EXEC PGM=IEFPARAM,REGION=(&REGN),                           X
//       PARM=(' UNCATLG DSNAME=:',                                    X
//        'TECH.SM.D.RMF&SID..BD&DATE&DATE1..ED&DATE&DATE2..RMFDAPLT')
//*
//*            THIS STEP GENERATES AN IEHPROGM CONTROL CARD
//*            TO UNCATALOG
//*    DSN=TECH.SM.D.RMF&SID..BD&DATE&DATE1..ED&DATE&DATE2..RMFDAPLT
//*
//SYSPUNCH DD  DISP=(MOD,PASS),DSN=&&CTLGCTL
//CTLGCTL7 EXEC PGM=IEFPARAM,REGION=(&REGN),                           X
//       PARM=(' UNCATLG DSNAME=:',                                    X
//        'TECH.SM.D.RMF&SID..BD&DATE&DATE1..ED&DATE&DATE2..RMFDAQUE')
//*
//*            THIS STEP GENERATES AN IEHPROGM CONTROL CARD
//*            TO UNCATALOG
//*    DSN=TECH.SM.D.RMF&SID..BD&DATE&DATE1..ED&DATE&DATE2..RMFDAQUE
//*
//SYSPUNCH DD  DISP=(MOD,PASS),DSN=&&CTLGCTL
//UNCATLG  EXEC PGM=IEHPROGM,REGION=(&REGN)
//*
//*            THIS STEP UNCATALOGS THE DATASETS IF THEY WERE
//*            NOT UNCATALOGED AND DELETED BY STEP "PP" ABOVE.
//*
//SYSIN    DD  DISP=(OLD,DELETE),DSN=&&CTLGCTL
//SYSPRINT DD  SYSOUT=*
//RMFDATE  EXEC PGM=IEFPARAM,REGION=(&REGN),                           X
//       PARM=('  DATE(&DATE&DATE1,&DATE&DATE2):',                     X
//             ' /*RMF RECORD SELECTION DATES */;',                    X
//             '  SYSID(&SID) /*RMF RECORD SELECTION SMFID*/')
//*
//*            THIS STEP GENERATES THE RMF CONTROL STATEMENT
//*            CONTAINING THE DATE(S) FOR SELECTED RMF RECORDS.
//*            THE DATA SET CREATED IN THIS STEP IS CONCATENATED
//*            AS PART OF THE CONTROL STATEMENT INPUT TO THE
//*            RMF POST PROCESSOR.
//*
//*
//SYSPUNCH DD  DISP=(NEW,CATLG,DELETE),UNIT=SYSDA,
//             SPACE=(TRK,(001,001),RLSE),                             X
//       DSN=TECH.SM.D.RMF&SID..BD&DATE&DATE1..ED&DATE&DATE2..DATE,    X
//             DCB=(RECFM=FB,LRECL=080,BLKSIZE=9040)
//SMFCTL1 EXEC PGM=IEFPARAM,REGION=(&REGN),                            X
//       PARM=('  INDD(SMFINPUT,OPTIONS(DUMP))  /*OUTPUT DATA SET*/')
//*
//*            THESE STEPS GENERATE CONTROL CARDS FOR IFASMFDP
//*            TO SELECT THE DESIRED RMF RECORDS FOR SORTING.
//*            RECORD SELECTION IS BASED ON RMF RECORD TYPE,
//*            TYPE OF SYSTEM ON WHICH THE RECORD WAS PRODUCED
//*            (MVS/370), AND SYSTEM IDENTIFIER
//*            TO REDUCE THE VOLUME OF INPUT PASSED TO THE SORT.
//*
//*            NOTE THAT IFASMFDP CONTROL STATEMENTS ARE NOT
//*            GENERATED TO CONTROL RECORD SELECTION BASED ON
//*            DATE.  THERE IS A DISCREPANCY BETWEEN THE SMF DATE
//*            AND RMF DATE WHICH AFFECTS THE RMF RECORDS
//*            REPORTING THE LAST INTERVAL OF THE DAY.  THE RMF
//*            DATE REFLECTS THE START OF THE REPORTING INTERVAL,
//*            AND THE SMF DATE INDICATES WHEN THE RECORD WAS
//*            WRITTEN TO SMF.  FOR THE RMF RECORDS REPORTING THE
//*            LAST INTERVAL OF A DAY, THERE IS A USUALLY A ONE
//*            DAY DISCREPANCY BETWEEN THE DATES.
//*
//*            DATE SELECTION IS DONE IN THE SORT STEP ONCE
//*            THE INPUT DATA HAS BEEN LIMITED TO RMF RECORDS.
//*
//*
//SYSPUNCH DD  DISP=(NEW,PASS),UNIT=VSYSDA,DSN=&&SMFCTL,               X
//             SPACE=(TRK,(1,1)),DCB=(RECFM=FB,LRECL=80,BLKSIZE=800)
//SMFCTL2 EXEC PGM=IEFPARAM,REGION=(&REGN),                            X
//       PARM=('  OUTDD(SRTINPUT,TYPE(70::79)) /*RMF RECORD TYPES*/')
//SYSPUNCH DD  DISP=(MOD,PASS),DSN=&&SMFCTL
//SMFCTL3 EXEC PGM=IEFPARAM,REGION=(&REGN),                            X
//       PARM=('  SID(&SID)               /*SMF SYSTEM IDENTIFIER*/')
//SYSPUNCH DD  DISP=(MOD,PASS),DSN=&&SMFCTL
//SMFCTL4 EXEC PGM=IEFPARAM,REGION=(&REGN),                            X
//       PARM=('  USER1(IFAUX1MV)   /*SELECT ONLY MVS/370 RECORDS*/')
//SYSPUNCH DD  DISP=(MOD,PASS),DSN=&&SMFCTL
//SMFCTL5 EXEC PGM=IEFPARAM,REGION=(&REGN),                            X
//       PARM=('  USER2(IFAUX2HT) /*ELIMINATE GENERATED TYPES 2/3*/')
//SYSPUNCH DD  DISP=(MOD,PASS),DSN=&&SMFCTL
//SMFDP   EXEC PGM=&SMFDP,REGION=(&REGN)
//*
//*            THIS STEP SELECTS RMF RECORDS FROM THE SMF INPUT FILE
//*            FOR INPUT TO THE SORT.
//*
//*            NOTE THAT THE IFASMFDP PROGRAM NORMALLY WRITES
//*            TYPE 2 (SMF DUMP HEADER) AND TYPE 3 (SMF DUMP TRAILER)
//*            RECORDS TO THE OUTPUT DATE SET REGARDLESS OF THE
//*            RECORD TYPES SELECTED.  THIS IS DONE EVEN IF RECORD
//*            TYPES 2 AND 3 ARE NOT BEING SELECTED.  THESE
//*            VESTIGIAL RECORDS ARE ELIMINATED BY USER EXIT
//*            IFAUX2HT.
//*
//*
//SMFINPUT DD  DISP=SHR,DSN=TECH.SM.D.P22000W(0),   USE GDG INPUT      X
//             DCB=(RECFM=VBS,LRECL=32760,BLKSIZE=32760,BUFNO=03)
//         DD  DISP=SHR,DSN=TECH.SM.D.P22000W(-1),  USE GDG INPUT      X
//             DCB=(RECFM=VBS,LRECL=32760,BLKSIZE=32760,BUFNO=03)
//         DD  DISP=SHR,DSN=TECH.SM.D.P22000W(-2),  USE GDG INPUT      X
//             DCB=(RECFM=VBS,LRECL=32760,BLKSIZE=32760,BUFNO=03)
//         DD  DISP=SHR,DSN=TECH.SM.D.P22000W(-3),  USE GDG INPUT      X
//             DCB=(RECFM=VBS,LRECL=32760,BLKSIZE=32760,BUFNO=03)
//         DD  DISP=SHR,DSN=TECH.SM.D.P22000W(-4),  USE GDG INPUT      X
//             DCB=(RECFM=VBS,LRECL=32760,BLKSIZE=32760,BUFNO=03)
//         DD  DISP=SHR,DSN=TECH.SM.D.P22000W(-5),  USE GDG INPUT      X
//             DCB=(RECFM=VBS,LRECL=32760,BLKSIZE=32760,BUFNO=03)
//SRTINPUT DD  DISP=(NEW,CATLG,DELETE),UNIT=(SYSDA,2),                 X
//             SPACE=(CYL,(200,050),RLSE),
//       DSN=TECH.SM.D.RMF&SID..BD&DATE&DATE1..ED&DATE&DATE2..RAWDATA, X
//             DCB=(RECFM=VBS,LRECL=32760,BLKSIZE=23476,BUFNO=02)
//SYSIN    DD  DISP=(OLD,DELETE),DSN=&&SMFCTL
//SYSPRINT DD  SYSOUT=*
//SYSUDUMP DD  SYSOUT=*
//SORTCTL1 EXEC PGM=IEFPARAM,REGION=(&REGN),                           X
//      PARM=(' SORT FIELDS=(25,04,PD,A,21,04,PD,A),EQUALS')
//*
//*            THIS STEP GENERATES THE CONTROL CARD FOR "SORT"
//*            TO CONTROL SORTING BASED ON RMF'S DATE AND
//*            TIME OF DAY.
//*
//SYSPUNCH DD  DISP=(NEW,PASS),UNIT=VSYSDA,DSN=&&SORTCTL,              X
//             SPACE=(TRK,(1,1)),DCB=(RECFM=FB,LRECL=80,BLKSIZE=800)
//SORTCTL2 EXEC PGM=IEFPARAM,REGION=(&REGN),                           X
//      PARM=(' OMIT COND=(:',                                         X
//            '(25,4,PD,LT,&DATE&DATE1),OR',                           X
//            '(25,4,PD,GT,&DATE&DATE2))')
//*
//*            THIS STEP GENERATES THE CONTROL CARD FOR "SORT"
//*            TO ELIMINATE RMF RECORDS OUTSIDE THE DESIRED
//*            DATE RANGE.
//*
//SYSPUNCH DD  DISP=(MOD,PASS),DSN=&&SORTCTL
//SORTCTL9 EXEC PGM=IEFPARAM,REGION=(&REGN),                           X
//      PARM=(' END')
//*
//*            THIS STEP GENERATES THE  "END" CONTROL STATEMENT
//*            REQUIRED BY "SORT".
//*
//SYSPUNCH DD  DISP=(MOD,PASS),DSN=&&SORTCTL
//SORT    EXEC PGM=SORT,REGION=(&REGN)
//*
//*            THIS STEP ELIMINATES RMF RECORDS WHICH ARE OUTSIDE
//*            THE DESIRED DATE RANGE, AND SORTS THE REMAINING
//*            RMF RECORDS INTO DATE/TIME INTERVAL ORDER.
//*            THE OUTPUT DATA SET CREATED BY THIS STEP IS
//*            CATALOGED AND CAN BE PROCESSED BY
//*            PROCEDURE "RMFPPDSP".
//*
//SORTMSG  DD  SYSOUT=*          C.U.'S $ORTPARM EXIT SENDS MSGS HERE
//SORTWK01 DD  UNIT=SYSDA,SPACE=(CYL,(150,025))
//SORTWK02 DD  UNIT=SYSDA,SPACE=(CYL,(150,025))
//SORTWK03 DD  UNIT=SYSDA,SPACE=(CYL,(150,025))
//SORTIN   DD  DISP=(OLD,DELETE),DCB=BUFNO=02,                         X
//       DSN=TECH.SM.D.RMF&SID..BD&DATE&DATE1..ED&DATE&DATE2..RAWDATA
//SORTOUT  DD  DISP=(NEW,CATLG,DELETE),UNIT=SYSDA,                     X
//             SPACE=(CYL,(100,010),RLSE),                             X
//       DSN=TECH.SM.D.RMF&SID..BD&DATE&DATE1..ED&DATE&DATE2..DATA,    X
//             DCB=(RECFM=VBS,LRECL=32760,BLKSIZE=23476,BUFNO=02)
//SYSIN    DD  DISP=(OLD,DELETE),DSN=&&SORTCTL
//SYSOUT   DD  SYSOUT=*
//SYSPRINT DD  SYSOUT=*
//RMFDVTBL EXEC PGM=&RMFDVTB,REGION=(&REGN),                           X
//        PARM='REPORT,COUNT,ISASIZE(-128K)/&SID&DATE&DATE1&DATE2'
//*
//*            THIS STEP PROCESSES THE OUTPUT DATA SET FROM THE SORT
//*            CONTAINING SELECTED RMF RECORDS AND GENERATES RMF
//*            CONTROL CARD DATA SETS THAT CAN BE PASSED TO THE
//*            RMF POST PROCESSOR TO PLOT DASD DEVICE ACTIVITY
//*            (RMFDAPLT), OR REQUEST DASD EXCEPTION REPORTS
//*            FOR DEVICE ACTIVITY RATE (RMFDAACT), DEVICE BUSY
//*            (RMFDABSY), OR DEVICE QUEUE LENGTH (RMFDAQUE).
//*
//STEPLIB  DD  DISP=SHR,DSN=TS13TJA.TOOLS.LOAD
//SMFIN    DD  DISP=SHR,        /*SELECTED AND SORTED RMF DATA*/       X
//             DCB=BUFNO=02,                                           X
//       DSN=TECH.SM.D.RMF&SID..BD&DATE&DATE1..ED&DATE&DATE2..DATA
//RMFDAACT DD  DISP=(NEW,CATLG,DELETE),UNIT=SYSDA,                     X
//             SPACE=(CYL,(001,001),RLSE),                             X
//       DSN=TECH.SM.D.RMF&SID..BD&DATE&DATE1..ED&DATE&DATE2..RMFDAACT,X
//             DCB=(RECFM=FB,LRECL=080,BLKSIZE=9040)
//RMFDABSY DD  DISP=(NEW,CATLG,DELETE),UNIT=SYSDA,                     X
//             SPACE=(CYL,(001,001),RLSE),                             X
//       DSN=TECH.SM.D.RMF&SID..BD&DATE&DATE1..ED&DATE&DATE2..RMFDABSY,X
//             DCB=(RECFM=FB,LRECL=080,BLKSIZE=9040)
//RMFDAPLT DD  DISP=(NEW,CATLG,DELETE),UNIT=SYSDA,                     X
//             SPACE=(CYL,(001,001),RLSE),                             X
//       DSN=TECH.SM.D.RMF&SID..BD&DATE&DATE1..ED&DATE&DATE2..RMFDAPLT,X
//             DCB=(RECFM=FB,LRECL=080,BLKSIZE=9040)
//RMFDAQUE DD  DISP=(NEW,CATLG,DELETE),UNIT=SYSDA,                     X
//             SPACE=(CYL,(001,001),RLSE),                             X
//       DSN=TECH.SM.D.RMF&SID..BD&DATE&DATE1..ED&DATE&DATE2..RMFDAQUE,X
//             DCB=(RECFM=FB,LRECL=080,BLKSIZE=9040)
//RMFDVLST DD  SYSOUT=*           /*DEVICE TABLE LISTING*/
//SYSPRINT DD  SYSOUT=*
//PLIDUMP  DD  SYSOUT=*
