         TITLE 'IEAVTRT2 RTM2 INITIALIZATION                           *
                        '
IEAVTRT2 CSECT ,                                                   0001
@MAINENT BALR  @15,0                                               0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'IEAVTRT2  77.285'                                 0001
         DROP  @15
@PROLOG  BALR  @06,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@06                                         0001
RBPRFXLN EQU 64
RBPRFLNA EQU 32
PRBLEN   EQU 136
SIRBLEN  EQU 200
TIRBLEN  EQU 136
IRBLEN   EQU 128
SVRBLEN  EQU 224
*                                                                  0069
*   /*****************************************************************/
*   /*                                                               */
*   /* SEGMENT IEAVTRT2 IS THE TOP RTM2 SEGMENT. IT INCLUDES SEGMENT */
*   /* RT2INCTL TO CONTROL ENTRY INITIALIZATION AND ACQUIRE A WORK   */
*   /* AREA AND SEGMENT RT2INWA TO INITIALIZE THE WORK AREA. IT THEN */
*   /* CALLS MODULE IEAVTRTC TO CONTROL THE MAIN PATH PROCESSING     */
*   /* THROUGH RTM2. FINALLY IT CALLS IEAVTRTE TO EFFECT THE EXIT. IF*/
*   /* CONTROL IS RETURNED TO IT AT ANY TIME WITH A RETURN CODE OF 4 */
*   /* IT INVOKES THE CRITICAL ERROR ROUTINE (SEGMENT RT2CRERR) TO   */
*   /* TERMINATE THE MEMORY.                                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0069
*   TCBNONPR=OFF;                   /* RTM2 SHOULD NOT RUN         0069
*                                      NONPREEMPTIVE         @Z40FPXH*/
         NI    TCBNONPR(R4),B'11101111'                            0069
*   IF TCBEOT=OFF THEN              /* THIS ENTRY IS NOT DUE TO AN 0070
*                                      ERROR IN SVC 3                */
         TM    TCBEOT(R4),B'00000100'                              0070
         BNZ   @RF00070                                            0070
*     DO;                                                          0071
*                                                                  0071
*       /*************************************************************/
*       /*                                                           */
*       /* SEGMENT RT2INCTL CONTROLS ENTRY INITIALIZATION. IT        */
*       /* INCLUDES SEGMENT RT2INESA TO SAVE INPUT DATA AND FLAGS IN */
*       /* THE ESA AND SEGMENT RT2GETWA TO OBTAIN AN RTM2 WORK AREA. */
*       /* IF IT IS NOT NECESSARY TO GET A WORK AREA IT ESTABLISHES  */
*       /* THE NECESSARY ADDRESSING FOR THE ONE THAT EXISTS. ON ENTRY*/
*       /* REG 3 LOCATES THE CVT, REG 4, THE TCB, REG 5, THE SVRB AND*/
*       /* REG 7, THE ASCB. NO LOCK IS HELD. IF IT DETECTS AN ERROR  */
*       /* IT RETURNS A CODE OF 4. OTHERWISE IT RETURNS A CODE OF 0. */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0072
*       DO;                         /* BEGIN SEGMENT RT2INCTL        */
*                                                                  0072
*         /***********************************************************/
*         /*                                                         */
*         /* SEGMENT RT2INESA SAVES ENTRY DATA IN THE ESA AND        */
*         /* INITIALIZES FLAGS IN THE TCB AND SVRB. REGISTERS        */
*         /* 0,1,3,4,5,7, AND 14 ARE SAVED IN THE ESA. THE RBABEND   */
*         /* FLAG IS SET ON. THE LOCAL LOCK IS OBTAINED TO SERIALIZE */
*         /* ACCESS TO TCB FIELDS. THE TCBPGNLY FLAG IS COPIED TO THE*/
*         /* ESA AND TURNED OFF IN THE TCB. THE TCBRTM2 FLAG IS      */
*         /* COPIED TO THE ESA AND TURNED ON IN THE TCB. IF THE      */
*         /* TCBABTRM FLAG IS ON, THAT FLAG AND THE TCBCMP AND       */
*         /* TCBRTM12 FIELDS ARE COPIED TO THE ESA. THEN THE TCBABTRM*/
*         /* AND TCBRTM12 FIELDS ARE CLEARED IN THE TCB. THE TCBFX   */
*         /* AND TCBFPRAP FLAGS ARE TURNED ON. THE LOCAL LOCK IS THEN*/
*         /* RELEASED. ON ENTRY REG 4 LOCATES THE TCB AND REG 5, THE */
*         /* SVRB. NO LOCK IS HELD ON ENTRY OR EXIT. IF IT DETECTS AN*/
*         /* ERROR IT SETS ESAFERR ON                                */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0073
*         DO;                       /* BEGIN SEGMENT RT2INESA        */
*           RTM2ESA=''B;            /* CLEAR ESA                     */
*                                                                  0074
         LA    @12,96                                              0074
         ALR   @12,R5                                              0074
         XC    RTM2ESA(48,@12),RTM2ESA(@12)                        0074
*           /*********************************************************/
*           /*                                                       */
*           /* SAVE INPUT REGISTERS                                  */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0075
*           ESAR0=R0;               /* PARAMETERS                    */
         ST    R0,ESAR0(,@12)                                      0075
*           ESAR1=R1;               /* PARAMETERS                    */
         ST    R1,ESAR1(,@12)                                      0076
*           ESAR3=R3;               /* CVT ADDRESS                   */
         ST    R3,ESAR3(,@12)                                      0077
*           ESAR4=R4;               /* TCB ADDRESS                   */
         ST    R4,ESAR4(,@12)                                      0078
*           ESAR5=R5;               /* SVRB ADDRESS                  */
         ST    R5,ESAR5(,@12)                                      0079
*           ESAR7=R7;               /* ASCB ADDRESS                  */
         ST    R7,ESAR7(,@12)                                      0080
*           ESAR14=R14;             /* RETURN ADDRESS                */
         ST    R14,ESAR14(,@12)                                    0081
*           R7=ADDR(RTM2ESA);       /* SET REG WITH ADDR OF RTM ESA  */
         LR    R7,@12                                              0082
*           RESPECIFY                                              0083
*             RTM2ESA BASED(R7);                                   0083
*           RESPECIFY                                              0084
*            (R0,                                                  0084
*             R1,                                                  0084
*             R14) UNRESTRICTED;    /* RELEASE REGS NOT USED TO    0084
*                                      LOCATE CONTROL BLOCKS         */
*           RBABEND=ON;             /* FLAG RB AS BELONGING TO RTM2  */
*                                                                  0085
         LR    @14,R5                                              0085
         AL    @14,@CF03202                                        0085
         OI    RBABEND-56(@14),B'00100000'                         0085
*           /*********************************************************/
*           /*                                                       */
*           /* TEST FOR NORMAL END OF TASK AND MEMTERM. OBTAIN LOCAL */
*           /* LOCK IF NEITHER. TCB FLAGS ARE NOT SET IN THOSE CASES */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0086
*RT2NE0T:                                                          0086
*           ;                       /* THIS STATEMENT IS LABELLED TO
*                                      PREVENT USE OF COMPILER     0086
*                                      TEMPORARY                     */
RT2NE0T  DS    0H                                                  0087
*           IF TCBABTRM=OFF&ESAEOT1=ON&TCBRTM2=OFF&RBLINKB->RBTCBNXT=ON
*             THEN                  /* NORMAL END OF TASK            */
         TM    TCBABTRM(R4),B'00100000'                            0087
         BNZ   @RF00087                                            0087
         TM    ESAEOT1(R7),B'00001000'                             0087
         BNO   @RF00087                                            0087
         TM    TCBRTM2(R4),B'00001000'                             0087
         BNZ   @RF00087                                            0087
         L     @14,RBLINKB-1(,R5)                                  0087
         TM    RBTCBNXT(@14),B'10000000'                           0087
         BNO   @RF00087                                            0087
*             ESANEOT=ON;           /* SET ESA FLAG TO SHOW NORMAL 0088
*                                      EOT                           */
         OI    ESANEOT(R7),B'00001000'                             0088
*           ELSE                    /* NOT NORMAL END OF TASK        */
*             IF TCBABTRM=OFF       /*                       @ZA16412*/
*                 &ESAEOM1=ON       /*                       @ZA16412*/
*                 &ESAR7->ASCBASID=MSTRID THEN/* MEMTERM ENTRY     0089
*                                                            @ZA16412*/
         B     @RC00087                                            0089
@RF00087 TM    TCBABTRM(R4),B'00100000'                            0089
         BNZ   @RF00089                                            0089
         TM    ESAEOM1(R7),B'00010000'                             0089
         BNO   @RF00089                                            0089
         L     @14,ESAR7(,R7)                                      0089
         CLC   ASCBASID(2,@14),@CH00067                            0089
         BNE   @RF00089                                            0089
*               ESAVEOM=ON;         /* TURN ON VALID EOM SW  @ZA16412*/
         OI    ESAVEOM(R7),B'00000010'                             0090
*             ELSE                  /*                       @ZA16412*/
*               DO;                 /*                       @ZA16412*/
*                                                                  0091
         B     @RC00089                                            0091
@RF00089 DS    0H                                                  0092
*                 /***************************************************/
*                 /*                                                 */
*                 /* OBTAIN LOCAL LOCK IF NOT RECURSION IN SETLOCK   */
*                 /*                                                 */
*                 /***************************************************/
*                                                                  0092
*                 IF TCBABTRM=ON&TCBCMPC=CNVCODE THEN/* ENTERED FOR
*                                      CONVERT TO STEP               */
         TM    TCBABTRM(R4),B'00100000'                            0092
         BNO   @RF00092                                            0092
         CLC   TCBCMPC(3,R4),@CB00063                              0092
         BNE   @RF00092                                            0092
*                   ESACTS=ON;      /* INDICATE CONVERT-TO-STEP      */
         OI    ESACTS(R7),B'00010000'                              0093
*                 ESAGLREC=ON;      /* MARK RB AS IN SETLOCK PROCESS */
@RF00092 OI    ESAGLREC(R7),B'00010000'                            0094
*                 DO;                                              0095
*                   RESPECIFY                                      0096
*                     R2 RESTRICTED;                               0096
*                   R2=RBLINKB;     /* LOCATE STARTING RB            */
*                                                                  0097
         L     R2,RBLINKB-1(,R5)                                   0097
         LA    R2,0(,R2)                                           0097
*                   /*************************************************/
*                   /*                                               */
*                   /* SCAN FOR RB WITH RBABEND ON. SCAN TERMINATED  */
*                   /* WHEN RBTCBNXT ON                              */
*                   /*                                               */
*                   /*************************************************/
*                                                                  0098
*                   DO WHILE(EVAL(R2-LENGTH(RBPRFX))->RBABEND='0'B&R2->
*                         RBTCBNXT='0'B);                          0098
         B     @DE00098                                            0098
@DL00098 DS    0H                                                  0099
*                     R2=R2->RBLINKB;/* LOCATE NEXT RB               */
         L     @14,RBLINKB-1(,R2)                                  0099
         LA    @14,0(,@14)                                         0099
         LR    R2,@14                                              0099
*                   END;            /* END SCAN OF RBS               */
@DE00098 LR    @14,R2                                              0100
         SL    @14,@CF00482                                        0100
         TM    RBABEND(@14),B'00100000'                            0100
         BNZ   @DC00098                                            0100
         TM    RBTCBNXT(R2),B'10000000'                            0100
         BZ    @DL00098                                            0100
@DC00098 DS    0H                                                  0101
*                   IF EVAL(R2-LENGTH(RBPRFX))->RBABEND='0'B THEN/*
*                                      CHECK WHETHER ONE WAS FOUND   */
         LR    @14,R2                                              0101
         SL    @14,@CF00482                                        0101
         TM    RBABEND(@14),B'00100000'                            0101
         BNZ   @RF00101                                            0101
*                     R2=0;         /* NO RTM2 RB FOUND              */
         SLR   R2,R2                                               0102
*                 END;                                             0103
@RF00101 DS    0H                                                  0104
*                 IF R2=0 THEN      /* NO PREV RTM2 RB FOUND @ZA01861*/
         LTR   R2,R2                                               0104
         BNZ   @RF00104                                            0104
*                   IF TCBRTM2=ON THEN/* IF RECURSIVE ENTRY, SHOULD
*                                      BE AN RTM2 RB UNLESS CTS    0105
*                                                            @ZA01861*/
         TM    TCBRTM2(R4),B'00001000'                             0105
         BNO   @RF00105                                            0105
*                     IF ESACTS=OFF THEN/* NOT CONVERT TO STEP CASE  */
         TM    ESACTS(R7),B'00010000'                              0106
         BNZ   @RF00106                                            0106
*                       ESAFERR=ON; /* INDICATE DISASTROUS ERROR     */
         OI    ESAFERR(R7),B'00001000'                             0107
*                     ELSE                                         0108
*                       ;           /* CONVERT-TO-STEP: NO ACTION    */
@RF00106 DS    0H                                                  0109
*                   ELSE                                           0109
*                     ;             /* NOT RECURSIVE ENTRY   @ZA01861*/
@RF00105 DS    0H                                                  0110
*                 ELSE              /* PREVIOUS RTM2 RB FOUND        */
*                   DO;             /*                       @ZA08087*/
         B     @RC00104                                            0110
@RF00104 DS    0H                                                  0111
*                     ESARFLAG=ADDR(R2->RBEXSAVE)->ESARFLAG;/* COPY
*                                      RECURSION FLAGS FROM PREVIOUS
*                                      RTM2 ESA              @ZA08087*/
         LA    @14,96                                              0111
         ALR   @14,R2                                              0111
         MVC   ESARFLAG(1,R7),ESARFLAG(@14)                        0111
*                     IF ADDR(R2->RBEXSAVE)->ESAGLREC=ON THEN/*    0112
*                                      SETLOCK RECURSION HAS OCCURED */
         TM    ESAGLREC(@14),B'00010000'                           0112
         BNO   @RF00112                                            0112
*                       ESAFERR=ON; /* PROCESSING CANNOT CONTINUE  0113
*                                      WITHOUT LOCAL LOCK            */
         OI    ESAFERR(R7),B'00001000'                             0113
*                   END;            /*                       @ZA08087*/
@RF00112 DS    0H                                                  0115
*                 IF ESAFERR=OFF THEN/* NO PREVIOUS SETLOCK ERROR -
*                                      PROCESSING CAN CONTINUE       */
@RC00104 TM    ESAFERR(R7),B'00001000'                             0115
         BNZ   @RF00115                                            0115
*                   DO;                                            0116
*                     DO;           /* SETLOCK OBTAIN TYPE(LOCAL)MODE
*                                      (UNCOND)RELATED(TCB,IEAVTRT2(R
*                                      T2INESA))                     */
*                       RESPECIFY                                  0118
*                        (GPR11P,                                  0118
*                         GPR12P,                                  0118
*                         GPR13P,                                  0118
*                         GPR14P) RESTRICTED;/* RESTRICT REGS USED BY
*                                      SETLOCK                       */
*                       GENERATE REFS(PSALITA,FLC) SETS(GPR11P,GPR12P,
*                           GPR13P,GPR14P);                        0119
         SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,                        C
               RELATED=(TCB,IEAVTRT2(RT2INESA))
*                       RESPECIFY                                  0120
*                        (GPR11P,                                  0120
*                         GPR12P,                                  0120
*                         GPR13P,                                  0120
*                         GPR14P) UNRESTRICTED;/* RELEASE REGS USED
*                                      BY SETLOCK                    */
*                     END;          /* SETLOCK OBTAIN TYPE(LOCAL)MODE
*                                      (UNCOND)RELATED(TCB,IEAVTRT2(R
*                                      T2INESA)) OBTAIN LOCAL LOCK   */
*                     ESAGLREC=OFF; /* LOCK OBTAINED WITHOUT FAILURE */
*                                                                  0122
         NI    ESAGLREC(R7),B'11101111'                            0122
*                     /***********************************************/
*                     /*                                             */
*                     /* COPY TCB DATA AND SET TCB FLAGS AND FIELDS  */
*                     /*                                             */
*                     /***********************************************/
*                                                                  0123
*                     IF TCBPGNLY=ON THEN/* ENTRY IS FOR PURGE ONLY  */
         TM    TCBPGNLY(R4),B'00010000'                            0123
         BNO   @RF00123                                            0123
*                       DO;                                        0124
*                         ESAPGNLY=ON;/* SAVE FLAG                   */
         OI    ESAPGNLY(R7),B'00100000'                            0125
*                         TCBPGNLY=OFF;/* CLEAR FLAG SO THAT       0126
*                                      RECURSION WON'T BE TAKEN FOR
*                                      PURGE ONLY                    */
         NI    TCBPGNLY(R4),B'11101111'                            0126
*                       END;                                       0127
*                     ELSE                                         0128
*                       ;           /* NOT PURGE ONLY                */
@RF00123 DS    0H                                                  0129
*                     IF TCBRTM2=ON&R2^=0 THEN/* RECURSION           */
         TM    TCBRTM2(R4),B'00001000'                             0129
         BNO   @RF00129                                            0129
         LTR   R2,R2                                               0129
         BZ    @RF00129                                            0129
*                       DO;                                        0130
*                         ESARTM2=ON;/* SAVE FLAG                    */
         OI    ESARTM2(R7),B'10000000'                             0131
*                         ESARFLAG=ADDR(R2->RBEXSAVE)->ESARFLAG;/* 0132
*                                      COPY RECURSION FLAGS FROM   0132
*                                      PREVIOUS RTM2 ESA             */
         MVC   ESARFLAG(1,R7),ESARFLAG+96(R2)                      0132
*                                                                  0133
*                         /*******************************************/
*                         /*                                         */
*                         /* PSEUDO RETRY FOR SLIP FAILURE. THIS WILL*/
*                         /* HANDLE RECURSION DUE TO A SLIP ERROR    */
*                         /*                                 @G17SP2I*/
*                         /*                                         */
*                         /*******************************************/
*                                                                  0133
*                         IF ESASLIPR=ON&/* PERFORM PSEUDO RETRY FOR */
*                             ESAPGNLY=OFF THEN/* SLIP FAILURE IF NEW
*                                      ENTRY TO RTM2 IS NOT FOR PURGE
*                                      ONLY                  @YM06385*/
         TM    ESASLIPR(R7),B'00000010'                            0133
         BNO   @RF00133                                            0133
         TM    ESAPGNLY(R7),B'00100000'                            0133
         BNZ   @RF00133                                            0133
*                           IF R2=RBLINKB THEN/* AND IF PREVIOUS RB
*                                      IS AN                         */
         L     @14,RBLINKB-1(,R5)                                  0134
         LA    @14,0(,@14)                                         0134
         CR    R2,@14                                              0134
         BNE   @RF00134                                            0134
*                             DO;   /* RTM2 RB SET UP FOR PSEUDO   0135
*                                      RETRY                 @YM06385*/
*                               R2->RBOPSW(6:8)=ADDR(SLIPRTRY);/*  0136
*                                      MOVE RETRY ADDR INTO PREVIOUS
*                                      RB                    @YM06385
*                                                            @G17SP2I*/
         LA    @14,SLIPRTRY                                        0136
         STCM  @14,7,RBOPSW+5(R2)                                  0136
*                               RFY                                0137
*                                 R12 RSTD;/* GET REG FOR ADDR PREV
*                                      ESA                   @YM06385*/
*                               R12=ADDR(R2->RBEXSAVE);/* LOCATE   0138
*                                      PREVIOUS ESA          @YM06385*/
         LA    R12,RBEXSAVE(,R2)                                   0138
*                               RBGRSAVE=R12->ESART2WA->RTM2RRG;/* 0139
*                                      PUT INTO RB REGS TO BE LOADED
*                                      BY EXIT PRO (NOTE: ONLY REGS
*                                      2-14 WILL BE LOADED FRON THIS
*                                      SAVEAREA. REGS 0,1,&15 WILL 0139
*                                      HAVE THEIR CURRENT VALUES). 0139
*                                                            @YM06385*/
         L     @14,ESART2WA(,R12)                                  0139
         MVC   RBGRSAVE(64,R5),RTM2RRG(@14)                        0139
*                               R12->ESART2WA->RTM2SCTR(RTM2SLPS)=ON;
         OI    RTM2SCTR+1(@14),B'00010000'                         0140
*                                   /* INSURE THAT SLIP WILL BE    0141
*                                      BYPASSED FOR FURTHER RTM2   0141
*                                      PROCESSING OF THIS TASK     0141
*                                                            @YM06385*/
*                                                                  0141
*                               /*************************************/
*                               /*                                   */
*                               /* CLEAR TCB FIELDS SET BY RTM1 WHEN */
*                               /* HANDLING SLIP ERROR       @YM06385*/
*                               /*                                   */
*                               /*************************************/
*                                                                  0141
*                               TCBMODE=0;/* FORGET MODE OF SLIP   0141
*                                      ERROR                         */
         MVI   TCBMODE(R4),X'00'                                   0141
*                               TCBERTYP=0;/* FORGET TYPE OF SLIP  0142
*                                      ERROR                         */
         MVI   TCBERTYP(R4),X'00'                                  0142
*                               TCBABGM=OFF;/* TURN OFF FLAG SET BY
*                                      RTM1                          */
         NI    TCBABGM(R4),B'11101111'                             0143
*                               IF TCBRTM12>EEDNULL THEN/* IF RTM1 0144
*                                      AQUIRED EEDS TO HANDLE SLIP 0144
*                                      ERROR, THEY MUST BE FREED BY
*                                      RTM2                  @YM06385*/
         LA    @14,1                                               0144
         C     @14,TCBRTM12(,R4)                                   0144
         BNL   @RF00144                                            0144
*                                 IF R12->ESAEEDQ>EEDNULL THEN/*   0145
*                                      THERE ALREADY ARE EEDS TO BE
*                                      FREED BY RTM2                 */
         L     @11,ESAEEDQ(,R12)                                   0145
         CR    @11,@14                                             0145
         BNH   @RF00145                                            0145
*                                   DO;/* SO NEW EEDS CAN JUST BE  0146
*                                      ADDED TO THE CHAIN    @YM06385*/
*                                    RFY                           0147
*                                      R9 RSTD;/* GET REG TO POINT TO
*                                      EED                   @YM06385*/
*                                    RFY                           0148
*                                      EED BASED(R9);/* USE EED    0148
*                                      POINTED TO BY BY EEDPTR (R9)
*                                                            @YM06385*/
*                                    R9=R12->ESAEEDQ;/* PT TO 1ST EED
*                                      OF CHAIN              @YM06385*/
         LR    R9,@11                                              0149
*                                    DO WHILE(EEDFWRDP>EEDNULL);/* 0150
*                                      LOOP TIL END OF CHAIN IS    0150
*                                      LOCATED               @YM06385*/
         B     @DE00150                                            0150
@DL00150 DS    0H                                                  0151
*                                     R9=EEDFWRDP;/* POINT TO NEXT 0151
*                                      EED IN CHAIN          @YM06385*/
         L     R9,EEDFWRDP(,R9)                                    0151
*                                    END;/* END OF EED LOOP  @YM06385*/
@DE00150 CLC   EEDFWRDP(4,R9),@CF00067                             0152
         BH    @DL00150                                            0152
*                                    EEDFWRDP=TCBRTM12;/* ADD NEW  0153
*                                      EEDS TO END OF CHAIN  @YM06385*/
         L     @14,TCBRTM12(,R4)                                   0153
         ST    @14,EEDFWRDP(,R9)                                   0153
*                                    RFY                           0154
*                                      R9 UNRSTD;/* RELEASE REG USED
*                                      FOR EED PTR           @YM06385*/
*                                   END;/*                   @YM06385*/
*                                 ELSE/* WHEN THER ARE NO PREVIOUS 0156
*                                      EEDS TO BE FREED BY RTM2    0156
*                                                            @YM06385*/
*                                   R12->ESAEEDQ=TCBRTM12;/* JUST  0156
*                                      SAVE PTR TO NEW EED CHAIN   0156
*                                                            @YM06385*/
         B     @RC00145                                            0156
@RF00145 L     @14,TCBRTM12(,R4)                                   0156
         ST    @14,ESAEEDQ(,R12)                                   0156
*                               TCBRTM12=0;/* CLEAR EEDPTR IN THE TCB
*                                                            @YM06385*/
@RC00145 DS    0H                                                  0157
@RF00144 SLR   @14,@14                                             0157
         ST    @14,TCBRTM12(,R4)                                   0157
*                               TCBABTRM=OFF;/* TURN OFF RTM1 ABTERM
*                                      FLAG                  @YM06385*/
         NI    TCBABTRM(R4),B'11011111'                            0158
*                               GOTO ESAR14;/* BRANCH TO EXIT      0159
*                                      PROLOGUE TO EFFECT PSEUDO   0159
*                                      RETRY                 @YM06385*/
         L     @14,ESAR14(,R7)                                     0159
         BR    @14                                                 0159
*                               RFY                                0160
*                                 R12 UNRSTD;                      0160
*                             END;  /* END OF SETUP FOR RETRY ON SLIP
*                                      ERROR                 @YM06385*/
*                         RESPECIFY                                0162
*                          (R2) UNRSTD;/* RELEASE REG WHICH LOCATED
*                                      PREVIOUS RTM2 RB              */
@RF00134 DS    0H                                                  0162
@RF00133 DS    0H                                                  0163
*                       END;                                       0163
*                     ELSE          /* NOT RECURSIVE ENTRY           */
*                       TCBRTM2=ON; /* FLAG TCB IN CASE OF FAILURE   */
         B     @RC00129                                            0164
@RF00129 OI    TCBRTM2(R4),B'00001000'                             0164
*                     IF TCBABTRM=ON THEN/* ENTRY FROM RTM1 - DATA IN
*                                      TCB                           */
@RC00129 TM    TCBABTRM(R4),B'00100000'                            0165
         BNO   @RF00165                                            0165
*                       DO;                                        0166
*                         ESAABTRM=ON;/* SAVE FLAG                   */
         OI    ESAABTRM(R7),B'01000000'                            0167
*                         ESAEEDQ=TCBRTM12;/* SAVE POINTER TO EED  0168
*                                      QUEUE                         */
         L     @14,TCBRTM12(,R4)                                   0168
         ST    @14,ESAEEDQ(,R7)                                    0168
*                         TCBRTM12=0;/* CLEAR FIELD IN CASE OF     0169
*                                      RECURSION                     */
         SLR   @14,@14                                             0169
         ST    @14,TCBRTM12(,R4)                                   0169
*                         ESAERTYP=TCBERTYP;/* SAVE ERROR TYPE DATA  */
