*                   R14 RESTRICTED; /* RESTRICT RETURN REGISTER      */
*                 R13=ADDR(RTM2PRWA->RTM2SFSA);/* SAVE AREA ADDRESS
*                                      TO RELOAD REGISTERS           */
         L     @15,RTM2PRWA(,R8)                                   0241
         LA    R13,RTM2SFSA(,@15)                                  0241
*                 R14=RTM2PRWA->RTM2SKRA;/* RECURSION RESUME ADDRESS */
         L     R14,RTM2SKRA(,@15)                                  0242
