*                             GPR13P,                              0254
*                             GPR14P) RESTRICTED;/* RESTRICT REGS  0254
*                                      USED BY SETLOCK               */
*                           GENERATE REFS(PSALITA,FLC) SETS(GPR11P,
*                               GPR12P,GPR13P,GPR14P);             0255
         SETLOCK RELEASE,TYPE=LOCAL,                                   C
               RELATED=(TCB,IEAVTRTE)
*                           RESPECIFY                              0256
*                            (GPR11P,                              0256
*                             GPR12P,                              0256
*                             GPR13P,                              0256
*                             GPR14P) UNRESTRICTED;/* RELEASE REGS 0256
