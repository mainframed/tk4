*                         ESASDREC=ON;/* SDUMP IN CONTROL            */
         OI    ESASDREC+96(R5),B'01000000'                         0259
         NI    ESAGLREC+96(R5),B'11101111'                         0259
*                         GEN(SDUMP SDATA=(SQA,LSQA,LPA,TRT,CSA,SWA));
         SDUMP SDATA=(SQA,LSQA,LPA,TRT,CSA,SWA)
