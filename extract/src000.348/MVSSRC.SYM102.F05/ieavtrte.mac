*                                      FAILURE                       */
*                   R2=R2->RTM2PREV;/* LOCATE NEXT WORK AREA ON CHAIN*/
         B     @RC00183                                            0185
@RF00183 L     R2,RTM2PREV(,R2)                                    0185
*               END;                /* END SEARCH FOR RECURSION WORK
*                                      AREA                          */
@RC00183 DS    0H                                                  0186
@DE00182 L     @12,RTM2PRWA(,R8)                                   0186
         LTR   @12,@12                                             0186
         BZ    @DL00182                                            0186
*               RESPECIFY                                          0187
*                 R2 UNRESTRICTED;                                 0187
*             END;                                                 0188
*           RTM2RCRX=OFF;           /* CLEAR EXIT FLAG               */
@RC00176 NI    RTM2RCRX(R8),B'01111111'                            0189
*           RTM2SCTC=NOSECTN;       /* CLEAR CURRENT SECTION FLAG    */
*                                                                  0190
         MVC   RTM2SCTC(4,R8),@CB00066                             0190
*           /*********************************************************/
*           /*                                                       */
*           /* SELECT APPROPRIATE EXIT                               */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0191
*           IF RTM2PRWA->RTM2TRRA^=0 THEN/* SUBFUNCTION RECURSION  0191
*                                      ENVIRONMENT SET UP FOR ERROR  */
*                                                                  0191
         L     @12,RTM2PRWA(,R8)                                   0191
         L     @09,RTM2TRRA(,@12)                                  0191
         LTR   @09,@09                                             0191
         BZ    @RF00191                                            0191
*             /*******************************************************/
*             /*                                                     */
*             /* SEGMENT RTESFRE ESTABLISHES THE INTERFACE TO A      */
*             /* SUBFUNCTION RECURSION HANDLING ROUTINE. IT COPIES   */
*             /* THE CONTROLLER RECURSION ENVIRONMENT TO THE CURRENT */
*             /* WORKAREA SO THAT THE SECTION WILL BE SKIPPED IF A   */
*             /* SECOND FAILURE OCCURS. THE SUBFUNCTION REGISTERS ARE*/
*             /* UPDATED TO REFLECT THE CURRENT SVRB AND WORK AREA.  */
*             /* THE LOCAL LOCK IS RELEASED, REGISTERS ARE RELOADED, */
*             /* AND CONTROL IS PASSED TO THE SUBFUNCTION RECURSION  */
*             /* ADDRESS.                                            */
*             /*                                                     */
*             /*******************************************************/
*                                                                  0192
*             DO;                   /* START OF RTESFRE              */
*                                                                  0192
*               /*****************************************************/
*               /*                                                   */
*               /* CLEAR SECTION FLAG IN RECURSION FLAGS             */
*               /*                                                   */
*               /*****************************************************/
*                                                                  0193
*               RTM2SCTR=RTM2SCTR&&RTM2SCTX;                       0193
         XC    RTM2SCTR(4,R8),RTM2SCTX(R8)                         0193
*               RTM2SCTX=NOSECTN;   /* CLEAR RECURSION EXIT FLAGS    */
*                                                                  0194
         MVC   RTM2SCTX(4,R8),@CB00066                             0194
*               /*****************************************************/
*               /*                                                   */
*               /* COPY CONTROLLER RECURSION ENVIRONMENT FROM        */
*               /* PREVIOUS WORKAREA AND UPDATE CONTROLLER RECURSION */
*               /* REGISTERS                                         */
*               /*                                                   */
*               /*****************************************************/
*                                                                  0195
*               RTM2SFRG=RTM2PRWA->RTM2SFRG;/* COPY CONTROLLER REGS  */
         MVC   RTM2SFRG(72,R8),RTM2SFRG(@12)                       0195
*               RTM2SFSA(11)=R5;    /* UPDATE RB PTR IN REG 5        */
         ST    R5,RTM2SFSA+40(,R8)                                 0196
*               RTM2SFSA(14)=R8;    /* UPDATE RTM2WA PTR IN REG 8    */
         ST    R8,RTM2SFSA+52(,R8)                                 0197
*               RTM2SCTC=RTM2PRWA->RTM2SCTC;/* INDICATE CURRENT    0198
*                                      SECTION                       */
         MVC   RTM2SCTC(4,R8),RTM2SCTC(@12)                        0198
*               RTM2SKRA=RTM2PRWA->RTM2SKRA;/* SKIP ADDRESS WILL SKIP
*                                      SECTION IF ANOTHER FAILURE  0199
*                                      OCCURS                        */
         L     @09,RTM2SKRA(,@12)                                  0199
         ST    @09,RTM2SKRA(,R8)                                   0199
*               RTM2CRG=RTM2PRWA->RTM2CRG;/* COPY REGS FOR IEAVTRT2  */
         MVC   RTM2CRG(72,R8),RTM2CRG(@12)                         0200
*               RTM2CREG(11)=R5;    /* UPDATE RB PTR                 */
         ST    R5,RTM2CREG+40(,R8)                                 0201
*               RTM2CREG(13)=ADDR(RBEXSAVE);/* UPDATE SAVED RTM2ESA
*                                      PTR                           */
         LA    @09,RBEXSAVE(,R5)                                   0202
         ST    @09,RTM2CREG+48(,R8)                                0202
*               RTM2CREG(14)=R8;    /* UPDATE RTM2 PTR               */
*                                                                  0203
         ST    R8,RTM2CREG+52(,R8)                                 0203
*               /*****************************************************/
*               /*                                                   */
*               /* SET UP ENVIRONMENT FOR SUBFUNCTION TO RESUME      */
*               /*                                                   */
*               /*****************************************************/
*                                                                  0204
*               RTM2RRG=RTM2PRWA->RTM2RRG;/* COPY SUBFUNCTION REGS 0204
*                                      FORWARD                       */
         MVC   RTM2RRG(64,R8),RTM2RRG(@12)                         0204
*               IF RTM2PRWA->RTM2WARG^=0 THEN/* WORK AREA PTR TO BE
*                                      UPDATED                       */
         CLI   RTM2WARG(@12),0                                     0205
         BE    @RF00205                                            0205
*                 DO;                                              0206
*                   RTM2RREG(RTM2PRWA->RTM2WARG)=R8;               0207
         SLR   @09,@09                                             0207
         IC    @09,RTM2WARG(,@12)                                  0207
         LR    @12,@09                                             0207
         SLA   @12,2                                               0207
         ST    R8,RTM2RREG-4(@12,R8)                               0207
*                   RTM2WARG=RTM2PRWA->RTM2WARG;                   0208
         STC   @09,RTM2WARG(,R8)                                   0208
*                 END;                                             0209
*               IF RTM2PRWA->RTM2RBRG^=0 THEN/* RB REG TO BE UPDATED */
@RF00205 L     @12,RTM2PRWA(,R8)                                   0210
         CLI   RTM2RBRG(@12),0                                     0210
         BE    @RF00210                                            0210
*                 DO;                                              0211
*                   RTM2RREG(RTM2PRWA->RTM2RBRG)=R5;               0212
         SLR   @09,@09                                             0212
         IC    @09,RTM2RBRG(,@12)                                  0212
         LR    @12,@09                                             0212
         SLA   @12,2                                               0212
         ST    R5,RTM2RREG-4(@12,R8)                               0212
*                   RTM2RBRG=RTM2PRWA->RTM2RBRG;                   0213
         STC   @09,RTM2RBRG(,R8)                                   0213
*                 END;                                             0214
*               RTM2RREG(14)=ADDR(RTM2SFSA);/* UPDATE REG 13         */
@RF00210 LA    @12,RTM2SFSA(,R8)                                   0215
         ST    @12,RTM2RREG+52(,R8)                                0215
*               RTM2RREG(15)=RTM2PRWA->RTM2TRRA;/* REG 14 WILL BE  0216
*                                      RETURN REG                    */
         L     @12,RTM2PRWA(,R8)                                   0216
         L     @12,RTM2TRRA(,@12)                                  0216
         ST    @12,RTM2RREG+56(,R8)                                0216
*               ESAGLREC=ON;                                       0217
         OI    ESAGLREC+96(R5),B'00010000'                         0217
*               DO;                 /* SETLOCK RELEASE TYPE(LOCAL)REL
*                                      ATED(TCB,IEAVTRTE)            */
*                 RESPECIFY                                        0219
*                  (GPR11P,                                        0219
*                   GPR12P,                                        0219
*                   GPR13P,                                        0219
*                   GPR14P) RESTRICTED;/* RESTRICT REGS USED BY    0219
*                                      SETLOCK                       */
*                 GENERATE REFS(PSALITA,FLC) SETS(GPR11P,GPR12P,GPR13P,
*                     GPR14P);                                     0220
         SETLOCK RELEASE,TYPE=LOCAL,                                   C
               RELATED=(TCB,IEAVTRTE)
*                 RESPECIFY                                        0221
*                  (GPR11P,                                        0221
*                   GPR12P,                                        0221
*                   GPR13P,                                        0221
*                   GPR14P) UNRESTRICTED;/* RELEASE REGS USED BY   0221
*                                      SETLOCK                       */
*               END;                /* SETLOCK RELEASE TYPE(LOCAL)REL
*                                      ATED(TCB,IEAVTRTE)            */
*               ESAGLREC=OFF;                                      0223
         NI    ESAGLREC+96(R5),B'11101111'                         0223
*               GEN REFS(RTM2RREG,R8);                             0224
         LM    R0,R15,RTM2RREG(R8)
         BR    R14
*             END;                  /* END OF RTESFRE                */
*           ELSE                    /* NO SUBFUNCTION ENVIRONMENT  0226
