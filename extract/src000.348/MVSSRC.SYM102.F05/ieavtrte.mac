         ST    R5,RTM2CREG+40(,R8)                                 0229
*                 RTM2CREG(13)=ADDR(RBEXSAVE);/* UPDATE SAVED RTM2ESA
*                                      POINTER                       */
         LA    @12,RBEXSAVE(,R5)                                   0230
         ST    @12,RTM2CREG+48(,R8)                                0230
*                 RTM2CREG(14)=R8;  /* UPDATE SAVED RTM2WA PTR       */
         ST    R8,RTM2CREG+52(,R8)                                 0231
*                 RTM2SCTX=NOSECTN; /* CLEAR RECURSION EXIT FLAGS    */
         MVC   RTM2SCTX(4,R8),@CB00066                             0232
*                 ESAGLREC=ON;                                     0233
         OI    ESAGLREC+96(R5),B'00010000'                         0233
*                 DO;               /* SETLOCK RELEASE TYPE(LOCAL)REL
*                                      ATED(TCB,IEAVTRTE)            */
*                   RESPECIFY                                      0235
*                    (GPR11P,                                      0235
*                     GPR12P,                                      0235
*                     GPR13P,                                      0235
*                     GPR14P) RESTRICTED;/* RESTRICT REGS USED BY  0235
*                                      SETLOCK                       */
*                   GENERATE REFS(PSALITA,FLC) SETS(GPR11P,GPR12P, 0236
*                       GPR13P,GPR14P);                            0236
         SETLOCK RELEASE,TYPE=LOCAL,                                   C
               RELATED=(TCB,IEAVTRTE)
*                   RESPECIFY                                      0237
