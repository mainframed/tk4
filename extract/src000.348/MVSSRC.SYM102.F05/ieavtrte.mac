*                           RESPECIFY                              0264
*                            (GPR11P,                              0264
*                             GPR12P,                              0264
*                             GPR13P,                              0264
*                             GPR14P) RESTRICTED;/* RESTRICT REGS  0264
*                                      USED BY SETLOCK               */
*                           GENERATE REFS(PSALITA,FLC) SETS(GPR11P,
*                               GPR12P,GPR13P,GPR14P);             0265
         SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,                        C
               RELATED=(TCB,'FREED BY STATUS')
*                           RESPECIFY                              0266
*                            (GPR11P,                              0266
*                             GPR12P,                              0266
