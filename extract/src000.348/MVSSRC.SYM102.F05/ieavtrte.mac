         BNO   @RF00171                                            0171
*         RTM2SCTX(RTM2EXTS)=ON;    /* INDICATE SECTION              */
         OI    RTM2SCTX+1(R8),B'01000000'                          0172
*       ELSE                        /* NOT RECURSION IN EXIT         */
*         RTM2SCTC(RTM2EXTS)=ON;    /* INDICATE CURRENT SECTION      */
*                                                                  0173
         B     @RC00171                                            0173
@RF00171 OI    RTM2SCTC+1(R8),B'01000000'                          0173
*       /*************************************************************/
*       /*                                                           */
*       /* ROUTE TO APPROPRIATE EXIT HANDLER                         */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0174
*       IF RTM2RCRX=ON              /* RECURSION EXIT REQUESTED      */
*           ×(RTM2RECR=ON&RTM2SCTR(RTM2EXTS)=ON) THEN/* RECURSION IN
*                                      EXIT                          */
*                                                                  0174
@RC00171 TM    RTM2RCRX(R8),B'10000000'                            0174
         BO    @RT00174                                            0174
         TM    RTM2RECR(R8),B'10000000'                            0174
         BNO   @RF00174                                            0174
         TM    RTM2SCTR+1(R8),B'01000000'                          0174
         BNO   @RF00174                                            0174
@RT00174 DS    0H                                                  0175
*         /***********************************************************/
*         /*                                                         */
*         /* SEGMENT RTERCREX HANDLES THE EXIT FUNCTION FOR RECURSIVE*/
*         /* ENTRIES. IF THE RECURSION WAS IN CODE NOT COVERED BY A  */
*         /* SECTION FLAG OR IN TASK RECOVERY PROCESSING THE         */
*         /* IMMEDIATELY PRECEDING WORK AREA CONTAINS THE ERROR DATA */
*         /* AND IS CHAINED FROM THE RECURSION WORK AREA FIELD.      */
*         /* OTHERWISE, THE WORK AREA WHICH CORRESPONDS TO THE       */
*         /* SECTION WHICH FAILED IS CHAINED FROM THE RECURSION WORK */
*         /* AREA FIELD. IF A SUBFUNCTION RECURSION ROUTINE EXISTS   */
*         /* SEGMENT RTESFRE IS THEN EXECUTED TO ESTABLISH THE       */
*         /* INTERFACE TO THE SUBFUNCTION RECURSION ROUTINE.         */
*         /* OTHERWISE CONTROL IS PASSED TO THE CONTROLLER RECURSION */
*         /* ADDRESS OR TO THE 'STEP CONVERSION RECURSION ROUTINE'   */
*         /* (SEGMENT RTECVERR) OR TO THE CRITICAL RECURSION         */
*         /* ROUTINE(RT2CRERR). THIS IS DONE BASED ON THE LEAST      */
*         /* SEVERE ONE WHICH HAS AN ACTIVE ADDRESS IN THE           */
*         /* APPROPRIATE RTM2WA.                                     */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0175
*         DO;                       /* START OF RTERCREX             */
*                                                                  0175
*           /*********************************************************/
*           /*                                                       */
*           /* LOCATE PREVIOUS WORK AREA                             */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0176
*           IF RTM2SCTX=NOSECTN THEN/* ERROR IN CONNECTING CODE      */
         CLC   RTM2SCTX(4,R8),@CB00066                             0176
         BNE   @RF00176                                            0176
*             RTM2PRWA=RTM2PREV;    /* IMMEDIATELY PREVIOUS WORK AREA
*                                      CONTAINS RECURSION INFORMATION*/
         L     @12,RTM2PREV(,R8)                                   0177
         ST    @12,RTM2PRWA(,R8)                                   0177
*           ELSE                    /* RECURSION IN SOME OTHER     0178
*                                                             SECTION*/
*             DO;                                                  0178
         B     @RC00176                                            0178
@RF00176 DS    0H                                                  0179
*               RESPECIFY                                          0179
*                 R2 RESTRICTED;    /* RESTRICT REG USED TO SCAN WORK
*                                      AREA QUEUE                    */
*               R2=RTM2PREV;        /* START SCAN WITH PREVIOUS WA   */
         L     R2,RTM2PREV(,R8)                                    0180
*               RTM2PRWA=0;         /* CLEAR RECURSION WORKAREA PTR
*                                      (THIS MAY NOT BE FIRST TIME 0181
*                                      THIS EXIT IS TAKEN)           */
         SLR   @12,@12                                             0181
         ST    @12,RTM2PRWA(,R8)                                   0181
*               DO WHILE(RTM2PRWA=0);/* SCAN UNTIL WORK AREA       0182
*                                      CONTAINING FAILURE DATA IS  0182
*                                      REACHED                       */
         B     @DE00182                                            0182
@DL00182 DS    0H                                                  0183
*                 IF RTM2SCTX=R2->RTM2SCTC THEN/* WORK AREA CURRENT
*                                      AT TIME OF FAILURE            */
         CLC   RTM2SCTX(4,R8),RTM2SCTC(R2)                         0183
         BNE   @RF00183                                            0183
