         TITLE 'IEAVTRTE - RTM2 EXIT HANDLER                           *
                        '
IEAVTRTE CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'IEAVTRTE  77.280'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @06,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@06                                         0001
RBPRFXLN EQU 64
RBPRFLNA EQU 32
PRBLEN   EQU 136
SIRBLEN  EQU 200
TIRBLEN  EQU 136
IRBLEN   EQU 128
SVRBLEN  EQU 224
*                                                                  0047
*   /*****************************************************************/
*   /*                                                               */
*   /* IEAVTRTE DETERMINES WHETHER TASK OR MEMORY PURGES ARE REQUIRED*/
*   /* AND ENTERS SEGMENT RTEMTINT OR RTETTINT ACCORDINGLY. THEN, IF */
*   /* CONVERSION TO A JOBSTEP ABEND WAS REQUESTED IT ENTERS SEGMENT */
*   /* RTECONV. IT THEN DOES EXIT PROCESSING.                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0047
*   IF RMPLTERM=ON THEN             /* MEMORY TERMINATION            */
*                                                                  0047
         TM    RMPLTERM+712(R8),B'01000000'                        0047
         BNO   @RF00047                                            0047
*     /***************************************************************/
*     /*                                                             */
*     /* RTEMTINT ESTABLISHES AN ESTAE EXIT SO THAT A FAILURE CAN BE */
*     /* RECOVERED AND INVOKES THE MEMORY TERMINATION PURGE ROUTINE. */
*     /* WHEN CONTROL IS RETURNED, IT SETS THE EXIT FLAG TO INDICATE */
*     /* THE TYPE OF EXIT DESIRED AND CANCELS THE ESTAE EXIT         */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0048
*     DO;                           /* START OF SEGMENT RTEMTINT     */
*       RESPECIFY                                                  0049
*        (R1) RESTRICTED;           /* RESTRICT INTERFACE REGISTERS  */
*       R1=ADDR(RTM2WA);            /* PARAMETER REGISTER            */
         LR    R1,R8                                               0050
*       R13=ADDR(RTM2SFSA);         /* SAVE AREA PTR                 */
         LA    R13,RTM2SFSA(,R8)                                   0051
*       CALL IEAVTMMT;              /* INVOKE MEMORY TERMINATION   0052
*                                      PURGES                        */
         L     @15,@CV02504                                        0052
         BALR  @14,@15                                             0052
*       RESPECIFY                                                  0053
*        (R1) UNRESTRICTED;         /* RELEASE INTERFACE REGISTERS   */
*       RTM2MTX=ON;                 /* INDICATE EXIT DESIRED         */
         OI    RTM2MTX(R8),B'10000000'                             0054
*     END;                          /* END OF RTEMTINT               */
*   ELSE                            /* TASK TERMINATION              */
*     IF RTM2FLX=NONE THEN          /* NO EXIT REQUESTED             */
         B     @RC00047                                            0056
@RF00047 CLC   RTM2FLX(2,R8),@CB00060                              0056
         BNE   @RF00056                                            0056
*       DO;                                                        0057
*         RTM2ISPC=OFF;             /* INDICATE INITIAL SUBTASK    0058
*                                      PURGES SHOULD BE DONE AGAIN IF
*                                      ANOTHER FAILURE OCCURS        */
         NI    RTM2ISPC(R8),B'11101111'                            0058
*         RTM2SCTR(RTM2EPS)=OFF;    /* CLEAR RECURSION FLAGS SO THAT
*                                      ANOTHER RECURSION WILL START
*                                      SUBTASK PURGES FROM BEGINNING */
*         RTM2SCTR(RTM2ISPS)=OFF;                                  0060
*                                                                  0060
         NI    RTM2SCTR(R8),B'11111100'                            0060
*         /***********************************************************/
*         /*                                                         */
*         /* SEGMENT RTETTINT INVOKES TASK TERMINATION PURGES. PRIOR */
*         /* TO DOING THIS, IF ENTRY IS FOR ABNORMAL TASK            */
*         /* TERMINATION,IT ESTABLISHES RECURSION CONTROL. ON RETURN */
*         /* FROM TASK PURGES IT DETERMINES THE TYPE OF EXIT         */
*         /* NECESSARY AND CLEARS THE RECURSION FLAGS. ON ENTRY      */
*         /* REGISTER 4 LOCATES THE TCB, REGISTER 5 LOCATES THE SVRB */
*         /* AND REGISTER 8 LOCATES THE RTM2WA                       */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0061
*         DO;                       /* BEGIN SEGMENT RTETTINT        */
*                                                                  0061
*           /*********************************************************/
*           /*                                                       */
*           /* DO RECURSION CONTROL                                  */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0062
*           IF RMPLTYPE=ON THEN     /* ENTRY IS FOR ABNORMAL TASK  0062
*                                      TERMINATION                   */
         TM    RMPLTYPE+712(R8),B'10000000'                        0062
         BNO   @RF00062                                            0062
*             IF RTM2RECR=ON&RTM2SCTR(RTM2TSTS)=ON THEN/* A RECURSION
*                                      HAS OCCURRED IN THIS PHASE    */
         TM    RTM2RECR(R8),B'10000000'                            0063
         BNO   @RF00063                                            0063
         TM    RTM2SCTR+1(R8),B'00000100'                          0063
         BNO   @RF00063                                            0063
*               DO;                                                0064
*                 RTM2RCRX=ON;      /* INDICATE RECURSIVE ENTRY      */
         OI    RTM2RCRX(R8),B'10000000'                            0065
*                 RTM2SCTX(RTM2TSTS)=ON;/* INDICATE SECTION          */
         OI    RTM2SCTX+1(R8),B'00000100'                          0066
*               END;                                               0067
*             ELSE                                                 0068
*               RTM2SCTC(RTM2TSTS)=ON;/* SET SECTION FLAG FOR THIS 0068
*                                      PHASE. NO SKIP ADDR IS SET  0068
*                                      BECAUSE IF TASK PURGES DOES 0068
*                                      NOT HAVE A VALID RECURSION  0068
*                                      ADDR THE STEP CONVERSION OR 0068
*                                      CRITICAL RECURSION RTN SHOULD
*                                      BE ENTERED                    */
         B     @RC00063                                            0068
@RF00063 OI    RTM2SCTC+1(R8),B'00000100'                          0068
*           ELSE                                                   0069
*             ;                     /* IF NORMAL EOT,CANNOT BE     0069
*                                      RECURSION                     */
@RF00062 DS    0H                                                  0070
*           IF RTM2FLX=NONE THEN    /* NO EXIT HAS BEEN REQUESTED    */
@RC00062 CLC   RTM2FLX(2,R8),@CB00060                              0070
         BNE   @RF00070                                            0070
*             DO;                                                  0071
*                                                                  0071
*               /*****************************************************/
*               /*                                                   */
*               /* INVOKE TASK TERMINATION PURGES                    */
*               /*                                                   */
*               /*****************************************************/
*                                                                  0072
*               RESPECIFY                                          0072
*                (R1) RESTRICTED;   /* RESTRICT PARM REG FOR TASK  0072
*                                      PURGE                         */
*               RMPLTCBA=R4;        /* PUT CURRENT TCB ADDRESS IN  0073
*                                      RMPL                          */
         ST    R4,RMPLTCBA+712(,R8)                                0073
*               R1=ADDR(RTM2WA);    /* PUT ADDR OF RTM2WA IN R1      */
         LR    R1,R8                                               0074
*               R13=ADDR(RTM2SFSA); /* SAVE AREA ADDRESS IN R13      */
         LA    R13,RTM2SFSA(,R8)                                   0075
*               RESPECIFY                                          0076
*                 R15 RESTRICTED;                                  0076
*               CALL IEAVTSKT;      /* REQUEST TASK PURGES           */
         L     @15,@CV02505                                        0077
         BALR  @14,@15                                             0077
*               RESPECIFY                                          0078
*                 R1 UNRESTRICTED;                                 0078
*                                                                  0078
*               /*****************************************************/
*               /*                                                   */
*               /* DETERMINE TYPE OF EXIT REQUESTED                  */
*               /*                                                   */
*               /*****************************************************/
*                                                                  0079
*               IF R15^=0 THEN      /* ERROR DETECTED                */
         LTR   R15,R15                                             0079
         BZ    @RF00079                                            0079
*                 RTM2CERX=ON;      /* INDICATE ERROR IN EXIT FLAGS  */
         OI    RTM2CERX(R8),B'01000000'                            0080
*               ELSE                /* TASK PURGES SUCCESSFUL        */
*                 IF TCBRSTSK=ON THEN/* TASK PURGE HAS PURGED A    0081
*                                      PERMANENTLY RESIDENT TASK     */
         B     @RC00079                                            0081
@RF00079 TM    TCBRSTSK(R4),B'00000100'                            0081
         BNO   @RF00081                                            0081
*                   RTM2PRX=ON;     /* INDICATE EXIT PROCESSING FOR
*                                      PERMANENTLY RESIDENT TASK     */
         OI    RTM2PRX(R8),B'00000100'                             0082
*                 ELSE                                             0083
*                   IF RTM2TMEM=ON THEN/* TASK PURGE HAS PURGED THE
*                                      LAST TASK IN THE MEMORY       */
         B     @RC00081                                            0083
@RF00081 TM    RTM2TMEM(R8),B'00100000'                            0083
         BNO   @RF00083                                            0083
*                     RTM2LTX=ON;   /* INDICATE LAST TASK EXIT       */
         OI    RTM2LTX(R8),B'00000010'                             0084
*                   ELSE                                           0085
*                     IF RMPLTYPE=ON THEN/* TASK PURGES WERE FOR   0085
*                                      ABNORMAL TERMINATION          */
         B     @RC00083                                            0085
@RF00083 TM    RMPLTYPE+712(R8),B'10000000'                        0085
         BNO   @RF00085                                            0085
*                       RTM2ABX=ON; /* INDICATE ABNORMAL TERMINATION
*                                      EXIT                          */
         OI    RTM2ABX(R8),B'00100000'                             0086
*                     ELSE                                         0087
*                       RTM2EOTX=ON;/* INDICATE NORMAL EOT EXIT      */
*                                                                  0087
         B     @RC00085                                            0087
@RF00085 OI    RTM2EOTX(R8),B'01000000'                            0087
*               /*****************************************************/
*               /*                                                   */
*               /* PUT ABEND CODE INTO TCB - IT MAY HAVE BEEN CHANGED*/
*               /* BY AN ERROR WHICH HAS BEEN RECOVERED      @YM05449*/
*               /*                                                   */
*               /*****************************************************/
*                                                                  0088
*               TCBCMPC=RTM2CC;     /*                       @YM05449*/
*                                                                  0088
@RC00085 DS    0H                                                  0088
@RC00083 DS    0H                                                  0088
@RC00081 DS    0H                                                  0088
@RC00079 MVC   TCBCMPC(3,R4),RTM2CC(R8)                            0088
*               /*****************************************************/
*               /*                                                   */
*               /* CLEAR RECURSION CONTROL                           */
*               /*                                                   */
*               /*****************************************************/
*                                                                  0089
*               RTM2SCTC(RTM2TSTS)=OFF;/* CLEAR RECURSION FLAG       */
         NI    RTM2SCTC+1(R8),B'11111011'                          0089
*             END;                                                 0090
*           ELSE                                                   0091
*             ;                     /* IF EXIT REQUESTED BYPASS ALL
*                                      PROCESSING                    */
@RF00070 DS    0H                                                  0092
*           RESPECIFY                                              0092
*             R15 UNRESTRICTED;                                    0092
*         END;                      /* END SEGMENT RTETTINT          */
*         IF RTM2RCRX=OFF           /* NO RECURSION DETECTED         */
*             &RTM2CERX=OFF         /* NO CRITICAL ERROR DETECTED    */
*             &RTM2STPT=ON THEN     /* STEP ABEND REQUESTED          */
*                                                                  0094
         TM    RTM2RCRX(R8),B'11000000'                            0094
         BNZ   @RF00094                                            0094
         TM    RTM2STPT(R8),B'10000000'                            0094
         BNO   @RF00094                                            0094
*           /*********************************************************/
*           /*                                                       */
*           /* SEGMENT RTECONV DETERMINES WHETHER CONVERSION TO STEP */
*           /* IS POSSIBLE AND NOTIFIES THE JOBSTEP TASK IF IT IS.   */
*           /* THE JOBSTEP TASK IS FIRST LOCATED AND SET             */
*           /* NONDISPATCHABLE. IF THE JOBSTEP TASK IS ALREADY       */
*           /* ABENDING OR BEING FORCED TO ABEND, IT IS NOT ELIGIBLE */
*           /* FOR CONVERSION. IT IS RESET DISPATCHABLE, AND THE     */
*           /* CURRENT TASK GOES TO THE EXIT HANDLER TO FINISH ITS   */
*           /* OWN TERMINATION WITH WHATEVER EXIT FLAG WAS SET BY    */
*           /* TASK TERMINATION. IF THE JOBSTEP TASK IS ELIGIBLE FOR */
*           /* CONVERSION, IT IS CHECKED TO SEE WHETHER IT IS IN     */
*           /* RTM2. IF IT IS IN RTM2, IT MUST BE PERMITTED TO       */
*           /* COMPLETE ITS CURRENT PROCESSING. AN ECB IS SET UP, A  */
*           /* FLAG IS SET IN THE JOBSTEP TCB, IT IS RESET           */
*           /* DISPATCHABLE TO COMPLETE ITS CURRENT PROCESSING. WHEN */
*           /* IT POSTS THE ECB, THE CURRENT TASK SETS AN EXIT FLAG  */
*           /* TO FORCE THE ABTERMING OF THE JOBSTEP TASK.           */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0095
*           DO;                     /* START OF RTECONV              */
*                                                                  0095
*             /*******************************************************/
*             /*                                                     */
*             /* OBTAIN LOCAL LOCK                                   */
*             /*                                                     */
*             /*******************************************************/
*                                                                  0096
*             ESAGLREC=ON;                                         0096
         OI    ESAGLREC+96(R5),B'00010000'                         0096
*             DO;                   /* SETLOCK OBTAIN TYPE(LOCAL)MODE
*                                      (UNCOND)RELATED(TCB,IEAVTRTE(R
*                                      TECONV))                      */
*               RESPECIFY                                          0098
*                (GPR11P,                                          0098
*                 GPR12P,                                          0098
*                 GPR13P,                                          0098
*                 GPR14P) RESTRICTED;/* RESTRICT REGS USED BY SETLOCK*/
*               GENERATE REFS(PSALITA,FLC) SETS(GPR11P,GPR12P,GPR13P,
*                   GPR14P);                                       0099
         SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,                        C
               RELATED=(TCB,IEAVTRTE(RTECONV))
*               RESPECIFY                                          0100
*                (GPR11P,                                          0100
*                 GPR12P,                                          0100
*                 GPR13P,                                          0100
*                 GPR14P) UNRESTRICTED;/* RELEASE REGS USED BY     0100
*                                      SETLOCK                       */
*             END;                  /* SETLOCK OBTAIN TYPE(LOCAL)MODE
*                                      (UNCOND)RELATED(TCB,IEAVTRTE(R
*                                      TECONV))                      */
*             ESAGLREC=OFF;                                        0102
*                                                                  0102
         LA    @12,96                                              0102
         ALR   @12,R5                                              0102
         NI    ESAGLREC(@12),B'11101111'                           0102
*             /*******************************************************/
*             /*                                                     */
*             /* LOCATE JOBSTEP TASK                                 */
*             /*                                                     */
*             /*******************************************************/
*                                                                  0103
*             RESPECIFY                                            0103
*               R9 RESTRICTED;      /* RESTRICT PTR TO J/S TCB       */
*             R9=TCBJSTCA;                                         0104
         L     R9,TCBJSTCA-1(,R4)                                  0104
         LA    R9,0(,R9)                                           0104
*             RESPECIFY                                            0105
*               TCB BASED(R9);                                     0105
*                                                                  0105
*             /*******************************************************/
*             /*                                                     */
*             /* SET JOB STEP TASK NONDISPATCHABLE                   */
*             /*                                                     */
*             /*******************************************************/
*                                                                  0106
*             RESPECIFY                                            0106
*              (R0,                                                0106
*               R1) RESTRICTED;     /* RESTRICT STATUS INTERFACE REGS*/
*             R0=STOPTASK;          /* CODE TO STOP ONE TASK WITH  0107
*                                      ABWF                          */
         L     R0,@CF00076                                         0107
*             R1=R9;                /* TASK TO BE STOPPED            */
         LR    R1,R9                                               0108
*             R13=0;                /* INDICATE CURRENT MEMORY       */
         SLR   R13,R13                                             0109
*             CALL STATUS;                                         0110
         L     @12,CVTPTR(,@12)                                    0110
         L     @12,SCVTSPTR(,@12)                                  0110
         L     @15,SCVTSTAT(,@12)                                  0110
         BALR  @14,@15                                             0110
*             RESPECIFY                                            0111
*              (R0,                                                0111
*               R1) UNRESTRICTED;   /* RELEASE STATUS INTERFACE REGS */
*                                                                  0111
*             /*******************************************************/
*             /*                                                     */
*             /* IF CONVERSION IS NOT POSSIBLE, RELEASE LOCAL LOCK   */
*             /* AND RESET JOBSTEP TASK DISPATCHABLE                 */
*             /*                                                     */
*             /*******************************************************/
*                                                                  0112
*             IF TCBFA=ON           /* TASK ALREADY ABENDING         */
*                 ×TCBFMW=ON        /* ANCESTOR FORCING TASK TO ABEND*/
*                 ×TCBFDW=ON THEN   /* ANOTHER SUBTASK FORCING TASK
*                                      TO ABEND                      */
         TM    TCBFA(R9),B'10000000'                               0112
         BO    @RT00112                                            0112
         TM    TCBFMW(R9),B'01100000'                              0112
         BZ    @RF00112                                            0112
@RT00112 DS    0H                                                  0113
*               DO;                                                0113
*                 ESAGLREC=ON;      /* INDICATE IN SETLOCK PROCESSING*/
         OI    ESAGLREC+96(R5),B'00010000'                         0114
*                 DO;               /* SETLOCK RELEASE TYPE(LOCAL)REL
*                                      ATED(TCB,IEAVTRTE(RTECONV))   */
*                   RESPECIFY                                      0116
*                    (GPR11P,                                      0116
*                     GPR12P,                                      0116
*                     GPR13P,                                      0116
*                     GPR14P) RESTRICTED;/* RESTRICT REGS USED BY  0116
*                                      SETLOCK                       */
*                   GENERATE REFS(PSALITA,FLC) SETS(GPR11P,GPR12P, 0117
*                       GPR13P,GPR14P);                            0117
         SETLOCK RELEASE,TYPE=LOCAL,                                   C
               RELATED=(TCB,IEAVTRTE(RTECONV))
*                   RESPECIFY                                      0118
*                    (GPR11P,                                      0118
*                     GPR12P,                                      0118
*                     GPR13P,                                      0118
*                     GPR14P) UNRESTRICTED;/* RELEASE REGS USED BY 0118
*                                      SETLOCK                       */
*                 END;              /* SETLOCK RELEASE TYPE(LOCAL)REL
*                                      ATED(TCB,IEAVTRTE(RTECONV))   */
*                 ESAGLREC=OFF;     /* SETLOCK COMPLETED SUCCESSFULLY*/
         NI    ESAGLREC+96(R5),B'11101111'                         0120
*                 GEN(STATUS RESET,ND,(R9),(10),E);                0121
         STATUS RESET,ND,(R9),(10),E
*               END;                                               0122
*             ELSE                                                 0123
*                                                                  0123
*               /*****************************************************/
*               /*                                                   */
*               /* CONVERSION POSSIBLE                               */
*               /*                                                   */
*               /*****************************************************/
*                                                                  0123
*               DO;                                                0123
         B     @RC00112                                            0123
@RF00112 DS    0H                                                  0124
*                 TCBFDW=ON;        /* SIGNAL JOBSTEP THAT DAUGHTER
*                                      IS WAITING                    */
*                                                                  0124
         OI    TCBFDW(R9),B'00100000'                              0124
*                 /***************************************************/
*                 /*                                                 */
*                 /* IF NECESSARY, WAIT FOR JOBSTEP TO FINISH CERTAIN*/
*                 /* RTM2 PROCESSING                                 */
*                 /*                                                 */
*                 /***************************************************/
*                                                                  0125
*                 IF TCBRTM2=ON THEN/* JOBSTEP TASK MAY BE IN      0125
*                                      PROCESSING WHICH SHOULD NOT BE
*                                      INTERRUPTED                   */
         TM    TCBRTM2(R9),B'00001000'                             0125
         BNO   @RF00125                                            0125
*                   DO;                                            0126
*                     RTM2ECBS(1)=0;/* CLEAR ECB TO BE USED          */
         SLR   @12,@12                                             0127
         ST    @12,RTM2ECBS(,R8)                                   0127
*                     TCBAECB=ADDR(RTM2ECBS);/* PROVIDE ADDRESS OF 0128
*                                      ECB TO JOBSTEP TASK           */
         LA    @12,RTM2ECBS(,R8)                                   0128
         ST    @12,TCBAECB(,R9)                                    0128
*                     ESAGLREC=ON;  /* INDICATE SETLOCK IN CONTROL   */
         OI    ESAGLREC+96(R5),B'00010000'                         0129
*                     DO;           /* SETLOCK RELEASE TYPE(LOCAL)REL
*                                      ATED(TCB,IEAVTRTE(RTECONV))   */
*                       RESPECIFY                                  0131
*                        (GPR11P,                                  0131
*                         GPR12P,                                  0131
*                         GPR13P,                                  0131
*                         GPR14P) RESTRICTED;/* RESTRICT REGS USED BY
*                                                             SETLOCK*/
*                       GENERATE REFS(PSALITA,FLC) SETS(GPR11P,GPR12P,
*                           GPR13P,GPR14P);                        0132
         SETLOCK RELEASE,TYPE=LOCAL,                                   C
               RELATED=(TCB,IEAVTRTE(RTECONV))
*                       RESPECIFY                                  0133
*                        (GPR11P,                                  0133
*                         GPR12P,                                  0133
*                         GPR13P,                                  0133
*                         GPR14P) UNRESTRICTED;/* RELEASE REGS USED
*                                      BY                     SETLOCK*/
*                     END;          /* SETLOCK RELEASE TYPE(LOCAL)REL
*                                      ATED(TCB,IEAVTRTE(RTECONV))   */
*                     ESAGLREC=OFF; /* SETLOCK COMPLETED WITHOUT   0135
*                                      FAILURE                       */
         NI    ESAGLREC+96(R5),B'11101111'                         0135
*                     GEN(STATUS RESET,ND,(R9),(10),E);            0136
         STATUS RESET,ND,(R9),(10),E
*                     RESPECIFY                                    0137
*                       R1 RESTRICTED;                             0137
*                     R1=ADDR(RTM2ECBS);/* SET UP PARAMETER TO WAIT  */
         LA    R1,RTM2ECBS(,R8)                                    0138
*                     GEN(WAIT  ECB=(1));/* WAIT FOR JOBSTEP TASK TO
*                                      FINISH CRITICAL PROCESSING    */
         WAIT  ECB=(1)
*                     RESPECIFY                                    0140
*                       R1 UNRESTRICTED;                           0140
*                   END;                                           0141
*                 ELSE              /* JOBSTEP TASK NOT IN RTM2    0142
*                                      PROCESSING                    */
*                   DO;                                            0142
         B     @RC00125                                            0142
@RF00125 DS    0H                                                  0143
*                     TCBRTM2=ON;   /* FORCE JOBSTEP'S ANCESTORS TO
*                                      STACK BEHIND IT               */
         OI    TCBRTM2(R9),B'00001000'                             0143
*                     ESAGLREC=ON;  /* INDICATE IN SETLOCK PROCESSING*/
         OI    ESAGLREC+96(R5),B'00010000'                         0144
*                     DO;           /* SETLOCK RELEASE TYPE(LOCAL)REL
*                                      ATED(TCB,IEAVTRTE(RTECONV))   */
*                       RESPECIFY                                  0146
*                        (GPR11P,                                  0146
*                         GPR12P,                                  0146
*                         GPR13P,                                  0146
*                         GPR14P) RESTRICTED;/* RESTRICT REGS USED BY
*                                      SETLOCK                       */
*                       GENERATE REFS(PSALITA,FLC) SETS(GPR11P,GPR12P,
*                           GPR13P,GPR14P);                        0147
         SETLOCK RELEASE,TYPE=LOCAL,                                   C
               RELATED=(TCB,IEAVTRTE(RTECONV))
*                       RESPECIFY                                  0148
*                        (GPR11P,                                  0148
*                         GPR12P,                                  0148
*                         GPR13P,                                  0148
*                         GPR14P) UNRESTRICTED;/* RELEASE REGS USED
*                                      BY SETLOCK                    */
*                     END;          /* SETLOCK RELEASE TYPE(LOCAL)REL
*                                      ATED(TCB,IEAVTRTE(RTECONV))   */
*                     ESAGLREC=OFF; /* SETLOCK PROCESSING COMPLETED  */
         NI    ESAGLREC+96(R5),B'11101111'                         0150
*                   END;                                           0151
*                                                                  0151
*                 /***************************************************/
*                 /*                                                 */
*                 /* SET EXIT FOR CONVERSION TO STEP                 */
*                 /*                                                 */
*                 /***************************************************/
*                                                                  0152
*                 RTM2FLX=NONE;     /* CLEAR EXISTING FLAGS          */
@RC00125 MVC   RTM2FLX(2,R8),@CB00060                              0152
*                 RTM2CVX=ON;       /* SET EXIT FLAG FOR CONVERSION  */
         OI    RTM2CVX(R8),B'00001000'                             0153
*               END;                                               0154
*             RESPECIFY                                            0155
*               TCB BASED(R4);                                     0155
@RC00112 DS    0H                                                  0156
*             RESPECIFY                                            0156
*               R9 UNRESTRICTED;                                   0156
*           END;                    /* END OF RTECONV                */
*         ELSE                                                     0158
*           ;                       /* EXIT REQUESTED OR NOT STEP    */
@RF00094 DS    0H                                                  0159
*       END;                                                       0159
*     ELSE                                                         0160
*       ;                           /* EXIT REQUESTED                */
@RF00056 DS    0H                                                  0161
*   RESPECIFY                                                      0161
*     R13 RESTRICTED;                                              0161
*                                                                  0161
@RC00047 DS    0H                                                  0162
*   /*****************************************************************/
*   /*                                                               */
*   /* IEAVTRTE OBTAINS THE LOCAL LOCK TO SERIALIZE ACCESS TO TCB    */
*   /* FIELDS AND SETS THE SECTION FLAG IF NOT RECURSION IN THIS     */
*   /* SECTION. IT ROUTES CONTROL TO THE APPROPRIATE HANDLER AS      */
*   /* DETERMINED BY THE EXIT FLAGS PREVIOUSLY SET.                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0162
*   IF RTM2CERX=OFF THEN            /* CONTROL IS RETURNED IF A    0162
*                                      CRITICAL ERROR HAS BEEN     0162
*                                      DETECTED                      */
         TM    RTM2CERX(R8),B'01000000'                            0162
         BNZ   @RF00162                                            0162
*     DO;                                                          0163
*                                                                  0163
*       /*************************************************************/
*       /*                                                           */
*       /* OBTAIN LOCAL LOCK                                         */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0164
*       ESAGLREC=ON;                                               0164
         OI    ESAGLREC+96(R5),B'00010000'                         0164
*       DO;                         /* SETLOCK OBTAIN TYPE(LOCAL)MODE
*                                      (UNCOND)RELATED(TCB,IEAVTRTE) */
*         RESPECIFY                                                0166
*          (GPR11P,                                                0166
*           GPR12P,                                                0166
*           GPR13P,                                                0166
*           GPR14P) RESTRICTED;     /* RESTRICT REGS USED BY SETLOCK */
*         GENERATE REFS(PSALITA,FLC) SETS(GPR11P,GPR12P,GPR13P,GPR14P);
         SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,                        C
               RELATED=(TCB,IEAVTRTE)
*         RESPECIFY                                                0168
*          (GPR11P,                                                0168
*           GPR12P,                                                0168
*           GPR13P,                                                0168
*           GPR14P) UNRESTRICTED;   /* RELEASE REGS USED BY SETLOCK  */
*       END;                        /* SETLOCK OBTAIN TYPE(LOCAL)MODE
*                                      (UNCOND)RELATED(TCB,IEAVTRTE) */
*       ESAGLREC=OFF;                                              0170
*                                                                  0170
         NI    ESAGLREC+96(R5),B'11101111'                         0170
*       /*************************************************************/
*       /*                                                           */
*       /* IF NOT RECURSION IN EXIT PROCESSING, SET SECTION FLAG TO  */
*       /* SHOW IN EXIT PROCESSING                                   */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0171
*       IF RTM2RECR=ON&RTM2SCTR(RTM2EXTS)=ON THEN/* RECURSION IN EXIT*/
         TM    RTM2RECR(R8),B'10000000'                            0171
         BNO   @RF00171                                            0171
