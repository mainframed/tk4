*                         ESAGLREC=ON;                             0262
         OI    ESAGLREC+96(R5),B'00010000'                         0262
         NI    ESASDREC+96(R5),B'10111111'                         0262
*                         DO;       /* SETLOCK OBTAIN TYPE(LOCAL)MODE
