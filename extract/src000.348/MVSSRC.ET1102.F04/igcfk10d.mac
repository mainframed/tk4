         TITLE 'IGCFK10D - REDRIVE PROCESSOR                           *
                        '
IGCFK10D CSECT ,                                                01S0001
@PROLOG  BALR  @12,0                                            01S0001
@PSTART  DS    0H                                               01S0001
         USING @PSTART,@12                                      01S0001
*                                   /*
IGCFK10D IEDHJN AFTERHJN,HJN
*                                                               01S0295
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZATION                                                */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0295
*   ROPCAVT=RPARM;                  /* SET POINTER TO OP. CTL. AVT   */
         LR    ROPCAVT,RPARM                                    01S0295
*   RAVT=OPCAVTPT;                  /* SET POINTER TO TCAM AVT       */
         L     RAVT,OPCAVTPT(,ROPCAVT)                          01S0296
*   RSAVE=ADDR(OPCSAVE);            /* SET POINTER TO SAVE AREA      */
         LA    RSAVE,OPCSAVE(,ROPCAVT)                          01S0297
*                                                               01S0298
*   /*****************************************************************/
*   /*                                                               */
*   /* CALCULATE 50% OF TCAM'S LINE UNITS                            */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0298
*   RUNITLVL=AVTNOLBF;              /* # OF TCAM LINE UNITS          */
         LH    RUNITLVL,AVTNOLBF(,RAVT)                         01S0298
*   SRL(RUNITLVL,1);                /* DIVIDE BY TWO FOR 50%         */
         SRL   RUNITLVL,1                                       01S0299
*   REQPRI=FREEREDR;                /* FREE REDRIVE ELEMENT          */
         L     @09,OPCCOPCE(,ROPCAVT)                           01S0300
         MVI   REQPRI(@09),X'00'                                01S0300
*   IF OPCADGOT>=RUNITLVL THEN      /* IF # BUFFER UNITS A/D USING IS
*                                      GT OR E TO 1/2 TCAM'S LINE
*                                      UNITS                         */
         LH    @09,OPCADGOT(,ROPCAVT)                           01S0301
         N     @09,@CF01487                                     01S0301
         CR    RUNITLVL,@09                                     01S0301
         BH    @RF00301                                         01S0301
*     OPCMODID=TO00;                /* SET TO XCTL TO IGC0010D       */
         MVC   OPCMODID(2,ROPCAVT),TO00                         01S0302
*   ELSE                                                        01S0303
*     DO;                                                       01S0303
*                                                               01S0303
         B     @RC00301                                         01S0303
@RF00301 DS    0H                                               01S0304
*       /*************************************************************/
*       /*                                                           */
*       /* OBTAIN LAST OPCE ON A/D BUFFER UNIT WAIT CHAIN            */
*       /*                                                           */
*       /*************************************************************/
*                                                               01S0304
*/* NOTE: THERE IS ALWAYS AT LEAST 1 OPCE ON CHAIN IF THIS ROUTINE   */
*/*       IS CALLED                                                  */
*                                                               01S0304
*       ROPCE=ADDR(OPCBUFWT)-(ADDR(OCLINK)-ADDR(IEDQOPCE));/* INIT.
*                                      OPCE POINTER SO THAT OCLINK
*                                      FIELD OF OPCE DSECT OVERLAYS
*                                      OPCBUFWT - THE WAIT CHAIN
*                                      ANCHOR                @YM08487*/
         LA    @09,OPCBUFWT(,ROPCAVT)                           01S0304
         LR    @15,ROPCE                                        01S0304
         SLR   @15,ROPCE                                        01S0304
         SLR   @09,@15                                          01S0304
         LR    ROPCE,@09                                        01S0304
*       DO UNTIL(OCLINK=0);                                     01S0305
@DL00305 DS    0H                                               01S0306
*         RNXTLAST=ROPCE;           /* SET PTR. FOR NEXT TO LAST OPCE*/
         LR    RNXTLAST,ROPCE                                   01S0306
*         ROPCE=OCLINK;             /* SET OPCE PTR. TO NEXT OPCE    */
         L     ROPCE,OCLINK(,ROPCE)                             01S0307
*       END;                                                    01S0308
@DE00305 L     @09,OCLINK(,ROPCE)                               01S0308
         LTR   @09,@09                                          01S0308
         BNZ   @DL00305                                         01S0308
*                                                               02S0309
*       /*************************************************************/
*       /*                                                           */
*       /* OBTAIN AS MANY BUFFER UNITS AS THE OPCE NEEDS UP TO 50% OF*/
*       /* TCAM'S UNITS. CHAIN THEM OFF OCUNIT FIELD OF THE REDRIVE  */
*       /* OPCE.                                                     */
*       /*                                                           */
*       /*************************************************************/
*                                                               02S0309
*       RFY                                                     02S0309
*         RZERO RSTD;               /* PARM. FOR ALLOCBUF            */
*       RUNITPTR=ADDR(OCUNIT)-EVAL(ADDR(PRFTIC)-ADDR(IEDQPRF));/* SET
*                                      UNIT PTR. TO POINT INTO OPCE
*                                      SO THAT THE PRFTIC FIELD OF
*                                      THE PREFIX DSECT OVERLAYS
*                                      OCUNIT                        */
         LA    @09,OCUNIT(,ROPCE)                               02S0310
         SL    @09,@CF00045                                     02S0310
         LR    RUNITPTR,@09                                     02S0310
*       IF OPCADGOT+OCBUFNED>RUNITLVL THEN/* IF TOTAL OF UNITS A/D
*                                      HAS AND UNITS NEEDED EXCEEDS
*                                      THE 50% LEVEL                 */
         LH    @09,OPCADGOT(,ROPCAVT)                           02S0311
         N     @09,@CF01487                                     02S0311
         LH    @15,OCBUFNED(,ROPCE)                             02S0311
         N     @15,@CF01487                                     02S0311
         ALR   @15,@09                                          02S0311
         CR    @15,RUNITLVL                                     02S0311
         BNH   @RF00311                                         02S0311
*         RUNITGET=RUNITLVL-OPCADGOT;/* GET UNITS UP TO 50% LEVEL    */
         LCR   @09,@09                                          02S0312
         ALR   @09,RUNITLVL                                     02S0312
         LR    RUNITGET,@09                                     02S0312
*       ELSE                                                    02S0313
*         RUNITGET=OCBUFNED;        /* GET AS MANY UNITS AS NEEDED   */
         B     @RC00311                                         02S0313
@RF00311 LH    RUNITGET,OCBUFNED(,ROPCE)                        02S0313
         N     RUNITGET,@CF01487                                02S0313
*       DO UNTIL(RUNITGET=0);       /* GO TO ALLOCBUF AS MANY TIMES
*                                      AS NECESSAY TO OBTAIN UNITS   */
@RC00311 DS    0H                                               02S0314
@DL00314 DS    0H                                               02S0315
*         IF RUNITGET>MAXUNTGT THEN /* IF FK IS TO GET MORE THAN THE
*                                      # OF UNITS ALLOCBUF CAN GIVE
*                                      PER CALL                      */
         LA    @09,255                                          02S0315
         CR    RUNITGET,@09                                     02S0315
         BNH   @RF00315                                         02S0315
*           RPARM=MAXUNTGT;         /* GET THE MAXIMUM               */
         LR    RPARM,@09                                        02S0316
*         ELSE                                                  02S0317
*           RPARM=RUNITGET;         /* GET # TO GET                  */
         B     @RC00315                                         02S0317
@RF00315 LR    RPARM,RUNITGET                                   02S0317
*         CALL ALLOCBUF;            /* GET BUFFER UNITS              */
*                                                               02S0318
@RC00315 L     @15,OPCGETBF(,ROPCAVT)                           02S0318
         BALR  @14,@15                                          02S0318
*         /***********************************************************/
*         /*                                                         */
*         /* LINK UNITS JUST GOTTEN OFF OF OCUNIT AT END OF CURRENT  */
*         /* CHAIN OF BUFFERS                                        */
*         /*                                                         */
*         /***********************************************************/
*                                                               02S0319
*         DO WHILE(TIC=VALTIC);     /* LOOP TO FIND END OF CURRENT
*                                      CHAIN                 @YM05731*/
         B     @DE00319                                         02S0319
@DL00319 DS    0H                                               02S0320
*           RUNITPTR=PRFTI0;        /* POINT TO NEXT UNIT            */
         L     @09,PRFTI0-1(,RUNITPTR)                          02S0320
         LA    @09,0(,@09)                                      02S0320
         LR    RUNITPTR,@09                                     02S0320
*         END;                                                  02S0321
@DE00319 TM    TIC(RUNITPTR),B'00000011'                        02S0321
         BZ    @DL00319                                         02S0321
*         PRFTI0=RPARM;             /* POINT TO UNITS JUST GOTTEN    */
         STCM  RPARM,7,PRFTI0(RUNITPTR)                         02S0322
*         RUNITGET=RUNITGET-RZERO;  /* UPDATE # OF UNITS TO GET      */
         SLR   RUNITGET,RZERO                                   02S0323
*         OCBUFNED=OCBUFNED-RZERO;  /* UPDATE # OF UNITS OPCE NEEDS  */
         LH    @09,OCBUFNED(,ROPCE)                             02S0324
         N     @09,@CF01487                                     02S0324
         SLR   @09,RZERO                                        02S0324
         STH   @09,OCBUFNED(,ROPCE)                             02S0324
*         OCBUFGOT=OCBUFGOT+RZERO;  /* UPDATE # OF UNITS OPCE HAS    */
         LH    @09,OCBUFGOT(,ROPCE)                             02S0325
         N     @09,@CF01487                                     02S0325
         ALR   @09,RZERO                                        02S0325
         STH   @09,OCBUFGOT(,ROPCE)                             02S0325
*         OPCADGOT=OPCADGOT+RZERO;  /* UPDATE # OF UNITS A/D HAS     */
         LH    @09,OPCADGOT(,ROPCAVT)                           02S0326
         N     @09,@CF01487                                     02S0326
         ALR   @09,RZERO                                        02S0326
         STH   @09,OPCADGOT(,ROPCAVT)                           02S0326
*       END;                                                    02S0327
@DE00314 SLR   @09,@09                                          02S0327
         CR    RUNITGET,@09                                     02S0327
         BNE   @DL00314                                         02S0327
*       RFY                                                     02S0328
*         RZERO UNRSTD;                                         02S0328
*       RNXTLAST->OCLINK=0;         /* DELINK LAST OPCE              */
         ST    @09,OCLINK(,RNXTLAST)                            01S0329
*       OPCCOPCE=ROPCE;             /* AND SPECIFY AS CURRENT OPCE   */
         ST    ROPCE,OPCCOPCE(,ROPCAVT)                         01S0330
*       OPCMODID=OCWTG;             /* SET TO XCTL TO OCWTG MODULE   */
         MVC   OPCMODID(2,ROPCAVT),OCWTG(ROPCE)                 01S0331
*       OCWTG=''B;                  /* CLEAR FOR SUBSEQUENT USE      */
         XC    OCWTG(2,ROPCE),OCWTG(ROPCE)                      01S0332
*     END;                                                      01S0333
*                                                               01S0333
*   /*****************************************************************/
*   /*                                                               */
*   /* XCTL LINKAGE                                                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0334
*   RPARM=ROPCAVT;                  /* SET OP. CTL. AVT AS PARM.     */
@RC00301 LR    RPARM,ROPCAVT                                    01S0334
*   GEN EXIT REFS(ROPCAVT,OPCXCTL);                             01S0335
         XCTL  SF=(E,OPCXCTL(ROPCAVT))
*   END IGCFK10D                                                01S0336
