         L     @10,MSGPTR(,AWAPTR)                                 0159
         MVC   MSGID(4,@10),@CC01372                               0159
*                                        /* INDICATE PUTGET          */
*                            MTPUTLSW='0'B;                        0160
         NI    MTPUTLSW(@10),B'10111111'                           0160
*                            VAR1=DDNPTR;/* PTR TO DDNAME VARIABLE   */
         L     @02,PTRPDL(,AWAPTR)                                 0161
         MVC   VAR1(3,@10),DDNPTR+1(@02)                           0161
*                            L1 = DDNLEN;/* SET VARIABLE LENGTH      */
         MVC   L1(1,@10),DDNLEN+1(@02)                             0162
