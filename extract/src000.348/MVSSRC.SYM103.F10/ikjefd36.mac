*          IF RETSIZE = 2 THEN         /* IF NULL RESPONSE   @YM05480*/
         B     @RC00089                                            0091
@RF00089 L     @10,MSGPTR(,AWAPTR)                                 0091
         L     @10,RETMSG-1(,@10)                                  0091
         LA    @10,0(,@10)                                         0091
         CLC   RETSIZE(2,@10),@CH00082                             0091
         BNE   @RF00091                                            0091
*            DO;                       /* THEN               @Y30NQKH*/
*              NULLRSP = '1'B;         /* INDICATE SO        @Y30NQKH*/
         OI    NULLRSP,B'10000000'                                 0093
*              GO TO INVRSP;           /* PROCESS INVALID    @YM05480*/
         B     INVRSP                                              0094
*            END;                      /*                    @Y30NQKH*/
*        ELSE                                                      0096
*          DO;                         /* IF PUTGET SUCCESSFUL,      */
@RF00091 DS    0H                                                  0097
*            CSPLPTR = ADDR(SCANAREA); /* CSCAN PARM LIST PTR        */
         LA    CSPLPTR,SCANAREA                                    0097
*            SYNCHK = '1'B;            /* NO SYNTAX CHECK            */
         OI    SYNCHK,B'10000000'                                  0098
*            CSPLFLG = ADDR(FLAGWORD); /* FLAGS IN CSCAN LIST        */
         LA    @10,FLAGWORD                                        0099
         ST    @10,CSPLFLG(,CSPLPTR)                               0099
*            IF RETSIZE > 20 THEN      /* IF LENGTH > 20     @YM05480*/
         LA    @10,20                                              0100
         L     @05,MSGPTR(,AWAPTR)                                 0100
         L     @05,RETMSG-1(,@05)                                  0100
         LA    @05,0(,@05)                                         0100
         CH    @10,RETSIZE(,@05)                                   0100
         BNL   @RF00100                                            0100
*              RETSIZE = 20;           /* SET LENGTH TO 20   @YM05480*/
         STH   @10,RETSIZE(,@05)                                   0101
*            PGLENGH = RETSIZE+2;      /* SET LEN OF INPUT   @YM05468*/
@RF00100 L     @10,MSGPTR(,AWAPTR)                                 0102
         L     @10,RETMSG-1(,@10)                                  0102
         LA    @10,0(,@10)                                         0102
         LH    @05,RETSIZE(,@10)                                   0102
         LA    @02,2                                               0102
         ALR   @02,@05                                             0102
         STH   @02,PGLENGH                                         0102
*            PGOFFST = 0;              /* SET OFFSET TO 0            */
         SLR   @02,@02                                             0103
         STH   @02,PGOFFST                                         0103
*                                      /* COPY INPUT INTO BUFFER     */
*            PGINPUT(1:RETSIZE-2) = RETCHAR(1:RETSIZE-2); /* @YM05480*/
         SL    @05,@CF00321                                        0104
         EX    @05,@SM01407                                        0104
*            CSPLCBUF = ADDR(PGBUFFR); /* RESPONSE IN LIST           */
         LA    @10,PGBUFFR                                         0105
         ST    @10,CSPLCBUF(,CSPLPTR)                              0105
*            CSPLOA = ADDR(SCANOUT);   /* PTR TO OUTPUT AREA         */
         LA    @10,SCANOUT                                         0106
         ST    @10,CSPLOA(,CSPLPTR)                                0106
*            CSPLUPT = CPPLUPT;        /* PTR TO UPT                 */
         L     @10,CPPLPTR(,AWAPTR)                                0107
         L     @05,CPPLUPT(,@10)                                   0107
         ST    @05,CSPLUPT(,CSPLPTR)                               0107
*            CSPLECT = CPPLECT;        /* PTR TO ECT                 */
         L     @10,CPPLECT(,@10)                                   0108
         ST    @10,CSPLECT(,CSPLPTR)                               0108
*            CSPLECB = ADDR(COMMECB);  /* PTR TO ECB                 */
         LA    @10,COMMECB(,AWAPTR)                                0109
         ST    @10,CSPLECB(,CSPLPTR)                               0109
*            R1 = CSPLPTR;             /* PTR IN REGISTER 1          */
         LR    R1,CSPLPTR                                          0110
*              DO; /*CALLTSSR EP(IKJSCAN)*/                        0111
*                RESPECIFY GPR01P RSTD;                            0112
*                IF CVTSCAN = 0                                    0113
*                  THEN                                            0113
         L     @10,CVTPTR                                          0113
         C     @02,CVTSCAN(,@10)                                   0113
         BNE   @RF00113                                            0113
*                    GEN(LINK EP=IKJSCAN);                         0114
         LINK EP=IKJSCAN
*                  ELSE                                            0115
*                    CALL CVTSCAN;                                 0115
         B     @RC00113                                            0115
@RF00113 L     @10,CVTPTR                                          0115
         L     @15,CVTSCAN(,@10)                                   0115
         BALR  @14,@15                                             0115
*                RESPECIFY GPR01P UNRSTD;                          0116
@RC00113 DS    0H                                                  0117
*              END;   /* INVOKE COMMAND SCAN@Y30NQKH*/             0117
*            RCODESV = R15;            /* SAVE RETURN CODE           */
         ST    R15,RCODESV(,AWAPTR)                                0118
*            IF RCODESV > 0 THEN       /* IF COMMAND SCAN FAILED     */
         L     @10,RCODESV(,AWAPTR)                                0119
         LTR   @10,@10                                             0119
         BNP   @RF00119                                            0119
*              DO;                     /* ISSUE SYSTEM ERROR MSG     */
*                ENTRYCD = 4;          /* SET ENTRY CODE             */
         MVC   ENTRYCD(2,AWAPTR),@CH00319                          0121
*                R1 = AWAPTR;          /* WORKAREA PTR IN REG 1      */
         LR    R1,AWAPTR                                           0122
*                                      /* ISSUE SYSTEM ERROR MSG     */
*                CALL IKJEFD35;                                    0123
         L     @15,@CV00329                                        0123
         BALR  @14,@15                                             0123
*              END;                                                0124
*            ELSE                                                  0125
*              DO;                     /* IF CMD SCAN SUCCESSFUL     */
         B     @RC00119                                            0125
@RF00119 DS    0H                                                  0126
*                CSOAPTR = CSPLOA;     /* SET PTR TO OUTPUT AREA     */
         L     CSOAPTR,CSPLOA(,CSPLPTR)                            0126
*                IF CSOANOC='1'B THEN  /* IF NULL RESPONSE   @Y30NQKH*/
         TM    CSOANOC(CSOAPTR),B'00010000'                        0127
         BNO   @RF00127                                            0127
*                  DO;                 /* THEN               @Y30NQKH*/
*                    NULLRSP = '1'B;   /* INDICATE SO        @Y30NQKH*/
         OI    NULLRSP,B'10000000'                                 0129
*                    GO TO INVRSP;     /* PROCESS INVALID    @Y30NQKH*/
         B     INVRSP                                              0130
*                  END;                /*                    @Y30NQKH*/
*                ELSE                  /* ELSE               @Y30NQKH*/
*                IF CSOACNM = 0 ×      /* IF PTR IS 0,       @Y30NQKH*/
*                   CSOALNM > 4 THEN   /* LENGTH > 4, THEN   @Y30NQKH*/
@RF00127 L     @10,CSOACNM(,CSOAPTR)                               0132
         LTR   @10,@10                                             0132
         BZ    @RT00132                                            0132
         LH    @15,CSOALNM(,CSOAPTR)                               0132
         C     @15,@CF00319                                        0132
         BH    @RT00132                                            0132
*                  GO TO INVRSP;       /* PROCESS INVALID    @Y30NQKH
*                                         RESPONSE           @Y30NQKH*/
*                ELSE                  /* ELSE               @Y30NQKH*/
*                                      /* IF FREE ENTERED,   @Y30NQKH*/
*                  IF CSOABUF(1:CSOALNM) = 'FREE' THEN    /* @Y30NQKH*/
         BCTR  @15,0                                               0134
         EX    @15,@SC01409                                        0134
         BNE   @RF00134                                            0134
*                    CALL VALIDRSP;    /* PROCESS VALID REPLY@Y30NQKH*/
         BAL   @14,VALIDRSP                                        0135
*                  ELSE                /* ELSE               @Y30NQKH*/
*                    IF CSOALNM>3 THEN /* IF LENGTH > 3 THEN @Y30NQKH*/
         B     @RC00134                                            0136
@RF00134 LH    @10,CSOALNM(,CSOAPTR)                               0136
         C     @10,@CF00321                                        0136
         BH    @RT00136                                            0136
*                      GO TO INVRSP;   /* PROCESS INVALID    @Y30NQKH
*                                         RESPONSE           @Y30NQKH*/
*                    ELSE              /* ELSE               @Y30NQKH*/
*                                      /* IF END ENTERED     @Y30NQKH*/
*                      IF CSOABUF(1:CSOALNM) = 'END' THEN /* @Y30NQKH*/
         L     @01,CSOACNM(,CSOAPTR)                               0138
         BCTR  @10,0                                               0138
         EX    @10,@SC01411                                        0138
         BNE   @RF00138                                            0138
*                        DO;           /* THEN               @Y30NQKH*/
*                                     /* INDICATE ALLOC ERROR@Y30NQKH*/
*                          RBCODE12 = '1'B;               /* @Y30NQKH*/
*                                      /* TERMINATION REQ    @Y30NQKH*/
*                          TERMOPT = '1'B;                /* @Y30NQKH*/
         OI    RBCODE12(AWAPTR),B'10001000'                        0141
*                        END;          /*                    @Y30NQKH*/
*                      ELSE            /* ELSE, PROCESS      @Y30NQKH*/
*                        DO;           /* INVALID RESPONSE   @Y30NQKH*/
         B     @RC00138                                            0143
@RF00138 DS    0H                                                  0144
*         INVRSP:                                                  0144
*                                      /* IF NULL RESPONSE   @Y30NQKH*/
*                        IF NULLRSP = '1'B THEN           /* @Y30NQKH*/
INVRSP   TM    NULLRSP,B'10000000'                                 0144
         BNO   @RF00144                                            0144
*                          DO;         /* THEN               @Y30NQKH*/
*                                      /* CLEAR INDICATOR    @Y30NQKH*/
*                            NULLRSP = '0'B;              /* @Y30NQKH*/
         NI    NULLRSP,B'01111111'                                 0146
*                                      /* SET MESSAGE ID     @Y30NQKH*/
*                            MSGID = '115B';              /* @Y30NQKH*/
         L     @10,MSGPTR(,AWAPTR)                                 0147
         MVC   MSGID(4,@10),@CC01370                               0147
*                          END;        /*                    @Y30NQKH*/
*                        ELSE          /* ELSE,PROCESS       @Y30NQKH*/
*                          DO;         /* OTHER THAN NULL    @Y30NQKH*/
         B     @RC00144                                            0149
@RF00144 DS    0H                                                  0150
*                                      /* SET MESSAGE ID     @Y30NQKH*/
*                            MSGID = '115A';              /* @Y30NQKH*/
         L     @10,MSGPTR(,AWAPTR)                                 0150
         MVC   MSGID(4,@10),@CC01371                               0150
*                                      /* PTR TO RESPONSE    @Y30NQKH*/
*                            VAR1 = ADDR(RETCHAR);        /* @Y30NQKH*/
         L     @02,RETMSG-1(,@10)                                  0151
         LA    @02,0(,@02)                                         0151
         LA    @15,RETCHAR(,@02)                                   0151
         ST    @15,@TF00001                                        0151
         MVC   VAR1(3,@10),@TF00001+1                              0151
*                                      /* LEN OF VARIABLE    @Y30NQKH*/
*                            L1 = RETSIZE-2;              /* @Y30NQKH*/
         LH    @02,RETSIZE(,@02)                                   0152
         BCTR  @02,0                                               0152
         BCTR  @02,0                                               0152
         STC   @02,L1(,@10)                                        0152
*                          END;        /*                    @Y30NQKH*/
*                        MTPUTLSW = '1'B;/* INDICATE PUTLINE         */
@RC00144 L     @10,MSGPTR(,AWAPTR)                                 0154
         OI    MTPUTLSW(@10),B'01000000'                           0154
*                        CALL PUTMSG;    /* ISSUE MESSAGE            */
         BAL   @14,PUTMSG                                          0155
*                                        /* IF PUTLINE FAILED,       */
*                        IF RCODESV > 0 THEN                       0156
         L     @10,RCODESV(,AWAPTR)                                0156
         LTR   @10,@10                                             0156
         BNP   @RF00156                                            0156
*                                        /* INDICATE SO              */
