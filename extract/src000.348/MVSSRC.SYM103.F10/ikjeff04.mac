*/********************************************************************/
*/*  QMGRERR INTERNAL PROCEDURE - ISSUES COMMAND SYSTEM ERROR        */
*/*  MESSAGE FOR QMGR ERROR CODE (CALLER SETS RETCODE)               */
*/********************************************************************/
*                                                                  0513
*QMGRERR:                                                          0513
*   PROC;                           /* CALLED BY ACNTCODE SEGMENT    */
QMGRERR  STM   @14,@12,@SA00005                                    0513
*   QUIT04=ON;                      /* TELL IKJEFF04 TO TAKE ERROR 0514
*                                      EXIT                          */
         OI    QUIT04,B'10000000'                                  0514
*   MSGID='17A';                    /* COMMAND SYSTEM ERROR MESSAGE  */
         MVI   MSGID+3,C' '                                        0515
         MVC   MSGID(3),@CC02154                                   0515
*   L1=LENGTH(SWAWORD);                                            0516
         MVI   L1,X'0B'                                            0516
*   VAR1=ADDR(SWAWORD);             /* INSERT 'SWA MGR' IN THE MSG   */
         LA    @10,SWAWORD                                         0517
         STCM  @10,7,VAR1                                          0517
*   L2='84'X;                       /* LENGTH = 4 + CONVERT FROM HEX */
         MVI   L2,X'84'                                            0518
*   VAR2=ADDR(RETCODE);             /* INSERT QMGR'S RETURN CODE     */
         LA    @10,RETCODE                                         0519
         STCM  @10,7,VAR2                                          0519
*   RFY                                                            0520
*     R1 RSTD;                                                     0520
*   R1=ADDR(MSGTABLE);              /* PARAMETER LIST TO IKJEFF02    */
         LA    R1,MSGTABLE                                         0521
*   CALL IKJEFF02;                  /* ISSUE THE MESSAGE             */
         L     @15,@CV00099                                        0522
         BALR  @14,@15                                             0522
*   RFY                                                            0523
*     R1 UNRSTD;                                                   0523
*   END QMGRERR;                                                   0524
@EL00005 DS    0H                                                  0524
@EF00005 DS    0H                                                  0524
@ER00005 LM    @14,@12,@SA00005                                    0524
         BR    @14                                                 0524
