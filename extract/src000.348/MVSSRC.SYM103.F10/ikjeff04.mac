*   /*                                                               */
*   /*****************************************************************/
*                                                                  0427
*   IF HOLDAILN>MAXACNTI THEN                                      0427
         CH    HOLDAILN,@CH00043                                   0427
         BNH   @RF00427                                            0427
*     DO;                                                          0428
*       MSGID='04A';                                               0429
         MVI   MSGID+3,C' '                                        0429
         MVC   MSGID(3),@CC02070                                   0429
*       CALL AICALL02;              /* ISSUE LENGTH ERROR MESSAGE    */
         BAL   @14,AICALL02                                        0430
*     END;                                                         0431
*   ELSE                            /* LENGTH O.K. SO CONTINUE       */
*HAVEAI:                            /* LABEL FOR DO/END GROUP        */
*     DO;                                                          0432
         B     @RC00427                                            0432
@RF00427 DS    0H                                                  0432
HAVEAI   DS    0H                                                  0433
*       HTACCNTL=HTACCNTL-1;        /* NOW WANT POSITION OF LAST CHAR
*                                      IN ACCOUNT INFO, NOT NEXT     */
*                                                                  0433
         LH    @10,HTACCNTL                                        0433
         BCTR  @10,0                                               0433
         STH   @10,HTACCNTL                                        0433
*       /*************************************************************/
*       /*                                                           */
*       /* PUT QUOTES AROUND ACCOUNTING INFORMATION IF IT HAS SPECIAL*/
*       /* CHARACTERS BUT FITS ON ONE JOB CARD AND HAS ONLY ONE      */
*       /* ACCOUNTING FIELD                                          */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0434
*       IF ANYSCSW=ON THEN          /* SPECIAL CHAR IN ACCOUNT INFO  */
         TM    ANYSCSW,B'00100000'                                 0434
         BNO   @RF00434                                            0434
*HAVESPCL:                          /* LABEL FOR DO/END GROUP        */
*         DO;                                                      0435
HAVESPCL DS    0H                                                  0436
*           IF HTACCNTL<53&ACTJNFLD=1 THEN/* IF HAVE SINGLE ACCOUNT
*                                      INFO FIELD AND HAVE ROOM TO 0436
*                                      INSERT IT (IN QUOTES) INTO  0436
*                                      GENERATED JOB CARD            */
         LH    @10,HTACCNTL                                        0436
         CH    @10,@CH02141                                        0436
         BNL   @RF00436                                            0436
         L     @01,SWEPAPTR                                        0436
         L     @01,ACTPTR(,@01)                                    0436
         CLI   ACTJNFLD(@01),1                                     0436
         BNE   @RF00436                                            0436
*             DO;                                                  0437
*               HTACCNTI(2:HTACCNTL+1)=HTACCNTI(1:HTACCNTL);/* MOVE
*                                      ACCOUNT INFO OVER 1           */
         LR    @15,@10                                             0438
         BCTR  @15,0                                               0438
         MVC   @TS00001(190),HTACCNTI                              0438
         EX    @15,@SM02169                                        0438
*               HTACCNTL=HTACCNTL+2;/* ADDING ONE CHAR TO BEGINNING
*                                      AND ONE TO END OF ACCOUNT INFO*/
         AH    @10,@CH00142                                        0439
         STH   @10,HTACCNTL                                        0439
*               HTACCNTI(1)=QUOTE;  /* FIRST CHAR IS QUOTE           */
         MVI   HTACCNTI,C''''                                      0440
*               HTACCNTI(HTACCNTL)=QUOTE;/* LAST CHAR IS QUOTE       */
         LA    @01,HTACCNTI-1(@10)                                 0441
         MVI   0(@01),C''''                                        0441
*             END;                                                 0442
*                                                                  0443
*           /*********************************************************/
*           /*                                                       */
*           /* ENCLOSE ACCOUNTING INFORMATION IN PARENTHESES AND PUT */
*           /* QUOTES AROUND ACCOUNT FIELDS WITH SPECIAL CHARACTERS. */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0443
*           ELSE                                                   0443
*HAVEPREN:                          /* LABEL FOR DO/END GROUP        */
*             DO;                                                  0443
         B     @RC00436                                            0443
@RF00436 DS    0H                                                  0443
HAVEPREN DS    0H                                                  0444
*               WORKAREA(1)=LPAREN;                                0444
         MVI   WORKAREA,C'('                                       0444
*               AILASTSW=ON;        /* REMEMBER THIS IS LAST ACCOUNT
*                                      FIELD'S ENTRY IN TABLE        */
         OI    AILASTSW(AIPTR),B'10000000'                         0445
*               AIENDC=HTACCNTL;    /* POSITION OF LAST CHAR IN LAST
*                                      ACCOUNTING FIELD              */
         MVC   AIENDC(1,AIPTR),HTACCNTL+1                          0446
*               AIPTR=ADDR(AICORE); /* NOW POINT TO FIRST ENTRY -  0447
*                                      WILL LOOP THRU FIELD ENTRIES  */
         LA    AIPTR,AICORE                                        0447
*               PREVCOL=1;                                         0448
         LA    PREVCOL,1                                           0448
*               WORKCOL=2;          /* ALREADY PUT LEFT PARENTHESIS
*                                      IN COLUMN 1                   */
*                                                                  0449
         LA    WORKCOL,2                                           0449
*               /*****************************************************/
*               /*                                                   */
*               /* LOOP TO PUT QUOTES AROUND FIELDS WITH SPECIAL     */
*               /* CHARACTERS                                        */
*               /*                                                   */
*               /*****************************************************/
*                                                                  0450
*               DO I=ACTJNFLD TO 1 BY-1 WHILE(WORKCOL<=LENGTH(WORKAREA)
*                     );            /* LOOP THRU ACCOUNT FIELDS AND
*                                      TAKE EARLY EXIT IF WORKAREA IS
*                                      TOO SMALL                     */
         L     @10,SWEPAPTR                                        0450
         L     @10,ACTPTR(,@10)                                    0450
         SLR   I,I                                                 0450
         IC    I,ACTJNFLD(,@10)                                    0450
         B     @DE00450                                            0450
@DL00450 CH    WORKCOL,@CH01449                                    0450
         BH    @DC00450                                            0450
*                 IF AISKIPSW=OFF THEN/* DO IF CHARACTERS IN FIELD   */
         TM    AISKIPSW(AIPTR),B'00100000'                         0451
         BNZ   @RF00451                                            0451
*                   DO;             /* HAVECHAR DO/END GROUP         */
*                     IF AISPCLSW=ON THEN/* IF SPECIAL CHARACTER IN
*                                      THIS FIELD, NEED LEADING QUOTE*/
         TM    AISPCLSW(AIPTR),B'01000000'                         0453
         BNO   @RF00453                                            0453
*                       DO;                                        0454
*                         WORKAREA(WORKCOL)=QUOTE;                 0455
         LA    @10,WORKAREA-1(WORKCOL)                             0455
         MVI   0(@10),C''''                                        0455
*                         WORKCOL=WORKCOL+1;                       0456
         AH    WORKCOL,@CH00036                                    0456
*                       END;                                       0457
*                     WPREVCOL=WORKCOL;/* SAVE NEXT FREE POSITION IN
*                                      WORKAREA                      */
@RF00453 LR    WPREVCOL,WORKCOL                                    0458
*                     WORKCOL=WORKCOL+AIENDC-PREVCOL;/* GET LAST   0459
*                                      POSITION FOR COPY OF ACCOUNT
*                                      FIELD INTO WORKAREA           */
         LR    @10,WORKCOL                                         0459
         SLR   @15,@15                                             0459
         IC    @15,AIENDC(,AIPTR)                                  0459
         ALR   @10,@15                                             0459
         SLR   @10,PREVCOL                                         0459
         LR    WORKCOL,@10                                         0459
*                     WORKAREA(WPREVCOL:WORKCOL)=HTACCNTI(PREVCOL: 0460
*                         AIENDC);  /* COPY THE FIELD FROM HISTORY 0460
*                                      TABLE TO WORKAREA             */
         LA    @10,WORKAREA-1(WPREVCOL)                            0460
         LR    @15,WORKCOL                                         0460
         SLR   @15,WPREVCOL                                        0460
         LA    @01,HTACCNTI-1(PREVCOL)                             0460
         EX    @15,@SM02171                                        0460
*                     WORKCOL=WORKCOL+1;/* UPDATE TO NEXT AVAILABLE
*                                      POSITION IN WORKAREA          */
         LA    @10,1                                               0461
         ALR   WORKCOL,@10                                         0461
*                     IF AISPCLSW=ON THEN/* IF SPECIAL CHARACTER IN
*                                      FIELD NEED TRAILING QUOTE     */
         TM    AISPCLSW(AIPTR),B'01000000'                         0462
         BNO   @RF00462                                            0462
*                       DO;                                        0463
*                         WORKAREA(WORKCOL)=QUOTE;                 0464
         LA    @07,WORKAREA-1(WORKCOL)                             0464
         MVI   0(@07),C''''                                        0464
*                         WORKCOL=WORKCOL+1;                       0465
         ALR   WORKCOL,@10                                         0465
*                       END;                                       0466
*                   END;            /* HAVECHAR DO/END GROUP         */
@RF00462 DS    0H                                                  0468
*                 IF AILASTSW=OFF THEN/* INSERT COMMA AND UPDATE   0468
*                                      POINTERS IF NOT THE LAST    0468
*                                      ACCOUNTING FIELD              */
@RF00451 TM    AILASTSW(AIPTR),B'10000000'                         0468
         BNZ   @RF00468                                            0468
*                   DO;                                            0469
*                     WORKAREA(WORKCOL)=COMMA;                     0470
         LA    @10,WORKAREA-1(WORKCOL)                             0470
         MVI   0(@10),C','                                         0470
*                     WORKCOL=WORKCOL+1;                           0471
         AH    WORKCOL,@CH00036                                    0471
*                     PREVCOL=AIENDC+2;/* UPDATE PAST COMMA TO POINT
*                                      TO FIRST CHARACTER IN NEXT  0472
*                                      HISTORY TABLE FIELD           */
         LA    @10,2                                               0472
         SLR   PREVCOL,PREVCOL                                     0472
         IC    PREVCOL,AIENDC(,AIPTR)                              0472
         ALR   PREVCOL,@10                                         0472
*                     AIPTR=AIPTR+LENGTH(AIENTRY);/* POINT TO NEXT 0473
*                                      ENTRY IN FIELD INFO TABLE     */
         ALR   AIPTR,@10                                           0473
*                   END;                                           0474
*               END;                /* END OF LOOP THRU FIELDS       */
@RF00468 BCTR  I,0                                                 0475
@DE00450 LTR   I,I                                                 0475
         BP    @DL00450                                            0475
@DC00450 DS    0H                                                  0476
*               IF WORKCOL>LENGTH(WORKAREA) THEN/* ISSUE SIZE ERROR
*                                      MESSAGE                       */
         CH    WORKCOL,@CH01449                                    0476
         BNH   @RF00476                                            0476
*                 DO;                                              0477
*                   MSGID='04B';                                   0478
         MVI   MSGID+3,C' '                                        0478
         MVC   MSGID(3),@CC02144                                   0478
*                   CALL AICALL02;  /* WILL TURN ON QUIT SWITCH, TOO */
         BAL   @14,AICALL02                                        0479
*                 END;                                             0480
*               ELSE                /* IF ACCNT INFO FITS IN WORKAREA*/
*INFOISOK:                          /* LABEL FOR DO/END GROUP        */
*                 DO;                                              0481
         B     @RC00476                                            0481
@RF00476 DS    0H                                                  0481
INFOISOK DS    0H                                                  0482
*                   WORKAREA(WORKCOL)=RPAREN;                      0482
         LA    @10,WORKAREA-1(WORKCOL)                             0482
         MVI   0(@10),C')'                                         0482
*                   HTACCNTL=WORKCOL;                              0483
         STH   WORKCOL,HTACCNTL                                    0483
*                   HTACCNTI(1:HTACCNTL)=WORKAREA(1:HTACCNTL);/* NOW
*                                      COPY THE COMPLETELY REBUILT 0484
*                                      ACCOUNTING INFORMATION BACK 0484
*                                      INTO SUBMIT HISTORY TABLE AREA
*                                                                    */
         LH    @10,HTACCNTL                                        0484
         LR    @03,@10                                             0484
         BCTR  @03,0                                               0484
         EX    @03,@SM02173                                        0484
*                                                                  0485
*                   /*************************************************/
*                   /*                                               */
*                   /* DO SPECIAL SCAN FOR COMMAS IF ACCOUNT INFO    */
*                   /* WON'T FIT ON FIRST JOB CARD. WILL SAVE        */
*                   /* BREAKING COLUMN IN HTACNTC1 (AND HTACNTC2 IF  */
*                   /* NECESSARY). NOTE: DON'T USE AIENTRY TABLE,    */
*                   /* BECAUSE COULD HAVE COMMAS WITHIN ACCOUNT      */
*                   /* FIELDS.                                       */
*                   /*                                               */
*                   /*************************************************/
*                                                                  0485
*                   IF HTACCNTL>HTC1MAX THEN/* MUST BREAK UP INFO    */
         CH    @10,@CH01455                                        0485
         BNH   @RF00485                                            0485
*                     DO;                                          0486
*                                                                  0486
*                       /*********************************************/
*                       /*                                           */
*                       /* FIND FIRST COMMA IN THE ACCOUNTING        */
*                       /* INFORMATION                               */
*                       /*                                           */
*                       /*********************************************/
*                                                                  0487
*                       DO PREVCOL=1 TO HTACCNTL WHILE(HTACCNTI(PREVCOL
*                             )^=COMMA);                           0487
         LA    PREVCOL,1                                           0487
         B     @DE00487                                            0487
@DL00487 LA    @10,HTACCNTI-1(PREVCOL)                             0487
         CLI   0(@10),C','                                         0487
         BE    @DC00487                                            0487
*                       END;                                       0488
         AH    PREVCOL,@CH00036                                    0488
@DE00487 CH    PREVCOL,HTACCNTL                                    0488
         BNH   @DL00487                                            0488
@DC00487 DS    0H                                                  0489
*                       CALL BREAKAI;/* FIND NEXT COMMA IN INFO      */
         BAL   @14,BREAKAI                                         0489
*                       HTACNTC1=PREVCOL-1;/* HAVE A BREAKING COLUMN
*                                      (SUBTRACT ONE BECAUSE WANT  0490
*                                      POSITION OF THE CHARACTER   0490
*                                      BEFORE THE COMMA)             */
*                                                                  0490
         LR    @10,PREVCOL                                         0490
         BCTR  @10,0                                               0490
         STH   @10,HTACNTC1                                        0490
*                       /*********************************************/
*                       /*                                           */
*                       /* CONTINUE LOOKING FOR NEXT BREAKING COLUMN */
*                       /* IF FOUND GOOD BREAKING POINT FOR FIRST    */
*                       /* CARD AND REMAINING ACCOUNTING INFORMATION */
*                       /* WON'T ALL FIT ON ONE CONTINUATION CARD.   */
*                       /*                                           */
*                       /*********************************************/
*                                                                  0491
*                       IF HTACNTC1<=HTC1MAX&HTACCNTL-PREVCOL>HTC23MAX
*                         THEN      /* NOTE - DON'T CONTINUE SCAN IF
*                                      COULDN'T BREAK ACCOUNT INFO AT
*                                      A COMMA (AND WAIT TO SEE    0491
*                                      WHETHER GENERATE JOB CARD IN
*                                      IKJEFF08 BEFORE ISSUING ERROR
*                                      MESSAGE THAT INFO WON'T FIT)  */
         CH    @10,@CH01455                                        0491
         BH    @RF00491                                            0491
         LH    @10,HTACCNTL                                        0491
         SLR   @10,PREVCOL                                         0491
         CH    @10,@CH01457                                        0491
         BNH   @RF00491                                            0491
*                         DO;       /* NEED TO GET HTACNTC2          */
*                           PREVCOL=WORKCOL;/* SAVE NEXT COMMA'S   0493
*                                      POSITION                      */
         LR    PREVCOL,WORKCOL                                     0493
*                           TEMPHOLD=HTC23MAX+1;/* SAVE LIMIT FOR  0494
*                                      BREAKAI                     0494
*                                                                    */
         LA    TEMPHOLD,68                                         0494
*                                                                  0495
*                           /*****************************************/
*                           /*                                       */
*                           /* FIND NEXT COMMA IN ACCOUNT INFO (STOP */
*                           /* THE SCAN IF PREVCOL IS O.K. OR TOO    */
*                           /* LARGE FOR INSERT).                    */
*                           /*                                       */
*                           /*****************************************/
*                                                                  0495
*                           CALL BREAKAI;                          0495
         BAL   @14,BREAKAI                                         0495
*                           HTACNTC2=PREVCOL-1;/* SAVE THE BREAKING
*                                      COLUMN - IF INSERT IS TOO LONG
*                                      IKJEFF08 WILL DETECT AND ISSUE
*                                      ERROR MESSAGE                 */
         LR    @10,PREVCOL                                         0496
         BCTR  @10,0                                               0496
         STH   @10,HTACNTC2                                        0496
*                         END;      /* END OF CODE TO GET HTACNTC2   */
*                     END;          /* END OF CODE TO GET HTACNTC1   */
*                 END INFOISOK;                                    0499
*             END HAVEPREN;                                        0500
*         END HAVESPCL;             /* END OF SPECIAL CHAR CODE      */
*     END HAVEAI;                   /* END OF INFO LENGTH O.K.     0502
*                                                                    */
*                                                                  0503
*/********************************************************************/
*/*  BREAKAI INTERNAL PROCEDURE (IN BUILDAI INTERNAL PROC) -         */
*/*  SCANS TO NEXT COMMA IN USER ACCOUNTING INFORMATION.  THE SCAN   */
*/*  IS STOPPED IF PREVCOL IS O.K. OR TOO LARGE FOR INSERT.          */
*/********************************************************************/
*                                                                  0503
*BREAKAI:                                                          0503
*   PROC OPTIONS(NOSAVEAREA,DONTSAVE);                             0503
         B     @PB00004                                            0503
BREAKAI  DS    0H                                                  0504
*   DO WHILE(PREVCOL<=HTC1MAX+1&ENDSW=OFF);/* RIGHTMOST COMMA DOESN'T
*                                      COUNT IN CHECKING WHETHER   0504
*                                      ACCOUNT INFO SEGMENT WILL FIT
*                                      ON JOB CARD                   */
         B     @DE00504                                            0504
@DL00504 DS    0H                                                  0505
*     DO WORKCOL=PREVCOL+1 TO HTACCNTL WHILE(HTACCNTI(WORKCOL)^=COMMA);
         LA    WORKCOL,1                                           0505
         ALR   WORKCOL,PREVCOL                                     0505
         B     @DE00505                                            0505
@DL00505 LA    @10,HTACCNTI-1(WORKCOL)                             0505
         CLI   0(@10),C','                                         0505
         BE    @DC00505                                            0505
*                                   /* FIND NEXT COMMA IN ACCOUNT  0506
*                                      INFORMATION                   */
*     END;                                                         0506
         AH    WORKCOL,@CH00036                                    0506
@DE00505 CH    WORKCOL,HTACCNTL                                    0506
         BNH   @DL00505                                            0506
@DC00505 DS    0H                                                  0507
*     IF WORKCOL<=HTC1MAX+1 THEN    /* CONTINUE IF MIGHT BE ABLE TO
*                                      FIT MORE INFO ON CARD         */
         CH    WORKCOL,@CH02158                                    0507
         BH    @RF00507                                            0507
*       PREVCOL=WORKCOL;                                           0508
         LR    PREVCOL,WORKCOL                                     0508
*     ELSE                          /* DONE, PREVCOL=COMMA'S POSITION*/
*       ENDSW=ON;                                                  0509
         B     @RC00507                                            0509
@RF00507 OI    ENDSW,B'01000000'                                   0509
*   END;                                                           0510
@RC00507 DS    0H                                                  0510
@DE00504 CH    PREVCOL,@CH02158                                    0510
         BH    @DC00504                                            0510
         TM    ENDSW,B'01000000'                                   0510
         BZ    @DL00504                                            0510
@DC00504 DS    0H                                                  0511
*   END BREAKAI;                                                   0511
@EL00004 DS    0H                                                  0511
@EF00004 DS    0H                                                  0511
@ER00004 BR    @14                                                 0511
*   END BUILDAI;                    /* END OF INTERNAL PROCEDURE   0512
*                                                                    */
@EL00003 DS    0H                                                  0512
@EF00003 DS    0H                                                  0512
@ER00003 LM    @14,@12,@SA00003                                    0512
         BR    @14                                                 0512
