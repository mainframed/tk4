*/********************************************************************/
*/*  INITSUBT SEGMENT - INITIALIZES INTERNAL TABLES USED BY          */
*/*  THE SUBMIT COMMAND                                              */
*/********************************************************************/
*                                                                  0116
*   MSGTABLE=MSGTABLE&&MSGTABLE;    /* CLEAR THE MESSAGE AREA AND  0116
*                                      INITIALIZE THIS INTERFACE     */
         XC    MSGTABLE(64),MSGTABLE                               0116
*   HISTORY=HISTORY&&HISTORY;       /* ZERO OUT HISTORY FIELD        */
         XC    HISTORY(52),HISTORY                                 0117
*   CONTAB=CONTAB&&CONTAB;          /* ZERO OUT CONTROL TABLE - MUST
*                                      DO BEFORE FIBCUSER SET        */
         XC    CONTAB(85),CONTAB                                   0118
*   EXAREA=EXAREA&&EXAREA;          /* ZERO THE AREA USED AS AN    0119
*                                      INTERFACE TO THE PARSE EXIT 0119
*                                      ROUTINE (IKJEFF16)            */
*                                                                  0119
         XC    EXAREA(46),EXAREA                                   0119
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE SUBMIT MESSAGE TABLE (IKJEFFMT)                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0120
*   ECB=0;                          /* ZERO THE SUBMIT ECB - NOT   0120
*                                      POSTED BECAUSE SUBMIT DOESN'T
*                                      HAVE SUBCOMMANDS              */
         SLR   @10,@10                                             0120
         ST    @10,ECB                                             0120
*   LISTPTR=ADDR(MSGCSECT);         /* ADDR OF MSG PARMLIST          */
         LA    @10,MSGCSECT                                        0121
         ST    @10,LISTPTR                                         0121
*   MTCPPL=ADDR(FIBCPPLC);          /* ADDR OF COPY OF TMP'S CPPL C.B*/
         LA    @10,FIBCPPLC(,FIBCMPTR)                             0122
         ST    @10,MTCPPL                                          0122
*   ECBPTR=ADDR(ECB);                                              0123
         LA    @04,ECB                                             0123
         ST    @04,ECBPTR                                          0123
*   MTPUTLSW=ON;                    /* INDICATE DOING PUTLINE        */
         OI    MTPUTLSW,B'01000000'                                0124
*   MSGCSECT=ADDR(IKJEFF03);        /* ADDR OF SUBMIT'S MESSAGES     */
         L     @15,@CV00100                                        0125
         ST    @15,MSGCSECT                                        0125
*   MSGRTN=ADDR(IKJEFF02);          /* ADDR OF MESSAGE ISSUER ROUTINE*/
         L     @15,@CV00099                                        0126
         ST    @15,MSGRTN                                          0126
*   MTHIGH=ON;                      /* END OF MSG PARM LIST          */
*                                                                  0127
         OI    MTHIGH,B'10000000'                                  0127
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE SUBMIT HISTORY TABLE (IKJEFFHT)                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0128
*   SUBTAB='HISTORY TAB ';          /* *** INIT HISTORY TABLE        */
         MVC   SUBTAB(12),@CC02058                                 0128
*   FIRST='1'B;                     /* FIRST SWITCH ON INDICATES   0129
*                                      PROCESSING IS TAKING PLACE AND
*                                      NO ERROR STATE EXISTS AT THIS
*                                      TIME                          */
         OI    FIRST,B'00100000'                                   0129
*   JOBX=ON;                        /* INDICATES THAT THE IBM EXIT 0130
*                                      ONLY WISHES TO LOOK AT JOB  0130
*                                      CARDS                         */
*                                                                  0130
         OI    JOBX,B'10000000'                                    0130
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE SUBMIT CONTROL TABLE (IKJEFFCT)                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0131
*   FIBCUSER=ADDR(CONTAB);          /* IKJEFF20, THE SVC 100 STAI  0131
*                                      RTN, NEEDS THIS ADDRESS. IT 0131
*                                      USES THE CLOSERPT AND CTACBPT
*                                      FIELDS                        */
         LA    @15,CONTAB                                          0131
         ST    @15,FIBCUSER(,FIBCMPTR)                             0131
*   TABID='SUBMIT TABLE';           /* *** INIT CONTROL TABLE        */
         MVC   TABID(12),@CC02060                                  0132
*   CONTABPT=ADDR(CONTAB);          /* CONTROL TABLE HAS PTR TO SELF */
         LA    @15,CONTAB                                          0133
         ST    @15,CONTABPT                                        0133
*   HISTPT=ADDR(HISTORY);           /* HISTORY TABLE HOLDS INFO ABOUT
*                                      INPUT RECORDS PASSING THRU    */
         LA    @15,HISTORY                                         0134
         ST    @15,HISTPT                                          0134
*   JOBNAMES=' ';                   /* INITIALIZE JOBNAMES AREA--USED
*                                      LATER FOR MSG INSERTS         */
         MVI   JOBNAMES+1,C' '                                     0135
         MVC   JOBNAMES+2(14),JOBNAMES+1                           0135
         MVI   JOBNAMES,C' '                                       0135
*   JOBNAMPT=ADDR(JOBNAMES);        /* SAVE PTR TO JOBNAMES IN     0136
*                                      CONTROL TABLE                 */
         LA    @15,JOBNAMES                                        0136
         ST    @15,JOBNAMPT                                        0136
*   MSGLISPT=ADDR(MSGTABLE);        /* INTERFACE TO ISSUE MESSAGE    */
         LA    @15,MSGTABLE                                        0137
         ST    @15,MSGLISPT                                        0137
*   USER=FIBPSCBU;                  /* COPY THE USERID TO AN 8-BYTE
*                                      FIELD                         */
         MVI   USER+7,C' '                                         0138
         MVC   USER(7),FIBPSCBU(FIBCMPTR)                          0138
*   USERIDPT=ADDR(USER);            /* USE FOR GENERATED JOBNAMES    */
         LA    @15,USER                                            0139
         ST    @15,USERIDPT                                        0139
*   CTIDLN=FIBPSCBL;                /* SAVE USERID LENGTH, TOO       */
         MVC   CTIDLN(1),FIBPSCBL(FIBCMPTR)                        0140
*   TMCTPT=ADDR(FIBCPPLC);          /* CPPL ADDR FOR SERVICE ROUTINES*/
         ST    @10,TMCTPT                                          0141
*   COMECBPT=ADDR(ECB);             /* COMMON ECB-NEVER POSTED     0142
*                                      BECAUSE NO SUBCOMMANDS        */
         ST    @04,COMECBPT                                        0142
*   RFY                                                            0143
*     R13 RSTD;                                                    0143
*   INITSAVE=R13;                   /* SAVING THE ADDR OF THE SAVE 0144
*                                      AREA IN THE CONTROL TABLE     */
         ST    R13,INITSAVE                                        0144
*   RFY                                                            0145
*     R13 UNRSTD;                                                  0145
*                                                                  0145
*   /*****************************************************************/
*   /*                                                               */
*   /* SAVE THE COMMAND NAME IN THE SUBMIT CONTROL TABLE             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0146
*   IF CMDNAMEL<=LENGTH(CTCMDNM) THEN                              0146
         L     @10,FIBCPPLC(,FIBCMPTR)                             0146
         LH    @04,CMDNAMEL(,@10)                                  0146
         CH    @04,@CH00071                                        0146
         BH    @RF00146                                            0146
*     CTCMDNM=CMDNAME(1:CMDNAMEL);  /* GET COMMAND NAME AS ENTERED BY
*                                      THE USER (GET FROM COMMAND  0147
*                                      BUFFER RATHER THAN ECTPCMD, 0147
*                                      SINCE COULD HAVE SUBMIT UNDER
*                                      EDIT COMMAND). NAME IS USED AS
*                                      INSERT IN ERROR MESSAGES.     */
         MVI   CTCMDNM+1,C' '                                      0147
         MVC   CTCMDNM+2(6),CTCMDNM+1                              0147
         BCTR  @04,0                                               0147
         EX    @04,@SM02162                                        0147
*   ELSE                            /* TRUNCATE NAME IF NECESSARY  0148
*                                      (COMMAND NAME IS MAXIMUM OF 8
*                                      BUT CMDNAMEL COULD INCLUDE  0148
*                                      TRAILING BLANKS IN THE COMMAND
*                                      BUFFER)                       */
*     CTCMDNM=CMDNAME(1:LENGTH(CTCMDNM));                          0148
         B     @RC00146                                            0148
@RF00146 L     @10,FIBCPPLC(,FIBCMPTR)                             0148
         MVC   CTCMDNM(8),CMDNAME(@10)                             0148
*                                                                  0149
*/********************************************************************/
*/*                                                                  */
*/* ACNTCODE SEGMENT - PUTS USER'S ACCOUNTING INFORMATION            */
*/*                    IN THE SUBMIT HISTORY TABLE                   */
*/*                                                                  */
*/*   (AT LOGON THE USER'S ACCOUNT # IS OBTAINED FROM THE UADS AND   */
*/*    PASSED TO THE READER INTERPRETER.  MODULES IEFVJA AND IEFVFA  */
*/*    CREATE THE ACCOUNT CONTROL TABLE (ACT) AND PLACE THE ACCOUNT  */
*/*    # IN THIS CONTROL BLOCK, WHICH IS IN THE SCHEDULER WORK AREA. */
*/*                                                                  */
*/*    THE LOGON INSTALLATION EXIT COULD SUPPLY ACCOUNTING           */
*/*    INFORMATION FOR THE USER, INSTEAD OF JUST THE ACCOUNT NUMBER.)*/
*/*                                                                  */
*/********************************************************************/
*                                                                  0149
*   RFY                                                            0149
*     IEFAACTB BASED(ACTPTR);       /* CHANGE BASE OF ACT C.B.       */
*                                                                  0149
@RC00146 DS    0H                                                  0150
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE SWA MANAGER PARAMETER LIST                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0150
*   SWEPAPTR=ADDR(MYEPA);           /* PASS THIS PTR TO SWA MANAGER  */
         LA    @10,MYEPA                                           0150
         ST    @10,SWEPAPTR                                        0150
*   MYEPA=MYEPA&&MYEPA;             /* ZERO SWAEPA PARM LIST         */
*                                                                  0151
         XC    MYEPA(16),MYEPA                                     0151
*   /*****************************************************************/
*   /*                                                               */
*   /* HAVE SWA MANAGER GET ADDRESS OF JCT CONTROL BLOCK (WHICH HAS  */
*   /* SWA TTR FOR ACT C.B.)                                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0152
*   SWVA=JSCBJCTA;                  /* TTR OF JCT IN SWA             */
*                                                                  0152
         L     @04,TCBPTR                                          0152
         L     @04,TCBJSCB(,@04)                                   0152
         MVC   SWVA(3,@10),JSCBJCTA(@04)                           0152
*   /*****************************************************************/
*   /*                                                               */
*   /* ?SWAREQ (RL) EPA (SWEPAPTR);                                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0153
*   CALL SWAMGR(SWEPAPTR,'RL');     /* GET PTR TO JCT FROM SWA MGR   */
         LA    @10,SWEPAPTR                                        0153
         ST    @10,@AL00001                                        0153
         LA    @10,@CC02064                                        0153
         ST    @10,@AL00001+4                                      0153
         L     @10,CVTPTR                                          0153
         L     @10,CVTJESCT(,@10)                                  0153
         L     @15,JESWAA(,@10)                                    0153
         LA    @01,@AL00001                                        0153
         BALR  @14,@15                                             0153
*   RFY                                                            0154
*     R15 RSTD;                                                    0154
*   IF R15^=0 THEN                  /* BAD R.C. FROM QMGR (ISSUE MSG)*/
         LTR   R15,R15                                             0155
         BZ    @RF00155                                            0155
*     DO;                                                          0156
*       RETCODE=R15;                /* SAVE QMGR'S RETURN CODE       */
         ST    R15,RETCODE                                         0157
*       RFY                                                        0158
*         R15 UNRSTD;                                              0158
*       CALL QMGRERR;               /* ISSUE COMMAND SYSTEM ERROR MSG*/
         BAL   @14,QMGRERR                                         0159
*     END;                                                         0160
*   ELSE                            /* CONTINUE IF HAVE ADDR OF JCT
*                                      IN SWBLKPTR FIELD (SWBLKID  0161
*                                      SHOULD BE 64 AND SWLNGTH =  0161
*                                      176)                          */
*HAVEJCT:                           /* LABEL FOR DO/END GROUP        */
*     DO;                                                          0161
*                                                                  0161
         B     @RC00155                                            0161
@RF00155 DS    0H                                                  0161
HAVEJCT  DS    0H                                                  0162
*       /*************************************************************/
*       /*                                                           */
*       /* HAVE SWA MANAGER GET ADDRESS OF ACT CONTROL BLOCK         */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0162
*       TEMPHOLD=JCTACTAD;          /* JCT HAS TTR FOR ACT IN SWA AND
*                                      SWAEPA'S SWBLKPTR -> JCT      */
         L     @10,SWEPAPTR                                        0162
         L     @01,JCTPTR(,@10)                                    0162
         SLR   TEMPHOLD,TEMPHOLD                                   0162
         ICM   TEMPHOLD,7,JCTACTAD(@01)                            0162
*       MYEPA=MYEPA&&MYEPA;         /* ZERO OUT SWAEPA PARMLIST AGAIN*/
         XC    MYEPA(16),MYEPA                                     0163
*       SWVA=TEMPHOLD;              /* PUT TTR FOR ACT IN SWAEPA     */
*                                                                  0164
         STCM  TEMPHOLD,7,SWVA(@10)                                0164
*       /*************************************************************/
*       /*                                                           */
*       /* ?SWAREQ (RL) EPA (SWEPAPTR);                              */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0165
*       CALL SWAMGR(SWEPAPTR,'RL'); /* GET PTR TO ACT FROM SWA MGR   */
         LA    @10,SWEPAPTR                                        0165
         ST    @10,@AL00001                                        0165
         LA    @10,@CC02064                                        0165
         ST    @10,@AL00001+4                                      0165
         L     @10,CVTPTR                                          0165
         L     @10,CVTJESCT(,@10)                                  0165
         L     @15,JESWAA(,@10)                                    0165
         LA    @01,@AL00001                                        0165
         BALR  @14,@15                                             0165
*       RFY                                                        0166
*         R15 RSTD;                                                0166
*       IF R15^=0 THEN              /* BAD R.C. FROM QMGR (ISSUE MSG)*/
         LTR   R15,R15                                             0167
         BZ    @RF00167                                            0167
*         DO;                                                      0168
*           RETCODE=R15;            /* SAVE QMGR'S RETURN CODE       */
         ST    R15,RETCODE                                         0169
*           RFY                                                    0170
*             R15 UNRSTD;                                          0170
*           CALL QMGRERR;           /* ISSUE COMMAND SYSTEM ERROR MSG*/
         BAL   @14,QMGRERR                                         0171
*         END;                                                     0172
*                                                                  0173
*       /*************************************************************/
*       /*                                                           */
*       /* NOW HAVE THE ACT, WHICH HOLDS THE TSO USER'S ACCOUNTING   */
*       /* INFO. NOTE THAT THE GENERATE JOB CARD ROUTINE (IKJEFF08)  */
*       /* AND INSTALLATION EXIT ROUTINES (IKJEFF09, IKJEFF10) WILL  */
*       /* USE THIS INFORMATION.                                     */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0173
*       ELSE                        /* CONTINUE IF HAVE ACT ADDR IN
*                                      SWAEPA'S SWBLKPTR FIELD       */
*HAVEACT:                           /* SWBLKID SHOULD BE 1           */
*         DO;                                                      0173
         B     @RC00167                                            0173
@RF00167 DS    0H                                                  0173
HAVEACT  DS    0H                                                  0174
*           HTACCNTP=ADDR(HTACCNTI);/* SAVE ADDRESS OF ACCOUNTING  0174
*                                      INFORMATION                   */
         LA    @10,HTACCNTI                                        0174
         ST    @10,HTACCNTP                                        0174
*           IF^(ACTJNFLD=0×(ACTJNFLD=1&ACNTLEN1=0)) THEN/* IF NOT  0175
*                                      TRIVIAL CASE (WITH HTACCNTL 0175
*                                      ALREADY INITIALIZED TO ZERO)  */
         L     @10,SWEPAPTR                                        0175
         L     @10,ACTPTR(,@10)                                    0175
         CLI   ACTJNFLD(@10),0                                     0175
         BE    @RF00175                                            0175
         CLI   ACTJNFLD(@10),1                                     0175
         BNE   @RT00175                                            0175
         CLI   ACNTLEN1(@10),0                                     0175
         BE    @RF00175                                            0175
@RT00175 DS    0H                                                  0176
*             DO;                                                  0176
*               IF ACNTLEN1>MAXACNTI THEN/* IF TOO BIG ACCOUNT NUMBER*/
         L     @10,SWEPAPTR                                        0177
         L     @10,ACTPTR(,@10)                                    0177
         CLI   ACNTLEN1(@10),142                                   0177
         BNH   @RF00177                                            0177
*                 DO;                                              0178
*                   MSGID='04A';                                   0179
         MVI   MSGID+3,C' '                                        0179
         MVC   MSGID(3),@CC02070                                   0179
*                   CALL AICALL02;  /* INTERNAL PROCEDURE WILL ISSUE
*                                      ERROR MESSAGE                 */
         BAL   @14,AICALL02                                        0180
*                 END;                                             0181
*                                                                  0181
*               /*****************************************************/
*               /*                                                   */
*               /* REBUILD USER'S ACCOUNTING INFORMATION FROM FIELDS */
*               /* IN ACT                                            */
*               /*                                                   */
*               /*****************************************************/
*                                                                  0182
*               ELSE                                               0182
*                 CALL BUILDAI;     /* INTERNAL PROCEDURE TO REBUILD
*                                      ACCOUNTING INFO               */
         B     @RC00177                                            0182
@RF00177 BAL   @14,BUILDAI                                         0182
*             END;                                                 0183
@RC00177 DS    0H                                                  0184
*         END HAVEACT;                                             0184
@RF00175 DS    0H                                                  0185
*     END HAVEJCT;                                                 0185
@RC00167 DS    0H                                                  0186
*   IF QUIT04=OFF THEN              /* IF NO ERROR SO FAR            */
@RC00155 TM    QUIT04,B'10000000'                                  0186
         BNZ   @RF00186                                            0186
*     DO;                           /* ACCTOK DO/END GROUP           */
*/********************************************************************/
