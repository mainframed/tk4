         LA    @10,INTFACE                                         0188
         ST    @10,PPLPTR                                          0188
*       PPLUPT=FIBCPPLU;                                           0189
         L     @04,FIBCPPLU(,FIBCMPTR)                             0189
         ST    @04,PPLUPT(,@10)                                    0189
*       PPLECT=FIBCPPLE;                                           0190
         L     @15,FIBCPPLE(,FIBCMPTR)                             0190
         ST    @15,PPLECT(,@10)                                    0190
*       PPLECB=ADDR(ECB);                                          0191
         LA    @14,ECB                                             0191
         ST    @14,PPLECB(,@10)                                    0191
