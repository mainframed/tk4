         TITLE 'ILRQSRIT-QSR INITIALIZATION ROUTINE                    *
                        '
*
* REFS ARE GENERATED FOR NVT FIELDS THAT CONTAIN ADDRESSES OF
* NIP SERVICE ROUTINES, SO IEAPMNIP MACRO CALLS TO THESE ROUTINES
* WILL ASSEMBLE PROPERLY.
*                                                      /*
ILRQSRIT  CSECT
          USING *,R15
          B PROLOG
          MODID  BR=NO
PROLOG    DS  0H
          STM R0,R15,NIP05SAV
          DROP R15
          BALR  R12,0
          USING *,R12
*   R13=ADDR(QSRSAVE);              /* SET R13 POINTS TO QSRIT SAVE  */
         LA    R13,QSRSAVE                                         0115
*/*SET VIRTUAL=REAL                                                  */
*                                                                  0116
*   RFY                                                            0116
*    (RENTRY,                                                      0116
*     REXIT) RSTD;                  /* RESTRICT REG 14 AND REG 15    */
*   GEN REFS(NVTREAL,RNVT) SETS(RENTRY,REXIT)(IEAPMNIP  TYPE=REAL);
         IEAPMNIP  TYPE=REAL
*   RFY                                                            0118
*    (RENTRY,                                                      0118
*     REXIT) UNRSTD;                /* RELEASE REG14 AND REG 15      */
*   QSRSAVE=''B;                    /* INIT QSRIT SAVE AREA          */
         XC    QSRSAVE(72),QSRSAVE                                 0119
*   MSGLIST=''B;                    /* INIT MSG PARAMETER LIST       */
         XC    MSGLIST(24),MSGLIST                                 0120
*   NVTPR=R2;                       /* SAVE NVT PTR                  */
         ST    R2,NVTPR                                            0121
*   CVTPR=R3;                       /* SAVE CVT PTR IN MESSAGE PARAM
*                                      LIST                          */
         ST    R3,CVTPR                                            0122
*   CVTPTR=R3;                      /* SET PTR TO CVT TABLE          */
         ST    R3,CVTPTR                                           0123
*   PREADLST=''B;                   /* INIT PREAD PARMETER LIST      */
         XC    PREADLST(12),PREADLST                               0124
*   ASMVTPTR=CVTASMVT;              /* SET ASMVT PTR                 */
         L     @04,CVTPTR                                          0125
         L     ASMVTPTR,CVTASMVT(,@04)                             0125
*   PARTPTR=ASMPART;                /* SET PART PTR                  */
         L     PARTPTR,ASMPART(,ASMVTPTR)                          0126
*   TPARPTR=PARTTPAR;               /* SET TPARB PTR                 */
         L     @04,PARTTPAR(,PARTPTR)                              0127
         ST    @04,TPARPTR                                         0127
*   QSRPTR=NVTQSBUF;                /* SET QSR PTR                   */
         L     QSRPTR,NVTQSBUF(,RNVT)                              0128
*   QSRLPAHA=NVTLPAHA;              /* SET PLPA HASH IN QSR          */
         L     @04,NVTLPAHA(,RNVT)                                 0129
         ST    @04,QSRLPAHA(,QSRPTR)                               0129
*   QSRLPALO=NVTLPALO;              /* SET PLPA LOW ADDR IN QSR      */
         L     @04,NVTLPALO(,RNVT)                                 0130
         ST    @04,QSRLPALO(,QSRPTR)                               0130
*   QSRLPAHI=NVTLPAND;              /* SET PLPA HIGH ADDR IN QSR     */
         L     @04,NVTLPAND(,RNVT)                                 0131
         ST    @04,QSRLPAHI(,QSRPTR)                               0131
*   IF ASMPLPAF=ON THEN             /* CHECK FOR PLPA FULL           */
         TM    ASMPLPAF(ASMVTPTR),B'00000100'                      0132
         BNO   @RF00132                                            0132
*     DO;                                                          0133
*       QSRPLPAF=ON;                /* YES,SET SW IN QSR             */
         OI    QSRPLPAF(QSRPTR),B'10000000'                        0134
*       ASMPLPAS=ON;                /* MARK PLPA SPILL IN ASMVT      */
         OI    ASMPLPAS(ASMVTPTR),B'00000001'                      0135
*     END;                                                         0136
*   IF ASMCOMMF=ON THEN             /* CHECK FOR COMMON DATA SET FULL*/
@RF00132 TM    ASMCOMMF(ASMVTPTR),B'00000010'                      0137
         BNO   @RF00137                                            0137
*     QSRCOMMF=ON;                  /* YES,SET SW IN QSR             */
         OI    QSRCOMMF(QSRPTR),B'01000000'                        0138
*   RFY                                                            0139
*    (R0,                                                          0139
*     R14,                                                         0139
*     R15) RSTD;                    /* RESTRICT REG 0,14,15          */
@RF00137 DS    0H                                                  0140
*   R0=ADDR(QSRMAP);                /* SET ADDR OF QSR LSID MAP      */
         LA    R0,QSRMAP(,QSRPTR)                                  0140
*   R1=LENGTH(QSRMAP);              /* SET LENGTH OF QSR LSID MAP    */
         L     R1,@CF00301                                         0141
*   R15=ZERO;                       /* SET PADDING CHAR ZERO         */
         SLR   R15,R15                                             0142
*   MVCL(R0,R14);                   /* ZERO QSR LSID MAP AREA        */
         MVCL  R0,R14                                              0143
*   RFY                                                            0144
*    (R0,                                                          0144
*     R14,                                                         0144
*     R15) UNRSTD;                  /* RELEASE REG 0,14,15           */
*                                                                  0145
*/********************************************************************/
*/*                                                                  */
*/* MAIN ROUTINE                                                     */
*/*                                                                  */
*/* FUNCTION - IT COPIES LSIDS FROM XPTE TO QSRCD FOR ALL THE PAGES  */
*/*            IN PLPA.                                              */
*/*                                                                  */
*/********************************************************************/
*                                                                  0145
*   LOWPAGE=QSRLPALO;               /* GET PLPA LOW ADDR             */
         L     @04,QSRLPALO(,QSRPTR)                               0145
         LR    LOWPAGE,@04                                         0145
*   SLL(LOWPAGE,SIXTEEN);           /* SHIFT OUT SEG INDEX           */
         SLL   LOWPAGE,16                                          0146
*   SRL(LOWPAGE,TWENEIGT);          /* SHIFT OUT BYTE INDEX          */
         SRL   LOWPAGE,28                                          0147
*   PAGENO=SIXTEEN-LOWPAGE;         /* OBTAIN NO OF PAGES FOR PLPA 0148
*                                      LOW                           */
         LA    PAGENO,16                                           0148
         SLR   PAGENO,LOWPAGE                                      0148
*   SEGADDR=QSRLPALO;               /* GET PLPA LOW ADDR             */
         ST    @04,SEGADDR                                         0149
*   GEN REFS(SEGADDR,SEGMASK) SETS(SEGADDR);                       0150
*                                         /*
      NC   SEGADDR,SEGMASK
*   LOWPLPA=ON;                     /* SET PROCESS LOW PLPA SEG SW   */
         OI    LOWPLPA,B'10000000'                                 0151
*   QSREPTR=ADDR(QSRMAP);           /* SET PTR TO FIRST ENTRY IN QSR */
         LA    QSREPTR,QSRMAP(,QSRPTR)                             0152
*   DO WHILE(SEGADDR<QSRLPAHI);     /* PROCESS ALL PAGES IN PIPA     */
         B     @DE00153                                            0153
@DL00153 DS    0H                                                  0154
*     R1=SEGADDR;                   /* PASS VIRTUAL ADDR TO FIND PAGE*/
         L     R1,SEGADDR                                          0154
*     RFY                                                          0155
*       R15 RSTD;                   /* RESTRICT REG 15               */
*     CALL IEAVFP1;                 /* GET EXTERNAL PAGE TABLE       */
         L     @04,CVTPTR                                          0156
         L     @15,CVTVFP(,@04)                                    0156
         BALR  @14,@15                                             0156
*     RETCODE=R15;                  /* SAVE RETURN CODE              */
         LR    RETCODE,R15                                         0157
*     RFY                                                          0158
*       R15 UNRSTD;                 /* RELEASE R15                   */
*     IF RETCODE^=ZERO THEN         /* XPTE FOUND:                   */
         LTR   RETCODE,RETCODE                                     0159
         BZ    @RF00159                                            0159
*       DO;                         /* NO                            */
*         MSGID=MSG943W;            /* SET MSG ID-IEA943W            */
         MVC   MSGID(4),@CC01323                                   0161
*         R1=ADDR(MSGLIST);         /* PASS PARAMETER LIST           */
         LA    R1,MSGLIST                                          0162
*         CALL ILRIMMSG;            /* ISSUE MSG                     */
         L     @15,@CV01273                                        0163
         BALR  @14,@15                                             0163
*       END;                        /* END OF ISSUE MSG              */
*     ELSE                          /* XPTE FOUND                    */
*       DO;                                                        0165
         B     @RC00159                                            0165
@RF00159 DS    0H                                                  0166
*         IF LOWPLPA=ON THEN        /* PROCESS LOWEST PAGE ADDR?     */
         TM    LOWPLPA,B'10000000'                                 0166
         BNO   @RF00166                                            0166
*           DO;                                                    0167
*             LOWPLPA=OFF;          /* MARK LOWEST PAGES PROCESSED   */
         NI    LOWPLPA,B'01111111'                                 0168
*             XPTEPTR=R1+LOWPAGE*LENGTH(XPTE);/* SKIP PAGES BELOW  0169
*                                      PLPALOW                       */
         LR    XPTEPTR,LOWPAGE                                     0169
         MH    XPTEPTR,@CH01351                                    0169
         ALR   XPTEPTR,R1                                          0169
*           END;                    /* END OF SETING NEXT SEG        */
*         ELSE                                                     0171
*           XPTEPTR=R1;             /* NO-SET PTR TO FIRST XPTE      */
         B     @RC00166                                            0171
@RF00166 LR    XPTEPTR,R1                                          0171
*         DO I=ONE TO PAGENO;       /* PROCESS XPTE ENTRY IN A SEG   */
@RC00166 LA    I,1                                                 0172
         B     @DE00172                                            0172
@DL00172 DS    0H                                                  0173
*           QSRENTRY=XPTLPID;       /* COPY LSID OF XPTE TO QSR      */
         MVC   QSRENTRY(8,QSREPTR),XPTLPID(XPTEPTR)                0173
*           XPTEPTR=XPTEPTR+LENGTH(XPTE);/* GET NEXT XPTE ENTRY      */
         AL    XPTEPTR,@CF01351                                    0174
*           QSREPTR=QSREPTR+LENGTH(QSRENTRY);/* GET NEXT QSR ENTRY   */
         AL    QSREPTR,@CF00038                                    0175
*         END;                      /* END OF PROCESS TOTAL PAGES    */
         AL    I,@CF00122                                          0176
@DE00172 CR    I,PAGENO                                            0176
         BNH   @DL00172                                            0176
*         SEGADDR=SEGADDR+SEGINCMT; /* GET NEXT SEG BOUNDARY         */
         L     @09,@CF01319                                        0177
         AL    @09,SEGADDR                                         0177
         ST    @09,SEGADDR                                         0177
*         PAGENO=SIXTEEN;           /* SET PAGE NUMBER IN A SEG      */
         LA    PAGENO,16                                           0178
*       END;                        /* END OF PROCESS FOUND XPTE     */
*   END;                            /* END OF PROCESS EACH SEG PAGES
*                                      IN PLPA                       */
@RC00159 DS    0H                                                  0180
@DE00153 CLC   SEGADDR(4),QSRLPAHI(QSRPTR)                         0180
         BL    @DL00153                                            0180
*                                                                  0181
*/********************************************************************/
*/* SEGMENT NAME-QSRTERM                                             */
*/*                                                                  */
*/* DESCRIPTIVE NAME-ILRQSRIT TERMINATION ROUTINE                    */
*/*                                                                  */
*/* FUNCTION-WRITE TPARB AND QSRCD TO PLPA DATA SET                  */
*/*          AND MARKS THE PLPA DATASET READ-ONLY AND COMMON DATASET */
*/*          ACTIVE                                                  */
*/*                                                                  */
*/********************************************************************/
*/* PARENUSE FLAG NEED NOT BE CONSIDERED FOR DATASET BAD CONDITIONS  */
*/* BECAUSE RECOVERY SETS THAT FLAG AND RECOVERY IS NOT BEEN INVOKED */
*/* THIS EARLY TIME IN NIP                                           */
*                                                                  0181
*   IF ADDR(PARTENTS(PARTPLPN+ONE))->PAREDSBD=OFF THEN/* PLAP NOT BAD*/
         LA    @04,PARTENTS(,PARTPTR)                              0181
         TM    PAREDSBD(@04),B'01000000'                           0181
         BNZ   @RF00181                                            0181
*     IF ADDR(PARTENTS(PARTCOMN+ONE))->PAREDSBD=ON THEN/* COMN BAD   */
         LA    @15,PARTENTS+64(,PARTPTR)                           0182
         TM    PAREDSBD(@15),B'01000000'                           0182
         BNO   @RF00182                                            0182
*       ADDR(PARTENTS(PARTPLPN+ONE))->PAREWTQE=ADDR(PARTCOMQ);/*   0183
*                                      COMMON REQUESTS ARE WRITTEN TO
*                                      PLPA DATASET                  */
         LA    @00,PARTCOMQ(,PARTPTR)                              0183
         ST    @00,PAREWTQE(,@04)                                  0183
*     ELSE                          /* COMMON DATASET GOOD           */
*       ADDR(PARTENTS(PARTPLPN+ONE))->PAREWTQE=ADDR(PARTSPLQ);/* SET
*                                      PTR TO SPILL QUEUE FOR COMN 0184
*                                      FULL                          */
         B     @RC00182                                            0184
@RF00182 LA    @04,PARTENTS(,PARTPTR)                              0184
         LA    @00,PARTSPLQ(,PARTPTR)                              0184
         ST    @00,PAREWTQE(,@04)                                  0184
*   ELSE                                                           0185
*     ;                             /* PLPA BAD                      */
@RF00181 DS    0H                                                  0186
*   IF ADDR(PARTENTS(PARTCOMN+ONE))->PAREDSBD=OFF THEN/* COMMON GOOD */
@RC00181 LA    @04,PARTENTS+64(,PARTPTR)                           0186
         TM    PAREDSBD(@04),B'01000000'                           0186
         BNZ   @RF00186                                            0186
*     ADDR(PARTENTS(PARTCOMN+ONE))->PAREWTQE=ADDR(PARTCOMQ);/* COMN
*                                      REQUESTS ARE WRITTEN TO COMNON
*                                      DATASET                       */
         LA    @00,PARTCOMQ(,PARTPTR)                              0187
         ST    @00,PAREWTQE(,@04)                                  0187
*   IF ADDR(PARTENTS(PARTPLPN+ONE))->PAREDSBD=OFF×/* PLPA GOOD       */
*       ADDR(PARTENTS(PARTCOMN+ONE))->PAREDSBD=OFF THEN/* COMN GOOD  */
@RF00186 LA    @04,PARTENTS(,PARTPTR)                              0188
         TM    PAREDSBD(@04),B'01000000'                           0188
         BZ    @RT00188                                            0188
         LA    @04,PARTENTS+64(,PARTPTR)                           0188
         TM    PAREDSBD(@04),B'01000000'                           0188
         BNZ   @RF00188                                            0188
@RT00188 DS    0H                                                  0189
*     ASMNOCWQ=OFF;                 /* ZERO NO WRITE QUEUE FOR COMN
*                                      DATASET                       */
         NI    ASMNOCWQ(ASMVTPTR),B'10111111'                      0189
*   ELSE                                                           0190
*     ;                             /* PLPA AND COMN BOTH BAD        */
@RF00188 DS    0H                                                  0191
*   ACTION=ON;                      /* WRITE TPARB AND QSRCD         */
*   TABLE=ON;                       /* TPARB IN USE                  */
         OI    ACTION,B'11000000'                                  0192
*   SLOTNO=SIX;                     /* WRITE 6 SLOTS                 */
         MVI   SLOTNO,X'06'                                        0193
*   FIRSTSLT=ZERO;                  /* SET FIRST SLOT NUMBER TO WRITE*/
         SLR   @00,@00                                             0194
         STH   @00,FIRSTSLT                                        0194
*   TABLEPTR=ADDR(TPARPENT);        /* SET PTR TO PLPA IN TPARTBLE   */
         L     @15,TPARPTR                                         0195
         LA    @00,TPARPENT(,@15)                                  0195
         ST    @00,TABLEPTR                                        0195
*   BUFFRPTR=TPARPTR;               /* SET BUFFER PTR TO TPARTBLE AND
*                                      QSRCD                         */
         ST    @15,BUFFRPTR                                        0196
*   DO;                             /* LOAD EP('ILRPREAD')           */
*     RESPECIFY                                                    0198
*      (GPR00P,                                                    0198
*       GPR01P) RESTRICTED;                                        0198
*     GPR00P=ADDR('ILRPREAD');      /* LOAD ENTRY POINT NAME ADDRESS */
         LA    GPR00P,@CC01335                                     0199
*     GPR01P=0;                     /* JOBLIB OR LINKLIB DCB         */
         SLR   GPR01P,GPR01P                                       0200
*     SVC(8);                       /* ISSUE LOAD SVC                */
         SVC   8                                                   0201
*     RESPECIFY                                                    0202
*      (GPR00P,                                                    0202
*       GPR01P) UNRESTRICTED;                                      0202
*   END;                            /* LOAD EP('ILRPREAD') LOAD    0203
*                                      ILRPREAD                      */
*   RFY                                                            0204
*     R0 RSTD;                      /* RESTRICT REG 0                */
*   PREADPTR=R0;                    /* SET ILRPREAD ENTRY POINT      */
         LR    PREADPTR,R0                                         0205
*   RFY                                                            0206
*     R0 UNRSTD;                    /* RELEASE REG 0                 */
*   R1=ADDR(PREADLST);              /* GET ADDR OF PARAMETER LIST    */
         LA    R1,PREADLST                                         0207
*   RFY                                                            0208
*     R15 RSTD;                     /* RESTRICTED R15                */
*   CALL ILRPREAD;                  /* CALL READ/WRITE ROUTINE       */
         LR    @15,PREADPTR                                        0209
         BALR  @14,@15                                             0209
*   RETCODE=R15;                    /* SAVE RETURN CODE              */
         LR    RETCODE,R15                                         0210
*   RFY                                                            0211
*     R15 UNRSTD;                   /* RELEASE R15                   */
*   IF RETCODE^=ZERO THEN           /* WRITE SUCCESSFUL?             */
         LTR   RETCODE,RETCODE                                     0212
         BZ    @RF00212                                            0212
*     DO;                           /* NO                            */
*       ASMNOTPT=ON;                /* MARK BAD TPART                */
         OI    ASMNOTPT(ASMVTPTR),B'00010000'                      0214
*       MSGID=MSG936I;              /* SET MSG ID                    */
         MVC   MSGID(4),@CC01325                                   0215
*       R1=ADDR(MSGLIST);           /* PASS PARAMETER LIST           */
         LA    R1,MSGLIST                                          0216
*       CALL ILRIMMSG;              /* ISSUE ERR MSG                 */
         L     @15,@CV01273                                        0217
         BALR  @14,@15                                             0217
*     END;                          /* END OF ISSUE ERR MSG          */
*   ELSE                                                           0219
*     ;                             /* RETURN FROM WRITE SUCCESSFUL  */
@RF00212 DS    0H                                                  0220
*   DO;                             /* DELETE EP('ILRPREAD')         */
*     RESPECIFY                                                    0221
*      (GPR00P) RESTRICTED;                                        0221
*     GPR00P=ADDR('ILRPREAD');      /* ->ENTRY NAME                  */
         LA    GPR00P,@CC01335                                     0222
*     SVC(9);                       /* DELETE SVC                    */
         SVC   9                                                   0223
*     RESPECIFY                                                    0224
*      (GPR00P) UNRESTRICTED;                                      0224
*   END;                            /* DELETE EP('ILRPREAD')         */
*   DO;                             /* FREEMAIN(RU)                0226
*                                      LV(LENGTH(QSR))A(QSR) SP(245) */
*     RESPECIFY                                                    0227
*      (GPR01F,                                                    0227
*       GPR15F,                                                    0227
*       GPR00F) RESTRICTED;                                        0227
*     GPR01F=0;                     /* REG 1 MUST BE ZERO            */
         SLR   GPR01F,GPR01F                                       0228
*     GPR15F=3;                     /* SET TYPE                      */
         LA    GPR15F,3                                            0229
*     GPR00F=LENGTH(QSR);           /* LENGTH REQUESTED              */
         L     GPR00F,@CF01352                                     0230
*     GPR01F=ADDR(QSR);             /* SET REGISTER ONE              */
         LR    GPR01F,QSRPTR                                       0231
*     GPR15F=GPR15F+((245)*256);    /* SUBPOOL VALUE                 */
         AL    GPR15F,@CF01353                                     0232
*     SVC(120);                     /* RC/RU-FORM OF GETMAIN         */
         SVC   120                                                 0233
*     RESPECIFY                                                    0234
*      (GPR01F,                                                    0234
*       GPR15F,                                                    0234
*       GPR00F) UNRESTRICTED;                                      0234
*   END;                            /* FREEMAIN(RU)                0235
*                                      LV(LENGTH(QSR))A(QSR) SP(245) */
*   NVTQSBUF=ZERO;                  /* ZERO QSR PTR IN NVT           */
         SLR   @00,@00                                             0236
         ST    @00,NVTQSBUF(,RNVT)                                 0236
*/*SET VIRTUAL=VIRTUAL                                               */
*                                                                  0237
*   RFY                                                            0237
*    (RENTRY,                                                      0237
*     REXIT) RSTD;                  /* RESTRICT REG 14 AND REG 15    */
*   GEN REFS(NVTVIRT,RNVT) SETS(RENTRY,REXIT)(IEAPMNIP  TYPE=VIRT);
         IEAPMNIP  TYPE=VIRT
*   RFY                                                            0239
*    (RENTRY,                                                      0239
*     REXIT) UNRSTD;                /* RELEASE REG14 AND REG15       */
*   LM(R0,R15,NIP05SAV);            /* RESTORE REG0-REG15 FOR NP05   */
         LM    R0,R15,NIP05SAV                                     0240
*   RETURN;                         /* RETURN TO CALLER              */
@EL00001 DS    0H                                                  0241
@EF00001 DS    0H                                                  0241
@ER00001 BR    @14                                                 0241
*   DECLARE                         /* GENERAL PURPOSE REGISTERS     */
*     GPR00F FIXED(31) REG(0),                                     0242
*     GPR01F FIXED(31) REG(1),                                     0242
*     GPR15F FIXED(31) REG(15),                                    0242
*     GPR00P PTR(31) REG(0),                                       0242
*     GPR01P PTR(31) REG(1);                                       0242
*   DECLARE                         /* COMMON VARIABLES              */
*     I256C CHAR(256) BASED,                                       0243
*     I031F FIXED(31) BASED,                                       0243
*     I031P PTR(31) BASED,                                         0243
*     I015F FIXED(15) BASED,                                       0243
*     I015P PTR(15) BASED,                                         0243
*     I008P PTR(8) BASED,                                          0243
*     I001C CHAR(1) BASED;                                         0243
*   END ILRQSRIT                                                   0244
*                                                                  0244
*/* THE FOLLOWING INCLUDE STATEMENTS WERE FOUND IN THIS PROGRAM.     */
*/*%INCLUDE SYSUT5  (QSRSPEC )                                       */
*/*%INCLUDE SYSUT5  (QSRDATA )                                       */
*/*%INCLUDE SYSLIB  (IHANVT  )                                       */
*/*%INCLUDE SYSLIB  (ILRQSRCD)                                       */
*/*%INCLUDE SYSLIB  (ILRTPARB)                                       */
*/*%INCLUDE SYSLIB  (CVT     )                                       */
*/*%INCLUDE SYSLIB  (IHAXPTE )                                       */
*/*%INCLUDE SYSLIB  (ILRPART )                                       */
*/*%INCLUDE SYSLIB  (ILRASMVT)                                       */
*/*%INCLUDE SYSUT5  (QSRINIT )                                       */
*/*%INCLUDE SYSUT5  (QSRTERM )                                       */
*                                                                  0244
*       ;                                                          0244
@DATA    DS    0H
         DS    0F
         DS    0F
@CF00122 DC    F'1'
@CF00038 DC    F'8'
@CF01351 DC    F'12'
@CH01351 EQU   @CF01351+2
@CF00301 DC    F'16000'
@CF01352 DC    F'16384'
@CF01353 DC    F'62720'
@CF01319 DC    F'65536'
@CV01273 DC    V(ILRIMMSG)
         DS    0D
TPARPTR  DS    A
TPAEPTR  DS    A
TPASPTR  DS    A
CVTPTR   DS    A
PAREPTR  DS    A
SEGADDR  DS    A
@CC01335 DC    C'ILRPREAD'
@CC01323 DC    C'943W'
@CC01325 DC    C'936I'
MSGLIST  DS    CL24
         ORG   MSGLIST
MSGID    DS    CL4
NIPPTRS  DS    CL8
         ORG   NIPPTRS
NVTPR    DS    AL4
CVTPR    DS    AL4
         ORG   MSGLIST+12
MSGDATA1 DS    AL4
MSGDATA2 DS    AL4
MSGDATA3 DS    AL4
         ORG   MSGLIST+24
NIP05SAV DS    CL64
QSRSAVE  DS    CL72
QSRFLAG  DS    CL1
         ORG   QSRFLAG
LOWPLPA  DS    BL1
@NM00045 EQU   QSRFLAG+0
         ORG   QSRFLAG+1
         DS    CL3
PREADLST DS    CL12
         ORG   PREADLST
PREADFLG DS    BL1
         ORG   PREADFLG
ACTION   DS    BL1
TABLE    EQU   PREADFLG+0
@NM00046 EQU   PREADFLG+0
         ORG   PREADLST+1
SLOTNO   DS    FL1
FIRSTSLT DS    FL2
TABLEPTR DS    AL4
BUFFRPTR DS    AL4
         ORG   PREADLST+12
PATAREA  DC    XL40'0'
SEGMASK  DC    X'00FF0000'
@00      EQU   00                      EQUATES FOR REGISTERS 0-15
@01      EQU   01
@02      EQU   02
@03      EQU   03
@04      EQU   04
@05      EQU   05
@06      EQU   06
@07      EQU   07
@08      EQU   08
@09      EQU   09
@10      EQU   10
@11      EQU   11
@12      EQU   12
@13      EQU   13
@14      EQU   14
@15      EQU   15
I        EQU   @09
RETCODE  EQU   @09
LOWPAGE  EQU   @08
PAGENO   EQU   @10
PREADPTR EQU   @05
ASMVTPTR EQU   @07
PARTPTR  EQU   @05
XPTEPTR  EQU   @04
QSREPTR  EQU   @11
QSRPTR   EQU   @06
RNVT     EQU   @02
RCVT     EQU   @03
REXIT    EQU   @14
RENTRY   EQU   @15
R0       EQU   @00
R1       EQU   @01
R2       EQU   @02
R3       EQU   @03
R12      EQU   @12
R13      EQU   @13
R14      EQU   @14
R15      EQU   @15
GPR00P   EQU   @00
GPR01P   EQU   @01
