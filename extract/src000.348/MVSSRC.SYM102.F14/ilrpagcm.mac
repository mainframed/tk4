         L     R1,ASHCAPQ(,RSMHDPTR)                               0546
*                 ASHCAPQ=0;        /* ZERO THE QUEUE                */
         ST    @12,ASHCAPQ(,RSMHDPTR)                              0547
*                 ATAAIA=0;         /* ZERO RECOVERY POINTER         */
*                                                                  0548
         ST    @12,ATAAIA(,ATAPTR)                                 0548
*                 /***************************************************/
*                 /*                                                 */
