*                     ASM3PIOP=ATAAIA;/* PUT FIRST AIA AT THE TOP OF
*                                      THE QUEUE                     */
         L     @12,ATAAIA(,ATAPTR)                                 0640
         ST    @12,ASM3PIOP(,ASMVTPTR)                             0640
*                   END;            /* END-NORMAL COMPLETION         */
*               END;                /* SWAP DATA SET PROCESSING      */
@RC00613 DS    0H                                                  0643
*           END;                    /* END READ PROCESSING           */
@RC00592 DS    0H                                                  0644
*       END;                        /* END-ERROR CHECK               */
@RC00579 DS    0H                                                  0645
