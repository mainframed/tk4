         B     @DE00522                                            0522
@DL00522 DS    0H                                                  0523
*                         AIANXAIA=AIAFQPA;/* RECHAIN BY NEXT        */
         L     @12,AIAFQPA(,AIAPTR)                                0523
         ST    @12,AIANXAIA(,AIAPTR)                               0523
*                         AIAFQPA=0;/* ZERO HORIZONTAL PTR           */
         SLR   @07,@07                                             0524
         ST    @07,AIAFQPA(,AIAPTR)                                0524
*                         AIAPTR=AIANXAIA;/* GET NEXT AIA            */
         LR    AIAPTR,@12                                          0525
*                         AIALSID=0;/* ZERO NEXT AIA                 */
         ST    @07,AIALSID(,AIAPTR)                                0526
*                       END;        /* END-CYCLE HORIZONTAL CHAIN    */
@DE00522 L     @12,AIAFQPA(,AIAPTR)                                0527
         LTR   @12,@12                                             0527
         BNZ   @DL00522                                            0527
*                     END;          /* END-CHECK THIS AIA            */
*                                                                  0529
*                   /*************************************************/
*                   /*                                               */
*                   /* WE NOW KNOW THERE WERE NO ERRORS ON THE SWAP  */
*                   /* DATA SET COMPLETION. THE AIA'S ARE RECHAINED  */
