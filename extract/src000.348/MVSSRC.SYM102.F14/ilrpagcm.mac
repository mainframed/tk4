*                       /*                                           */
*                       /*********************************************/
*                                                                  0520
*                       AIALSID=0;  /* ZERO LSID                     */
@RC00514 SLR   @12,@12                                             0520
         ST    @12,AIALSID(,AIAPTR)                                0520
*                       AIAGRPSZ=0; /* ZERO THE GROUP SIZE           */
         ST    @12,AIAGRPSZ(,AIAPTR)                               0521
*                       DO WHILE(AIAFQPA^=0);/* CYCLE HORIZONTAL   0522
