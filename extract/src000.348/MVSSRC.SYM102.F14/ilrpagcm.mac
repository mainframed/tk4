@DL00580 DS    0H                                                  0581
*             AIANXAIA=ASM3PIOP;    /* ADD QUEUE TO THIS AIA         */
         L     @12,ASM3PIOP(,ASMVTPTR)                             0581
         ST    @12,AIANXAIA(,AIAPTR)                               0581
*             ASM3PIOP=AIAPTR;      /* PUT THIS AIA AT TOP OF QUE    */
         ST    AIAPTR,ASM3PIOP(,ASMVTPTR)                          0582
*             IF AIAPAGDS='1'B THEN /* IS THIS FROMGPAGE DS          */
         TM    AIAPAGDS(AIAPTR),B'00000100'                        0583
         BNO   @RF00583                                            0583
*               ASMIORQC=ASMIORQC+1;/* INC COMPLETED COUNT           */
         LA    @12,1                                               0584
         AL    @12,ASMIORQC(,ASMVTPTR)                             0584
         ST    @12,ASMIORQC(,ASMVTPTR)                             0584
*             IF AIAFQPA^=0 THEN    /* IS THERE A HORIZONTAL CHAIN   */
@RF00583 SLR   @12,@12                                             0585
         L     @07,AIAFQPA(,AIAPTR)                                0585
         CR    @07,@12                                             0585
