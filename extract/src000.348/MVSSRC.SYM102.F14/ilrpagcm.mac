*                     IF AIAPRIER='0'B THEN/* NO ERROR?              */
         TM    AIAPRIER(AIAPTR),B'00001000'                        0637
         BNZ   @RF00637                                            0637
*                       CALL FREESWAP;/* FREE UP THE SWAP GROUP      */
         BAL   @14,FREESWAP                                        0638
*                     AIANXAIA=ASM3PIOP;/* PICK UP QUEUE FOR IEAVPIOP*/
@RF00637 L     @12,ASM3PIOP(,ASMVTPTR)                             0639
