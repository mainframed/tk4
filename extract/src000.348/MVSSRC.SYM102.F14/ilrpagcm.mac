         NI    SPCTLVAL(@12),B'01111111'                           0625
*                       AIANXAIA=AIAFQPA;/* RECHAIN AIA'S            */
         L     @12,AIAFQPA(,AIAPTR)                                0626
         ST    @12,AIANXAIA(,AIAPTR)                               0626
*                       AIAFQPA=0;  /* ZERO HORIZONTAL PTR           */
         SLR   @07,@07                                             0627
         ST    @07,AIAFQPA(,AIAPTR)                                0627
*                       IF AIAPRIER='1'B THEN/* I/O ERROR?           */
