         LA    R13,ASMSAVE(,ASMVTPTR)                              0661
*   CALL ILRFRSL1;                  /* CALL FREESLOT MODULE          */
         L     @15,ASMPFRSL(,ASMVTPTR)                             0662
         BALR  @14,@15                                             0662
*   R14=ASM3R14B;                   /* RESTORE RETURN ADDRESS        */
         L     R14,ASM3R14B(,ASMVTPTR)                             0663
*   RFY                                                            0664
*    (R1,                                                          0664
*     R13,                                                         0664
*     R15) UNRSTD;                  /* RESTRICT USE OF REGS          */
*   END FREESLT1;                                                  0665
@EL00004 DS    0H                                                  0665
@EF00004 DS    0H                                                  0665
@ER00004 BR    @14                                                 0665
*FREESLT2:                                                         0666
