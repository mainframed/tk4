*             ASHSWPCT=ASHSWPCT-AIAGRPSZ;/* DECREMENT SWP CNT        */
@RC00498 LH    @12,ASHSWPCT(,RSMHDPTR)                             0504
         SL    @12,AIAGRPSZ(,AIAPTR)                               0504
         STH   @12,ASHSWPCT(,RSMHDPTR)                             0504
*             ATAAIA=0;             /* ZERO THE RECOVERY POINTER     */
         SLR   @07,@07                                             0505
         ST    @07,ATAAIA(,ATAPTR)                                 0505
*             AIANXAIA=ASHCAPQ;     /* PICK UP CAPTURE QUEUE         */
         L     @05,ASHCAPQ(,RSMHDPTR)                              0506
         ST    @05,AIANXAIA(,AIAPTR)                               0506
*             ASHCAPQ=AIAPTR;       /* PUT AIA ON TOP OF CAPTURE   0507
*                                      QUEUE                         */
         ST    AIAPTR,ASHCAPQ(,RSMHDPTR)                           0507
*             IF ASHSWPCT=0 THEN    /* IS SWAP COMPLETE              */
         CR    @12,@07                                             0508
         BNE   @RF00508                                            0508
*               DO;                 /* YES-PROCESS CAPTURE QUEUE     */
*                 DO WHILE(AIAPTR^=0);/* CYCLE AIA'S ON CAP Q        */
         B     @DE00510                                            0510
@DL00510 DS    0H                                                  0511
*                   ASM3GRPA=AIANXAIA;/* SAVE NEXT GROUP             */
         L     @12,AIANXAIA(,AIAPTR)                               0511
         ST    @12,ASM3GRPA(,ASMVTPTR)                             0511
*                                                                  0512
*                   /*************************************************/
*                   /*                                               */
*                   /* IF THERE WAS A PREVIOUS ERROR IT MAY HAVE TO  */
*                   /* BE REDRIVEN. ASHCAPER MEANS IT CAN BE REDRIVEN*/
*                   /* WHILE ASHPERME CANNOT.                        */
*                   /*                                               */
*                   /*************************************************/
*                                                                  0512
*                   IF ASHCAPER='1'B×/* I/O ERROR?                   */
*                       ASHPERME='1'B THEN/* MISC. ERROR             */
         TM    ASHCAPER(RSMHDPTR),B'01100000'                      0512
         BZ    @RF00512                                            0512
*                     DO;           /* YES-CHECK THIS AIA            */
*                                                                  0513
*                       /*********************************************/
*                       /*                                           */
*                       /* IF THIS AIA HAS AN ERROR THE I/O ERROR    */
*                       /* FLAG IS TURNED OFF IF THERE WAS NO        */
