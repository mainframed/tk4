*             /* BE REDRIVEN.                                        */
*             /*                                                     */
*             /*******************************************************/
*                                                                  0500
*             ELSE                  /* NO I/O ERROR                  */
*               DO;                 /* MISCELLANEOUS ERROR CHECK     */
         B     @RC00498                                            0500
@RF00498 DS    0H                                                  0501
*                 IF AIAERROR='1'B THEN/* MISCELLANEOUS ERR?         */
         TM    AIAERROR(AIAPTR),B'00000010'                        0501
         BNO   @RF00501                                            0501
*                   ASHPERME='1'B;  /* YES TURN ON PERMANENT ERROR 0502
*                                      FLAG                          */
         OI    ASHPERME(RSMHDPTR),B'00100000'                      0502
*               END;                                               0503
*                                                                  0503
@RF00501 DS    0H                                                  0504
*             /*******************************************************/
*             /*                                                     */
*             /* THE NEXT CHECK IS TO SEE IF THE SWAP HAS COMPLETED  */
