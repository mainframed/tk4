*         /*                                                         */
*         /***********************************************************/
*                                                                  0487
*         ELSE                      /* NOT A PAGE DATA SET           */
*           DO;                     /* SWAP DATA SET PROCESSING      */
         B     @RC00434                                            0487
@RF00434 DS    0H                                                  0488
*             IF AIAIORTY='1'B THEN /* I/O RETRY SPECIFIED           */
         TM    AIAIORTY(AIAPTR),B'00100000'                        0488
         BNO   @RF00488                                            0488
*               DO;                 /* RETRY PROCESSING              */
*                 AIAIORTY='0'B;    /* RESET RETRY FLAG              */
         NI    AIAIORTY(AIAPTR),B'11011111'                        0490
*                 ATAAIA=0;         /* ZERO RECOVERY POINTER         */
         SLR   @12,@12                                             0491
         ST    @12,ATAAIA(,ATAPTR)                                 0491
*                 AIANXAIA=SARWAITF;/* PICK UP QUEUE                 */
         L     @07,SARWAITF(,SARTPTR)                              0492
         ST    @07,AIANXAIA(,AIAPTR)                               0492
*                 SARWAITF=AIAPTR;  /* PUT AIA ON QUEUE              */
         ST    AIAPTR,SARWAITF(,SARTPTR)                           0493
*                 IF SARWAITL=0 THEN/* IS QUEUE EMPTY                */
         C     @12,SARWAITL(,SARTPTR)                              0494
         BNE   @RF00494                                            0494
*                   SARWAITL=AIAPTR;/* YES-ALSO MAKE AIA LAST        */
         ST    AIAPTR,SARWAITL(,SARTPTR)                           0495
*                 GO TO CKDONE;     /* GET ANOTHER AIA               */
         B     CKDONE                                              0496
*               END;                /* END RETRY PROCESSING          */
*                                                                  0497
*             /*******************************************************/
*             /*                                                     */
*             /* IF THERE WAS AN I/O ERROR (AIAPRIER=1) THE ASHCAPER */
