         TITLE 'ILRPAGCM-PAGE COMPLETION MODULE OF I/O CONTROL         *
                        '
ILRPAGCM CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'ILRPAGCM  77.028'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @08,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@08                                         0001
*ILRCBPGM:                                                         0078
*   R0=R13;                         /* SAVE SAVE AREA ADDRESS        */
ILRCBPGM LR    R0,R13                                              0078
*   ASMVTPTR=CVTASMVT;              /* GET ASMVT ADDRESS             */
         L     @12,CVTPTR                                          0079
         L     ASMVTPTR,CVTASMVT(,@12)                             0079
*   GENERATE REFS(PSALITA,FLC,PSACSTK,PSA,FRRSCURR,FRRS,R12,FRRSLAST,
*       PSALSFCC,FRRSELEN,FRRSFRRA,FRRSENTR);                      0080
GETLOCK SETLOCK OBTAIN,TYPE=SALLOC,MODE=UNCOND,                       CC
               RELATED=(PAGE/SWAP,ILRPAGCM(FREELOCK))
        L     R12,ASMPIOFR(,ASMVTPTR)
        SETFRR A,FRRAD=(R12),WRKREGS=(6,7),PARMAD=(4)
*   ASMWKSA3=''B;                   /* CLEAR WORKAREA TO ZEROES      */
         XC    ASMWKSA3(80,ASMVTPTR),ASMWKSA3(ASMVTPTR)            0081
*   ASM3SR13=R0;                    /* SAVE SAVE AREA ADDRESS        */
         ST    R0,ASM3SR13(,ASMVTPTR)                              0082
*   ATAMODID=ATAMPGCM;              /* SET MODID FOR RECOVERY        */
         MVI   ATAMODID(ATAPTR),X'02'                              0083
*   ATAPAGCM='1'B;                  /* SET FOOTPRINT IN ATA          */
         OI    ATAPAGCM(ATAPTR),B'00000001'                        0084
*   AIAPTR=R1;                      /* GET THE ADDRESS OF 1ST AIA    */
         LR    AIAPTR,R1                                           0085
*   DO WHILE(AIAPTR^=0);            /* LOOP TO CHAIN AIA'S           */
         B     @DE00086                                            0086
@DL00086 DS    0H                                                  0087
*     ATAAIA=AIANXAIA;              /* SAVE ADDRESS OF NEXT AIA      */
         L     @12,AIANXAIA(,AIAPTR)                               0087
         ST    @12,ATAAIA(,ATAPTR)                                 0087
*     IF AIALSQA='1'B THEN          /* IF AIA IS LSQA                */
         TM    AIALSQA(AIAPTR),B'00001000'                         0088
         BNO   @RF00088                                            0088
*       DO;                         /* PUT IT ON THE SWAP QUEUE      */
*         AIANXAIA=ASM3SWPQ;        /* CHAIN QUEUE TO THIS AIA       */
         L     @12,ASM3SWPQ(,ASMVTPTR)                             0090
         ST    @12,AIANXAIA(,AIAPTR)                               0090
*         ASM3SWPQ=AIAPTR;          /* PUT THIS AIA ON TOP OF QUE    */
         ST    AIAPTR,ASM3SWPQ(,ASMVTPTR)                          0091
*       END;                        /* END SWAP QUEUEING             */
*     ELSE                          /* AIA IS NOT LSQA               */
*       DO;                         /* PUT IT ON PAGECOMP'S QUEUE    */
         B     @RC00088                                            0093
@RF00088 DS    0H                                                  0094
*         AIANXAIA=ASM3GENQ;        /* CHAIN QUE TO THIS AIA         */
         L     @12,ASM3GENQ(,ASMVTPTR)                             0094
         ST    @12,AIANXAIA(,AIAPTR)                               0094
*         ASM3GENQ=AIAPTR;          /* PUT THIS AIA AT TOP OF QUE    */
         ST    AIAPTR,ASM3GENQ(,ASMVTPTR)                          0095
*       END;                        /* END PAGE QUEUEING             */
*     AIAPTR=ATAAIA;                /* GET THE NEXT AIA              */
@RC00088 L     AIAPTR,ATAAIA(,ATAPTR)                              0097
*   END;                                                           0098
@DE00086 SLR   @12,@12                                             0098
         CR    AIAPTR,@12                                          0098
         BNE   @DL00086                                            0098
*                                                                  0099
*   /*****************************************************************/
*   /*                                                               */
*   /* THE INTERNAL QUEUES THAT WERE JUST BUILT ARE NOW CHECKED TO   */
*   /* SEE IF THERE ARE ANY IOE'S FOR PAGECOMP OR SWAPCOMP           */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0099
*   IF ASM3GENQ^=0 THEN             /* ANY AIA'S FOR PAGECOMP        */
         L     @09,ASM3GENQ(,ASMVTPTR)                             0099
         CR    @09,@12                                             0099
         BE    @RF00099                                            0099
*     DO;                           /* YES                           */
*       ATAAIA=ASM3GENQ;            /* PICK UP THE AIA CHAIN         */
         ST    @09,ATAAIA(,ATAPTR)                                 0101
*       ASM3GENQ=0;                 /* ZERO THE QUEUE PTR            */
         ST    @12,ASM3GENQ(,ASMVTPTR)                             0102
*       CALL PAGECOMP;              /* GO TO PROCESS AIA'S           */
         BAL   @14,PAGECOMP                                        0103
*     END;                                                         0104
*   IF ASM3SWPQ^=0 THEN             /* ANY AIA'S FOR SWAPCOMP        */
@RF00099 SLR   @12,@12                                             0105
         L     @09,ASM3SWPQ(,ASMVTPTR)                             0105
         CR    @09,@12                                             0105
         BE    @RF00105                                            0105
*     DO;                           /* YES                           */
*       ATAAIA=ASM3SWPQ;            /* PICK UP AIA CHAIN             */
         ST    @09,ATAAIA(,ATAPTR)                                 0107
*       ASM3SWPQ=0;                 /* ZERO THE QUEUE PTR            */
         ST    @12,ASM3SWPQ(,ASMVTPTR)                             0108
*       CALL SWAPCOMP;              /* GO TO PROCESS THE AIA'S       */
         BAL   @14,SWAPCOMP                                        0109
*     END;                                                         0110
*                                                                  0111
*   /*****************************************************************/
*   /*                                                               */
*   /* DURING PROCESSING IN PAGECOMP AND SWAPCOMP AIA'S MAY HAVE BEEN*/
*   /* PUT ON THE QUEUE TO BE PASSED TO RSM(IEAVPIOP). THESE AIA'S   */
*   /* MAY BE NORMAL COMPLETIONS OR ERRORS WHICH COULD NOT BE        */
*   /* REDRIVEN. THIS QUEUE(ASM3PIOP) IS CHECKED NOW TO SEE IF ANY   */
*   /* MUST BE PASSED. ON RETURN FROM PAGECOMP AND SWAPCOMP THE      */
*   /* ATAAIA FIELD IS ALWAYS ZERO.                                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0111
*ILRCRPC1:                                                         0111
*   IF ASM3PIOP^=0 THEN             /* ANY AIA'S FOR IEAVPIOP        */
@RF00105 DS    0H                                                  0111
ILRCRPC1 SLR   @12,@12                                             0111
         L     @09,ASM3PIOP(,ASMVTPTR)                             0111
         CR    @09,@12                                             0111
         BE    @RF00111                                            0111
*     DO;                           /* YES                           */
*       RFY                                                        0113
*         R1 RSTD;                  /* RESTRICT USAGE OF REG         */
*       R1=ASM3PIOP;                /* PUT QUEUE ADDRESS IN REG      */
         LR    R1,@09                                              0114
*       ASM3PIOP=0;                 /* ZERO INTERNAL QUEUE PTR       */
         ST    @12,ASM3PIOP(,ASMVTPTR)                             0115
*       CALL IEAVPIOP;              /* SEND AIA'S TO IEAVPIOP        */
         L     @15,@CV01860                                        0116
         BALR  @14,@15                                             0116
*       RFY                                                        0117
*         R1 UNRSTD;                /* RELEASE REG FOR USE           */
*     END;                                                         0118
*                                                                  0118
*   /*****************************************************************/
*   /*                                                               */
*   /* IF ANY AIA'S FOR PAGE DATA SETS HAD TO BE REDRIVEN THEY WOULD */
*   /* HAVE BEEN PUT ON THE ASMSTAGQ. IF ANY AIA'S ARE ON THE STAGE  */
*   /* QUEUE THEY ARE SENT TO ILRQIOE FOR REDRIVE.                   */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0119
*   IF ASMSTAGF^=0 THEN             /* ANY AIA'S FOR ILRQIOE         */
@RF00111 L     @12,ASMSTAGF(,ASMVTPTR)                             0119
         LTR   @12,@12                                             0119
         BZ    @RF00119                                            0119
*     DO;                           /* YES                           */
*       ASMCALLQ='1'B;              /* TURN ON BIT TO INDICATE THAT
*                                      COMPLETION IS CALLING QIOE    */
         OI    ASMCALLQ(ASMVTPTR),B'10000000'                      0121
*       CALL ILRQIOE;               /* GO TO QIOE TO BUILD IOE'S     */
         L     @15,ASMPQIOE(,ASMVTPTR)                             0122
         BALR  @14,@15                                             0122
*       ASMCALLQ='0'B;              /* TURN OF INDICATOR BIT         */
         NI    ASMCALLQ(ASMVTPTR),B'01111111'                      0123
*     END;                                                         0124
*   R15=ASM3SR13;                   /* SAVE THE SAVE AREA ADDRESS    */
*                                                                  0125
@RF00119 L     R15,ASM3SR13(,ASMVTPTR)                             0125
*   /*****************************************************************/
*   /*                                                               */
*   /* PROCESSING IS NOW COMPLETE SO THE FRR IS DELETED AND THE      */
*   /* SALLOC LOCK IS RELEASED.                                      */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0126
*   GENERATE REFS(R6,R7,FRRSELEN,FRRSCURR,FRRSEMP,PSACSTK,FRRS,CVTGSMQ,
*       SRBFLNK,SRBSECT);                                          0126
        SETFRR D,WRKREGS=(6,7)
FREELOCK SETLOCK RELEASE,TYPE=SALLOC,                                 CC
               RELATED=(PAGE/SWAP,ILRPAGCM(GETLOCK))
*   R13=R15;                        /* GET SAVE AREA ADDRESS         */
         LR    R13,R15                                             0127
*   RETURN;                         /* RETURN TO CALLER              */
@EL00001 DS    0H                                                  0128
@EF00001 DS    0H                                                  0128
@ER00001 LM    @14,@12,12(@13)                                     0128
         BR    @14                                                 0128
*                                                                  0129
*   /*****************************************************************/
*   /*                                                               */
*   /* THE PAGE COMPLETIONS THAT WERE PUT ON THE INTGENQ IN ILRPAGCM */
*   /* ARE PASSED TO THIS ROUTINE VIA THE ATAAIA.                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0129
*PAGECOMP:                                                         0129
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0129
PAGECOMP DS    0H                                                  0130
*   ATAPCOMP='1'B;                  /* SET FOOTPRINT FOR RECOVERY    */
         OI    ATAPCOMP(ATAPTR),B'00001000'                        0130
*   ASM3R14A=R14;                   /* SAVE RETURN ADDRESS           */
         ST    R14,ASM3R14A(,ASMVTPTR)                             0131
*   DO WHILE(ATAAIA^=0);            /* LOOP THROUGH ALL AIA'S        */
         B     @DE00132                                            0132
@DL00132 DS    0H                                                  0133
*     AIAPTR=ATAAIA;                /* GET AIA ADDRESS               */
*                                                                  0133
         L     AIAPTR,ATAAIA(,ATAPTR)                              0133
*     /***************************************************************/
*     /*                                                             */
*     /* FIRST THE AIATERMR FLAG IS CHECKED TO SEE IF THE AIA IS FROM*/
*     /* A MEMORY WHICH HAS BEEN TERMINATED. IF IT IS WE CANNOT      */
*     /* ADDRESS ANY CONTROL BLOCKS FOR THIS ADDRESS SPACE BECAUSE   */
*     /* THEY MAY HAVE BEEN WIPED OUT. THE OBJECTIVE OF THIS         */
*     /* PROCESSING IS TO CLEAN UP WHERE POSSIBLE AND GIVE THE AIA   */
*     /* BACK TO RSM.                                                */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0134
*     IF AIATERMR='1'B THEN         /* IS THIS AIA FROM A MEMORY   0134
*                                      WHICH HAS BEEN TERMINATED     */
         TM    AIATERMR(AIAPTR),B'01000000'                        0134
         BNO   @RF00134                                            0134
*       DO;                         /* DO TERMR PROCESSING           */
*         IF AIAPRIER='0'B THEN     /* WAS THERE AN ERROR ON SLOT    */
         TM    AIAPRIER(AIAPTR),B'00001000'                        0136
         BNZ   @RF00136                                            0136
*           CALL FREESLT1;          /* FREE THE SLOT                 */
         BAL   @14,FREESLT1                                        0137
*                                                                  0138
*         /***********************************************************/
*         /*                                                         */
*         /* NEXT WE DETERMINE IF WE HAVE TO DECREMENT ANY OF THE    */
*         /* ALLOCATED SLOT COUNTS. ASMVSC MUST BE DECREMENTED FOR   */
*         /* VIO PAGES AND ASMNVSC MUST BE DECREMENTED FOR NON-VIO   */
*         /* PRIVATE AREA PAGES.                                     */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0138
*         IF AIAVIO='1'B THEN       /* IS THIS A VIO PAGE            */
@RF00136 TM    AIAVIO(AIAPTR),B'00100000'                          0138
         BNO   @RF00138                                            0138
*           DO;                     /* YES-DECREMENT ASMVSC          */
*             RFY                                                  0140
*              (R5,                                                0140
*               R6) RSTD;           /* RESTRICT REGS FROM USE        */
*             R5=ASMVSC;            /* GET THE COUNT                 */
         L     R5,ASMVSC(,ASMVTPTR)                                0141
*AGAIN1:                                                           0142
*             R6=R5-1;              /* DECREMENT IT                  */
AGAIN1   LR    R6,R5                                               0142
         BCTR  R6,0                                                0142
*             CS(R5,R6,ASMVSC);     /* COMPARE AND SWAP              */
         CS    R5,@06,ASMVSC(ASMVTPTR)                             0143
*             BC(7,AGAIN1);         /* NOT SUCCESSFUL-TRY AGAIN      */
         BC    7,AGAIN1                                            0144
*           END;                    /* END-DECREMENT ASMVSC          */
*         ELSE                      /* NOT A VIO PAGE                */
*           IF AIAPRIV='1'B THEN    /* IS IT A PRIVATE AREA PAGE     */
         B     @RC00138                                            0146
@RF00138 TM    AIAPRIV(AIAPTR),B'01000000'                         0146
         BNO   @RF00146                                            0146
*             ASMNVSC=ASMNVSC-1;    /* DECREMENT ASMNVSC             */
*                                                                  0147
         L     @12,ASMNVSC(,ASMVTPTR)                              0147
         BCTR  @12,0                                               0147
         ST    @12,ASMNVSC(,ASMVTPTR)                              0147
*         /***********************************************************/
*         /*                                                         */
*         /* IF A COUNT HAD TO BE DECREMENTED IT HAS BEEN DONE SO WE */
*         /* WILL NOW PICK UP THE NEXT AIA AND PUT THIS ONE ON THE   */
*         /* INTERNAL QUEUE FOR IEAVPIOP. THE TOTAL COUNT OF AIA'S   */
*         /* COMPLETED IS INCREMENTED                                */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0148
*         ATAAIA=AIANXAIA;          /* GET THE NEXT AIA              */
@RF00146 DS    0H                                                  0148
@RC00138 L     @12,AIANXAIA(,AIAPTR)                               0148
         ST    @12,ATAAIA(,ATAPTR)                                 0148
*         AIANXAIA=ASM3PIOP;        /* CHAIN THE QUEUE TO THIS AIA   */
         L     @12,ASM3PIOP(,ASMVTPTR)                             0149
         ST    @12,AIANXAIA(,AIAPTR)                               0149
*         ASM3PIOP=AIAPTR;          /* PUT THIS AIA TO TOP OF Q      */
         ST    AIAPTR,ASM3PIOP(,ASMVTPTR)                          0150
*         ASMIORQC=ASMIORQC+1;      /* INCREMENT THE COMPLETED CNT   */
*                                                                  0151
         LA    @12,1                                               0151
         AL    @12,ASMIORQC(,ASMVTPTR)                             0151
         ST    @12,ASMIORQC(,ASMVTPTR)                             0151
*         /***********************************************************/
*         /*                                                         */
*         /* A BRANCH IS TAKEN TO AVOID THE CHECK TO SEE IF SWAP     */
*         /* PROCESSING HAS COMPLETED. THE CONTROL BLOCKS MAY NOT BE */
*         /* AVAILABLE AND THE TEST CANNOT BE DONE.                  */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0152
*         GO TO CHKMORE;            /* GET ANOTHER AIA               */
         B     CHKMORE                                             0152
*       END;                        /* END-TERMR PROCESSING          */
*                                                                  0154
*     /***************************************************************/
*     /*                                                             */
*     /* SINCE THE AIATERMR FLAG WAS NOT SET WE HAVE ACCESS TO THE   */
*     /* CONTROL BLOCKS. WE SET UP THE ADDRESSES OF THE NECESSARY    */
*     /* CONTROL BLOCKS.                                             */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0154
*     PCBPTR=AIAPTR-(LENGTH(PCB)-LENGTH(AIA));                     0154
@RF00134 LR    PCBPTR,AIAPTR                                       0154
         SL    PCBPTR,@CF01923                                     0154
*     RFY                                                          0155
*       XPTEPTR RSTD;               /* RESTRICT USE OF REGISTER      */
*     XPTEPTR=PCBXPT;               /* GET ADDRESS OF THE XPTE       */
         L     XPTEPTR,PCBXPT(,PCBPTR)                             0156
*     RSMHDPTR=ASCBRSM;             /* GET ADDRESS OF THE RSMHD      */
         L     @01,ASCBPTR(,PCBPTR)                                0157
         L     RSMHDPTR,ASCBRSM(,@01)                              0157
*                                                                  0158
*     /***************************************************************/
*     /*                                                             */
*     /* THIS IS THE PROCESSING SECTION FOR VIO COMPLETIONS. IF IT IS*/
*     /* A RETRY CASE THE AIA CAN BE REQUEUED TO THE ASMSTAGQ FOR    */
*     /* REDRIVE AND ON A WRITE REDRIVE THE ALLOCATED SLOT WILL BE   */
*     /* FREED. THE AIA WILL ALSO BE REDRIVEN IF THERE WAS AN ERROR  */
*     /* ON A WRITE REQUEST. ANY OTHER CASE WILL BE SENT TO VIO      */
*     /* COMPLETION AND SUBSEQUENTLY TO RSM.                         */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0158
*     IF AIAVIO='1'B THEN                                          0158
         TM    AIAVIO(AIAPTR),B'00100000'                          0158
         BNO   @RF00158                                            0158
*       DO;                         /* DO-VIO COMPLETION             */
*         IF AIAIORTY='0'B THEN     /* IS THIS AIA OK FOR RETRY      */
         TM    AIAIORTY(AIAPTR),B'00100000'                        0160
         BNZ   @RF00160                                            0160
*           DO;                     /* NO-DO-NO RETRY PROCESSING     */
*             IF AIAWRITE='1'B&     /* IS IT A WRITE??               */
*                 AIAPRIER='1'B THEN/* YES-DID I/O ERROR OCCUR       */
         TM    AIAWRITE(AIAPTR),B'10000000'                        0162
         BNO   @RF00162                                            0162
         TM    AIAPRIER(AIAPTR),B'00001000'                        0162
         BNO   @RF00162                                            0162
*               DO;                 /* YES-WRITE ERROR-SET IT UP FOR
*                                      RECOVERY                      */
*                 AIAPRIER='0'B;    /* TURN OFF ERROR INDICATOR      */
         NI    AIAPRIER(AIAPTR),B'11110111'                        0164
*                 AIALSID=''B;      /* ZERO THE LSID IN THE AIA      */
         SLR   @12,@12                                             0165
         ST    @12,AIALSID(,AIAPTR)                                0165
*                 GO TO STAGEAIA;   /* GO TO QUE AIA FOR QIOE        */
         B     STAGEAIA                                            0166
*               END;                                               0167
*                                                                  0167
*             /*******************************************************/
*             /*                                                     */
*             /* EITHER THERE ARE NO ERRORS OR THERE IS A READ       */
*             /* ERROR(WRITE ERRORS ARE ALWAYS REDRIVEN). THE AIA    */
*             /* WILL BE SENT TO VIO COMPLETION.                     */
*             /*                                                     */
*             /*******************************************************/
*                                                                  0168
*             ELSE                  /* THIS IS NOT A WRITE AIA       */
*               CALL ILRVIOCM;      /* CALL VIO COMPLETION FOR TO  0168
*                                      UPDATE HIS TABLES             */
@RF00162 L     @15,@CV01859                                        0168
         BALR  @14,@15                                             0168
*           END;                    /* END-NO RETRY PROCESSING       */
*                                                                  0170
*         /***********************************************************/
*         /*                                                         */
*         /* THIS IS VIO PROCESSING FOR THE REDRIVE CASE. THE I/O CAN*/
*         /* BE REDRIVEN SO THE AIA IS PUT ON THE ASMSTAGQ.          */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0170
*         ELSE                      /* RETRY CASE                    */
*           DO;                     /* DO-RETRY PROCESSING           */
         B     @RC00160                                            0170
@RF00160 DS    0H                                                  0171
*             AIAIORTY='0'B;        /* TURN OFF RETRY INDICATOR      */
         NI    AIAIORTY(AIAPTR),B'11011111'                        0171
*             IF AIAWRITE='1'B THEN /* IS THIS AIA FOR A WRITE       */
         TM    AIAWRITE(AIAPTR),B'10000000'                        0172
         BNO   @RF00172                                            0172
*               CALL FREESLT1;      /* YES-FREE THE ALLOCATED SLOT   */
         BAL   @14,FREESLT1                                        0173
*             GO TO STAGEAIA;       /* GO TO QUE AIA FOR QIOE        */
         B     STAGEAIA                                            0174
*           END;                    /* END-RETRY PROCESSING          */
*         GO TO ENDUP;              /* GO TO QUEUE FOR PIOP          */
         B     ENDUP                                               0176
*       END;                        /* END-VIO COMPLETION            */
*                                                                  0178
*     /***************************************************************/
*     /*                                                             */
*     /* INITIALLY ALL SPECIAL BITS ARE CHECKED FOR ZERO TO MAKE SURE*/
*     /* THE AIA HAS NO ERRORS. THEN THE NO ERROR PROCESSING IS DONE */
*     /* AND THE SPECIAL PROCESSING FOLLOWS IT WITH ALL THE TESTS FOR*/
*     /* EXCEPTIONAL CONDITIONS. THE REASONING IS TO GET THE NORMAL  */
*     /* COMPLETION THROUGH AS FAST AS POSSIBLE.                     */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0178
*     IF AIAFRAUX='0'B&             /* WAS REQUEST PURGED            */
*         AIAIORTY='0'B&            /* MUST IT BE RETRIED            */
*         AIAPRIER='0'B&            /* WAS THERE AN I/O ERROR        */
*         AIASECER='0'B&            /* WAS THERE AN I/O ERROR        */
*         AIAERROR='0'B&            /* MISCELLANEOUS ERROR           */
*         AIABADID='0'B THEN        /* WAS THERE A BAD LSID          */
@RF00158 TM    AIAFRAUX(AIAPTR),B'10101111'                        0178
         BNZ   @RF00178                                            0178
*       DO;                         /* NO ERROR PROCESSING           */
*         IF AIAWRITE='1'B THEN     /* IS REQUEST FOR A WRITE        */
         TM    AIAWRITE(AIAPTR),B'10000000'                        0180
         BNO   @RF00180                                            0180
*           DO;                     /* DO-WRITE PROCESSING           */
*                                                                  0181
*             /*******************************************************/
*             /*                                                     */
*             /* THE AIA HAS COMPLETED OK. THE LSID MUST BE MOVED    */
*             /* INTO THE XPTE. WE NEXT DETERMINE WHAT TYPE OF       */
*             /* COMPLETION WE HAVE.                                 */
*             /*                                                     */
*             /*******************************************************/
*                                                                  0182
*             IF AIAPRIV='1'B THEN  /* PRIVATE?                      */
         TM    AIAPRIV(AIAPTR),B'01000000'                         0182
         BNO   @RF00182                                            0182
*               DO;                 /* YES-PRIVATE AREA COMPLETION   */
*                                                                  0183
*                 /***************************************************/
*                 /*                                                 */
*                 /* IF THE PAGE IS INVOLVED IN A SWAP OPERATION PUT */
*                 /* THE LSID INTO THE SPCTE FROM THE AIA.           */
*                 /*                                                 */
*                 /***************************************************/
*                                                                  0184
*                 IF AIASWPFX='1'B THEN                            0184
         TM    AIASWPFX(AIAPTR),B'00000010'                        0184
         BNO   @RF00184                                            0184
*                   DO;             /* UPDATE SPCTE                  */
*                     SPCTSSID=AIASSID;/* LSID                       */
         L     @12,SWAPPTR(,PCBPTR)                                0186
         MVC   SPCTSSID(3,@12),AIASSID(AIAPTR)                     0186
*                     SPCTLVAL='1'B;/* VALIDATE                      */
         OI    SPCTLVAL(@12),B'10000000'                           0187
*                   END;            /* END UPDATE SPCTE              */
*                                                                  0189
*                 /***************************************************/
*                 /*                                                 */
*                 /* A TRAS IS DONE TO GET THE CORRECT STOR TO       */
*                 /* ADDRESS THE XPTE FOR THIS SLOT.                 */
*                 /*                                                 */
*                 /***************************************************/
*                                                                  0189
*                 RFY                                              0189
*                   R5 RSTD;                                       0189
@RF00184 DS    0H                                                  0190
*                 R5=RSMASCB;       /* ASCB ADDRESS                  */
         L     R5,RSMASCB(,RSMHDPTR)                               0190
*                 ATAASCB=R5;       /* RECOVERY PTR                  */
         ST    R5,ATAASCB(,ATAPTR)                                 0191
*                 DO;               /* ILRTRAS TO(R5)                */
*                   DCL                                            0193
*                     TRASASID PTR(15) DEFINED(ASCBASID);          0193
*                   PSALCCAV->LCCAASID=R5->TRASASID;               0194
         LH    @12,TRASASID(,R5)                                   0194
         L     @07,PSALCCAV                                        0194
         ST    @12,LCCAASID(,@07)                                  0194
*                   GEN(LCTL  1,1,ASCBSTOR(R5))                    0195
*                   REFS(ASCBSTOR,R5);                             0195
         LCTL  1,1,ASCBSTOR(R5)
