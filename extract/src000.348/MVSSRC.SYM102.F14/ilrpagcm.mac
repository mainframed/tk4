*                         DO;       /* YES-PROPAGATE ERROR           */
*                           AIAPTR=AIANXAIA;/* GET NEXT AIA          */
         LR    AIAPTR,@12                                          0630
*                           AIAPRIER='1'B;/* SET ERROR BIT           */
         OI    AIAPRIER(AIAPTR),B'00001000'                        0631
*                         END;      /* END PROPAGATE ERROR           */
*                       ELSE        /* NO I/O ERROR                  */
*                         AIAPTR=AIANXAIA;/* GET NEXT AIA            */
         B     @RC00628                                            0633
@RF00628 L     AIAPTR,AIANXAIA(,AIAPTR)                            0633
*                     END;          /* END-REQUEUE LOOP              */
@RC00628 DS    0H                                                  0634
@DE00623 L     @12,AIAFQPA(,AIAPTR)                                0634
         LTR   @12,@12                                             0634
         BNZ   @DL00623                                            0634
*                     PCBPTR=AIAPTR-(LENGTH(PCB)-LENGTH(AIA));     0635
