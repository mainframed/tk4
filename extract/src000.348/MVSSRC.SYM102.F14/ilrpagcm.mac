@RF00585 DS    0H                                                  0590
@DE00580 C     AIAPTR,ASM3PIOP(,ASMVTPTR)                          0590
         BNE   @DL00580                                            0590
*         ELSE                      /* NORMAL READ COMPLETION        */
*           DO;                     /* DO READ PROCESSING            */
         B     @RC00579                                            0591
@RF00579 DS    0H                                                  0592
*             IF AIAPAGDS='1'B THEN /* WAS PAGE DATA SET USED        */
         TM    AIAPAGDS(AIAPTR),B'00000100'                        0592
         BNO   @RF00592                                            0592
*               DO;                 /* PAGE DATA SET PROCESSING      */
*                                                                  0593
