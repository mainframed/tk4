*                           IF ASHPERME='0'B THEN/* PERM ERR?        */
         TM    ASHPERME(RSMHDPTR),B'00100000'                      0516
         BNZ   @RF00516                                            0516
*                             AIAPRIER='0'B;/* NO-TURN OFF           */
         NI    AIAPRIER(AIAPTR),B'11110111'                        0517
*                         END;      /* END-TURN OFF I/O ERROR FLAG   */
*                                                                  0518
*                       /*********************************************/
*                       /*                                           */
*                       /* THIS SWAP SET CAN BE FREED SINCE THERE IS */
