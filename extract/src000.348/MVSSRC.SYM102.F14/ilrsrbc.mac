*                                                                  0255
ACEDEQFR DS    0H                                                  0256
*/********************************************************************/
*/*                                                                  */
*/* ACEDEQFR WILL DEQUEUE AN ACE FROM THE LGE PROCESS QUEUE AND THEN */
*/* FREE THE ACE VIA THE ILRGMA MACRO. THE LGE PROCESS QUEUE IS A    */
*/* DOUBLE HEADED, DOUBLE THREADED QUEUE.                            */
*/*                                                                  */
*/* INPUT:  ACE ADDR IN ACEPTR                                       */
*/*         LGE ADDR IN LGEPTR                                       */
*/*         R2 THRU R4 STANDARD AS IN MAPPING MACRO POINTERS         */
*/*                                                                  */
*/* OUTPUT:  LGEPROCF, LGEPROCL, ACEFQPA, AND/OR ACEBQPA UPDATED.    */
*/*                                                                  */
*/********************************************************************/
*/********************************************************************/
*/*ACEDEQFR: CHART (AREF)                                            */
*/*         HEADER                                                 0256
*/*         ILRSRBC                                                0256
*/* ACEDEQFR - SUBROUTINE TO DEQUEUE AND FREE AN ACE                 */
*/*         E ENTRY                                                  */
*/*         P SAVE RETURN ADDRESS                                    */
*/*         D (Y,,N,%SRBC11) ACE IS FIRST AND LAST ON Q              */
*/*         P (,%SRBC14) FIRST AND LAST PTRS = 0                     */
*/*%SRBC11: D (Y,,N,%SRBC12) ACE IS FIRST ON Q (LGEPROCF)            */
*/*         P FIRST AIA/ACE PTR = ACE FORWARD PTR                    */
*/*         P (,%SRBC14) BACK PTR OF ACE FORWARD PTR = 0             */
*/*%SRBC12: D (Y,,N,%SRBC13) ACE IS LAST ON Q (LGEPROCL)             */
*/*         P LAST AIA/ACE PTR = ACE BACK PTR                        */
*/*         P (,%SRBC14) FORWARD PTR OF ACE BACK PTR = 0             */
*/*%SRBC13: P BACK PTR OF ACE FORWARD PTR = BACK PTR OF ACE          */
*/*         P FORWARD PTR OF ACE BACK PTR = FORWARD PTR OF ACE       */
*/*%SRBC14: P INITIALIZE CELL@ FOR GMA MACRO                         */
*/*         L ILRGMA: RETURN THE ACE TO THE POOL                     */
*/*         R RETURN                                                 */
*/*ACEDEQFR: END                                                     */
*/********************************************************************/
*                                                                  0256
*                                                                  0256
*   /*****************************************************************/
*   /*                                                               */
*   /* DEQUEUES ACE FROM LGE PROCESS QUEUE AND THEN FREES ACE VIA THE*/
*   /* ILRGMA MACRO                                                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0256
*   RFY                                                            0256
*    (R10,                                                         0256
*     R14) RSTD;                                                   0256
*   R10=R14;                        /* MAINTAIN THE RETURN ADDR      */
         LR    R10,R14                                             0257
*   RFY                                                            0258
*     R14 UNRSTD;                                                  0258
*   IF ACEPTR=LGEPROCF&ACEPTR=LGEPROCL THEN                        0259
         C     ACEPTR,LGEPROCF(,LGEPTR)                            0259
         BNE   @RF00259                                            0259
         C     ACEPTR,LGEPROCL(,LGEPTR)                            0259
         BNE   @RF00259                                            0259
*     DO;                           /* ACE IS BOTH THE FIRST AND THE
*                                      LAST ELEMENTS ON THE QUEUE    */
*       LGEPROCF=ZERO;              /* CLEAR BOTH THE FIRST AND      */
         SLR   @12,@12                                             0261
         ST    @12,LGEPROCF(,LGEPTR)                               0261
*       LGEPROCL=ZERO;              /* LAST QUEUE ELEMENT POINTERS   */
         ST    @12,LGEPROCL(,LGEPTR)                               0262
*     END;                                                         0263
*   ELSE                                                           0264
*     IF ACEPTR=LGEPROCF THEN                                      0264
         B     @RC00259                                            0264
@RF00259 C     ACEPTR,LGEPROCF(,LGEPTR)                            0264
         BNE   @RF00264                                            0264
*       DO;                         /* ACE IS FIRST BUT NOT LAST   0265
*                                      ELEMENT ON THE QUEUE          */
*         LGEPROCF=ACEFQPA;         /* FIRST ELEMENT SET TO NEXT ACE */
         L     @12,ACEFQPA(,ACEPTR)                                0266
         ST    @12,LGEPROCF(,LGEPTR)                               0266
*         ACEFQPA->ACEBQPA=ZERO;    /* CLEAR BACK POINTER OF NEXT ACE*/
         SLR   @11,@11                                             0267
         ST    @11,ACEBQPA(,@12)                                   0267
*       END;                                                       0268
*     ELSE                                                         0269
*       IF ACEPTR=LGEPROCL THEN                                    0269
         B     @RC00264                                            0269
@RF00264 C     ACEPTR,LGEPROCL(,LGEPTR)                            0269
         BNE   @RF00269                                            0269
*         DO;                       /* ACE IS LAST BUT NOT FIRST   0270
*                                      ELEMENT ON THE QUEUE          */
*           LGEPROCL=ACEBQPA;       /* LAST ELEMENT SET TO PREVIOUS
*                                      ACE                           */
         L     @12,ACEBQPA(,ACEPTR)                                0271
         ST    @12,LGEPROCL(,LGEPTR)                               0271
*           ACEBQPA->ACEFQPA=ZERO;  /* CLEAR FORWARD POINTER OF    0272
*                                      PREVIOUS ACE                  */
         SLR   @11,@11                                             0272
         ST    @11,ACEFQPA(,@12)                                   0272
*         END;                                                     0273
*       ELSE                                                       0274
*         DO;                       /* ACE ON MIDDLE OF QUEUE, NOT 0274
*                                      FIRST OR LAST                 */
         B     @RC00269                                            0274
@RF00269 DS    0H                                                  0275
*           ACEFQPA->ACEBQPA=ACEBQPA;/* BACK PTR OF NEXT ACE SET TO
*                                      BACK PTR OF ACE DEQ'ING       */
         L     @12,ACEFQPA(,ACEPTR)                                0275
         L     @11,ACEBQPA(,ACEPTR)                                0275
         ST    @11,ACEBQPA(,@12)                                   0275
*           ACEBQPA->ACEFQPA=ACEFQPA;/* FORWARD PTR OF PREVIOUS ACE
*                                      SET TO FORWARD PTR OF ACE   0276
*                                      DEQ'ING                       */
         ST    @12,ACEFQPA(,@11)                                   0276
*         END;                                                     0277
*   RFY                                                            0278
*     R1 RSTD;                                                     0278
@RC00269 DS    0H                                                  0278
@RC00264 DS    0H                                                  0278
@RC00259 DS    0H                                                  0279
*   R1=ACEPTR;                      /* OBTAIN ADDR OF ACE TO FREE    */
*                                                                  0279
         LR    R1,ACEPTR                                           0279
*   /*****************************************************************/
*   /*                                                               */
*   /* BEGIN ILRGMA MACRO EXPANSION                                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0280
*   DO;                             /* ILRGMA FREE                 0280
*                                      CELL(ACE)WORKREGS(R11,R12)    */
*     RESPECIFY                                                    0281
*      (R0,                                                        0281
*       R1,                                                        0281
*       R11,                                                       0281
*       R12,                                                       0281
*       R14,                                                       0281
*       R15) RESTRICTED;                                           0281
*     RESPECIFY                                                    0282
*       ASMPOOLS BASED(R11);                                       0282
*     RESPECIFY                                                    0283
*       GMACELL BASED(R1);                                         0283
*     R11=ADDR(ASMACEPC);                                          0284
         LA    R11,ASMACEPC(,ASMVTPTR)                             0284
*     LM(R14,R15,ASMCPRSQ);         /* GET CONTENTS OF Q             */
         LM    R14,R15,ASMCPRSQ(R11)                               0285
*GMA00031:                                                         0286
*     IF R14^=0 THEN                /* TEST RESERVE Q                */
GMA00031 LTR   R14,R14                                             0286
         BZ    @RF00286                                            0286
*       DO;                         /* REPLENISH ACE RESERVE Q       */
*         R0=R14-1;                 /* SET UP FOR COMPARE AND SWAP   */
         LR    R0,R14                                              0288
         BCTR  R0,0                                                0288
*         GMANEXT=R15;              /* CELL TO FREE IN R1            */
         ST    R15,GMANEXT(,R1)                                    0289
*         CDS(R14,R0,ASMCPRSQ);     /* ADD CELL TO RESERVE Q         */
         CDS   R14,R0,ASMCPRSQ(R11)                                0290
*         BC(7,GMA00031);                                          0291
         BC    7,GMA00031                                          0291
*       END;                                                       0292
*     ELSE                                                         0293
*       DO;                         /* RETURN ACE TO NORMAL Q        */
         B     @RC00286                                            0293
@RF00286 DS    0H                                                  0294
*         R14=ASMCPAVL;                                            0294
         L     R14,ASMCPAVL(,R11)                                  0294
*GMA00032:                                                         0295
*         GMANEXT=R14;              /* PUT ADDR CURRENT FIRST IN   0295
*                                      INPUT CELL                    */
GMA00032 ST    R14,GMANEXT(,R1)                                    0295
*         CS(R14,R1,ASMCPAVL);      /* ADD INPUT CELL TO POOL        */
         CS    R14,@01,ASMCPAVL(R11)                               0296
*         BC(7,GMA00032);                                          0297
         BC    7,GMA00032                                          0297
*       END;                        /* ACE RETURNED TO NORMAL Q      */
*     RESPECIFY                                                    0299
*      (R0,                                                        0299
*       R1,                                                        0299
*       R11,                                                       0299
*       R12,                                                       0299
*       R14,                                                       0299
*       R15) UNRESTRICTED;                                         0299
@RC00286 DS    0H                                                  0300
*   END;                            /* ILRGMA FREE                 0300
*                                      CELL(ACE)WORKREGS(R11,R12)    */
*                                                                  0300
*   /*****************************************************************/
*   /*                                                               */
*   /* END ILRGMA MACRO EXPANSION                                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0301
*   RFY                                                            0301
*     R14 RSTD;                                                    0301
*   R14=R10;                        /* RESTORE RETURN ADDR           */
         LR    R14,R10                                             0302
*   END ACEDEQFR;                                                  0303
@EL00002 DS    0H                                                  0303
@EF00002 DS    0H                                                  0303
@ER00002 BR    @14                                                 0303
