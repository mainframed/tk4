         TITLE 'ILRSRBC - SRB CONTROLLER                               *
                        '
ILRSRBC  CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL2(@EP00001-@MAINENT)
         DC    AL1(16)                                             0001
         DC    C'ILRSRBC   76.114'                                 0001
ILRSRBRM DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL2(@EP00241-ILRSRBRM)
         ENTRY ILRSRBRM
         DROP  @15
@PROLOG  BALR  @06,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@06                                         0001
         AH    @15,4(,@15)                                         0001
         BR    @15                                                 0001
@EP00001 DS    0H                                                  0002
*   SRBPTR=R0;                      /* OBTAIN ADDRESSIBILITY TO SRB  */
         LR    SRBPTR,R0                                           0050
*   RFY                                                            0051
*     R0 UNRSTD;                                                   0051
*   R1=ADDR(ILRSRB01);              /* OBTAIN FUNCTIONAL RECOVERY  0052
*                                      ROUTINE ADDR                  */
         L     R1,@CV01751                                         0052
*   GEN REFS(R1) SETS(ATAPTR,R11,R12);                             0053
     SETFRR  A,FRRAD=(R1),PARMAD=(ATAPTR),WRKREGS=(R11,R12),          **
               RELATED=(SETFRR,ILRSRBC)  ADD RECOVERY
*   RSMHDPTR=SRBASCB->ASCBRSM;      /* OBTAIN ADDRESS RSM HEADER     */
         L     @12,SRBASCB(,SRBPTR)                                0054
         L     RSMHDPTR,ASCBRSM(,@12)                              0054
*   ASMVTPTR=CVTASMVT;              /* OBTAIN ADDRESS ASMVT          */
*                                                                  0055
         L     @12,CVTPTR                                          0055
         L     ASMVTPTR,CVTASMVT(,@12)                             0055
*   /*****************************************************************/
*   /*                                                               */
*   /* BEGIN ILRGMA MACRO EXPANSION                                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0056
*   DO;                             /* ILRGMA GET                  0056
*                                      CELL(BWK)WORKREGS(R11,R12)    */
*     DECLARE                       /* ASMVT VECTOR TO ILRPEX        */
*       ILRPEX ENTRY BASED(ASMPEX) VALRG(0);                       0057
*     DECLARE                       /* MAP FOR CELL CONTROL WORDS    */
*       1 GMACELL BASED BDY(WORD),                                 0058
*        2 GMANEXT PTR(31);         /* PTR NEXT FREE CELL            */
*     RESPECIFY                                                    0059
*      (R0,                                                        0059
*       R1,                                                        0059
*       R11,                                                       0059
*       R12,                                                       0059
*       R14,                                                       0059
*       R15) RESTRICTED;                                           0059
*     RESPECIFY                                                    0060
*       ASMPOOLS BASED(R11);                                       0060
*     RESPECIFY                                                    0061
*       GMACELL BASED(R14);                                        0061
*     R11=ADDR(ASMBWKPC);                                          0062
         LA    R11,ASMBWKPC(,ASMVTPTR)                             0062
*     LM(R14,R15,ASMCPAVQ);         /* GET ADDR OF 1ST CELL          */
         LM    R14,R15,ASMCPAVQ(R11)                               0063
*GMA00010:                                                         0064
*     IF R14^=0 THEN                /* TEST CELL POOL                */
GMA00010 LTR   R14,R14                                             0064
         BZ    @RF00064                                            0064
*       DO;                         /* CELL AVAILABLE, REMOVE CELL   */
*         R0=GMANEXT;                                              0066
         L     R0,GMANEXT(,R14)                                    0066
*         R1=R15-1;                                                0067
         LR    R1,R15                                              0067
         BCTR  R1,0                                                0067
*         CDS(R14,R0,ASMCPAVQ);                                    0068
         CDS   R14,R0,ASMCPAVQ(R11)                                0068
*         BC(7,GMA00010);                                          0069
         BC    7,GMA00010                                          0069
*         R1=R14;                                                  0070
         LR    R1,R14                                              0070
*       END;                                                       0071
*     ELSE                                                         0072
*       DO;                         /* CELL POOL EMPTY               */
         B     @RC00064                                            0072
@RF00064 DS    0H                                                  0073
*         R1=R11;                   /* PREPARE TO CALL PEX           */
         LR    R1,R11                                              0073
*         CALL ILRPEX;              /* EXTEND THE POOL               */
         L     @15,ASMPEX(,ASMVTPTR)                               0074
         BALR  @14,@15                                             0074
*       END;                                                       0075
*     RESPECIFY                                                    0076
*      (R0,                                                        0076
*       R1,                                                        0076
*       R11,                                                       0076
*       R12,                                                       0076
*       R14,                                                       0076
*       R15) UNRESTRICTED;                                         0076
@RC00064 DS    0H                                                  0077
*   END;                            /* ILRGMA GET                  0077
*                                      CELL(BWK)WORKREGS(R11,R12)    */
*                                                                  0077
*   /*****************************************************************/
*   /*                                                               */
*   /* END ILRGMA MACRO EXPANSION                                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0078
*   RFY                                                            0078
*     R1 RSTD;                                                     0078
*   IF R1=ZERO THEN                                                0079
         LTR   R1,R1                                               0079
         BNZ   @RF00079                                            0079
*     DO;                           /* GMA UNSUCCESSFUL, NOT ENOUGH
*                                      STORAGE                       */
*       GEN REFS(SRBPTR);                                          0081
*                 /* RESCHEDULE SRB CONTROLLER */
     SCHEDULE SRB=(SRBPTR),SCOPE=LOCAL  REDRIVE SRB CONTROLLER
*     END;                          /* END GMA UNSUCCESSFUL          */
*                                                                  0083
*   /*****************************************************************/
*   /*                                                               */
*   /* ILRGMA SUCCESSFUL, INITIALIZE EPATH AND ATA, AND GET THE ASM  */
*   /* CLASS LOCK                                                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0083
*   ELSE                                                           0083
*     DO;                           /* GMA SUCCESSFUL, WORK AREA   0083
*                                      OBTAINED                      */
         B     @RC00079                                            0083
@RF00079 DS    0H                                                  0084
*       WKSAV@=R1;                  /* OBTAIN ADDRESSIBILITY TO WORK
*                                      AREA                          */
         LR    WKSAV@,R1                                           0084
*       GEN(USING      @DATD,WKSAV@);/* INDICATE MEANS TO ADDRESS  0085
*                                      WORK AREA                     */
         USING      @DATD,WKSAV@
*       EPATH=''B;                  /* CLEAR THE EPATH               */
         XC    EPATH(52),EPATH                                     0086
*       ATAEPATH=ADDR(EPATH);       /* RRR-SET EPATH ADDR IN ATA-RRR */
         LA    @12,EPATH                                           0087
         ST    @12,ATAEPATH(,ATAPTR)                               0087
*       EPASWKA=WKSAV@;             /* RRR-CHECKPOINT WORK AREA IN 0088
*                                      EPATH-RRR                     */
         ST    WKSAV@,EPASWKA                                      0088
*       RFY                                                        0089
*         R1 UNRSTD;                                               0089
*       RFY                                                        0090
*         R11 RSTD;                                                0090
*       R11=ADDR(ASHLOCK);          /* OBTAIN ADDR ASM LOCK WORD     */
         LA    R11,ASHLOCK(,RSMHDPTR)                              0091
*       GEN REFS(R11);                                             0092
GETASM  SETLOCK  OBTAIN,TYPE=ASM,ADDR=(11),MODE=UNCOND,               **
               RELATED=(VIO,ILRSRBC(FREASM))
*       RFY                                                        0093
*         R11 UNRSTD;                                              0093
*       ATAMODID=ATAMSRBC;          /* RRR-SET MOD ID IN ATA-RRR     */
         MVI   ATAMODID(ATAPTR),X'08'                              0094
*       ASHSCHED=OFF;               /* RRR-RESET SRB SCHEDULED     0095
*                                      FLAG-RRR                      */
         NI    ASHSCHED(RSMHDPTR),B'01111111'                      0095
*                                                                  0096
*       /*************************************************************/
*       /*                                                           */
*       /* LOOP THRU ALL LGES SEARCHING FOR WORK PENDING WHICH CAN BE*/
*       /* STARTED                                                   */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0096
*       LGEPTR=ASHLGEQ;             /* OBTAIN ADDR FIRST LGE ON ASMHD
*                                      LGE QUEUE                     */
         L     LGEPTR,ASHLGEQ(,RSMHDPTR)                           0096
*       DO WHILE LGEPTR^=ZERO;      /* LOOP THRU ALL LGES            */
         B     @DE00097                                            0097
@DL00097 DS    0H                                                  0098
*         IF LGEWRKPD=ON&LGEGRINP=OFF THEN                         0098
         TM    LGEWRKPD(LGEPTR),B'10000000'                        0098
         BNO   @RF00098                                            0098
         TM    LGEGRINP(LGEPTR),B'01000000'                        0098
         BNZ   @RF00098                                            0098
*           DO;                     /* WORK PENDING AND NO GROUP   0099
*                                      OPERATION IN PROCESS          */
*             LGEWRKPD=OFF;         /* RESET WORK PENDING FLAG       */
*                                                                  0100
         NI    LGEWRKPD(LGEPTR),B'01111111'                        0100
*             /*******************************************************/
*             /*                                                     */
*             /* LOOP THRU ALL ELEMENTS ON THE LGE PROCESS QUEUE,    */
*             /* UNTIL A GROUP-OP ACE IS FOUND OR ALL ELEMENTS HAVE  */
*             /* BEEN PROCESSED. AIAS ARE QUEUED TO THE ATAAIAQ AND  */
*             /* ILRTRANS IS CALLED FOR TRANSFER PAGE ACES. AIA AND  */
*             /* ACE ARE BASED ON THE SAME REGISTER (R7).            */
*             /*                                                     */
*             /*******************************************************/
*                                                                  0101
*             AIAPTR=LGEPROCF;      /* OBTAIN ADDR FIRST AIA OR ACE
*                                      ON PROCESS Q                  */
         L     AIAPTR,LGEPROCF(,LGEPTR)                            0101
*             DO WHILE AIAPTR^=ZERO&ACEGRPRQ=OFF;/* LOOP THRU PROCESS
*                                      QUEUE UNTIL A GROUP-OP ACE IS
*                                      FOUND OR NO ELEMENTS ARE LEFT */
*                                                                  0102
         B     @DE00102                                            0102
@DL00102 DS    0H                                                  0103
*               /*****************************************************/
*               /*                                                   */
*               /* PROCESSING INSIDE THIS LOOP IS FOR AIAS OR        */
*               /* TRANSFER PAGE ACES ONLY                           */
*               /*                                                   */
*               /*****************************************************/
*                                                                  0103
*               NXTAIACE=AIAFQPA;   /* OBTAIN ADDR NEXT ELEMENT ON Q */
         L     NXTAIACE,AIAFQPA(,AIAPTR)                           0103
*               IF AIAPRINO=OFF THEN                               0104
         TM    AIAPRINO(AIAPTR),B'01000000'                        0104
         BNZ   @RF00104                                            0104
*                 DO;               /* PROCESS NOT IN OPERATION FOR Q
*                                      ELEMENT                       */
*                   AIAPRINO=ON;    /* SET PROCESS IN OPERATION FLAG */
         OI    AIAPRINO(AIAPTR),B'01000000'                        0106
*                   IF AIALPMEC=ON THEN/* LPID CONVERTED TO LPME ADDR*/
         TM    AIALPMEC(AIAPTR),B'00001000'                        0107
         BNO   @RF00107                                            0107
*                     IF AIALPMEP->ASPLOPIN=ON THEN/* PROCESS IN   0108
*                                      OPERATION FOR LPME            */
         L     @12,AIALPMEP(,AIAPTR)                               0108
         TM    ASPLOPIN(@12),B'00100000'                           0108
         BNO   @RF00108                                            0108
*                       IF ACEOP=ACETRPAG&ACEOVRID=ON THEN         0109
         CLI   ACEOP(ACEPTR),4                                     0109
         BNE   @RF00109                                            0109
         TM    ACEOVRID(ACEPTR),B'00100000'                        0109
         BNO   @RF00109                                            0109
*                         DO;       /* TRANSFER PAGE ACE AND TRANSFER
*                                      PAGE OVERIDE FLAG IS ON       */
*                           RFY                                    0111
*                            (R13,                                 0111
*                             R0,                                  0111
*                             R1) RSTD;                            0111
*                           R13=ADDR(SAVAREA);/* SET SAVE AREA     0112
*                                      ADDRESS                       */
         LA    R13,SAVAREA                                         0112
*                           R0=ADDR(SAVAREA1);/* SAVE AREA FOR FREE
*                                      SLOT PASSED TO ILRTRANS       */
         LA    R0,SAVAREA1                                         0113
*                           R1=ACEPTR;/* SET ADDR TRANSFER PAGE ACE
*                                      TO BE PROCESSED               */
         LR    R1,ACEPTR                                           0114
*                           CALL ILRTRANS;/* PERFORMS THE TRANSFER 0115
*                                      PAGE OPERATION                */
         L     @15,@CV01752                                        0115
         BALR  @14,@15                                             0115
*                           RFY                                    0116
*                            (R13,                                 0116
*                             R0,                                  0116
*                             R1) UNRSTD;                          0116
*                           CALL ACEDEQFR;/* DEQUEUES AND FREES THE
*                                      ACE                           */
         BAL   @14,ACEDEQFR                                        0117
*                         END;      /* TRANSFER PAGE ACE, OVERRIDE ON*/
*                       ELSE                                       0119
*                         DO;                                      0119
         B     @RC00109                                            0119
@RF00109 DS    0H                                                  0120
*                           AIAPRINO=OFF;/* RESET PROCESS IN       0120
*                                      OPERATION FOR AIA FLAG        */
         NI    AIAPRINO(AIAPTR),B'10111111'                        0120
*                           LGEWRKPD=ON;/* SET WORK PENDING FLAG FOR
*                                      LGE                           */
         OI    LGEWRKPD(LGEPTR),B'10000000'                        0121
*                         END;                                     0122
*                                                                  0123
*                   /*************************************************/
*                   /*                                               */
*                   /* PROCESS NOT IN OPERATION FOR LPME, CALL       */
*                   /* ILRTRANS ON TRANSFER PAGE REQUESTS, QUEUE AIAS*/
*                   /* FOR ILRESTRT ON PAGING REQUESTS               */
*                   /*                                               */
*                   /*************************************************/
*                                                                  0123
*                     ELSE          /* PROCESS NOT IN OPERATION FOR
*                                      LPME                          */
*                       IF ACEOP=ACETRPAG THEN/* TRANSFER PAGE ACE   */
         B     @RC00108                                            0123
@RF00108 CLI   ACEOP(ACEPTR),4                                     0123
         BNE   @RF00123                                            0123
*                         IF ACETRPWT=ON THEN                      0124
         TM    ACETRPWT(ACEPTR),B'01000000'                        0124
         BNO   @RF00124                                            0124
*                           DO;     /* TRANSFER PAGE REQUEST WAITING
*                                      FOR I/O TO COMPLETE           */
*                             ACETLPME->ASPLOPIN=ON;/* SET PROCESS IN
*                                      OPERATION FLAG                */
         L     @12,ACETLPME(,ACEPTR)                               0126
         OI    ASPLOPIN(@12),B'00100000'                           0126
*                             ACEPRINO=OFF;/* RESET PROCESS IN     0127
*                                      OPERATION FOR ACE FLAG        */
         NI    ACEPRINO(ACEPTR),B'10111111'                        0127
*                             LGEWRKPD=ON;/* SET WORK PENDING FLAG 0128
*                                      FOR LGE                       */
         OI    LGEWRKPD(LGEPTR),B'10000000'                        0128
*                           END;    /* TRANSFER PAGE WAITING         */
*                         ELSE                                     0130
*                           DO;     /* TRANSFER PAGE NOT WAITING,  0130
*                                      CALL ILRTRANS                 */
         B     @RC00124                                            0130
@RF00124 DS    0H                                                  0131
*                             RFY                                  0131
*                              (R1,                                0131
*                               R0,                                0131
*                               R13) RSTD;                         0131
*                             R13=ADDR(SAVAREA);/* SET SAVE AREA   0132
*                                      ADDRESS                       */
         LA    R13,SAVAREA                                         0132
*                             R0=ADDR(SAVAREA1);/* SAVE AREA FOR FREE
*                                      SLOT PASSED TO ILRTRANS       */
         LA    R0,SAVAREA1                                         0133
*                             R1=ACEPTR;/* SET ADDR TRANSFER PAGE ACE
*                                      TO BE PROCESSED               */
         LR    R1,ACEPTR                                           0134
*                             CALL ILRTRANS;/* PERFORMS THE TRANSFER
*                                      PAGE REQUEST                  */
         L     @15,@CV01752                                        0135
         BALR  @14,@15                                             0135
*                             RFY                                  0136
*                              (R1,                                0136
*                               R0,                                0136
*                               R13) UNRSTD;                       0136
*                             CALL ACEDEQFR;/* DEQUEUES AND FREES THE
*                                      ACE                           */
         BAL   @14,ACEDEQFR                                        0137
*                           END;    /* TRANSFER PAGE NOT WAITING     */
*                       ELSE                                       0139
*                         DO;       /* PAGING AIA, QUEUE AIA FOR   0139
*                                      ILRESTRT ROUTINE              */
         B     @RC00123                                            0139
@RF00123 DS    0H                                                  0140
*                           AIALPMEP->ASPLOPIN=ON;/* SET PROCESS IN
*                                      OPERATION FLAG                */
         L     @12,AIALPMEP(,AIAPTR)                               0140
         OI    ASPLOPIN(@12),B'00100000'                           0140
*                           AIANXAIA=ATAAIAQ;/* Q ATAAIAQ TO AIA,  0141
*                                      LIFO Q                        */
         L     @12,ATAAIAQ(,ATAPTR)                                0141
         ST    @12,AIANXAIA(,AIAPTR)                               0141
*                           ATAAIAQ=AIAPTR;/* RRR-MAKE THE AIA THE 0142
*                                      FIRST ELEMENT ON THE Q-RRR    */
         ST    AIAPTR,ATAAIAQ(,ATAPTR)                             0142
*                         END;      /* PAGING AIA                    */
*                                                                  0144
*                   /*************************************************/
*                   /*                                               */
*                   /* AN IMPOSSIBLE SITUATION EXISTS, THE LPID      */
*                   /* CONVERTED FLAG (AIALPMEC) IS OFF, RECORD THE  */
*                   /* ERROR, FREE THE ACE, OR Q THE AIA TO THE PART */
*                   /* AIA ERROR QUEUE.                              */
*                   /*                                               */
*                   /*************************************************/
*                                                                  0144
*                   ELSE                                           0144
*                     DO;           /* LPID NOT CONVERTED            */
         B     @RC00107                                            0144
@RF00107 DS    0H                                                  0145
*                       ATARCODE=REACOD20;/* SET REASON CODE FOR C0D
*                                      ABEND                         */
         MVI   ATARCODE(ATAPTR),X'14'                              0145
*                       GEN(ABEND      X'C0D',,,SYSTEM);/* ISSUE C0D
*                                      RECORD ONLY ABEND             */
         ABEND      X'C0D',,,SYSTEM
*                       IF ACEOP=ACETRPAG THEN                     0147
         CLI   ACEOP(ACEPTR),4                                     0147
         BNE   @RF00147                                            0147
*                         CALL ACEDEQFR;/* DEQUEUE AND FREE THE ACE
*                                      ON A TRANSFER PAGE REQUEST    */
         BAL   @14,ACEDEQFR                                        0148
*                       ELSE                                       0149
*                         DO;       /* PAGING AIA REQUEST, Q THE AIA
*                                      TO THE PART ERROR QUEUE       */
         B     @RC00147                                            0149
@RF00147 DS    0H                                                  0150
*                           AIAERROR=ON;/* SET LOGICAL ERROR FLAG    */
         OI    AIAERROR(AIAPTR),B'00000010'                        0150
*                           ASHIOCNT=ASHIOCNT+ONE;/* INCREMENT I/O 0151
*                                      REQUEST STARTED COUNT         */
         LA    @12,1                                               0151
         AL    @12,ASHIOCNT(,RSMHDPTR)                             0151
         ST    @12,ASHIOCNT(,RSMHDPTR)                             0151
*                           RFY                                    0152
*                            (PARTPTR,                             0152
*                             R0) RSTD;                            0152
*                           PARTPTR=ASMPART;/* OBTAIN ADDRESSIBILITY
*                                      TO PART                       */
         L     PARTPTR,ASMPART(,ASMVTPTR)                          0153
*                           R0=PARTAIAE;/* OBTAIN PART AIA ERROR Q,
*                                      LIFO Q                        */
         L     R0,PARTAIAE(,PARTPTR)                               0154
*TRYAGAIN:                                                         0155
*                           AIANXAIA=R0;/* CHAIN ERROR Q TO AIA      */
TRYAGAIN ST    R0,AIANXAIA(,AIAPTR)                                0155
*                           CS(R0,AIAPTR,PARTAIAE);/* MAKE AIA FIRST
*                                      ELEMENT ON Q                  */
         CS    R0,@07,PARTAIAE(PARTPTR)                            0156
*                           BC(NE,TRYAGAIN);/* NOT SUCCESSFUL, TRY 0157
*                                      AGAIN                         */
         BC    7,TRYAGAIN                                          0157
*                           RFY                                    0158
*                            (PARTPTR,                             0158
*                             R0) UNRSTD;                          0158
*                         END;      /* PAGING AIA ERROR              */
*                     END;          /* END LPID NOT CONVERTED        */
@RC00147 DS    0H                                                  0161
*                 END;              /* PROCESS NOT IN OPERATION FOR Q
*                                      ELEMENT                       */
@RC00107 DS    0H                                                  0162
*               AIAPTR=NXTAIACE;    /* OBTAIN ADDR NEXT AIA OR ACE   */
@RF00104 LR    AIAPTR,NXTAIACE                                     0162
*             END;                  /* LOOP THRU PROCESS Q ELEMENTS  */
@DE00102 LTR   AIAPTR,AIAPTR                                       0163
         BZ    @DC00102                                            0163
         TM    ACEGRPRQ(ACEPTR),B'10000000'                        0163
         BZ    @DL00102                                            0163
@DC00102 DS    0H                                                  0164
*                                                                  0164
*             /*******************************************************/
*             /*                                                     */
*             /* IF GROUP-OP ACE WAS FOUND TO BE FIRST ON THE LGE    */
*             /* PROCESS QUEUE THEN QUEUE IT TO ATAACEQ. WHETHER     */
*             /* FOUND OR NOT CONTINUE TO LOOP THRU THE LGES.        */
*             /*                                                     */
*             /*******************************************************/
*                                                                  0164
*             IF ACEPTR^=ZERO THEN  /* GROUP-OP ACE                  */
         SLR   @12,@12                                             0164
         CR    ACEPTR,@12                                          0164
         BE    @RF00164                                            0164
*               IF ACEPTR=LGEPROCF THEN/* GROUP-OP ACE IS FIRST ON 0165
*                                      PROCESS Q                     */
         C     ACEPTR,LGEPROCF(,LGEPTR)                            0165
         BNE   @RF00165                                            0165
*                 DO;               /* QUEUE THE GROUP-OP ACE, LIFO
*                                      QUEUE                         */
*                   LGEGRINP=ON;    /* SET GROUP-OP IN PROCESS FLAG  */
         OI    LGEGRINP(LGEPTR),B'01000000'                        0167
*                   ACESRBWK=ATAACEQ;/* QUEUE ATAACEQ TO ACE         */
         L     @15,ATAACEQ(,ATAPTR)                                0168
         ST    @15,ACESRBWK(,ACEPTR)                               0168
*                   ATAACEQ=ACEPTR; /* RRR-MAKE ACE FIRST ELEMENT ON
*                                      QUEUE-RRR                     */
         ST    ACEPTR,ATAACEQ(,ATAPTR)                             0169
*                   IF ACEFQPA^=ZERO THEN                          0170
         C     @12,ACEFQPA(,ACEPTR)                                0170
         BE    @RF00170                                            0170
*                     LGEWRKPD=ON;  /* MORE ELEMENTS ARE ON THE LGE
*                                      PROCESS QUEUE, SET THE WORK 0171
*                                      PENDING FLAG                  */
         OI    LGEWRKPD(LGEPTR),B'10000000'                        0171
*                 END;              /* QUEUE GROUP-OP ACE            */
*               ELSE                                               0173
*                 LGEWRKPD=ON;      /* SET WORK PENDING FLAG IN LGE  */
         B     @RC00165                                            0173
@RF00165 OI    LGEWRKPD(LGEPTR),B'10000000'                        0173
*           END;                    /* WORK PENDING AND NO GROUP-OP
*                                      IN PROCESS                    */
@RC00165 DS    0H                                                  0174
@RF00164 DS    0H                                                  0175
*         LGEPTR=LGENEXT;           /* OBTAIN ADDR NEXT LGE          */
@RF00098 L     LGEPTR,LGENEXT(,LGEPTR)                             0175
*       END;                        /* END LOOP THRU ALL LGES        */
@DE00097 LTR   LGEPTR,LGEPTR                                       0176
         BNZ   @DL00097                                            0176
*                                                                  0177
*       /*************************************************************/
*       /*                                                           */
*       /* RELEASE THE ASM LOCK, CALL ILRESTRT IF NECESSARY AND PASS */
*       /* THE QUEUE OF PAGING AIAS, CALL ILRRLG FOR RELEASE AND     */
*       /* DEACTIVATE ACES AND POST THE ACE ECB FOR OTHER GROUP-OP   */
*       /* ACES IF THERE ARE ANY ON THE ACE QUEUE.                   */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0177
*       RFY                                                        0177
*         R11 RSTD;                                                0177
*       R11=ADDR(ASHLOCK);          /* OBTAIN ADDR ASM LOCK WORD     */
         LA    R11,ASHLOCK(,RSMHDPTR)                              0178
*       GEN REFS(R11);                                             0179
FREASM  SETLOCK  RELEASE,TYPE=ASM,ADDR=(11),                          **
               RELATED=(VIO,ILRSRBC(GETASM))
*       RFY                                                        0180
*         R11 UNRSTD;                                              0180
*       IF ATAAIAQ^=ZERO THEN                                      0181
         L     @12,ATAAIAQ(,ATAPTR)                                0181
         LTR   @12,@12                                             0181
         BZ    @RF00181                                            0181
*         DO;                       /* PAGING AIAS FOUND             */
*           RFY                                                    0183
*            (R1,                                                  0183
*             R13) RSTD;                                           0183
*           R13=ADDR(SAVAREA);      /* OBTAIN SAVE AREA FOR RESTART  */
         LA    R13,SAVAREA                                         0184
*           R1=ATAAIAQ;             /* OBTAIN ADDR AIA QUEUE FOR   0185
*                                      RESTART                       */
         LR    R1,@12                                              0185
*           CALL ILRESTRT;          /* ROUTINE RESTARTS PROCESSING 0186
*                                      FOR THE QUEUE OF STARTABLE  0186
