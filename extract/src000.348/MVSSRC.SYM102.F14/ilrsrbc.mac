*/********************************************************************/
*/*                                                                  */
*/* ILRSRBRM IS A RESOURCE MANAGER TERMINATION ROUTINE INVOKED BY    */
*/* PURGE DEQUEUE. IF THE RSM HEADER EXISTS THE ASM HEADER SRB       */
*/* SCHEDULED FLAG IS RESET, OTHERWISE, THE SRB IS FREED.            */
*/*                                                                  */
*/* INPUT: R1 CONTAINS THE SRB ADDRESS                               */
*/*        R14 CONTAINS THE RETURN ADDRESS                           */
*/*                                                                  */
*/* OUTPUT: ASHSCHED RESET OR SRB FREED                              */
*/*                                                                  */
*/********************************************************************/
*/********************************************************************/
*/*SRBRM:   CHART (AREF)                                             */
*/*         HEADER                                                 0242
*/*         ILRSRBC                                                0242
*/* ILRSRBRM - SRBC RESOURCE MANAGER                                 */
*/*ILRSRBRM: E ENTRY                                                 */
*/*         P SAVE RETURN ADDR                                       */
*/*         P MAINTAIN SRBPTR (SRBPTR=R1)                            */
*/*         P OBTAIN ASCBPTR, ASCBPTR = SRBASCB                      */
*/*         P OBTAIN ADDRESSIBILITY TO ASMH BY RSMHDPTR = ASCBRSM    */
*/*         D (Y,,N,%SRBC26) RSMHDPTR = 0, NO RSMHD                  */
*/*         L (,%SRBC27) FREEMAIN: FREE SQA SPACE CONTAINING THE SRB */
*/*%SRBC26: P RESET SRB SCHEDULED FLAG (ASHSCHED)                    */
*/*%SRBC27: P RESTORE RETURN ADDR                                    */
*/*         R RETURN                                                 */
*/*SRBRM:   END                                                      */
*/********************************************************************/
*                                                                  0242
*   RFY                                                            0242
*    (R1,                                                          0242
*     R10,                                                         0242
*     R11,                                                         0242
*     R14,                                                         0242
*     R15) RSTD;                                                   0242
*   R10=R14;                        /* MAINTAIN RETURN ADDR          */
         LR    R10,R14                                             0243
*   SRBPTR=R1;                      /* OBTAIN ADDRESSIBILITY TO THE
*                                      SRB                           */
         LR    SRBPTR,R1                                           0244
*   IF SRBASCB->ASCBRSM=ZERO THEN                                  0245
         L     @12,SRBASCB(,SRBPTR)                                0245
         L     @12,ASCBRSM(,@12)                                   0245
         LTR   @12,@12                                             0245
         BNZ   @RF00245                                            0245
*     DO;                           /* RSM HEADER DOES NOT EXIST,  0246
*                                      FREE THE SRB                  */
*       R11=LENGTH(SRB);            /* OBTAIN LENGTH OF THE SRB      */
         LA    R11,44                                              0247
*       GEN REFS(R11,SRBPTR);                                      0248
*                     /* FREE THE SQA SPACE CONTAINING THE SRB */
         FREEMAIN  RC,SP=245,A=(SRBPTR),LV=(R11)
*     END;                          /* RSM HEADER DOES NOT EXIST     */
*   ELSE                                                           0250
*     SRBASCB->ASCBRSM->ASHSCHED=OFF;/* RESET SRB SCHEDULED FLAG     */
         B     @RC00245                                            0250
@RF00245 L     @12,SRBASCB(,SRBPTR)                                0250
         L     @12,ASCBRSM(,@12)                                   0250
         NI    ASHSCHED(@12),B'01111111'                           0250
*   R15=ZERO;                       /* ZERO RETURN CODE              */
@RC00245 SLR   R15,R15                                             0251
*   R14=R10;                        /* RESTORE RETURN ADDR           */
         LR    R14,R10                                             0252
*   RFY                                                            0253
*    (R1,                                                          0253
*     R10,                                                         0253
*     R11,                                                         0253
*     R15) UNRSTD;                                                 0253
*SRBRMEND:                                                         0254
*   GEN(BR     R14      RETURN TO CALLER);/* RETURN                  */
SRBRMEND BR     R14      RETURN TO CALLER
*ACEDEQFR:                                                         0255
