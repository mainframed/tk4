*/********************************************************************/
*/*                                                                  */
*/* VMTVER:    SUBSECTION TO VERIFY A DATA SPACE AS BEING MOUNTED    */
*/*            AND ON-LINE.                                          */
*/*                                                                  */
*/* INPUT:     DATA SET NAME                                         */
*/*            VOLSER                                                */
*/*                                                                  */
*/* PROCESS:   AN ATTEMPT IS MADE TO MOUNT AND VERIFY THE DATA SET   */
*/*            VIA THE NIPMOUNT MACRO PROVIDED BY IEAVMNIP. THE      */
*/*            PARAMETER LIST TO NIPMOUNT CONSISTS OF A POINTER TO   */
*/*            THE DATA SET NAME AND A VOLSER.                       */
*/*                                                                  */
*/*            ON RETURN FROM NIPMOUNT REGISTER 1 WILL CONTAIN       */
*/*            EITHER A UCB POINTER OR ZERO(UNSUCCESSFUL).           */
*/*                                                                  */
*/* OUTPUT:    SUCCESSFUL - UCB POINTER INITIALIZED                  */
*/*            UCB MARKED  PERMANENTLY RESIDENT, ALLOCATED, AND      */
*/*            CONTAINING A DATA SET                                 */
*/*                                                                  */
*/********************************************************************/
*/*VMTVER:   CHART                                                   */
*/*VMTVER:   N   ATTEMPT TO MOUNT A PAGE/SWAP SPACE  VOLUME          */
*/*          P    ZERO PARAMETER AREA                                */
*/*          P    MOVE VOLSER TO PARAMETER LIST                      */
*/*          P    MOVE POINTER TO DS NAME TO PARAMETER LIST          */
*/*          L    IEAVMNIP:  MOUNT PAGE OR SWAP SPACE                */
*/*          D    (YES,,NO,%VMN20) NIPMOUNT SUCCESSFUL ?             */
*/*          P    SAVE UCB POINTER                                   */
*/*          R    END OF VOLUME MOUNT SECTION                        */
*/*%VMN20:   R     ERROR EXIT - R15 = ERROR CODE( 8)                 */
*/*VMTVER:   END                                                     */
*                                                                  0318
*   VOLSERO=VOLSER;                 /* SET VOLUME SERIAL NUMBER IN 0318
*                                      OUTPUT PARM                   */
@RC00276 L     @15,FPL4DPTR                                        0318
         MVC   VOLSERO(6,PARMPTR),VOLSER(@15)                      0318
*   IF AFTERNIP=OFF THEN            /* PROCESSING AT NIP TIME?       */
         TM    AFTERNIP(PARMPTR),B'01000000'                       0319
         BNZ   @RF00319                                            0319
*     DO;                           /* PROCESSING MOUNT IN NIP TIME  */
*       MNTPARM=MNTPARM&&MNTPARM;   /* CLEAR PARAMETER LIST          */
         XC    MNTPARM(24),MNTPARM                                 0321
*       RFY                                                        0322
*         NIPMNTPL BASED(ADDR(MNTPARM));/* ADDR FOR PARM LIST        */
*       NMNTVS=VOLSER;              /* MOVE VOLSER TO PARM LIST      */
         LA    @14,MNTPARM                                         0323
         MVC   NMNTVS(6,@14),VOLSER(@15)                           0323
*       WKDSNAME=SVDSNAME;          /* GET PAGE OR SWAP DSNAME       */
         MVC   WKDSNAME(44),SVDSNAME                               0324
*       NMNTDSA=ADDR(WKDSNAME);     /* DSNAME PTR TO PARM LIST       */
         LA    @15,WKDSNAME                                        0325
         STCM  @15,15,NMNTDSA(@14)                                 0325
*       RFY                                                        0326
*        (R1,                                                      0326
*         R15) RSTD;                                               0326
*       GEN REFS(NVTMOUNT) SETS(R1);                               0327
*                                   /*
        IEAPMNIP TYPE=MOUNT,PARAM=MNTPARM,OPT=(,LC,LI)
*       UCBPTR=R1;                  /* SAVE UCB POINTER              */
         ST    R1,UCBPTR                                           0328
*       RFY                                                        0329
*        (R1,                                                      0329
*         R15) UNRSTD;              /* RELEASE REG 1,REG15           */
*       IF UCBPTR=ZERO THEN         /* MOUNT SUCCESSFUL?             */
         L     @15,UCBPTR                                          0330
         LTR   @15,@15                                             0330
         BNZ   @RF00330                                            0330
*         DO;                                                      0331
*           RTCODE=NOVOLMNT;        /* SET VOLUME CANNOT BE MOUNTED  */
         MVC   RTCODE(4),@CF00053                                  0332
*           GOTO OPSEXIT;           /* ERROR EXIT                    */
         B     OPSEXIT                                             0333
*         END;                                                     0334
*       ELSE                                                       0335
*         OUUCBPR=UCBPTR;           /* SET UCB PTR IN OUTPUT PARM    */
@RF00330 L     @15,UCBPTR                                          0335
         ST    @15,OUUCBPR(,PARMPTR)                               0335
*     END;                          /* END NIPMOUNT                  */
*   ELSE                            /* AFTER NIP TIME                */
