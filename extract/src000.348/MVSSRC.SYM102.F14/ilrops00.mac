*/********************************************************************/
*/*                                                                  */
*/* GETCORE:   SUBSECTION TO ACQUIRE STORAGE IN SQA AND IN THE       */
*/*            NUCLEUS BUFFER AREA FOR I/O CONTROL BLOCKS.           */
*/*                                                                  */
*/* INPUT:     DEVICE TYPE                                           */
*/*            NUMBER OF EXTENTS                                     */
*/*                                                                  */
*/* PROCESS:   CALCULATE THE AMOUNT OF SQA AND NUCLEUS BUFFER AREA   */
*/*            REQUIRED TO BUILD THE I/O CONTROL BLOCKS.             */
*/*            THE DEVICE TYPE IS USED TO DETERMINE BOTH THE NUMBER  */
*/*            OF IORB-IOSB-SRB STRINGS: 1 FOR DISK OR 4 FOR DRUM,   */
*/*            AND THE NUMBER OF PCCWS REQUIRED:                     */
*/*                 DEVIDE THE DESIRED I/O RESPONSE TIME (BURST TIME */
*/*                 = CONSTANT OF 50000 MICRO SECS.) BY THE TIME TO  */
*/*                 PROCESS I/O FOR ONE SLOT ON THIS DEVICE.         */
*/*            ISSUE A CONDITIONAL GETMAIN FOR THE SQA AND CHECK FOR */
*/*            AVAILABLE NUCLEUS STORAGE. ZERO BOTH AREAS OF STORAGE.*/
*/*                                                                  */
*/* OUTPUT:    SUCCESSFUL - ZEROED AVAILABLE STORAGE                 */
*/*                                                                  */
*/********************************************************************/
*/*GETCORE:  CHART                                                   */
*/*GETCORE:  N    ACQUIRE CORE FOR ASM I/O CONTROL BLOCKS            */
*/*          N    LOOP TO FIND MATCHING DEVICE TYPE                  */
*/*          M    SET DEVICE MATCH NOT FOUND                         */
*/*%GET10:   P    GET A DEVICE TYPE ENTRY FROM THE TABLE             */
*/*          D    (YES,,NO,%GET30) THIS DEVICE MATCH TABLE ENTRY?    */
*/*          D    (YES,%GET35,NO,) DOES THE DEVICE IS 2314 FOR SWAP? */
*/*          P    CALCULATE BURST NUMBER FOR THIS DEVICE             */
*/*          P    INDICATE DEVICE MATCH FOUND                        */
*/*          P    COMPUTE NBR OF IORBS REQUIRED                      */
*/*          D   (YES,%GET20,NO,) DEVICE MATCH FOUND?                */
*/*%GET30:  D   (YES,%GET10,NO,%GET35) MORE ENTRIES IN DEVICE TABLE  */
*/*%GET20:   P    COMPUTE SIZE OF ALL SQA CONTROL BLOCKS             */
*/*          P    CONDITIONAL SQA GETMAIN FOR TOTAL LENGTH           */
*/*          D    (YES,,NO,%GET40) GETMAIN SUCCESSFUL ?              */
*/*          P    SAVE GETMAIN PTR AND LENGTH IN OUTPUT PARM         */
*/*          P    ZERO ACQUIRED SQA STORAGE                          */
*/*          D   (YES,,NO,%GET70) IS DATASET A PAGE SPACE & AT NIP   */
*/*          P    CALCULATE CORE LENGTH NEEDED FOR PCCWS             */
*/*          P    CALCULATE SIZE OF NUCLEUS BUFFER AREA AVAILABLE    */
*/*          D    (YES,,NO,%GET50) ENOUGH CORE FOR PCCWS ?           */
*/*          P    SAVE POINTER TO NUCLEUS BUFFER AREA CORE           */
*/*          P    UPDATE POINTER TO TOP OF NUCLEUS BUFFER AREA       */
*/*          P    ADJUST AMOUNT OF BUFFER SPACE REMAINING            */
*/*          P    (,%GET70) CLEAR NUCLEUS BUFFER AREA                */
*/*%GET35:   R    RETURN: ERROR EXIT - R15 = ERROR CODE(12)          */
*/*%GET40:   R    RETURN: ERROR EXIT - R15 = ERROR CODE(16)          */
*/*%GET50:   R    RETURN: ERROR EXIT - R15 = ERROR CODE(20)          */
*/*%GET70:   R    END OF ACQUIRE STORAGE SECTION                     */
*/*GETCORE:  END                                                     */
*                                                                  0390
*                                                                  0390
*/* GETCORE-ACQUIRE NEEDED STORAGE                                   */
*                                                                  0390
*   IF NOCONBK=ON THEN                                             0390
@RC00319 TM    NOCONBK(PARMPTR),B'00001000'                        0390
         BO    @RT00390                                            0390
*     GOTO OPSEXIT;                 /* SHORT PATH EXIT               */
*   DEVTBPTR=ADDR(DEVIDTB);         /* SET DEVICE ID TABLE PTR       */
         LA    @15,DEVIDTB                                         0392
         LR    DEVTBPTR,@15                                        0392
*   DEVTBEND=ADDR(DEVIDTB)+LENGTH(DEVIDTB);/* SET END OF DEVID TABLE
*                                      ADDRESS                       */
         AL    @15,@CF00368                                        0393
         LR    DEVTBEND,@15                                        0393
*   DEVMATCH=NO;                    /* SET MATCH NOT FOUND FLAG      */
         NI    DEVMATCH,B'01111111'                                0394
*   DO WHILE(DEVMATCH=NO&DEVTBPTR<DEVTBEND);/* SEARCH DEVID TABLE    */
         B     @DE00395                                            0395
@DL00395 DS    0H                                                  0396
*     IF DEVID=DEVTYP(THREE:FOUR) THEN/* DEVICE TYPES MATCH ?        */
         L     @01,FPL3DPTR                                        0396
         CLC   DEVID(2,DEVTBPTR),DEVTYP+2(@01)                     0396
         BNE   @RF00396                                            0396
*       DO;                                                        0397
*         DEVMATCH=YES;             /* DEVICE TYPE FOUND             */
         OI    DEVMATCH,B'10000000'                                0398
*         BURSTNO=ASMBURST/SLOTTIME;/* CALC BURST NBR FOR DEV TYPE   */
         L     @01,ASMVTPTR                                        0399
         L     @00,ASMBURST(,@01)                                  0399
         SRDA  @00,32                                              0399
         LH    @15,SLOTTIME(,DEVTBPTR)                             0399
         DR    @00,@15                                             0399
         STH   @01,BURSTNO                                         0399
*         DVTYPE=DEVID;             /* SAVE DEVICE TYPE FOR CALLER   */
         MVC   DVTYPE(2,PARMPTR),DEVID(DEVTBPTR)                   0400
*         IF DEVID=D2305M1×         /* DEVICE A DRUM ?               */
*             DEVID=D2305M2 THEN                                   0401
         CLC   DEVID(2,DEVTBPTR),D2305M1                           0401
         BE    @RT00401                                            0401
         CLC   DEVID(2,DEVTBPTR),D2305M2                           0401
         BNE   @RF00401                                            0401
@RT00401 DS    0H                                                  0402
*           STRNGNO=DRUMSTRS;       /* SET NBR IORBS FOR DRUM        */
         MVC   STRNGNO(2),@CH00095                                 0402
*         ELSE                                                     0403
*           IF PAGESP=OFF THEN      /* SWAP SPACE?                   */
         B     @RC00401                                            0403
@RF00401 TM    PAGESP(PARMPTR),B'10000000'                         0403
         BNZ   @RF00403                                            0403
*             STRNGNO=DISKSTRS;     /* SET NBR IORBS TO BE CREATED   */
         MVC   STRNGNO(2),@CH00260                                 0404
*           ELSE                    /* NO-PAGE SPACE                 */
*             STRNGNO=ONE;          /* SET NRB IORBS FOR PAGE-DISK   */
         B     @RC00403                                            0405
@RF00403 MVC   STRNGNO(2),@CH00036                                 0405
*       END;                                                       0406
*     ELSE                                                         0407
*       ;                                                          0407
@RF00396 DS    0H                                                  0408
*     DEVTBPTR=DEVTBPTR+LENGTH(DEVENT);/* GET NEXT DEVICE TABLE ENTRY*/
@RC00396 AL    DEVTBPTR,@CF00095                                   0408
*   END;                            /* END OF SEARCH DEVICE TYPE   0409
*                                      TABLE                         */
@DE00395 TM    DEVMATCH,B'10000000'                                0409
         BNZ   @DC00395                                            0409
         CR    DEVTBPTR,DEVTBEND                                   0409
         BL    @DL00395                                            0409
@DC00395 DS    0H                                                  0410
*   IF DEVMATCH=NO×(DEVTYP(THREE:FOUR)=D2314&PAGESP=OFF) THEN/* VALID
*                                      DEVICE TYPE FOUND?            */
         TM    DEVMATCH,B'10000000'                                0410
         BZ    @RT00410                                            0410
         L     @01,FPL3DPTR                                        0410
         CLC   DEVTYP+2(2,@01),D2314                               0410
         BNE   @RF00410                                            0410
         TM    PAGESP(PARMPTR),B'10000000'                         0410
         BNZ   @RF00410                                            0410
@RT00410 DS    0H                                                  0411
*     RTCODE=LOCFAILD;              /* NO-LOCATEFAIL                 */
         MVC   RTCODE(4),@CF00488                                  0411
*   ELSE                            /* DEVICE TYPE FOUND             */
*     DO;                                                          0412
         B     @RC00410                                            0412
@RF00410 DS    0H                                                  0413
*       IOSBLGH=(LENGTH(IOSB)+SEVEN)/EIGHT*EIGHT;/* ROUND TO DBL WORD*/
         LA    @05,112                                             0413
         STC   @05,IOSBLGH                                         0413
*       SRBLGH=(LENGTH(SRB)+SEVEN)/EIGHT*EIGHT;/* ROUND TO DOUBLE  0414
*                                      WORD                          */
         LA    @15,48                                              0414
         STC   @15,SRBLGH                                          0414
*       LPMBLGH=(LENGTH(IDALPMB)+SEVEN)/EIGHT*EIGHT;               0415
         LA    @14,32                                              0415
         STC   @14,LPMBLGH                                         0415
*       TOSIZEDB=((EDBLGH*NOEXTNT+EDBHDLGH)+SEVEN)/EIGHT*EIGHT;    0416
*                                                                  0416
         L     @01,FPL4DPTR                                        0416
         SLR   @07,@07                                             0416
         IC    @07,NOEXTNT(,@01)                                   0416
         SLR   TOSIZEDB,TOSIZEDB                                   0416
         IC    TOSIZEDB,EDBLGH                                     0416
         MR    @06,TOSIZEDB                                        0416
         LR    @00,@07                                             0416
         SLR   TOSIZEDB,TOSIZEDB                                   0416
         IC    TOSIZEDB,EDBHDLGH                                   0416
         ALR   @00,TOSIZEDB                                        0416
         AL    @00,@CF00076                                        0416
         SRDA  @00,32                                              0416
         D     @00,@CF00053                                        0416
         SLA   @01,3                                               0416
         LR    TOSIZEDB,@01                                        0416
*       /*************************************************************/
*       /*                                                           */
*       /* CALCULATE AMOUNT OF SQA REQUIRED FOR I/O CONTROL BLOCKS   */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0417
*       CORESIZE=LPMBLGH+TOSIZEDB+STRNGNO*(IORBLGH+SAVALGH+IOSBLGH+
*           SRBLGH);                /* SQA SIZE NEEDED               */
         ALR   @14,TOSIZEDB                                        0417
         SLR   @01,@01                                             0417
         IC    @01,IORBLGH                                         0417
         SLR   @07,@07                                             0417
         IC    @07,SAVALGH                                         0417
         ALR   @01,@07                                             0417
         ALR   @01,@05                                             0417
         ALR   @01,@15                                             0417
         MH    @01,STRNGNO                                         0417
         ALR   @14,@01                                             0417
         STH   @14,CORESIZE                                        0417
*       DO;                         /* GETMAIN (RC) LV(CORESIZE)   0418
*                                      SP(245)                       */
*         RESPECIFY                                                0419
*          (GPR01F,                                                0419
*           GPR15F,                                                0419
*           GPR00F) RESTRICTED;                                    0419
*         GPR01F=0;                 /* REG 1 MUST BE ZERO            */
         SLR   GPR01F,GPR01F                                       0420
*         GPR15F=0;                 /* RC-TYPE GETMAIN               */
         SLR   GPR15F,GPR15F                                       0421
*         GPR00F=CORESIZE;          /* LENGTH REQUESTED              */
         LR    GPR00F,@14                                          0422
*         GPR15F=GPR15F×((245)*256);/* SP IN BYTE 2                  */
         O     GPR15F,@CF03836                                     0423
*         SVC(120);                 /* RC/RU-FORM OF GETMAIN         */
         SVC   120                                                 0424
*         RESPECIFY                                                0425
*          (GPR01F,                                                0425
*           GPR15F,                                                0425
*           GPR00F) UNRESTRICTED;                                  0425
*       END;                        /* GETMAIN (RC) LV(CORESIZE)   0426
*                                      SP(245)                       */
*                                                                  0426
*       /*************************************************************/
*       /*                                                           */
*       /* TEST FOR SUCCESSFUL GETMAIN                               */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0427
*       RFY                                                        0427
*        (R0,                                                      0427
*         R1,                                                      0427
*         R14,                                                     0427
*         R15) RSTD;                /* RESTRICT REG0,1,14,AND 15     */
*       IF R15=SUCCESS THEN         /* GETMAIN SUCCESSFUL ?          */
         LTR   R15,R15                                             0428
         BNZ   @RF00428                                            0428
*         DO;                       /* YES - GETMAIN SUCCESSFUL      */
*           COREADDR=R1;            /* SAVE PTR TO SQA CORE          */
         LR    COREADDR,R1                                         0430
*           SQAPTR=R1;              /* SAVE PTR TO SQA CORE IN OUTPUT*/
         ST    R1,SQAPTR(,PARMPTR)                                 0431
*           SQALEN=CORESIZE;        /* SAVE SQA CORE LENGTH IN OUTPUT*/
         LH    @07,CORESIZE                                        0432
         ST    @07,SQALEN(,PARMPTR)                                0432
*           R0=COREADDR;            /* SET GETMAIN ADDRESS           */
         LR    R0,COREADDR                                         0433
*           R1=CORESIZE;            /* SET GETMAIN LENGTH            */
         LR    R1,@07                                              0434
*           MVCL(R0,R14);           /* MOVE ZERO TO CLEAR BUFFER AREA*/
         MVCL  R0,R14                                              0435
*         END;                                                     0436
*       ELSE                                                       0437
*         RTCODE=NOVCBSTG;          /* SET NO SPACE IN SQA ERROR     */
         B     @RC00428                                            0437
@RF00428 MVC   RTCODE(4),@CF00107                                  0437
*       RFY                                                        0438
*        (R0,                                                      0438
*         R1,                                                      0438
*         R14,                                                     0438
*         R15) UNRSTD;              /* RELEASE R0,R1,R14,R15         */
*                                                                  0438
@RC00428 DS    0H                                                  0439
*       /*************************************************************/
*       /*                                                           */
*       /* ACQUIRE NUCLEUS BUFFER STORAGE FOR PCCWS                  */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0439
*       IF RTCODE=SUCCESS&AFTERNIP=OFF&PAGESP=ON THEN/* CHECK PAGE 0439
*                                      SPACE AND PROCESS AT NIP TIME */
         L     @15,RTCODE                                          0439
         LTR   @15,@15                                             0439
         BNZ   @RF00439                                            0439
         TM    PAGESP(PARMPTR),B'10000000'                         0439
         BNO   @RF00439                                            0439
         TM    AFTERNIP(PARMPTR),B'01000000'                       0439
         BNZ   @RF00439                                            0439
*         DO;                       /* SET UP PCCWS IN NUCLEUS       */
*           CORESIZE=BURSTNO*PCCWLGH;/* AMOUNT OF CORE REQUIRED      */
         SLR   @15,@15                                             0441
         IC    @15,PCCWLGH                                         0441
         MH    @15,BURSTNO                                         0441
         STH   @15,CORESIZE                                        0441
*           NVTNUCND=(NVTNUCND+SEVEN)/EIGHT*EIGHT;/* ROUND TO DBL  0442
*                                      WORD BDY                      */
         LA    @00,7                                               0442
         AL    @00,NVTNUCND(,NVTPTR)                               0442
         SRDA  @00,32                                              0442
         D     @00,@CF00053                                        0442
         SLA   @01,3                                               0442
         ST    @01,NVTNUCND(,NVTPTR)                               0442
*           IF NVTNBFND-NVTNUCND^<CORESIZE THEN/* ENOUGH NUCLEUS   0443
*                                      BUFFER SPACE LEFT ?           */
         L     @14,NVTNBFND(,NVTPTR)                               0443
         SLR   @14,@01                                             0443
         CR    @14,@15                                             0443
         BL    @RF00443                                            0443
*             DO;                   /* YES - ENOUGH SPACE            */
*               PCCWPTR=NVTNUCND;   /* SAVE CORE ADDRESS             */
         LR    PCCWPTR,@01                                         0445
*               NVTNUCND=NVTNUCND+CORESIZE;/* UPDATE AMOUNT OF CORE  */
         ALR   @01,@15                                             0446
         ST    @01,NVTNUCND(,NVTPTR)                               0446
*               RFY                                                0447
*                (R0,                                              0447
*                 R1,                                              0447
*                 R14,                                             0447
*                 R15) RSTD;        /* RESTRICT REG0,1,14 AND 15     */
*               R0=PCCWPTR;                                        0448
         LR    R0,PCCWPTR                                          0448
*               R1=CORESIZE;                                       0449
         LH    @07,CORESIZE                                        0449
         LR    R1,@07                                              0449
*               MVCL(R0,R14);       /* MOVE ZERO TO CLEAR BUFFER AREA*/
         MVCL  R0,R14                                              0450
*               RFY                                                0451
*                (R0,                                              0451
*                 R1,                                              0451
*                 R14,                                             0451
*                 R15) UNRSTD;      /* RELEASE REG0,1,14 AND 15      */
*             END;                                                 0452
*           ELSE                                                   0453
*             RTCODE=NORCBSTG;      /* NO NUCLEUR CORE AVAILABLE     */
         B     @RC00443                                            0453
@RF00443 MVC   RTCODE(4),@CF00362                                  0453
*         END;                      /* END OF OBTAIN NUCLEUS BUFFER  */
@RC00443 DS    0H                                                  0455
*     END;                          /* END OF PROCESS CORRECT DEVICE
*                                      TYPE                          */
@RF00439 DS    0H                                                  0456
*   IF RTCODE^=SUCCESS THEN         /* CHECK FOR GETMAIN PROCESS   0456
*                                      SUCCESSFUL                    */
@RC00410 L     @15,RTCODE                                          0456
         LTR   @15,@15                                             0456
         BNZ   @RT00456                                            0456
*     GOTO OPSEXIT;                 /* NO-ERROR EXIT                 */
