*/*                                                                  */
*/* INITLPMB:  SUBSECTION TO INITIALIZE THE LOGICAL TO PHYSICAL      */
*/*            MAPPING BLOCK.                                        */
*/*                                                                  */
*/* INPUT:     POINTER TO END OF USED STORAGE                        */
*/*            CATALOG INFORMATION                                   */
*/*                                                                  */
*/* PROCESS:   INITIALIZE A LPMB IN THE NEXT AVAILABLE AREA OF       */
*/*            STORAGE. GENERALLY THIS ENTAILS MOVING DATA FROM THE  */
*/*            LOCATE PARAMETER LISTS TO THE LPMB.                   */
*/*                                                                  */
*/* OUTPUT:    AN INITIALIZED LPMB                                   */
*/*                                                                  */
*/********************************************************************/
*/*INITLPMB: CHART                                                   */
*/*INITLPMB: N    INITIALIZE THE LPMB                                */
*/*          P    INIT LPMBPTR TO NEXT AVAILABLE STORAGE             */
*/*          P    INIT LPMB IDENTIFIER FIELD                         */
*/*          P    INIT LPMB LENGTH FIELD                             */
*/*          P    INIT TRACKS PER ALLOCATED UNIT                     */
*/*          P    INIT TRACKS/CYLINDER                               */
*/*          P    INIT BLOCKS/TRACK                                  */
*/*          P    INIT BLOCKSIZE = PAGESIZE( 4096 BYTES)             */
*/*          D    (YES,,NO,%LPM10) TRACK OVERFLOW DEVICE ?           */
*/*          P    INIT BYTES/ALLOCATED UNIT FROM VOLDEV LOCATE       */
*/*          P    INIT BYTES/TRACK FROM VOLDEV LOCATE                */
*/*          P    (,%LPM20) INDICATE TRACK OVERFLOW IN LPMB FLAGS    */
*/*%LPM10:   P    INIT BYTES/ALLOCATED UNIT FROM AMDSB LOCATE        */
*/*          P    CALCULATE BYTES/TRACK                              */
*/*%LPM20:   D    (YES,,NO,%LPM30) OPENING AN RPS DEVICE ?           */
*/*          P    INDICATE RPS DEVICE IN LPMB FLAGS                  */
*/*%LPM30:   P    SAVE PTR TO END OF USED SQA                        */
*/*          R    END OF INITIALIZE LPMB SECTION                     */
*/*INITLPMB: END                                                     */
*                                                                  0503
*                                                                  0503
*/*  INITLPMB-INITIALIZE LPMB                                        */
*                                                                  0503
*   RFY                                                            0503
*     IDALPMB BASED(LPMBPTR);                                      0503
*   LPMBPTR=COREADDR;               /* NEXT AVAILABLE STORAGE        */
         LR    LPMBPTR,COREADDR                                    0504
*   LPMBID=LPMBIDC;                 /* INIT IDENTIFIER FIELD         */
         MVC   LPMBID(1,LPMBPTR),LPMBIDC                           0505
*   LPMBLEN=LPMBLGH;                /* LPMB LENGTH                   */
         SLR   @12,@12                                             0506
         IC    @12,LPMBLGH                                         0506
         STH   @12,LPMBLEN(,LPMBPTR)                               0506
*   LPMTRKAU=NOTRKAU;               /* TRACKS PER ALLOCATED UNIT     */
         L     @12,FPL3DPTR                                        0507
         LH    @05,NOTRKAU(,@12)                                   0507
         STH   @05,LPMTRKAU(,LPMBPTR)                              0507
*   LPMTPC=DEVTABTC;                /* TRACKS PER CYLINDER           */
         L     @05,CVTZDTAB(,CVTPTR)                               0508
         L     @01,UCBPTR                                          0508
         SLR   @15,@15                                             0508
         IC    @15,UCBTBYT4(,@01)                                  0508
         ALR   @15,@05                                             0508
         SLR   @14,@14                                             0508
         IC    @14,DEVTABOF(,@15)                                  0508
         ALR   @05,@14                                             0508
         LH    @05,DEVTABTC(,@05)                                  0508
         STH   @05,LPMTPC(,LPMBPTR)                                0508
*   LPMBLKTR=NOBLKTRK;              /* BLOCKS PER TRACK              */
         LH    @05,NOBLKTRK(,@12)                                  0509
         STH   @05,LPMBLKTR(,LPMBPTR)                              0509
*   LPMBLKSZ=PHYBLKSZ;              /* BLOCK SIZE                    */
         L     @05,PHYBLKSZ(,@12)                                  0510
         ST    @05,LPMBLKSZ(,LPMBPTR)                              0510
*   IF TRACKOVL=YES THEN            /* TRACK OVERFLOW DEVICE ?       */
         L     @05,FPL5DPTR                                        0511
         TM    TRACKOVL(@05),B'00000001'                           0511
         BNO   @RF00511                                            0511
*     DO;                           /* YES - TRACK OVERFLOW          */
*       LPMAUSZ=NOBYTAU;            /* BYTES PER ALLOCATED UNIT      */
         L     @05,NOBYTAU(,@12)                                   0513
         ST    @05,LPMAUSZ(,LPMBPTR)                               0513
*       LPMBPTRK=NOBYTTRK;          /* BYTES PER TRACK               */
         L     @12,NOBYTTRK(,@12)                                  0514
         ST    @12,LPMBPTRK(,LPMBPTR)                              0514
*       LPMBTOFL=ON;                /* INDICATE TRACK OVERFLOW       */
         OI    LPMBTOFL(LPMBPTR),B'00010000'                       0515
*     END;                                                         0516
*   ELSE                                                           0517
*     DO;                           /* NOT TRACK OVERFLOW DEVICE     */
         B     @RC00511                                            0517
@RF00511 DS    0H                                                  0518
*       RFY                                                        0518
*         IDAAMDSB BASED(FPL6DPTR); /* SET PARM FOR AMDSB            */
*       LPMAUSZ=AMDCINV*AMDCIPCA;   /* BYTES PER ALLOCATED UNIT      */
         L     @12,FPL6DPTR                                        0519
         L     @05,AMDCINV(,@12)                                   0519
         MH    @05,AMDCIPCA(,@12)                                  0519
         ST    @05,LPMAUSZ(,LPMBPTR)                               0519
*       LPMBPTRK=LPMBLKSZ*LPMBLKTR; /* BYTES PER TRACK               */
         L     @12,LPMBLKSZ(,LPMBPTR)                              0520
         MH    @12,LPMBLKTR(,LPMBPTR)                              0520
         ST    @12,LPMBPTRK(,LPMBPTR)                              0520
*     END;                                                         0521
*   IF UCBRPS=ON THEN               /* RPS DEVICE ?                  */
@RC00511 L     @12,UCBPTR                                          0522
         TM    UCBRPS(@12),B'00010000'                             0522
         BNO   @RF00522                                            0522
*     LPMBRPS=ON;                   /* YES - SO INDICATE             */
         OI    LPMBRPS(LPMBPTR),B'10000000'                        0523
*   ELSE                                                           0524
*     ;                                                            0524
@RF00522 DS    0H                                                  0525
*   COREADDR=COREADDR+LPMBLGH;      /* UPDATE NEXT AVAIL STORAGE     */
         SLR   @12,@12                                             0525
         IC    @12,LPMBLGH                                         0525
         ALR   COREADDR,@12                                        0525
*                                                                  0526
*/********************************************************************/
*/*                                                                  */
*/* INITEDB:   SUBSECTION TO INITIALIZE THE EXTENT DEFINITION BLOCK  */
*/*                                                                  */
*/* INPUT:     POINTER TO NEXT AVAILABLE STORAGE                     */
*/*            CATALOG INFORMATION                                   */
*/*                                                                  */
*/* PROCESS:   THE EDB IS INITIALIZED WITH INFORMATION ACQUIRED FROM */
*/*            THE CATALOG LOCATE. IF THERE ARE MULTIPLE EXTENTS, AN */
*/*            EDB HAS TO BE INITIALIZED FOR EACH EXTENT. IN THE     */
*/*            CASE OF MULTIPLE EXTENTS, THERE IS A SORT BY RELATIVE */
*/*            STARTING TRACK ADDRESS IN ORDER THAT ILRSRT00 WILL BE */
*/*            ABLE TO CHOOSE THE 'BEST' SLOT. THE AMS UTILITY       */
*/*            PICKS EXTENTS BY SIZE RATHER THAN BY LOCATION.        */
*/*                                                                  */
*/*            THE SORT LOOPS THROUGH THE CATALOG PARAMETER LISTS    */
*/*            SEARCHING FOR THE LOWEST EXTENT STARTING TRACK WHICH  */
*/*            HAS NOT BEEN USED. WHEN FOUND, THE NEXT EDB ENTRY IS  */
*/*            INITIALIZED WITH THE CATALOG INFORMATION.             */
*/*                                                                  */
*/* OUTPUT:    A COMPLETED EDB                                       */
*/*                                                                  */
*/********************************************************************/
*/*INITEDB:  CHART                                                   */
*/*INITEDB:  N    INITIALIZE THE EDB                                 */
*/*          P    INITIALIZE EDBHDR PTR TO NEXT AVAILABLE STORAGE    */
*/*          P    INIT EDB IDENTIFIER                                */
*/*          P    INIT EDB LENGTH IN HEADER                          */
*/*          P    INIT NUMBER OF EDBS IN HEADER                      */
*/*          P    SET EDB LPMB POINTER IN HEADER                     */
*/*          N    SORT EDB ENTRIES, IF NECESSARY                     */
*/*          P    INIT PTR TO FIRST EDB ENTRY                        */
*/*          P    INIT VALUE OF LAST USED HIRBA TO ZERO              */
*/*%EDB10:   D    (YES,,NO,%EDB20) ANOTHER EDB TO PROCESS ?          */
*/*          P    INITIALIZE CURRENT SORT COMPARE VALUE              */
*/*          N    LOOP THROUGH LOCATE LIST EXTENT ENTRIES            */
*/*%EDB50:   D    (YES,,NO,%EDB30) ANOTHER ENTRY IN LOCATE LIST ?    */
*/*          D    (YES,,NO,%EDB40) EDBSTTRK < CURRENT & ENTRY ^ USED */
*/*          P    SET CURRENT VALUE TO THIS STARTING TRACK           */
*/*          P    SAVE INDEX TO THIS LIST ENTRY                      */
*/*%EDB40:   P    (,%EDB50) UPDATE POINTER TO NEXT LOCATE LIST ENTRY */
*/*%EDB30:   P    SET EDB LPMB POINTER                               */
*/*          P    INITIALIZE NUMBER OF THIS EXTENT                   */
*/*          P    INITIALIZE STARTING TRACK OF EXTENT                */
*/*          P    INIT LOW RBA LIMIT                                 */
*/*          P    INIT HIGH RBA LIMIT                                */
*/*          D    (YES,,NO,%EDB60) TRACK OVERFLOW DEVICE ?           */
*/*          P    INIT TRACK OVERFLOW FLAG FOR THIS ENTRY            */
*/*%EDB60:   P    MARK EXTENT ENTRY USED FLAG FOR THIS ENTRY         */
*/*          P    (,%EDB10) UPDATE PTR TO NEXT EDB ENTRY             */
*/*%EDB20:   P    SET CATALOG HIRBA TO EDB HIGH RBA                  */
*/*          R    END OF INITIALIZE EDB                              */
*/*INITEDB:  END                                                     */
*                                                                  0526
*                                                                  0526
*/*INITEDB-INITIALIZE EDB                                            */
*                                                                  0526
*   RFY                                                            0526
*     IDAEDB BASED(EDBPTR);                                        0526
*   RFY                                                            0527
*     IDAEDBHD BASED(EDBHDPTR);                                    0527
*   RFY                                                            0528
*     CTEXTENT BASED(WRKPTR);                                      0528
*   EDBHDPTR=COREADDR;              /* NEXT AVAILABLE STORAGE        */
         LR    EDBHDPTR,COREADDR                                   0529
*   OUEDBPR=EDBHDPTR;               /* SET EDB ADDRESS IN OUTPUT PARM*/
         ST    EDBHDPTR,OUEDBPR(,PARMPTR)                          0530
*   EDBID=EDBIDC;                   /* INIT IDENTIFIER FIELD         */
         IC    @12,EDBIDC                                          0531
         STC   @12,EDBID(,EDBHDPTR)                                0531
*   EDBLEN=EDBLGH;                  /* EDB ENTRY LENGTH              */
         SLR   @12,@12                                             0532
         IC    @12,EDBLGH                                          0532
         STH   @12,EDBLEN(,EDBHDPTR)                               0532
*   EDBNO=NOEXTNT;                  /* NBR OF EDB ENTRIES            */
         L     @12,FPL4DPTR                                        0533
         IC    @06,NOEXTNT(,@12)                                   0533
         STC   @06,EDBNO(,EDBHDPTR)                                0533
*   EDBLPMBC=LPMBPTR;               /* LPMB ADDRESS                  */
         ST    LPMBPTR,EDBLPMBC(,EDBHDPTR)                         0534
*   EDBPTR=COREADDR+EDBHDLGH;       /* ADDRESS FIRST EDB ENTRY       */
         LR    EDBPTR,COREADDR                                     0535
         SLR   @12,@12                                             0535
         IC    @12,EDBHDLGH                                        0535
         ALR   EDBPTR,@12                                          0535
*   LOEDBVAL=ZERO;                  /* VALUE OF LAST HIRBA USED      */
         SLR   LOEDBVAL,LOEDBVAL                                   0536
*   PROCFLG=''B;                    /* INIT EDB USED FLAG            */
         XC    PROCFLG(5),PROCFLG                                  0537
*   DO I=1 TO EDBNO;                /* LOOP THROUGH EDB ENTRIES      */
         LA    @12,1                                               0538
         B     @DE00538                                            0538
@DL00538 DS    0H                                                  0539
*     LOCHECK=SORTMASK;             /* SORT COMPARE VALUE            */
         LH    @12,SORTMASK                                        0539
         STH   @12,LOCHECK                                         0539
*     DO J=1 TO EDBNO;              /* LOOP THROUGH LOCATE LIST      */
         LA    J,1                                                 0540
         B     @DE00540                                            0540
@DL00540 DS    0H                                                  0541
*       WRKPTR=ADDR(EXTENT(J));     /* BASE FOR LOCATE DATA          */
         LR    @15,J                                               0541
         MH    @15,@CH00362                                        0541
         L     WRKPTR,FPL4DPTR                                     0541
         LA    WRKPTR,EXTENT-20(@15,WRKPTR)                        0541
*       IF ENTUSEDF(J)^=ON&         /* ENTRY NOT CHOSEN YET ?        */
*           CTCCBEG<LOCHECK THEN    /* AND LOWER THAN COMP VALUE     */
         LA    @01,ENTUSEDF-1(J)                                   0542
         TM    0(@01),B'10000000'                                  0542
         BO    @RF00542                                            0542
         ICM   @15,12,CTCCBEG(WRKPTR)                              0542
         SRA   @15,16                                              0542
         CH    @15,LOCHECK                                         0542
         BNL   @RF00542                                            0542
*         DO;                                                      0543
*           LOCHECK=CTCCBEG;        /* SWAP SORT COMPARE VALUE       */
         STH   @15,LOCHECK                                         0544
*           K=J;                    /* REMEMBER THIS LOCATE ENTRY    */
         STC   J,K                                                 0545
*         END;                                                     0546
*       ELSE                                                       0547
*         ;                                                        0547
@RF00542 DS    0H                                                  0548
*     END;                                                         0548
         AL    J,@CF00036                                          0548
@DE00540 SLR   @06,@06                                             0548
         IC    @06,EDBNO(,EDBHDPTR)                                0548
         CR    J,@06                                               0548
         BNH   @DL00540                                            0548
*     WRKPTR=ADDR(EXTENT(K));       /* BASE FOR LOCATE DATA          */
         SLR   @12,@12                                             0549
         IC    @12,K                                               0549
         MH    @12,@CH00362                                        0549
         L     WRKPTR,FPL4DPTR                                     0549
         LA    WRKPTR,EXTENT-20(@12,WRKPTR)                        0549
*     EDBLPMBA=LPMBPTR;             /* INIT LPMB ADDR IN EDB ENTRY   */
         ST    LPMBPTR,EDBLPMBA(,EDBPTR)                           0550
*     EDBM=I-ONE;                   /* EDB EXTENT NBR (BASE ZERO)    */
         SLR   @12,@12                                             0551
         IC    @12,I                                               0551
         BCTR  @12,0                                               0551
         STC   @12,EDBM(,EDBPTR)                                   0551
*     EDBSTTRK=CTCCBEG*LPMTPC+CTHHBEG;/* STARTING TRACK OF EXTENT    */
         ICM   @12,12,CTCCBEG(WRKPTR)                              0552
         SRA   @12,16                                              0552
         MH    @12,LPMTPC(,LPMBPTR)                                0552
         ICM   @15,12,CTHHBEG(WRKPTR)                              0552
         SRA   @15,16                                              0552
         ALR   @12,@15                                             0552
         ST    @12,EDBSTTRK(,EDBPTR)                               0552
*     EDBLORBA=LOEDBVAL;            /* TOP OF LAST EXTENT            */
         ST    LOEDBVAL,EDBLORBA(,EDBPTR)                          0553
*     EDBHIRBA=EDBLORBA+(CTRBAEND-CTRBABEG);/* INIT HIGH RBA         */
         L     @12,EDBLORBA(,EDBPTR)                               0554
         ICM   @15,15,CTRBAEND(WRKPTR)                             0554
         ICM   @14,15,CTRBABEG(WRKPTR)                             0554
         SLR   @15,@14                                             0554
         ALR   @12,@15                                             0554
         ST    @12,EDBHIRBA(,EDBPTR)                               0554
*     LOEDBVAL=EDBHIRBA+ONE;        /* NEW LO VALUE FOR NEXT ENTRY   */
         AL    @12,@CF00036                                        0555
         LR    LOEDBVAL,@12                                        0555
*     IF TRACKOVL=YES THEN          /* TRACK OVERFLOW DEVICE ?       */
         L     @12,FPL5DPTR                                        0556
         TM    TRACKOVL(@12),B'00000001'                           0556
         BNO   @RF00556                                            0556
*       EDBTOFLW=ON;                /* TRACK OVERFLOW FLAG           */
         OI    EDBTOFLW(EDBPTR),B'01000000'                        0557
*     ELSE                                                         0558
*       ;                                                          0558
@RF00556 DS    0H                                                  0559
*     EDBPSDS=ON;                   /* EDB DESCRIBES A PAGE SPACE    */
         OI    EDBPSDS(EDBPTR),B'00100000'                         0559
*     ENTUSEDF(K)=ON;               /* LOCATE LIST ENTRY USED        */
         SLR   @12,@12                                             0560
         IC    @12,K                                               0560
         LA    @06,ENTUSEDF-1(@12)                                 0560
         OI    0(@06),B'10000000'                                  0560
*     EDBPTR=EDBPTR+EDBLGH;         /* ADDR OF NEXT EDB ENTRY        */
         SLR   @12,@12                                             0561
         IC    @12,EDBLGH                                          0561
         ALR   EDBPTR,@12                                          0561
*   END;                                                           0562
         LA    @12,1                                               0562
         SLR   @06,@06                                             0562
         IC    @06,I                                               0562
         ALR   @12,@06                                             0562
@DE00538 STC   @12,I                                               0562
         SLR   @06,@06                                             0562
         IC    @06,EDBNO(,EDBHDPTR)                                0562
         CR    @12,@06                                             0562
         BNH   @DL00538                                            0562
*   HARBA=LOEDBVAL;                 /* INSURE HIRBA VALUES MATCH     */
         L     @12,FPL4DPTR                                        0563
         STCM  LOEDBVAL,15,HARBA(@12)                              0563
*   SLOTNBR=HARBA/PAGESIZE;         /* SET SLOT NUMBER FOR PAGE/SWAP
*                                      SPACE IN OUTPUT PARM          */
         L     @12,FPL4DPTR                                        0564
         ICM   @10,15,HARBA(@12)                                   0564
         SRL   @10,12                                              0564
         ST    @10,SLOTNBR(,PARMPTR)                               0564
*                                                                  0565
*/********************************************************************/
*/*                                                                  */
*/*   ENDOPS00:                                                      */
*/*      RESTORE CALLER REGISTERS AND RETURN TO CALLER               */
*/*                                                                  */
*/********************************************************************/
*                                                                  0565
*OPSEXIT:                                                          0565
*   RETURN CODE(RTCODE);            /* RETURN TO CALLER              */
OPSEXIT  L     @12,RTCODE                                          0565
         L     @13,4(,@13)                                         0565
         L     @00,@SIZDATD                                        0565
         LR    @01,@09                                             0565
         FREEMAIN R,LV=(0),A=(1)
         LR    @15,@12                                             0565
         L     @14,12(,@13)                                        0565
         LM    @00,@12,20(@13)                                     0565
         BR    @14                                                 0565
*   DECLARE                         /* GENERAL PURPOSE REGISTERS     */
*     GPR00F FIXED(31) REG(0),                                     0566
*     GPR01F FIXED(31) REG(1),                                     0566
*     GPR15F FIXED(31) REG(15);                                    0566
*   DECLARE                         /* COMMON VARIABLES              */
*     I256C CHAR(256) BASED,                                       0567
*     I031F FIXED(31) BASED,                                       0567
*     I031P PTR(31) BASED,                                         0567
*     I015F FIXED(15) BASED,                                       0567
*     I015P PTR(15) BASED,                                         0567
*     I008P PTR(8) BASED,                                          0567
*     I001C CHAR(1) BASED;                                         0567
