*/*                                                                  */
*/*  DYNALLO: TO VERIFY A DATA SET AS BEING MOUNTED                  */
*/*            AND ON-LINE AFTER NIP TIME                            */
*/*                                                                  */
*/*  INPUT:    DSNAME                                                */
*/*            DISP=OLD (SINCE THE DATA SET HAS BEEN CATALOGED)      */
*/*                                                                  */
*/*  PROCESS:  AN ATTEMPT IS MADE TO MOUNT AND VERIFY ON-LINE        */
*/*            VIA DYNALLOC MACRO. THE REQUEST BLOCK CONTAINS        */
*/*            INFO FOR DSNAME AND DISPOSITION OF A DATA SET.        */
*/*                                                                  */
*/*            ON RETURN FROM DYNALLOC REGISTER 15 WILL CONTAIN      */
*/*            A RETURN CODE. IF IT IS SUCCESSFUL THE ADDRESS OF UCB */
*/*            CAN BE OBTAINED THRU DD NAME. IF IT IS UNSUCCESSFUL   */
*/*            A NONZERO RETURN CODE WILL BE RETURNED TO CALLER      */
*/*  OUTPUT:   SUCCESSFUL - A DD NAME WILL BE RETURNED FROM DYNALLOC.*/
*/*                         UCB ADDRESS WILL BE FOUND                */
*/*                                                                  */
*/********************************************************************/
*/*DYNALLO:    CHART                                                 */
*/*DYNALLO:    N    ATTEMPT TO MOUNT A PAGE OR SWAP VOLUME AFTER NIP */
*/*            P    SET REQUEST BLOCK FOR DYNAMIC ALLOC              */
*/*            P    SET VERB CODE TO DSNAME ALLOCATION               */
*/*            P    CONSIDER OFFLINE DEVICE                          */
*/*            P   VOLUME MAY BE MOUNTED                             */
*/*            P    SET TEXT UNIT FOR DSNAME                         */
*/*            P    SET TEXT UNIT FOR DISP=OLD                       */
*/*            P    SET TEXT UNIT FOR RETURNED DD NAME               */
*/*            L    DYNALLOC: ISSUE DYNAMIC ALLOC(SVC99)             */
*/*            D    (YES,,NO,%DYNFAIL) DYNAMIC ALLOC SUCCESSFUL?     */
*/*            P    GET RETURNED DDNAME                              */
*/*            P    GET CURRENT TCB ADDRESS                          */
*/*            P    GET JSCB ADDRESS FROM TCBJSCB                    */
*/*%SEARCHD:   D   (YES,,NO,%DYNFAIL) MORE DSABS EXIST IN THE CHAIN? */
*/*            P    GET DSAB                                         */
*/*            P    GET TIOT DD ENTRY                                */
*/*            D    (YES,,NO,%SEARCHD) DD NAME MATCH?                */
*/*            P    GET CORRESPONDING UCB ADDRESS                    */
*/*            R    END OF DYNAMIC ALLOC SECTION                     */
*/*%DYNFAIL:   P    SET MOUNT ERROR RETURN CODE(8) IN REG 15         */
*/*            R    RETUR TO CALLER                                  */
*/*DYNALLO:    END                                                   */
*                                                                  0337
*     DO;                           /* ISSUE DYNALLOC AFTER NIP FOR
*                                      MOUNT                         */
         B     @RC00319                                            0337
@RF00319 DS    0H                                                  0338
*/*DYNALLO-DYNAMIC ALLOCATION A DATA SET                             */
*                                                                  0338
*       S99RBPTR=ADDR(S99RB);       /* SET DYNALLOC REQUEST BLOCK PTR*/
         LA    @15,S99RB                                           0338
         ST    @15,S99RBPTR                                        0338
*       S99RBPND=ON;                /* SET RB LAST INDICATOR ON      */
         OI    S99RBPND,B'10000000'                                0339
*       S99RB=''B;                  /* INIT DYNALLOC REQUEST BLOCK   */
         XC    S99RB(20),S99RB                                     0340
*       S99RBLN=LENGTH(S99RB);      /* SET REQUEST BLOCK LEN         */
         MVI   S99RBLN,X'14'                                       0341
*       S99VERB=S99VRBAL;           /* SET DSNAME ALLOC              */
         MVI   S99VERB,X'01'                                       0342
*       S99OFFLN=ON;                /* CONSIDER OFF LINE UNITS       */
*       S99MOUNT=ON;                /* SET MOUNT VOLUMES             */
         OI    S99OFFLN,B'00001001'                                0344
*       S99TXTPP=ADDR(DYNTL);       /* SET ADDR OF LIST OF TEXT UNITS*/
         LA    @15,DYNTL                                           0345
         ST    @15,S99TXTPP                                        0345
*       DYNTL=''B;                  /* INIT DYNALLOC PTRS LIST       */
         XC    DYNTL(12),DYNTL                                     0346
*       DYNTU='';                   /* INIT DYNALLOC TEXT UNITS      */
         MVI   DYNTU,C' '                                          0347
         MVC   DYNTU+1(70),DYNTU                                   0347
*       S99TUPTR(ONE)=ADDR(DYTUNIT1);/* INIT TEXT UNIT PTR 1         */
         LA    @14,DYTUNIT1                                        0348
         ST    @14,S99TUPTR(,@15)                                  0348
*       S99TUPTR(TWO)=ADDR(DYTUNIT2);/* INIT TEXT UNIT PTR 2         */
         LA    @14,DYTUNIT2                                        0349
         ST    @14,S99TUPTR+4(,@15)                                0349
*       S99TUPTR(THREE)=ADDR(DYTUNIT3);/* INIT TEXT UNIT PTR 3       */
         LA    @14,DYTUNIT3                                        0350
         ST    @14,S99TUPTR+8(,@15)                                0350
*       S99TUPLN(THREE)=ON;         /* SET LAST TEXT UNIT PTR FLAG   */
         OI    S99TUPLN+8(@15),B'10000000'                         0351
*       RFY                                                        0352
*         S99TUP BASED(S99TUPTR);   /* BASE TEXT UNIT PTR LIST       */
*       S99TUPTR(ONE)->S99TUKEY=DALDSNAM;/* SET KEY AS DSNAME        */
         L     @14,S99TUPTR(,@15)                                  0353
         MVC   S99TUKEY(2,@14),@CB03274                            0353
*       S99TUPTR(ONE)->S99TUNUM=ONE;/* SET NUMBER OF PARMS           */
         LA    @07,1                                               0354
         STCM  @07,3,S99TUNUM(@14)                                 0354
*       S99TUPTR(ONE)->S99TULNG=LENGTH(SVDSNAME);/* SET LENGTH OF  0355
*                                      PARM                          */
         MVC   S99TULNG(2,@14),@CH01877                            0355
*       S99TUPTR(ONE)->S99TUPAR(ONE:LENGTH(SVDSNAME))=SVDSNAME;    0356
*                                                                  0356
         MVC   S99TUPAR(44,@14),SVDSNAME                           0356
*       /*************************************************************/
*       /*                                                           */
*       /* SET DSNAME                                                */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0357
*       S99TUPTR(TWO)->S99TUKEY=DALSTATS;/* SET KEY AS DISP STATUS   */
         L     @14,S99TUPTR+4(,@15)                                0357
         MVC   S99TUKEY(2,@14),@CB03278                            0357
*       S99TUPTR(TWO)->S99TUNUM=ONE;/* SET NUMBER OF PARMS           */
         STCM  @07,3,S99TUNUM(@14)                                 0358
*       S99TUPTR(TWO)->S99TULNG=ONE;/* SET LENGTH OF PARM            */
         STCM  @07,3,S99TULNG(@14)                                 0359
*       S99TUPTR(TWO)->S99TUPAR(ONE)=STATOLD;/* SET DISP=OLD         */
         MVC   S99TUPAR(1,@14),STATOLD                             0360
*       S99TUPTR(THREE)->S99TUKEY=DALRTDDN;/* SET RETURN DD NAME KEY */
         L     @06,S99TUPTR+8(,@15)                                0361
         MVC   S99TUKEY(2,@06),@CB03440                            0361
*       S99TUPTR(THREE)->S99TUNUM=ONE;/* SET NUMBER OF PARMS         */
         STCM  @07,3,S99TUNUM(@06)                                 0362
*       S99TUPTR(THREE)->S99TULNG=EIGHT;/* SET LENGTH OF DD NAME     */
         MVC   S99TULNG(2,@06),@CH00053                            0363
*       RFY                                                        0364
*        (R1,                                                      0364
*         R15) RSTD;                /* RESTRICTED REG1 REG15         */
*       R1=ADDR(S99RBPTR);          /* SET R1 TO ADDR OF DYNALLOC  0365
*                                      REQUEST BLOCK PTR             */
*                                                                  0365
         LA    R1,S99RBPTR                                         0365
*       /*************************************************************/
*       /*                                                           */
*       /* MACDATE Y-2 73082                                         */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0366
*       SVC(99);                                                   0366
         SVC   99                                                  0366
*       IF R15=SUCCESS THEN         /* DYNALLOC SUCCESSFUL?          */
         LTR   R15,R15                                             0367
         BNZ   @RF00367                                            0367
*         DO;                       /* YES-FIND UCB ADDRESS          */
*           TCBPTR=PSATOLD;         /* SET TCB OLD PTR               */
         L     TCBPTR,PSATOLD                                      0369
*           QDBPTR=JSCDSABQ;        /* GET QDB PTR FOR DSAB CHAIN    */
         L     @14,TCBJSCB(,TCBPTR)                                0370
         L     QDBPTR,JSCDSABQ(,@14)                               0370
*           DSABPTR=QDBFELMP;       /* GET PTR TO FIRST DSAB         */
         L     DSABPTR,QDBFELMP(,QDBPTR)                           0371
*           DNAMFUND=OFF;           /* INIT DD NAME NOT FOUND        */
         NI    DNAMFUND,B'01111111'                                0372
*           DO I=ONE TO QDBNELMS WHILE(DSABPTR^=ZERO&DNAMFUND=OFF);/*
*                                      SEARCH DSAB CHAIN FOR DD NAME */
         LA    @14,1                                               0373
         B     @DE00373                                            0373
@DL00373 LTR   DSABPTR,DSABPTR                                     0373
         BZ    @DC00373                                            0373
         TM    DNAMFUND,B'10000000'                                0373
         BNZ   @DC00373                                            0373
*             IF TIOEDDNM=S99TUPTR(THREE)->S99TUPAR(ONE:EIGHT) THEN/*
*                                      SAME DD NAME?                 */
         L     @14,DSABTIOT(,DSABPTR)                              0374
         L     @05,S99TXTPP                                        0374
         L     @05,S99TUPTR+8(,@05)                                0374
         CLC   TIOEDDNM(8,@14),S99TUPAR(@05)                       0374
         BNE   @RF00374                                            0374
*               DO;                 /* YES-DD NAME FOUND             */
*                 UCBPTR=TIOEFSRT;  /* GET UCB ADDRESS               */
         L     @14,TIOEFSRT-1(,@14)                                0376
         LA    @14,0(,@14)                                         0376
         ST    @14,UCBPTR                                          0376
*                 OUUCBPR=UCBPTR;   /* SET UCB PTR IN OUTPUT PARM    */
         ST    @14,OUUCBPR(,PARMPTR)                               0377
*                 DNAMFUND=ON;      /* SET DD NAME FOUND FLAG        */
         OI    DNAMFUND,B'10000000'                                0378
*               END;                                               0379
*             ELSE                                                 0380
*               DSABPTR=DSABFCHN;   /* NO-GET NEXT DSAB              */
         B     @RC00374                                            0380
@RF00374 L     DSABPTR,DSABFCHN(,DSABPTR)                          0380
*           END;                    /* END OF SEARCH DD NAME LOOP    */
@RC00374 LA    @14,1                                               0381
         SLR   @00,@00                                             0381
         IC    @00,I                                               0381
         ALR   @14,@00                                             0381
@DE00373 STC   @14,I                                               0381
         C     @14,QDBNELMS(,QDBPTR)                               0381
         BNH   @DL00373                                            0381
@DC00373 DS    0H                                                  0382
*           IF DNAMFUND=OFF THEN                                   0382
         TM    DNAMFUND,B'10000000'                                0382
         BNZ   @RF00382                                            0382
*             RTCODE=NOVOLMNT;      /* DD NAME NOT FOUND             */
         MVC   RTCODE(4),@CF00053                                  0383
*         END;                      /* END OF PROCESSING DYNALLOC  0384
*                                      SUCCESSFUL                    */
*       ELSE                                                       0385
*         RTCODE=NOVOLMNT;          /* DYNALLOC FAILS                */
         B     @RC00367                                            0385
@RF00367 MVC   RTCODE(4),@CF00053                                  0385
*       RFY                                                        0386
*        (R1,                                                      0386
*         R15) UNRSTD;              /* RELEASE R1 AND R15            */
@RC00367 DS    0H                                                  0387
*       IF RTCODE=NOVOLMNT THEN     /* CHECK DYNALLOC SUCCESSFUL?    */
         CLC   RTCODE(4),@CF00053                                  0387
         BE    @RT00387                                            0387
*         GO TO OPSEXIT;            /* ERROR EXIT-RETURN TO CALLER   */
*     END;                          /* END OF DYNALLOC               */
