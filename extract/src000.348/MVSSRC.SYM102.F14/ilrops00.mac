         TITLE 'ILROPS00:  ASM OPEN ROUTINE                            *
                        '
ILROPS00 CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'ILROPS00  77.272'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @11,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@11                                         0001
         L     @00,@SIZDATD                                        0001
         GETMAIN  R,LV=(0)
         LR    @09,@01                                             0001
         USING @DATD,@09                                           0001
         ST    @13,@SA00001+4                                      0001
         LM    @00,@01,20(@13)                                     0001
         ST    @09,8(,@13)                                         0001
         LR    @13,@09                                             0001
*
* REFS ARE GENERATED FOR NVT FIELDS THAT CONTAIN ADDRESSES OF
* NIP SERVICE ROUTINES, SO IEAPMNIP MACRO CALLS TO THESE ROUTINES
* WILL ASSEMBLE PROPERLY.
JSCBDISP EQU 188
JSCBAOS1 EQU  72
JSCBAOS2 EQU 192
*   PARMPTR=RPARM;                  /* SAVE PARAMETER LIST POINTER   */
         LR    PARMPTR,RPARM                                       0228
*   RFY                                                            0229
*     RPARM UNRSTD;                                                0229
*   SVDSNAME=DSNAME;                /* SAVE INPUT DSNAME             */
         MVC   SVDSNAME(44),DSNAME(PARMPTR)                        0230
*   OUDSNAM=''B;                    /* ZERO OUTPUT DSNAME FIELD FOR
*                                      ILRPGEXP ESTAE                */
         XC    OUDSNAM(44,PARMPTR),OUDSNAM(PARMPTR)                0231
*   RTCODE=SUCCESS;                 /* INIT RETURN CODE FOR CALLER   */
         SLR   @15,@15                                             0232
         ST    @15,RTCODE                                          0232
*   IF AFTERNIP=ON THEN             /* PROCESSING AFTER NIPTIME      */
         TM    AFTERNIP(PARMPTR),B'01000000'                       0233
         BNO   @RF00233                                            0233
*     CVTPTR=FLCCVT;                /* SET CVT PTR                   */
         L     CVTPTR,FLCCVT                                       0234
*   ASMVTPTR=CVTASMVT;              /* INIT ASMVT PTR                */
@RF00233 L     @15,CVTASMVT(,CVTPTR)                               0235
         ST    @15,ASMVTPTR                                        0235
*                                                                  0236
*/********************************************************************/
*/*                                                                  */
*/* LOCPREP:   SECTION  TO PREPARE THE CATALOG LOCATE PARAMETER      */
*/*            LISTS.                                                */
*/*                                                                  */
*/* INPUT:     DATA SET NAME TO BE OPENED                            */
*/*                                                                  */
*/* PROCESS:   ESSENTIAL CONSTANT FIELDS IN THE CATALOG PARAMETER    */
*/*            LIST (CTGPL) AND CATALOG FIELD PARAMETER LISTS        */
*/*            (CTGFLS) ARE INITIALIZED FOR BOTH THE CLUSTER         */
*/*            LOCATE ON DATA SET  NAME AND THE DATA OBJECT          */
*/*            LOCATE ON DATA CONTROL INTERVAL NUMBER.               */
*/*                                                                  */
*/* OUTPUT:    INITIALIZED CATALOG LOCATE PARAMETER LISTS            */
*/*                                                                  */
*/********************************************************************/
*/*LOCPREP:    CHART                                                 */
*/*LOCPREP:  N    SECTION TO PREPARE LOCATE PARM LISTS               */
*/*          N    INITIALIZE CATALOG PARM LIST FOR DATA CI LOCATE    */
*/*          P    INITIALIZE WORK AREA LENGTH IN REPLY AREA 2        */
*/*          P    INIT ADDRESS OF  FIELD PARAMETER ENTRIES           */
*/*          P    INDICATE PASSWORD CHECKING NOT WANTED              */
*/*          P    INDICATE CATALOG NAME FIELD IS ZERO                */
*/*          P    INDICATE LOCATE REQUEST                            */
*/*          P    INDICATE VSAM REQUEST                              */
*/*          P    SET ADDRESS OF LOCATE REPLY AREA                   */
*/*          P    INITIALIZE COUNT OF FIELD PARAMETER LISTS          */
*/*          N    INITIALIZE CATALOG PARM LIST FOR CLUSTER LOCATE    */
*/*          P    INITIALIZE WORK AREA LENGTH IN REPLY AREA 1        */
*/*          P    INIT ADDRESS OF  FIELD PARAMETER ENTRIES           */
*/*          P    COPY CATALOG PARM LIST INITIALIZED ABOVE           */
*/*          P    COPY DSNAME INTO DSNAME WORK AREA                  */
*/*          P    INDICATE LOCATE ON DATA SET NAME                   */
*/*          P    SET ADDRESS OF DATA SET NAME FIELD                 */
*/*          P    SET ADDRESS OF LOCATE REPLY AREA                   */
*/*          P    INITIALIZE COUNT OF FIELD PARAMETER LISTS          */
*/*          N    INITIALIZE FIELD PARAMETER LISTS NOW...            */
*/*          P    SET NAMEDS COMBINATION TO BE LOCATED IN FIRST    0236
*/*               FIELD PARM LIST                                    */
*/*          P    SET CATTR  TO BE LOCATED IN SECOND FIELD PARM LIST */
*/*          P    SET VOLDEV COMBINATION TO BE LOCATED IN THIRD    0236
*/*               FIELD PARM LIST                                    */
*/*          P    SET VOLPHY COMBINATION TO BE LOCATED IN FOURTH   0236
*/*               FIELD PARM LIST                                    */
*/*          P    SET DSATTR COMBINATION TO BE LOCATED IN FIFTH    0236
*/*               FIELD PARM LIST                                    */
*/*          P    SET AMDSB  TO BE LOCATED IN SIXTH FIELD PARM LIST  */
*/*          R    END OF LOCATE PREPARATION SECTION                  */
*/*LOCPREP:  END                                                     */
*/*  LOCPREP-PREPARE LOCATE PARM LIST                                */
*/*  CLEAR CORE FOR CATALOG PARAMETER LISTS                          */
*                                                                  0236
*   RFY                                                            0236
*    (R0,                                                          0236
*     R1,                                                          0236
*     R14,                                                         0236
*     R15) RSTD;                    /* RESERVE REGISTERS             */
*   R0=ADDR(ZEROLIST);              /* ADDRESS OF CORE TO CLEAR      */
         LA    R0,ZEROLIST                                         0237
*   R1=LENGTH(ZEROLIST);            /* LENGTH OF CORE TO CLEAR       */
         LA    R1,496                                              0238
*   R15=ZERO;                       /* PADDING CHAR OF ZERO          */
         SLR   R15,R15                                             0239
*   MVCL(R0,R14);                   /* MOVE ZERO TO PARM LIST        */
         MVCL  R0,R14                                              0240
*   RFY                                                            0241
*    (R0,                                                          0241
*     R1,                                                          0241
*     R14,                                                         0241
*     R15) UNRSTD;                  /* RELEASE REGISTERS             */
*                                                                  0241
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE CATALOG PARAMETER LIST (CTGPL) FOR DATA CI LOCATE  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0242
*   RFY                                                            0242
*     CTGPL BASED(ADDR(RIMCPL02));  /* ESTAB CATALOG PARM LIST     0242
*                                      ADDRESSABILITY                */
*   CWA2LEN=LENGTH(CWKAREA2);       /* INIT WORK AREA LENGTH         */
         MVC   CWA2LEN(2),@CH03830                                 0243
*   FPL3PTR=ADDR(FPL3);             /* ADDR OF 3RD FIELD PARM        */
         LA    @15,FPL3                                            0244
         ST    @15,FPL3PTR                                         0244
*   FPL4PTR=ADDR(FPL4);             /* ADDR OF 4TH FIELD PARM        */
         LA    @14,FPL4                                            0245
         ST    @14,FPL4PTR                                         0245
*   FPL5PTR=ADDR(FPL5);             /* ADDR OF 5TH FIELD PARM        */
         LA    @07,FPL5                                            0246
         ST    @07,FPL5PTR                                         0246
*   FPL6PTR=ADDR(FPL6);             /* ADDR OF 6TH FIELD PARM        */
         LA    @06,FPL6                                            0247
         ST    @06,FPL6PTR                                         0247
*   CTGBYPSS=YES;                   /* INDICATE NO PASSWORD CHECK    */
*   CTGCNAME=YES;                   /* INDICATE CATALOG NAME FIELD IS
*                                      ZERO                          */
         LA    @05,RIMCPL02                                        0249
         OI    CTGBYPSS(@05),B'10000010'                           0249
*   CTGFUNC=CTGLOC;                 /* INDICATE LOCATE REQUEST       */
         NI    CTGFUNC(@05),B'00111111'                            0250
         OI    CTGFUNC(@05),B'00100000'                            0250
*   CTGAM0=YES;                     /* INDICATE VSAM REQUEST         */
         OI    CTGAM0(@05),B'00000001'                             0251
*   CTGWKA=ADDR(CWKAREA2);          /* SET ADDR OF LOCATE REPLY AREA */
         LA    @04,CWKAREA2                                        0252
         ST    @04,CTGWKA(,@05)                                    0252
*   CTGNOFLD=FPL2CTR;               /* INDICATE FOUR FIELD PARM      */
*                                                                  0253
         MVI   CTGNOFLD(@05),X'04'                                 0253
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE CATALOG PARAMETER LIST (CTGPL) FOR CLUSTER LOCATE  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0254
*   RFY                                                            0254
*     CTGPL BASED(ADDR(RIMCPL01));  /* ESTAB CATALOG PARM LIST     0254
*                                      ADDRESSABILITY                */
*   CWA1LEN=LENGTH(CWKAREA1);       /* INIT WORK AREA LENGTH         */
         MVC   CWA1LEN(2),@CH00362                                 0255
*   FPL1PTR=ADDR(FPL1);             /* ADDR OF 1ST FIELD PARM        */
         LA    @05,FPL1                                            0256
         ST    @05,FPL1PTR                                         0256
*   FPL2PTR=ADDR(FPL2);             /* ADDR OF 2ND FIELD PARM        */
         LA    @04,FPL2                                            0257
         ST    @04,FPL2PTR                                         0257
*   CPL1FXD=CPL2FXD;                /* COPY PARMLIST JUST INITLZED   */
         MVC   CPL1FXD(28),CPL2FXD                                 0258
*   WKDSNAME=SVDSNAME;              /* GET PAGE OR SWAP DSNAME       */
         MVC   WKDSNAME(44),SVDSNAME                               0259
*   CTGNAME=YES;                    /* INDICATE LOCATE ON DSNAME     */
         LA    @01,RIMCPL01                                        0260
         OI    CTGNAME(@01),B'00000100'                            0260
*   CTGENT=ADDR(WKDSNAME);          /* SET ADDR OF DSNAME FIELD      */
         LA    @00,WKDSNAME                                        0261
         ST    @00,CTGENT(,@01)                                    0261
*   CTGWKA=ADDR(CWKAREA1);          /* SET ADDR OF LOCATE REPLY AREA */
         LA    @00,CWKAREA1                                        0262
         ST    @00,CTGWKA(,@01)                                    0262
*   CTGNOFLD=FPLCTR;                /* INDICATE TWO FIELD PARM       */
*                                                                  0263
         MVI   CTGNOFLD(@01),X'02'                                 0263
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE THE FIELD PARAMETER LISTS                          */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0264
*   FPL1PTR->CTGFLDNM=ADDR(NAMEDSC);/* SET NAMEDS TO BE LOCATED      */
         LA    @00,NAMEDSC                                         0264
         ST    @00,CTGFLDNM(,@05)                                  0264
*   FPL1PTR->CTGFLDNO=ONE;          /* SET FPL ARRAY COUNT           */
         MVI   CTGFLDNO(@05),X'01'                                 0265
*   FPL2PTR->CTGFLDNM=ADDR(CATTRC); /* SET CATTR TO BE LOCATED       */
         LA    @05,CATTRC                                          0266
         ST    @05,CTGFLDNM(,@04)                                  0266
*   FPL2PTR->CTGFLDNO=ONE;          /* SET FPL ARRAY COUNT           */
         MVI   CTGFLDNO(@04),X'01'                                 0267
*   FPL3PTR->CTGFLDNM=ADDR(TODEVC); /* SET TOVOLDEV TO BE LOCATED    */
         LA    @00,TODEVC                                          0268
         ST    @00,CTGFLDNM(,@15)                                  0268
*   FPL3PTR->CTGFLDNO=ONE;          /* SET FPL ARRAY COUNT           */
         MVI   CTGFLDNO(@15),X'01'                                 0269
*   FPL4PTR->CTGFLDNM=ADDR(VOLPHYC);/* SET VOLPHY TO BE LOCATED      */
         LA    @15,VOLPHYC                                         0270
         ST    @15,CTGFLDNM(,@14)                                  0270
*   FPL4PTR->CTGFLDNO=ONE;          /* SET FPL ARRAY COUNT           */
         MVI   CTGFLDNO(@14),X'01'                                 0271
*   FPL5PTR->CTGFLDNM=ADDR(DSATTRC);/* SET DSATTR TO BE LOCATED      */
         LA    @00,DSATTRC                                         0272
         ST    @00,CTGFLDNM(,@07)                                  0272
*   FPL5PTR->CTGFLDNO=ONE;          /* SET FPL ARRAY COUNT           */
         MVI   CTGFLDNO(@07),X'01'                                 0273
*   FPL6PTR->CTGFLDNM=ADDR(AMDSBC); /* SET AMDSB TO BE LOCATED       */
         LA    @07,AMDSBC                                          0274
         ST    @07,CTGFLDNM(,@06)                                  0274
*   FPL6PTR->CTGFLDNO=ONE;          /* SET FPL ARRAY COUNT           */
         MVI   CTGFLDNO(@06),X'01'                                 0275
*   IF PAGESP=ON THEN               /* INPUT IS A PAGE SPACE?        */
         TM    PAGESP(PARMPTR),B'10000000'                         0276
         BNO   @RF00276                                            0276
*                                                                  0277
*/********************************************************************/
*/*  LOCPAGE: ISSUE LOCATES FOR A SINGLE PAGE SPACE                  */
*/*                                                                  */
*/*  INPUT: CATALOG PARAMETER LISTS AND FIELD PARAMETER LISTS ARE    */
*/*         PREVIOUSLY SET UP BY 'LOCPREP'.                          */
*/*                                                                  */
*/*  PROCESS: AN ATTEMPT IS MADE TO LOCATE A PAGING SPACE NAMED      */
*/*           IN THE INPUT PARAMETER LIST.                           */
*/*                                                                  */
*/*           FIRST LOCATE CLUSTER BY NAME AND VERIFY IT IS A PAGE   */
*/*           SPACE. IF IT IS SUCCESSFUL,A SECOND LOCATE IS ISSUED   */
*/*           ON THE DATA CONTROL INTERVAL NUMBER TO GET THE PAGE    */
*/*           SPACE  VOLUME SERIAL,DEVICE TYPE,DSATTR AND AMDSB.     */
*/*                                                                  */
*/*           IF EITHER LOCATE IS UNSUCCESSFUL,OR THE DATA SET IS    */
*/*           NOT A VALID PAGE SPACE A NONZERO RETURN CODE WILL BE   */
*/*           RETURNED TO THE CALLER.PROCESSING IS TERMINATED.       */
*/*                                                                  */
*/*  OUTPUT:  THE REQUIRED INFORMATION TO OPEN  A  PAGE SPACE        */
*/*                                                                  */
*/********************************************************************/
*/*LOCPAGE:    CHART                                                 */
*/*            N    SECTION TO ISSUE LOCATE FOR A PAGE SPACE         */
*/*            P    SET ADDRESS OF CATALOG PARM LIST IN REG 1        */
*/*            L    SVC26: LOCATE CLUSTER BY DSNAME                  */
*/*            D    (YES,,NO,%LOCFAIL) LOCATE CLUSTER SUCCESSFUL?    */
*/*            D    (YES,,NO,%LOCFAIL) DSNAME NOT IN CVOL?           */
*/*            D    (YES,,NO,%LOCFAIL) DATA SET REALLY A PAGE SPACE? */
*/*            D  (YES,,NO,%LOCFAIL) DATASET HAS ONLY A 'DATA' TYPE? */
*/*            P  SET ADDR OF DATA CI IN CATLOG PARM LIST            */
*/*            L    SVC26:LOCATE INFO BY CI NUMBER                   */
*/*            D    (YES,,NO,%LOCFAIL) LOCATE INFO SUCCESSFUL?       */
*/*            D     (YES,%LOCFAIL,NO,) NOT USABLE FLAG ON?          */
*/*            R    END OF LOCATE PAGE SECTION                       */
*/*%LOCFAIL:   P    SET RETURN CODE(12)-LOCATE FAIL                  */
*/*            R    RETURN TO CALLER                                 */
*/*LOCPAGE:    END                                                   */
*                                                                  0277
*                                                                  0277
*/*LOCPGSP-LOCATE PAGE SPACE                                         */
*                                                                  0277
*     DO;                                                          0277
*       RFY                                                        0278
*        (R1,                                                      0278
*         R15) RSTD;                                               0278
*                                                                  0278
*       /*************************************************************/
*       /*                                                           */
*       /* SET UP FOR CLUSTER LOCATE (GET DATA CI NUMBER AND ENTRY   */
*       /* TYPE)                                                     */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0279
*       R1=ADDR(RIMCPL01);          /* ADDR OF CATALOG PARM LIST     */
         LA    @14,RIMCPL01                                        0279
         LR    R1,@14                                              0279
*       SVC(26);                    /* LOCATE THE PAGING SPACE       */
*                                                                  0280
         SVC   26                                                  0280
*       /*************************************************************/
*       /*                                                           */
*       /* IF THE CLUSTER LOCATE WAS SUCCESSFUL, VERIFY THAT THE     */
*       /* DATASET IS INDEED A PAGE SPACE. ISSUE LOCATE FOR PAGE     */
*       /* SPACE VOLUME SERIAL, DEVICE TYPE, ATTR1, AND AMDSB USING  */
*       /* THE DATA CONTROL INTERVAL NUMBER FOUND IN THE LOCATE      */
*       /* ABOVE. IF EITHER LOCATE FAILS, TAKE AN ERROR EXIT.        */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0281
*       IF R15=SUCCESS              /* CHECK 1ST LOCATE SUCCESSFUL   */
*           &FPL1PTR->CTGFLPT(ONE)^=ZERO&FPL2PTR->CTGFLPT(ONE)^=ZERO
*                                   /* CHECK DSNAME NOT IN CVOL      */
*           &(PSPACEDS=YES&SWPAGE=NO)&(DSTYPED=DATACHAR) THEN/* CHECK
*                                      PAGE SPACE AND CHECK DATA TYPE*/
         SLR   @14,@14                                             0281
         CR    R15,@14                                             0281
         BNE   @RF00281                                            0281
         L     @07,FPL1PTR                                         0281
         C     @14,CTGFLPT(,@07)                                   0281
         BE    @RF00281                                            0281
         L     @07,FPL2PTR                                         0281
         C     @14,CTGFLPT(,@07)                                   0281
         BE    @RF00281                                            0281
         L     @14,FPL2DPTR                                        0281
         TM    PSPACEDS(@14),B'00000001'                           0281
         BNO   @RF00281                                            0281
         TM    SWPAGE(@14),B'00000010'                             0281
         BNZ   @RF00281                                            0281
         L     @14,FPL1DPTR                                        0281
         CLI   DSTYPED(@14),C'D'                                   0281
         BNE   @RF00281                                            0281
*         DO;                       /* FIRST LOCATE SUCCESSFUL       */
*           RFY                                                    0283
*             CTGPL BASED(ADDR(RIMCPL02));/* SET ADDRESSABILITY      */
*           CTGENT=ADDR(DATACI);    /* GET CI NUMBER                 */
         LA    @07,RIMCPL02                                        0284
         LA    @14,DATACI(,@14)                                    0284
         ST    @14,CTGENT(,@07)                                    0284
*           R1=ADDR(RIMCPL02);      /* GET CATALOG PARMLIST ADDR     */
         LR    R1,@07                                              0285
*           SVC(26);                                               0286
         SVC   26                                                  0286
*           IF R15^=SUCCESS×NOTUSE=ON THEN/* 2ND LOCATE SUCCESSFUL?  */
         LTR   R15,R15                                             0287
         BNZ   @RT00287                                            0287
         L     @14,FPL5DPTR                                        0287
         TM    NOTUSE(@14),B'00000001'                             0287
         BNO   @RF00287                                            0287
@RT00287 DS    0H                                                  0288
*             RTCODE=LOCFAILD;      /* SET LOCATE FAILED             */
         MVC   RTCODE(4),@CF00488                                  0288
*         END;                                                     0289
*       ELSE                                                       0290
*         RTCODE=LOCFAILD;          /* FIRST LOCATE FAILED           */
         B     @RC00281                                            0290
@RF00281 MVC   RTCODE(4),@CF00488                                  0290
*       RFY                                                        0291
*        (R1,                                                      0291
*         R15) UNRSTD;              /* RELEASE REG1 AND REG15        */
@RC00281 DS    0H                                                  0292
*       IF RTCODE^=SUCCESS THEN     /* A LOCATE FAILED               */
         L     @15,RTCODE                                          0292
         LTR   @15,@15                                             0292
         BNZ   @RT00292                                            0292
*         GOTO OPSEXIT;             /* ERROR EXIT-RETURN TO CALLER   */
*     END;                                                         0294
*   ELSE                            /* LOCATE A SWAP SPACE           */
*                                                                  0295
*/********************************************************************/
*/*                                                                  */
*/*  LOCSWAP: ISSUE LOCATES FOR A SWAP DATA SET                      */
*/*                                                                  */
*/*  INPUT: CATALOG PARAMETER LISTS AND FIELD PARAMETER LISTS ARE    */
*/*         PREVIOUSLY SET UP BY 'LOCPREP'.                          */
*/*                                                                  */
*/*  PROCESS: AN ATTEMP IS MADE TO LOCATE A SWAP DATA SET NAMED      */
*/*           IN THE INPUT PARAMETER LIST.                           */
*/*                                                                  */
*/*           FIRST LOCATE CLUSTER BY NAME AND VERIFY IT IS A SWAP   */
*/*           DATA SET. IF IT IS SUCCESSFUL,A SECOND LOCATE IS ISSUED*/
*/*           ON THE DATA CONTROL INTERVAL NUMBER TO GET THE VOLUME  */
*/*           SERIAL NUMBER,DEVICE TYPE,DSATTR AND AMDSB FOR THE SWAP*/
*/*           DATA SET.                                              */
*/*                                                                  */
*/*           IF EITHER LOCATE IS UNSUCCESSFUL,OR THE DATA SET IS    */
*/*           NOT A VALID SWAP SPACE A NON-ZERO RETURN CODE WILL BE  */
*/*           RETURNED TO THE CALLER. PROCESSING IS TERMINATED       */
*/*                                                                  */
*/*  OUTPUT:  THE REQUIRED INFORMATION TO OPEN  A  SWAP SPACE        */
*/*                                                                  */
*/********************************************************************/
*/*LOCSWAP:    CHART                                                 */
*/*            N    SECTION TO ISSUE LOCATE FOR A SWAP SPACE         */
*/*            P    SET ADDRESS OF CATALOG PARM LIST IN REG 1        */
*/*            L    SVC26:LOCATE CLUSTER BY DSNAME                   */
*/*            D    (YES,,NO,%LOSFAIL) LOCATE CLUSTER SUCCESSFUL?    */
*/*            D    (YES,,NO,%LOSFAIL) DSNAME NOT IN CVOL?           */
*/*            D    (YES,,NO,%LOSFAIL) DATA SET REALLY A SWAP SPACE? */
*/*            D    (YES,,NO,%LOSFAIL) DATA SET IS A 'DATA' TYPE?    */
*/*            P    SET ADDR OF DATA CI IN CATLOG PARM LIST          */
*/*            L    SVC26:LOCATE INFO BY CI NUMBER                   */
*/*            D    (YES,,NO,%LOSFAIL) LOCATE INFO SUCCESSFUL?       */
*/*            D    (YES,,NO,%LOSFAIL)  NO TRACK OVERFLOW?           */
*/*            D     (YES,%LOSFAIL,NO,) NOT USABLE FLAG ON?          */
*/*            R    END OF LOCATE SWAP SECTION                       */
*/*%LOSFAIL:   P    SET RETURN CODE(12)-LOCATE FAIL                  */
*/*            R    RETURN TO CALLER                                 */
*/*LOCSWAP:    END                                                   */
*                                                                  0295
*     DO;                           /* LOCATE A SWAP SPACE           */
         B     @RC00276                                            0295
@RF00276 DS    0H                                                  0296
*/* LOCSWAP-LOCATE A SWAP SPACE                                      */
*                                                                  0296
*       RFY                                                        0296
*        (R1,                                                      0296
*         R15) RSTD;                /* RESTRICT REG 1 AND REG15      */
*       R1=ADDR(RIMCPL01);          /* SET CATALOG PARM LIST         */
         LA    R1,RIMCPL01                                         0297
*       SVC(26);                    /* ISSUE 1ST LOCATE-CLUSTER      */
         SVC   26                                                  0298
*       IF R15=SUCCESS&FPL1PTR->CTGFLPT(ONE)^=ZERO&/* 1ST LOCATE   0299
*                                      SUCCESSFUL?                   */
*           FPL2PTR->CTGFLPT(ONE)^=ZERO THEN/* CHECK DSNAME NOT IN 0299
*                                      CVOL                          */
         SLR   @14,@14                                             0299
         CR    R15,@14                                             0299
         BNE   @RF00299                                            0299
         L     @07,FPL1PTR                                         0299
         C     @14,CTGFLPT(,@07)                                   0299
         BE    @RF00299                                            0299
         L     @07,FPL2PTR                                         0299
         C     @14,CTGFLPT(,@07)                                   0299
         BE    @RF00299                                            0299
*         IF(PSPACEDS=YES&SWPAGE=YES&/* CHECK SWAP SPACE             */
*             DSTYPED=DATACHAR) THEN/* CHECK DATA TYPE               */
         L     @14,FPL2DPTR                                        0300
         TM    PSPACEDS(@14),B'00000011'                           0300
         BNO   @RF00300                                            0300
         L     @14,FPL1DPTR                                        0300
         CLI   DSTYPED(@14),C'D'                                   0300
         BNE   @RF00300                                            0300
*           DO;                     /* ABOVE CHECK SUCCESSFUL        */
*             RFY                                                  0302
*               CTGPL BASED(ADDR(RIMCPL02));/* SET ADDRESSABILITY    */
*             CTGENT=ADDR(DATACI);  /* GET CONTROL INTERVAL NUMBER   */
         LA    @07,RIMCPL02                                        0303
         LA    @14,DATACI(,@14)                                    0303
         ST    @14,CTGENT(,@07)                                    0303
*             R1=ADDR(RIMCPL02);    /* SET CATALOG CI PARM LIST      */
         LR    R1,@07                                              0304
*             SVC(26);              /* ISSUE 2ND LOCATE-CI           */
         SVC   26                                                  0305
*             IF R15=SUCCESS THEN   /* CHECK 2ND LOCATE SUCCESSFUL   */
         LTR   R15,R15                                             0306
         BNZ   @RF00306                                            0306
*               IF TRACKOVL=ON×NOTUSE=ON THEN/* CHECK FOR TRACK    0307
*                                      OVERFLOW OR SPACE NOT USABLE  */
         L     @14,FPL5DPTR                                        0307
         TM    TRACKOVL(@14),B'00000001'                           0307
         BO    @RT00307                                            0307
         TM    NOTUSE(@14),B'00000001'                             0307
         BNO   @RF00307                                            0307
@RT00307 DS    0H                                                  0308
*                 RTCODE=LOCFAILD;  /* FAILS-SET LOCATE ERROR        */
         MVC   RTCODE(4),@CF00488                                  0308
*               ELSE                                               0309
*                 ;                 /* NO TRACK OVERFLOW AND SPACE 0309
*                                      USABLE                        */
@RF00307 DS    0H                                                  0310
*             ELSE                                                 0310
*               RTCODE=LOCFAILD;    /* 2ND LOCATE FAILED             */
         B     @RC00306                                            0310
@RF00306 MVC   RTCODE(4),@CF00488                                  0310
