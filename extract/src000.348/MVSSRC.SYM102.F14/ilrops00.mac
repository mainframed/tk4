*                                                                  0458
*   IF AFTERNIP=OFF THEN            /* PROCESSING IN NIP TIME?       */
         TM    AFTERNIP(PARMPTR),B'01000000'                       0458
         BNZ   @RF00458                                            0458
*     DO;                           /* YES - IN NIP TIME             */
*       UCBALOC=YES;                /* ALLOCATED                     */
         L     @15,UCBPTR                                          0460
         OI    UCBALOC(@15),B'00001000'                            0460
*       UCBUSER=UCBUSER+ONE;        /* INCREMENT USE COUNT IN UCS    */
         LA    @14,1                                               0461
         SLR   @07,@07                                             0461
         IC    @07,UCBUSER(,@15)                                   0461
         ALR   @14,@07                                             0461
         STC   @14,UCBUSER(,@15)                                   0461
*     END;                          /* END OF INFORMATION FOR NIP  0462
*                                      TIME                          */
*   UCBPGFL=YES;                    /* OPEN PAGE/SWAP SPACE ON VOLUME*/
@RF00458 L     @15,UCBPTR                                          0463
         OI    UCBPGFL(@15),B'01000000'                            0463
