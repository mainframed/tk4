*/********************************************************************/
*/*                                                                  */
*/* INITIOSB:  PROCEDURE TO INITIALIZE IOSB-SRB PAIR ASSOCIATED      */
*/*            WITH THE I/O REQUEST BLOCK(IORB).                     */
*/*                                                                  */
*/* INPUT:     POINTER TO END OF USED STORAGE                        */
*/*            POINTER TO IORB                                       */
*/*            CATALOG INFORMATION                                   */
*/*            PSAAOLD  (ASCB OLD PTR IN PSA)                        */
*/*            ASMVT    (FOR I/O TERMINATION,NORMAL,ABNORMAL ADDRESS)*/
*/*                                                                  */
*/* PROCESS:   INITIALIZE A IOSB-SRB IN THE NEXT AVAILABLE AREA OF   */
*/*            STORAGE. GENERALLY THIS ENTAILS MOVING DATA FROM THE  */
*/*            INPUT SOURCES TO THE IOSB OR SRB.                     */
*/*                                                                  */
*/* OUTPUT:    AN INITIALIZED IOSB-SRB PAIR                          */
*/*                                                                  */
*/********************************************************************/
*/*INITIOSB: CHART                                                   */
*/*INITIOSB: E    INITIALIZE THE IOSB                                */
*/*          P    SAVE RETURN REGISTER                               */
*/*          P    INIT IOSBPTR TO NEXT AVAILABLE STORAGE             */
*/*          P    INIT IOSB DRIVER ID FIELD                          */
*/*          P    INIT IOSB FILE MASK                                */
*/*          P    INIT ASCB ADDRESS-MASTER SCHEDULER                 */
*/*          P    INIT IOSB ASID                                     */
*/*          P    INIT IORB POINTER IN IOSB                          */
*/*          P    INIT I/O TERMINATION ADDRESS                       */
*/*          P    INIT POINTER TO NORMAL END APPENDAGE               */
*/*          P    INIT POINTER TO ABNORMAL END APPENDAGE             */
*/*          P    INIT IOS DIE ADDRESS                               */
*/*          P    INIT ASYNCHRONOUS SCHEDULING FLAG                  */
*/*          P    SET POST ON CONDITION CODE 3 FLAG                  */
*/*          P    INDICATE POST STATUS TO RUN WITHOUT LOCAL LOCK     */
*/*          P    UPDATE POINTER TO NEXT AVAILABLE STORAGE           */
*/*          P    INIT INTERNAL SRB POINTER                          */
*/*          P    INIT SRB ADDRESS IN IOSB                           */
*/*          N    INITIALIZE SRB FIELDS                              */
*/*          P    INITIALIZE SRB IDENTIFIER                          */
*/*          P    SET SRB PARAMETER TO IOSB POINTER                  */
*/*          P    SET SRB PRIORITY LEVEL (LOCAL)                     */
*/*          P    UPDATE POINTER TO NEXT AVAILABLE STORAGE           */
*/*          P    RESTORE RETURN REGISTER                            */
*/*          R    RETURN TO INITIORB                                 */
*/*INITIOSB: END                                                     */
*                                                                  0568
*INITIOSB:                          /* INITIALIZE IOSB               */
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0568
INITIOSB DS    0H                                                  0569
*   RFY                                                            0569
*     R14 RSTD;                                                    0569
*   SAVEPTR=R14;                    /* SAVE REG 14                   */
         LR    SAVEPTR,R14                                         0570
*   RFY                                                            0571
*     R14 UNRSTD;                                                  0571
*   IOSBPTR=COREADDR;               /* NEXT AVAILABLE STORAGE        */
*                                                                  0572
         LR    IOSBPTR,COREADDR                                    0572
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE IOSB FIELDS F0R CURRENT IORB.                      */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0573
*   IOSDVRID=IOSMISID;              /* MISCELLANEOUS DRVR ID('01')   */
         MVI   IOSDVRID(IOSBPTR),X'01'                             0573
*   IOSFMSK=FILEMASK;               /* PAGE SPACE FILE MASK          */
         MVC   IOSFMSK(1,IOSBPTR),FILEMASK                         0574
*   ASCBPTR=PSAAOLD;                /* SET ADDRESSABILITY TO ASCB    */
         L     ASCBPTR,PSAAOLD                                     0575
*   IOSASID=ASCBASID;               /* ASM'S ASID                    */
         MVC   IOSASID(2,IOSBPTR),ASCBASID(ASCBPTR)                0576
*   IOSUSE=IORBPTR;                 /* IORB ADDRESS                  */
         ST    IORBPTR,IOSUSE(,IOSBPTR)                            0577
*   IOSPGAD=ASMPCMP;                /* I/O TERMINATION ADDRESS       */
         L     @07,ASMVTPTR                                        0578
         L     @15,ASMPCMP(,@07)                                   0578
         ST    @15,IOSPGAD(,IOSBPTR)                               0578
*   IOSNRM=ASMPCMPN;                /* NORMAL END APPENDAGE          */
         L     @15,ASMPCMPN(,@07)                                  0579
         ST    @15,IOSNRM(,IOSBPTR)                                0579
*   IOSABN=ASMPCMPA;                /* ABNORMAL END APPENDAGE        */
         L     @15,ASMPCMPA(,@07)                                  0580
         ST    @15,IOSABN(,IOSBPTR)                                0580
*   IOSIDR=ON;                      /* ASYNCHRONOUS SCHEDULE FLAG    */
         OI    IOSIDR(IOSBPTR),B'00000100'                         0581
*   IOSCC3WE=ON;                    /* CC3 POST FLAG                 */
         OI    IOSCC3WE(IOSBPTR),B'00100000'                       0582
*   IOSOPT=NOLOCLK;                 /* NO LOCAL LOCK INDICATOR       */
         MVC   IOSOPT(1,IOSBPTR),NOLOCLK                           0583
*   IOSDIE=ASMPCMPD;                /* SET IOS DIE ADDRESS           */
         L     @07,ASMPCMPD(,@07)                                  0584
         ST    @07,IOSDIE(,IOSBPTR)                                0584
*   IOSNOTRS=ON;                    /* SET NOTRAS FLAG       @ZA25907*/
         OI    IOSNOTRS(IOSBPTR),B'00100000'                       0585
*   COREADDR=COREADDR+IOSBLGH;      /* UPDATE FREE STG ADDRESS       */
         SLR   @07,@07                                             0586
         IC    @07,IOSBLGH                                         0586
         ALR   COREADDR,@07                                        0586
*   SRBPTR=COREADDR;                /* NEXT AVAILABLE STORAGE        */
         LR    SRBPTR,COREADDR                                     0587
*   IOSSRB=SRBPTR;                  /* SRB ADDRESS INTO IOSB         */
*                                                                  0588
         ST    SRBPTR,IOSSRB(,IOSBPTR)                             0588
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE SRB FIELDS F0R CURRENT IORB-IOSB.                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0589
*   SRBID=SRBIDC;                   /* SRB IDENTIFIER FIELD          */
         MVC   SRBID(4,SRBPTR),SRBIDC                              0589
*   SRBPARM=IOSBPTR;                /* ASSOCIATED IOSB AS PARM       */
         ST    IOSBPTR,SRBPARM(,SRBPTR)                            0590
*   SRBPRIOR=NQUISCFG;              /* NON-QUIESCEABLE PRIORITY      */
         IC    @07,NQUISCFG                                        0591
         STC   @07,SRBPRIOR(,SRBPTR)                               0591
*   COREADDR=COREADDR+SRBLGH;       /* UPDATE FREE STG ADDRESS       */
         SLR   @04,@04                                             0592
         IC    @04,SRBLGH                                          0592
         ALR   COREADDR,@04                                        0592
*   RFY                                                            0593
*     R14 RSTD;                                                    0593
*   R14=SAVEPTR;                    /* RESTORE REG 14                */
         LR    R14,SAVEPTR                                         0594
*   RFY                                                            0595
*     R14 UNRSTD;                                                  0595
*   END INITIOSB;                                                  0596
