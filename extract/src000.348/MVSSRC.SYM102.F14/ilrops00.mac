*/*                                                                  */
*/* INITPCCW:  SUBSECTION TO INITIALIZE THE PAGING CHANNEL COMMAND   */
*/*            WORKAREA(PCCW).                                       */
*/*                                                                  */
*/* INPUT:     POINTER TO NUCLEUS BUFFER AREA ACQUIRED               */
*/*            POINTER TO ASMVT                                      */
*/*                                                                  */
*/* PROCESS:   BUILD THE CCW STRING IN A PCCW WHICH IS USED TO READ  */
*/*            OR WRITE ONE PAGE. THE ADDRESS PORTIONS OF THE CCWS   */
*/*            ARE INITIALIZED WITH THE EXCEPTION OF THE READ/WRITE  */
*/*            CCW. NOTE THAT THE SAME CCW STRING WILL BE USED FOR A */
*/*            READ OR WRITE.                                        */
*/*                                                                  */
*/*            THE PCCWS ARE CHAINED TO EACH OTHER. THE FIRST PCCW   */
*/*            IS CHAINED TO THE PCCW QUEUE IN THE ASMVT.            */
*/*                                                                  */
*/* OUTPUT:    INITIALIZED PCCWS IN THE NUCLEUS BUFFER AREA          */
*/*            THE ASMVT PCCW QUEUE INCREMENTED BY THE NUMBER OF     */
*/*            THIS PAGE SPACE'S PCCWS.                              */
*/*                                                                  */
*/********************************************************************/
*/*INITPCCW: CHART                                                   */
*/*INITPCCW: N INITIALIZE THE PCCWS                                  */
*/*          P    INIT ASMVTPTR FROM CVT                             */
*/*          P    SET POINTER TO FIRST PCCW                          */
*/*          P    SET NUMBER OF PCCWS TO PROCESS                     */
*/*%PCC10:   D    (YES,,NO,%PCC20) ANOTHER PCCW TO PROCESS ?         */
*/*          P    COPY MODEL PCCWS IN THIS PCCW                      */
*/*          P    POINT SEEK HEAD CCW TO CCHHR                       */
*/*          P    POINT 2ND(SET SECTOR) CCW TO SECTOR VALUE          */
*/*          P    POINT SEARCH CCW TO CCHHR                          */
*/*          P    POINT 4TH(TIC) CCW TO SEARCH CCW                   */
*/*          P    INIT PCCW CHAINING FIELD (PCCWPCCW)                */
*/*          P    (,%PCC10) UPDATE POINTER TO NEXT PCCW              */
*/*%PCC20:   P    RESTORE POINTER TO LAST PCCW                       */
*/*          P    MOVE ASMVT QUEUE ANCHOR TO NEXT PTR IN LAST PCCW   */
*/*          P    SET FIRST PCCW AS QUEUE HEADER                     */
*/*          P    ADD PCCW NUMBER TO ASMVT PCCW COUNT                */
*/*%PCCW30:  R    END OF INITIALIZE PCCW SECTION                     */
*/*INITPCCW: END                                                     */
*/*INITPCCW-INITIALIZE PAGE CHANNEL COMMAND WORKAREA                 */
*                                                                  0465
*   IF PAGESP=ON&AFTERNIP=OFF THEN  /* PROCESS PAGE SPACE IN NIP?    */
         TM    PAGESP(PARMPTR),B'10000000'                         0465
         BNO   @RF00465                                            0465
         TM    AFTERNIP(PARMPTR),B'01000000'                       0465
         BNZ   @RF00465                                            0465
*     DO;                           /* YES-PROCESS PAGE SPACE IN NIP */
*       WRKPTR=PCCWPTR;             /* POINTER TO FIRST PCCW         */
         LR    WRKPTR,PCCWPTR                                      0467
*       DO I=1 TO BURSTNO;          /* PROCESS ALL PCCWS             */
         LA    @07,1                                               0468
         B     @DE00468                                            0468
@DL00468 DS    0H                                                  0469
*         PCCW=SKELETON;            /* FILL IN CONSTANT VALUES       */
         MVC   PCCW(72,PCCWPTR),SKELETON                           0469
*         PCCWSKAD=ADDR(PCCWBB);    /* INIT SEEK ARGUMENT            */
         LA    @15,PCCWBB(,PCCWPTR)                                0470
         STCM  @15,7,PCCWSKAD(PCCWPTR)                             0470
*         PCCWSSAD=ADDR(PCCWSECT);  /* INIT SET SECTOR ARGUMENT      */
         LA    @15,PCCWSECT(,PCCWPTR)                              0471
         STCM  @15,7,PCCWSSAD(PCCWPTR)                             0471
*         PCCWSIAD=ADDR(PCCWCC);    /* INIT SEARCH ID ARG            */
         LA    @15,PCCWCC(,PCCWPTR)                                0472
         STCM  @15,7,PCCWSIAD(PCCWPTR)                             0472
*         PCCWTAD=ADDR(PCCWSRCH);   /* INIT TIC ADDRESS              */
         LA    @15,PCCWSRCH(,PCCWPTR)                              0473
         STCM  @15,7,PCCWTAD(PCCWPTR)                              0473
*         PCCWPCCW=PCCWPTR+LENGTH(PCCW);/* INITIALIZE CHAIN FIELD    */
         LA    @15,72                                              0474
         ALR   @15,PCCWPTR                                         0474
         ST    @15,PCCWPCCW(,PCCWPTR)                              0474
*         PCCWPTR=PCCWPCCW;         /* POINT TO NEXT PCCW            */
         LR    PCCWPTR,@15                                         0475
*       END;                                                       0476
         AL    @07,@CF00036                                        0476
@DE00468 STC   @07,I                                               0476
         CH    @07,BURSTNO                                         0476
         BNH   @DL00468                                            0476
*       PCCWPTR=PCCWPTR-LENGTH(PCCW);/* BACK UP TO LAST PCCW         */
         SL    PCCWPTR,@CF00342                                    0477
*       PCCWPCCW=ASMPCCWA;          /* ADD QUEUE TO END OF CHAIN     */
         L     @07,ASMVTPTR                                        0478
         L     @15,ASMPCCWA(,@07)                                  0478
         ST    @15,PCCWPCCW(,PCCWPTR)                              0478
*       ASMPCCWA=WRKPTR;            /* SET FIRST PCCW IN CHAIN AS  0479
*                                      ASMVT PCCW QUEUE ANCHOR       */
         ST    WRKPTR,ASMPCCWA(,@07)                               0479
*       ASMPCCWN=ASMPCCWN+BURSTNO;  /* ADD PCCWS TO PCCW COUNT       */
         LH    @12,BURSTNO                                         0480
         AL    @12,ASMPCCWN(,@07)                                  0480
         ST    @12,ASMPCCWN(,@07)                                  0480
*     END;                          /* END OF BUILDING PCCWS FOR PAGE
*                                      SPACE                         */
*                                                                  0482
*/********************************************************************/
*/*                                                                  */
*/* INITIORB:  SUBSECTION TO INITIALIZE THE I/O REQUEST BLOCK        */
*/*                                                                  */
*/* INPUT:     POINTER TO NEXT AVAILABLE STORAGE                     */
*/*            CATALOG INFORMATION                                   */
*/*            POINTER TO AMB                                        */
*/*            NUMBER OF IORB(S) REQUIRED                            */
*/*                                                                  */
*/* PROCESS:   THE IORB IS INITIALIZED WITH DATA ACQUIRED FROM THE   */
*/*            INPUT SOURCES. ALSO, POINTERS TO IOS SAVE AREA, THE   */
*/*            NEXT IORB, AND THE ASSOCIATED IOSB ARE INITIALIZED.   */
*/*                                                                  */
*/* OUTPUT:    A COMPLETED IORB(S)                                   */
*/*                                                                  */
*/********************************************************************/
*/*INITIORB: CHART                                                   */
*/*INITIORB: N    INITIALIZE THE I/O REQUEST BLOCK                   */
*/*          M    INIT INTERNAL IORB POINTER                         */
*/*%IOR10:   D    (YES,,NO,%IOR20) ANOTHER IORB TO PROCESS ?         */
*/*          P    INIT IORB IDENTIFIER FIELD                         */
*/*          P    INIT NUMBER OF IORBS                               */
*/*          D    (YES,,NO,%IOR25) OPENING AN RPS DEVICE ?           */
*/*          P    SET RPS DEVICE FLAG ON                             */
*/*%IOR25:    D     (YES,,NO,%IOR30) OPENING AN SWAP DATA SET?       */
*/*          P      SET SWAP DATA SET FLAG                           */
*/*%IOR30:   P    UPDATE POINTER TO NEXT AVAILABLE CORE              */
*/*          P    INIT IOS SAVE AREA POINTER                         */
*/*          P    UPDATE POINTER TO NEXT AVAILABLE CORE              */
*/*          P    INIT POINTER TO IOSB                               */
*/*          S    INITIOSB: INITIALIZE IOSB AND SRB                  */
*/*          D    (YES,,NO,%IOR40) IS THIS LAST IORB ?               */
*/*          P    (,%IOR10) ZERO CHAIN POINTER TO NEXT IORB          */
*/*%IOR40:   P    INIT CHAIN POINTER TO NEXT IORB                    */
*/*          P    (,%IOR10) UPDATE PTR TO NEXT IORB                  */
*/*%IOR20:   R    END OF INITIALIZE IORB SECTION                     */
*/*INITIORB: END                                                     */
*                                                                  0482
*                                                                  0482
*/* INITIORB        INITIALIZE IORB                                  */
*                                                                  0482
*   IORBPTR=COREADDR;               /* NEXT AVAILABLE STORAGE        */
@RF00465 LR    IORBPTR,COREADDR                                    0482
*   OUIORBPR=IORBPTR;               /* SET IORB PTR IN OUTPUT PARM   */
         ST    IORBPTR,OUIORBPR(,PARMPTR)                          0483
*   DO I=1 TO STRNGNO;              /* PROCESS EACH IORB STRING      */
         LA    @12,1                                               0484
         B     @DE00484                                            0484
@DL00484 DS    0H                                                  0485
*     IORID=IORBIDC;                /* INIT IDENTIFIER FIELD         */
         MVC   IORID(1,IORBPTR),IORBIDC                            0485
*     IORNUM=STRNGNO;               /* NBR OF IORBS IN PAGE SPACE    */
         LH    @12,STRNGNO                                         0486
         STC   @12,IORNUM(,IORBPTR)                                0486
*     IF UCBRPS=ON THEN             /* RPS DEVICE ?                  */
         L     @12,UCBPTR                                          0487
         TM    UCBRPS(@12),B'00010000'                             0487
         BNO   @RF00487                                            0487
*       IORFRPS=ON;                 /* YES - SO INDICATE             */
         OI    IORFRPS(IORBPTR),B'01000000'                        0488
*     IF PAGESP=OFF THEN            /* PROCESSING SWAP DATA SET?     */
@RF00487 TM    PAGESP(PARMPTR),B'10000000'                         0489
         BNZ   @RF00489                                            0489
*       IORSWAP=ON;                 /* SET SWAP DATA SET FLAG        */
         OI    IORSWAP(IORBPTR),B'00001000'                        0490
*     COREADDR=COREADDR+IORBLGH;    /* UPDATE STORAGE ADDRESS        */
@RF00489 SLR   @12,@12                                             0491
         IC    @12,IORBLGH                                         0491
         ALR   COREADDR,@12                                        0491
*     IORSAVE=COREADDR;             /* IOS SAVE AREA                 */
         ST    COREADDR,IORSAVE(,IORBPTR)                          0492
*     COREADDR=COREADDR+SAVALGH;    /* UPDATE FOR SAVE AREA          */
         SLR   @12,@12                                             0493
         IC    @12,SAVALGH                                         0493
         ALR   COREADDR,@12                                        0493
*     IORIOSB=COREADDR;             /* I/O STATUS BLOCK ADDRESS      */
         ST    COREADDR,IORIOSB(,IORBPTR)                          0494
*     CALL INITIOSB;                /* INITIALIZE IOSB-SRB PAIR      */
         BAL   @14,INITIOSB                                        0495
*     IF I=STRNGNO THEN             /* IS THIS LAST STRING ?         */
         SLR   @12,@12                                             0496
         IC    @12,I                                               0496
         CH    @12,STRNGNO                                         0496
         BNE   @RF00496                                            0496
*       IORIORB=ZERO;               /* NEXT IORB ADDRESS             */
         SLR   @12,@12                                             0497
         ST    @12,IORIORB(,IORBPTR)                               0497
*     ELSE                                                         0498
*       DO;                         /* LOOP FOR NEXT IORB            */
         B     @RC00496                                            0498
@RF00496 DS    0H                                                  0499
*         IORIORB=COREADDR;         /* NEXT IORB ADDRESS             */
         ST    COREADDR,IORIORB(,IORBPTR)                          0499
*         IORBPTR=IORIORB;          /* PROCESS NEXT IORB             */
         L     @12,IORIORB(,IORBPTR)                               0500
         LR    IORBPTR,@12                                         0500
*       END;                                                       0501
*   END;                                                           0502
@RC00496 LA    @12,1                                               0502
         SLR   @04,@04                                             0502
         IC    @04,I                                               0502
         ALR   @12,@04                                             0502
@DE00484 STC   @12,I                                               0502
         CH    @12,STRNGNO                                         0502
         BNH   @DL00484                                            0502
*                                                                  0503
