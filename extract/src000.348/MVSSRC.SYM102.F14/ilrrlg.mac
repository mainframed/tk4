         L     @12,AFSLNGTH                                        0267
         C     @12,@CF01428                                        0267
         BNE   @RF00267                                            0267
*     AFSSPID=AFSSP255;             /* SET SUBPOOL FOR LSQA          */
         MVC   AFSSPID(4),@CF01390                                 0268
*   ELSE                            /* I/O BUFFERS                   */
