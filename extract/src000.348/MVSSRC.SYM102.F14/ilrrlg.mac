*/********************************************************************/
*/*                                                                  */
*/*  A TEST IS NOW MADE FOR EXTENSIONS. IF THERE ARE EXTENSIONS,     */
*/*  THEY ARE FREED FIRST. FIRST ALL LPME EXTENSIONS FOR A PARTICULAR*/
*/*  ASST EXTENSION ARE FREED, THEN THAT ASST EXTENSION IS FREED.    */
*/*  THIS PROCESS IS REPEATED UNTIL ALL EXTENSIONS ARE FREED.        */
*/*                                                                  */
*/********************************************************************/
*                                                                  0270
*   IF AFSASPCT->ASPAEXCT^=AFSZERO  /* ARE THERE ANY EXTENSIONS?     */
*     THEN                                                         0270
@RC00267 L     @12,AFSASPCT                                        0270
         LH    @11,ASPAEXCT(,@12)                                  0270
         LTR   @11,@11                                             0270
         BZ    @RF00270                                            0270
*     DO;                           /* YES                           */
*       AFSB=AFSONE;                /* SET BASE SUBSCRIPT FOR FIRST
*                                      ASST EXTENSION                */
         LA    @07,1                                               0272
         STH   @07,AFSB                                            0272
*       AFSA=AFSTWO;                /* SET ASST SUBSCRIPT FOR FIRST
*                                      LPME EXTENSION OFF FIRST ASST
*                                      EXTENSION. GET PTR TO FIRST 0273
*                                      ASST EXT                      */
         LA    AFSA,2                                              0273
*       AFSEXPTR=AFSASPCT->ASPASSTP(AFSB);                         0274
         SLA   @07,2                                               0274
         L     AFSEXPTR,ASPASSTP-4(@07,@12)                        0274
*       DO WHILE AFSEXPTR^=AFSZERO; /* LOOP UNTIL WE HAVE PROCESSED
*                                      ALL ASST EXTENSIONS GET PTR TO
*                                      LPME EXT                      */
         B     @DE00275                                            0275
@DL00275 DS    0H                                                  0276
*         AFSWORK=AFSEXPTR->ASPASSTE(AFSA);                        0276
         LR    @07,AFSA                                            0276
         SLA   @07,2                                               0276
         L     AFSWORK,ASPASSTE-4(@07,AFSEXPTR)                    0276
*         DO WHILE AFSWORK^=AFSZERO;/* LOOP UNTIL WE HAVE PROCESSED
*                                      ALL LPME EXTENSIONS FOR A   0277
*                                      PARTICULAR ASST EXTENSION     */
         B     @DE00277                                            0277
@DL00277 DS    0H                                                  0278
*           CALL AFSFREE;           /* FREE AN LPME EXTENSION        */
         BAL   @14,AFSFREE                                         0278
*           RFY                                                    0279
*             R15 RSTD;                                            0279
*           IF R15=AFSZERO          /* SUCCESSFUL FREEMAIN?          */
*             THEN                                                 0280
         LTR   R15,R15                                             0280
         BNZ   @RF00280                                            0280
*             DO;                   /* YES                           */
*               RFY                                                0282
*                 R15 UNRSTD;                                      0282
*               AFSA=AFSA+AFSONE;   /* INCREMENT ASST SUBSCRIPT      */
         AL    AFSA,@CF00055                                       0283
*               IF AFSA<AFS257      /* REACHED END OF ARRAY          */
*                 THEN              /* NO, GET PTR TO NEXT LPME EX   */
         C     AFSA,@CF01392                                       0284
         BNL   @RF00284                                            0284
*                 AFSWORK=AFSEXPTR->ASPASSTE(AFSA);                0285
         LR    @07,AFSA                                            0285
         SLA   @07,2                                               0285
         L     AFSWORK,ASPASSTE-4(@07,AFSEXPTR)                    0285
*               ELSE                /* YES, FORCE EXIT FROM LOOP     */
*                 AFSWORK=AFSZERO;                                 0286
         B     @RC00284                                            0286
@RF00284 SLR   AFSWORK,AFSWORK                                     0286
*             END;                  /* THIS IS THE LOGICAL END OF THE
*                                      DO WHILE LOOP FOR LPME      0287
*                                      EXTENSIONS. PHYSICAL END IS 0287
*                                      AFTER THE ERROR CODE PROCESS
*                                      ING FOR A NON-ZERO RETURN FROM
*                                      AFSFREE.                      */
         EJECT
*                                                                  0288
*/********************************************************************/
*/* THIS SECTION WILL PROCESS A NON-ZERO RETURN CODE FROM AFSFREE    */
*/* FOR LPME EXTENSIONS. PROCESSING CONSISTS OF GETTING THE POINTER  */
*/* TO THE NEXT LPME EXTENSION, AND VERIFYING THAT IT IS AN LPME     */
*/* EXTENSION. IF IT IS WE WILL CONTINUE FREEING LPME EXTENSIONS,    */
*/* IF NOT, WE WILL FORCE AN EXIT FROM THE LOOP.                     */
*/********************************************************************/
*                                                                  0288
*           ELSE                                                   0288
*             DO;                   /* NON-ZERO RETURN FROM AFSFREE  */
         B     @RC00280                                            0288
@RF00280 DS    0H                                                  0289
*               AFSA=AFSA+AFSONE;   /* INCREMENT ASST SUBSCRIPT      */
         AL    AFSA,@CF00055                                       0289
*               IF AFSA<AFS257      /* REACHED END OF ASST ARRAY?    */
*                 THEN                                             0290
         C     AFSA,@CF01392                                       0290
         BNL   @RF00290                                            0290
*                 DO;               /* NO GET PTR TO NEXT LPME EXT   */
*                   AFSWORK=AFSEXPTR->ASPASSTE(AFSA);/* IS THIS A  0292
*                                      VALID LPME EXT?               */
         LR    @07,AFSA                                            0292
         SLA   @07,2                                               0292
         L     AFSWORK,ASPASSTE-4(@07,AFSEXPTR)                    0292
*                   IF AFSWORK->ASPEIDNT^='LPME' THEN/* NO           */
         CLC   ASPEIDNT(4,AFSWORK),@CC01401                        0293
         BE    @RF00293                                            0293
*                     AFSWORK=AFSZERO;/* FORCE LOOP EXIT             */
         SLR   AFSWORK,AFSWORK                                     0294
*                 END;                                             0295
*               ELSE                /* YES, REACHED END OF ARRAY     */
*                 AFSWORK=AFSZERO;  /* FORCE LOOP EXIT               */
         B     @RC00290                                            0296
@RF00290 SLR   AFSWORK,AFSWORK                                     0296
*             END;                                                 0297
@RC00290 DS    0H                                                  0298
*         END;                      /* THIS IS THE PHYSICAL END OF 0298
*                                      THE DO WHILE LOOP FOR LPME  0298
*                                      EXTENSIONS.                   */
@RC00280 DS    0H                                                  0298
@DE00277 LTR   AFSWORK,AFSWORK                                     0298
         BNZ   @DL00277                                            0298
         EJECT
*                                                                  0299
*/********************************************************************/
*/* THIS SECTION PROCESSES THE ASST EXTENSIONS                       */
*/********************************************************************/
*                                                                  0299
*         AFSWORK=AFSEXPTR;         /* SET FREEMAIN POINTER TO AN  0299
*                                      ASST EXTENSION                */
         LR    AFSWORK,AFSEXPTR                                    0299
*         CALL AFSFREE;             /* FREE THE ASST EXTENSION       */
         BAL   @14,AFSFREE                                         0300
*         RFY                                                      0301
*           R15 RSTD;                                              0301
*         IF R15=AFSZERO            /* FREEMAIN SUCCESSFUL?          */
*           THEN                                                   0302
         LTR   R15,R15                                             0302
         BNZ   @RF00302                                            0302
*           DO;                     /* YES                           */
*             RFY                                                  0304
*               R15 UNRSTD;                                        0304
*             AFSB=AFSB+AFSONE;     /* INCREMENT BASE SUBSCRIPT      */
         LA    @12,1                                               0305
         LH    @07,AFSB                                            0305
         ALR   @07,@12                                             0305
         STH   @07,AFSB                                            0305
*             IF AFSB<AFSFIVE       /* REACHED END OF ARRAY?         */
*               THEN                                               0306
         C     @07,@CF01047                                        0306
         BNL   @RF00306                                            0306
*               DO;                 /* NO GET PTR TO NEXT ASST EXT   */
*                 AFSEXPTR=AFSASPCT->ASPASSTP(AFSB);               0308
         SLA   @07,2                                               0308
         L     @01,AFSASPCT                                        0308
         L     AFSEXPTR,ASPASSTP-4(@07,@01)                        0308
*                 AFSA=AFSONE;      /* RESET ASST SUBSCRIPT          */
         LR    AFSA,@12                                            0309
*               END;                                               0310
*             ELSE                  /* YES, REACHED END OF ARRAY     */
*               AFSEXPTR=AFSZERO;   /* FORCE LOOP EXIT               */
         B     @RC00306                                            0311
@RF00306 SLR   AFSEXPTR,AFSEXPTR                                   0311
*           END;                    /* THIS THE LOGICAL END OF THE DO
*                                      WHILE LOOP FOR ASST         0312
*                                      EXTENSIONS. PHYSICAL END IS 0312
*                                      AFTER THE ERROR CODE PROCESS
*                                      ING FOR A NON-ZERO RETURN FROM
*                                      AFSFREE                       */
         EJECT
*                                                                  0313
*/********************************************************************/
*/* THIS SECTION WILL PROCESS A NON-ZERO RETURN CODE FROM AFSFREE    */
*/* FOR ASST EXTENSIONS. PROCESSING CONSISTS OF GETTING THE POINTER  */
*/* TO THE NEXT ASST EXTENSION, AND VERIFYING THAT IT IS AN ASST     */
*/* EXTENSION. IF IT IS WE WILL CONTINUE FREEING ASST EXTENSIONS,    */
*/* IF NOT, WE WILL FORCE AN EXIT FROM THE LOOP.                     */
*/********************************************************************/
*                                                                  0313
*         ELSE                                                     0313
*           DO;                     /* NON-ZERO RETURN CODE FROM   0313
*                                      AFSFREE                       */
         B     @RC00302                                            0313
@RF00302 DS    0H                                                  0314
*             AFSB=AFSB+AFSONE;     /* INCREMENT BASE SUBSCRIPT      */
         LA    @12,1                                               0314
         LH    @07,AFSB                                            0314
         ALR   @07,@12                                             0314
         STH   @07,AFSB                                            0314
*             IF AFSB<AFSFIVE       /* REACHED END OF ARRAY          */
*               THEN                                               0315
         C     @07,@CF01047                                        0315
         BNL   @RF00315                                            0315
*               DO;                 /* GET PTR TO NEXT ASST          */
*                 AFSEXPTR=AFSASPCT->ASPASSTP(AFSB);/* VALID ASST  0317
*                                      EXTENSION?                    */
         SLA   @07,2                                               0317
         L     @01,AFSASPCT                                        0317
         L     AFSEXPTR,ASPASSTP-4(@07,@01)                        0317
*                 IF AFSEXPTR->ASPEIDNT='ASST' THEN/* YES            */
         CLC   ASPEIDNT(4,AFSEXPTR),@CC01402                       0318
         BNE   @RF00318                                            0318
*                   AFSA=AFSONE;    /* RESET ASST SUBSCRIPT          */
         LR    AFSA,@12                                            0319
*                 ELSE              /* NO                            */
*                   AFSEXPTR=AFSZERO;/* FORCE LOOP EXIT              */
         B     @RC00318                                            0320
@RF00318 SLR   AFSEXPTR,AFSEXPTR                                   0320
*               END;                                               0321
*             ELSE                  /* YES, REACHED END OF ARRAY     */
*               AFSEXPTR=AFSZERO;   /* FORCE LOOP EXIT               */
         B     @RC00315                                            0322
@RF00315 SLR   AFSEXPTR,AFSEXPTR                                   0322
*           END;                                                   0323
@RC00315 DS    0H                                                  0324
*       END;                        /* PHYSICAL END OF DO WHILE LOOP
*                                      FOR ASST EXTENSIONS           */
@RC00302 DS    0H                                                  0324
@DE00275 LTR   AFSEXPTR,AFSEXPTR                                   0324
         BNZ   @DL00275                                            0324
*     END;                                                         0325
         EJECT
*                                                                  0326
*/********************************************************************/
*/* THIS SECTION FREES THE ASPCT BASE                                */
*/********************************************************************/
*                                                                  0326
*   AFSWORK=AFSASPCT;               /* GET PTR TO THE ASPCT BASE     */
@RF00270 L     AFSWORK,AFSASPCT                                    0326
*   CALL AFSFREE;                   /* FREE THE BASE                 */
         BAL   @14,AFSFREE                                         0327
         EJECT
*   RFY                                                            0328
*    (R3,                                                          0328
*     R4,                                                          0328
*     R7,                                                          0328
*     R14) RSTD;                                                   0328
*   R3=AFSREG3;                     /* RESTORE CALLER'S REG 3        */
         L     R3,AFSREG3                                          0329
*   R4=AFSREG4;                     /* RESTORE CALLER'S REG 4        */
         L     R4,AFSREG4                                          0330
*   R7=AFSREG7;                     /* RESTORE CALLER'S REG 7        */
         L     R7,AFSREG7                                          0331
*   R14=AFSRET1;                    /* GET RETURN ADDRESS            */
         L     R14,AFSRET1                                         0332
*   RETURN;                         /* RETURN TO CALLER              */
@EL00003 DS    0H                                                  0333
@EF00003 DS    0H                                                  0333
@ER00003 BR    @14                                                 0333
*   RFY                                                            0334
*    (R3,                                                          0334
*     R4,                                                          0334
*     R7,                                                          0334
*     R14) UNRSTD;                                                 0334
         EJECT
*                                                                  0335
*/********************************************************************/
*/*                                                                  */
*/*  AFSFREE:  FREE ASPCT EXTN OF STORAGE                            */
*/*                                                                  */
*/*  INPUT:  ADDRESS OF EXTN IN REGISTER 1                           */
*/*                                                                  */
*/*  PROCESS:  SET UP REGS, ISSUE FREEMAIN MACRO TO FREE ASPCT EXTN. */
*/*                                                                  */
*/*  OUTPUT:  RETURN CODE IN REGISTER 15                             */
*/*             00 - NORMAL RETURN                                   */
*/*             44 - FREEMAIN RETURNED ERROR CODE                    */
*/*                                                                  */
*/********************************************************************/
*/*AFSFREE: CHART (AREF)                                             */
*/*HEADER                                                          0335
*/*  ILRAFS00                                                      0335
*/* AFSFREE - FREEMAIN THE ASPCT                                     */
*/*AFSFREE: E  ENTRY                                                 */
*/*         P  SAVE THE RETURN ADDRESS                               */
*/*         N  THIS NEXT TEST AND RESULTING LEGS OF CODE, IS A MACRO */
*/*         N  PASS TEST AND EXPANSION.                              */
*/*         D  (YES,,NO,%AFS07) SVC OK?                              */
*/*         P  LOAD REG 14 WITH SUBPOOL ID                           */
*/*         P  LOAD REG 0 WITH LENGTH                                */
*/*         P  LOAD REG 1 WITH ADDRESS OF AREA TO BE FREED           */
*/*         S  (,%AFS08) FREEMAIN: FREE THE ASPCT                    */
*/*%AFS07:  P  LOAD REG 0 WITH LENTH                                 */
*/*         P  LOAD REG 1 WITH ADDRESS OF AREA TO BE FREED           */
*/*         P  LOAD REG 4 WITH CURRENT TCB ADDRESS                   */
*/*         P  LOAD REG 7 WITH CURRENT ASCB ADDRESS                  */
*/*         P  LOAD REG 3 WITH SUBPOOL ID                            */
*/*         S  IEAVGM00: FREE THE ASPCT                              */
*/*%AFS08:  D  (YES,%AFS09,NO,) ZERO RETURN CODE?                    */
*/*         P  SAVE FREEMAINS RETURN CODE                            */
*/*         P  SET RETURN CODE TO 68                                 */
*/*%AFS09:  R  RETURN TO MAINLINE                                    */
*                                                                  0335
         EJECT
*AFSFREE:                           /* FREEMAIN AN EXTN OF ASPCT     */
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);                          0335
AFSFREE  DS    0H                                                  0336
*   RFY                                                            0336
*     R14 RSTD;                                                    0336
*   AFSRET2=R14;                    /* SAVE THE RETURN ADDRESS       */
         ST    R14,AFSRET2                                         0337
*   RFY                                                            0338
*    (R0,                                                          0338
*     R1,                                                          0338
*     R3,                                                          0338
*     R4,                                                          0338
*     R7) RSTD;                                                    0338
*   R0=AFSLNGTH;                    /* GET THE LENGTH OF AREA TO BE
*                                      FREED                         */
         L     R0,AFSLNGTH                                         0339
*   R1=AFSWORK;                     /* GET ADDRESS OF AREA TO FREE   */
         LR    R1,AFSWORK                                          0340
*   R4=PSATOLD;                     /* GET CURRENT TCB ADDRESS       */
         L     R4,PSATOLD                                          0341
*   R7=PSAAOLD;                     /* GET CURRENT ASCB ADDRESS      */
         L     R7,PSAAOLD                                          0342
*   R3=AFSSPID;                     /* GET SUBPOOL ID                */
         L     R3,AFSSPID                                          0343
*   GENERATE REFS(R0,R1,R3,R4,R7,CVTCRMN,CVTMAP);                  0344
        FREEMAIN RC,A=(1),LV=(0),SP=(3),BRANCH=YES
*   RFY                                                            0345
*    (R0,                                                          0345
*     R1,                                                          0345
*     R3,                                                          0345
*     R4,                                                          0345
*     R7,                                                          0345
*     R14) UNRSTD;                                                 0345
*   RFY                                                            0346
*    (R14,                                                         0346
*     R15) RSTD;                                                   0346
*   IF R15^=AFSZERO                 /* FREEMAIN SUCCESSFUL?          */
*     THEN                          /* NO                            */
         LTR   R15,R15                                             0347
         BZ    @RF00347                                            0347
*     R15=AFS44;                    /* SET RETURN CODE TO 44         */
         LA    R15,44                                              0348
*   R14=AFSRET2;                    /* GET RETURN ADDRESS            */
@RF00347 L     R14,AFSRET2                                         0349
*   RFY                                                            0350
*    (R14,                                                         0350
*     R15) UNRSTD;                                                 0350
*   END AFSFREE;                                                   0351
@EL00004 DS    0H                                                  0351
@EF00004 DS    0H                                                  0351
@ER00004 BR    @14                                                 0351
*   END ILRAFS00;                                                  0352
