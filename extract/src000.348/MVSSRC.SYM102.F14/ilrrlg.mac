         TITLE 'ILRRLG - RELEASE LOGICAL GROUP EXECUTIVE               *
                        '
ILRRLG   CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'ILRRLG    76.315'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @10,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@10                                         0001
*                                                                  0059
*/********************************************************************/
*/*                                                                  */
*/*                           INITIALIZATION                         */
*/*                                                                  */
*/*  THE FOLLOWING POINTERS ARE INPUT TO THIS MODULE FROM            */
*/*  THE CALLER.                                                     */
*/*       REGISTER 1 = @ACE                                          */
*/*       REGISTER 2 = @RSMHD                                        */
*/*       REGISTER 3 = @ASMVT                                        */
*/*       REGISTER 4 = @EPATH                                        */
*/*       REGISTER 13= @SAVEAREA                                     */
*/*  REGISTERS ARE SAVED IN THE SAVEAREA POINTED TO BY REGISTER      */
*/*  13.  A WORKAREA IS OBTAINED, OR ELSE SET ERROR CODE AND RETURN. */
*/*                                                                  */
*/********************************************************************/
*                                                                  0059
*   EPARLG=ON;                      /* RECORD ENTRY INFO IN EPATH    */
         OI    EPARLG(EPAPTR),B'01000000'                          0059
*   RFY                                                            0060
*    (R1,                                                          0060
*     R2) RSTD;                     /* RESTRICT USE OF REG 2         */
*   R2=R1;                          /* GET POINTER TO ACE FROM REG1  */
*                                                                  0061
         LR    R2,R1                                               0061
*   /*****************************************************************/
*   /*                                                               */
*   /* BEGIN ILRGMA MACRO EXPANSION                                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0062
*   DO;                             /* ILRGMA GET                  0062
*                                      CELL(BWK)WORKREGS(R11,R12)    */
*     DECLARE                       /* ASMVT VECTOR TO ILRPEX        */
*       ILRPEX ENTRY BASED(ASMPEX) VALRG(0);                       0063
*     DECLARE                       /* MAP FOR CELL CONTROL WORDS    */
*       1 GMACELL BASED BDY(WORD),                                 0064
*        2 GMANEXT PTR(31);         /* PTR NEXT FREE CELL            */
*     RESPECIFY                                                    0065
*      (R0,                                                        0065
*       R1,                                                        0065
*       R11,                                                       0065
*       R12,                                                       0065
*       R14,                                                       0065
*       R15) RESTRICTED;                                           0065
*     RESPECIFY                                                    0066
*       ASMPOOLS BASED(R11);                                       0066
*     RESPECIFY                                                    0067
*       GMACELL BASED(R14);                                        0067
*     R11=ADDR(ASMBWKPC);                                          0068
         LA    R11,ASMBWKPC(,ASMVTPTR)                             0068
*     LM(R14,R15,ASMCPAVQ);         /* GET ADDR OF 1ST CELL          */
         LM    R14,R15,ASMCPAVQ(R11)                               0069
*GMA00010:                                                         0070
*     IF R14^=0 THEN                /* TEST CELL POOL                */
GMA00010 LTR   R14,R14                                             0070
         BZ    @RF00070                                            0070
*       DO;                         /* CELL AVAILABLE, REMOVE CELL   */
*         R0=GMANEXT;                                              0072
         L     R0,GMANEXT(,R14)                                    0072
*         R1=R15-1;                                                0073
         LR    R1,R15                                              0073
         BCTR  R1,0                                                0073
*         CDS(R14,R0,ASMCPAVQ);                                    0074
         CDS   R14,R0,ASMCPAVQ(R11)                                0074
*         BC(7,GMA00010);                                          0075
         BC    7,GMA00010                                          0075
*         R1=R14;                                                  0076
         LR    R1,R14                                              0076
*       END;                                                       0077
*     ELSE                                                         0078
*       DO;                         /* CELL POOL EMPTY               */
         B     @RC00070                                            0078
@RF00070 DS    0H                                                  0079
*         R1=R11;                   /* PREPARE TO CALL PEX           */
         LR    R1,R11                                              0079
*         CALL ILRPEX;              /* EXTEND THE POOL               */
         L     @15,ASMPEX(,ASMVTPTR)                               0080
         BALR  @14,@15                                             0080
*       END;                                                       0081
*     RESPECIFY                                                    0082
*      (R0,                                                        0082
*       R1,                                                        0082
*       R11,                                                       0082
*       R12,                                                       0082
*       R14,                                                       0082
*       R15) UNRESTRICTED;                                         0082
@RC00070 DS    0H                                                  0083
*   END;                            /* ILRGMA GET                  0083
*                                      CELL(BWK)WORKREGS(R11,R12)    */
*                                                                  0083
*   /*****************************************************************/
*   /*                                                               */
*   /* END ILRGMA MACRO EXPANSION                                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0084
*                                   /* GET WORKAREA                  */
*   RFY                                                            0084
*     R1 RSTD;                      /* RESTRICT REG 1                */
*   IF R1=ZERO                      /* IF NO WORKAREA AVAILABLE,     */
*     THEN                          /* RETURN TO CALLER WITH RETURN  */
         LTR   R1,R1                                               0085
         BNZ   @RF00085                                            0085
*     RETURN CODE(28);              /* CODE 28.                      */
         LA    @15,28                                              0086
         L     @14,12(,@13)                                        0086
         LM    @00,@12,20(@13)                                     0086
         BR    @14                                                 0086
*   WKAREG=R1;                      /* GET POINTER TO WORK AREA      */
@RF00085 LR    WKAREG,R1                                           0087
*   EPAOWKA=R1;                     /* SAVE WORK AREA PTR FOR      0088
*                                      RECOVERY                      */
         ST    R1,EPAOWKA(,EPAPTR)                                 0088
*   GEN REFS(WKAREG,R12) SETS(R12,R13);                            0089
         USING @DATD,WKAREG       /* ESTABLISH ADDR TO WORK AREA    */
         LA    R12,SAVEREGS      /* OBTAIN ADDR OF RLG00'S SAVE AREA*/
         ST    R13,4(R12)        /* STORE ADDR OF PREVIOUS SAVE AREA*/
         ST    R12,8(R13)         /* STORE ADDR OF RLG00'S SAVE AREA*/
         LR    R13,R12            /* SET R13 TO RLG00'S SAVE AREA   */
*   RFY                                                            0090
*     R1 UNRSTD;                    /* FREE REG 1 FOR USE            */
*   WORKDCLS=WORKDCLS&&WORKDCLS;    /* ZERO OUT WORK AREA            */
         XC    WORKDCLS(17),WORKDCLS                               0091
*   ACEPTR=R2;                      /* GET POINTER TO ACE FROM REG 2 */
         ST    R2,ACEPTR                                           0092
*   RFY                                                            0093
*     R2 UNRSTD;                    /* FREE REG 2 FOR USE            */
*   IF ACEUSYM=ON                   /* LG BEEN SAVED?                */
*     THEN                                                         0094
         L     @12,ACEPTR                                          0094
         TM    ACEUSYM(@12),B'10000000'                            0094
         BNO   @RF00094                                            0094
*     LGSAVED=ON;                   /* SET LG -SAVED FLAG ON         */
         OI    LGSAVED,B'10000000'                                 0095
*   LGEPTR=ACELGE;                  /* GET ADDRESSABILITY TO LGE     */
@RF00094 L     @12,ACEPTR                                          0096
         L     @12,ACELGE(,@12)                                    0096
         ST    @12,LGEPTR                                          0096
*   IF LGEASPCT^=ZERO               /* IF THERE IS AN ACTIVE ASPCT,  */
*     THEN                          /* THEN                          */
         L     @12,LGEASPCT(,@12)                                  0097
         LTR   @12,@12                                             0097
         BZ    @RF00097                                            0097
*     DO;                                                          0098
*       ASPCTPTR=LGEASPCT;          /* GET POINTER TO ASPCT FROM LGE */
         ST    @12,ASPCTPTR                                        0099
*       EPAAASP=ASPCTPTR;           /* RECORD PTR TO ACTIVE ASPCT    */
         ST    @12,EPAAASP(,EPAPTR)                                0100
*       BACKSLOT=ASPBKSLT;          /* SAVE # OF BACKUP SLOTS        */
         L     @11,ASPBKSLT(,@12)                                  0101
         ST    @11,BACKSLOT                                        0101
*       SAVESLOT=ASPSAVCT;          /* SAVE # OF SAVED SLOTS         */
         L     @12,ASPSAVCT(,@12)                                  0102
         ST    @12,SAVESLOT                                        0102
*     END;                                                         0103
*                                                                  0104
*/********************************************************************/
*/*                                                                  */
*/* RLGSAVED: PROCESS A SAVED ASPCT                                  */
*/*                                                                  */
*/*          THE RETURN ADDRESS IS SAVED. IF THE ASPCT IS INACTIVE,  */
*/*          TURN THE ACENOACT FLAG ON IN THE ACE. IF THE ASPCT IS   */
*/*          ACTIVE, INVOKE ILRALS00 TO RELEASE THE UNSAVED SLOTS,   */
*/*          INVOKE ILRAFS00 TO FREE THE LSQA STORAGE USED BY THIS   */
*/*          ASPCT AND ITS EXTENSIONS.                               */
*/*          IF STGINDEX IS NOT OPEN OR ILRTMRLG IS NOT ACTIVE, AND  */
*/*         THIS IS NOT A DEACTIVATE REQUEST, SET THE RETURN CODE TO */
*/*         16, AND RETURN. IF THIS IS A RELEASE REQUEST, AND        */
*/*         STGINDEX IS OPEN AND ILRTMRLG IS ACTIVE, ADD THE ACE     */
*/*         TO THE RELEASE QUEUE AND POST ILRTMRLG. RETURN TO CALLER */
*/*                                                                  */
*/*         ACTIVE ASPCT STORAGE FREED                               */
*/*         UNSAVED SLOTS RELEASED                                   */
*/*         IF ILRTMRLG IS ACTIVE, AND THIS IS A RELEASE REQUEST,    */
*/*         THE ACE IS QUEUED TO THE ASMVT RELEASE QUEUE. IF THIS WAS*/
*/*         A RELEASE REQUEST AND ILRTMRLG IS INACTIVE, REGISTER 15  */
*/*         WILL CONTAIN A RETURN CODE OF 16.                        */
*/*                                                                  */
*/********************************************************************/
*                                                                  0104
*   IF LGSAVED=ON                   /* IF LG -SAVED FLAG IS ON       */
*     THEN                                                         0104
@RF00097 TM    LGSAVED,B'10000000'                                 0104
         BNO   @RF00104                                            0104
*     DO;                                                          0105
*       IF LGEASPCT=ZERO THEN                                      0106
         L     @12,LGEPTR                                          0106
         L     @12,LGEASPCT(,@12)                                  0106
         LTR   @12,@12                                             0106
         BNZ   @RF00106                                            0106
*         ACENOACT=ON;              /* IF ASPCT NOT ACTIVE,SET     0107
*                                      ACENOACT                      */
         L     @12,ACEPTR                                          0107
         OI    ACENOACT(@12),B'00010000'                           0107
*       ELSE                        /* IF ASPCT IS ACTIVE            */
*         DO;                                                      0108
         B     @RC00106                                            0108
@RF00106 DS    0H                                                  0109
*           RFY                                                    0109
*            (R0,                                                  0109
*             R1,                                                  0109
*             R15) RSTD;            /* RESTRICT USE OF REG 0         */
*           R0=ASPCTPTR;            /* LOAD REG0 WITH ADDR(ASPCT)    */
         L     R0,ASPCTPTR                                         0110
*           LGEASPCT=ZERO;          /* ZERO THE LGE PTR TO THE ASPCT */
         L     @14,LGEPTR                                          0111
         SLR   @12,@12                                             0111
         ST    @12,LGEASPCT(,@14)                                  0111
*           CALL ILRALS00;          /* FREE THE UNSAVED SLOTS        */
         BAL   @14,ILRALS00                                        0112
*           R1=ASPCTPTR;            /* LOAD REG1 WITH ADDR(ASPCT)    */
         L     R1,ASPCTPTR                                         0113
*           R0=LENGTH(ASPCT);       /* SET REG0 TO LENGTH OF ACTIVE
*                                      ASPCT                         */
         LA    R0,1088                                             0114
*           EPAAASP=ZERO;           /* ZERO EPATH PTR TO ASPCT       */
         SLR   @14,@14                                             0115
         ST    @14,EPAAASP(,EPAPTR)                                0115
*           CALL ILRAFS00;          /* FREE THE ACTIVE ASPCT STORAGE */
         BAL   @14,ILRAFS00                                        0116
*           IF R15^=ZERO            /* FOR NON-ZERO RETURN CODE FROM
*                                      AFS, ABEND                    */
*             THEN                                                 0117
         LTR   R15,R15                                             0117
         BZ    @RF00117                                            0117
*             DO;                                                  0118
*               EPARTYRG=R13+12;    /* SAVED REGISTERS FOR RECOVERY  */
         LA    @14,12                                              0119
         ALR   @14,R13                                             0119
         ST    @14,EPARTYRG(,EPAPTR)                               0119
*               GEN REFS(R0,R13,R14);                              0120
         STM   R0,R14,12(R13)
         ABEND X'087',,,SYSTEM
*             END;                                                 0121
*           RFY                                                    0122
*            (R0,                                                  0122
*             R1,                                                  0122
*             R15) UNRSTD;                                         0122
@RF00117 DS    0H                                                  0123
*         END;                      /* END ACTIVE ASPCT PROCESSING   */
*       IF(ASMSTGXA=ZERO            /* IF STORAGE INDEX NOT OPEN     */
*           ×ASMNOTMR=ON)           /* OR ILRTMRLG NOT OPERATING     */
*         THEN                                                     0124
@RC00106 L     @12,ASMSTGXA(,ASMVTPTR)                             0124
         LTR   @12,@12                                             0124
         BZ    @RT00124                                            0124
         TM    ASMNOTMR(ASMVTPTR),B'00100000'                      0124
         BNO   @RF00124                                            0124
@RT00124 DS    0H                                                  0125
*         DO;                                                      0125
*           RETACE=ON;              /* SET RETURN ACE BIT ON         */
         OI    RETACE,B'01000000'                                  0126
*           RETCODE=20;             /* SET RETURN CODE TO 20         */
         MVC   RETCODE(4),@CF00213                                 0127
*         END;                                                     0128
*       ELSE                                                       0129
*         IF ACEOP=ACERELLG         /* IF THIS IS A RELEEASE LG      */
*           THEN                    /* REQUEST                       */
         B     @RC00124                                            0129
@RF00124 L     @12,ACEPTR                                          0129
         CLI   ACEOP(@12),12                                       0129
         BNE   @RF00129                                            0129
*           DO;                     /* THEN                          */
*             ACEFQPA=ZERO;         /* ZERO FORWARD POINTER IN ACE   */
         SLR   @11,@11                                             0131
         ST    @11,ACEFQPA(,@12)                                   0131
*             RFY                                                  0132
*              (R8,                                                0132
*               R9) RSTD;           /* RESTRICT USE OF C&S REGISTERS */
*             R8=ACEPTR;                                           0133
         LR    R8,@12                                              0133
*             R9=ASMRLGRQ;                                         0134
         L     R9,ASMRLGRQ(,ASMVTPTR)                              0134
*TRYOVER0:                                                         0135
*             ACEFQPA=R9;                                          0135
TRYOVER0 L     @12,ACEPTR                                          0135
         ST    R9,ACEFQPA(,@12)                                    0135
*             CS(R9,R8,ASMRLGRQ);   /* COMPARE AND SWAP THE ACE ONTO */
         CS    R9,@08,ASMRLGRQ(ASMVTPTR)                           0136
*             BC(7,TRYOVER0);       /* THE HEAD OF THE RELEASE QUEUE */
         BC    7,TRYOVER0                                          0137
*             RFY                                                  0138
*              (R8,                                                0138
*               R9) UNRSTD;         /* FREE C&S REGISTERS FOR USE    */
*             GEN REFS(R8,R10,R13)(STM   R10,R8,12(R13));          0139
         STM   R10,R8,12(R13)
*             RFY                                                  0140
*              (R9,                                                0140
*               R10,                                               0140
*               R11,                                               0140
*               R12) RSTD;          /* RESTRICT POST REGS            */
*             R9=R13;               /* SAVE ADDRESS OF ILRRLG'S    0141
*                                      SAVEAREA                      */
         LR    R9,R13                                              0141
*             R11=ADDR(ASMTMECB);   /* ADDR OF ECB TO BE POSTED      */
         LA    R11,ASMTMECB(,ASMVTPTR)                             0142
*             R11=R11×'80000000'X;  /* SET HI-ORDER BIT ON FOR XMPOST*/
         O     R11,@CF01355                                        0143
*             R10=ZERO;             /* INITIALIZE COMPLETION CODE    */
         SLR   R10,R10                                             0144
*             R12=ADDR(CVTBRET);    /* SYSTEM ERRET ENTRY PT ADDRESS */
         L     @08,CVTPTR                                          0145
         LA    R12,CVTBRET(,@08)                                   0145
*             R13=ASMTASCB;         /* ADDR OF TMRLG'S ASCB          */
         L     R13,ASMTASCB(,ASMVTPTR)                             0146
*             CALL XMPOST;          /* BRANCH ENTRY CROSS-MEMORY POST*/
         L     @15,CVT0PT01(,@08)                                  0147
         BALR  @14,@15                                             0147
*             RFY                                                  0148
*              (R9,                                                0148
*               R10,                                               0148
*               R11,                                               0148
*               R12) UNRSTD;        /* FREE POST REGS                */
*             GEN REFS(R8,R9,R10)(LM    R10,R8,12(R9));            0149
         LM    R10,R8,12(R9)
*           END;                    /* END DEACTIVATE REQ PROCESSING */
*     END;                          /* END EPATH-SAVED PROCESSING    */
*                                                                  0152
*/********************************************************************/
*/*                                                                  */
*/*  WHEN LG IS NOT SAVED (LGSAVED=OFF) THE FOLLOWING BLOCK OF       */
*/*  CODE WILL BE EXECUTED.                                          */
*/*                                                                  */
*/********************************************************************/
*                                                                  0152
*   ELSE                                                           0152
*     IF LGEASPCT^=ZERO THEN                                       0152
         B     @RC00104                                            0152
@RF00104 SLR   @12,@12                                             0152
         L     @11,LGEPTR                                          0152
         C     @12,LGEASPCT(,@11)                                  0152
         BE    @RF00152                                            0152
*       DO;                                                        0153
*         RFY                                                      0154
*          (R0,                                                    0154
*           R1,                                                    0154
*           R15) RSTD;              /* RESTRICT USE OF REGS 0 AND 15 */
*         R0=ASPCTPTR;              /* LOAD REG0 WITH ADDR OF ASPCT  */
         L     R0,ASPCTPTR                                         0155
*         LGEASPCT=ZERO;            /* ZERO LGE PTR TO THE ASPCT     */
         ST    @12,LGEASPCT(,@11)                                  0156
*         CALL ILRALS00;            /* RELEASE SLOTS ASSIGNED TO THIS
*                                      LG                            */
         BAL   @14,ILRALS00                                        0157
*         R1=ASPCTPTR;              /* LOAD REG1 WITH ADDR OF ASPCT  */
         L     R1,ASPCTPTR                                         0158
*         R0=LENGTH(ASPCT);         /* LOAD REG0 WITH LENGTH OF ASPCT*/
         LA    R0,1088                                             0159
*         EPAAASP=ZERO;             /* ZERO EPATH PTR TO ACTIVE ASPCT*/
         SLR   @14,@14                                             0160
         ST    @14,EPAAASP(,EPAPTR)                                0160
*         CALL ILRAFS00;            /* RELEASE STORAGE FOR THIS ASPCT*/
         BAL   @14,ILRAFS00                                        0161
*         IF R15^=ZERO              /* IF NON-ZERO RETURN FROM AFS,  */
*           THEN                                                   0162
         LTR   R15,R15                                             0162
         BZ    @RF00162                                            0162
*           DO;                     /* THEN ABEND 807                */
*             EPARTYRG=R13+12;                                     0164
         LA    @14,12                                              0164
         ALR   @14,R13                                             0164
         ST    @14,EPARTYRG(,EPAPTR)                               0164
*             GEN REFS(R0,R13,R14);                                0165
         STM   R0,R14,12(R13)
         ABEND X'087',,,SYSTEM
*           END;                                                   0166
*         RFY                                                      0167
*          (R0,                                                    0167
*           R1,                                                    0167
*           R15) UNRSTD;            /* FREE REGS 0 AND 15 FOR USE    */
@RF00162 DS    0H                                                  0168
*       END;                        /* END EPATH-SAVED PROCESSING    */
*                                                                  0169
*/********************************************************************/
*/*                                                                  */
*/*      CHECK SLOT COUNTERS FOR NON-ZERO                            */
*/*                                                                  */
*/********************************************************************/
*                                                                  0169
*   IF RETACE=OFF                   /* IF RETURN ACE BIT IS OFF      */
*       &BACKSLOT^=ZERO             /* AND BACKSLOT IS NON-ZERO      */
*     THEN                                                         0169
@RF00152 DS    0H                                                  0169
@RC00104 TM    RETACE,B'01000000'                                  0169
         BNZ   @RF00169                                            0169
         L     @12,BACKSLOT                                        0169
         LTR   @12,@12                                             0169
         BZ    @RF00169                                            0169
*     DO;                                                          0170
*       RFY                                                        0171
*        (R8,                                                      0171
*         R9) RSTD;                 /* THEN ADD BACKSLOT TO ASMBKSLT */
*       R8=ASMBKSLT;                                               0172
         L     R8,ASMBKSLT(,ASMVTPTR)                              0172
*TRYOVER1:                                                         0173
*       R9=R8+BACKSLOT;                                            0173
TRYOVER1 LR    R9,R8                                               0173
         AL    R9,BACKSLOT                                         0173
*       CS(R8,R9,ASMBKSLT);                                        0174
         CS    R8,@09,ASMBKSLT(ASMVTPTR)                           0174
*       BC(7,TRYOVER1);                                            0175
         BC    7,TRYOVER1                                          0175
*       RFY                                                        0176
*        (R8,                                                      0176
*         R9) UNRSTD;                                              0176
*     END;                                                         0177
*   IF FREESLOT^=ZERO               /* IF FREESLOT COUNTER NON-ZERO  */
*     THEN                                                         0178
@RF00169 L     @12,FREESLOT                                        0178
         LTR   @12,@12                                             0178
         BZ    @RF00178                                            0178
*     DO;                                                          0179
*       RFY                                                        0180
*        (R8,                                                      0180
*         R9) RSTD;                 /* THEN SUBTRACT FREESLOT FROM   */
*       R8=ASMVSC;                  /* ASMVSC                        */
         L     R8,ASMVSC(,ASMVTPTR)                                0181
*TRYOVER2:                                                         0182
*       R9=R8-FREESLOT;                                            0182
TRYOVER2 LR    R9,R8                                               0182
         SL    R9,FREESLOT                                         0182
*       CS(R8,R9,ASMVSC);                                          0183
         CS    R8,@09,ASMVSC(ASMVTPTR)                             0183
*       BC(7,TRYOVER2);                                            0184
         BC    7,TRYOVER2                                          0184
*       RFY                                                        0185
*        (R8,                                                      0185
*         R9) UNRSTD;                                              0185
*       LGESLTCT=LGESLTCT-FREESLOT; /* SUBTRACT FREED SLOTS          */
         L     @12,LGEPTR                                          0186
         LH    @11,LGESLTCT(,@12)                                  0186
         SL    @11,FREESLOT                                        0186
         STH   @11,LGESLTCT(,@12)                                  0186
*     END;                                                         0187
*   IF SAVESLOT^=ZERO               /* IF SAVESLOT NON-ZERO THEN SUB-*/
*     THEN                                                         0188
@RF00178 L     @12,SAVESLOT                                        0188
         LTR   @12,@12                                             0188
         BZ    @RF00188                                            0188
*     LGESLTCT=LGESLTCT-SAVESLOT;   /* TRACT SAVESLOT FROM LGE     0189
*                                      COUNTER                       */
         L     @11,LGEPTR                                          0189
         LCR   @12,@12                                             0189
         AH    @12,LGESLTCT(,@11)                                  0189
         STH   @12,LGESLTCT(,@11)                                  0189
*                                                                  0190
*/********************************************************************/
*/*                                                                  */
*/*  ZERO THE POINTER TO THE ACE AND FREE THE LGE                    */
*/*                                                                  */
*/********************************************************************/
*                                                                  0190
*   LGEPROCF=ZERO;                  /* RESET FORWARD AND             */
@RF00188 SLR   @12,@12                                             0190
         L     @11,LGEPTR                                          0190
         ST    @12,LGEPROCF(,@11)                                  0190
*   LGEPROCL=ZERO;                  /* BACKWARD POINTERS TO THIS ACE */
         ST    @12,LGEPROCL(,@11)                                  0191
*   RFY                                                            0192
*     R1 RSTD;                      /* RESTRICT USE OF REG 1         */
*   R1=LGEPTR;                      /* LOAD REG1 WITH POINTER TO LGE */
         LR    R1,@11                                              0193
*   EPAACE=ZERO;                    /* ZERO THE ACE PTR IN EPATH     */
         ST    @12,EPAACE(,EPAPTR)                                 0194
*   CALL ILRFRELG;                  /* RELEASE THE LGE               */
         L     @15,ASMPFRLG(,ASMVTPTR)                             0195
         BALR  @14,@15                                             0195
*   IF LGSAVED=OFF                  /* IF LG -SAVED FLAG IS OFF OR   */
*       ×RETACE=ON                  /* IF RETURN ACE BIT IS ON OR    */
*       ×ACEOP=ACEDEACT             /* IF THIS IS A DEACTIVATE     0196
*                                      REQUEST                       */
*     THEN                                                         0196
         TM    RETACE,B'01000000'                                  0196
         BNZ   @RT00196                                            0196
         TM    LGSAVED,B'10000000'                                 0196
         BNO   @RT00196                                            0196
         L     @12,ACEPTR                                          0196
         CLI   ACEOP(@12),28                                       0196
         BNE   @RF00196                                            0196
@RT00196 DS    0H                                                  0197
*     DO;                           /* THEN FREE THE ACE TO ITS POOL */
