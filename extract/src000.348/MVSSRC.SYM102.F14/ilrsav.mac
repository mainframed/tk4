         LR    @05,AFSA                                            0379
         SLA   @05,2                                               0379
         L     AFSWORK,ASPASSTE-4(@05,AFSEXPTR)                    0379
*         DO WHILE AFSWORK^=AFSZERO;/* LOOP UNTIL WE HAVE PROCESSED
*                                      ALL LPME EXTENSIONS FOR A   0380
*                                      PARTICULAR ASST EXTENSION     */
         B     @DE00380                                            0380
@DL00380 DS    0H                                                  0381
*           CALL AFSFREE;           /* FREE AN LPME EXTENSION        */
         BAL   @14,AFSFREE                                         0381
*           RFY                                                    0382
*             R15 RSTD;                                            0382
*           IF R15=AFSZERO          /* SUCCESSFUL FREEMAIN?          */
*             THEN                                                 0383
         LTR   R15,R15                                             0383
         BNZ   @RF00383                                            0383
*             DO;                   /* YES                           */
*               RFY                                                0385
*                 R15 UNRSTD;                                      0385
