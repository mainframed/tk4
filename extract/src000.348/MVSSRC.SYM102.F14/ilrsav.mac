         C     AFSA,@CF01426                                       0387
         BNL   @RF00387                                            0387
*                 AFSWORK=AFSEXPTR->ASPASSTE(AFSA);                0388
         LR    @05,AFSA                                            0388
         SLA   @05,2                                               0388
         L     AFSWORK,ASPASSTE-4(@05,AFSEXPTR)                    0388
*               ELSE                /* YES, FORCE EXIT FROM LOOP     */
*                 AFSWORK=AFSZERO;                                 0389
         B     @RC00387                                            0389
@RF00387 SLR   AFSWORK,AFSWORK                                     0389
*             END;                  /* THIS IS THE LOGICAL END OF THE
*                                      DO WHILE LOOP FOR LPME      0390
*                                      EXTENSIONS. PHYSICAL END IS 0390
*                                      AFTER THE ERROR CODE PROCESS
*                                      ING FOR A NON-ZERO RETURN FROM
*                                      AFSFREE.                      */
         EJECT
*                                                                  0391
*/********************************************************************/
*/* THIS SECTION WILL PROCESS A NON-ZERO RETURN CODE FROM AFSFREE    */
*/* FOR LPME EXTENSIONS. PROCESSING CONSISTS OF GETTING THE POINTER  */
*/* TO THE NEXT LPME EXTENSION, AND VERIFYING THAT IT IS AN LPME     */
*/* EXTENSION. IF IT IS WE WILL CONTINUE FREEING LPME EXTENSIONS,    */
*/* IF NOT, WE WILL FORCE AN EXIT FROM THE LOOP.                     */
*/********************************************************************/
*                                                                  0391
*           ELSE                                                   0391
*             DO;                   /* NON-ZERO RETURN FROM AFSFREE  */
         B     @RC00383                                            0391
@RF00383 DS    0H                                                  0392
*               AFSA=AFSA+AFSONE;   /* INCREMENT ASST SUBSCRIPT      */
         AL    AFSA,@CF00075                                       0392
*               IF AFSA<AFS257      /* REACHED END OF ASST ARRAY?    */
*                 THEN                                             0393
         C     AFSA,@CF01426                                       0393
         BNL   @RF00393                                            0393
*                 DO;               /* NO GET PTR TO NEXT LPME EXT   */
*                   AFSWORK=AFSEXPTR->ASPASSTE(AFSA);/* IS THIS A  0395
*                                      VALID LPME EXT?               */
         LR    @05,AFSA                                            0395
         SLA   @05,2                                               0395
         L     AFSWORK,ASPASSTE-4(@05,AFSEXPTR)                    0395
*                   IF AFSWORK->ASPEIDNT^='LPME' THEN/* NO           */
         CLC   ASPEIDNT(4,AFSWORK),@CC01456                        0396
         BE    @RF00396                                            0396
*                     AFSWORK=AFSZERO;/* FORCE LOOP EXIT             */
         SLR   AFSWORK,AFSWORK                                     0397
*                 END;                                             0398
*               ELSE                /* YES, REACHED END OF ARRAY     */
*                 AFSWORK=AFSZERO;  /* FORCE LOOP EXIT               */
         B     @RC00393                                            0399
@RF00393 SLR   AFSWORK,AFSWORK                                     0399
*             END;                                                 0400
@RC00393 DS    0H                                                  0401
*         END;                      /* THIS IS THE PHYSICAL END OF 0401
*                                      THE DO WHILE LOOP FOR LPME  0401
*                                      EXTENSIONS.                   */
@RC00383 DS    0H                                                  0401
@DE00380 LTR   AFSWORK,AFSWORK                                     0401
         BNZ   @DL00380                                            0401
         EJECT
*                                                                  0402
*/********************************************************************/
*/* THIS SECTION PROCESSES THE ASST EXTENSIONS                       */
*/********************************************************************/
*                                                                  0402
*         AFSWORK=AFSEXPTR;         /* SET FREEMAIN POINTER TO AN  0402
*                                      ASST EXTENSION                */
         LR    AFSWORK,AFSEXPTR                                    0402
*         CALL AFSFREE;             /* FREE THE ASST EXTENSION       */
         BAL   @14,AFSFREE                                         0403
*         RFY                                                      0404
*           R15 RSTD;                                              0404
*         IF R15=AFSZERO            /* FREEMAIN SUCCESSFUL?          */
*           THEN                                                   0405
         LTR   R15,R15                                             0405
         BNZ   @RF00405                                            0405
*           DO;                     /* YES                           */
*             RFY                                                  0407
*               R15 UNRSTD;                                        0407
*             AFSB=AFSB+AFSONE;     /* INCREMENT BASE SUBSCRIPT      */
         LA    @08,1                                               0408
         ALR   AFSB,@08                                            0408
*             IF AFSB<AFSFIVE       /* REACHED END OF ARRAY?         */
*               THEN                                               0409
         C     AFSB,@CF00462                                       0409
         BNL   @RF00409                                            0409
*               DO;                 /* NO GET PTR TO NEXT ASST EXT   */
*                 AFSEXPTR=AFSASPCT->ASPASSTP(AFSB);               0411
         LR    @05,AFSB                                            0411
         SLA   @05,2                                               0411
         L     @01,AFSASPCT                                        0411
         L     AFSEXPTR,ASPASSTP-4(@05,@01)                        0411
*                 AFSA=AFSONE;      /* RESET ASST SUBSCRIPT          */
         LA    @05,1                                               0412
         LR    AFSA,@05                                            0412
*               END;                                               0413
*             ELSE                  /* YES, REACHED END OF ARRAY     */
*               AFSEXPTR=AFSZERO;   /* FORCE LOOP EXIT               */
         B     @RC00409                                            0414
@RF00409 SLR   AFSEXPTR,AFSEXPTR                                   0414
*           END;                    /* THIS THE LOGICAL END OF THE DO
*                                      WHILE LOOP FOR ASST         0415
*                                      EXTENSIONS. PHYSICAL END IS 0415
*                                      AFTER THE ERROR CODE PROCESS
*                                      ING FOR A NON-ZERO RETURN FROM
*                                      AFSFREE                       */
         EJECT
*                                                                  0416
*/********************************************************************/
*/* THIS SECTION WILL PROCESS A NON-ZERO RETURN CODE FROM AFSFREE    */
*/* FOR ASST EXTENSIONS. PROCESSING CONSISTS OF GETTING THE POINTER  */
*/* TO THE NEXT ASST EXTENSION, AND VERIFYING THAT IT IS AN ASST     */
*/* EXTENSION. IF IT IS WE WILL CONTINUE FREEING ASST EXTENSIONS,    */
*/* IF NOT, WE WILL FORCE AN EXIT FROM THE LOOP.                     */
*/********************************************************************/
*                                                                  0416
*         ELSE                                                     0416
*           DO;                     /* NON-ZERO RETURN CODE FROM   0416
*                                      AFSFREE                       */
         B     @RC00405                                            0416
@RF00405 DS    0H                                                  0417
*             AFSB=AFSB+AFSONE;     /* INCREMENT BASE SUBSCRIPT      */
         LA    @08,1                                               0417
         ALR   AFSB,@08                                            0417
*             IF AFSB<AFSFIVE       /* REACHED END OF ARRAY          */
*               THEN                                               0418
         C     AFSB,@CF00462                                       0418
         BNL   @RF00418                                            0418
*               DO;                 /* GET PTR TO NEXT ASST          */
*                 AFSEXPTR=AFSASPCT->ASPASSTP(AFSB);/* VALID ASST  0420
*                                      EXTENSION?                    */
         LR    @05,AFSB                                            0420
         SLA   @05,2                                               0420
         L     @01,AFSASPCT                                        0420
         L     AFSEXPTR,ASPASSTP-4(@05,@01)                        0420
*                 IF AFSEXPTR->ASPEIDNT='ASST' THEN/* YES            */
         CLC   ASPEIDNT(4,AFSEXPTR),@CC01457                       0421
         BNE   @RF00421                                            0421
*                   AFSA=AFSONE;    /* RESET ASST SUBSCRIPT          */
         LA    @05,1                                               0422
         LR    AFSA,@05                                            0422
*                 ELSE              /* NO                            */
*                   AFSEXPTR=AFSZERO;/* FORCE LOOP EXIT              */
         B     @RC00421                                            0423
@RF00421 SLR   AFSEXPTR,AFSEXPTR                                   0423
*               END;                                               0424
*             ELSE                  /* YES, REACHED END OF ARRAY     */
*               AFSEXPTR=AFSZERO;   /* FORCE LOOP EXIT               */
         B     @RC00418                                            0425
@RF00418 SLR   AFSEXPTR,AFSEXPTR                                   0425
*           END;                                                   0426
@RC00418 DS    0H                                                  0427
*       END;                        /* PHYSICAL END OF DO WHILE LOOP
*                                      FOR ASST EXTENSIONS           */
@RC00405 DS    0H                                                  0427
@DE00378 LTR   AFSEXPTR,AFSEXPTR                                   0427
         BNZ   @DL00378                                            0427
*     END;                                                         0428
         EJECT
