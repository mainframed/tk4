@EL00004 DS    0H                                                  0232
@EF00004 DS    0H                                                  0232
@ER00004 BR    @14                                                 0232
         EJECT
*                                                                  0233
*/********************************************************************/
*/*                                                                  */
*/*  SAVLIMBO: FLAG UNSAVED LPMES AS SAVED AND RELEASE LIMBO SLOTS   */
*/*                                                                  */
*/*  INPUT: REGISTER 0 = CORRESPONDING RPN                           */
*/*         REGISTER 1 = PTR TO LPME                                 */
*/*                                                                  */
*/*  PROCESS: IF THE LPME IS FLAGGED AS RELEASED AFTER SAVED AND     */
*/*           THERE IS A RETRIEVED ASPCT, CONVERT THE RPN TO AN LPME */
*/*           IN THE RETRIEVED ASPCT. CALL ILRFRSLT TO RELEASE THE   */
*/*           SLOT ASSIGNED TO THIS LPME. INCREMENT THE FREED SLOT   */
*/*           COUNTER, TURN THE RELEASED AFTER SAVE FLAG OFF IN THE  */
*/*           ACTIVE ASPCT. IF THE ACTIVE LPME IS VALID, FLAG THE    */
*/*           LPME AS SAVED AND INCREMENT THE SAVED SLOT COUNTER.    */
*/*           IF THE LPME IS FLAGGED AS RELEASED AFTER SAVE, BUT     */
*/*           THERE IS NO RETRIEVED ASPCT, TURN THE RELEASED AFTER   */
*/*           SAVE FLAG OFF. IF THE LPME IS VALID, TURN THE SAVED    */
*/*           FLAG ON AND INCREMENT THE SAVED SLOT COUNTER.          */
*/*           IF THE LPME IS NOT FLAGGED AS SAVED OR RELEASED AFTER  */
*/*           SAVE, FLAG THE LPME AS SAVED AND INCREMENT THE SAVED   */
*/*           SLOT COUNTER.                                          */
*/*                                                                  */
*/*  OUTPUT: VALID UNSAVED LPME FLAGGED AS SAVED                     */
*/*          LIMBO SLOTS RELEASED IF THERE IS A RETRIEVED ASPCT      */
*/*                                                                  */
*/********************************************************************/
*/*SAVLIMBO: CHART (AREF)                                            */
*/*HEADER                                                          0233
*/*  ILRSAV                                                        0233
*/* SAVLIMBO: FLAG UNSAVED LPME'S AS SAVED AND FREE LIMBO SLOTS      */
*/*SAVLIMBO: E ENTRY                                                 */
*/*         P  SAVE THE RETURN ADDRESS                               */
*/*         P  SAVE THE PTR TO ACTIVE LPME (LPMEPTR=R1)              */
*/*         P  SAVE THE CORRESPONDING RPN (CORRPN=R0)                */
*/*         D  (YES,%SAV22,NO,) LPME FLAGGED AS SAVED?               */
*/*         D  (YES,%SAV20,NO,) RELEASED AFTER SAVE                  */
*/*%SAV19:  D  (NO,%SAV22,YES,) LPME VALID?                          */
*/*         P  FLAG THE LPME AS SAVED                                */
*/*         P  (,%SAV22) INCREMENT THE INTERNAL SAVED SLOT COUNTER   */
*/*%SAV20:  P  FLAG THE LPME AS NOT RELEASED AFTER SAVE              */
*/*         D  (NO,%SAV19,YES,) IS THERE A RETRIEVED ASPCT?          */
*/*         P  CONVERT RPN TO LPME                                   */
*/*         D  (YES,%SAV21,NO,) RPN CONVERTED?                       */
*/*         P  SAVE REGS 0 - 14 FOR RECOVERY                         */
*/*         S  ABEND: 085                                            */
*/*         D  (YES,,NO,%SAV22) ACTIVE LPME VALID?                   */
*/*         P  (,%SAV22) FLAG IT AS SAVED                            */
*/*%SAV21:  P  SAVE THE POINTER TO THE ACTIVE LPME                   */
*/*         P  LOAD REG 1 WITH LSID TO BE FREED                      */
*/*         S  ILRFRSL1: FREE THE SLOT                               */
*/*         P  RESET LPMEPTR TO ACTIVE LPME                          */
*/*         P  DECREMENT THE SAVED SLOT COUNTER                      */
*/*         P  (,%SAV19) INCREMENT THE FREED SLOT COUNTER            */
*/*%SAV22:  R  RETURN TO ILRALS00                                    */
*/*SAVLIMBO: END                                                     */
*                                                                  0233
         EJECT
*SAVLIMBO:                                                         0233
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);                          0233
SAVLIMBO DS    0H                                                  0234
*   RFY                                                            0234
*    (R0,                                                          0234
*     R1,                                                          0234
*     R14) RSTD;                                                   0234
*                                                                  0234
*/********************************************************************/
*/* INITIALIZATION - SAVE THE RETURN ADDRESS, POINTER TO THE         */
*/* INDIVIDUAL LPME AND ITS CORRESPONDING RPN.                       */
*/********************************************************************/
*                                                                  0235
*   RETADDR2=R14;                   /* SAVE THE RETURN ADDRESS       */
         ST    R14,RETADDR2                                        0235
*   LPMEPTR=R1;                     /* SAVE PTR TO LPME              */
         LR    LPMEPTR,R1                                          0236
*   CORRPN=R0;                      /* SAVE THE CORRESPONDING RPN    */
         ST    R0,CORRPN                                           0237
*   RFY                                                            0238
*    (R0,                                                          0238
*     R1,                                                          0238
*     R14) UNRSTD;                                                 0238
         EJECT
*                                                                  0239
*/********************************************************************/
*/* IF THE LPME IS FLAGGED AS SAVED, RETURN TO ILRALS00.             */
*/* IF THE LPME IS FLAGGED AS RELEASED AFTER SAVE, AND THERE IS NOT  */
*/* A RETRIEVED ASPCT, BUT THE LPME IS VALID, FLAG IT AS SAVED AND   */
*/* INCREMENT INTERNAL SAVED SLOT COUNTER, AND THEN RETURN TO        */
*/* ILRALS00 WHETHER THE LPME WAS VALID OR INVALID.                  */
*/********************************************************************/
*                                                                  0239
*   IF ASPLSAVE=OFF                 /* LPME FLAGGED AS SAVED         */
*     THEN                                                         0239
         TM    ASPLSAVE(LPMEPTR),B'01000000'                       0239
         BNZ   @RF00239                                            0239
*     DO;                           /* NO                            */
*       IF ASPLSVRP=ON              /* LPME BEEN RELEASED AFTER SAVE */
*         THEN                                                     0241
         TM    ASPLSVRP(LPMEPTR),B'00001000'                       0241
         BNO   @RF00241                                            0241
*         DO;                       /* YES                           */
*           ASPLSVRP=OFF;           /* TURN REL AFTER SAVE FLG OFF   */
         NI    ASPLSVRP(LPMEPTR),B'11110111'                       0243
*           IF RETASPCT=ZERO        /* IS THERE A RETRIEVED ASPCT    */
*             THEN                                                 0244
         LTR   RETASPCT,RETASPCT                                   0244
         BNZ   @RF00244                                            0244
*             DO;                   /* NO                            */
*               IF ASPLVALD=ON      /* LPME VALID?                   */
*                 THEN                                             0246
         TM    ASPLVALD(LPMEPTR),B'10000000'                       0246
         BNO   @RF00246                                            0246
*                 DO;               /* YES, FLAG LPME AS SAVED       */
*                   ASPLSAVE=ON;    /* INCREMENT THE SAVED SLOT    0248
*                                      COUNTER                       */
         OI    ASPLSAVE(LPMEPTR),B'01000000'                       0248
*                   SAVESLOT=SAVESLOT+ONE;                         0249
         LA    @06,1                                               0249
         AL    @06,SAVESLOT                                        0249
         ST    @06,SAVESLOT                                        0249
*                 END;                                             0250
*               ELSE                                               0251
*                 ;                                                0251
@RF00246 DS    0H                                                  0252
*               RFY                                                0252
*                 R14 RSTD;                                        0252
*               R14=RETADDR2;       /* GET THE RETURN ADDRESS        */
         L     R14,RETADDR2                                        0253
*               RETURN;             /* RETURN TO ILRALS00            */
@EL00005 DS    0H                                                  0254
@EF00005 DS    0H                                                  0254
@ER00005 BR    @14                                                 0254
*               RFY                                                0255
*                 R14 UNRSTD;                                      0255
*             END;                                                 0256
         EJECT
*                                                                  0257
*/********************************************************************/
*/* CONVERT THE CORRESPONDING RPL INTO AN LPME ADDRESS. THE RPN      */
*/* CAN BE BROKEN DOWN AS FOLLOWS: BAALL  WHERE B IS THE BASE ARRAY  */
*/* ELEMENT POINTING TO AN ASST EXTENSION, AA IS THE ASST EXT ARRAY  */
*/* ELEMENT POINTING TO AN LPME EXTENSION, AND LL IS THE LPME EXT    */
*/* ARRAY ELEMENT CONTAINING THE INDIVIDUAL LPME.                    */
*/********************************************************************/
*                                                                  0257
*           ELSE                                                   0257
*             DO;                   /* YES THERE IS A RET ASPCT      */
@RF00244 DS    0H                                                  0258
*               IF CORRPN>255       /* ARE THERE EXTENSIONS?         */
*                 THEN                                             0258
         L     @05,CORRPN                                          0258
         C     @05,@CF01405                                        0258
         BNH   @RF00258                                            0258
*                 DO;               /* YES, CONVERT THE RPN          */
*                   RFY                                            0260
*                     ASPCT BASED(RETASPCT);                       0260
*                   ASPEXPTR=(ASPASSTP((BINDEX+1)));               0261
         SLR   @15,@15                                             0261
         IC    @15,BINDEX                                          0261
         SLA   @15,2                                               0261
         L     ASPEXPTR,ASPASSTP(@15,RETASPCT)                     0261
*                   ASPEXPTR=(ASPASSTE((AINDEX+1)));               0262
         SLR   @03,@03                                             0262
         IC    @03,AINDEX                                          0262
         SLA   @03,2                                               0262
         L     @03,ASPASSTE(@03,ASPEXPTR)                          0262
         LR    ASPEXPTR,@03                                        0262
*                   SLPMEPTR=(ADDR(ASPLPMEE((LINDEX+1))));         0263
         SLR   @03,@03                                             0263
         IC    @03,LINDEX                                          0263
         SLA   @03,2                                               0263
         LA    SLPMEPTR,ASPLPMEE(@03,ASPEXPTR)                     0263
*                 END;                                             0264
*               ELSE                                               0265
*                 SLPMEPTR=(ADDR(ASPLPMES((LINDEX+1))));           0265
         B     @RC00258                                            0265
@RF00258 SLR   @15,@15                                             0265
         IC    @15,LINDEX                                          0265
         SLA   @15,2                                               0265
         LA    SLPMEPTR,ASPLPMES(@15,RETASPCT)                     0265
         EJECT
*                                                                  0266
*/********************************************************************/
*/* IF THE RETRIEVED LPME IS VALID, CALL ILRFRSL1 TO RETURN THE LOCAL*/
*/* SLOT BACK TO THE SYSTEM. IF THE RETRIEVED LPME IS INVALID, ISSUE */
*/* AN 085 ABEND FOR RECORDING.                                      */
*/********************************************************************/
*                                                                  0266
*               IF SLPMEPTR->ASPLVALD=ON/* VALID LPME?               */
*                 THEN                                             0266
@RC00258 TM    ASPLVALD(SLPMEPTR),B'10000000'                      0266
         BNO   @RF00266                                            0266
*                 DO;               /* YES                           */
*                   RFY                                            0268
*                    (R1,                                          0268
*                     R3) RSTD;                                    0268
*                   R3=ASMVTWPT;    /* LOAD PTR TO ASMVT             */
         L     @15,ASMVTWPT                                        0269
         LR    R3,@15                                              0269
*                   R1=SLPMEPTR->ASPVLSID;/* GET LSID                */
         L     R1,ASPVLSID-1(,SLPMEPTR)                            0270
         LA    R1,0(,R1)                                           0270
*                   CALL ILRFRSL1;  /* FREE THE SLOT                 */
         L     @15,ASMPFRSL(,@15)                                  0271
         BALR  @14,@15                                             0271
*                   RFY                                            0272
*                    (R1,                                          0272
*                     R3) UNRSTD;   /* DECREMENT SAVED SLOT COUNT    */
*                   SAVESLOT=SAVESLOT-ONE;/* INCREMENT FREED SLOT  0273
*                                      COUNT                         */
         L     @05,SAVESLOT                                        0273
         BCTR  @05,0                                               0273
         ST    @05,SAVESLOT                                        0273
*                   FREESLOT=FREESLOT+ONE;                         0274
         LA    @05,1                                               0274
         AL    @05,FREESLOT                                        0274
         ST    @05,FREESLOT                                        0274
*                 END;                                             0275
*               ELSE                                               0276
*                 DO;                                              0276
         B     @RC00266                                            0276
@RF00266 DS    0H                                                  0277
*                   EPARTYRG=ADDR(SAVREGSA);/* RECORD REG SAVEAREA 0277
*                                      PTR                           */
         LA    @05,SAVREGSA                                        0277
         ST    @05,EPARTYRG(,EPAPTR)                               0277
*                   STM(R0,R14,SAVREGSA);/* STORE REGISTERS FOR    0278
*                                      RECOVERY                      */
         STM   R0,R14,SAVREGSA                                     0278
*                   GEN(ABEND X'85',,,SYSTEM);                     0279
         ABEND X'85',,,SYSTEM
*                   IF ASPLVALD=ON  /* IS ACTIVE LPME VALID          */
*                     THEN          /* YES                           */
         TM    ASPLVALD(LPMEPTR),B'10000000'                       0280
         BNO   @RF00280                                            0280
*                     ASPLSAVE=ON;  /* FLAG IT AS SAVED              */
         OI    ASPLSAVE(LPMEPTR),B'01000000'                       0281
*                   RFY                                            0282
*                     R14 RSTD;                                    0282
@RF00280 DS    0H                                                  0283
*                   R14=RETADDR2;   /* GET RETURN ADDRESS            */
         L     R14,RETADDR2                                        0283
*                   RETURN;         /* RETURN TO ILRALS00            */
         B     @EL00005                                            0284
*                   RFY                                            0285
*                     R14 UNRSTD;                                  0285
*                 END;                                             0286
*               RFY                                                0287
*                 ASPCT BASED(ASPCTPTR);                           0287
@RC00266 DS    0H                                                  0288
*             END;                                                 0288
*         END;                                                     0289
*       IF ASPLVALD=ON              /* LPME VALID?                   */
*         THEN                                                     0290
@RF00241 TM    ASPLVALD(LPMEPTR),B'10000000'                       0290
         BNO   @RF00290                                            0290
*         DO;                       /* YES                           */
*           ASPLSAVE=ON;            /* FLAG LPME AS SAVED            */
         OI    ASPLSAVE(LPMEPTR),B'01000000'                       0292
*           SAVESLOT=SAVESLOT+ONE;  /* INCREMENT THE SAVED SLOT    0293
*                                      COUNTER                       */
         LA    @06,1                                               0293
         AL    @06,SAVESLOT                                        0293
         ST    @06,SAVESLOT                                        0293
*         END;                                                     0294
*     END;                                                         0295
@RF00290 DS    0H                                                  0296
*   RFY                                                            0296
*     R14 RSTD;                                                    0296
@RF00239 DS    0H                                                  0297
*   R14=RETADDR2;                   /* GET RETURN ADDRESS            */
         L     R14,RETADDR2                                        0297
*   RFY                                                            0298
*     R14 UNRSTD;                                                  0298
*   END SAVLIMBO;                                                  0299
         B     @EL00005                                            0299
         EJECT
*                                                                  0300
*/*  MACCOMP Y-4SC1CW/PZD81                                          */
*/*  MACSTAT Y-475082/415304                                         */
*/********************************************************************/
*/*                                                                  */
*/*  NAME:  ILRALS00                                                 */
*/*                                                                  */
*/*  STATUS:  OS/VS2 RELEASE 4, LEVEL 0                              */
*/*                                                                  */
*/*  ENTRY POINT:  ILRALS00                                          */
*/*                                                                  */
*/*  FUNCTION:  THIS SUBROUTINE SEARCHES THE LPME SECTORS OF A GIVEN */
*/*             ASPCT AND CALLS A PROCESSING ROUTINE (ALSPROC) FOR   */
*/*             EVERY VALID LSID IT FINDS.                           */
*/*                                                                  */
*/*  INPUT:  ADDRESS OF ASPCT IN REGISTER 0.                         */
*/*                                                                  */
*/*  PROCESS:  FOR EACH LPME EXTENSION, FIND ALL LPMES CONTAINING    */
*/*            VALID LSIDS AND SEND THEIR ADDRESSES, ONE AT A TIME   */
*/*            (ALONG WITH THE INPUT CONTROL INFORMATION), TO ALSPROC*/
*/*              ALSPROC INPUT -                                     */
*/*                REGISTER 0 = RPN OF LPME PASSED                   */
*/*                REGISTER 1 = ADDRESS OF LPME                      */
*/*                                                                  */
*/*  OUTPUT:  NONE                                                   */
*/*                                                                  */
*/*  EXIT-NORMAL:  RETURN TO CALLER                                  */
*/*                                                                  */
*/*  EXIT-ERROR:  NONE                                               */
*/*                                                                  */
*/*  EXTERNAL REFERENCES:  NONE                                      */
*/*                                                                  */
*/*  REGISTERS CLOBBERED:  0 AND 1, 14 AND 15                        */
*/*                                                                  */
*/*  MAPPING MACROS USED:  ILRASPCT                                  */
*/*                                                                  */
*/*   NOTES:  DUE TO PLS OPTIMIZATION, RESTRICT ALL NECESSARY        */
*/*          REGISTERS AROUND THIS ROUTINE.                          */
*/*          ALL LABELS AND DATA AREAS CREATED BY THIS ROUTINE BEGIN */
*/*          WITH THE THREE CHARACTERS 'ALS'.  THE CALLING MODULE    */
*/*          MUST INCLUDE SUFFICIENT WORKSPACE FOR THIS ROUTINE'S    */
*/*          DATA AREAS.  THIS AMOUNT IS COMPILER DEPENDENT WITH A   */
*/*          MAXIMUM REQUIREMENT OF 44 BYTES.  THIS ROUTINE ALSO     */
*/*          REQUIRES THAT THE CALLING MODULE DECLARES REGISTERS     */
*/*          R0-R15.                                                 */
*/*          MAPPING MACROS LISTED ABOVE MUST BE INCLUDED IN THE     */
*/*          CALLING MODULE.  THE PROCEDURE ALSPROC MUST BE DEFINED  */
*/*          BY THE CALLING MODULE.  THE PL/S MACRO FACILITY CAN BE  */
*/*          USED TO CHANGE THIS NAME, IF DESIRED.                   */
*/*                                                                  */
*/*  CHANGE-ACTIVITY:  LINE 052480 DELETED FOR APAR OZ15336          */
*/*                                                                  */
*/********************************************************************/
*/*ILRALS00: CHART (AREF)                                            */
*/*HEADER                                                          0300
*/*           ILRALS00 - FIND ALL VALID LSIDS (& RPNS) IN AN ASPCT   */
*/*ILRALS00: E ENTRY                                                 */
*/*         P  SAVE THE RETURN ADDRESS                               */
*/*         P  SAVE PTR TO ASPCT BASE                                */
*/*         P  INITIALIZE BASE ARRAY SUBSCRIPT TO ONE                */
*/*         P  INITIALIZE ASST EXT ARRAY SUBSCRIPT TO TWO            */
*/*         P  INITIALIZE LPME EXT ARRAY SUBSCRIPT TO ONE            */
*/*         P  INITIALIZE RPN VALUE TO ZERO                          */
*/*         P  GET THE NUMBER OF LPME EXTS FROM BASE                 */
*/*         P  GET PTR TO BASE LPME EXTENSION                        */
*/*         P  GET PTR TO FIRST LPME                                 */
*/*%ALS01:  D  (YES,%ALS02,NO,) LPME VALID?                          */
*/*         D  (YES,,NO,%ALS03) RELEASED AFTER SAVE?                 */
*/*%ALS02:  P  LOAD REG 0 WITH RPN                                   */
*/*         P  LOAD REG 1 WITH PTR TO LPME                           */
*/*         S  ALSPROC: CALL USER'S ROUTINE                          */
*/*%ALS03:  P  INCREMENT THE RPN                                     */
*/*         P  INCREMENT THE LPME EXT ARRAY SUBSCRIPT                */
*/*         P  GET PTR TO NEXT LPME                                  */
*/*         D  (YES,%ALS04,NO,) REACHED END OF ARRAY?                */
*/*         D  (YES,,NO,%ALS01) LPME ZERO?                           */
*/*%ALS04:  P  RESET LPME EXT SUBSCRIPT TO ONE                       */
*/*         D  (YES,%ALS05,NO,) ARE THERE ANY EXTENSIONS?            */
*/*         R  RETURN TO CALLER                                      */
*/*%ALS05:  D  (YES,,NO,%ALS07) REACHED END OF ASST EXT ARRAY?       */
*/*         P  RESET ASST EXT ARRAY SUBSCRIPT                        */
*/*         P  INCREMENT THE BASE ARRAY SUBSCRIPT                    */
*/*         D  (YES,%ALS06,NO,) REACHED END OF BASE ARRAY            */
*/*         D  (YES,%ALS07,NO,) ANYMORE ASST EXTS?                   */
*/*%ALS06:  R  RETURN TO CALLER                                      */
*/*%ALS07:  P  GET PTR TO NEXT ASST EXTENSION                        */
*/*         P  GET PTR TO NEXT LPME EXTENSION                        */
*/*         P  GET PTR TO FIRST LPME IN EXTENSION                    */
*/*         P  (,%ALS01) INCREMENT ASST EXT ARRAY SUBSCRIPT          */
*                                                                  0300
*ILRALS00:                          /* LPME SEARCH                   */
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);                          0300
*                                                                  0300
ILRALS00 DS    0H                                                  0301
*/*A-000000-999999                                           @Z40WPWH*/
*                                                                  0301
*   DCL                                                            0301
*     ALSRETRN FIXED(31),           /* RETURN ADDRESS SAVE AREA      */
*     ALSASPCT PTR(31),             /* PTR TO ASPCT BASE             */
*     ALSEXPTR PTR(31),             /* PTR TO ASST EXTENSION         */
*     ALSLEPTR PTR(31),             /* PTR TO LPME EXTENSION         */
*     ALSLPME PTR(31),              /* PTR TO INDIVIDUAL LPME        */
*     ALSRPN FIXED(31),             /* RELATIVE PAGE NUMBER          */
*     ALSA FIXED(31),               /* ASST ARRAY SUBSCRIPT          */
*     ALSL FIXED(31),               /* LPME ARRAY SUBSCRIPT          */
*     ALSB FIXED(31),               /* BASE ARRAY SUBSCRIPT          */
*     ALSCNTL FIXED(8);             /* LOOP CONTROLLER               */
*   DCL                                                            0302
*     ALSZERO FIXED(31) CONSTANT(0),                               0302
*     ALSONE FIXED(31) CONSTANT(1),                                0302
*     ALSTWO FIXED(31) CONSTANT(2),                                0302
*     ALSFIVE FIXED(31) CONSTANT(5),                               0302
*     ALS257 FIXED(31) CONSTANT(257),                              0302
*     ALSLPMEL FIXED(31) INIT(LENGTH(ASPLPME)),                    0302
*     ALSON BIT(1) CONSTANT('1'B);                                 0302
*   RFY                                                            0303
*    (R0,                                                          0303
*     R14) RSTD;                                                   0303
*   ALSRETRN=R14;                   /* SAVE RETURN ADDRESS           */
         ST    R14,ALSRETRN                                        0304
*   ALSASPCT=R0;                    /* SAVE PTR TO ASPCT BASE        */
         ST    R0,ALSASPCT                                         0305
*   RFY                                                            0306
*    (R0,                                                          0306
*     R14) UNRSTD;                                                 0306
*   ALSB=ALSONE;                    /* INIT BASE SUBSCRIPT TO FIRST
*                                      ASST EXTENSION                */
         LA    @06,1                                               0307
         ST    @06,ALSB                                            0307
*   ALSA=ALSTWO;                    /* INIT ASST SUBSCRIPT TO FIRST
*                                      LPME EXTENSION                */
         MVC   ALSA(4),@CF00105                                    0308
*   ALSL=ALSONE;                    /* INIT LPME SUBSCRIPT TO FIRST
*                                      LPME                          */
         LR    ALSL,@06                                            0309
*   ALSRPN=ALSZERO;                 /* INIT THE RPN                  */
         SLR   @05,@05                                             0310
         ST    @05,ALSRPN                                          0310
*   ALSCNTL=ALSONE;                 /* INIT LOOP CONTROLLER GET PTR
*                                      TO LPME'S IN BASE             */
         LR    ALSCNTL,@06                                         0311
*   ALSLPME=ADDR(ALSASPCT->ASPLPMES(ALSL));                        0312
         L     @06,ALSASPCT                                        0312
         LR    @05,ALSL                                            0312
         SLA   @05,2                                               0312
         LA    ALSLPME,ASPLPMES-4(@05,@06)                         0312
*   DO WHILE ALSCNTL^=ALSZERO;                                     0313
         B     @DE00313                                            0313
@DL00313 DS    0H                                                  0314
*     DO WHILE ALSL<ALS257;         /* IS THE LPME VALID OR HAS IT 0314
*                                      BEEN RELEASED AFTER SAVE?     */
         B     @DE00314                                            0314
@DL00314 DS    0H                                                  0315
*       IF ALSLPME->ASPLVALD=ALSONALSLPME->ASPLSVRP=ALSON THEN    0315
         TM    ASPLVALD(ALSLPME),B'10001000'                       0315
         BZ    @RF00315                                            0315
*         DO;                       /* YES                           */
*           RFY                                                    0317
*            (R0,                                                  0317
*             R1) RSTD;                                            0317
*           R0=ALSRPN;              /* LOAD REG0 WITH CURRENT RPN    */
         L     R0,ALSRPN                                           0318
*           R1=ALSLPME;             /* LOAD REG1 WITH PTR TO LPME    */
         LR    R1,ALSLPME                                          0319
*           CALL ALSPROC;           /* CALL USER'S PROC TO PERFORM   */
         L     @15,PROCPTR                                         0320
         BALR  @14,@15                                             0320
*           RFY                                                    0321
*            (R0,                                                  0321
*             R1) UNRSTD;           /* USER'S FUNCTION               */
*         END;                                                     0322
*       ELSE                                                       0323
*         ;                                                        0323
@RF00315 DS    0H                                                  0324
*       ALSRPN=ALSRPN+ALSONE;       /* INCREMENT RPN FOR NEXT LPME   */
         LA    @06,1                                               0324
         L     @05,ALSRPN                                          0324
         ALR   @05,@06                                             0324
         ST    @05,ALSRPN                                          0324
*       ALSLPME=ALSLPME+ALSLPMEL;   /* INCREMENT LPME PTR TO NEXT  0325
*                                      LPME                          */
         AL    ALSLPME,ALSLPMEL                                    0325
*       ALSL=ALSL+ALSONE;           /* INCREMENT LPME SUBSCRIPT      */
         ALR   ALSL,@06                                            0326
*     END;                          /* ARE THERE ANY EXTENSIONS?     */
@DE00314 C     ALSL,@CF01426                                       0327
         BL    @DL00314                                            0327
*     IF ALSASPCT->ASPAEXCT=ALSZERO THEN/* NO                        */
         L     @06,ALSASPCT                                        0328
         LH    @06,ASPAEXCT(,@06)                                  0328
         LTR   @06,@06                                             0328
         BNZ   @RF00328                                            0328
*       ALSCNTL=ALSZERO;            /* FORCE LOOP EXIT               */
         SLR   ALSCNTL,ALSCNTL                                     0329
*     ELSE                                                         0330
*       DO;                         /* YES, PROCESS ALL LPME'S IN  0330
*                                      EXTENSIONS                    */
         B     @RC00328                                            0330
@RF00328 DS    0H                                                  0331
*         ALSL=ALSONE;              /* RESET LPME SUBSCRIPT          */
         LA    ALSL,1                                              0331
*         IF ALSA<ALS257            /* REACHED END OF ASST ARRAY     */
*           THEN                                                   0332
         L     @06,ALSA                                            0332
         C     @06,@CF01426                                        0332
         BNL   @RF00332                                            0332
*           DO;                     /* NO ANYMORE ASST EXTS?         */
*             IF ALSASPCT->ASPASSTP(ALSB)=ALSZERO THEN/* NO          */
         L     @06,ALSB                                            0334
         SLA   @06,2                                               0334
         L     @05,ALSASPCT                                        0334
         L     @06,ASPASSTP-4(@06,@05)                             0334
         LTR   @06,@06                                             0334
         BNZ   @RF00334                                            0334
*               ALSCNTL=ALSZERO;    /* FORCE LOOP EXIT               */
         SLR   ALSCNTL,ALSCNTL                                     0335
*             ELSE                                                 0336
*               DO;                 /* YES GET PTR TO NEXT ASST EXT  */
         B     @RC00334                                            0336
@RF00334 DS    0H                                                  0337
*ALSLOOP:                                                          0337
*                 ALSEXPTR=ALSASPCT->ASPASSTP(ALSB);               0337
*                                                                  0337
ALSLOOP  L     @06,ALSB                                            0337
         SLA   @06,2                                               0337
         L     @05,ALSASPCT                                        0337
         L     ALSEXPTR,ASPASSTP-4(@06,@05)                        0337
*                 /***************************************************/
*                 /*                                                 */
*                 /* IF THERE ARE ANYMORE LPME EXTENSIONS, GET THE   */
*                 /* POINTER TO THE LPME EXTENSION, POINTER TO THE   */
*                 /* LPME, AND INCREMENT THE ASST ARRAY SUBSCRIPT    */
*                 /*                                                 */
*                 /***************************************************/
*                                                                  0338
*                 IF ALSEXPTR->ASPSECTA(ALSA)^=ALSZERO THEN        0338
         L     @06,ALSA                                            0338
         LR    @05,@06                                             0338
         SLA   @05,2                                               0338
         L     @05,ASPSECTA-4(@05,ALSEXPTR)                        0338
         LTR   @05,@05                                             0338
         BZ    @RF00338                                            0338
*                   DO;                                            0339
*                     ALSLEPTR=ALSEXPTR->ASPSECTA(ALSA);           0340
         LR    ALSLEPTR,@05                                        0340
*                     ALSLPME=ADDR(ALSLEPTR->ASPLPMEE(ALSL));      0341
         LR    @05,ALSL                                            0341
         SLA   @05,2                                               0341
         LA    ALSLPME,ASPLPMEE-4(@05,ALSLEPTR)                    0341
*                     ALSA=ALSA+ALSONE;                            0342
         AL    @06,@CF00075                                        0342
         ST    @06,ALSA                                            0342
*                   END;                                           0343
*                 ELSE                                             0344
*                   ALSCNTL=ALSZERO;                               0344
         B     @RC00338                                            0344
@RF00338 SLR   ALSCNTL,ALSCNTL                                     0344
*               END;                                               0345
*           END;                                                   0346
*         ELSE                                                     0347
*           DO;                                                    0347
         B     @RC00332                                            0347
@RF00332 DS    0H                                                  0348
*             ALSA=ALSONE;          /* RESET ASST SUBSCRIPT          */
         LA    @06,1                                               0348
         ST    @06,ALSA                                            0348
*             ALSB=ALSB+ALSONE;     /* INCREMENT BASE SUBSCRIPT ARE
*                                      THERE ANYMORE ASST EXT        */
         AL    @06,ALSB                                            0349
         ST    @06,ALSB                                            0349
*             IF ALSB<ALSFIVE&ALSASPCT->ASPASSTP(ALSB)^=ALSZERO THEN
         C     @06,@CF00462                                        0350
         BNL   @RF00350                                            0350
         SLA   @06,2                                               0350
         L     @05,ALSASPCT                                        0350
         L     @06,ASPASSTP-4(@06,@05)                             0350
         LTR   @06,@06                                             0350
         BNZ   @RT00350                                            0350
*                                   /* YES                           */
*               GOTO ALSLOOP;       /* CONTINUE LOOPING              */
*             ELSE                  /* NO                            */
*               ALSCNTL=ALSZERO;    /* FORCE LOOP EXIT               */
@RF00350 SLR   ALSCNTL,ALSCNTL                                     0352
*           END;                                                   0353
*       END;                                                       0354
@RC00332 DS    0H                                                  0355
*   END;                                                           0355
@RC00328 DS    0H                                                  0355
@DE00313 C     ALSCNTL,@CF00100                                    0355
         BNE   @DL00313                                            0355
*   RFY                                                            0356
*     R14 RSTD;                                                    0356
*   R14=ALSRETRN;                                                  0357
         L     R14,ALSRETRN                                        0357
*   RFY                                                            0358
*     R14 UNRSTD;                                                  0358
*   END ILRALS00;                                                  0359
@EL00006 DS    0H                                                  0359
@EF00006 DS    0H                                                  0359
@ER00006 BR    @14                                                 0359
         EJECT
*                                                                  0360
*/*  MACCOMP Y-4SC1CW/PZD81                                          */
*/*  MACSTAT Y-475082/415304                                         */
*/********************************************************************/
*/*                                                                  */
*/*  NAME:  ILRAFS00                                                 */
*/*                                                                  */
*/*  STATUS:  OS/VS2 RELEASE 4.1, LEVEL 0                            */
*/*                                                                  */
*/*  ENTRY POINT:  ILRAFS00                                          */
*/*                                                                  */
*/*  FUNCTION:  THIS SUBROUTINE FREES AN ASPCT.                      */
*/*                                                                  */
*/*  INPUT:  ADDRESS OF ASPCT IN REGISTER 1.                         */
*/*          LENGTH OF ASPCT IN REGISTER 0.                          */
*/*                                                                  */
*/*  PROCESS:  TWO LOOPS, ONE TO FREE ASST EXTENSIONS AND ONE TO     */
*/*            FREE LPME EXTENSIONS, ARE EXECUTED.  EACH USES THE    */
*/*            SUBROUTINE AFSFREE TO INVOKE FREEMAIN.  A FINAL CALL  */
*/*            TO AFSFREE FREES THE BASE ASPCT EXTN.                 */
*/*                                                                  */
*/*  OUTPUT:  RETURN CODE IN REGISTER 15.                            */
*/*             00 = NORMAL RETURN                                   */
*/*             44 = FREEMAIN FAILURE                                */
*/*                                                                  */
*/*  EXIT-NORMAL:  RETURN TO CALLER WITH RETURN CODE (SEE OUTPUT)    */
*/*                                                                  */
*/*  EXIT-ERROR:  NONE                                               */
*/*                                                                  */
*/*  EXTERNAL REFERENCES:  FREEMAIN                                  */
*/*                                                                  */
*/*  REGISTERS CLOBBERED:  0 AND 1, 14 AND 15                        */
*/*                                                                  */
*/*  MAPPING MACROS USED:  ILRASPCT                                  */
*/*                                                                  */
*/*  NOTES:  DUE TO PLS OPTIMIZATION, RESTRICT ALL NECESSARY         */
*/*          REGISTERS AROUND THIS ROUTINE.                          */
*/*          INVOKING ROUTINE MUST HOLD THE LOCAL LOCK, OR HAVE      */
*/*          SPECIFIED %AFSSVC=YES.                                  */
*/*          ALL LABELS AND DATA AREAS CREATED BY THIS ROUTINE BEGIN */
*/*          WITH THE THREE CHARACTERS 'AFS'.  THE CALLING MODULE    */
*/*          MUST INCLUDE SUFFICIENT WORKSPACE FOR THIS ROUTINE'S    */
*/*          DATA AREAS.  THIS AMOUNT IS COMPILER DEPENDENT WITH A   */
*/*          MAXIMUM REQUIREMENT OF 46 BYTES.  THIS ROUTINE ALSO     */
*/*          REQUIRES THAT THE CALLING MODULE DECLARES REGISTERS     */
*/*          R0-R15.  THE MAPPING MACRO LISTED ABOVE MUST BE         */
*/*          INCLUDED.  THE BASE POINTER FOR THE ASPCT IS NOT        */
*/*          REQUIRED BY THIS SUBROUTINE.                            */
*/*                                                                  */
*/*  METHOD OF ACCESS:                                               */
