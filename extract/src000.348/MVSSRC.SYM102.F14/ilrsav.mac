         TITLE 'ILRSAV,  SAVE LOGICAL GROUP                            *
                        '
ILRSAV   CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'ILRSAV    76.315'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @10,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@10                                         0001
*/*                    END OF CVT MACRO                              */
*                                                                  0048
         EJECT
*                                                                  0048
*/*ILRSAV: CHART (AREF)                                              */
*/*HEADER                                                          0048
*/*            ILRSAV - SAVE GROUP OPERATOR                          */
*/*ILRSAV: E ENTRY                                                   */
*/*         P  SAVE CALLERS REGISTERS                                */
*/*         S  ILRGMA: GET A WORKAREA                                */
*/*         D  (YES,%SAV01A,NO,) DID WE OBTAIN A WORKAREA?           */
*/*         P  SET RETURN CODE TO 28                                 */
*/*         R  RETURN TO CALLER                                      */
*/*%SAV01A: P  SAVE POINTER TO WORKAREA IN REG 9                     */
*/*         P  SET ENTRY REGISTERS                                   */
*/*         P  GET PTR TO EPATH FROM INPUT REG 4                     */
*/*         P  RECORD ENTRY IN EPATH                                 */
*/*         P  RECORD PTR TO WORKAREA IN EPATH                       */
*/*         P  SAVE INPUT PTRS TO ACE, ASMVT & EPATH                 */
*/*         P  RECORD PTR TO REG SAVEAREA IN EPATH                   */
*/*         P  GET THE POINTER TO LGE FROM ACE                       */
*/*         P  INITIALIZE ASMVTPTR FROM INPUT REG3                   */
*/*         P  GET THE POINTER TO ASPCT FROM LGE                     */
*/*         P  ZERO INTERNAL RETURN CODE & RETRIEVED ASPCT PTR       */
*/*         P  RECORD PTR TO ACTIVE ASPCT IN EPATH                   */
*/*         D  (YES,%SAV03,NO,%SAV04) LG BEEN SAVED BEFORE?          */
*/*%SAV03:  S (,%SAV04A) SAVEDASP: RETRIEVE THE COPY FROM STGINDEX   */
*/*%SAV04:  P  DEFINE ALSPROC AS SAVLPME                             */
*/*%SAV04A: P  ZERO THE FREESLOT COUNTER                             */
*/*         P  ZERO THE SAVESLOT COUNTER                             */
*/*         P  SET R0 TO THE ADDRESS OF THE ACTIVE ASPCT             */
*/*         S  ILRALS00: FLAG THE LPME'S AS SAVED                    */
*/*         P  TURN RECOVERYS UNSAVE FLAG ON IN EPATH                */
*/*         D  (YES,,NO,%SAV05) IS THERE A RETRIEVED ASPCT           */
*/*         L  SETLOCK: UNCOND OBTAIN LOCAL LOCK                     */
*/*         P  ZERO EPATH PTR TO RETRIEVED ASPCT                     */
*/*         P  LOAD REG1 WITH RETRIEVED ASPCT BASE                   */
*/*         P  SET REG0 TO LENGTH OF RETRIVED ASPCT                  */
*/*         S  ILRAFS00: FREE THE RETRIEVED ASPCT STORAGE            */
*/*         P  SAVE THE RETURN CODE                                  */
*/*         L  SETLOCK: UNCOND RELEASE LOCAL LOCK                    */
*/*         D  (YES,%SAV05,NO,) RETURN CODE ZERO                     */
*/*         P  SAVE REGS 0 - 14 FOR RECOVERY                         */
*/*         S  ABEND: ISSUE AN 087 ABEND FOR RECORDING               */
*/*%SAV05:  D  (YES,,NO,%SAV06) ANY SLOTS BEEN FREED?                */
*/*         P  USE C&S LOGIC TO SUBTRACT FREESLOT FROM ASMVSC        */
*/*         P  SUBTRACT FREESLOT FROM THE LGE COUNTER                */
*/*%SAV06:  P  ADD SAVESLOT TO THE SAVED SLOT COUNT IN THE ASPCT     */
*/*%SAV07:  P  STORE PTR TO ASPCT BASE IN PARM LIST                  */
*/*         P  SET REQ OP-CODE TO 02                                 */
*/*         S  ILRVSAMI: 02-PUT REQUEST                              */
*/*         P  SAVE THE RETURN CODE FROM VSAMI                       */
*/*         D  (NO,,YES,%SAV09) WAS WRITE SUCCESSFUL?                */
*/*         P  FLAG ASPCT AS HAVING NO SLOTS RELEASED AFTER SAVE     */
*/*         P  FLAG THE ASPCT AS NOT SAVED                           */
*/*         P  DEFINE ALSPROC AS UNSAVLPM                            */
*/*         P  LOAD REG0 WITH ADDR OF ASPCT BASE                     */
*/*         S  ILRALS00: FLAG THE LPME'S AS NOT SAVED                */
*/*         P  (,%SAV11) ZERO THE SAVED SLOT COUNTER IN THE ASPCT    */
*/*%SAV09:  P  FLAG THE ASPCT AS HAVING NO SLOTS RELEASED AFTER SAVE */
*/*%SAV10:  P  FLAG THE ASPCT AS SAVED                               */
*/*         P  COPY THE 'S' SYMBOL TO THE ACE                        */
*/*%SAV11:  P  TURN OFF RECOVERYS UNSAVE FLAG IN EPATH               */
*/*%SAV12:  P  ZERO THE EPATH PTR TO THE ACTIVE ASPCT                */
*/*%SAV12B: P  ZERO THE EPATH PTR TO THE WORKAREA                    */
*/*         P  TURN OFF ENTRY BIT IN EPATH                           */
*/*         S  ILRGMA: FREE THE WORKAREA                             */
*/*         R  RETURN                                                */
*/*ILRSAV: END                                                       */
*                                                                  0048
         EJECT
*                                                                  0048
*/********************************************************************/
*/* MODULE INITIALIZATION. RECORD ENTRY INFORMATION FOR RECOVERY,    */
*/* GET POINTER TO EPATH, AND USE ILRGMA TO OBTAIN A WORKAREA. IF    */
*/* ILRGMA IS UNSUCCESSFUL, RETURN TO ILRGOS WITH A RETURN CODE OF   */
*/* 28. IF ILRGMA IS SUCCESSFUL, ESTABLISH ADDRESSIBILITY TO THE     */
*/* WORKAREA AND ESTABLISH A SAVEAREA.                               */
*/********************************************************************/
*                                                                  0048
*                                   /* GET A WORKAREA FROM POOL      */
*                                                                  0048
*   /*****************************************************************/
*   /*                                                               */
*   /* BEGIN ILRGMA MACRO EXPANSION                                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0048
*   DO;                             /* ILRGMA GET                  0048
*                                      CELL(BWK)WORKREGS(R11,R12)    */
*     DECLARE                       /* ASMVT VECTOR TO ILRPEX        */
*       ILRPEX ENTRY BASED(ASMPEX) VALRG(0);                       0049
*     DECLARE                       /* MAP FOR CELL CONTROL WORDS    */
*       1 GMACELL BASED BDY(WORD),                                 0050
*        2 GMANEXT PTR(31);         /* PTR NEXT FREE CELL            */
*     RESPECIFY                                                    0051
*      (R0,                                                        0051
*       R1,                                                        0051
*       R11,                                                       0051
*       R12,                                                       0051
*       R14,                                                       0051
*       R15) RESTRICTED;                                           0051
*     RESPECIFY                                                    0052
*       ASMPOOLS BASED(R11);                                       0052
*     RESPECIFY                                                    0053
*       GMACELL BASED(R14);                                        0053
*     R11=ADDR(ASMBWKPC);                                          0054
         LA    R11,ASMBWKPC(,ASMVTPTR)                             0054
*     LM(R14,R15,ASMCPAVQ);         /* GET ADDR OF 1ST CELL          */
         LM    R14,R15,ASMCPAVQ(R11)                               0055
*GMA00010:                                                         0056
*     IF R14^=0 THEN                /* TEST CELL POOL                */
GMA00010 LTR   R14,R14                                             0056
         BZ    @RF00056                                            0056
*       DO;                         /* CELL AVAILABLE, REMOVE CELL   */
*         R0=GMANEXT;                                              0058
         L     R0,GMANEXT(,R14)                                    0058
*         R1=R15-1;                                                0059
         LR    R1,R15                                              0059
         BCTR  R1,0                                                0059
*         CDS(R14,R0,ASMCPAVQ);                                    0060
         CDS   R14,R0,ASMCPAVQ(R11)                                0060
*         BC(7,GMA00010);                                          0061
         BC    7,GMA00010                                          0061
*         R1=R14;                                                  0062
         LR    R1,R14                                              0062
*       END;                                                       0063
*     ELSE                                                         0064
*       DO;                         /* CELL POOL EMPTY               */
         B     @RC00056                                            0064
@RF00056 DS    0H                                                  0065
*         R1=R11;                   /* PREPARE TO CALL PEX           */
         LR    R1,R11                                              0065
*         CALL ILRPEX;              /* EXTEND THE POOL               */
         L     @15,ASMPEX(,ASMVTPTR)                               0066
         BALR  @14,@15                                             0066
*       END;                                                       0067
*     RESPECIFY                                                    0068
*      (R0,                                                        0068
*       R1,                                                        0068
*       R11,                                                       0068
*       R12,                                                       0068
*       R14,                                                       0068
*       R15) UNRESTRICTED;                                         0068
@RC00056 DS    0H                                                  0069
*   END;                            /* ILRGMA GET                  0069
*                                      CELL(BWK)WORKREGS(R11,R12)    */
*                                                                  0069
*   /*****************************************************************/
*   /*                                                               */
*   /* END ILRGMA MACRO EXPANSION                                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0070
*   RFY                                                            0070
*    (R1,                                                          0070
*     R3,                                                          0070
*     R15) RSTD;                                                   0070
*   IF R1=ZERO                      /* DID WE GET A WORKAREA?        */
*     THEN                          /* NO                            */
         LTR   R1,R1                                               0071
         BNZ   @RF00071                                            0071
*     RETURN CODE(28);              /* RETURN INDICATING WE COULD NOT
*                                      OBTAIN STORAGE                */
         LA    @15,28                                              0072
         L     @14,12(,@13)                                        0072
         LM    @00,@12,20(@13)                                     0072
         BR    @14                                                 0072
*   ELSE                                                           0073
*     DO;                           /* YES, ESTABLISH ADDRESSIBILITY
*                                      TO WORKAREA & SAVEAREA        */
@RF00071 DS    0H                                                  0074
*       GENERATE REFS(R1,R13) SETS(R1,R8,R9,R13);                  0074
        LR    R9,R1                    GET ADDRESS OF WORKAREA
        USING @DATD,R9
        LA    R8,SAVEREGA              GET ADDRESS OF SAVEAREA
        ST    R13,4(R8)                STORE BACKWARD POINTER
        ST    R8,8(R13)                STORE FORWARD POINTER
        L     R1,24(R13)               GET ADDRESS OF INPUT ACE
        L     R3,32(R13)               GET ADDRESS OF ASMVT
        LR    R13,R8                   SET R13 TO OUR SAVEAREA
*     END;                                                         0075
         EJECT
*                                                                  0076
*/********************************************************************/
*/* SAVE INPUT POINTERS TO THE ACE AND ASMVT, AND RECORD ENTRY       */
*/* VSAM REQUEST PARAMETER LIST (RPL). IF UNSUCCESSFUL, RELEASE THE  */
*/* WORKAREA AND RETURN TO ILRGOS.                                   */
*/********************************************************************/
*                                                                  0076
*   ACEPTR=R1;                      /* GET POINTER TO INPUT ACE      */
         ST    R1,ACEPTR                                           0076
*   ASMVTWPT=R3;                    /* GET POINTER TO ASMVT          */
         ST    R3,ASMVTWPT                                         0077
*   EPATHPTR=EPAPTR;                /* SAVE INPUT PTR TO EPATH       */
         ST    EPAPTR,EPATHPTR                                     0078
*   RFY                                                            0079
*    (R1,                                                          0079
*     R3,                                                          0079
*     R15,                                                         0079
*     ASMVTPTR) UNRSTD;                                            0079
*   RFY                                                            0080
*     ASMVT BASED(ASMVTWPT);                                       0080
*   RFY                                                            0081
*    (R8,                                                          0081
*     R9,                                                          0081
*     R13) RSTD;                                                   0081
*   EPASAVE=ON;                     /* RECORD ENTRY                  */
         OI    EPASAVE(EPAPTR),B'00100000'                         0082
*   EPAOWKA=R9;                     /* RECORD POINTER TO WORKAREA    */
         ST    R9,EPAOWKA(,EPAPTR)                                 0083
         EJECT
*                                                                  0084
*/********************************************************************/
*/*      INITIALIZE POINTERS AND COUNTERS                            */
*/********************************************************************/
*                                                                  0084
*   LGEPTR=ACELGE;                  /* GET ADDRESS OF LGE            */
         L     @12,ACEPTR                                          0084
         L     @12,ACELGE(,@12)                                    0084
         ST    @12,LGEPTR                                          0084
*   ASPCTPTR=LGEASPCT;              /* GET ADDRESS OF BASE ASPCT     */
         L     @12,LGEASPCT(,@12)                                  0085
         ST    @12,ASPCTPTR                                        0085
*   EPAAASP=ASPCTPTR;               /* RECORD PTR TO BASE ASPCT      */
         ST    @12,EPAAASP(,EPAPTR)                                0086
*   RETCODE=ZERO;                   /* INIT INTERNAL RETURN CODE     */
         SLR   @12,@12                                             0087
         ST    @12,RETCODE                                         0087
*   RETASPCT=ZERO;                  /* INIT RETRIEVED ASPCT PTR      */
         SLR   RETASPCT,RETASPCT                                   0088
*   IF ASPSAVED=ON                  /* ASPCT BEEN SAVED BEFORE?      */
*     THEN                          /* YES, THEN CALL SUBROUTINE     */
         L     @12,ASPCTPTR                                        0089
         TM    ASPSAVED(@12),B'10000000'                           0089
         BNO   @RF00089                                            0089
*     CALL SAVEDASP;                /* TO RETRIEVE SAVED COPY PROCPTR
*                                      WILL BE SET BY SAVEDASP       */
         BAL   @14,SAVEDASP                                        0090
*   ELSE                            /* NO, DEFINE THE SAVLPME        */
*     PROCPTR=ADDR(SAVLPME);        /* SUBROUTINE FOR ALS PROCESS.   */
         B     @RC00089                                            0091
@RF00089 LA    @12,SAVLPME                                         0091
         ST    @12,PROCPTR                                         0091
*   FREESLOT=ZERO;                  /* ZERO THE FREED SLOT COUNTER   */
@RC00089 SLR   @12,@12                                             0092
         ST    @12,FREESLOT                                        0092
*   SAVESLOT=ZERO;                  /* ZERO SAVED SLOT COUNTER       */
         ST    @12,SAVESLOT                                        0093
         EJECT
*                                                                  0094
*/********************************************************************/
*/* USE ILRALS00 AND EITHER SAVLPME OR SAVLIMBO TO PROCESS THIS ASPCT*/
*/* SAVLPME WILL BE USED TO PROCESS AN ASPCT THAT HAS NEVER BEEN     */
*/* SAVED, OR AN ASPCT THAT HAS BEEN SAVED BEFORE, BUT DOESN'T HAVE  */
*/* ANY SLOTS THAT HAVE BEEN RELEASED AFTER SAVE. SAVLIMBO IS USED   */
*/* FOR AN ASPCT THAT HAS BEEN SAVED BEFORE, AND HAS SLOTS THAT HAVE */
*/* BEEN RELEASED AFTER SAVE.                                        */
*/********************************************************************/
*                                                                  0094
*   RFY                                                            0094
*     R0 RSTD;                                                     0094
*   R0=ASPCTPTR;                    /* LOAD PTR TO BASE ASPCT        */
         L     R0,ASPCTPTR                                         0095
*   CALL ILRALS00;                  /* MARK THE LPME'S AS SAVED AND
*                                      RELEASE ANY LIMBO SLOTS       */
         BAL   @14,ILRALS00                                        0096
*   RFY                                                            0097
*     R0 UNRSTD;                                                   0097
*   EPAUNSAV=ON;                    /* INDICATE THAT RECOVERY TO FLAG
*                                      THE LPMES AS NOT SAVED.       */
         OI    EPAUNSAV(EPAPTR),B'00000010'                        0098
         EJECT
*                                                                  0099
*/********************************************************************/
*/* USE ILRAFS00 TO FREEMAIN THE I/O BUFFER CHAIN OF RETRIEVED ASPCTS*/
*/********************************************************************/
*                                                                  0099
*   IF RETASPCT^=ZERO               /* WAS THERE A PREVIOUSLY SAVED
*                                      ASPCT?                        */
*     THEN                                                         0099
         LTR   RETASPCT,RETASPCT                                   0099
         BZ    @RF00099                                            0099
*     DO;                           /* YES                           */
*       RFY                                                        0101
*         R8 RSTD;                                                 0101
*       R8=R13;                     /* SAVE SAVEAREA POINTER GET THE
*                                      LOCAL LOCK                    */
         LR    R8,R13                                              0102
*GET1:                                                             0103
*       DO;                         /* SETLOCK (OBTAIN) MODE(UNCOND)
*                                      TYPE(LOCAL)                 0103
*                                      RELATED(VIO,ILRSAV(REL1))     */
GET1     DS    0H                                                  0104
*         RFY                                                      0104
*          (GPR11P,                                                0104
*           GPR12P,                                                0104
*           GPR13P,                                                0104
*           GPR14P) RSTD;                                          0104
*         GPR13P=PSALITA;           /* ADDRESS OF LOCK INTERFACE   0105
*                                      TABLE.                        */
         L     GPR13P,PSALITA                                      0105
*         GEN(LM GPR11P,GPR13P,576(GPR13P));/* LOAD LOCKWORD, CLHT 0106
*                                      AND ENTRY ADRS.               */
         LM GPR11P,GPR13P,576(GPR13P)
*         GEN(BALR GPR14P,GPR13P);  /* BALR TO SETLOCK RTN.          */
         BALR GPR14P,GPR13P
*         RFY                                                      0108
*          (GPR11P,                                                0108
*           GPR12P,                                                0108
*           GPR13P,                                                0108
*           GPR14P) UNRSTD;                                        0108
*       END;                        /* SETLOCK (OBTAIN) MODE(UNCOND)
*                                      TYPE(LOCAL)                 0109
*                                      RELATED(VIO,ILRSAV(REL1))     */
*       R13=R8;                     /* RESTORE SAVEAREA POINTER      */
         LR    R13,R8                                              0110
*       RFY                                                        0111
*         R8 UNRSTD;                                               0111
*       EPARASP=ZERO;               /* ZERO THE EPATH POINTER TO THE
*                                      RETRIEVED ASPCT               */
         SLR   @12,@12                                             0112
         ST    @12,EPARASP(,EPAPTR)                                0112
*       RFY                                                        0113
*        (R0,                                                      0113
*         R1,                                                      0113
*         R15) RSTD;                                               0113
*       R0=BUFLNGTH;                /* SET REG0 TO I/O BUFFER LNG    */
         LA    R0,2048                                             0114
*       R1=RETASPCT;                /* SET REG1 TO ADDR OF BUFFER  0115
*                                      ASPCT BASE                    */
         LR    R1,RETASPCT                                         0115
*       CALL ILRAFS00;              /* FREE THE BUFFERS              */
         BAL   @14,ILRAFS00                                        0116
*       RETCODE=R15;                /* SAVE THE RETURN CODE          */
         ST    R15,RETCODE                                         0117
*       EPAPTR=EPATHPTR;            /* RESTORE EPATH PTR DESTROYED BY
*                                      FREEMAIN IN ILRAFS00          */
         L     EPAPTR,EPATHPTR                                     0118
*       RFY                                                        0119
*        (R0,                                                      0119
*         R1,                                                      0119
*         R15) UNRSTD;                                             0119
*       RFY                                                        0120
*         R8 RSTD;                                                 0120
*       R8=R13;                     /* SAVE SAVEAREA POINTER RELEASE
*                                      THE LOCAL LOCK                */
         LR    R8,R13                                              0121
*REL1:                                                             0122
*       DO;                         /* SETLOCK (RELEASE) TYPE(LOCAL)
*                                      RELATED(VIO,ILRSAV(GET1))     */
REL1     DS    0H                                                  0123
*         RFY                                                      0123
*          (GPR11P,                                                0123
*           GPR12P,                                                0123
*           GPR13P,                                                0123
*           GPR14P) RSTD;                                          0123
*         GPR13P=PSALITA;           /* ADDRESS OF LOCK INTERFACE   0124
*                                      TABLE.                        */
         L     GPR13P,PSALITA                                      0124
*         GEN(LM GPR11P,GPR13P,588(GPR13P));/* LOAD LOCKWORD, CLHT 0125
*                                      AND ENTRY ADRS.               */
         LM GPR11P,GPR13P,588(GPR13P)
*         GEN(BALR GPR14P,GPR13P);  /* BALR TO SETLOCK RTN.          */
         BALR GPR14P,GPR13P
*         RFY                                                      0127
*          (GPR11P,                                                0127
*           GPR12P,                                                0127
*           GPR13P,                                                0127
*           GPR14P) UNRSTD;                                        0127
*       END;                        /* SETLOCK (RELEASE) TYPE(LOCAL)
*                                      RELATED(VIO,ILRSAV(GET1))     */
*       R13=R8;                     /* RESTORE SAVEAREA POINTER      */
         LR    R13,R8                                              0129
*       RFY                                                        0130
*         R8 UNRSTD;                                               0130
*       IF RETCODE^=ZERO            /* DID WE SUCCESSFULLY FREE THE
*                                      I/O BUFFERS                   */
*         THEN                                                     0131
         L     @12,RETCODE                                         0131
         LTR   @12,@12                                             0131
         BZ    @RF00131                                            0131
*         DO;                       /* NO, ISSUE AN 087 ABEND TO   0132
*                                      RECORD THE FREEMAIN ERROR     */
*           EPARTYRG=ADDR(SAVREGSA);/* RECORD REG SAVEAREA PTR       */
         LA    @12,SAVREGSA                                        0133
         ST    @12,EPARTYRG(,EPAPTR)                               0133
*           STM(R0,R14,SAVREGSA);   /* STORE REGS FOR RECOVERY       */
         STM   R0,R14,SAVREGSA                                     0134
*           GEN(ABEND X'87',,,SYSTEM);                             0135
         ABEND X'87',,,SYSTEM
*         END;                                                     0136
*     END;                                                         0137
@RF00131 DS    0H                                                  0138
         EJECT
*                                                                  0138
*/********************************************************************/
*/* UPDATE THE VIO SLOT COUNT IN THE ASMVT, AND SAVED SLOT COUNT IN  */
*/* THE ASPCT BASE.                                                  */
*/********************************************************************/
*                                                                  0138
*   IF FREESLOT^=ZERO               /* WERE ANY SLOTS RELEASED?      */
*     THEN                                                         0138
@RF00099 L     @12,FREESLOT                                        0138
         LTR   @12,@12                                             0138
         BZ    @RF00138                                            0138
*     DO;                           /* YES USE COMPARE AND SWAP TO 0139
*                                      DECREMENT THE VIO SLOT COUNT
*                                      IN THE ASVMT                  */
*       RFY                                                        0140
*        (R1,                                                      0140
*         R14) RSTD;                                               0140
*       R1=ASMVSC;                                                 0141
         L     @15,ASMVTWPT                                        0141
         L     R1,ASMVSC(,@15)                                     0141
*SAVELOOP:                                                         0142
*       R14=R1-FREESLOT;                                           0142
SAVELOOP LR    R14,R1                                              0142
         SL    R14,FREESLOT                                        0142
*       CS(R1,R14,ASMVSC);                                         0143
         L     @15,ASMVTWPT                                        0143
         CS    R1,@14,ASMVSC(@15)                                  0143
*       BC(7,SAVELOOP);                                            0144
         BC    7,SAVELOOP                                          0144
*       LGESLTCT=LGESLTCT-FREESLOT; /* DECREMENT THE LGE SLOT COUNT
*                                      FOR ILRGOS'S UPDATING OF THE
*                                      VIO SLOT COUNT IN THE ASCB    */
         L     @15,LGEPTR                                          0145
         LH    @12,LGESLTCT(,@15)                                  0145
         SL    @12,FREESLOT                                        0145
         STH   @12,LGESLTCT(,@15)                                  0145
*     END;                                                         0146
*   ASPSAVCT=ASPSAVCT+SAVESLOT;     /* INCREMENT THE SAVED SLOT    0147
*                                      COUNTER BY THE NUMBER OF SLOTS
*                                      SAVED                         */
@RF00138 L     @15,ASPCTPTR                                        0147
         L     @12,ASPSAVCT(,@15)                                  0147
         AL    @12,SAVESLOT                                        0147
         ST    @12,ASPSAVCT(,@15)                                  0147
         EJECT
*                                                                  0148
*/********************************************************************/
*/* BUILD THE PARAMETER LIST AND CALL ILRVSAMI TO WRITE THE ASPCT    */
*/* CHAIN OUT TO SYS1.STGINDEX.                                      */
*/********************************************************************/
*                                                                  0148
*   BASEPTR=ASPCTPTR;               /* STORE POINTER TO ASPCT BASE IN
*                                      VSAMI PARAMETER LIST          */
         ST    @15,BASEPTR                                         0148
*   OPCODE=VPUT;                    /* REQUEST A PUT                 */
         MVI   OPCODE,X'02'                                        0149
*   RFY                                                            0150
*    (R1,                                                          0150
*     R3,                                                          0150
*     R15) RSTD;                                                   0150
*   R1=ADDR(PARMLIST);              /* GET ADDR OF PARAMETER LIST    */
         LA    R1,PARMLIST                                         0151
*   R3=ASMVTWPT;                    /* OBTAIN ASMVT ADDR             */
         L     R3,ASMVTWPT                                         0152
*   CALL ILRVSAMI;                  /* CALL ILRVSAMI TO WRITE ASPCT
*                                      OUT TO SYS1.STGINDEX          */
         L     @15,@CV00097                                        0153
         BALR  @14,@15                                             0153
*   RFY                                                            0154
*     R3 UNRSTD;                                                   0154
         EJECT
*                                                                  0155
*/********************************************************************/
*/* SAVE THE RETURN CODE FROM ILRVSAMI. IF SUCCESSFUL, FLAG THE ASPCT*/
*/* AS SAVED AND AS HAVING NO SLOTS RELEASED AFTER SAVE, AND COPY THE*/
*/* 'S' SYMBOL TO THE INPUT ACE.                                     */
*/********************************************************************/
*                                                                  0155
*   RETCODE=R15;                    /* SAVE THE RETURN CODE          */
         ST    R15,RETCODE                                         0155
*   IF R15=ZERO                     /* DID WE SUCCESSFULLY WRITE THE
*                                      ASPCT OUT TO SYS1.STGINDEX    */
*     THEN                                                         0156
         LTR   R15,R15                                             0156
         BNZ   @RF00156                                            0156
*     DO;                           /* YES                           */
*       RFY                                                        0158
*        (R1,                                                      0158
*         R15) UNRSTD;                                             0158
*       ASPSAVRP=OFF;               /* INDICATE NO SLOTS HAVE BEEN 0159
*                                      RELEASED-AFTER-SAVE           */
*       ASPSAVED=ON;                /* INDICATE ASPCT HAS BEEN SAVED */
         L     @15,ASPCTPTR                                        0160
         OI    ASPSAVED(@15),B'10000000'                           0160
         NI    ASPSAVRP(@15),B'10111111'                           0160
*       ACESYM=ASPSSYM;             /* COPY THE 'S' SYMBOL TO THE  0161
*                                      INPUT ACE                     */
         L     @12,ACEPTR                                          0161
         MVC   ACESYM(8,@12),ASPSSYM(@15)                          0161
*     END;                                                         0162
*                                                                  0162
*/********************************************************************/
*/* THE WRITE WAS UNSUCCESSFUL. FLAG THE ASPCT AS NOT SAVED, AND AS  */
*/* HAVING NO SLOTS RELEASED AFTER SAVE. USE ILRALS00 AND UNSAVLPM   */
*/* TO TURN THE SAVED FLAG OFF IN EACH VALID LPME. ZERO THE SAVED    */
*/* SLOT COUNTER IN THE ASPCT BASE.                                  */
*/********************************************************************/
*                                                                  0163
*   ELSE                                                           0163
*     DO;                           /* NO, HAD AN ERROR WRITTING     */
         B     @RC00156                                            0163
@RF00156 DS    0H                                                  0164
*       ASPSAVRP=OFF;               /* INDICATE NO SLOTS HAVE BEEN 0164
*                                      RELEASED-AFTER-SAVE           */
*       ASPSAVED=OFF;               /* INDICATE THE ASPCT HAS NOT  0165
*                                      BEEN SAVED                    */
         L     @15,ASPCTPTR                                        0165
         NI    ASPSAVRP(@15),B'00111111'                           0165
*       PROCPTR=ADDR(UNSAVLPM);     /* DEFINE ALSPROC AS UNSAVLPM    */
         LA    @12,UNSAVLPM                                        0166
         ST    @12,PROCPTR                                         0166
*       RFY                                                        0167
*         R0 RSTD;                                                 0167
*       R0=ASPCTPTR;                /* LOAD REG0 WITH PTR TO ASPCT 0168
*                                      BASE                          */
         LR    R0,@15                                              0168
*       CALL ILRALS00;              /* TURN THE SAVED FLAG OFF IN    */
         BAL   @14,ILRALS00                                        0169
*       RFY                                                        0170
*         R0 UNRSTD;                /* THE LPME'S                    */
*       ASPSAVCT=ZERO;              /* ZERO THE ASPCT SAVED SLOT   0171
*                                      COUNT IN BASE                 */
         L     @15,ASPCTPTR                                        0171
         SLR   @12,@12                                             0171
         ST    @12,ASPSAVCT(,@15)                                  0171
*     END;                                                         0172
         EJECT
*                                                                  0173
*/********************************************************************/
*/* CLEANUP: RETURN WORKAREA TO ITS POOL.                            */
*/********************************************************************/
*                                                                  0173
*   EPAAASP=ZERO;                   /* ZERO RECOVERYS PTR TO ACTIVE
*                                      ASPCT                         */
@RC00156 SLR   @15,@15                                             0173
         ST    @15,EPAAASP(,EPAPTR)                                0173
*   EPAUNSAV=OFF;                   /* TURN OFF RECOVERYS UNSAVE FLAG
*                                      IN THE EPATH                  */
         NI    EPAUNSAV(EPAPTR),B'11111101'                        0174
*   EPAOWKA=ZERO;                   /* ZERO RECOVERY'S WKA PTR       */
         ST    @15,EPAOWKA(,EPAPTR)                                0175
*   EPASAVE=OFF;                    /* TURN ENTRY FLAG OFF           */
         NI    EPASAVE(EPAPTR),B'11011111'                         0176
*   EPARTYRG=ZERO;                  /* ZERO REG SAVEAREA PTR         */
         ST    @15,EPARTYRG(,EPAPTR)                               0177
*   GENERATE;                                                      0178
*                                     /* SAVE INTERNAL RETURN CODE  */
        L       R13,4(R13)               GET PTR CALLERS SAVEAREA
        MVC     16(4,R13),RETCODE        SET CALLERS R15 TO RET CODE
*   RFY                                                            0179
*     R1 RSTD;                                                     0179
*   R1=R9;                          /* PUT WORKAREA ADDR IN REG 1  0180
*                                      FREE THE WKA BACK TO POOL     */
*                                                                  0180
         LR    R1,R9                                               0180
*   /*****************************************************************/
*   /*                                                               */
*   /* BEGIN ILRGMA MACRO EXPANSION                                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0181
*   DO;                             /* ILRGMA FREE                 0181
*                                      CELL(BWK)WORKREGS(R11,R12)    */
*     RESPECIFY                                                    0182
*      (R0,                                                        0182
*       R1,                                                        0182
*       R11,                                                       0182
*       R12,                                                       0182
*       R14,                                                       0182
*       R15) RESTRICTED;                                           0182
*     RESPECIFY                                                    0183
*       ASMPOOLS BASED(R11);                                       0183
*     RESPECIFY                                                    0184
*       GMACELL BASED(R1);                                         0184
*     R11=ADDR(ASMBWKPC);                                          0185
         L     R11,ASMVTWPT                                        0185
         LA    R11,ASMBWKPC(,R11)                                  0185
*     R14=ASMCPAVL;                                                0186
         L     R14,ASMCPAVL(,R11)                                  0186
*GMA00062:                                                         0187
*     GMANEXT=R14;                  /* PUT ADDR CURRENT FIRST IN   0187
*                                      INPUT CELL                    */
GMA00062 ST    R14,GMANEXT(,R1)                                    0187
*     CS(R14,R1,ASMCPAVL);          /* ADD INPUT CELL TO POOL        */
         CS    R14,@01,ASMCPAVL(R11)                               0188
*     BC(7,GMA00062);                                              0189
         BC    7,GMA00062                                          0189
*     RESPECIFY                                                    0190
*      (R0,                                                        0190
*       R1,                                                        0190
*       R11,                                                       0190
*       R12,                                                       0190
*       R14,                                                       0190
*       R15) UNRESTRICTED;                                         0190
*   END;                            /* ILRGMA FREE                 0191
*                                      CELL(BWK)WORKREGS(R11,R12)    */
*                                                                  0191
*   /*****************************************************************/
*   /*                                                               */
*   /* END ILRGMA MACRO EXPANSION                                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0192
*   RETURN;                         /* RETURN TO CALLER              */
@EL00001 DS    0H                                                  0192
@EF00001 DS    0H                                                  0192
@ER00001 LM    @14,@12,12(@13)                                     0192
         BR    @14                                                 0192
         EJECT
*                                                                  0193
*/********************************************************************/
*/*                                                                  */
*/*  SAVEDASP: RETRIEVE AND ERASE A SAVED ASPCT                      */
*/*                                                                  */
*/*  INPUT: PTR TO INPUT ACE                                         */
*/*                                                                  */
*/*  PROCESS: SAVE THE RETURN ADDRESS. BUILD THE PARAMETER LIST FOR  */
*/*           ILRVSAMI, AND CALL ILRVSAMI TO RETRIEVE AND ERASE THE  */
*/*           ASPCT FROM SYS1.STGINDEX. IF THE READ WAS UNSUCCESSFUL */
*/*           FLAG THE ACTIVE ASPCT AS NOT SAVED. IF ANY SLOTS WERE  */
*/*           RELEASED AFTER SAVE DEFINE ALSPROC AS SAVLIMBO, FOR    */
*/*           ILRALS00 PROCESSING. IF NO SLOTS HAVE BEEN RELEASED    */
*/*           AFTER SAVE, DEFINE ALSPROC AS SAVLPME.                 */
*/*                                                                  */
*/*  OUTPUT: PTR TO THE RETRIEVED ASPCT OR THE ACTIVE ASPCT FLAGGED  */
*/*          AS NOT SAVED.                                           */
*/*                                                                  */
*/********************************************************************/
*/*SAVEDASP: CHART (AREF)                                            */
*/*HEADER                                                          0193
*/*  ILRSAV                                                        0193
*/* SAVEDASP: RE-ESTABLISH A SAVED ASPCT                             */
*/*SAVEDASP: E ENTRY                                                 */
*/*         P  SAVE THE RETURN ADDRESS                               */
*/*         P  GET THE ADDRESS OF THE 'S' SYMBOL                     */
*/*         P  STORE PTR TO 'S' SYMBOL IN PARM LIST                  */
*/*         P  SET REQ OP-CODE TO 03                                 */
*/*         S  ILRVSAMI: 03-RETRIEVE AND ERASE                       */
*/*         P  SAVE RETURN CODE FROM VSAMI                           */
*/*         P  SAVE PTR TO RETRIEVED ASPCT                           */
*/*         P  SET RECOVERYS UNSAVE FLAG IN EPATH                    */
*/*         D  (NO,,YES,%SAV14) ZERO RETURN CODE?                    */
*/*         P  FLAG THE ASPCT AS NOT SAVED                           */
*/*%SAV15:  D  (NO,,YES,%SAV13) ANY SLOTS RELEASED AFTER SAVE?       */
*/*         P  (,%SAV16) DEFINE ALSPROC AS SAVLPME                   */
*/*%SAV13:  P  (,%SAV16) DEFINE ALSPROC AS SAVLIMBO                  */
*/*%SAV14:  P  (,%SAV15) RECORD PTR TO RETRIEVED ASPCT IN EPATH      */
*/*%SAV16:  R  RETURN                                                */
*/*SAVEDASP: END                                                     */
*                                                                  0193
         EJECT
*SAVEDASP:                                                         0193
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);                          0193
SAVEDASP DS    0H                                                  0194
*   RFY                                                            0194
*     R14 RSTD;                                                    0194
*   RETADDR1=R14;                   /* SAVE THE RETURN ADDRESS       */
         LR    RETADDR1,R14                                        0195
*   RFY                                                            0196
*     R14 UNRSTD;                                                  0196
*                                                                  0196
*/********************************************************************/
*/* STORE THE POINTER TO THE 'S' SYMBOL, IN THE ASPCT BASE, IN THE   */
*/* PARAMETER LIST FOR ILRVSAMI AND SET THE REQUEST OP-CODE FOR      */
*/* RETRIEVE AND ERASE.                                              */
*/********************************************************************/
*                                                                  0197
*   SSYMPTR=ADDR(ASPSSYM);          /* STORE PTR TO 'S' SYMBOL IN  0197
*                                      VSAMI PARAMETER LIST          */
         L     @12,ASPCTPTR                                        0197
         LA    @12,ASPSSYM(,@12)                                   0197
         ST    @12,SSYMPTR                                         0197
*   OPCODE=RETE;                    /* SET REQUEST OPCODE FOR      0198
*                                      RETRIEVING AND ERASE          */
         MVI   OPCODE,X'03'                                        0198
*   RFY                                                            0199
*    (R1,                                                          0199
*     R3,                                                          0199
*     R15) RSTD;                                                   0199
*   R1=ADDR(PARMLIST);              /* GET ADDR OF PARAMETER LIST    */
         LA    R1,PARMLIST                                         0200
*   R3=ASMVTWPT;                    /* OBTAIN ASMVT ADDR             */
         L     R3,ASMVTWPT                                         0201
*   CALL ILRVSAMI;                  /* CALL VSAMI TO RETRIEVE AND  0202
*                                      ERASE SAVED COPY              */
         L     @15,@CV00097                                        0202
         BALR  @14,@15                                             0202
*   RFY                                                            0203
*     R3 UNRSTD;                                                   0203
         EJECT
*                                                                  0204
*/****************************************************************** */
*/* SAVE THE RETURN CODE AND POINTER TO THE RETRIEVED ASPCT. IF THE  */
*/* RETRIEVAL WAS UNSUCCESSFUL, MARK THE ACTIVE ASPCT AS NOT BEING   */
*/* SAVED. IF IT WAS SUCCESSFUL, REOCRD THE POINTER TO THE RETRIEVED */
*/* ASPCT CHAIN IN THE EPATH.                                        */
*/********************************************************************/
*                                                                  0204
*   RETCODE=R15;                    /* SAVE THE RETURN CODE          */
         ST    R15,RETCODE                                         0204
*   RETASPCT=R1;                    /* SAVE PTR TO ASPCT BASE IN I/O
*                                      BUFFER                        */
         LR    RETASPCT,R1                                         0205
*   EPAUNSAV=ON;                    /* INDICATE THAT RECOVERY TO FLAG
*                                      THE LPMES AS NOT SAVED.       */
         OI    EPAUNSAV(EPAPTR),B'00000010'                        0206
*   IF R15^=ZERO                    /* DID WE RETRIEVE ASPCT         */
*     THEN                          /* NO                            */
         LTR   R15,R15                                             0207
         BZ    @RF00207                                            0207
*     ASPSAVED=OFF;                 /* FLAG ACTIVE ASPCT AS NOT SAVED*/
         L     @14,ASPCTPTR                                        0208
         NI    ASPSAVED(@14),B'01111111'                           0208
*   ELSE                            /* YES                           */
*     EPARASP=RETASPCT;             /* RECORD PTR TO RETRIEVED ASPCT
*                                      FOR RECOVERY                  */
         B     @RC00207                                            0209
@RF00207 ST    RETASPCT,EPARASP(,EPAPTR)                           0209
*   RFY                                                            0210
*    (R1,                                                          0210
*     R15) UNRSTD;                                                 0210
*                                                                  0210
@RC00207 DS    0H                                                  0211
*/********************************************************************/
*/* IF ANY SLOTS HAVE BEEN RELEASED AFTER SAVE, DEFINE ALSPROC AS    */
*/* SAVLIMBO, IF NOT, DEFINE ALSPROC AS SAVLPME. RETURN TO MAINLINE  */
*/********************************************************************/
*                                                                  0211
*   IF ASPSAVRP=ON                  /* ANY SLOTS REL-AFTER-SAVE IN 0211
*                                      ACTIVE ASPCT                  */
*     THEN                          /* YES                           */
         L     @12,ASPCTPTR                                        0211
         TM    ASPSAVRP(@12),B'01000000'                           0211
         BNO   @RF00211                                            0211
*     PROCPTR=ADDR(SAVLIMBO);       /* DEFINE ALSPROC AS SAVLIMBO    */
         LA    @12,SAVLIMBO                                        0212
         ST    @12,PROCPTR                                         0212
*   ELSE                            /* NO                            */
*     PROCPTR=ADDR(SAVLPME);        /* DEFINE ALSPROC AS SAVLPME     */
         B     @RC00211                                            0213
@RF00211 LA    @12,SAVLPME                                         0213
         ST    @12,PROCPTR                                         0213
*   RFY                                                            0214
*     R14 RSTD;                                                    0214
@RC00211 DS    0H                                                  0215
*   R14=RETADDR1;                   /* GET RETURN ADDRESS            */
         LR    R14,RETADDR1                                        0215
*   END SAVEDASP;                                                  0216
@EL00002 DS    0H                                                  0216
@EF00002 DS    0H                                                  0216
@ER00002 BR    @14                                                 0216
*   RFY                                                            0217
*     R14 UNRSTD;                                                  0217
         EJECT
*                                                                  0218
*/********************************************************************/
*/*                                                                  */
*/*  SAVLPME: FLAG UNSAVED LPMES AS SAVED.                           */
*/*                                                                  */
*/*  INPUT: REGISTER 0 = CORRESPONDING RPN                           */
*/*         REGISTER 1 = PTR TO LPME                                 */
*/*                                                                  */
*/*  PROCESS: IF THE LPME SAVE FLAG IS OFF, FLAG THE LPME AS SAVED   */
*/*           AND INCREMENT THE SAVED SLOT COUNTER.                  */
*/*                                                                  */
*/*  OUTPUT: THE LPME FLAGGED AS SAVED.                              */
*/*                                                                  */
*/********************************************************************/
*/*SAVLPME: CHART (AREF)                                             */
*/*HEADER                                                          0218
*/*  ILRSAV                                                        0218
*/* SAVLPME: FLAG UNSAVED LPME'S AS SAVED                            */
*/*SAVLPME: E  ENTRY                                                 */
*/*         D  (NO,,YES,%SAV17) LPME BEEN SAVED BEFORE?              */
*/*         P  FLAG THE LPME AS SAVED                                */
*/*         P  INCREMENT THE INTERNAL SAVED SLOT COUNTER             */
*/*%SAV17:  R  RETURN TO ILRALS00                                    */
*/*SAVLPME: END                                                      */
*                                                                  0218
*SAVLPME:                                                          0218
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);                          0218
SAVLPME  DS    0H                                                  0219
*   RFY                                                            0219
*    (R1,                                                          0219
*     R14) RSTD;                                                   0219
*   IF R1->ASPLSAVE=OFF             /* LPME BEEN SAVED BEFORE?       */
*     THEN                                                         0220
         TM    ASPLSAVE(R1),B'01000000'                            0220
         BNZ   @RF00220                                            0220
*     DO;                           /* NO                            */
*       R1->ASPLSAVE=ON;            /* FLAG THE LPME AS SAVED        */
         OI    ASPLSAVE(R1),B'01000000'                            0222
*       SAVESLOT=SAVESLOT+ONE;      /* INCREMENT SAVED SLOT          */
         LA    @15,1                                               0223
         AL    @15,SAVESLOT                                        0223
         ST    @15,SAVESLOT                                        0223
*     END;                          /* COUNTER                       */
*   ELSE                                                           0225
*     ;                                                            0225
@RF00220 DS    0H                                                  0226
*   RFY                                                            0226
*    (R1,                                                          0226
*     R14) UNRSTD;                                                 0226
*   END SAVLPME;                                                   0227
@EL00003 DS    0H                                                  0227
@EF00003 DS    0H                                                  0227
@ER00003 BR    @14                                                 0227
         EJECT
*                                                                  0228
*/********************************************************************/
*/*                                                                  */
*/*  UNSAVLPM: FLAG THE SAVED LPMES AS NOT SAVED.                    */
*/*                                                                  */
*/*  INPUT: REGISTER 0 = CORRESPONDING RPN                           */
*/*         REGISTER 1 = PTR TO LPME                                 */
*/*                                                                  */
*/*  PROCESS: IF THE LPME SAVE FLAG IS ON, FLAG THE LPME AS NOT SAVED*/
*/*           AND INCREMENT THE SAVED SLOT COUNTER.                  */
*/*                                                                  */
*/*  OUTPUT: THE LPME SAVE FLAG TURNED OFF.                          */
*/*                                                                  */
*/********************************************************************/
*/*UNSAVLPM: CHART (AREF)                                            */
*/*HEADER                                                          0228
*/*  ILRSAV                                                        0228
*/* UNSAVLPM: FLAG SAVED LPME'S AS NOT SAVED                         */
*/*UNSAVLPM: E ENTRY                                                 */
*/*         P  FLAG THE LPME AS NOT SAVED                            */
*/*%SAV18:  R  RETURN TO ILRALS00                                    */
*/*UNSAVLPM: END                                                     */
*                                                                  0228
*UNSAVLPM:                                                         0228
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);                          0228
UNSAVLPM DS    0H                                                  0229
*   RFY                                                            0229
*    (R1,                                                          0229
*     R14) RSTD;                                                   0229
*   R1->ASPLSAVE=OFF;               /* TURN THE LPME SAVE FLAG OFF   */
         NI    ASPLSAVE(R1),B'10111111'                            0230
*   RFY                                                            0231
*    (R1,                                                          0231
*     R14) UNRSTD;                                                 0231
