         L     R4,AFSREG4                                          0433
*   R7=AFSREG7;                     /* RESTORE CALLER'S REG 7        */
         L     R7,AFSREG7                                          0434
*   R14=AFSRET1;                    /* GET RETURN ADDRESS            */
         L     R14,AFSRET1                                         0435
