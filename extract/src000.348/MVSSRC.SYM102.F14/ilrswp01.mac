         L     REPLREG,SRESCCW(,SAREPTR)                           0250
*RTRYCS2:                                                          0251
*           SCCWSCCW=COMPREG;       /* GET SART SCCW QUEUE ANCHOR    */
RTRYCS2  ST    COMPREG,SCCWSCCW(,SCCWPTR)                          0251
*           CS(COMPREG,REPLREG,SARSCCWQ);/* UPDATE SART SCCW QUEUE   */
         CS    COMPREG,@07,SARSCCWQ(SARTPTR)                       0252
*           BC(NE,RTRYCS2);         /* TRY AGAIN                     */
         BC    7,RTRYCS2                                           0253
*           RFY                                                    0254
*            (COMPREG,                                             0254
*             REPLREG,                                             0254
*             SCCWPTR) UNRSTD;                                     0254
*         END;                      /* NON-ZERO SARTE SCCW QUEUE     */
*                                                                  0256
*       /*************************************************************/
*       /*                                                           */
*       /* THE AVAILABLE SWAP SET COUNT FOR THIS SART ENTRY IS ZEROED*/
