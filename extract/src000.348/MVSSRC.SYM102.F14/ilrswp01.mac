*           /*                                                       */
*           /*********************************************************/
*                                                                  0292
*           ATARCRF6=ON;            /* SET RECURSION FLAG            */
         OI    ATARCRF6(ATAPTR),B'00000100'                        0292
*           SDWARTYA=ADDR(ILRCRSP1);/* SWAP PROCESSOR'S RETRY ADDRESS*/
         L     @12,@CV02457                                        0293
         ST    @12,SDWARTYA(,SDWAPTR)                              0293
*           RFY                                                    0294
*            (R0,                                                  0294
*             SWAPBASE,                                            0294
*             R15) RSTD;                                           0294
*           SWAPBASE=ADDR(ILRCBSP1);/* LABEL ON SWAP'S BASE REG      */
         L     SWAPBASE,@CV02459                                   0295
*           STM(R0,R15,SDWASRSV);   /* RETRY REGISTERS: R2- RSMHD, 0296
*                                      R3- ASMVT, AND R12- BASE REG  */
         STM   R0,R15,SDWASRSV(SDWAPTR)                            0296
*           RFY                                                    0297
*            (R0,                                                  0297
*             SWAPBASE,                                            0297
*             R15) UNRSTD;                                         0297
*         END;                      /* RETRY ALLOWED                 */
*     END;                          /* NON ZERO SWAP QUEUE           */
@RF00290 DS    0H                                                  0300
*   RFY                                                            0300
*     R14 RSTD;                     /* RETURN REGISTER               */
@RF00287 DS    0H                                                  0301
*   R14=SAVER14;                    /* RESTORE RETURN ADDRESS        */
         L     R14,SAVER14                                         0301
*   RFY                                                            0302
