*/********************************************************************/
*/*                                                                  */
*/*  ILRCSWAP: SWAP PROCESSOR (FRONT END) RECOVERY                   */
*/*                                                                  */
*/*  INPUT: REGISTER INTERFACE  REG 0 - SDUMP BUFFER ADDRESS OR 0    */
*/*                             REG 1 - SDWA ADDRESS                 */
*/*                             REG 3 - ASMVT ADDRESS                */
*/*                             REG 4 - ATA ADDRESS                  */
*/*                             REG 8 - VERIFICATION WORKAREA ADDR   */
*/*                             REG 9 - FRR WORKAREA ADDRESS         */
*/*         THIS ROUTINE IS ENTERED WITH THE SALLOC LOCK HELD. THIS  */
*/*         ROUTINE IS NOT ENTERED FOR NONRETRYABLE ERRORS.          */
*/*                                                                  */
*/*  PROCESS: THE ASMHD SWAP QUEUE IS VALIDITY CHECKED AND A RETRY   */
*/*           IS SET UP TO RETURN THE SWAP REQUEST TO RSM.           */
*/*                                                                  */
*/*  OUTPUT: NO STREAM OR LIST OUTPUT                                */
*/*                                                                  */
*/********************************************************************/
*/*ILRCSWAP: CHART SWAP PROCESSOR RECOVERY                           */
*/*ILRCSWAP:E ENTRY TO ILRSWAP RECOVERY                              */
*/*         N INPUT- R1=SDWA@, R3=ASMVT@, R4=ATA@, R8=WORKAREA@      */
*/*         P ESTABLISH ASMHD ADDRESSIBILITY                         */
*/*         P ESTABLISH REGISTER SAVE AREA                           */
*/*         P SET UP REGISTERS FOR ILRVSPAQ                          */
*/*         S ILRVSPAQ: VALIDITY CHECK ASMHD SWAP QUEUE              */
*/*         D (YES,,NO,%RTN) FIRST AIA VALID ?                       */
*/*%VDAIA:  P SET PERMANENT ERROR FLAG IN 1ST AIA                    */
*/*         D (YES,%RTN,NO,) RECURSION ? ATARCRF6='1'B               */
*/*         P SET UP RETRY ADDRESS IN SDWA                           */
*/*         P SET RECURSION FLAG IN ATA                              */
*/*%RTN:    R RETURN TO CALLER (ILRIOFRR)                            */
*/*ILRCSWAP: END                                                     */
*                                                                  0277
*ILRCSWAP:                                                         0277
*   ENTRY;                          /* ENTRY TO FRONT-END SWAP     0277
*                                      RECOVERY                      */
@EP00277 DS    0H                                                  0278
*   RFY                                                            0278
*     R14 RSTD;                     /* RETURN REGISTER               */
*   SAVER14=R14;                    /* SAVE RETURN ADDRESS           */
         ST    R14,SAVER14                                         0279
*   RFY                                                            0280
*     R14 UNRSTD;                                                  0280
*   RSMHDPTR=ASM4RG2;               /* RSMHD ADDR STORED BY ILRSWAP  */
         L     RSMHDPTR,ASM4RG2(,ASMVTPTR)                         0281
*   SAREG=ADDR(ASMSAVE);            /* REGISTER SAVE AREA (REG 13)   */
         LA    SAREG,ASMSAVE(,ASMVTPTR)                            0282
*   RFY                                                            0283
*    (VERFREG,                                                     0283
*     R5,                                                          0283
*     R6,                                                          0283
*     R7) RSTD;                     /* VERIFY RTN REGISTERS          */
*   VERFREG=ADDR(ASHSWAPQ);         /* ADDRESS OF QUEUE TO BE      0284
*                                      VERIFIED                      */
         LA    VERFREG,ASHSWAPQ(,RSMHDPTR)                         0284
*   CALL ILRVSPAQ;                  /* VERIFY SWAP QUEUE             */
         L     @15,@CV02465                                        0285
         BALR  @14,@15                                             0285
*   RFY                                                            0286
*    (VERFREG,                                                     0286
*     R5,                                                          0286
*     R6,                                                          0286
*     R7) UNRSTD;                   /* VERIFY RTN REGISTERS          */
*   IF ASHSWAPQ^=ZERO THEN          /* IS QUEUE EMPTY ?              */
         L     @12,ASHSWAPQ(,RSMHDPTR)                             0287
         LTR   @12,@12                                             0287
         BZ    @RF00287                                            0287
*     DO;                           /* NO- SETUP FOR RETRY           */
*       ASHSWAPQ->AIAERROR=ON;      /* ERROR FLAG IN FIRST AIA       */
         OI    AIAERROR(@12),B'00000010'                           0289
*       IF ATARCRF6=OFF THEN        /* RECURSIVE ERROR ?             */
         TM    ATARCRF6(ATAPTR),B'00000100'                        0290
         BNZ   @RF00290                                            0290
*         DO;                       /* NO- ALLOW RETRY               */
*                                                                  0291
*           /*********************************************************/
*           /*                                                       */
*           /* SET UP FOR RETRY. A NON-ZERO RETRY ADDRESS IN SDWARTYA*/
