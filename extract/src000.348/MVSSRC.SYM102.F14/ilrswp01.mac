*/********************************************************************/
*/*                                                                  */
*/*  ILRCSLSQ: SWAP LSQA PROCESSOR RECOVERY                          */
*/*                                                                  */
*/*  INPUT: REGISTER INTERFACE  REG 1 - SDWA ADDRESS                 */
*/*                             REG 3 - ASMVT ADDRESS                */
*/*                             REG 4 - ATA ADDRESS                  */
*/*                             REG 8 - VERIFICATION WORKAREA ADDR   */
*/*                             REG 9 - FRR WORKAREA ADDRESS         */
*/*         THIS ROUTINE IS ENTERED WITH THE SALLOC LOCK HELD.       */
*/*                                                                  */
*/*  PROCESS: RESOURCES ARE VALIDITY CHECKED AND RETURNED TO QUEUES, */
*/*           ERROR INFORMATION IS RECORDED, AND CONTINUITY OF ASM   */
*/*           SWAP PROCESSING IS INSURED.                            */
*/*                                                                  */
*/*  OUTPUT: NO STREAM OR LIST OUTPUT                                */
*/*                                                                  */
*/********************************************************************/
*/*ILRCSLSQ: CHART SWAP LSQA RECOVERY                                */
*/*ILRCSLSQ:E ENTRY TO ILRSLSQA RECOVERY                             */
*/*         N INPUT- R1=SDWA@, R3=ASMVT@, R4=ATA@, R8=WORKAREA@      */
*/*         P ESTABLISH ASMHD ADDRESSIBILITY                         */
*/*         P ESTABLISH REGISTER SAVE AREA                           */
*/*         D (YES,,NO,%SKIP1) ASIGNSET SUBROUTINE IN CONTROL ?      */
*/*         D (YES,,NO,%CKSPQ) C0D ABEND ?                           */
*/*         S (,%C0D) ASETRCVY: PROCESS C0D ABEND IN ASIGNSET        */
*/*%SKIP1:  D (YES,,NO,%CKSPQ) SCCWPROC SUBROUTINE IN CONTROL ?      */
*/*         S SCCWRCVY: PROCESS ERROR IN SCCWPROC                    */
*/*         D (YES,%C0D,NO,%RECRS) RECORD ONLY ABEND ?               */
*/*%CKSPQ:  D (YES,%NOQUE1,NO,) ASMHD SWAP QUEUE = 0 ?               */
*/*         D (YES,,NO,%TERM1) SWAP REQUEST ON STAGE Q ?             */
*/*         P (,%RECRS) CLEAR ASMHD SWAP Q ANCHOR                    */
*/*%TERM1:  D (YES,,NO,%SOUT) SWAP IN REQUEST ?                      */
*/*         P SET SWAPIN FAIL FLAG (RSMFAIL= '1'B)                   */
*/*%SOUT:   P SET UP REGS FOR CALLRTM MACRO                          */
*/*         L (,%RECRS) CALLRTM: TERMINATE THE MEMORY                */
*/*%NOQUE1: S ILRVSWTQ: VALIDITY CHECK SART WAIT QUEUE               */
*/*         P SET UP REGS FOR ILRVSPAQ                               */
*/*         S ILRVSPAQ: VALIDITY CHECK ATA AIA QUEUE                 */
*/*         D (YES,,NO,%RECRS) 1ST AIA VALID ?                       */
*/*         P SEARCH SART WAIT QUEUE FOR ATA AIA                     */
*/*         D (YES,%RECRS,NO,) AIA ON WAIT QUEUE ?                   */
*/*         P SEARCH ATA AIA QUEUE FOR LAST GROUP AIA                */
*/*         D (YES,,NO,%NEMPTY) SART WAIT QUEUE EMPTY ?              */
*/*         P (,%STLST1) SET 1ST AIA(SARWAITF) = ATA AIA QUEUE       */
*/*%NEMPTY: P ADD ATA AIA QUEUE TO END OF SART WAIT QUEUE            */
*/*%STLST1: P SART WAIT Q LAST AIA= ATA LAST GROUP AIA               */
*/*%RECRS:  D (YES,%RTN1,NO,) RECURSION ? ATARCRF6='1'B              */
*/*         P SET RECURSION FLAG IN ATA                              */
*/*         P (,%RTN1) SET NORMAL RETRY@ IN SDWA                     */
*/*%C0D:    P SET SDWA RETRY@ = NSI AFTER ERROR PSW                  */
*/*%RTN1:   R RETURN TO CALLER (ILRIOFRR)                            */
*/*ILRCSLSQ: END                                                     */
*                                                                  0304
*ILRCSLSQ:                                                         0304
*   ENTRY;                          /* SWAP LSQA PROCESSOR RECOVERY  */
@EP00304 DS    0H                                                  0305
*   RFY                                                            0305
*     R14 RSTD;                                                    0305
*   SAVER14=R14;                    /* SAVE RETURN ADDRESS           */
         ST    R14,SAVER14                                         0306
*   RFY                                                            0307
*     R14 UNRSTD;                                                  0307
*   RSMHDPTR=ASM5RG2;               /* RSMHD ADDR STORED BY ILRSLSQA */
         L     RSMHDPTR,ASM5RG2(,ASMVTPTR)                         0308
*   STM(SDWAPTR,ATAPTR,GNSAVE);     /* DESTROYED BY VERIFY ROUTINES  */
         STM   SDWAPTR,ATAPTR,GNSAVE                               0309
*   SAREG=ADDR(ASMSAVE);            /* REGISTER SAVE AREA            */
*                                                                  0310
         LA    SAREG,ASMSAVE(,ASMVTPTR)                            0310
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK FOR ONE OF THE 2 POSSIBLE 'C0D' ABENDS ISSUED BY THE    */
*   /* ASSIGNSET SUBROUTINE OF ILRSLSQA.                             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0311
*   IF ATASGNST=YES&SDWACMPC=C0DABND THEN                          0311
         TM    ATASGNST(ATAPTR),B'10000000'                        0311
         BNO   @RF00311                                            0311
         CLC   SDWACMPC(3,SDWAPTR),@CB02428                        0311
         BNE   @RF00311                                            0311
*     CALL ASETRCVY;                /* PROCESS ASSIGN SET C0D ABEND  */
*                                                                  0312
         BAL   @14,ASETRCVY                                        0312
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK FOR ILRSLSQA'S OTHER SUBROUTINE, SCCWPROC, IN CONTROL AT*/
*   /* THE TIME OF ERROR.                                            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0313
*   ELSE                                                           0313
*     IF ATASCCWP=YES THEN          /* SCCW PROC SUBROUTINE ?        */
         B     @RC00311                                            0313
@RF00311 TM    ATASCCWP(ATAPTR),B'01000000'                        0313
         BNO   @RF00313                                            0313
*       CALL SCCWRCVY;              /* YES- PROCESS SCCWPROC ERROR   */
         BAL   @14,SCCWRCVY                                        0314
*     ELSE                                                         0315
*       DO;                         /* TREAT AS ILRSLSQA ERROR       */
         B     @RC00313                                            0315
@RF00313 DS    0H                                                  0316
*         IF RSMHDPTR^=ZERO&        /* REDRIVE PATH ?                */
*             ASHSWAPQ^=ZERO THEN   /* OR SWAP QUEUE EMPTY ?         */
         SLR   @12,@12                                             0316
         CR    RSMHDPTR,@12                                        0316
         BE    @RF00316                                            0316
         C     @12,ASHSWAPQ(,RSMHDPTR)                             0316
         BE    @RF00316                                            0316
*           DO;                     /* NO- PROCESS SWAP QUEUE        */
*                                                                  0317
*             /*******************************************************/
*             /*                                                     */
*             /* IF THE SWAP REQUEST IS CURRENTLY ON THE ASMVT       */
*             /* STAGING QUEUE, IT MAY BE SENT TO ILRQIOE BY RETRYING*/
*             /* INTO ILRSLSQA.                                      */
*             /*                                                     */
*             /*******************************************************/
*                                                                  0318
*             AIAPTR=ASMSTAGF;      /* GET THE STAGE QUEUE ANCHOR    */
         L     AIAPTR,ASMSTAGF(,ASMVTPTR)                          0318
*             DO WHILE AIAPTR^=ZERO&AIAPTR^=ASHSWAPQ;/* SEARCH QUEUE */
         B     @DE00319                                            0319
@DL00319 DS    0H                                                  0320
*               AIAPTR=AIANXAIA;    /* GET NEXT AIA                  */
         L     AIAPTR,AIANXAIA(,AIAPTR)                            0320
*             END;                  /* END OF STAGE QUEUE SEARCH     */
@DE00319 LTR   AIAPTR,AIAPTR                                       0321
         BZ    @DC00319                                            0321
         C     AIAPTR,ASHSWAPQ(,RSMHDPTR)                          0321
         BNE   @DL00319                                            0321
@DC00319 DS    0H                                                  0322
*             IF AIAPTR^=ZERO THEN  /* SWAP REQUEST ON STAGE QUEUE ? */
         SLR   @12,@12                                             0322
         CR    AIAPTR,@12                                          0322
         BE    @RF00322                                            0322
*               ASHSWAPQ=ZERO;      /* CLEAR SWAP QUEUE ANCHOR       */
         ST    @12,ASHSWAPQ(,RSMHDPTR)                             0323
*             ELSE                                                 0324
*               DO;                                                0324
*                                                                  0324
         B     @RC00322                                            0324
@RF00322 DS    0H                                                  0325
*                 /***************************************************/
*                 /*                                                 */
*                 /* THE SWAP REQUEST IS NOT READY TO BE SENT TO SWAP*/
*                 /* DATA SETS. THE SWAP MEMORY IS TERMINATED.       */
*                 /* ILRTERMR WILL RECOVER ASM RESOURCES.            */
*                 /*                                                 */
*                 /***************************************************/
*                                                                  0325
*                 IF ASHSWPOT=OFF THEN/* SWAPIN REQUEST ?            */
         TM    ASHSWPOT(RSMHDPTR),B'10000000'                      0325
         BNZ   @RF00325                                            0325
*                   RSMFAIL=YES;    /* NOTIFY RSM OF SWAP IN FAILURE */
         OI    RSMFAIL(RSMHDPTR),B'00000010'                       0326
*                 RFY                                              0327
*                   TRMASID RSTD;                                  0327
@RF00325 DS    0H                                                  0328
*                 TRMASID=ASCBASID; /* GET ASID OF ERROR MEMORY      */
         L     @12,ASCBPTR(,RSMHDPTR)                              0328
         SLR   TRMASID,TRMASID                                     0328
         ICM   TRMASID,3,ASCBASID(@12)                             0328
*                 GENERATE REFS(CVTBTERM)                          0329
*(CALLRTM TYPE=MEMTERM,COMPCOD=X'028',ASID=(TRMASID));             0329
         CALLRTM TYPE=MEMTERM,COMPCOD=X'028',ASID=(TRMASID)
*                 LM(SDWAPTR,RSMHDPTR,GNSAVE);/* RESTORE REGISTERS   */
         LM    SDWAPTR,RSMHDPTR,GNSAVE                             0330
*                 RFY                                              0331
*                   TRMASID UNRSTD;                                0331
*               END;                /* TERMINATION PROCESSING        */
*           END;                    /* NON-ZERO SWAP QUEUE           */
*         ELSE                                                     0334
*           DO;                     /* ASMHD SWAP QUEUE WAS EMPTY    */
*                                                                  0334
         B     @RC00316                                            0334
@RF00316 DS    0H                                                  0335
*             /*******************************************************/
*             /*                                                     */
*             /* PROCESS THE CURRENT AIA, ATTEMPTING TO RETURN THE   */
*             /* CURRENT AIA TO THE SART WAIT QUEUE FOR REPROCESSING.*/
*             /*                                                     */
*             /*******************************************************/
*                                                                  0335
*             RFY                                                  0335
*              (VERFREG,                                           0335
*               R5,                                                0335
*               R6,                                                0335
*               R7) RSTD;           /* VERIFY RTN REGISTERS          */
*             CALL ILRVSWTQ;        /* VERIFY SART WAIT QUEUE        */
         L     @15,@CV02466                                        0336
         BALR  @14,@15                                             0336
*             VERFREG=ADDR(ATAAIA); /* ADDRESS OF AIA QUEUE TO VERIFY*/
         LA    VERFREG,ATAAIA(,ATAPTR)                             0337
*             CALL ILRVSPAQ;        /* VERIFY CURRENT SWAP AIA QUEUE */
         L     @15,@CV02465                                        0338
         BALR  @14,@15                                             0338
*             RFY                                                  0339
*              (VERFREG,                                           0339
*               R5,                                                0339
*               R6,                                                0339
*               R7) UNRSTD;         /* VERIFY RTN REGISTERS          */
*             IF ATAAIA^=ZERO THEN  /* WAS QUEUE ZEROED ?            */
         ICM   @12,15,ATAAIA(ATAPTR)                               0340
         BZ    @RF00340                                            0340
*               DO;                 /* NO- PROCESS ATA AIA QUEUE     */
*                 RFY                                              0342
*                   SARTPTR RSTD;                                  0342
*                 SARTPTR=ASMSART;  /* ADDRESSIBILITY TO SART        */
         L     SARTPTR,ASMSART(,ASMVTPTR)                          0343
*                 AIAPTR=SARWAITF;  /* GET FIRST AIA ON WAIT QUEUE   */
         L     AIAPTR,SARWAITF(,SARTPTR)                           0344
*                 DO WHILE AIAPTR^=ZERO&AIAPTR^=ATAAIA;/* SEARCH   0345
*                                      QUEUE                         */
         B     @DE00345                                            0345
@DL00345 DS    0H                                                  0346
*                   AIAPTR=AIANXAIA;/* UPDATE AIA POINTER            */
         L     AIAPTR,AIANXAIA(,AIAPTR)                            0346
*                 END;              /* END OF SEARCH                 */
@DE00345 LTR   AIAPTR,AIAPTR                                       0347
         BZ    @DC00345                                            0347
         C     AIAPTR,ATAAIA(,ATAPTR)                              0347
         BNE   @DL00345                                            0347
@DC00345 DS    0H                                                  0348
*                 IF AIAPTR=ZERO THEN/* ATA AIA FOUND ON WAIT QUEUE ?*/
         LTR   AIAPTR,AIAPTR                                       0348
         BNZ   @RF00348                                            0348
*                   DO;             /* NO- ADD ATA Q TO SART WAIT Q  */
*                     AIAPTR=ATAAIA;/* GET FIRST AIA                 */
         L     AIAPTR,ATAAIA(,ATAPTR)                              0350
*                     DO WHILE AIANXAIA^=ZERO;/* FIND LAST AIA       */
         B     @DE00351                                            0351
@DL00351 DS    0H                                                  0352
*                       AIAPTR=AIANXAIA;/* UPDATE AIA POINTER        */
         L     AIAPTR,AIANXAIA(,AIAPTR)                            0352
*                     END;                                         0353
@DE00351 SLR   @12,@12                                             0353
         C     @12,AIANXAIA(,AIAPTR)                               0353
         BNE   @DL00351                                            0353
*                     IF SARWAITF=ZERO THEN/* SART WAIT QUEUE EMPTY ?*/
         C     @12,SARWAITF(,SARTPTR)                              0354
         BNE   @RF00354                                            0354
*                       SARWAITF=ATAAIA;/* YES- SET FOWARD PTR       */
         MVC   SARWAITF(4,SARTPTR),ATAAIA(ATAPTR)                  0355
*                     ELSE                                         0356
*                       SARWAITL->AIANXAIA=ATAAIA;/* NO- ADD TO END
*                                      OF QUEUE                      */
         B     @RC00354                                            0356
@RF00354 L     @12,SARWAITL(,SARTPTR)                              0356
         MVC   AIANXAIA(4,@12),ATAAIA(ATAPTR)                      0356
*                     SARWAITL=AIAPTR;/* UPDATE LAST AIA POINTER     */
@RC00354 ST    AIAPTR,SARWAITL(,SARTPTR)                           0357
*                   END;                                           0358
*                 RFY                                              0359
*                   SARTPTR UNRSTD;                                0359
@RF00348 DS    0H                                                  0360
*               END;                /* NON-ZERO ATA AIA QUEUE        */
*           END;                    /* EMPTY ASMHD QUEUE PROCESSING  */
@RF00340 DS    0H                                                  0362
*       END;                        /* MAINLINE ILRSLSQA ERROR       */
@RC00316 DS    0H                                                  0363
*                                                                  0363
*   /*****************************************************************/
*   /*                                                               */
*   /* A NON-ZERO SDWARTYA FIELD REQUESTS THAT ILRIOFRR ATTEMPT A    */
*   /* RETRY. EVEN IF THIS RETRY IS IGNORED BY ILRIOFRR THE SWAP     */
*   /* REQUEST AND ASM RESOURCES HAVE BEEN RETURNED TO SWAP QUEUES   */
*   /* FOR REPROCESSING. THE RETRY REGISTERS ARE NOT REINITIALIZED   */
*   /* FOR A RECORD TYPE ABEND SINCE THE SALLOC LOCK INSURES THE     */
*   /* VALIDITY OF THE REGISTERS AT ERROR(SDWASRSV).                 */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0363
*   IF SDWACMPC=EDBERRSDWACMPC=C0DABND THEN/* RECORD ABEND ?        */
@RC00313 DS    0H                                                  0363
@RC00311 CLC   SDWACMPC(3,SDWAPTR),@CB02424                        0363
         BE    @RT00363                                            0363
         CLC   SDWACMPC(3,SDWAPTR),@CB02428                        0363
         BNE   @RF00363                                            0363
@RT00363 DS    0H                                                  0364
*     SDWARTYA=SDWANXT1;            /* RETRY AT NEXT SEQUENTIAL INST.*/
         MVC   SDWARTYA(4,SDWAPTR),SDWANXT1(SDWAPTR)               0364
*   ELSE                                                           0365
*     IF ATARCRF6=OFF THEN          /* RECURSIVE ERROR               */
         B     @RC00363                                            0365
@RF00363 TM    ATARCRF6(ATAPTR),B'00000100'                        0365
         BNZ   @RF00365                                            0365
*       DO;                         /* NO- SETUP RETRY               */
*         ATARCRF6=ON;              /* SET RECURSION FLAG            */
         OI    ATARCRF6(ATAPTR),B'00000100'                        0367
*         ATASGNST=OFF;             /* REMOVE SUBROUTINE FLAGS       */
*         ATASCCWP=OFF;                                            0369
         NI    ATASGNST(ATAPTR),B'00111111'                        0369
*         SDWARTYA=ADDR(ILRCRSP2);  /* SWAP LSQA RETRY ADDRESS       */
         L     @12,@CV02458                                        0370
         ST    @12,SDWARTYA(,SDWAPTR)                              0370
*         RFY                                                      0371
*          (R0,                                                    0371
*           SARTPTR,                                               0371
*           SWAPBASE,                                              0371
*           R15) RSTD;                                             0371
*         SARTPTR=ASMSART;          /* REQUIRED AT RETRY POINT       */
         L     SARTPTR,ASMSART(,ASMVTPTR)                          0372
*         SWAPBASE=ADDR(ILRCBSP2);  /* LABEL ON SWAP LSQA BASE REG   */
         L     SWAPBASE,@CV02460                                   0373
*         STM(R0,R15,SDWASRSV);     /* RETRY REGS: R2- RSMHD,      0374
*                                      R3-ASMVT, R5- SART, AND R12-
*                                      BASE REG                      */
         STM   R0,R15,SDWASRSV(SDWAPTR)                            0374
*         RFY                                                      0375
*          (R0,                                                    0375
*           SARTPTR,                                               0375
*           SWAPBASE,                                              0375
*           R15) UNRSTD;                                           0375
*       END;                                                       0376
*     ELSE                                                         0377
*       ;                           /* RECURSION= CWT                */
@RF00365 DS    0H                                                  0378
*   ATACLEAR=''B;                   /* ZERO ATA FOR RETRY PATHS      */
@RC00363 XC    ATACLEAR(16,ATAPTR),ATACLEAR(ATAPTR)                0378
*   RFY                                                            0379
*     R14 RSTD;                                                    0379
*   R14=SAVER14;                    /* RESTORE RETURN ADDRESS        */
         L     R14,SAVER14                                         0380
*   RFY                                                            0381
*     R14 UNRSTD;                                                  0381
