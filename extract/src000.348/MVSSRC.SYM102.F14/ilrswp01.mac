         OI    SRENUSE(SAREPTR),B'10000000'                        0236
*       VERFREG=ADDR(SRESCCW);      /* ADDRESS OF QUEUE TO VERIFY    */
         LA    VERFREG,SRESCCW(,SAREPTR)                           0237
*       CALL ILRVSCWQ;              /* VERIFY SARTE SCCW QUEUE       */
         L     @15,@CV02464                                        0238
         BALR  @14,@15                                             0238
*       RFY                                                        0239
*        (VERFREG,                                                 0239
*         R5,                                                      0239
*         R6,                                                      0239
*         R7) UNRSTD;               /* VERIFY RTN REGISTERS          */
*       SARTPTR=ASMSART;            /* ADDRESSIBILITY TO SART        */
         L     SARTPTR,ASMSART(,ASMVTPTR)                          0240
*       IF SRESCCW^=ZERO THEN       /* SCCW QUEUE TO PROCESS ?       */
         L     @14,SRESCCW(,SAREPTR)                               0241
         LTR   @14,@14                                             0241
         BZ    @RF00241                                            0241
*         DO;                       /* YES- PROCESS SCCW QUEUE       */
*           RFY                                                    0243
*             SCCWPTR RSTD;                                        0243
*           SCCWPTR=SRESCCW;        /* GET FIRST SCCW                */
         LR    SCCWPTR,@14                                         0244
*           DO WHILE SCCWSCCW^=ZERO;/* FIND LAST SCCW FOR C&S        */
         B     @DE00245                                            0245
@DL00245 DS    0H                                                  0246
*             SCCWPTR=SCCWSCCW;     /* NEXT SCCW                     */
         L     SCCWPTR,SCCWSCCW(,SCCWPTR)                          0246
