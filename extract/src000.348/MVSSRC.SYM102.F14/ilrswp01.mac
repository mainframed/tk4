         TITLE 'ILRSWP01:  ASM SWAP RECOVERY                           *
                        '
ILRSWP01 CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL2(@EP00001-@MAINENT)
         DC    AL1(16)                                             0001
         DC    C'ILRSWP01  76.091'                                 0001
ILRCSWAP DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL2(@EP00277-ILRCSWAP)
         ENTRY ILRCSWAP
ILRCSLSQ DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL2(@EP00304-ILRCSLSQ)
         ENTRY ILRCSLSQ
         DROP  @15
@PROLOG  BALR  @10,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@10                                         0001
         AH    @15,4(,@15)                                         0001
         BR    @15                                                 0001
@EP00001 DS    0H                                                  0002
         USING @DATD,SWPDREG
*   SAVER14=R14;                    /* SAVE RETURN ADDRESS           */
         ST    R14,SAVER14                                         0102
*   RFY                                                            0103
*     R14 UNRSTD;                                                  0103
*   SAREG=WSACASMS;                 /* SWAP DRIVER REG SAVEAREA      */
         L     @11,LCCAPTR                                         0104
         L     @11,LCCACPUS(,@11)                                  0104
         L     SAREG,WSACASMS(,@11)                                0104
*   RFY                                                            0105
*     SAREPTR RSTD;                                                0105
*   SAREPTR=ATASARTE;               /* ADDRESSIBILITY TO SART ENTRY  */
         L     SAREPTR,ATASARTE(,ATAPTR)                           0106
*   STM(SDWAPTR,ATAPTR,GNSAVE);     /* DESTROYED BY G/M & VERIFY RTNS*/
*                                                                  0107
         STM   SDWAPTR,ATAPTR,GNSAVE                               0107
*   /*****************************************************************/
*   /*                                                               */
*   /* A SART ENTRY IS CHECKPOINTED IN THE ATA WHENEVER SWAP DRIVER  */
*   /* IS PROCESSING A SARTE WITH ASSOCIATED SCCWS AND IORB. IF A    */
*   /* SARTE IS NOT CHECKPOINTED, RECOVERY CONSISTS OF RESCHEDULING  */
*   /* THE SWAP DRIVER.                                              */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0108
*   IF SAREPTR^=ZERO THEN           /* SART ENTRY CHECKPOINTED ?     */
         LTR   SAREPTR,SAREPTR                                     0108
         BZ    @RF00108                                            0108
*     DO;                           /* YES - PROCESS SARTE           */
*                                                                  0109
*       /*************************************************************/
*       /*                                                           */
*       /* IF SWAP DRIVER FOUND A SARTE WITH A ZERO IORB POINTER, A  */
*       /* '084' ABEND IS ISSUED. AN ATTEMPT IS MADE TO REBUILD AN   */
*       /* IORB.                                                     */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0110
*       IF SDWACMPC=NOIORB THEN     /* SWAP DRIVER '084' ABEND ?     */
         CLC   SDWACMPC(3,SDWAPTR),@CB02426                        0110
         BNE   @RF00110                                            0110
*         CALL RBDIORB;             /* ATTEMPT TO REBUILD IORB       */
         BAL   @14,RBDIORB                                         0111
*       ELSE                                                       0112
*         DO;                       /* NO- NORMAL ERROR PATH         */
*                                                                  0112
         B     @RC00110                                            0112
@RF00110 DS    0H                                                  0113
*           /*********************************************************/
*           /*                                                       */
*           /* PROCESS A CHECKPOINTED IORB                           */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0113
*           RFY                                                    0113
*            (VERFREG,                                             0113
*             R5,                                                  0113
*             R6,                                                  0113
*             R7,                                                  0113
*             RTCODE) RSTD;                                        0113
*           IORBPTR=ATAIORB;        /* ADDRESSIBILITY TO IORB        */
         L     IORBPTR,ATAIORB(,ATAPTR)                            0114
*           VERFREG=IORBPTR;        /* ADDR OF IORB TO VERIFY        */
         LR    VERFREG,IORBPTR                                     0115
*           CALL ILRVIORB;          /* VERIFY IORB                   */
         L     @15,@CV02462                                        0116
         BALR  @14,@15                                             0116
*           LM(SDWAPTR,ATAPTR,GNSAVE);/* DESTROYED ACROSS CALL       */
         LM    SDWAPTR,ATAPTR,GNSAVE                               0117
*           IF RTCODE=SUCCESS THEN  /* IORB VALID ?                  */
         LTR   RTCODE,RTCODE                                       0118
         BNZ   @RF00118                                            0118
*             DO;                   /* YES- PROCESS IORB             */
*                                                                  0119
*               /*****************************************************/
*               /*                                                   */
*               /* THE QUEUE OF SCCWS ANCHORED IN THE IORB IS        */
*               /* VALIDITY CHECKED. THOSE SCCWS, WHICH ARE NOT      */
*               /* ALREADY PART OF THE ATA SCCW QUEUE, ARE ADDED TO  */
*               /* THE ATA SCCW QUEUE.                               */
*               /*                                                   */
*               /*****************************************************/
*                                                                  0120
*               VERFREG=ADDR(IORSCCW);/* ADDRESS OF QUEUE TO VERIFY  */
         LA    VERFREG,IORSCCW(,IORBPTR)                           0120
*               CALL ILRVSCWQ;      /* VERIFY IORB SCCW QUEUE        */
         L     @15,@CV02464                                        0121
         BALR  @14,@15                                             0121
*               IF IORSCCW^=ZERO&IORSCCW^=ATASCCW THEN/* UNIQUE SCCW
*                                      QUEUE?                        */
         L     @14,IORSCCW(,IORBPTR)                               0122
         LTR   @14,@14                                             0122
         BZ    @RF00122                                            0122
         C     @14,ATASCCW(,ATAPTR)                                0122
         BE    @RF00122                                            0122
*                 DO;               /* YES- FIND LAST UNIQUE SCCW    */
*                   RFY                                            0124
*                     SCCWPTR RSTD;                                0124
*                   SCCWPTR=IORSCCW;/* GET QUEUE ANCHOR              */
         LR    SCCWPTR,@14                                         0125
*                   DO WHILE(SCCWSCCW^=ATASCCW)&(SCCWSCCW^=ZERO);  0126
         B     @DE00126                                            0126
@DL00126 DS    0H                                                  0127
*                     SCCWPTR=SCCWSCCW;/* GET NEXT SCCW              */
         L     SCCWPTR,SCCWSCCW(,SCCWPTR)                          0127
*                   END;            /* SEARCH FOR LAST UNIQUE SCCW   */
@DE00126 L     @14,SCCWSCCW(,SCCWPTR)                              0128
         C     @14,ATASCCW(,ATAPTR)                                0128
         BE    @DC00126                                            0128
         LTR   @14,@14                                             0128
         BNZ   @DL00126                                            0128
@DC00126 DS    0H                                                  0129
*                   SCCWSCCW=ATASCCW;/* COMBINE ATA AND IORB QUEUES  */
         MVC   SCCWSCCW(4,SCCWPTR),ATASCCW(ATAPTR)                 0129
*                   ATASCCW=IORSCCW;/* UPDATE ATA SCCW QUEUE ANCHOR  */
         MVC   ATASCCW(4,ATAPTR),IORSCCW(IORBPTR)                  0130
*                   RFY                                            0131
*                     SCCWPTR UNRSTD;                              0131
*                 END;              /* UNIQUE IORB SCCW QUEUE        */
*               IORSCCW=ZERO;       /* CLEAR IORB SCCW ANCHOR        */
@RF00122 SLR   @14,@14                                             0133
         ST    @14,IORSCCW(,IORBPTR)                               0133
*               IORFUSE=OFF;        /* RELEASE IORB                  */
         NI    IORFUSE(IORBPTR),B'01111111'                        0134
*             END;                  /* VALID IORB PROCESSING         */
*           ELSE                                                   0136
*             DO;                   /* IORB IS INVALID               */
*                                                                  0136
         B     @RC00118                                            0136
@RF00118 DS    0H                                                  0137
*               /*****************************************************/
*               /*                                                   */
*               /* THE CHECKPOINTED IORB IS INVALID. IF IT IS THE    */
*               /* LAST IORB ON THIS SART ENTRY, ATTEMPT TO REBUILD  */
*               /* AN IORB. OTHERWISE, REMOVE THE INVALID IORB FROM  */
*               /* THE SARTE IORB QUEUE.                             */
*               /*                                                   */
*               /*****************************************************/
*                                                                  0137
*               VERFREG=SREIORB;    /* ADDR OF ANCHOR IORB           */
         L     VERFREG,SREIORB(,SAREPTR)                           0137
*               CALL ILRVIORB;      /* VERIFY IORB                   */
         L     @15,@CV02462                                        0138
         BALR  @14,@15                                             0138
*               LM(SDWAPTR,ATAPTR,GNSAVE);/* DESTROYED ACROSS CALL   */
         LM    SDWAPTR,ATAPTR,GNSAVE                               0139
*               IF RTCODE^=SUCCESS THEN/* IORB ANCHOR INVALID ?      */
         LTR   RTCODE,RTCODE                                       0140
         BZ    @RF00140                                            0140
*                 CALL RBDIORB;     /* YES - ATTEMPT TO REBUILD IORB */
         BAL   @14,RBDIORB                                         0141
*               ELSE                                               0142
*                 DO;               /* LOCATE INVALID IORB ON QUEUE  */
         B     @RC00140                                            0142
@RF00140 DS    0H                                                  0143
*                   IORBPTR=SREIORB;/* GET SARTE IORB QUEUE ANCHOR   */
         L     IORBPTR,SREIORB(,SAREPTR)                           0143
*                   DO WHILE IORIORB^=ZERO;/* VERIFY REST OF IORB Q  */
         B     @DE00144                                            0144
@DL00144 DS    0H                                                  0145
*                     VERFREG=IORIORB;/* VERIFY INPUT REGISTER       */
         L     VERFREG,IORIORB(,IORBPTR)                           0145
*                     CALL ILRVIORB;/* VERIFY THIS IORB              */
         L     @15,@CV02462                                        0146
         BALR  @14,@15                                             0146
*                     LM(SDWAPTR,ATAPTR,GNSAVE);/* DESTROYED BY CALL */
         LM    SDWAPTR,ATAPTR,GNSAVE                               0147
*                     IF RTCODE=SUCCESS THEN/* IORB VALID ?          */
         LTR   RTCODE,RTCODE                                       0148
         BNZ   @RF00148                                            0148
*                       IORBPTR=IORIORB;/* YES- GET NEXT IORB        */
         L     IORBPTR,IORIORB(,IORBPTR)                           0149
*                     ELSE                                         0150
*                       IORIORB=ZERO;/* NO- TRUNCATE IORB Q          */
         B     @RC00148                                            0150
@RF00148 SLR   @14,@14                                             0150
         ST    @14,IORIORB(,IORBPTR)                               0150
*                   END;            /* IORB QUEUE LOOP               */
@RC00148 DS    0H                                                  0151
@DE00144 ICM   @14,15,IORIORB(IORBPTR)                             0151
         BNZ   @DL00144                                            0151
*                 END;              /* IORB NOT SREIORB              */
*             END;                  /* END OF INVALID IORB PROCESSING*/
@RC00140 DS    0H                                                  0154
*           RFY                                                    0154
*            (VERFREG,                                             0154
*             R5,                                                  0154
*             R6,                                                  0154
*             R7,                                                  0154
*             RTCODE) UNRSTD;                                      0154
@RC00118 DS    0H                                                  0155
*         END;                      /* END OF NON 084 PROCESSING     */
*     END;                          /* END OF SARTE PROCESSING       */
@RC00110 DS    0H                                                  0157
*                                                                  0157
*   /*****************************************************************/
*   /*                                                               */
*   /* THE SARTE CHECKPOINTED IN THE ATA MAY HAVE BEEN ZEROED BY     */
*   /* RBDIORB SUBROUTINE. IF THE SARTE IS STILL USABLE, VALIDITY    */
*   /* CHECK THE ATA SCCW QUEUE.                                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0157
*   IF ATASARTE^=ZERO THEN          /* SARTE STILL CHECKPOINTED ?    */
@RF00108 SLR   @12,@12                                             0157
         C     @12,ATASARTE(,ATAPTR)                               0157
         BE    @RF00157                                            0157
*     DO;                           /* UNLOCK IT                     */
*       RFY                                                        0159
*         COMPREG RSTD;                                            0159
*       SREDRIVE=OFF;               /* RESET SARTE REDRIVE FLAG      */
         NI    SREDRIVE(SAREPTR),B'11011111'                       0160
*       COMPREG=ONE;                /* COMPARE VALUE FOR C&S         */
         LA    COMPREG,1                                           0161
*       CS(COMPREG,ZERO,SRELOCK);   /* UNLOCK SARTE ENTRY            */
         CS    COMPREG,@12,SRELOCK(SAREPTR)                        0162
*       RFY                                                        0163
*        (VERFREG,                                                 0163
*         R5,                                                      0163
*         R6,                                                      0163
*         R7) RSTD;                 /* VERIFY RTN REGISTERS          */
*       VERFREG=ADDR(ATASCCW);      /* ADDR OF QUEUE TO VERIFY       */
         LA    VERFREG,ATASCCW(,ATAPTR)                            0164
*       CALL ILRVSCWQ;              /* VERIFY ATA SCCW QUEUE         */
         L     @15,@CV02464                                        0165
         BALR  @14,@15                                             0165
*       RFY                                                        0166
*        (VERFREG,                                                 0166
*         R5,                                                      0166
*         R6,                                                      0166
*         R7) UNRSTD;               /* VERIFY RTN REGISTERS          */
*       IF ATASCCW^=ZERO THEN       /* QUEUE EMPTY ?                 */
         L     @12,ATASCCW(,ATAPTR)                                0167
         LTR   @12,@12                                             0167
         BZ    @RF00167                                            0167
*         DO;                       /* NO- FIRST SCCW IS VALID       */
*                                                                  0168
*           /*********************************************************/
*           /*                                                       */
*           /* LOOP THROUGH THE ATA SCCW QUEUE SEARCHING FOR THE LAST*/
*           /* SCCW. SINCE SWAP DRIVER MAY HAVE SET COMMAND CHAINING */
*           /* FLAGS IN EVERY SCCW LAST CCW EXCEPT IN THE LAST SCCW, */
*           /* CLEAR THE FLAG HERE.                                  */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0169
*           RFY                                                    0169
*             SCCWPTR RSTD;                                        0169
*           SCCWPTR=ATASCCW;        /* GET FIRST SCCW                */
         LR    SCCWPTR,@12                                         0170
*           DO WHILE SCCWSCCW^=ZERO;/* FIND LAST SCCW ON QUEUE       */
         B     @DE00171                                            0171
@DL00171 DS    0H                                                  0172
*             CCFLAG=NCHAIN;        /* CLEAR COMMAND CHAIN FLAG      */
         L     @12,SCCWLCCW(,SCCWPTR)                              0172
         MVI   CCFLAG(@12),X'00'                                   0172
*             SCCWPTR=SCCWSCCW;     /* GET NEXT SCCW                 */
         L     SCCWPTR,SCCWSCCW(,SCCWPTR)                          0173
*           END;                    /* END OF QUEUE SEARCH           */
@DE00171 ICM   @12,15,SCCWSCCW(SCCWPTR)                            0174
         BNZ   @DL00171                                            0174
*           RFY                                                    0175
*             REPLREG RSTD;         /* C&S REGISTER                  */
*           COMPREG=SRESCCW;        /* COMPARE VALUE- SARTE SCCW Q   */
         L     COMPREG,SRESCCW(,SAREPTR)                           0176
*           REPLREG=ATASCCW;        /* REPLACE VALUE- ATA SCCW QUEUE */
         L     REPLREG,ATASCCW(,ATAPTR)                            0177
*RTRYCS1:                                                          0178
*           SCCWSCCW=COMPREG;       /* GET SARTE SCCW QUEUE ANCHOR   */
RTRYCS1  ST    COMPREG,SCCWSCCW(,SCCWPTR)                          0178
*           CS(COMPREG,REPLREG,SRESCCW);/* UPDATE SARTE SCCW QUEUE   */
         CS    COMPREG,@07,SRESCCW(SAREPTR)                        0179
*           BC(NE,RTRYCS1);         /* TRY AGAIN                     */
         BC    7,RTRYCS1                                           0180
*           RFY                                                    0181
*            (SCCWPTR,                                             0181
*             REPLREG) UNRSTD;      /* FREE REGISTERS                */
*         END;                      /* NON-ZERO ATA SCCW QUEUE       */
*       RFY                                                        0183
*         COMPREG UNRSTD;                                          0183
@RF00167 DS    0H                                                  0184
*     END;                          /* UNLOCKING SARTE               */
*   RFY                                                            0185
*     SAREPTR UNRSTD;                                              0185
@RF00157 DS    0H                                                  0186
*                                                                  0186
*   /*****************************************************************/
*   /*                                                               */
*   /* RESCHEDULE THE SWAP DRIVER TO PROCESS SCCWS RETURNED TO THE   */
*   /* SART ENTRY WORK QUEUES BY THIS RECOVERY.                      */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0186
*   RFY                                                            0186
*     SARTPTR RSTD;                                                0186
*   SARTPTR=ASMSART;                /* ADDRESSIBILITY TO SART        */
         L     SARTPTR,ASMSART(,ASMVTPTR)                          0187
*   IF SARSRBCT=ZERO THEN           /* SCHEDULE REQUIRED ?           */
         ICM   @12,15,SARSRBCT(SARTPTR)                            0188
         BNZ   @RF00188                                            0188
*     DO;                           /* YES- ATTEMPT SCHEDULE         */
*       RFY                                                        0190
*        (COMPREG,                                                 0190
*         SRBPTR) RSTD;                                            0190
*       COMPREG=ZERO;               /* COMPARE VALUE FOR SRB COUNT   */
         SLR   COMPREG,COMPREG                                     0191
*       CS(COMPREG,ONE,SARSRBCT);   /* UPDATE SRB COUNT              */
         LA    @12,1                                               0192
         CS    COMPREG,@12,SARSRBCT(SARTPTR)                       0192
*       BC(NE,SWPDCRTN);            /* BEAT TO THE PUNCH             */
         BC    7,SWPDCRTN                                          0193
*       SRBPTR=SARSRBP;             /* SWAP DRIVER'S SRB             */
         L     SRBPTR,SARSRBP(,SARTPTR)                            0194
*       GENERATE REFS(SRBPTR,CVTPTR,CVTLSMQ,SRBFLNK) SETS(SRBFLNK) 0195
*           (SCHEDULE SRB=(SRBPTR),SCOPE=LOCAL);                   0195
         SCHEDULE SRB=(SRBPTR),SCOPE=LOCAL
*       SDWAPTR=SAVSDWA;            /* SDWAPTR DESTROYED BY SCHEDULE */
         L     SDWAPTR,SAVSDWA                                     0196
*       RFY                                                        0197
*        (COMPREG,                                                 0197
*         SRBPTR) UNRSTD;                                          0197
*     END;                                                         0198
*   RFY                                                            0199
*     SARTPTR UNRSTD;                                              0199
@RF00188 DS    0H                                                  0200
*SWPDCRTN:                                                         0200
*   RFY                                                            0200
*     R14 RSTD;                                                    0200
SWPDCRTN DS    0H                                                  0201
*   R14=SAVER14;                    /* RESTORE RETURN ADDRESS        */
         L     R14,SAVER14                                         0201
*   RFY                                                            0202
*     R14 UNRSTD;                                                  0202
*   RETURN;                                                        0203
@EL00001 DS    0H                                                  0203
@EF00001 DS    0H                                                  0203
@ER00001 BR    @14                                                 0203
*                                                                  0204
*/********************************************************************/
*/*                                                                  */
*/*  RBDIORB : SUBROUTINE TO REBUILD AN IORB                         */
*/*                                                                  */
*/*  INPUT: REGISTER INTERFACE  REG 1 - SDWA ADDRESS                 */
*/*                             REG 3 - ASMVT ADDRESS                */
*/*                             REG 4 - ATA ADDRESS                  */
*/*                             REG 8 - VERIFICATION WORKAREA ADDR   */
*/*                             REG 9 - FRR WORKAREA ADDRESS         */
*/*         THIS ROUTINE IS ENTERED WITHOUT THE SALLOC LOCK. THE     */
*/*         SARTE TO BE PROCESSED IS CHECKPOINTED IN THE ATA.        */
*/*                                                                  */
*/*  PROCESS: THE SALLOC LOCK IS OBTAINED, A GETMAIN IS ISSUED FOR   */
*/*           THE LENGTH OF AN IORB, AND THE IORB IS REINITIALIZED.  */
*/*           IF SUCCESSFUL, THE IORB IS ANCHORED IN THE SARTE. IF   */
*/*           THE GETMAIN FAILS, THE WORK QUEUE OF SCCWS IS          */
*/*           CLEANED UP, A MESSAGE IS SENT TO THE OPERATOR, AND THE */
*/*           SARTE POINTER IN THE ATA IS ZEROED TO PREVENT ILRSWP01 */
*/*           FROM UNLOCKING THE SARTE.                              */
*/*                                                                  */
*/*  OUTPUT: NO STREAM OR LIST OUTPUT                                */
*/*                                                                  */
*/********************************************************************/
*/*RBDIORB: CHART RBDIORB SUBROUTINE OF ILRSWP01                     */
*/*RBDIORB: E ENTRY TO REBUILD IORB SUBROUTINE                       */
*/*         L SETLOCK: OBTAIN SALLOC LOCK (UNC)                      */
*/*         L GETMAIN: SQA FOR LENGTH OF IORB-IOSB-SRB STRING        */
*/*         D (YES,,NO,%BDSRE) STORAGE ACQUIRED ?                    */
*/*         P INIT FIELDS CHECKED BY ILRVIORB                        */
*/*         S ILRVIORB: REINITIALIZE IORB-IOSB-SRB                   */
*/*         P (,%RBDRTN) ANCHOR IORB IN SARTE                        */
*/*%BDSRE:  P TURN ON SARTE UNUSABLE FLAG                            */
*/*         P SET UP REGISTER FOR ILRVSCWQ                           */
*/*         S ILRVSCWQ: VERIFY SARTE SCCW QUEUE                      */
*/*         D (YES,%CLRCT,NO,) SARTE SCCW Q EMPTY ?                  */
*/*         P GET 1ST SCCW (SCCWPTR= SRESCCW)                        */
*/*%SCCWLP: D (YES,%SQUE,NO,) LAST SCCW ?                            */
*/*         P (,%SCCWLP) GET NEXT SCCW (SCCWPTR= SCCWSCCW)           */
*/*%SQUE:   P GET SART SCCW QUEUE (SCCWSCCW= SARSCCWQ)               */
*/*         P C&S SAVSCCW TO SART SCCW Q ANCHOR                      */
*/*         D (YES,,NO,%SQUE) C&S SUCCESSFUL                         */
*/*%CLRCT:  P ZERO SARTE AVAILABLE SET COUNT                         */
*/*         P ZERO SART AVAILABLE SET COUNT                          */
*/*         P GET FIRST SARTE (SREPTR= SARENTS@)                     */
*/*%SRELP:  P ADD SARTE AVAILABLE SETS TO SART SET COUNT             */
*/*         D (YES,%SMSG,NO,) LAST SARTE IN USE ?                    */
*/*         P (,%SRELP) GET NEXT SARTE                               */
*/*%SMSG:   P SET UP PARAMETER LIST FOR ILRMSG00                     */
*/*         S ILRMSG00: UNUSABLE SWAP DATA SET MESSAGE               */
*/*         P CLEAR ATA SARTE POINTER                                */
*/*%RBDRTN: R RETURN TO CALLER (ILRSWP01)                            */
*/*RBDIORB: END                                                      */
*                                                                  0204
*RBDIORB:                                                          0204
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);/* REBUILD IORB ROUTINE          */
RBDIORB  DS    0H                                                  0205
*   RFY                                                            0205
*    (MPRMREG,                                                     0205
*     R14,                                                         0205
*     RTCODE) RSTD;                                                0205
*   SAVE2R14=R14;                   /* SAVE RETURN ADDRESS           */
         ST    R14,SAVE2R14                                        0206
*   RFY                                                            0207
*     R14 UNRSTD;                                                  0207
*   MPRMREG=LENGTH(STRSIZE);        /* SETUP FOR GETMAIN             */
         LA    MPRMREG,272                                         0208
*   GENERATE REFS(PSALITA,MPRMREG,CVTSPSA,WSAGGMFM,CVTGLMN);       0209
         SETLOCK OBTAIN,TYPE=SALLOC,MODE=UNCOND,REGS=SAVE,             X
               RELATED=(SWAP,IEAVTRTS)
         GETMAIN RC,LV=(MPRMREG),SP=245,BRANCH=(YES,GLOBAL)
*   IORBPTR=MPRMREG;                /* GET SQA ADDRESS               */
         LR    IORBPTR,MPRMREG                                     0210
*   RFY                                                            0211
*     MPRMREG UNRSTD;                                              0211
*   LM(SDWAPTR,ATAPTR,GNSAVE);      /* RESTORE RECOVERY REGISTERS    */
         LM    SDWAPTR,ATAPTR,GNSAVE                               0212
*   IF RTCODE=SUCCESS THEN          /* SUCCESSFUL ?                  */
         LTR   RTCODE,RTCODE                                       0213
         BNZ   @RF00213                                            0213
*     DO;                           /* YES- REBUILD IORB             */
*                                                                  0214
*       /*************************************************************/
*       /*                                                           */
*       /* IORB MUST PASS VALIDITY CHECKING BEFORE BEING INITIALIZED.*/
*       /* THE IORB FIELDS CHECKED BY ILRVIORB ARE INITIALIZED HERE. */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0215
*       RFY                                                        0215
*        (R6,                                                      0215
*         R7,                                                      0215
*         R14) RSTD;                /* REGS FOR MVCL                 */
*       R14=IORBPTR;                /* STORAGE TO BE CLEARED         */
         LR    R14,IORBPTR                                         0216
*       R6=R14;                     /* SAME STORAGE ADDRESS          */
         LR    R6,R14                                              0217
*       R7=LENGTH(STRSIZE);         /* LENGTH OF STORAGE             */
         LA    R7,272                                              0218
*       MVCL(R6,R14);               /* CLEAR ACQUIRED STORAGE        */
         MVCL  R6,R14                                              0219
*       RFY                                                        0220
*        (R14,                                                     0220
*         R6,                                                      0220
*         R7) UNRSTD;               /* FREE REGS                     */
*       IORID=IORBID;               /* CONTROL BLOCK IDENTIFIER      */
         MVI   IORID(IORBPTR),X'88'                                0221
*       IORSWAP=ON;                 /* SWAP IORB                     */
         OI    IORSWAP(IORBPTR),B'00001000'                        0222
*       IORPARTE=ATASARTE;          /* SART ENTRY                    */
         MVC   IORPARTE(4,IORBPTR),ATASARTE(ATAPTR)                0223
*       IOSBPTR=IORBPTR+SALGTH+LENGTH(IORB);                       0224
         LA    IOSBPTR,112                                         0224
