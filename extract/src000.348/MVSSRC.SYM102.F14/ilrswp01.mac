         LA    I,1                                                 0259
         B     @DE00259                                            0259
@DL00259 DS    0H                                                  0260
*         SARSETCT=SARSETCT+SREAVLSL;/* ADD THIS SARTE'S SWAP SETS   */
         L     @14,SARSETCT(,SARTPTR)                              0260
         AL    @14,SREAVLSL(,SAREPTR)                              0260
         ST    @14,SARSETCT(,SARTPTR)                              0260
*         SAREPTR=SAREPTR+LENGTH(SARTE);/* GET NEXT SARTE            */
         AH    SAREPTR,@CH00408                                    0261
*       END;                        /* LOOP THRU SARTES              */
         AH    I,@CH00043                                          0262
@DE00259 C     I,SARUSE(,SARTPTR)                                  0262
         BNH   @DL00259                                            0262
*       RFY                                                        0263
*         SARTPTR UNRSTD;                                          0263
*                                                                  0264
*       /*************************************************************/
