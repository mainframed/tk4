*       /*                                                           */
*       /*************************************************************/
*                                                                  0256
*       SREAVLSL=ZERO;              /* ZERO AVAILABLE SWAP SET COUNT */
@RF00241 SLR   @14,@14                                             0256
         ST    @14,SREAVLSL(,SAREPTR)                              0256
*       SARSETCT=ZERO;              /* CLEAR SART SWAP SET COUNT     */
         ST    @14,SARSETCT(,SARTPTR)                              0257
*       SAREPTR=ADDR(SARENTS);      /* GET FIRST SART ENTRY          */
         LA    SAREPTR,SARENTS(,SARTPTR)                           0258
