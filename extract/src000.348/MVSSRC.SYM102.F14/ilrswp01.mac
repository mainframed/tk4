         L     @15,ASMPMSG0(,ASMVTPTR)                             0267
         BALR  @14,@15                                             0267
*       SDWAPTR=SAVSDWA;            /* RESTORE SDWA POINTER (REG 1)  */
         L     SDWAPTR,SAVSDWA                                     0268
