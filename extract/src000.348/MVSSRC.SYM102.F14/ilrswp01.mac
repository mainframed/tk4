         B     @EL00001                                            0382
*                                                                  0383
*/********************************************************************/
*/*                                                                  */
*/*  SCCWRCVY: SWAP LSQA PROCESSOR SCCWPROC SUBROUTINE RECOVERY      */
*/*                                                                  */
*/*  INPUT: REGISTER INTERFACE  REG 1 - SDWA ADDRESS                 */
*/*                             REG 2 - RSMHD ADDRESS                */
*/*                             REG 3 - ASMVT ADDRESS                */
*/*                             REG 4 - ATA ADDRESS                  */
*/*                             REG 8 - FRR WORKAREA ADDRESS         */
*/*         THIS ROUTINE IS ENTERED WITH THE SALLOC LOCK HELD. THE   */
*/*         AIA AND SCCW CONNECTED WITH THE ERROR WILL BE            */
*/*         CHECKPOINTED IN THE ATA OR WILL BE ZERO.                 */
*/*                                                                  */
*/*  PROCESS: RESOURCES SUCH AS THE SCCW AND AIA ARE VALIDITY        */
*/*           CHECKED AND RETURNED TO QUEUES. THE ONLY PROCESSING    */
*/*           FOR SCCWPROC'S C0D ABEND IS TO COPY THE ERROR AIA AND  */
*/*           EDB INTO THE SDWA VARIABLE RECORDING AREA.             */
*/*                                                                  */
*/*  OUTPUT: NO STREAM OR LIST OUTPUT                                */
*/*                                                                  */
*/********************************************************************/
*/*SCCWRCVY: CHART SCCWPROC RECOVERY                                 */
*/*SCCWRCVY:E ENTRY TO SCCWPROC RECOVERY                             */
*/*         D (YES,,NO,%AIACHK) 083 ABEND ?                          */
*/*         D (YES,,NO,%RETRN2) SDWA SPACE AVAILABLE ?               */
*/*         P (,%RETRN2) COPY ERROR AIA AND EDB TO SDWA              */
*/*%AIACHK: D (YES,,NO,%RETRN2) AIA CHECKPOINTED IN ATA ?            */
*/*         P SET UP REGS FOR ILRVAIA                                */
*/*         S ILRVAIA:  VALIDITY CHECK ATA AIA                       */
*/*         D (YES,,NO,%SCWCHK) AIA VALID ?                          */
*/*         D (YES,%SCWCHK,NO,) AIA ERROR FLAG ON ?                  */
*/*         P NEXT AIA FIELD IN ATA AIA= SART WAIT QUEUE 1ST AIA     */
*/*         P SART WAIT QUEUE 1ST AIA= ATA AIA                       */
*/*         D (YES,,NO,%SCWCHK) SART WAIT QUEUE WAS EMPTY ?          */
*/*         P SART WAIT QUEUE LAST AIA= ATA AIA                      */
*/*%SCWCHK: P SET UP REGS FOR ILRVSCCW                               */
*/*         S ILRVSCCW: VALIDITY CHECK ATA SCCW                      */
*/*         D (YES,,NO,%RETRN2) SCCW VALID ?                         */
*/*%CSRTY2: P LINK FIELD IN ATA SCCW= SART SCCW QUEUE                */
*/*         P C&S ATA SCCW TO SART SCCW QUEUE                        */
*/*         D (YES,,NO,%CSRTY2) C&S SUCCESSFUL ?                     */
*/*%RETRN2: R RETURN TO CALLER (ILRCSLSQ)                            */
*/*SCCWRCVY: END                                                     */
*                                                                  0383
*SCCWRCVY:                                                         0383
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);/* SCCWPROC RECOVERY             */
SCCWRCVY DS    0H                                                  0384
*   RFY                                                            0384
*     R14 RSTD;                     /* RETURN REGISTER               */
*   SAVE2R14=R14;                   /* SAVE RETURN ADDRESS           */
         ST    R14,SAVE2R14                                        0385
*   RFY                                                            0386
*     R14 UNRSTD;                                                  0386
*   RFY                                                            0387
*     IDAEDBHD BASED(SREEDB);       /* ADDRESSIBILITY TO EDB HEADER  */
*   RFY                                                            0388
*     IDAEDB BASED(ADDR(IDAEDBHD)+LENGTH(IDAEDBHD));               0388
*   IF SDWACMPC=EDBERR THEN         /* MAINLINE ISSUED '083' ABEND ? */
         CLC   SDWACMPC(3,SDWAPTR),@CB02424                        0389
         BNE   @RF00389                                            0389
*     IF SDWAURAL+LGTH083^>SDWAVRAL THEN/* ENOUGH SDWA VARIABLE    0390
*                                      RECORDING AREA LEFT ?         */
         SLR   @12,@12                                             0390
         IC    @12,SDWAURAL(,SDWAPTR)                              0390
         LH    @11,LGTH083                                         0390
         ALR   @11,@12                                             0390
         SLR   @05,@05                                             0390
         ICM   @05,3,SDWAVRAL(SDWAPTR)                             0390
         CR    @11,@05                                             0390
         BH    @RF00390                                            0390
*       DO;                         /* YES - PROCESS EDB ERROR       */
*         RFY                                                      0392
*           SAREPTR RSTD;                                          0392
*         RECSDWA(1:LENGTH(AIA))=ATAAIA->AIA;/* COPY ERROR AIA       */
         LA    @11,SDWAVRA(,SDWAPTR)                               0393
         LR    @05,@11                                             0393
         ALR   @05,@12                                             0393
         L     @15,ATAAIA(,ATAPTR)                                 0393
         MVC   RECSDWA(28,@05),AIA(@15)                            0393
*         SDWAURAL=SDWAURAL+LENGTH(AIA);/* AMOUNT OF RECORD AREA USED*/
         AH    @12,@CH00386                                        0394
         STC   @12,SDWAURAL(,SDWAPTR)                              0394
*         SAREPTR=ATASARTE;         /* ADDRESSIBILITY TO SART ENTRY  */
         L     SAREPTR,ATASARTE(,ATAPTR)                           0395
*         RECSDWA(1:LENGTH(IDAEDB))=IDAEDB;/* COPY ERROR EDB         */
         ALR   @11,@12                                             0396
         L     @05,SREEDB(,SAREPTR)                                0396
         MVC   RECSDWA(24,@11),IDAEDB+8(@05)                       0396
*         SDWAURAL=SDWAURAL+LENGTH(IDAEDB);/* UPDATE SPACE USED      */
         AH    @12,@CH00350                                        0397
         STC   @12,SDWAURAL(,SDWAPTR)                              0397
*         RFY                                                      0398
*           SAREPTR UNRSTD;                                        0398
*       END;                                                       0399
*     ELSE                                                         0400
*       ;                           /* NOT ENOUGH RECORDING AREA     */
@RF00390 DS    0H                                                  0401
*   ELSE                                                           0401
*     DO;                           /* UNEXPECTED ERROR              */
*                                                                  0401
         B     @RC00389                                            0401
@RF00389 DS    0H                                                  0402
*       /*************************************************************/
*       /*                                                           */
*       /* CURRENT AIA PROCESSING. AN AIA WITH ERROR FLAGS IS IGNORED*/
*       /* TO PREVENT ADDING IT TO THE PART ERROR QUEUE TWICE.       */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0402
*       RFY                                                        0402
*         SARTPTR RSTD;                                            0402
*       IF ATAAIA^=ZERO THEN        /* AIA CHECKPOINTED IN ATA ?     */
         ICM   @12,15,ATAAIA(ATAPTR)                               0403
         BZ    @RF00403                                            0403
*         DO;                       /* YES- PROCESS AIA              */
*           RFY                                                    0405
*            (VERFREG,                                             0405
*             R5,                                                  0405
*             R6,                                                  0405
*             R7,                                                  0405
*             RTCODE) RSTD;                                        0405
*           CALL ILRVSWTQ;          /* VERIFY SART WAIT QUEUE        */
         L     @15,@CV02466                                        0406
         BALR  @14,@15                                             0406
*           VERFREG=ATAAIA;         /* ADDRESS OF AIA TO BE VERIFIED */
         L     VERFREG,ATAAIA(,ATAPTR)                             0407
*           CALL ILRVAIA;           /* VERIFY CHECKPOINTED AIA       */
         L     @15,@CV02461                                        0408
         BALR  @14,@15                                             0408
*           RFY                                                    0409
*            (VERFREG,                                             0409
*             R5,                                                  0409
*             R6,                                                  0409
*             R7) UNRSTD;           /* VERIFY RTN REGISTERS          */
*           LM(SDWAPTR,ATAPTR,GNSAVE);/* DESTROYED ACROSS CALL       */
         LM    SDWAPTR,ATAPTR,GNSAVE                               0410
*           IF RTCODE=SUCCESS THEN  /* IS AIA VALID ?                */
         SLR   @14,@14                                             0411
         CR    RTCODE,@14                                          0411
         BNE   @RF00411                                            0411
*             DO;                   /* YES- PROCESS AIA              */
*               AIAPTR=ATAAIA;      /* ADDRESSIBILITY TO AIA FIELDS  */
         L     AIAPTR,ATAAIA(,ATAPTR)                              0413
*               IF AIAPRIER=OFF&AIABADID=OFF THEN/* ERROR FLAG ON ?  */
         TM    AIAPRIER(AIAPTR),B'00001001'                        0414
         BNZ   @RF00414                                            0414
*                 DO;               /* NO- RETURN AIA TO WAIT QUEUE  */
*                   SARTPTR=ASMSART;/* ADDRESSIBILITY TO SART        */
         L     SARTPTR,ASMSART(,ASMVTPTR)                          0416
*                   AIANXAIA=SARWAITF;/* GET CURRENT WAIT QUEUE      */
         L     @12,SARWAITF(,SARTPTR)                              0417
         ST    @12,AIANXAIA(,AIAPTR)                               0417
*                   SARWAITF=AIAPTR;/* UPDATE SART WAIT QUEUE ANCHOR */
         ST    AIAPTR,SARWAITF(,SARTPTR)                           0418
*                   IF AIANXAIA=ZERO THEN/* WAS WAIT QUEUE EMPTY ?   */
         CR    @12,@14                                             0419
         BNE   @RF00419                                            0419
*                     SARWAITL=AIAPTR;/* YES- UPDATE LAST POINTER    */
         ST    AIAPTR,SARWAITL(,SARTPTR)                           0420
*                 END;              /* NON-ERROR FLAG PROCESSING     */
@RF00419 DS    0H                                                  0422
*             END;                  /* AIA VALID PROCESSING          */
@RF00414 DS    0H                                                  0423
*                                                                  0423
*           /*********************************************************/
*           /*                                                       */
*           /* CURRENT SCCW PROCESSING. A ZERO AIA POINTER INDICATES */
*           /* THAT THE SCCW HAS ALREADY BEEN USED BY SCCWPROC.      */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0423
*           RFY                                                    0423
*            (VERFREG,                                             0423
*             R5,                                                  0423
*             R6,                                                  0423
*             R7) RSTD;             /* VERIFY RTN REGISTERS          */
@RF00411 DS    0H                                                  0424
*           VERFREG=ATASCCW;        /* ADDRESS OF SCCW TO BE VERIFIED*/
         L     VERFREG,ATASCCW(,ATAPTR)                            0424
*           CALL ILRVSCCW;          /* VERIFY CHECKPOINTED SCCW      */
         L     @15,@CV02463                                        0425
         BALR  @14,@15                                             0425
*           LM(SDWAPTR,ATAPTR,GNSAVE);/* DESTROYED ACROSS CALL       */
         LM    SDWAPTR,ATAPTR,GNSAVE                               0426
*           RFY                                                    0427
*            (VERFREG,                                             0427
*             R5,                                                  0427
*             R6,                                                  0427
*             R7) UNRSTD;           /* VERIFY RTN REGISTERS          */
*           IF RTCODE=SUCCESS THEN  /* SCCW VAILD ?                  */
         LTR   RTCODE,RTCODE                                       0428
         BNZ   @RF00428                                            0428
*             DO;                   /* YES- RETURN TO SART SCCW Q    */
*               RFY                                                0430
*                (COMPREG,                                         0430
*                 REPLREG) RSTD;    /* C&S REGISTERS                 */
*               SARTPTR=ASMSART;    /* ADDRESSIBILITY TO SART        */
         L     SARTPTR,ASMSART(,ASMVTPTR)                          0431
*               COMPREG=SARSCCWQ;   /* COMPARE VALUE- SART SCCW QUEUE*/
         L     COMPREG,SARSCCWQ(,SARTPTR)                          0432
*               REPLREG=ATASCCW;    /* REPLACE VALUE- ATA SCCW       */
         L     REPLREG,ATASCCW(,ATAPTR)                            0433
*RTRYCS3:                                                          0434
*               ATASCCW->SCCWSCCW=COMPREG;/* GET SARSCCWQ ANCHOR     */
RTRYCS3  L     @14,ATASCCW(,ATAPTR)                                0434
         ST    COMPREG,SCCWSCCW(,@14)                              0434
*               CS(COMPREG,REPLREG,SARSCCWQ);/* UPDATE SART SCCW Q   */
         CS    COMPREG,@07,SARSCCWQ(SARTPTR)                       0435
*               BC(NE,RTRYCS3);     /* TRY AGAIN                     */
         BC    7,RTRYCS3                                           0436
*               RFY                                                0437
*                (COMPREG,                                         0437
*                 REPLREG) UNRSTD;                                 0437
*             END;                  /* VALID SCCW PROCESSING         */
*           RFY                                                    0439
*             RTCODE UNRSTD;                                       0439
@RF00428 DS    0H                                                  0440
*         END;                      /* CHECKPOINTED AIA PROCESSING   */
*       RFY                                                        0441
*         SARTPTR UNRSTD;                                          0441
@RF00403 DS    0H                                                  0442
*     END;                          /* UNEXPECTED ERROR PROCESSING   */
*   RFY                                                            0443
*     R14 RSTD;                                                    0443
@RC00389 DS    0H                                                  0444
*   R14=SAVE2R14;                   /* RESTORE RETURN ADDRESS        */
         L     R14,SAVE2R14                                        0444
*   RFY                                                            0445
*     R14 UNRSTD;                   /* FREE RETURN REGISTER          */
*   END SCCWRCVY;                   /* RETURN TO ILRCSLSQ            */
@EL00003 DS    0H                                                  0446
@EF00003 DS    0H                                                  0446
@ER00003 BR    @14                                                 0446
*                                                                  0447
*/********************************************************************/
*/*                                                                  */
*/*  ASETRCVY: SWAP LSQA PROCESSOR ASIGNSET SUBROUTINE RECOVERY      */
*/*                                                                  */
*/*  INPUT: REGISTER INTERFACE  REG 1 - SDWA ADDRESS                 */
*/*                             REG 4 - ATA ADDRESS                  */
*/*         THIS ROUTINE IS ENTERED WITH THE SALLOC LOCK HELD. THE   */
*/*         SART ENTRY CONNECTED WITH THE ERROR WILL BE CHECKPOINTED */
*/*         IN THE ATA.                                              */
*/*                                                                  */
*/*  PROCESS: SPECIAL PROCESSING FOR EITHER OF THE TWO POSSIBLE C0D  */
*/*           ABENDS FROM ASIGNSET SUBROUTINE OF ILRSLSQA.           */
*/*                                                                  */
*/*  OUTPUT: NO STREAM OR LIST OUTPUT                                */
*/*                                                                  */
*/********************************************************************/
*/*ASETRCVY: CHART ASSIGN SET SUBROUTINE  RECOVERY                   */
*/*ASETRCVY:E ENTRY TO ASSIGN SET RECOVERY                           */
*/*         P ZERO SART AVAILABLE SWAP SET COUNT                     */
*/*         D (YES,,NO,%RTN3) SARTE CHECKPOINTED ?                   */
*/*         P ZERO SARTE AVAILABLE SET COUNT                         */
*/*         P GET FIRST SARTE (SAREPTR= @SARENTS)                    */
*/*%SRECT:  P ADD SARTE AVAILABLE SETS TO SART SET COUNT             */
*/*         D (YES,%RTN3,NO,) LAST SARTE IN USE ?                    */
*/*         P (,%SRECT) GET NEXT SARTE                               */
*/*%RTN3:   R RETURN TO CALLER (ILRCSLSQ)                            */
*/*ASETRCVY: END                                                     */
*                                                                  0447
*ASETRCVY:                                                         0447
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);/* ASSIGN SET RECOVERY           */
ASETRCVY DS    0H                                                  0448
*   RFY                                                            0448
*    (SARTPTR,                                                     0448
*     R14) RSTD;                    /* RESTRICT RETURN REGISTER      */
*   SARTPTR=ASMSART;                /* ADDRESSIBILITY TO SART        */
         L     SARTPTR,ASMSART(,ASMVTPTR)                          0449
*   SARSETCT=ZERO;                  /* COUNT MUST BE RECALCULATED    */
         SLR   @15,@15                                             0450
         ST    @15,SARSETCT(,SARTPTR)                              0450
*   IF ATASARTE^=ZERO THEN          /* PROBLEM WITH SART ENTRY ?     */
         L     @12,ATASARTE(,ATAPTR)                               0451
         CR    @12,@15                                             0451
         BE    @RF00451                                            0451
*     DO;                           /* CLEAR SARTE SWAP SET COUNT    */
*       RFY                                                        0453
*         SAREPTR RSTD;                                            0453
*       ATASARTE->SREAVLSL=ZERO;    /* COUNT WAS INCORRECT           */
         ST    @15,SREAVLSL(,@12)                                  0454
*       SAREPTR=ADDR(SARENTS);      /* GET FIRST SART ENTRY          */
         LA    SAREPTR,SARENTS(,SARTPTR)                           0455
*       DO I=1 TO SARUSE;           /* LOPP THRU SART ENTRIES        */
         LA    I,1                                                 0456
         B     @DE00456                                            0456
@DL00456 DS    0H                                                  0457
*         SARSETCT=SARSETCT+SREAVLSL;/* ADD SARTE'S SETS TO TOTAL    */
         L     @15,SARSETCT(,SARTPTR)                              0457
         AL    @15,SREAVLSL(,SAREPTR)                              0457
         ST    @15,SARSETCT(,SARTPTR)                              0457
*         SAREPTR=SAREPTR+LENGTH(SARTE);/* GET NEXT SARTE            */
         AH    SAREPTR,@CH00408                                    0458
*       END;                        /* LOOP THRU SART ENTRIES        */
