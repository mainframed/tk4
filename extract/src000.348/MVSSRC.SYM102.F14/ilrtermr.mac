         TITLE 'ILRTERMR-ASM MEMORY TERMINATION ROUTINE                *
                        '
ILRTERMR CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL2(@EP00001-@MAINENT)
         DC    AL1(16)                                             0001
         DC    C'ILRTERMR  77.343'                                 0001
ILRSLTRV DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL2(@EP00464-ILRSLTRV)
         ENTRY ILRSLTRV
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @12,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@12                                         0001
         AH    @15,4(,@15)                                         0001
         BR    @15                                                 0001
@EP00001 DS    0H                                                  0002
*   R2=PARMPTR;                     /* SAVE INPUT OVER GMA           */
         LR    R2,PARMPTR                                          0130
*   ASMVTPTR=CVTASMVT;              /* GMA NEEDS ASMVT ADDRESS       */
         L     @10,CVTPTR                                          0131
         L     ASMVTPTR,CVTASMVT(,@10)                             0131
*   RFY                                                            0132
*    (R4,                                                          0132
*     R5) RSTD;                                                    0132
*                                                                  0132
*   /*****************************************************************/
*   /*                                                               */
*   /* BEGIN ILRGMA MACRO EXPANSION                                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0133
*   DO;                             /* ILRGMA GET                  0133
*                                      CELL(BWK)WORKREGS(R4,R5)      */
*     DECLARE                       /* ASMVT VECTOR TO ILRPEX        */
*       ILRPEX ENTRY BASED(ASMPEX) VALRG(0);                       0134
*     DECLARE                       /* MAP FOR CELL CONTROL WORDS    */
*       1 GMACELL BASED BDY(WORD),                                 0135
*        2 GMANEXT PTR(31);         /* PTR NEXT FREE CELL            */
*     RESPECIFY                                                    0136
*      (R0,                                                        0136
*       R1,                                                        0136
*       R4,                                                        0136
*       R5,                                                        0136
*       R14,                                                       0136
*       R15) RESTRICTED;                                           0136
*     RESPECIFY                                                    0137
*       ASMPOOLS BASED(R4);                                        0137
*     RESPECIFY                                                    0138
*       GMACELL BASED(R14);                                        0138
*     R4=ADDR(ASMBWKPC);                                           0139
         LA    R4,ASMBWKPC(,ASMVTPTR)                              0139
*     LM(R14,R15,ASMCPAVQ);         /* GET ADDR OF 1ST CELL          */
         LM    R14,R15,ASMCPAVQ(R4)                                0140
*GMA00040:                                                         0141
*     IF R14^=0 THEN                /* TEST CELL POOL                */
GMA00040 LTR   R14,R14                                             0141
         BZ    @RF00141                                            0141
*       DO;                         /* CELL AVAILABLE, REMOVE CELL   */
*         R0=GMANEXT;                                              0143
         L     R0,GMANEXT(,R14)                                    0143
*         R1=R15-1;                                                0144
         LR    R1,R15                                              0144
         BCTR  R1,0                                                0144
*         CDS(R14,R0,ASMCPAVQ);                                    0145
         CDS   R14,R0,ASMCPAVQ(R4)                                 0145
*         BC(7,GMA00040);                                          0146
         BC    7,GMA00040                                          0146
*         R1=R14;                                                  0147
         LR    R1,R14                                              0147
*       END;                                                       0148
*     ELSE                                                         0149
*       DO;                         /* CELL POOL EMPTY               */
         B     @RC00141                                            0149
@RF00141 DS    0H                                                  0150
*         R1=R4;                    /* PREPARE TO CALL PEX           */
         LR    R1,R4                                               0150
*         RFY                                                      0151
*          (R11,                                                   0151
*           R12) RSTD;                                             0151
*         R4=R11;                   /* SAVE R11                      */
         LR    R4,R11                                              0152
*         R5=R12;                   /* SAVE R12                      */
         LR    R5,R12                                              0153
*         CALL ILRPEX;              /* EXTEND THE POOL               */
         L     @15,ASMPEX(,ASMVTPTR)                               0154
         BALR  @14,@15                                             0154
*         R11=R4;                   /* RESTORE R11                   */
         LR    R11,R4                                              0155
*         R12=R5;                   /* RESTORE R12                   */
         LR    R12,R5                                              0156
*         RFY                                                      0157
*          (R11,                                                   0157
*           R12) UNRSTD;                                           0157
*       END;                                                       0158
*     RESPECIFY                                                    0159
*      (R0,                                                        0159
*       R1,                                                        0159
*       R4,                                                        0159
*       R5,                                                        0159
*       R14,                                                       0159
*       R15) UNRESTRICTED;                                         0159
@RC00141 DS    0H                                                  0160
*   END;                            /* ILRGMA GET                  0160
*                                      CELL(BWK)WORKREGS(R4,R5)      */
*                                                                  0160
*   /*****************************************************************/
*   /*                                                               */
*   /* END ILRGMA MACRO EXPANSION                                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0161
*                                   /* OBTAIN WORK AREA              */
*   IF PARMPTR^=0 THEN                                             0161
         LTR   PARMPTR,PARMPTR                                     0161
         BZ    @RF00161                                            0161
*     DO;                           /* IF WORKAREA RETURNED          */
*       WORKAPTR=PARMPTR;           /* SAVE WORK AREA ADDRESS        */
         LR    WORKAPTR,PARMPTR                                    0163
*       RFY                                                        0164
*        (PARMPTR,                                                 0164
*         R4,                                                      0164
*         R5) UNRSTD;                                              0164
*       GEN REFS(WORKAPTR)(USING @DATD,WORKAPTR);/* ADDRESS WORK AREA*/
         USING @DATD,WORKAPTR
*       WORKAR=''B;                 /* INITIALIZE WORK AREA          */
         XC    WORKAR(256,WORKAPTR),WORKAR(WORKAPTR)               0166
*       RMPLPTR=R2->RMPLADR;        /* OBTAIN RMPL ADDRESS           */
         L     @10,RMPLADR(,R2)                                    0167
         ST    @10,RMPLPTR                                         0167
*       RFY                                                        0168
*         R2 UNRSTD;                                               0168
*       RFY                                                        0169
*        (ATAPTR,                                                  0169
*         R1,                                                      0169
*         R5,                                                      0169
*         R6) RSTD;                                                0169
*       R6=ADDR(TERMRFRR);          /* SET UP FRR ADDRESS            */
         LA    R6,TERMRFRR                                         0170
*       GEN REFS(PSACSTK,FRRSCURR,FRRSLAST,PSALSFCC,FRRSELEN,FRRSPARM,
*           PSALITA,FLC) SETS(FRRSFRRA,FRRSENTR);                  0171
SETLO   SETLOCK OBTAIN,TYPE=SALLOC,MODE=UNCOND,REGS=USE,               X
               RELATED=(PAGE/SWAP,ILRTERMR(SETLR))
        SETFRR A,FRRAD=(6),WRKREGS=(5,1),PARMAD=(4)
*       ATAADDR=ATAPTR;             /* SAVE ADDRESS OF ATA           */
         LR    ATAADDR,ATAPTR                                      0172
*       ATARMPL=RMPLPTR;            /* PUT RMPL ADDR IN FRR PARMS    */
         L     @10,RMPLPTR                                         0173
         ST    @10,ATARMPL(,ATAPTR)                                0173
*       ATAWORKA=WORKAPTR;          /* PUT WORK AREA @ IN FRR PARM   */
         ST    WORKAPTR,ATAWORKA(,ATAPTR)                          0174
*       RFY                                                        0175
*         ATA BASED(ATAADDR);                                      0175
*       RFY                                                        0176
*        (ATAPTR,                                                  0176
*         R1,                                                      0176
*         R5,                                                      0176
*         R6) UNRSTD;                                              0176
*       SAVER13=SAVEPTR;            /* SAVE CALLERS REG 13           */
         ST    SAVEPTR,SAVER13                                     0177
*       SAVEPTR=ADDR(ASMSAVE);      /* PERFORM SAVE AREA LINKAGE     */
         LA    SAVEPTR,ASMSAVE(,ASMVTPTR)                          0178
*       RFY                                                        0179
*         SAVEA BASED(SAVER13);     /* POINT TO CALLERS SAVE AREA    */
*       FWDCHAIN=SAVEPTR;           /* CHAIN UP CALLERS SAVE AREA    */
         L     @06,SAVER13                                         0180
         ST    SAVEPTR,FWDCHAIN(,@06)                              0180
*       RFY                                                        0181
*         SAVEA BASED(SAVEPTR);     /* POINT TO MY SAVE AREA         */
*       BCKCHAIN=SAVER13;           /* CHAIN MY SAVE AREA            */
         ST    @06,BCKCHAIN(,SAVEPTR)                              0182
*       RSMHDPTR=ASCBRSM;           /* SET UP RSMHD ADDRESS          */
         L     @10,ASCBPTR(,@10)                                   0183
         L     RSMHDPTR,ASCBRSM(,@10)                              0183
*       IF RSMHDPTR^=0 THEN                                        0184
         LTR   RSMHDPTR,RSMHDPTR                                   0184
         BZ    @RF00184                                            0184
*         DO;                       /* IF RSMHD ADDR IS 0 RETURN     */
*           IF ASCBMCC=MEMCREAT THEN                               0186
         CLC   ASCBMCC(4,@10),@CF02584                             0186
         BNE   @RF00186                                            0186
*             WKLSQA=ON;            /* MEMORY CREATE ERROR           */
         OI    WKLSQA,B'01000000'                                  0187
*           ELSE                                                   0188
*             DO;                   /* NOT MEMORY CREATE ERROR       */
         B     @RC00186                                            0188
@RF00186 DS    0H                                                  0189
*               WKRETRY=TRSOUT;     /* SET RETRY INDEX TO ONE        */
         MVI   WKRETRY,X'01'                                       0189
*               IF RMPLTYPE=OFF THEN                               0190
         L     @10,RMPLPTR                                         0190
         TM    RMPLTYPE(@10),B'10000000'                           0190
         BNZ   @RF00190                                            0190
*                 WKLSQA=ON;        /* IF NORMAL MEM TERM LSQA IS  0191
*                                      AVAILABLE                     */
         OI    WKLSQA,B'01000000'                                  0191
*               ELSE                                               0192
*                 DO;               /* NOT NORMAL MEM TERM           */
         B     @RC00190                                            0192
@RF00190 DS    0H                                                  0193
*                   IF RSMFAIL=OFF THEN                            0193
         TM    RSMFAIL(RSMHDPTR),B'00000010'                       0193
         BNZ   @RF00193                                            0193
*                     DO;           /* IF NOT LSQA SWAP IN ERROR     */
*                       IF RSMLSQAF=0 THEN                         0195
         LH    @10,RSMLSQAF(,RSMHDPTR)                             0195
         N     @10,@CF02800                                        0195
         LTR   @10,@10                                             0195
         BNZ   @RF00195                                            0195
*                         DO;       /* IF NO LSQA FRAMES             */
*                           ATARCODE=16;/* SET CODE 16 IN ATA        */
         MVI   ATARCODE(ATAADDR),X'10'                             0197
*                           GEN(ABEND X'C0D',,,SYSTEM);/* ISSUE C0D
*                                      ABEND                         */
         ABEND X'C0D',,,SYSTEM
*                         END;      /* END NO LSQA FRAMES            */
*                       ELSE                                       0200
*                         DO;       /* LSQA FRAMES AVAILABLE         */
         B     @RC00195                                            0200
@RF00195 DS    0H                                                  0201
*                           IF SPCTPTR^=0 THEN                     0201
         L     @10,SPCTPTR(,RSMHDPTR)                              0201
         LTR   @10,@10                                             0201
         BZ    @RF00201                                            0201
*                             DO;   /* SPCT ADDR NON ZERO            */
*                               IF SPCTSWIN=ON THEN                0203
         TM    SPCTSWIN(@10),B'10000000'                           0203
         BNO   @RF00203                                            0203
*                                 DO;/* IF SWAP IN IN PROGRESS       */
*                                   ATARCODE=16;/* SET CODE OF 16 IN
*                                      ATA                           */
         MVI   ATARCODE(ATAADDR),X'10'                             0205
*                                   GEN(ABEND X'C0D',,,SYSTEM);/*  0206
*                                      ISSUE ABEND                   */
         ABEND X'C0D',,,SYSTEM
*                                 END;/* END SWAP IN IN PROGRESS     */
*                               ELSE                               0208
*                                 DO;/* SWAP IN NOT IN PROGRESS      */
         B     @RC00203                                            0208
@RF00203 DS    0H                                                  0209
*                                   IF ASHSWPOT=ON THEN/* IF SWAP OUT
*                                      IN PROGRESS                   */
         TM    ASHSWPOT(RSMHDPTR),B'10000000'                      0209
         BNO   @RF00209                                            0209
*                                    CALL TERMSOUT;/* CALL SWAP OUT
*                                      PROCESSING                    */
         BAL   @14,TERMSOUT                                        0210
*                                   IF FIRSTPT=SEGINDX&/* VALIDATE 0211
*                                      SEGMENT ORIGIN                */
*                                       SECPART='000000'B THEN/*   0211
*                                      ADDRESS (ASCBSTOR)            */
@RF00209 L     @10,RMPLPTR                                         0211
         L     @10,ASCBPTR(,@10)                                   0211
         CLI   FIRSTPT(@10),X'0F'                                  0211
         BNE   @RF00211                                            0211
         TM    SECPART(@10),B'00111111'                            0211
         BNZ   @RF00211                                            0211
*                                    WKLSQA=ON;/* INDICATE LSQA    0212
*                                      AVAILABLE                     */
         OI    WKLSQA,B'01000000'                                  0212
*                                 END;/* END NOT SWAP IN             */
@RF00211 DS    0H                                                  0214
*                             END;  /* END SPCT ADDR NON ZERO        */
@RC00203 DS    0H                                                  0215
*                         END;      /* END LSQA FRAMES AVAILABLE     */
@RF00201 DS    0H                                                  0216
*                     END;          /* END NOT LSQA SWAP IN ERROR    */
@RC00195 DS    0H                                                  0217
*                 END;              /* END ABNORMAL MEM TERM         */
@RF00193 DS    0H                                                  0218
*RETRY1:                                                           0218
*               WKRETRY=TRAIA1;     /* INCREMENT RETRY INDEX         */
@RC00190 DS    0H                                                  0218
RETRY1   MVI   WKRETRY,X'02'                                       0218
*                                                                  0219
*/********************************************************************/
*/*                                                                  */
*/* TERMAIA1:                                                        */
*/* UPDATE THE AIAS ON THE LOCAL I/O QUEUE SO THAT SLOTS WILL BE     */
*/* FREED WHEN IN-PROCESS REQUESTS COMPLETE                          */
*/*       PCBPTR   = ADDRESS OF CURRENT PCB/AIA                      */
*/*       SAVEPTR1 = ADDRESS OF NEXT PCB/AIA                         */
*/*                                                                  */
*/********************************************************************/
*                                                                  0219
*               STACK1=ADDR(AIA1NEXT);/* PUSH RETRY ADDR INTO STACK  */
         LA    @10,AIA1NEXT                                        0219
         ST    @10,STACK1                                          0219
*               WKLSTKX=1;          /* INCREMENT LOOP RTRY INDEX     */
         MVI   WKLSTKX,X'01'                                       0220
*               PCBPTR=RSMLIOQF;    /* GET ADDR OF ACTIVE PCB        */
         L     PCBPTR,RSMLIOQF(,RSMHDPTR)                          0221
*               RFY                                                0222
*                 PCB BASED(PCBPTR);                               0222
*               DO WHILE PCBPTR^=0; /* LOOP THRU PCBS                */
         B     @DE00223                                            0223
@DL00223 DS    0H                                                  0224
*                 SAVEPTR1=PCBFQPA; /* GET NEXT PCB/AIA ADDR         */
         L     @10,PCBFQPA-1(,PCBPTR)                              0224
         LA    @10,0(,@10)                                         0224
         ST    @10,SAVEPTR1                                        0224
*                 RFY                                              0225
*                   AIA BASED(ADDR(PCBAIA));                       0225
*                 AIATERMR=ON;      /* INDICATE ADDR SPACE TERM      */
         OI    AIATERMR+36(PCBPTR),B'01000000'                     0226
*AIA1NEXT:                                                         0227
*                 PCBPTR=SAVEPTR1;  /* POINT TO NEXT PCB/AIA         */
AIA1NEXT L     PCBPTR,SAVEPTR1                                     0227
*               END;                /* END PCB LOOP                  */
@DE00223 LTR   PCBPTR,PCBPTR                                       0228
         BNZ   @DL00223                                            0228
*               WKLSTKX=0;          /* POP RETRY ADDR FROM STACK     */
         MVI   WKLSTKX,X'00'                                       0229
*RETRY2:                                                           0230
*               WKRETRY=TRAVIO;     /* INCREMENT RETRY INDEX         */
RETRY2   MVI   WKRETRY,X'03'                                       0230
*                                                                  0231
*/********************************************************************/
*/*                                                                  */
*/* TERMAVIO:                                                        */
*/* ASPCT VIO PROCESSING                                             */
*/*        LGEPTR   = ADDRESS OF LGE BEING PROCESSED                 */
*/*                                                                  */
*/********************************************************************/
*                                                                  0231
*               IF WKLSQA=ON THEN                                  0231
         TM    WKLSQA,B'01000000'                                  0231
         BNO   @RF00231                                            0231
*                 DO;               /* LSQA AVAILABLE                */
*                   WKLTRAS=ON;     /* INDICATE IN TRAS MODE         */
         OI    WKLTRAS,B'10000000'                                 0233
*                   RFY                                            0234
*                     R4 RSTD;                                     0234
*                   R4=RMPLASCB;    /* PUT ASCB @ IN REG FOR TRAS    */
         L     @10,RMPLPTR                                         0235
         L     R4,RMPLASCB(,@10)                                   0235
*                   DO;             /* ILRTRAS TO(R4)                */
*                     DCL                                          0237
*                       TRASASID PTR(15) DEFINED(ASCBASID);        0237
*                     PSALCCAV->LCCAASID=R4->TRASASID;             0238
         LH    @10,TRASASID(,R4)                                   0238
         L     @01,PSALCCAV                                        0238
         ST    @10,LCCAASID(,@01)                                  0238
*                     GEN(LCTL  1,1,ASCBSTOR(R4))                  0239
*                     REFS(ASCBSTOR,R4);                           0239
         LCTL  1,1,ASCBSTOR(R4)
*                   END;            /* ILRTRAS TO(R4) TRAS TO      0240
*                                      TERMINATING MEMORY            */
*                   RFY                                            0241
*                     R4 UNRSTD;                                   0241
*                   LGEPTR=ASHLGEQ; /* GET ADDR OF LGE QUEUE         */
         L     LGEPTR,ASHLGEQ(,RSMHDPTR)                           0242
*                   DO WHILE LGEPTR^=0;/* DO WHILE THERE ARE LGES    */
         B     @DE00243                                            0243
@DL00243 DS    0H                                                  0244
*                     RFY                                          0244
*                       LGE BASED(LGEPTR);                         0244
*                     RFY                                          0245
*                       R0 RSTD;                                   0245
*                     R0=LGEASPCT;  /* OBTAIN ASPCT ADDR             */
         L     R0,LGEASPCT(,LGEPTR)                                0246
*                     IF R0^=0 THEN                                0247
         LTR   R0,R0                                               0247
         BZ    @RF00247                                            0247
*                       DO;         /* ASPCT ADDR^= 0                */
*                         CALL ILRALS00;/* SCAN LPMES FOR THIS ASPCT */
         BAL   @14,ILRALS00                                        0249
*                         IF RMPLTYPE=ON THEN/* ABNORMAL MEMORY    0250
*                                      TERMINATION                   */
         L     @10,RMPLPTR                                         0250
         TM    RMPLTYPE(@10),B'10000000'                           0250
         BNO   @RF00250                                            0250
*                           IF ASPSAVED=OFF×/* ASPCT HAS NOT BEEN  0251
*                                      SAVED                         */
*                               ASPSSYM=ZERO THEN/* ZERO S SYMBOL    */
         L     @10,ASPCTPTR(,LGEPTR)                               0251
         TM    ASPSAVED(@10),B'10000000'                           0251
         BZ    @RT00251                                            0251
         CLC   ASPSSYM(8,@10),ZERO                                 0251
         BNE   @RF00251                                            0251
@RT00251 DS    0H                                                  0252
*                             WKVIOSC=WKVIOSC+ASPBKSLT;/* INCREMENT
*                                      INTERNAL VIO CNT              */
         L     @01,ASPCTPTR(,LGEPTR)                               0252
         L     @10,WKVIOSC                                         0252
         AL    @10,ASPBKSLT(,@01)                                  0252
         ST    @10,WKVIOSC                                         0252
*                         RFY                                      0253
*                           R0 UNRSTD;                             0253
@RF00251 DS    0H                                                  0253
@RF00250 DS    0H                                                  0254
*                       END;        /* END NON ZERO ASPCT ADDR       */
*                     LGEPTR=LGENEXT;/* GET NEXT LGE                 */
@RF00247 L     LGEPTR,LGENEXT(,LGEPTR)                             0255
*                   END;            /* END DO WHILE THERE ARE LGES   */
@DE00243 LTR   LGEPTR,LGEPTR                                       0256
         BNZ   @DL00243                                            0256
*                   DO;             /* ILRTRAS BACK                  */
*                     RFY                                          0258
*                       R15 RSTD;                                  0258
*                     R15=PSAAOLD;                                 0259
         L     R15,PSAAOLD                                         0259
*                     GEN(LCTL  1,1,ASCBSTOR(15))                  0260
*                     REFS(ASCBSTOR);                              0260
         LCTL  1,1,ASCBSTOR(15)
*                     PSALCCAV->LCCAASID=0;                        0261
         L     @14,PSALCCAV                                        0261
         SLR   @10,@10                                             0261
         ST    @10,LCCAASID(,@14)                                  0261
*                     RFY                                          0262
*                       R15 UNRSTD;                                0262
*                   END;            /* ILRTRAS BACK TRAS BACK        */
*                   WKLTRAS=OFF;    /* RESET TRAS MODE INDICATOR     */
         NI    WKLTRAS,B'01111111'                                 0264
*                 END;              /* END LSQA AVAILABLE            */
*RETRY3:                                                           0266
*               WKRETRY=TRVIO;      /* INCREMENT RETRY INDEX         */
@RF00231 DS    0H                                                  0266
RETRY3   MVI   WKRETRY,X'04'                                       0266
*                                                                  0267
*/********************************************************************/
*/*                                                                  */
*/* TERMVIO:                                                         */
*/* COMMON VIO PROCESSING - DEQ ALL AIAS/ACES FROM LGE QUEUES -      */
*/* FREE LGVTES AND LGES                                             */
*/*        LGEPTR   = ADDRESS OF LGE BEING PROCESSED                 */
*/*        WORKPTR1 = ADDRESS OF AIA/ACE BEING PROCESSED             */
*/*                                                                  */
*/********************************************************************/
*                                                                  0267
*               RFY                                                0267
*                 LGE BASED(LGEPTR);                               0267
*               LGEPTR=ASHLGEQ;     /* POINT TO LGE QUEUE            */
         L     LGEPTR,ASHLGEQ(,RSMHDPTR)                           0268
*               STACK1=ADDR(VIORETRY);/* PUSH LOOP RETRY ADDR INTO   */
         LA    @10,VIORETRY                                        0269
         ST    @10,STACK1                                          0269
*               WKLSTKX=1;          /* STACK                         */
         MVI   WKLSTKX,X'01'                                       0270
*               DO WHILE LGEPTR^=0; /* DO WHILE THERE ARE LGES       */
         B     @DE00271                                            0271
@DL00271 DS    0H                                                  0272
*                 WORKPTR1=LGEPROCF;/* GET 1ST AIA ON PROCESS QUE    */
         L     WORKPTR1,LGEPROCF(,LGEPTR)                          0272
*                 STACK2=ADDR(VIONEXT);/* PUSH LOOP RETRY ADDR INTO  */
         LA    @10,VIONEXT                                         0273
         ST    @10,STACK2                                          0273
*                 WKLSTKX=2;        /* STACK                         */
         MVI   WKLSTKX,X'02'                                       0274
*                 DO WHILE WORKPTR1^=0;/* DO WHILE THERE ARE AIA/ACES*/
         B     @DE00275                                            0275
@DL00275 DS    0H                                                  0276
*                   RFY                                            0276
*                     AIA BASED(WORKPTR1);                         0276
*                   LGEPROCF=AIAFQPA;/* GET NEXT ELEMENT ADDR        */
         L     @10,AIAFQPA(,WORKPTR1)                              0277
         ST    @10,LGEPROCF(,LGEPTR)                               0277
*                   IF AIAFQPA=0 THEN/* IF LAST AIA ON QUEUE         */
         SLR   @15,@15                                             0278
         CR    @10,@15                                             0278
         BNE   @RF00278                                            0278
*                     LGEPROCL=0;   /* SET LGE ADDR TO ZERO          */
         ST    @15,LGEPROCL(,LGEPTR)                               0279
*                   IF AIAOP=ACEIO THEN                            0280
@RF00278 CLI   AIAOP(WORKPTR1),0                                   0280
         BNE   @RF00280                                            0280
*                     DO;           /* IF AIA                        */
*                       AIAFQPA=0;  /* SET FORWARD AND BACKWARD      */
         SLR   @10,@10                                             0282
         ST    @10,AIAFQPA(,WORKPTR1)                              0282
*                       AIABQPA=0;  /* QUEUE POINTERS TO ZERO        */
         ST    @10,AIABQPA(,WORKPTR1)                              0283
*                       IF AIAPRINO=OFF THEN                       0284
         TM    AIAPRINO(WORKPTR1),B'01000000'                      0284
         BNZ   @RF00284                                            0284
*                         DO;       /* IF PAGE I/O NOT STARTED       */
*                           AIANXAIA=WKPIOPQ;/* QUEUE AIA TO INTERNAL
*                                      PIOP                          */
         L     @10,WKPIOPQ                                         0286
         ST    @10,AIANXAIA(,WORKPTR1)                             0286
*                           WKPIOPQ=WORKPTR1;/* QUEUE                */
         ST    WORKPTR1,WKPIOPQ                                    0287
*                         END;      /* END PAGE I/O NOT STARTED      */
*                     END;          /* END AIA                       */
*                   ELSE                                           0290
