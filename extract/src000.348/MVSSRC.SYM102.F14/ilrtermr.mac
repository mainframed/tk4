*/*                                                                  */
*/* ILRSLTRV ALTERNATE ENTRY POINT -                                 */
*/* PURPOSE - TO DETERMINE IF THERE ARE ENOUGH SLOTS TO CREATE A NEW */
*/*          ADDRESS SPACE                                           */
*/* INPUT -  REGISTER 2 = RSMHD ADDRESS                              */
*/*          REGISTER 3 = PVT ADDRESS                                */
*/* OUTPUT - REGISTER 15 = 0 THEN THERE ARE ENOUGH SLOTS             */
*/*          REGISTER 15 = 4 THERE ARE NOT ENOUGH SLOTS              */
*/* REGISTER USAGE - 0 SLOTS REMAINING                               */
*/*                  1 ASMVT ADDRESS                                 */
*/*                  2 RSMHD ADDRESS                                 */
*/*                  3 PVT ADDRESS                                   */
*/*                  4 REQUIRED SLOTS                                */
*/*                  5 BACK SLOTS                                    */
*/*                 15 RETURN CODE                                   */
*/*                                                                  */
*/********************************************************************/
*                                                                  0464
*ILRSLTRV:                                                         0464
*   ENTRY;                                                         0464
*                                                                  0464
@EP00464 DS    0H                                                  0465
*   /*****************************************************************/
*   /*                                                               */
*   /* REGISTER DECLARES FOR ILRSLTRV                                */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0465
*   DCL                                                            0465
*     ASMVTP PTR(31) REG(1);        /* REGISTER 1 ASMVT PTR          */
*   DCL                                                            0466
*     PVTPTR PTR(31) REG(3);        /* REGISTER 3 PVT ADDRESS        */
*   DCL                                                            0467
*     RC PTR(31) REG(15);           /* REGISTER 15 - RETURN CODE     */
*   DCL                                                            0468
*     REQSLOTS PTR(31) REG(4);      /* REGISTER 4 SLOTS REQUIRED     */
*   DCL                                                            0469
*     REMAINDR FIXED(31) REG(0);    /* REGISTER 0 SLOTS REMAINING    */
*   DCL                                                            0470
*     BACKSLOT PTR(31) REG(5);      /* REGISTER 5 BACK SLOTS         */
*   RFY                                                            0471
*    (ASMVTP,                                                      0471
*     PVTPTR,                                                      0471
*     REQSLOTS) RSTD;                                              0471
*   REQSLOTS=(PVTLCSA-PVTLPRIV)/16; /* COMPUTE PRIVATE AREA SIZE     */
         LH    REQSLOTS,PVTLCSA(,PVTPTR)                           0472
         N     REQSLOTS,@CF02800                                   0472
         LH    @10,PVTLPRIV(,PVTPTR)                               0472
         N     @10,@CF02800                                        0472
         SLR   REQSLOTS,@10                                        0472
         SRDA  REQSLOTS,32                                         0472
         D     REQSLOTS,@CF00157                                   0472
         LR    REQSLOTS,@05                                        0472
*   IF ILRSLOTC^=0 THEN             /* DIVIDE NEEDED SLOTS BY        */
         L     @10,@CA02573                                        0473
         L     @10,ILRSLOTC-ILRSLOTC(,@10)                         0473
         LTR   @10,@10                                             0473
         BZ    @RF00473                                            0473
*     REQSLOTS=REQSLOTS/ILRSLOTC;   /* ILRSLOTC CONSTANT IF SLOTC    */
         LR    @07,REQSLOTS                                        0474
         SLR   @06,@06                                             0474
         DR    @06,@10                                             0474
         LR    REQSLOTS,@07                                        0474
*   RFY                                                            0475
*     RC RSTD;                      /* CONSTANT IS NON ZERO          */
@RF00473 DS    0H                                                  0476
*   ASMVTP=CVTASMVT;                /* OBTAIN ASMVT ADDRESS          */
         L     @14,CVTPTR                                          0476
         L     ASMVTP,CVTASMVT(,@14)                               0476
*   RFY                                                            0477
*     ASMVT BASED(ASMVTP);                                         0477
*   RFY                                                            0478
*     BACKSLOT RSTD;                                               0478
*   BACKSLOT=ASMBKSLT;              /* OBTAIN CURRENT BACK SLOT NO   */
         L     BACKSLOT,ASMBKSLT(,ASMVTP)                          0479
*   RFY                                                            0480
*     REMAINDR RSTD;                                               0480
*SLTAGAIN:                                                         0481
*   REMAINDR=BACKSLOT-REQSLOTS;     /* ARE THERE ENOUGH TO FILL      */
SLTAGAIN LR    REMAINDR,BACKSLOT                                   0481
         SLR   REMAINDR,REQSLOTS                                   0481
*   IF REMAINDR<0 THEN                                             0482
         LTR   REMAINDR,REMAINDR                                   0482
         BNM   @RF00482                                            0482
*     RC=4;                         /* THIS REQUEST - IF NOT SET     */
         LA    RC,4                                                0483
*   ELSE                                                           0484
*     DO;                           /* RETURN CODE TO 4              */
         B     @RC00482                                            0484
@RF00482 DS    0H                                                  0485
*       CS(BACKSLOT,REMAINDR,ASMBKSLT);/* UPDATE ASMBKSLT            */
         CS    BACKSLOT,@00,ASMBKSLT(ASMVTP)                       0485
*       BC(7,SLTAGAIN);             /* IF COMPARE & SWAP WAS OK      */
         BC    7,SLTAGAIN                                          0486
*       ASHBKSLT=REQSLOTS;          /* UPDATE ASHBKSLT               */
         ST    REQSLOTS,ASHBKSLT(,RSMHDPTR)                        0487
*       RC=0;                       /* SET GOOD RETURN CODE          */
         SLR   RC,RC                                               0488
*       RFY                                                        0489
*        (BACKSLOT,                                                0489
*         REMAINDR) UNRSTD;                                        0489
*     END;                          /* END ENOUGH SLOTS FOR MEMORY   */
*   RETURN CODE(RC);                /* RETURN                        */
@RC00482 L     @14,12(,@13)                                        0491
         LM    @00,@12,20(@13)                                     0491
         BR    @14                                                 0491
*   RFY                                                            0492
*    (RC,                                                          0492
*     REMAINDR,                                                    0492
*     REQSLOTS,                                                    0492
