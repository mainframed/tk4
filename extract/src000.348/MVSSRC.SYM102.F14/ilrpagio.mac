         TITLE 'ILRPAGIO-PAGING I/O CONTROL MODULE                     *
                        '
ILRPAGIO CSECT
        USING *,R15
        MODID
        L     R15,FLCCVT              GET THE ADDRESS OF THE CVT
        USING CVTMAP,R15              USE R15 TO ADDRESS THE CVT
        L     R15,CVTASMVT            PICK UP ADDRESS OF THE ASMVT
        USING ASMVT,R15               USE R15 TO ADDRESS THE ASMVT
        STM   R2,R14,ASMWKSA1+4       STORE REGISTERS IN WORK/SAVE
        BALR  R10,0                   GET BASE ADDRESS
        USING *,R10                   SET UP CODE ADDRESSABILITY
        DROP 15                       DROP 15 AS BASE
*ILRCBPGI:                                                         0077
*   ASMVTPTR=R15;                   /* PUT ASMVT ADDRESS IN STANDARD
*                                      POINTER REGISTER              */
ILRCBPGI LR    ASMVTPTR,R15                                        0077
*   RFY                                                            0078
*     R15 UNRSTD;                   /* RELEASE REG FOR USE           */
*   ASM1RG1=0;                      /* ZERO PARAMETER REG SAVE       */
         SLR   @12,@12                                             0079
         ST    @12,ASM1RG1(,ASMVTPTR)                              0079
*   ASM1RG15=0;                     /* ZERO RETURN CODE REG SAVE     */
         ST    @12,ASM1RG15(,ASMVTPTR)                             0080
*   RFY                                                            0081
*     R8 RSTD;                      /* RESTRICT REG FROM USE         */
*   R8=ASMPIOFR;                    /* GET ADDRESS OF THE FRR        */
         L     R8,ASMPIOFR(,ASMVTPTR)                              0082
*                                                                  0083
*   /*****************************************************************/
*   /*                                                               */
*   /* WE WILL NOW ESTABLISH THE FUNCTIONAL RECOVERY ROUTINE AND ON  */
*   /* RETURN REGISTER 4 WILL HAVE THE ADDRESS OF THE 24 BYTE ASM    */
*   /* TRACKING AREA(ATA).                                           */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0083
*   GENERATE REFS(PSACSTK,PSA,FRRSCURR,FRRS,FRRSLAST,PSALSFCC,FRRSELEN,
*       FRRSFRRA,FRRSENTR,FRRSPARM);                               0083
        SETFRR A,FRRAD=(8),WRKREGS=(9,12),PARMAD=(4)
*   RFY                                                            0084
*     R8 UNRSTD;                    /* RELEASE REG FOR USE           */
*   ATAMODID=ATAMPGIO;              /* PUT MODID IN ATA              */
         MVI   ATAMODID(ATAPTR),X'01'                              0085
*   ATAPAGIO='1'B;                  /* TURN ON FLAG IN ATA           */
         OI    ATAPAGIO(ATAPTR),B'00000010'                        0086
*   ATAAIA=R1;                      /* GET THE ADDRESS OF THE AIA    */
         ST    R1,ATAAIA(,ATAPTR)                                  0087
*   AIAPTR=R1;                      /* SET UP AIA POINTER            */
         LR    AIAPTR,R1                                           0088
*   RFY                                                            0089
*     R1 UNRSTD;                    /* RELEASE REG FOR USE           */
*                                                                  0089
*   /*****************************************************************/
*   /*                                                               */
*   /* NEXT WE COMPUTE THE ADDRESS OF THE PCB WHICH PRECEDES THE AIA.*/
*   /* WE NEED THIS ADDRESS TO GET THE XPTE                          */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0090
*   RFY                                                            0090
*     PCBPTR RSTD;                  /* RESTRICT USE OF XPTEPTR       */
*   RFY                                                            0091
*     ASCB BASED(ASM1ASCB);         /* ADDRESSABILITY FOR ASCB       */
*   PCBPTR=AIAPTR-(LENGTH(PCB)-LENGTH(AIA));                       0092
         LR    PCBPTR,AIAPTR                                       0092
         SL    PCBPTR,@CF01755                                     0092
*   ASM1ASCB=PCBASCB;               /* GET THE ADDRESS OF THE ASCB   */
         L     @12,PCBASCB(,PCBPTR)                                0093
         ST    @12,ASM1ASCB(,ASMVTPTR)                             0093
*                                                                  0094
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS DO LOOP WILL PROCESS THE CHAIN OF AIA'S JUST PASSED BY   */
*   /* THE CALLER.                                                   */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0094
*   DO WHILE(AIAPTR^=0);            /* CYCLE THROUGH AIA CHAIN       */
         B     @DE00094                                            0094
@DL00094 DS    0H                                                  0095
*     RFY                                                          0095
*       XPTEPTR RSTD;               /* RESTRICT USE OF XPTEPTR       */
*     XPTEPTR=PCBXPT;               /* GET ADDRESS OF THE XPTE       */
         L     XPTEPTR,PCBXPT(,PCBPTR)                             0096
*     RFY                                                          0097
*       PCBPTR UNRSTD;              /* RELEASE PCBPTR FOR USE        */
*     IF AIAWRITE='0'B THEN         /* IS THIS A READ AIA            */
         TM    AIAWRITE(AIAPTR),B'10000000'                        0098
         BNZ   @RF00098                                            0098
*       DO;                         /* YES-DO THE READ PROCESSING    */
*/********************************************************************/
*/*                                                                  */
*/* FUNCTION = THIS CODE PROCESSES READ REQUESTS(AIAWRITE='0').      */
*/*            IF THIS IS A PRIVATE AREA PAGE IN A SWAP, UPDATE THE  */
*/*            ASHIOCNT AND QUEUE THE AIA TO THE ASMSTAGQ.           */
*/*            IF THE XPTE SPECIFIES A VIO SLOT THE AIA IS SENT TO   */
*/*            ILRPOS TO CHECK OUT THE REQUEST. THE AIA IS DEQUEUED  */
*/*            AND A SINGLE AIA IS SENT. IF POS RETURNS AN AIA IN THE*/
*/*            ATA IT MEANS THE AIA IS AN ERROR SO THIS AIA AND THE  */
*/*            REST OF THE CHAIN WILL BE RETURNED TO RSM.            */
*/*            IF AN ERROR BIT IS ON IN THE XPTE PUT AN ERROR BIT ON */
*/*            IN THE AIA AND RETURN THE AIA CHAIN TO RSM.           */
*/*            IF NO ERRORS ARE FOUND AND IT IS NOT A VIO REQUEST    */
*/*            MOVE THE LSID FROM THE XPTE TO THE AIA.               */
*/*                                                                  */
*/* INPUT = RSMHDPTR = ADDRESS OF THE RSMHD                          */
*/*         ASMVTPTR = ADDRESS OF THE ASMVT                          */
*/*         ATAPTR = ADDRESS OF THE ATA                              */
*/*         AIAPTR = ADDRESS OF THE CURRENT AIA                      */
*/*         XPTEPTR = ADDRESS OF THE XPTE                            */
*/*                                                                  */
*/* EXTERNAL ROUTINES AND REFERENCES = ILRPOS TO CHECK VIO REQUESTS  */
*/*                                                                  */
*/********************************************************************/
*                                                                  0100
*         IF AIASWPFX='1'B THEN     /* IS THIS A P.A. PAGE IN SWAP   */
         TM    AIASWPFX(AIAPTR),B'00000010'                        0100
         BNO   @RF00100                                            0100
*           DO;                     /* YES-NO SETUP REQUIRED         */
*             ASHIOCNT=ASHIOCNT+1;  /* INCREMENT COUNT OF P.A. I/O 0102
*                                      REQUESTS                      */
         LA    @12,1                                               0102
         AL    @12,ASHIOCNT(,RSMHDPTR)                             0102
         ST    @12,ASHIOCNT(,RSMHDPTR)                             0102
*             GO TO QUEIT;          /* TO TO QUEUE THE AIA           */
         B     QUEIT                                               0103
*           END;                    /* END SWAP PROCESSING           */
*                                                                  0105
*         /***********************************************************/
*         /*                                                         */
*         /* VIO AIA'S MUST BE SENT TO POS TO BE CHECKED. POS WILL   */
*         /* PUT IT ON THE ASMSTAGQ IF IT CAN BE STARTED. A NON-ZERO */
*         /* ATAAIA FROM POS MEANS THE AIA IS AN ERROR.              */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0105
*         IF XPTVIOLP='1'B THEN     /* IS THIS A VIO XPTE            */
@RF00100 TM    XPTVIOLP(XPTEPTR),B'10000000'                       0105
         BNO   @RF00105                                            0105
*           DO;                     /* PREPARE FOR ILRPOS00          */
*             AIALPID=XPTLPID;      /* MOVE LPID INTO AIA            */
         MVC   AIALPID(8,AIAPTR),XPTLPID(XPTEPTR)                  0107
*             AIAVIO='1'B;          /* MARK AIA AS VIO AIA           */
         OI    AIAVIO(AIAPTR),B'00100000'                          0108
*             ASM1RG1=AIANXAIA;     /* SAVE NEXT AIA ADDRESS         */
         L     @12,AIANXAIA(,AIAPTR)                               0109
         ST    @12,ASM1RG1(,ASMVTPTR)                              0109
*             AIANXAIA=0;           /* DEQUEUE THE REST OF CHAIN     */
         SLR   @12,@12                                             0110
         ST    @12,AIANXAIA(,AIAPTR)                               0110
*             CALL ILRPOS;          /* CALL POS FOR CHECKING AIA'S   */
         L     @15,@CV01693                                        0111
         BALR  @14,@15                                             0111
*             IF ATAAIA=0 THEN      /* IS THIS AIA OK                */
         L     @12,ATAAIA(,ATAPTR)                                 0112
         LTR   @12,@12                                             0112
         BNZ   @RF00112                                            0112
*               DO;                 /* YES-THERE IS NO ERROR         */
*                 ATAAIA=ASM1RG1;   /* GET NEXT AIA FROM SAVE AREA   */
         L     @12,ASM1RG1(,ASMVTPTR)                              0114
         ST    @12,ATAAIA(,ATAPTR)                                 0114
*                 GO TO CHKNEXT;    /* GO CHECK FOR ANOTHER AIA      */
         B     CHKNEXT                                             0115
*               END;                /* END OF NORMAL PROCESSING      */
*             ELSE                  /* AIA FROM POS IS ERROR         */
*               DO;                 /* SET UP ERROR RETURN           */
@RF00112 DS    0H                                                  0118
*                 AIANXAIA=ASM1RG1; /* REESTABLISH AIA CHAIN         */
         L     @12,ASM1RG1(,ASMVTPTR)                              0118
         ST    @12,AIANXAIA(,AIAPTR)                               0118
*                 ASM1RG15=4;       /* PUT CODE IN SAVE AREA(R15)    */
         MVC   ASM1RG15(4,ASMVTPTR),@CF00094                       0119
*                 ASM1RG1=AIAPTR;   /* PUT ERROR AIA IN SAVE(R1)     */
         ST    AIAPTR,ASM1RG1(,ASMVTPTR)                           0120
*                 GO TO FINISH;     /* GET OUT OF LOOP               */
         B     FINISH                                              0121
*               END;                                               0122
*           END;                    /* END VIO AIA PROCESSSING       */
*                                                                  0123
*         /***********************************************************/
*         /*                                                         */
*         /* THIS IS NOT A VIO READ SO THE XPTE MUST BE CHECKED TO   */
*         /* SEE IF THE SLOT CAN BE FREED                            */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0124
*         ELSE                      /* NOT A VIO PAGE-MUST BE A READ */
*           DO;                                                    0124
@RF00105 DS    0H                                                  0125
*             IF XPTVALID='1'B&     /* IS XPTE VALID                 */
*                 XPTIOERR='0'B THEN/* NO PREVIOUS ERROR             */
         TM    XPTVALID(XPTEPTR),B'10000000'                       0125
         BNO   @RF00125                                            0125
         TM    XPTIOERR(XPTEPTR),B'00010000'                       0125
         BNZ   @RF00125                                            0125
*               AIALSID=XPTLSID;    /* NO ERRORS-MOVE THE LSID INTO
*                                      THE AIA FOR THE READ          */
         L     @12,XPTLSID(,XPTEPTR)                               0126
         ST    @12,AIALSID(,AIAPTR)                                0126
*             ELSE                  /* THERE WAS AN ERROR IN XPTE    */
*               DO;                 /* DETERMINE WHICH ERROR         */
         B     @RC00125                                            0127
@RF00125 DS    0H                                                  0128
*                 IF XPTVALID='0'B THEN/* XPT NOT VALID?             */
         TM    XPTVALID(XPTEPTR),B'10000000'                       0128
         BNZ   @RF00128                                            0128
*                   AIABADID='1'B;  /* SET ERROR BIT IN AIA          */
         OI    AIABADID(AIAPTR),B'00000001'                        0129
*                 ELSE              /* MUST BE XPTIOERR              */
*                   AIAPRIER='1'B;  /* SET I/O ERROR BIT             */
         B     @RC00128                                            0130
@RF00128 OI    AIAPRIER(AIAPTR),B'00001000'                        0130
*                 ASM1RG15=4;       /* SET BAD RETURN CODE           */
@RC00128 MVC   ASM1RG15(4,ASMVTPTR),@CF00094                       0131
*                 ASM1RG1=AIAPTR;   /* GET BAD AIA ADDRESS           */
         ST    AIAPTR,ASM1RG1(,ASMVTPTR)                           0132
*                 GO TO FINISH;     /* GET OUT OF LOOP               */
         B     FINISH                                              0133
*               END;                /* END-DETERMINE WHICH ERROR     */
*           END;                                                   0135
@RC00125 DS    0H                                                  0136
*       END;                        /* END READ PROCESSING           */
*                                                                  0137
*/********************************************************************/
*/*                                                                  */
*/* FUNCTION = THIS CODE WILL BE EXECUTED FOR VAMSI REQUESTS         */
*/*            (AIAVIO='1'). THIS MEANS THAT RSM HAS SCHEDULED A     */
*/*            MOVEOUT OF VIO BUFFERS AND THE ENTIRE CHAIN HAS TO BE */
*/*            SENT TO ILRPOS TO BE CHECKED. IF AN ADDRESS IS        */
*/*            RETURNED IN THE ATA IT MEANS THERE IS AN ERROR AND IT */
*/*            MUST BE SENT BACK TO RSM ALONG WITH THE REST OF THE   */
*/*            CHAIN.                                                */
*/*                                                                  */
*/* INPUT = RSMHDPTR = ADDRESS OF THE RSMHD                          */
*/*         ASMVTPTR = ADDRESS OF THE ASMVT                          */
*/*         ATAPTR = ADDRESS OF THE ATA                              */
*/*         XPTEPTR = ADDRESS OF THE XPTE                            */
*/*                                                                  */
*/* EXTERNAL REOUTINES AND REFERENCES = ILRPOS                       */
*/*                                                                  */
*/********************************************************************/
*                                                                  0137
*     ELSE                          /* IF NOT A READ                 */
*       DO;                         /* NON-READ PROCESSING           */
         B     @RC00098                                            0137
@RF00098 DS    0H                                                  0138
*         IF AIAVIO='1'B THEN       /* IS THIS A VAMSI REQUEST       */
         TM    AIAVIO(AIAPTR),B'00100000'                          0138
         BNO   @RF00138                                            0138
*           DO;                     /* YES-DO VAMSI PROCESSING       */
*             CALL ILRPOS;          /* GO TO POS FOR CHECKING        */
         L     @15,@CV01693                                        0140
         BALR  @14,@15                                             0140
*             IF ATAAIA^=0 THEN     /* ANY ERRORS RETURNED           */
         L     @12,ATAAIA(,ATAPTR)                                 0141
         LTR   @12,@12                                             0141
         BZ    @RF00141                                            0141
*               DO;                 /* YES-RETURN ERRORS             */
*                 ASM1RG15=4;       /* SET A BAD RETURN CODE         */
         MVC   ASM1RG15(4,ASMVTPTR),@CF00094                       0143
*                 ASM1RG1=ATAAIA;   /* ERROR AIA ADDR IN REG 1 IN THE
*                                      SAVE AREA                     */
         ST    @12,ASM1RG1(,ASMVTPTR)                              0144
*               END;                /* END ERROR SETUP               */
*             GO TO FINISH;         /* GET OUT OF THE LOOP           */
         B     FINISH                                              0146
*           END;                    /* END VAMSI PROCESSING          */
*         ELSE                      /* NO READ AND NO VAMSI          */
*                                                                  0148
*           /*********************************************************/
*           /*                                                       */
*           /* HERE WE KNOW WE HAVE A NON-VAMSI WRITE REQUEST WHICH  */
*           /* MEANS THAT THE SLOT/SLOTS REPRESENTED BY THE XPTE CAN */
*           /* BE FREED IF THE XPTE IS VALID AND NOT A VIO SLOT      */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0148
*           DO;                     /* WRITE PROCESSING              */
@RF00138 DS    0H                                                  0149
*             IF XPTVALID='1'B THEN /* IS THE XPTE VALID             */
         TM    XPTVALID(XPTEPTR),B'10000000'                       0149
         BNO   @RF00149                                            0149
*               DO;                 /* YES-DO VALID PROCESSING       */
*                 RFY                                              0151
*                  (R1,                                            0151
*                   R13) RSTD;      /* RESTRICT USE OF REGS          */
*                 R1=XPTLSID;       /* GET LSID TO BE FREED          */
         L     R1,XPTLSID(,XPTEPTR)                                0152
*                 R13=ADDR(ASMSAVE);/* GET ADDRESS OF SAVE AREA      */
         LA    R13,ASMSAVE(,ASMVTPTR)                              0153
*                 CALL FREESLOT;    /* GO TO FREE THE SLOT           */
         L     @15,ASMPFRSL(,ASMVTPTR)                             0154
         BALR  @14,@15                                             0154
*                 IF AIAPRIV='0'B THEN/* PRIVATE AREA PAGE??         */
         TM    AIAPRIV(AIAPTR),B'01000000'                         0155
         BNZ   @RF00155                                            0155
*                   DO;             /* FREE SECONDARY SLOT           */
*                     IF XPTLSID2^=0 THEN/* IS THERE A SECONDARY ??  */
         L     @12,XPTLSID2(,XPTEPTR)                              0157
         LTR   @12,@12                                             0157
         BZ    @RF00157                                            0157
*                       DO;         /* FREE SECONDARY                */
*                         R1=XPTLSID2;/* GET LSID TO BE FREED        */
         LR    R1,@12                                              0159
*                         CALL FREESLOT;/* GO TO FREE THE SLOT       */
         L     @15,ASMPFRSL(,ASMVTPTR)                             0160
         BALR  @14,@15                                             0160
*                         RFY                                      0161
*                          (R1,                                    0161
*                           R13) UNRSTD;/* RELEASE REGS FOR USE      */
*                       END;        /* END FREE SECONDARY            */
*                     GO TO CHKDPX; /* GO CHK DUPLEX ACTIVE          */
         B     CHKDPX                                              0163
*                   END;            /* END-FREE SECONDARY SLOT       */
*               END;                /* END VALID PROCESSING          */
*                                                                  0166
*             /*******************************************************/
*             /*                                                     */
*             /* IF THIS IS A PRIVATE AREA PAGE THE PRIVATE AREA     */
*             /* ALLOCATED COUNTS IN THE ASMVT AND ASCB MUST BE      */
*             /* INCREMENTED. THE COUNTS ARE INCREMENTED ONLY FOR    */
*             /* SLOTS THAT HAVE TO BE ALLOCATED. IF A SLOT WAS FREED*/
*             /* AND REUSED THE COUNT IS NOT INCREMENTED SINCE THERE */
*             /* WAS AN ALLOCATED SLOT.                              */
*             /*                                                     */
*             /*******************************************************/
*                                                                  0166
*             ELSE                  /* THE XPTE IS NOT VALID SO NO 0166
*                                      SLOTS CAN BE FREED            */
*               DO;                 /* UPDATE CNTS AND CHK DPX       */
         B     @RC00149                                            0166
@RF00149 DS    0H                                                  0167
*                 IF AIAPRIV='1'B THEN/* IF THIS IS A PRIVATE AREA 0167
*                                      PAGE AND THERE WAS NO       0167
*                                      PREVIOUSLY WRITTEN SLOT THE 0167
*                                      ALLOCATED COUNTS IN THE ASCB
*                                      AND ASMVT ARE INCREMENTED     */
         TM    AIAPRIV(AIAPTR),B'01000000'                         0167
         BNO   @RF00167                                            0167
*                   DO;             /* UPDATE COUNTS                 */
*                     XPTVIOLP='0'B;/* MAKE SURE VIOLP BIT IS OFF    */
         NI    XPTVIOLP(XPTEPTR),B'01111111'                       0169
*                     ASCBNVSC=ASCBNVSC+1;/* INCREMENT THE ASCB    0170
*                                      NON-VIO ALLOCATED SLOT COUNT  */
         L     @12,ASM1ASCB(,ASMVTPTR)                             0170
         LA    @07,1                                               0170
         LH    @15,ASCBNVSC(,@12)                                  0170
         ALR   @15,@07                                             0170
         STH   @15,ASCBNVSC(,@12)                                  0170
*                     ASMNVSC=ASMNVSC+1;/* INCREMENT ASMVT COUNT     */
         AL    @07,ASMNVSC(,ASMVTPTR)                              0171
         ST    @07,ASMNVSC(,ASMVTPTR)                              0171
*                   END;                                           0172
*                 ELSE              /* NOT PRIVATE PAGE-CHECK IF DPX
*                                      ACTIVE                        */
*                                                                  0173
*                   /*************************************************/
*                   /*                                               */
*                   /* THIS IS NOT A PRIVATE AREA PAGE SO WE CHECK IF*/
*                   /* DUPLEXING IS ACTIVE AND IF A COMMON DATA SET  */
*                   /* IS AVAILABLE. IF DUPLEXING IS ACTIVE THE      */
*                   /* DUPLEX FLAG IN THE AIA IS TURNED ON. IF THE   */
*                   /* COMMON DATA SET IS NOT AVAILABLE THE SECONDARY*/
*                   /* ERROR FLAG IS TURNED ON.                      */
*                   /*                                               */
*                   /*************************************************/
*                                                                  0173
*CHKDPX:                                                           0173
*                   DO;             /* CHECK COMMON DS AVAILABILITY  */
         B     @RC00167                                            0173
@RF00167 DS    0H                                                  0173
CHKDPX   DS    0H                                                  0174
*                     IF ASMDUPLX='1'B THEN/* IS DUPLEXING ACTIVE    */
         TM    ASMDUPLX(ASMVTPTR),B'10000000'                      0174
         BNO   @RF00174                                            0174
*                       DO;         /* YES-CHECK FOR A COMMON DS     */
*                         AIADUPLX='1'B;/* TURN ON DUPLEX FLAG       */
         OI    AIADUPLX(AIAPTR),B'00010000'                        0176
*                         IF ASMNOCWQ='1'B THEN/* COMMON DS AVAIL    */
         TM    ASMNOCWQ(ASMVTPTR),B'01000000'                      0177
         BNO   @RF00177                                            0177
*                           AIASECER='1'B;/* NO-TURN ON THE ERROR  0178
*                                      FLAG TO MAKE ILRQIOE THINK  0178
*                                      THIS IS A REDRIVE AND ONLY THE
*                                      SECONDARY COPY WILL BE WRITTEN
*                                      THUS BYPASSING THE COMMON DATA
*                                      SET.                          */
         OI    AIASECER(AIAPTR),B'00000100'                        0178
*                       END;        /* END-CHECK FOR A COMMON DS     */
@RF00177 DS    0H                                                  0180
*                   END;            /* END COMMON DS AVAILABILITY    */
@RF00174 DS    0H                                                  0181
*               END;                /* UPDATE COUNTS AND DPX CHK     */
*                                                                  0181
@RC00167 DS    0H                                                  0182
*             /*******************************************************/
*             /*                                                     */
*             /* THE XPTE IS NOW ZEROED AND THE PAGE OUT IN PROGRESS */
*             /* FLAG IS TURNED ON. FOR PRIVATE AREA PAGES THE       */
*             /* ASHIOCNT IS INCREMENTED. THE ASHIOCNT ACTS AS A     */
*             /* TRIGGER FOR SWAP OUT OPERATIONS. WHEN ALL PRIVATE   */
*             /* AREA PAGES ARE FINISHED BEING SWAPPED OUT THE LSQA  */
*             /* CAN BE STARTED. THIS COUNT(DECREMENTED ON           */
*             /* COMPLETION) TELLS WHEN THE PRIVATE AREA IS FINISHED */
*             /*                                                     */
*             /*******************************************************/
*                                                                  0182
*             XPTVALID='0'B;        /* INVALIDATE THE XPTE           */
@RC00149 NI    XPTVALID(XPTEPTR),B'01111111'                       0182
*             XPTLPID=XPTLPID&&XPTLPID;/* CLEAR THE XPTE FIELD       */
         XC    XPTLPID(8,XPTEPTR),XPTLPID(XPTEPTR)                 0183
*             XPTPOINP='1'B;        /* TURN ON PAGE OUT IN PROGRESS
*                                      FLAG                          */
         OI    XPTPOINP(XPTEPTR),B'00100000'                       0184
*             RFY                                                  0185
*               XPTEPTR UNRSTD;     /* RELEASE REG FOR USE           */
*           END;                    /* END WRITE PROCESSING          */
*       END;                        /* END NON-READ PROCESSING       */
*                                                                  0188
*     /***************************************************************/
*     /*                                                             */
*     /* THIS SECTION OF CODE IS COMMON FOR READ, VIO, AND WRITE     */
*     /* PROCESSING. THE NORMAL FLOW WILL COME THROUGH HERE.         */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0188
*     IF AIAPRIV='1'B THEN          /* PRIVATE AREA PAGE?            */
@RC00098 TM    AIAPRIV(AIAPTR),B'01000000'                         0188
         BNO   @RF00188                                            0188
*       ASHIOCNT=ASHIOCNT+1;        /* YES-INCREMENT ASMHD COUNT     */
*                                                                  0189
         LA    @12,1                                               0189
         AL    @12,ASHIOCNT(,RSMHDPTR)                             0189
         ST    @12,ASHIOCNT(,RSMHDPTR)                             0189
*     /***************************************************************/
*     /*                                                             */
*     /* THE AIA IS NOW PUT ON THE ASMSTAGQ FOR ILRQIOE. THIS QUEUE  */
*     /* ALSO SERVES TO TELL PAGIO IF ILRQIOE SHOULD BE INVOKED.     */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0190
*QUEIT:                                                            0190
*     ASM1NXTA=AIANXAIA;            /* SAVE NEXT AIA ADDRESS         */
@RF00188 DS    0H                                                  0190
QUEIT    L     @12,AIANXAIA(,AIAPTR)                               0190
         ST    @12,ASM1NXTA(,ASMVTPTR)                             0190
*     AIANXAIA=0;                   /* ZERO THE NEXT AIA ADDRESS     */
         SLR   @12,@12                                             0191
         ST    @12,AIANXAIA(,AIAPTR)                               0191
*     ASMIORQR=ASMIORQR+1;          /* INCREMENT AIA'S RECIEVED COUNT*/
         LA    @15,1                                               0192
         AL    @15,ASMIORQR(,ASMVTPTR)                             0192
         ST    @15,ASMIORQR(,ASMVTPTR)                             0192
*     IF ASMSTAGF=0 THEN            /* IS STAGEQ EMPTY               */
         C     @12,ASMSTAGF(,ASMVTPTR)                             0193
         BNE   @RF00193                                            0193
*       ASMSTAGF=AIAPTR;            /* SET FIRST POINTER             */
         ST    AIAPTR,ASMSTAGF(,ASMVTPTR)                          0194
*     ELSE                          /* ASMSTAGQ NOT EMPTY            */
*                                                                  0195
*       /*************************************************************/
*       /*                                                           */
*       /* THE ASMSTAGQ IS NOT EMPTY SO WE WILL QUEUE THE CURRENT AIA*/
*       /* TO THE END OF THE QUEUE.                                  */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0195
