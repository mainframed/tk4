*             IF AIAPRIV='1'B×      /* IS THIS A P.A. PAGE           */
*                 AIALSQA='1'B THEN /* NO-IS IT LSQA SWAP PAGE TO GO
*                                      TO PRIVATE AREA DATA SET      */
         TM    AIAPRIV(AIAPTR),B'01001000'                         0293
         BZ    @RF00293                                            0293
*               DO;                                                0294
*                 IOENXT=ASM2PAQ;   /* POINT THIS IOE TO QUEUE       */
         L     @12,ASM2PAQ(,ASMVTPTR)                              0295
         ST    @12,IOENXT(,IOEPTR)                                 0295
*                 ASM2PAQ=IOEPTR;   /* PUT THIS IOE ON TOP OF Q      */
         ST    IOEPTR,ASM2PAQ(,ASMVTPTR)                           0296
*               END;                                               0297
*             ELSE                  /* PUT IT ON C.A. QUEUE          */
*               DO;                                                0298
         B     @RC00293                                            0298
@RF00293 DS    0H                                                  0299
*                 IOENXT=ASM2CAQ;   /* POINT THIS IOE TO QUE         */
         L     @12,ASM2CAQ(,ASMVTPTR)                              0299
         ST    @12,IOENXT(,IOEPTR)                                 0299
*                 ASM2CAQ=IOEPTR;   /* PUT THIS IOE ON TOP OF QUE    */
         ST    IOEPTR,ASM2CAQ(,ASMVTPTR)                           0300
*               END;                                               0301
*             ASM2WFLG='1'B;        /* SET WRITE IOE BUILT FLAG      */
@RC00293 OI    ASM2WFLG(ASMVTPTR),B'01000000'                      0302
*           END;                                                   0303
*                                                                  0304
*/********************************************************************/
*/*                                                                  */
*/* FUNCTION = READ IOE'S ARE CHAINED FROM A PART ENTRY SO THE PART  */
*/*            HAS TO BE INDEXED TO GET TO THE RIGHT ENTRY. IF THERE */
*/*            IS NO ENTRY FOR THE NN VALUE SPECIFIED IN THE LSID THE*/
*/*            AIA IS PUT ON THE PARTAIAE QUEUE AND THE IOE IS FREED.*/
*/*            ALSO IF THE DATA SET IS BAD(PAREDSBD='1'B) THE AIA IS */
*/*            PUT ON THE PARTAIAE QUEUE AND THE IOE IS FREED. ONCE  */
*/*            THE IOE IS CHAINED TO THE PART ENTRY THE IORB'S       */
*/*            CHAINED FROM THAT ENTRY ARE SEARCHED FOR ONE THAT IS  */
*/*            NOT BUSY. IF ONE IS FOUND, THE INTERNAL FLAG(ASM2RFLG)*/
*/*            IS SET TO INDICATE THAT PART MONITOR CAN BE SCHEDULED */
*/*            SINCE USEFUL WORK CAN BE DONE.                        */
*/*                                                                  */
*/* INPUT = ASMVTPTR = ADDRESS OF THE ASMVT                          */
*/*         ATAPTR = ADDRESS OF THE ATA                              */
*/*         AIAPTR = ADDRESS OF THE CURRENT AIA                      */
*/*                                                                  */
*/* EXTERNAL ROUTINES AND REFERENCES = NONE                          */
*/*                                                                  */
*/********************************************************************/
*                                                                  0304
*         ELSE                      /* NOW WRITE-MUST BE READ        */
*           DO;                     /* DO-READ PROCESSING            */
         B     @RC00290                                            0304
@RF00290 DS    0H                                                  0305
*             RFY                                                  0305
*               PARTPTR RSTD;       /* RESTRICT USE OF PARTPTR       */
*             RFY                                                  0306
*               PARTENDX RSTD;      /* RESTRICT USE OF PARTENDX      */
*             PARTENDX=AIALSID;     /* GET THE LSID FROM THE AIA     */
         L     PARTENDX,AIALSID(,AIAPTR)                           0307
*             SLL(PARTENDX,8);      /* SHIFT OUT UNUSED BITS         */
         SLL   PARTENDX,8                                          0308
*             SRL(PARTENDX,24);     /* ISOLATE NN PORTION OF LSID    */
         SRL   PARTENDX,24                                         0309
*             PARTPTR=ASMPART;      /* GET THE ADDRESS OF THE PART   */
         L     PARTPTR,ASMPART(,ASMVTPTR)                          0310
*                                                                  0311
*             /*******************************************************/
