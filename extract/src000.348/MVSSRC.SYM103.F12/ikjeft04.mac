*STAIMSG:                                                       01S0355
*      /**************************************************************/
*      /* THIS PROCEDURE RECEIVES CONTROL FROM TMP OR TEST AFTER A   */
*      /* STAI EXIT. ITS FUNCTION IS TO ESTABLISH ADDRESSABILITY     */
*      /* AND PASS CONTROL TO THE PROCEDURE WHICH SETS UP AND ISSUES */
*      /* THE MESSAGE WHICH INFORMS THE TERMINAL USER OF AN ABEND    */
*      /* CONDITION.                                                 */
*      /* UPON ENTRY TO THIS ROUTINE, REGISTER 1 IS EXPECTED TO      */
*      /* CONTAIN POINTER TO THE 5-WORD PARAMETER LIST WHICH WAS     */
*      /* PLACED IN THE POSTED STAI ECB,TMPIECB, BY THE TMP STAI     */
*      /* EXIT ROUTINE, IKJEFT04.                                    */
*      /**************************************************************/
*                                                               01S0355
*      PROCEDURE                                                01S0355
*        OPTIONS (                                              01S0355
*        DONTSAVE(15),                                          01S0355
*        NOSAVEAREA);                                           01S0355
*                                                               01S0355
         B     @EL00001                                         01S0355
STAIMSG  ST    @14,12(,@13)                                     01S0355
         STM   @00,@12,20(@13)                                  01S0355
*      RESPECIFY (STRTPARM) BASED(R1);                          01S0356
*      RESPECIFY (WRKAPTR1) RESTRICTED;                         01S0357
*      WRKAPTR2 = STRTTMPA;         /* POINTER TO TMP PROTECTED WA   */
         L     WRKAPTR2,STRTTMPA(,R1)                           01S0358
*      WRKAPTR1 = WRKA1PTR;         /* POINTER TO TMP PP WORK AREA   */
         L     WRKAPTR1,WRKA1PTR(,WRKAPTR2)                     01S0359
*      RESPECIFY  (R11,R12) RESTRICTED;                         01S0360
*      R11 = STRTBASE;              /* ADDRESSABILITY TO CSECT       */
         L     R11,STRTBASE(,R1)                                01S0361
*      R12 = STRTDYNA;              /* ADDRESSABILITY TO WORK AREA   */
*                                                               01S0362
         L     R12,STRTDYNA(,R1)                                01S0362
*      CALL CONVTCC;                /* GO TO ISSUE ABEND MESSAGE     */
         BAL   @14,CONVTCC                                      01S0363
*      RETURN;                      /* RETURN TO TMP/TEST            */
@EL00002 DS    0H                                               01S0364
@EF00002 DS    0H                                               01S0364
@ER00002 L     @14,12(,@13)                                     01S0364
         LM    @00,@12,20(@13)                                  01S0364
         BR    @14                                              01S0364
*      END STAIMSG;                                             01S0365
