         TITLE 'IKJEFT04 -- TMP STAI EXIT ROUTINE                      *
                        '
IKJEFT04 CSECT ,                                                01S0001
@MAINENT DS    0H                                               01S0001
         USING *,@15                                            01S0001
         B     @PROLOG                                          01S0001
         DC    AL2(@EP00001-@MAINENT)
         DC    AL1(16)                                          01S0001
         DC    C'IKJEFT04  78.177'                              01S0001
IKJEFT0D DS    0H                                               01S0001
         USING *,@15                                            01S0001
         B     @PROLOG                                          01S0001
         DC    AL2(@EP00101-IKJEFT0D)
         ENTRY IKJEFT0D
         DROP  @15
@PROLOG  AH    @15,4(,@15)                                      01S0001
         BR    @15                                              01S0001
@EP00001 DS    0H                                               01S0002
         USING   IKJEFT04,R15
*                                                               01S0074
         USING *,@15
*        /************************************************************/
*        /*   CHECK THE CODE IN REGISTER 0.  IF IT IS 12 NO SYSTEM   */
*        /*   SAVE AREA (POINTER IN REGISTER 13) HAS BEEN PROVIDED   */
*        /*   BY THE STAE PROCESSOR.  A SIMULATED SYSTEM SAVE AREA   */
*        /*   IS SUPPLIED AND REGISTER 13 SET AS POINTER             */
*        /************************************************************/
*                                                               01S0075
*        IF R0 = STAIWA             /* CHK IF WORK AREA SUPPLIED     */
*          THEN                     /* NO,                           */
         C     R0,@CF00035                                      01S0075
         BNE   @RF00075                                         01S0075
*            DO;                                                01S0076
*              R0 = R2;             /* RESERVE ENTRY REG 2           */
         LR    R0,R2                                            01S0077
*              GEN(L   R2,8(R2));   /* PTR TO ENTRY SAVE AREA        */
         L   R2,8(R2)
*              GEN(STM   R14,R12,12(R2));  /* SAVE ENTRY REGS        */
         STM   R14,R12,12(R2)
*              R13 = R2;            /* SET SIMULATED EP SAVE PTR     */
         LR    R13,R2                                           01S0080
*              RESPECIFY (SAVARY) BASED(R13);                   01S0081
*              SR13BAK = R13;       /* INDICATE END OF CHAIN         */
         ST    R13,SR13BAK(,R13)                                01S0082
*              WRKA2PTR = R0;       /* SET PTR TO PROTECTED WA       */
         ST    R0,WRKA2PTR(,WRKAPTR2)                           01S0083
*              REG2 = R0;           /* RESTORE ENTRY REG 2           */
         ST    R0,REG2(,R13)                                    01S0084
*              R0 = STAIWA;         /* SET ENTRY NO-SDWA FLAG        */
         LA    R0,12                                            01S0085
*              REG0 = R0;           /* RESTORE ENTRY REG 0           */
         ST    R0,REG0(,R13)                                    01S0086
*              R6 = R1;             /* RESERVE ABEND CODE FROM REG1  */
         LR    R6,R1                                            01S0087
*            END;                                               01S0088
*          ELSE                     /* YES, STAI WORK AREA SUPPLIED  */
*            DO;                                                01S0089
         B     @RC00075                                         01S0089
@RF00075 DS    0H                                               01S0090
*              RESPECIFY (SDWA) BASED(REG1);                    01S0090
*              GEN(STM    R14,R12,12(R13));  /* SAVE ENTRY REGS      */
         STM    R14,R12,12(R13)
*              R0 = R0&&R0;         /* SET NON-NO-SDWA FLAG          */
         XR    R0,R0                                            01S0092
*              WRKAPTR2 = SDWAPARM; /* SET TMPWA PTR (8) VIA PL      */
         L     @14,REG1(,R13)                                   01S0093
         L     WRKAPTR2,SDWAPARM(,@14)                          01S0093
*              R6 = SDWAABCC;       /* RESERVE ABEND CODE            */
         L     R6,SDWAABCC(,@14)                                01S0094
*            END;                                               01S0095
*                                                               01S0095
*        /* ESTABLISH CODE ADDRESABILITY                             */
*        R11 = T0DEP;               /* SET CODE ADDRESSABILITY       */
@RC00075 L     R11,T0DEP                                        01S0096
*        GEN(DROP  @15);                                        01S0097
         DROP  @15
*        GEN(USING IKJEFT04,R11);                               01S0098
*                                                               01S0098
         USING IKJEFT04,R11
*        /* SET TO PROBLEM PROGRAM STATE AND KEY                     */
*        GEN(MODESET    KEY=NZERO,MODE=PROB);                   01S0099
*                                                               01S0099
*                                                               01S0099
         MODESET    KEY=NZERO,MODE=PROB
*        GO TO T4MEET;              /* START COMMON PATH             */
         B     T4MEET                                           01S0100
*IKJEFT0D:  /* EP FROM TEST-ATTACHED-CP ABEND  (PP STATE AND KEY)    */
*   ENTRY;                                                      01S0101
*                                                               01S0101
@EP00101 DS    0H                                               01S0102
*        GEN(USING *,@15);                                      01S0102
*                                                               01S0102
         USING *,@15
*        /************************************************************/
*        /*   CHECK THE CODE IN REGISTER 0.  IF IT IS 12 NO SYSTEM   */
*        /*   SAVE AREA (POINTER IN REGISTER 13) HAS BEEN PROVIDED   */
*        /*   BY THE STAE PROCESSOR.  A SIMULATED SYSTEM SAVE AREA   */
*        /*   IS SUPPLIED AND REGISTER 13 SET AS POINTER             */
*        /************************************************************/
*                                                               01S0103
*        IF R0 = STAIWA             /* CHK IF WORK AREA SUPPLIED     */
*          THEN                     /* NO,                           */
         C     R0,@CF00035                                      01S0103
         BNE   @RF00103                                         01S0103
*            DO;                                                01S0104
*              R0 = R2;             /* RESERVE ENTRY REG 2           */
         LR    R0,R2                                            01S0105
*              GEN(L   R2,8(R2));   /* PTR TO ENTRY SAVE AREA        */
         L   R2,8(R2)
*              GEN(STM   R14,R12,12(R2));  /* SAVE ENTRY REGS        */
         STM   R14,R12,12(R2)
*              R13 = R2;            /* SET SIMULATED EP SAVE PTR     */
         LR    R13,R2                                           01S0108
*              RESPECIFY (SAVARY) BASED(R13);                   01S0109
*              SR13BAK = R13;       /* INDICATE END OF CHAIN         */
         ST    R13,SR13BAK(,R13)                                01S0110
*              WRKA2PTR = R0;       /* SET PTR TO PROTECTED WA       */
         ST    R0,WRKA2PTR(,WRKAPTR2)                           01S0111
*              REG2 = R0;           /* RESTORE ENTRY REG 2           */
         ST    R0,REG2(,R13)                                    01S0112
*              R0 = STAIWA;         /* SET ENTRY NO-SDWA FLAG        */
         LA    R0,12                                            01S0113
*              REG0 = R0;           /* RESTORE ENTRY REG 0           */
         ST    R0,REG0(,R13)                                    01S0114
*              R6 = R1;             /* RESERVE ABEND CODE FROM REG1  */
         LR    R6,R1                                            01S0115
*            END;                                               01S0116
*          ELSE                     /* YES, STAI WORK AREA SUPPLIED  */
*            DO;                                                01S0117
         B     @RC00103                                         01S0117
@RF00103 DS    0H                                               01S0118
*              RESPECIFY (SDWA) BASED(REG1);                    01S0118
*              GEN(STM    R14,R12,12(R13));  /* SAVE ENTRY REGS      */
         STM    R14,R12,12(R13)
*              R0 = R0&&R0;         /* SET NON-NO-SDWA FLAG          */
         XR    R0,R0                                            01S0120
*              WRKAPTR2 = SDWAPARM; /* SET TMPWA PTR (8) VIA PL      */
         L     @14,REG1(,R13)                                   01S0121
         L     WRKAPTR2,SDWAPARM(,@14)                          01S0121
*              R6 = SDWAABCC;       /* RESERVE ABEND CODE            */
         L     R6,SDWAABCC(,@14)                                01S0122
*            END;                                               01S0123
*                                                               01S0123
*        /* ESTABLISH CODE ADDRESSABILITY                            */
*        RESPECIFY (R11) RESTRICTED;                            01S0124
@RC00103 DS    0H                                               01S0125
*        R11 = T0DEP;               /* SET CODE ADDRESSABILITY       */
         L     R11,T0DEP                                        01S0125
*        GEN(DROP  R15);                                        01S0126
         DROP  R15
*        GEN(USING   IKJEFT04,R11);                             01S0127
         USING   IKJEFT04,R11
*        RESPECIFY (PARMPTR) UNRESTRICTED;                      01S0128
*    T4MEET:   /* START OF COMMON PATH                               */
*                                                               01S0129
*        WRKAPTR1 = WRKA1PTR;       /* SET PTR TO NONPROTECTED WA    */
*                                                               01S0129
T4MEET   L     WRKAPTR1,WRKA1PTR(,WRKAPTR2)                     01S0129
*        /* ESTABLISH SAVE AREA ADDRESSABILITY                       */
*        SVT041A=SVT041A&&SVT041A;  /* CLEAR NEW SAVE AREA           */
         L     @14,TMPWA2P(,WRKAPTR2)                           01S0130
         XC    SVT041A(72,@14),SVT041A(@14)                     01S0130
*        SAVT041(2) = R13;          /* RESERVE CURRENT BACKCHAIN PTR */
         ST    R13,SAVT041+4(,@14)                              01S0131
*        R15 = ADDR(SAVT041);       /* RESERVE CURRENT SAVE AREA PTR */
         LA    R15,SAVT041(,@14)                                01S0132
*        SR13FOR = R15;             /* SET OLD FORWARD CHAIN PTR     */
         ST    R15,SR13FOR(,R13)                                01S0133
*        R13 = R15;                 /* SET CURRENT SAVE AREA PTR     */
         LR    R13,R15                                          01S0134
*        RESPECIFY (SAVARY) BASED(SAVRBAK);                     01S0135
*                                                               01S0135
*        /* SET UP DATA FOR ENQ/DEQ                                  */
*        MINQNAM = PSCBUSER;        /* INITIALIZE MINOR NAME         */
         MVI   MINQNAM+7(@14),C' '                              01S0136
         L     @12,PSCBPTR(,WRKAPTR2)                           01S0136
         MVC   MINQNAM(7,@14),PSCBUSER(@12)                     01S0136
*        RESPECIFY (R2,R15) UNRSTD;                             01S0137
*                                                               01S0137
*        IF TPLCTCB = TPLNTCB       /* CHK IF PRIME ABEND            */
*          THEN                     /* YES,                          */
         CLC   TPLCTCB(4,WRKAPTR1),TPLNTCB(WRKAPTR1)            01S0138
         BNE   @RF00138                                         01S0138
*            FRSTLAB = YES;         /* SET PRIME LEVEL ABEND FLAG    */
*                                                               01S0139
         OI    FRSTLAB(WRKAPTR1),B'00000010'                    01S0139
*        ECTRTCD = SDWACMPC;        /* ABEND CODE FOR CLIST  Z30NQKM */
@RF00138 L     @12,ECTPTR(,WRKAPTR2)                            01S0140
         L     @10,SAVRBAK(,R13)                                01S0140
         L     @10,REG1(,@10)                                   01S0140
         MVC   ECTRTCD(3,@12),SDWACMPC(@10)                     01S0140
*        ECTRCDF = ECTRCDF×HIORDR;  /* ABEND FLAG FOR CLIST  Z30NQKM */
*                                                               01S0141
         OI    ECTRCDF(@12),X'80'                               01S0141
*        RESPECIFY (R10,R12,R15) RESTRICTED;                    01S0142
*        RESPECIFY (ABMSGAR) BASED(R10);                        01S0143
*        GEN(USING T04WA,R1);                                   01S0144
*                                                               01S0144
         USING T04WA,R1
*        DO; /*GETMAIN (RC) LV(LENGTH(T04WA)) SP(SPNUM) RTCD(R15)*/
*        RESPECIFY (GPR01F,GPR15F,GPR00F) RESTRICTED;           01S0146
*        GPR01F = 0; /* REG 1 MUST BE ZERO*/                    01S0147
         SLR   GPR01F,GPR01F                                    01S0147
*        GPR15F = 0; /* RC-TYPE GETMAIN */                      01S0148
         SLR   GPR15F,GPR15F                                    01S0148
*        GPR00F = LENGTH(T04WA); /* LENGTH REQUESTED*/          01S0149
         LA    GPR00F,224                                       01S0149
*        GPR15F = GPR15F×((SPNUM)*256); /* SP IN BYTE 2 */      01S0150
         O     GPR15F,@CF01199                                  01S0150
*        SVC(120); /*RC/RU-FORM OF GETMAIN*/                    01S0151
         SVC   120                                              01S0151
*        R15 = GPR15F; /* SET RETURN CODE */                    01S0152
*        RESPECIFY (GPR01F,GPR15F,GPR00F) UNRESTRICTED;         01S0153
*        END; /*GETMAIN (RC) LV(LENGTH(T04WA)) SP(SPNUM) RTCD(R15)*/
*                                                               01S0154
*        IF R15 = ZERO              /* CHK FOR SUCCESSFUL GETMAIN    */
*            & REG0 ^= STAIWA       /*    & SDWA PRESENT             */
*          THEN DO;                 /* YES,                          */
         LTR   R15,R15                                          01S0155
         BNZ   @RF00155                                         01S0155
         L     @14,SAVRBAK(,R13)                                01S0155
         CLC   REG0(4,@14),@CF00035                             01S0155
         BE    @RF00155                                         01S0155
*            GEN(USING     @DATD,R12);                          01S0157
         USING     @DATD,R12
*            R12 = R1;              /* SET REENTRANT ADDRESSABILITY  */
         LR    R12,R1                                           01S0158
*            GEN(USING   T04WA,R12);                            01S0159
         USING   T04WA,R12
*            SVT043A = SVT041A;     /* SET NEW RENT SAVE AREA        */
         L     @14,TMPWA2P(,WRKAPTR2)                           01S0160
         MVC   SVT043A(72),SVT041A(@14)                         01S0160
*            R13 = ADDR(SAVT043);   /* SET CURRENT SAVE AREA PTR     */
         LA    R13,SAVT043                                      01S0161
*            SR13FOR = R13;         /* SET OLD FORWARD CHAIN PTR     */
         L     @14,SAVRBAK(,R13)                                01S0162
         ST    R13,SR13FOR(,@14)                                01S0162
*            R10 = ADDR(ABMSG4);    /* PTR TO TO4WA MESSAGE AREA     */
         LA    R10,ABMSG4                                       01S0163
*          END;                                                 01S0164
*          ELSE                     /* GETMAIN FAILED,               */
*            R10 = ADDR(ABMSGSP);   /* TO TMPWA                      */
*                                                               01S0165
         B     @RC00155                                         01S0165
@RF00155 LA    R10,ABMSGSP(,WRKAPTR1)                           01S0165
*        /* IF ECT ABNORMAL TERMINATION BIT IS ON, EXIT DIRECTLY     */
*        IF ECTATRM = '1'B          /* TEST ABNORMAL TERMINATION BIT */
*          THEN                     /* ON,                           */
@RC00155 L     @14,ECTPTR(,WRKAPTR2)                            01S0166
         TM    ECTATRM(@14),B'00100000'                         01S0166
         BO    @RT00166                                         01S0166
*            GO TO WAKEUP;          /* GO TO EXIT                    */
*        RESPECIFY (R7) RESTRICTED;                             01S0168
*        IF SUBCODE > MAXCDE        /* CHK IF REASON CODE > FFF      */
*          THEN                     /* YES,                          */
         L     @14,SAVRBAK(,R13)                                01S0169
         L     @14,REG1(,@14)                                   01S0169
         CLC   SUBCODE(4,@14),@CF00090                          01S0169
         BNH   @RF00169                                         01S0169
*            SUBCDRS = ZERO;        /*   NOT SIGNIFICANT CODE        */
         SLR   @14,@14                                          01S0170
         ST    @14,SUBCDRS(,R10)                                01S0170
*          ELSE                     /* NO, IS POSSIBLE CODE          */
*            SUBCDRS = SUBCODE;     /* ERROR SUBCODE TO WORK AREA    */
         B     @RC00169                                         01S0171
@RF00169 L     @14,SAVRBAK(,R13)                                01S0171
         L     @14,REG1(,@14)                                   01S0171
         L     @14,SUBCODE(,@14)                                01S0171
         ST    @14,SUBCDRS(,R10)                                01S0171
*        ABNDCD = R6;               /* REGISTER 6 TO ABEND CODE      */
@RC00169 ST    R6,ABNDCD(,R10)                                  01S0172
*        GEN(SLL    R6,4);          /* DROP HIGH ORDER               */
         SLL    R6,4
*        GEN(SRDL   R6,16);         /* SYSTEM ABEND CODE INTO REG 6  */
         SRDL   R6,16
*        GEN(SRL    R7,20);         /* SET USER ABEND CODE IN REG 7  */
         SRL    R7,20
*        NONSCUR = NO;              /* RESET SECURITY FAILURE FLAG   */
         NI    NONSCUR(WRKAPTR1),B'11111110'                    01S0176
*        IF (R6 = CD913             /* CHK FOR OPEN FAILURE          */
*            &(SUBCODE = CD38                                   01S0177
*             ×SUBCODE = CD3C                                   01S0177
*             ×SUBCODE = CD40))                                 01S0177
*          ×(R6 = CD937             /* CHK FOR EOV FAILURE           */
*            &(SUBCODE = CD28                                   01S0177
*             ×SUBCODE = CD2C                                   01S0177
*             ×SUBCODE = CD30                                   01S0177
*             ×SUBCODE = CD34                                   01S0177
*             ×SUBCODE = CD38                                   01S0177
*             ×SUBCODE = CD3C))                                 01S0177
*          THEN                     /* YES, IS SECURITY FAILURE      */
         C     R6,@CF00065                                      01S0177
         BNE   @GL00002                                         01S0177
         L     @14,SAVRBAK(,R13)                                01S0177
         L     @14,REG1(,@14)                                   01S0177
         L     @14,SUBCODE(,@14)                                01S0177
         C     @14,@CF00059                                     01S0177
         BE    @RT00177                                         01S0177
         C     @14,@CF00061                                     01S0177
         BE    @RT00177                                         01S0177
         C     @14,@CF00063                                     01S0177
         BE    @RT00177                                         01S0177
@GL00002 C     R6,@CF00067                                      01S0177
         BNE   @RF00177                                         01S0177
         L     @14,SAVRBAK(,R13)                                01S0177
         L     @14,REG1(,@14)                                   01S0177
         L     @14,SUBCODE(,@14)                                01S0177
         C     @14,@CF00051                                     01S0177
         BE    @RT00177                                         01S0177
         C     @14,@CF00053                                     01S0177
         BE    @RT00177                                         01S0177
         C     @14,@CF00055                                     01S0177
         BE    @RT00177                                         01S0177
         C     @14,@CF00057                                     01S0177
         BE    @RT00177                                         01S0177
         C     @14,@CF00059                                     01S0177
         BE    @RT00177                                         01S0177
         C     @14,@CF00061                                     01S0177
         BNE   @RF00177                                         01S0177
@RT00177 DS    0H                                               01S0178
*            NONSCUR = YES;         /*   SET SECURITY FAILURE FLAG   */
*                                                               01S0178
         OI    NONSCUR(WRKAPTR1),B'00000001'                    01S0178
*        IF R6 = IOERR              /* CHK FOR 806 ABEND AND         */
*            & REG15 = D4           /*    IF NAME NOT FOUND          */
*            & ATCHNOW = YES        /*    IF FUNCTION WAS ATTACH     */
*            & REG0 ^= STAIWA       /*    IF STAI WORK AREA SUPPLIED */
*          THEN                     /* YES,                          */
@RF00177 C     R6,@CF00087                                      01S0179
         BNE   @RF00179                                         01S0179
         L     @14,SAVRBAK(,R13)                                01S0179
         CLC   REG15(4,@14),@CF00069                            01S0179
         BNE   @RF00179                                         01S0179
         TM    ATCHNOW(WRKAPTR1),B'10000000'                    01S0179
         BNO   @RF00179                                         01S0179
         CLC   REG0(4,@14),@CF00035                             01S0179
         BE    @RF00179                                         01S0179
*            IF ECTPCMD=SDWANAME    /* CHK IF ATTACH WAS INITIATED   */
*                × ECTPCMD=TABNAME  /*     BY TMP                    */
*              THEN                 /* YES,                          */
         L     @03,ECTPTR(,WRKAPTR2)                            01S0180
         L     @14,REG1(,@14)                                   01S0180
         CLC   ECTPCMD(8,@03),SDWANAME(@14)                     01S0180
         BE    @RT00180                                         01S0180
         L     @14,SDWARBAD(,@14)                               01S0180
         CLC   ECTPCMD(8,@03),TABNAME(@14)                      01S0180
         BNE   @RF00180                                         01S0180
@RT00180 DS    0H                                               01S0181
*                ABND806 = YES;     /* SET COMMAND-NOT-FOUND         */
         OI    ABND806(WRKAPTR1),B'00000100'                    01S0181
*        IF ABND806 = NO            /* CHK FOR COMMAND-NOT-FOUND     */
*          THEN                     /* NOT ATTACH-806,               */
@RF00180 DS    0H                                               01S0182
@RF00179 TM    ABND806(WRKAPTR1),B'00000100'                    01S0182
         BNZ   @RF00182                                         01S0182
*            DO;                    /* INDICATE ERROR                */
*              IF R7 = ZERO         /* CHK IF ANY USER CODE          */
*                THEN DO;           /* NO,                           */
         LTR   R7,R7                                            01S0184
         BNZ   @RF00184                                         01S0184
*                  HOLCHAR = SCHAR; /* RESERVE SYSTEM FLAG           */
         MVI   HOLCHAR(R10),C'S'                                01S0186
*                  UNPK  (CODEAR,SYSCDAR);  /* PREPARE TO TRANSLATE  */
         UNPK  CODEAR(6,R10),SYSCDAR(3,R10)                     01S0187
*                  EBCDPTR=ADDR(XLATAB)-D240;  /* FOR TRANSLATION    */
         LA    @14,XLATAB                                       01S0188
         SL    @14,@CF00080                                     01S0188
         ST    @14,EBCDPTR(,WRKAPTR1)                           01S0188
*                  TR  (VALAR,EBCDTAB);  /* GET PRINTABLE CHARS S    */
         TR    VALAR(3,R10),EBCDTAB(@14)                        01S0189
*                  VALAR6(4) = CBLNK;    /* SET TERMINAL BLANK       */
         MVI   VALAR6+3(R10),C' '                               01S0190
*                  UNPK  (SUBCAR,SUBCDBI);  /* PREPARE TO TRANSLATE  */
         UNPK  SUBCAR(6,R10),SUBCDBI(3,R10)                     01S0191
*                  EBCDPTR=ADDR(XLATAB)-D240;  /* PTR TO TRANS TABLE */
         ST    @14,EBCDPTR(,WRKAPTR1)                           01S0192
*                  TR  (SUBCTXT,EBCDTAB);   /* GET PRINTABLE CHARS   */
         TR    SUBCTXT(3,R10),EBCDTAB(@14)                      01S0193
*                  SUBCLEN = RCDELEN;  /* LEN OF REASON CODE SEGMENT */
         MVC   SUBCLEN(2,R10),@CH00033                          01S0194
*                  SUBCOFF = RCDEOFF;  /* OFFSET FOR REASON SEGMENT  */
         MVC   SUBCOFF(2,R10),@CH00113                          01S0195
*                END;                                           01S0196
*                ELSE DO;           /* IS USER CODE                  */
         B     @RC00184                                         01S0197
@RF00184 DS    0H                                               01S0198
*                  HOLCHAR = UCHAR; /* RESERVE USER FLAG             */
         MVI   HOLCHAR(R10),C'U'                                01S0198
*                  RESPECIFY (R2,R3) RESTRICTED;                01S0199
*                  CVD  (USRCDBI,CVDAR);  /* OBTAIN DECIMAL VALUE    */
         LH    @14,USRCDBI(,R10)                                01S0200
         CVD   @14,CVDAR(,R10)                                  01S0200
*                  UNPK (VALAR6,USRCDEC); /* PREPARE TO PRINT        */
         UNPK  VALAR6(6,R10),USRCDEC(4,R10)                     01S0201
*                END;                                           01S0202
*              RESPECIFY (R7) UNRSTD;                           01S0203
*                                                               01S0203
@RC00184 DS    0H                                               01S0204
*                                                               01S0204
*              IF R15 ^= ZERO       /* CHK FOR GETMAIN FAILURE       */
*                  × REG0 = STAIWA  /* CHK FOR ERROR MESSAGE FLAG    */
*                THEN DO;           /* YES,                          */
         LTR   R15,R15                                          01S0204
         BNZ   @RT00204                                         01S0204
         L     @14,SAVRBAK(,R13)                                01S0204
         CLC   REG0(4,@14),@CF00035                             01S0204
         BNE   @RF00204                                         01S0204
@RT00204 DS    0H                                               01S0205
*                  RESPECIFY (R15) UNRESTRICTED;                01S0206
*                  TYPAR=HOLCHAR;   /* INDICATE TYPE OF ABEND        */
         MVC   TYPAR(1,R10),HOLCHAR(R10)                        01S0207
*                  MSGTXT=M604BAS;  /* INSERT MESSAGE TEXT           */
         L     @07,@CA00170                                     01S0208
         MVC   MSGTXT(16,R10),M604BAS-MSG56604(@07)             01S0208
*                  IF BKGMODE=YES   /* CHK FOR FOREGROUND TASK       */
*                    THEN DO;       /* NO, BACKGROUND TASK           */
