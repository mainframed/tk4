*/* THIS VALIDITY CHECKING ROUTINE IS ENTERED BY PARSE WHENEVER      */
*/* THE TERMINAL USER ENTERS THE 'LINE' KEYWORD AND A CHARACTER.     */
*/* ANY CHARACTER IS VALID WITH THE EXCEPTION OF THE NEW LINE        */
*/* (NL) CHARACTER.THE CHARACTERS, 'ATTN' INDICATES THAT             */
*/* THE USER WANTS AN ATTENTION TO BE USED AS THE LINE DELETE        */
*/* CHARACTER.  THE CHARACTERS 'CTLX' INDICATE THAT THE USER WANTS   */
*/* X AND CTRL KEYS (DEPRESSED TOGETHER) ON A TELETYPE TERMINAL      */
*/* TO BE USED AS THE LINE DELETE CHARACTER.                         */
*/********************************************************************/
*LINECHEK:                      /*************************************/
*        PROCEDURE                                                 0518
*      OPTIONS(DONTSAVE(15),NOSAVEAREA);                           0518
LINECHEK ST    @14,12(,@13)                                        0518
         STM   @00,@12,20(@13)                                     0518
*     DECLARE                                                      0519
*        DELLINE CHAR(4) BASED(CHARPTR);/* LINE DELETE CHARACTER     */
*        RFY (R3)RSTD;         /* REGISTER 3 IS TO BE USED Z40DQJD  */
*        /* ESTABLISH ADDRESSABILITY IN THIS ROUTINE                 */
*        GENERATE;                                                 0521
              L   3,4(1)             GET PTR TO PROFILE'S SAVE AREA
        LM   11,12,64(3)       ESTABLISH ADDRESSABILITY
        L    3,0(1)            GET PDE POINTER
*        PDEADDR=R3;            /* INITIALIZE PDE PTR                */
         ST    R3,PDEADDR                                          0522
*        RFY (R3)UNRSTD;                  /*                Z40DQJD*/
*        IF CHARLEN=1           /* LENGTH OF CHARACTER = 1           */
*          THEN                 /* CHARACTER IS VALID UNLESS IS 'NL' */
         L     @10,PDEADDR                                         0524
         CLC   CHARLEN(2,@10),@CH00054                             0524
         BNE   @RF00524                                            0524
*            DO;                                                   0525
*              IF DELLINE(1)='15'X  /* 'NL' SPECIFIED                */
*                THEN                                              0526
         L     @10,CHARPTR(,@10)                                   0526
         CLI   DELLINE(@10),X'15'                                  0526
         BE    @RT00526                                            0526
*                  GO TO FINI;  /* GO TO RETURN TO PARSE             */
*              R15=0;           /* SET VALID RETURN CODE             */
         SLR   R15,R15                                             0528
*              RETURN;          /* RETURN TO PARSE                   */
@EL00004 DS    0H                                                  0529
@EF00004 DS    0H                                                  0529
@ER00004 L     @14,12(,@13)                                        0529
         LM    @00,@12,20(@13)                                     0529
         BR    @14                                                 0529
*            END;                                                  0530
*        IF CHARLEN=4           /* IS LENGTH OF INPUT=4?             */
*          THEN                                                    0531
@RF00524 L     @10,PDEADDR                                         0531
         CLC   CHARLEN(2,@10),@CH00045                             0531
         BNE   @RF00531                                            0531
*            DO;                                                   0532
*              TEMPCHAR(1:4)=DELLINE  '40404040'X; /*MAKE UPPER CASE*/
         L     @10,CHARPTR(,@10)                                   0533
         MVC   TEMPCHAR(4),DELLINE(@10)                            0533
         OC    TEMPCHAR(4),@CB01639                                0533
*              IF TEMPCHAR(1:4)='CTLX'  /* TELETYPE DELETE CHARACTER?*/
*                THEN                                              0534
         CLC   TEMPCHAR(4),@CC01584                                0534
         BNE   @RF00534                                            0534
*                  DO;                                             0535
*                    LINDELET='18'X; /*TELETYPE LINE DELETE CHARACTER*/
         MVI   LINDELET,X'18'                                      0536
*                    GO TO FINISH;                                 0537
         B     FINISH                                              0537
*                  END;                                            0538
*              LINDELET='00'X;       /* NO TELETYPE CHARACTER        */
@RF00534 MVI   LINDELET,X'00'                                      0539
*              IF TEMPCHAR(1:4)='ATTN' /*WAS INPUT ATTENTION        **/
*                THEN           /* 'ATTN', ATTENTION WAS SPECIFIED   */
         CLC   TEMPCHAR(4),@CC01583                                0540
         BNE   @RF00540                                            0540
*                  DO;                                             0541
*FINISH:                                                           0542
*                    R15=0;     /* SET VALID RETURN CODE            **/
FINISH   SLR   R15,R15                                             0542
*                    RETURN;    /* RETURN TO PARSE                   */
         B     @EL00004                                            0543
*                  END;                                            0544
*            END;                                                  0545
@RF00540 DS    0H                                                  0546
*FINI:                          /* CODE IMPLIES MESSAGE,PROMPT       */
*        R15=4;                 /* SET INVALID RETURN CODE          **/
@RF00531 DS    0H                                                  0546
FINI     LA    R15,4                                               0546
