         TM    ECTNOPD(@07),B'10000000'                            0084
         BO    @RT00084                                            0084
*              GO TO DEFLIST;   /* DEFAULT TO LIST           @Z40NQKH*/
*        /************************************************************/
*        /* INITIALIZE POINTERS FOR PARSE (IKJPARS)                  */
*        /************************************************************/
*        PPLPTR=ADDR(PPLWA);    /* INITIALIZE PARAMETER LIST         */
         LA    @14,PPLWA                                           0086
         ST    @14,PPLPTR                                          0086
*        PPLCBUF=CPPLCBUF;      /* COMMAND BUFFER PTR                */
         MVC   PPLCBUF(4,@14),CPPLCBUF(@10)                        0087
*        PPLUPT=CPPLUPT;                                           0088
         ST    @15,PPLUPT(,@14)                                    0088
*        PPLECT=CPPLECT;                                           0089
         ST    @07,PPLECT(,@14)                                    0089
*        PPLECB=ADDR(CPECB);                                       0090
         LA    @10,CPECB                                           0090
         ST    @10,PPLECB(,@14)                                    0090
*        PPLPCL=ADDR(PARSEPLS); /* INITIALIZE PCL PTR                */
         L     @10,@CV01531                                        0091
         ST    @10,PPLPCL(,@14)                                    0091
*        PPLANS=ADDR(PDEPTR);   /* GET  PARSE'S ANSWER PLACE PTR     */
         LA    @10,PDEPTR                                          0092
         ST    @10,PPLANS(,@14)                                    0092
*        PPLUWA=R13;            /* PTR TO PROFILE SAVE AREA          */
         ST    R13,PPLUWA(,@14)                                    0093
