         LR    R1,@14                                              0094
*        /************************************************************/
*        /* GO TO PARSE TO VALIDATE THE OPERANDS                     */
*        /************************************************************/
*        GENERATE(CALLTSSR EP=IKJPARS);/* LINK TO IKJPARS            */
         CALLTSSR EP=IKJPARS
*        RCODE=R15;             /* GET THE RETURN CODE FROM PARSE    */
         ST    R15,RCODE                                           0096
*        IF R15^=0              /* CHECK FOR SUCCESS                 */
*          THEN                 /* PARSE WAS UNSUCCESSFUL            */
         LTR   R15,R15                                             0097
         BZ    @RF00097                                            0097
*            /********************************************************/
*            /* SET UP MULTI-LEVEL MESSAGE LIST AND CONVERT CODE     */
*            /********************************************************/
*            DO;                                                   0098
*FIBMESS:                       /*                             Y01676*/
*              IF FIBBIT = '1'B THEN/* FROM SVC 100?           Y01676*/
FIBMESS  TM    FIBBIT,B'10000000'                                  0099
         BO    @RT00099                                            0099
*                GOTO FIBMESS2;     /* YES-ISSUE DIFFERENT MSG Y01676*/
*              IF RCODE=4                                          0101
*                THEN                                              0101
         L     @10,RCODE                                           0101
         CH    @10,@CH00045                                        0101
         BE    @RT00101                                            0101
*                  GO TO RET12; /* RETURN-UNSUCCESSFUL RETURN CODE=12*/
*              RFY (R5)RSTD;   /*                            Z40DQJD*/
*              R5=RCODE;        /*PREPARE FOR CVD              M05017*/
         LR    R5,@10                                              0104
*              GEN;             /*                             M05017*/
              CVD   R5,PACKDEC   CONVERT RETURN CODE TO DEC   M05017
              UNPK  CMESSAG(8),PACKDEC(8)  UNPACK CODE        M05017
              OI    CMESSAG+7,X'F0'  FIX SIGN FOR PRINTING    M05017
*              RFY (R5)UNRSTD;  /*                            Z40DQJD*/
*                                                                  0106
*              CODEML=8;        /* GET CODE LENGTH                   */
         MVC   CODEML(2),@CH00069                                  0107
*              CODEOFFP=26;     /* GET CODE OFFSET                   */
         MVC   CODEOFFP(2),@CH01472                                0108
*              ERCODEM(1)=' ';  /* BLANK THE FIRST CHARACTER         */
         MVI   ERCODEM,C' '                                        0109
*FIBMESS2:                      /*                             Y01676*/
*              SEGMENT=ADDR(INVPARMM);/* GET FIRST LEVEL MSG PTR     */
FIBMESS2 L     @10,@CV01447                                        0110
         LA    @10,INVPARMM(,@10)                                  0110
         ST    @10,SEGMENT                                         0110
*              IF FIBBIT = '1'B THEN  /* GET SECOND LEVEL PTR  Y01676*/
         TM    FIBBIT,B'10000000'                                  0111
         BNO   @RF00111                                            0111
*                SEGMENT2=ADDR(FIBMSGLN);/*                    Y01676*/
         L     @10,@CV01447                                        0112
         LA    @10,FIBMSGLN(,@10)                                  0112
         ST    @10,SEGMENT2                                        0112
*              ELSE                                                0113
*              SEGMENT2=ADDR(ERRCODEM);/* GET 2ND LEVEL MSG PTR      */
         B     @RC00111                                            0113
@RF00111 L     @10,@CV01447                                        0113
         LA    @10,ERRCODEM(,@10)                                  0113
         ST    @10,SEGMENT2                                        0113
*              SEGMENT3=ADDR(CMESSAG); /* GET CODE MESSAGE ADDRESS   */
@RC00111 LA    @10,CMESSAG                                         0114
         ST    @10,SEGMENT3                                        0114
*              IF FIBBIT='1'B THEN     /* NUMBER OF SEGMENTS   Y01676*/
         TM    FIBBIT,B'10000000'                                  0115
         BNO   @RF00115                                            0115
*                NUMSEG2=1;            /*                      Y01676*/
         MVC   NUMSEG2(4),@CF00054                                 0116
*              ELSE                    /*                      Y01676*/
*              NUMSEG2=2;       /* GET NUMBER MESSAGE SEGMENTS      **/
         B     @RC00115                                            0117
@RF00115 MVC   NUMSEG2(4),@CF00050                                 0117
*              LEVELNP=NONEXTLM;/* INDICATE NO NEXT LEVEL MSG       **/
@RC00115 MVC   LEVELNP(4),@CF01368                                 0118
*              LEVEL2P=ADDR(LEVELNP);/* GET NEXT LEVEL MSG PTR       */
         LA    @10,LEVELNP                                         0119
         ST    @10,LEVEL2P                                         0119
*              CALL PUTMSGS;    /* PUT OUT PARSE ERROR MESSAGE       */
         BAL   @14,PUTMSGS                                         0120
*              IF FIBBIT = '1'B THEN   /*IF NOT FROM PARSE     Y01676*/
         TM    FIBBIT,B'10000000'                                  0121
         BO    @RT00121                                            0121
*                GOTO CHKINVM2; /* CONTINUE NORMAL PROCESSING  Y01676*/
*              GO TO RET12;     /* GO TO RETURN TO TMP-RETURN CODE=12*/
         B     RET12                                               0123
*            END;               /* END CHEK FOR UNSUCCESSFUL PARSE   */
*        /************************************************************/
*        /* CHECK THE PARAMETER DESCRIPTOR LIST FROM PARSE           */
*        /* SERVICE ROUTINE. IF NO VALID OPERANDS HAVE BEEN          */
*        /* ENTERED, ISSUE WARNING MESSAGE.                          */
*        /************************************************************/
*        IF PRMT=0 &           /*IF NO VALID OPERANDS WERE     Y01156*/
*           MSGID=0 &          /*SPECIFIED ON PROFILE COMMAND  Y01156*/
*           INTERCOM=0 &                                     /*Y01156*/
*           PAUSE=0 &                                        /*Y01156*/
*           CHART=0 &                                        /*Y01156*/
*           LINE=0 &                                         /*Y01156*/
*           LIST=0 &                                         /*Y01676*/
*           MODE=0 &                                         /*Y02676*/
*           PREFIX=0 &                                       /*Y02676*/
*           WTPMSG=0 &                                       /*Y02676*/
*           RECOV=0                                     /*@Z40NQKV*/
*           THEN                                             /*Y01156*/
@RF00097 SLR   @10,@10                                             0125
         L     @07,PDEPTR                                          0125
         CH    @10,PRMT(,@07)                                      0125
         BNE   @RF00125                                            0125
         CH    @10,MSGID(,@07)                                     0125
         BNE   @RF00125                                            0125
         CH    @10,INTERCOM(,@07)                                  0125
         BNE   @RF00125                                            0125
         CH    @10,PAUSE(,@07)                                     0125
         BNE   @RF00125                                            0125
         CH    @10,CHART(,@07)                                     0125
         BNE   @RF00125                                            0125
         CH    @10,LINE(,@07)                                      0125
         BNE   @RF00125                                            0125
         CH    @10,LIST(,@07)                                      0125
         BNE   @RF00125                                            0125
         CH    @10,MODE(,@07)                                      0125
         BNE   @RF00125                                            0125
         CH    @10,PREFIX(,@07)                                    0125
         BNE   @RF00125                                            0125
         CH    @10,WTPMSG(,@07)                                    0125
         BNE   @RF00125                                            0125
         CH    @10,RECOV(,@07)                                     0125
         BE    @RT00125                                            0125
*                              /*THEN DEFAULT TO LIST        @Z40NQKH*/
*             GO TO DEFLIST;                                       0126
*                               /* END EMPTY PDE CHECK               */
*        /************************************************************/
*        /* THESE STATEMENTS DETERMINE THE OPERANDS THAT             */
*        /* WERE ENTERED BY THE TERMINAL USER. THE APPROPRIATE       */
*        /* ACTION SPECIFIED IS TAKEN.                               */
*        /************************************************************/
*        IF PRMT=0              /* 'PROMPT','NOPROMPT' KEYWORDS?    **/
*          THEN                 /* PROMPT OPERAND IS NOT PRESENT     */
@RF00125 L     @10,PDEPTR                                          0127
         LH    @10,PRMT(,@10)                                      0127
         LTR   @10,@10                                             0127
         BZ    @RT00127                                            0127
*            GO TO CHKMSGID;    /* CHECK FOR NEXT OPERAND            */
*        IF PRMT=1              /* 'PROMPT'?                        **/
*          THEN                 /* PROMPT WAS SPECIFIED              */
         CH    @10,@CH00054                                        0129
         BNE   @RF00129                                            0129
*            DO;                                                   0130
*              UPTNPRM='0'B;    /*SET THE UPT PROMPT BIT TO PROMPT  **/
         NI    UPTNPRM(UPTPTR),B'10111111'                         0131
*              GO TO CHKMSGID;  /* CHECK NEXT PARAMETER              */
         B     CHKMSGID                                            0132
*            END;               /* END OF PROMPT CHECK               */
*        UPTNPRM='1'B;          /* NOPROMPT SPECIFIED-SET BIT TO 1  **/
@RF00129 OI    UPTNPRM(UPTPTR),B'01000000'                         0134
*CHKMSGID:                      /* CHECK FOR MESSAGE ID OPERAND      */
*        IF MSGID=0             /* MSGID OR NOMSGID OPERAND?        **/
*          THEN                 /* NEITHER OF THESE IS PRESENT       */
CHKMSGID L     @10,PDEPTR                                          0135
         LH    @10,MSGID(,@10)                                     0135
         LTR   @10,@10                                             0135
         BZ    @RT00135                                            0135
*            GOTO CHKINT;       /* GO TO CHECK FOR NEXT OPERAND      */
*        IF MSGID=1             /* IS MSGID SPECIFIED?              **/
*          THEN                 /* MSGID WAS SPECIFIED               */
         CH    @10,@CH00054                                        0137
         BNE   @RF00137                                            0137
*            DO;                                                   0138
*              UPTMID='1'B;     /* MSGID,SET UPT MSGID BIT TO 1     **/
         OI    UPTMID(UPTPTR),B'00100000'                          0139
*              GO TO CHKINT;    /* GO CHECK NEXT PARAMETER           */
         B     CHKINT                                              0140
*            END;               /* END OF MESSAGE ID CHECK           */
*        UPTMID='0'B;           /* NOMSGID,SET UPT MSGID BIT TO 0   **/
@RF00137 NI    UPTMID(UPTPTR),B'11011111'                          0142
*        /************************************************************/
*CHKINT: /* CHECK FOR TERMINAL INTER-COMMUNICATION OPERAND           */
*        /************************************************************/
*        IF INTERCOM=0          /* INTERCOM OR NO INTERCOM?         **/
*          THEN                 /* NEITHER WAS SPECIFIED             */
CHKINT   L     @10,PDEPTR                                          0143
         LH    @10,INTERCOM(,@10)                                  0143
         LTR   @10,@10                                             0143
         BZ    @RT00143                                            0143
*            GOTO CHKPAUS;      /* GO CHECK FOR NEXT OPERAND         */
*        IF INTERCOM=1          /* INTER COMMUNICATIONS?            **/
*          THEN                 /* INTERCOMMUNICATION SPECIFIED      */
         CH    @10,@CH00054                                        0145
         BNE   @RF00145                                            0145
*            DO;                /* SET TO INTERCOM STATUS            */
*              GENERATE(STCOM YES);/* ISSUE STCOM MACRO              */
         STCOM YES
*              UPTNCOM='0'B;    /* SET UPT INTERCOM BIT             **/
         NI    UPTNCOM(UPTPTR),B'11101111'                         0148
*              GO TO CHKPAUS;   /* GO TO CHECK NEXT OPERAND          */
         B     CHKPAUS                                             0149
*            END;               /* END OF INTERCOM CHECK             */
*        GENERATE(STCOM NO);    /* LEAVE INTERCOM STATUS AS IS       */
@RF00145 DS    0H                                                  0151
         STCOM NO
*        UPTNCOM='1'B;          /* SET UPT INTERCOM BIT TO 1        **/
         OI    UPTNCOM(UPTPTR),B'00010000'                         0152
*        /************************************************************/
*CHKPAUS:/* CHECK FOR THE TERMINAL PAUSE OPERAND                     */
*        /************************************************************/
*        IF PAUSE=0             /* PAUSE OR NO PAUSE?               **/
*          THEN                 /* NEITHER IS SPECIFIED              */
CHKPAUS  L     @10,PDEPTR                                          0153
         LH    @10,PAUSE(,@10)                                     0153
         LTR   @10,@10                                             0153
         BZ    @RT00153                                            0153
*            GOTO CHKMODE;      /* GO CHECK FOR NEXT OPERAND   Y01676*/
*        IF PAUSE=1             /* CHECK FOR PAUSE KEYWORD          **/
*          THEN                 /* PAUSE WAS SPECIFIED               */
         CH    @10,@CH00054                                        0155
         BNE   @RF00155                                            0155
*            DO;                                                   0156
*              UPTPAUS='1'B;    /* SET PAUSE BIT IN UPT             **/
         OI    UPTPAUS(UPTPTR),B'00001000'                         0157
*              GO TO CHKMODE;   /* GO TO CHECK NEXT PARAMETER  Y01676*/
         B     CHKMODE                                             0158
*            END;               /* END OF PAUSE CHECK                */
*        UPTPAUS='0'B;          /* SET UPT NO PAUSE BIT             **/
@RF00155 NI    UPTPAUS(UPTPTR),B'11110111'                         0160
*CHKMODE:                                                    /*Y01676*/
*        IF MODE=0 THEN         /* MODE OR NOMODE SPECIFIED?   Y01676*/
CHKMODE  L     @10,PDEPTR                                          0161
         LH    @10,MODE(,@10)                                      0161
         LTR   @10,@10                                             0161
         BZ    @RT00161                                            0161
*          GOTO CHKWTP;         /* GO CHECK NEXT OPERAND       Y01676*/
*        IF MODE=1 THEN         /* IF MODE SPECIFIED           Y01676*/
         CH    @10,@CH00054                                        0163
         BNE   @RF00163                                            0163
*          DO;                  /* THEN                        Y01676*/
*            UPTMODE='1'B;      /* UPDATE UPT FOR MODE         Y01676*/
         OI    UPTMODE(UPTPTR),B'00000010'                         0165
*            GOTO CHKWTP;       /* GO CHECK NEXT OPERAND       Y01676*/
         B     CHKWTP                                              0166
*          END;                 /*                             Y01676*/
*        UPTMODE='0'B;          /* INDICATE IN UPT NOMODE      Y01676*/
@RF00163 NI    UPTMODE(UPTPTR),B'11111101'                         0168
*       /*************************************************************/
*       /* CHECK FOR WTP OPERAND                                     */
*       /*************************************************************/
*CHKWTP:                        /* SEE IF WTPMSG OR            Y02676*/
*        IF WTPMSG = 0 THEN     /* NOWTPMSG OPERANDS ARE       Y02676*/
CHKWTP   L     @10,PDEPTR                                          0169
         LH    @10,WTPMSG(,@10)                                    0169
         LTR   @10,@10                                             0169
         BZ    @RT00169                                            0169
*          GOTO CHKPRFIX;       /* PRESENT--NO GOTO NEXT       Y02676*/
*        IF WTPMSG = 1 THEN     /* IS WTPMSG THE REQUEST?      Y02676*/
         CH    @10,@CH00054                                        0171
         BNE   @RF00171                                            0171
*          DO;                  /* YES                         Y02676*/
*            UPTWTP='1'B;       /* SET BIT IN UPT              Y02676*/
         OI    UPTWTP(UPTPTR),B'00000001'                          0173
*            GOTO CHKPRFIX;     /* CHECK FOR NEXT OPERAND.     Y02676*/
         B     CHKPRFIX                                            0174
*          END;                 /*                             Y02676*/
*        UPTWTP='0'B;           /* NOWTPMSG OPERAND            Y02676*/
@RF00171 NI    UPTWTP(UPTPTR),B'11111110'                          0176
*       /*************************************************************/
*       /*  CHECK FOR PREFIX OPERAND.                          Y02676*/
*       /*************************************************************/
*CHKPRFIX:                      /*                             Y02676*/
*        IF PREFIX = 0 THEN     /* USER PREFIX OPTION?         Y02676*/
CHKPRFIX L     @10,PDEPTR                                          0177
         LH    @10,PREFIX(,@10)                                    0177
         LTR   @10,@10                                             0177
         BZ    @RT00177                                            0177
*          GOTO CHKRCVR;        /* NO -- GOTO RECOVER OPTION @Z40NQKH*/
*        IF PREFIX = 2          /* USER SPECIFY NOPREFIX?      Y02676*/
*          THEN                 /*  YES                        Y02676*/
         CH    @10,@CH00050                                        0179
         BNE   @RF00179                                            0179
*            DO;                /*                             Y02676*/
*              UPTPREFL='00'X;  /*  SET PREFIX LENGTH = 0      Y02676*/
         MVI   UPTPREFL(UPTPTR),X'00'                              0181
*              UPTPREFX(1:7)=' ';/* PUT PREFIX IN UPT          Y02676*/
         MVI   UPTPREFX+1(UPTPTR),C' '                             0182
         MVC   UPTPREFX+2(5,UPTPTR),UPTPREFX+1(UPTPTR)             0182
         MVI   UPTPREFX(UPTPTR),C' '                               0182
*            END;               /* CHECK CHAR OPTION           Y02676*/
*          ELSE                 /* USER MUST HAVE SPECIFIED    Y02676*/
*            DO;                /* PREFIX OPTION.              Y02676*/
         B     @RC00179                                            0184
@RF00179 DS    0H                                                  0185
*              UPTPREFL=PRFIXLEN;/* PUT LENGTH IN UPT          Y02676*/
         L     @10,PDEPTR                                          0185
         LH    @07,PRFIXLEN(,@10)                                  0185
         STC   @07,UPTPREFL(,UPTPTR)                               0185
*              UPTPREFX(1:7)=' ';/* BLANK OUT USERID FIELD     Y02676*/
         MVI   UPTPREFX+1(UPTPTR),C' '                             0186
         MVC   UPTPREFX+2(5,UPTPTR),UPTPREFX+1(UPTPTR)             0186
         MVI   UPTPREFX(UPTPTR),C' '                               0186
*              UPTPREFX(1:PRFIXLEN)= /* INIT USERID FIELD WITH Y02676*/
*                PRFIXID->USER(1:PRFIXLEN);/* SPECIFIED PREFIX Y02676*/
         BCTR  @07,0                                               0187
         L     @10,PRFIXID(,@10)                                   0187
         EX    @07,@SM01657                                        0187
*            END;               /*                             Y02676*/
*       /************************************************************/
*       /*  CHECK FOR RECOVER OPERAND                               */
*       /************************************************************/
*CHKRCVR:                                                          0189
*        IF RECOV = 0 THEN      /* RECOVER ENTERED ?      @Z40DQKV*/
@RC00179 DS    0H                                                  0189
CHKRCVR  SLR   @10,@10                                             0189
         L     @07,PDEPTR                                          0189
         LH    @07,RECOV(,@07)                                     0189
         CR    @07,@10                                             0189
         BE    @RT00189                                            0189
*          GOTO CHKCHAR;        /* NO -- GOTO CHAR OPTION @Z40DQKV*/
*        IF RECOV > 0 THEN      /* RECOVER ENTERED ??     @Z40DQKV*/
         CR    @07,@10                                             0191
         BNH   @RF00191                                            0191
*          DO;                  /*                        @Z40DQKV*/
*            IF PSCBRCVR = '0'B /* IS THE USER AUTHORIZED @Z40DQKV*/
*              THEN DO;         /* LETS CHECK FURTHER     @Z40DQKV*/
         L     @10,CPPLPTR                                         0193
         L     @10,PSCBPTR(,@10)                                   0193
         TM    PSCBRCVR(@10),B'00000100'                           0193
         BNZ   @RF00193                                            0193
*                IF JSCBPSCB = ADDR(PSCB)                          0195
*                               /* DOES JSCB -> SAME PSCB?@Z40DQKV*/
*                  THEN  DO;    /* YES LET EM DOIT       @Z40DQKV*/
         L     @01,CVTPTR                                          0195
         L     @01,CVTTCBP(,@01)                                   0195
         L     @01,TCBPTR(,@01)                                    0195
         L     @01,TCBJSCB(,@01)                                   0195
         C     @10,JSCBPSCB(,@01)                                  0195
         BNE   @RF00195                                            0195
*                    IF RECOV = 1 THEN /* RECOVER WANTE ?@Z40DQKV*/
         CH    @07,@CH00054                                        0197
         BNE   @RF00197                                            0197
*                       UPTRCVR = '1'B; /* TELL EDIT     @Z40DQKV*/
         OI    UPTRCVR(UPTPTR),B'10000000'                         0198
*                    ELSE              /* MUST BE NORECVR@Z40DQKV*/
*                       UPTRCVR = '0'B;/* TELL EDIT      @Z40DQKV*/
         B     @RC00197                                            0199
@RF00197 NI    UPTRCVR(UPTPTR),B'01111111'                         0199
*                     GOTO CHKCHAR;    /* CONTINUE PROCES@Z40DQKV*/
         B     CHKCHAR                                             0200
*                    END;                                          0201
*                END;          /* USER NOT AUTHORIZED     @Z40DQKV*/
@RF00195 DS    0H                                                  0203
*              RFY (R8) RSTD;  /* REG 8 IS MSG CSECT BASE @Z40DQKV*/
@RF00193 DS    0H                                                  0204
*              R8 = ADDR(PROFMSGS); /* GET MSG CSECT ADDR @Z40DQKV*/
         L     R8,@CV01447                                         0204
*              GEN(USING PROFMSGS,R8);/*                  @Z40DQKV*/
         USING PROFMSGS,R8
*              SEGMENT=ADDR(AUTHMSG); /* GET MSG   @Z40DQKV*/      0206
         LA    @10,AUTHMSG                                         0206
         ST    @10,SEGMENT                                         0206
*              LEVEL2P=ADDR(LEVELNP); /*PTR 2ND LEV@Z40DQKV*/      0207
         LA    @10,LEVELNP                                         0207
