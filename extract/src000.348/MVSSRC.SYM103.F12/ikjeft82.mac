         ST    @11,8(,@13)                                         0001
         LR    @13,@11                                             0001
         XC    @ZTEMPS(@ZLEN),@ZTEMPS
         MVC   @PC00001(4),0(@01)                                  0001
JSCBDISP EQU 188
JSCBAOS1 EQU  72
JSCBAOS2 EQU 192
*        CPPLPTR=R1;            /*INTIALIZE COMMAND PARAMETER POINTER*/
         ST    R1,CPPLPTR                                          0075
*        CHARLNMG='00'X;        /*INIT 'CHAR-LINE INVALID' MSG Y01156*/
         MVI   CHARLNMG,X'00'                                      0076
*                               /* TO NO ERRORS FOUND          Y01156*/
*        RFY (R3,R6)RSTD;      /* REGS 3 & 6 USED BY THIS ROUTINE  0077
*                                                            Z40DQJD*/
*        /************************************************************/
*        /* INITIALIZE POINTERS TO BE USED BY SERVICE ROUTINES       */
*        /************************************************************/
*        ECTPTR=CPPLECT;        /* INITIALIZE PARSE ECT POINTER      */
         L     @10,CPPLPTR                                         0078
         L     @07,CPPLECT(,@10)                                   0078
         ST    @07,ECTPTR                                          0078
*        UPTPTR=CPPLUPT;        /* INITIALIZE PARSE UPT PTR          */
         L     @15,CPPLUPT(,@10)                                   0079
         LR    UPTPTR,@15                                          0079
*        CPECB=0;               /* INITIALIZE CP ECB                 */
         SLR   @14,@14                                             0080
         ST    @14,CPECB                                           0080
*        PDEPTR=0;                                                 0081
         ST    @14,PDEPTR                                          0081
*        FIBPRFIL=FIBPRFIL&&FIBPRFIL;/* INIT SVC 100 LIST      Y01676*/
         XC    FIBPRFIL(4),FIBPRFIL                                0082
*        WORKBYTE='00'X;        /* INIT WORK AREA TO 0.        Y01676*/
         MVI   WORKBYTE,X'00'                                      0083
