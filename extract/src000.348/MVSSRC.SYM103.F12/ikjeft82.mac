*/* THIS VALIDITY CHECKING ROUTINE IS ENTERED BY PARSE WHENEVER      */
*/* THE TERMINAL USER ENTERS THE 'CHAR' KEYWORD AND A CHARACTER.     */
*/* ANY CHARACTER IS VALID WITH THE EXCEPTION OF THE NEW LINE        */
*/* (NL) CHARACTER.THE CHARACTERS, 'BS' INDICATES THAT               */
*/* THE USER WANTS THE BACKSPACE CHARACTER TO BE USED AS THE         */
*/* CHARACTER DELETE CHARACTER.                                      */
*/********************************************************************/
*CHARCHEK:                      /*************************************/
*        PROCEDURE                                                 0492
*        OPTIONS(DONTSAVE(15),  /* REG 15 IS THE RETURN CODE REG     */
*           NOSAVEAREA);        /* GET NO SAVE AREA                  */
CHARCHEK ST    @14,12(,@13)                                        0492
         STM   @00,@12,20(@13)                                     0492
*     DECLARE                   /*************************************/
*        DELCHAR CHAR(2) BASED(CHARPTR);/* MAP THE 'BS' CHARACTER    */
*        RFY (R3)RSTD;     /* REGISTER 3 IS USED BY THIS RTN Z40DQJD */
*        /* ESTABLISH ADDRESSABILITY IN THIS ROUTINE                 */
*        GENERATE;                                                 0495
              L   3,4(1)             GET PTR TO PROFILE'S SAVE AREA
       LM  11,12,64(3)         RESTORE REG 11,12 FOR ADDRESSABILITY
       L   3,0(1)              GET THE PDE POINTER
*       PDEADDR=R3;             /* INITIALIZE PTR FROM PARSE         */
         ST    R3,PDEADDR                                          0496
*       RFY(R3)UNRSTD;         /* RFY REGISTER 3 TO COMPILER Z40DQJD */
*       IF CHARLEN=1            /* IF LENGTH=1,CHAR IS GOOD         **/
*         THEN                  /* CHARACTER IS VALID OR NEW LINE    */
         L     @10,PDEADDR                                         0498
         CLC   CHARLEN(2,@10),@CH00054                             0498
         BNE   @RF00498                                            0498
*           DO;                 /* CONTINUE PROCESSING               */
*                               /*************************************/
*             IF DELCHAR(1)='15'X /*CHECK FOR NEW LINE CHARACTER    **/
*               THEN            /* NEW LINE CHAR WAS SPECIFIED       */
         L     @10,CHARPTR(,@10)                                   0500
         CLI   DELCHAR(@10),X'15'                                  0500
         BE    @RT00500                                            0500
*                 GO TO RETUR;  /* INVALID-RETURN TO  PARSE          */
*             CHARTDEL='00'X;   /* CHARACTER NOT BACKSPACE          **/
         MVI   CHARTDEL,X'00'                                      0502
*             R15=0;            /* CHARACTER IS VALID               **/
         SLR   R15,R15                                             0503
*             RETURN;           /* RETURN TO PARSE                   */
@EL00003 DS    0H                                                  0504
@EF00003 DS    0H                                                  0504
@ER00003 L     @14,12(,@13)                                        0504
         LM    @00,@12,20(@13)                                     0504
         BR    @14                                                 0504
*           END;                                                   0505
*      IF CHARLEN>2             /* STRING LENGTH MORE THAN 2        **/
*        THEN                                                      0506
@RF00498 L     @10,PDEADDR                                         0506
         LH    @07,CHARLEN(,@10)                                   0506
         CH    @07,@CH00050                                        0506
         BH    @RT00506                                            0506
*          GO TO RETUR;                                            0507
*      TEMPCHAR(1:2)=DELCHAR  '4040'X; /*CHANGE TO UPPER CASE   */
         L     @10,CHARPTR(,@10)                                   0508
         MVC   TEMPCHAR(2),DELCHAR(@10)                            0508
         OC    TEMPCHAR(2),@CB01632                                0508
*      IF TEMPCHAR(1:2)='BS' /*IS DELETE CHARACTER A BACKSPACE   **/
*        THEN                /*   CHAR IS BACKSPACE               */
         CLC   TEMPCHAR(2),@CC01582                                0509
         BNE   @RF00509                                            0509
*          DO;                                                     0510
*            CHARTDEL='16'X; /* SET BACKSPACE CHARACTER          **/
         MVI   CHARTDEL,X'16'                                      0511
*            R15=0;         /* RETURN CODE IMPLIES VALID CHAR   **/
         SLR   R15,R15                                             0512
*            RETURN;        /* RETURN TO PARSE                   */
         B     @EL00003                                            0513
*          END;                                                    0514
*RETUR:                         /* GIVE ERROR MSG AND PROMPT         */
*        R15=4;                 /* INVALID RETURN CODE              **/
@RF00509 DS    0H                                                  0515
RETUR    LA    R15,4                                               0515
*        RETURN;                /* RETURN TO PARSE                   */
         B     @EL00003                                            0516
*        END CHARCHEK;                                             0517
