*/*                                                                  */
*/* EXIT-ERROR -- RETURN BY LINK REGISTER 14                         */
*/*                                                                  */
*/*   CONDITIONS -- PARSE ERROR (MSGNO = 1)                          */
*/*              -- BLDL ERROR (MSGNO = 3)                           */
*/*              -- PUTLINE FAILURE (MSGNO = 88)                     */
*/*   RETURN CODE -- N/A                                             */
*/*                                                                  */
*/* * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * * */
*DOBLDL:    /* PERFORM BLDL FOR CALLED CP                            */
*                                                               01S0204
*    ENTRY;                                                     01S0204
DOBLDL   DS    0H                                               01S0205
*    /* PERFORM BLDL, PROMPT FOR MEMBERNAME IF REQUIRED              */
*    BLDLRC = D4;                   /* FORCE INITIAL ATTEMPT         */
         MVC   BLDLRC(4,WRKAPTR1),@CF00044                      01S0205
*    DO WHILE PARSRC = ZERO         /* CHK FOR SUCCESSFUL PARSE      */
*        & PUTLRC = ZERO            /*   & SUCCESSFUL PUTLINE        */
*        & BLDLRC = D4              /*   & MEMBER NOT FOUND          */
*        & MSGNO = ZERO;            /*   & NO TERMINATION FLAG       */
         B     @DE00206                                         01S0206
@DL00206 DS    0H                                               01S0207
*      BLDLENT = '0001'X;           /* NUMBER OF ENTRIES = 1         */
         MVC   BLDLENT(2,WRKAPTR1),@CH02616                     01S0207
*      BLDLELNG = '0040'X;          /* MEMBER BLDL LENGTH            */
         MVC   BLDLELNG(2,WRKAPTR1),@CH02617                    01S0208
*      BLDLNAME = MBRSTXT;          /* MOVE MEMBER NAME TO BLDL      */
         MVC   BLDLNAME(8,WRKAPTR1),MBRSTXT(WRKAPTR1)           01S0209
