         OI    ATCHNOW(WRKAPTR1),B'10000000'                    01S0287
*    RESPECIFY (R2) UNRSTD;                                     01S0288
*                                                               01S0288
*    RESPECIFY (R14) RSTD;                                      01S0289
*    R14 = ADDR(DOCALLE);           /* SET FOR PSEUDO-RETURN         */
         LA    R14,DOCALLE                                      01S0290
*    RETURN;                        /* RETURN TO CALLER              */
         B     @EL00003                                         01S0291
*    RESPECIFY (R14) UNRSTD;                                    01S0292
*/* * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * * */
*/*                                                                  */
*/* SEGMENT NAME -- DOMSG                                            */
*/*                                                                  */
*/* DESCRIPTIVE NAME -- N/A                                          */
*/*                                                                  */
*/* FUNCTION -- TO ISSUE ANY PENDING MESSAGES                        */
*/*   OPERATION --                                                   */
*/*           - CHECK FOR ANY PENDING MESSAGES                       */
*/*           - IF MSGNO = 1 AND PARSRC = 16, SET UP NOT-ENOUGH-SPACE*/
*/*             MESSAGE                                              */
*/*           - IF MSGNO = 1 AND PARSRC ^= 16, SET UP PARSE-ERROR    */
*/*             MESSAGE                                              */
*/*           - IF MSGNO = 99 (ALLOCATION FAILURE), PERFORM DAIRFAIL */
*/*           - IF MSGNO = 11, SET UP DATA-SET-TYPE ERROR MESSAGE    */
*/*           - IF MSGNO = 21, SET UP OPEN-FAILURE MESSAGE           */
*/*           - IF MSGNO = 3, SET UP BLDL-I/O-FAILURE MESSAGE        */
*/*           - IF MSGNO = 88, PUTLINE FAILURE, NO MESSAGE POSSIBLE  */
*/*           - IF MESSAGE POSSIBLE, CALL PUTLINE TO ISSUE MESSAGE   */
*/*           - IF MSGNO ^= 1, SET FAILURE RETURN CODE (12)          */
*/*                                                                  */
*/* ENTRY -- LINKED BY REGISTER 14                                   */
*/*                                                                  */
*/* INPUT --  MSGNO                                                  */
*/*                                                                  */
*/* OUTPUT -- ISSUES APPROPRIATE MESSAGE IF MSGNO IS NOT 0 OR 88     */
*/*                                                                  */
*/* EXTERNAL REFERENCES -- N/A                                       */
*/*                                                                  */
*/* EXIT-NORMAL --  RETURN BY LINK REGISTER 14                       */
*/*                                                                  */
*/*   RETURN CODE -- N/A                                             */
*/*                                                                  */
*/* EXIT-ERROR -- RETURN BY LINK REGISTER 14                         */
*/*                                                                  */
*/*   CONDITIONS -- NOT ENOUGH SPACE TO EXECUTE CMD (MSGNO =1,       */
*/*                  PARSRC = 16)                                    */
*/*              -- PARSE ERROR (MSGNO = 1)                          */
*/*              -- ALLOCATION ERROR (MSGNO = 99)                    */
*/*              -- DATA-SET-TYPE-ERROR (MSGNO = 11)                 */
*/*              -- OPEN FAILURE (MSGNO = 21)                        */
*/*              -- BLDL FAILURE (MSGNO = 3)                         */
*/*              -- PUTLINE FAILURE (MSGNO = 88)                     */
*/*   RETURN CODE -- RTNCODE = 12                                    */
*/*                                                                  */
*/* * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * * */
*DOMSG:     /* ISSUE ANY PENDING MESSAGE                             */
*                                                               01S0293
*    ENTRY;                         /* EP TO ROUTINE                 */
DOMSG    DS    0H                                               01S0294
*    RESPECIFY (R2,R4,R14) RSTD;                                01S0294
*                                                               01S0294
*    /* ESTABLISH CURRENT MESSAGE                                    */
*    IF MSGNO ^= ZERO               /* CHK FOR SOME MESSAGE          */
*      THEN DO;                     /* YES, ISSUE MESSAGE            */
         L     @10,MSGNO(,WRKAPTR1)                             01S0295
         LTR   @10,@10                                          01S0295
         BZ    @RF00295                                         01S0295
*        IF MSGNO = D1              /* CHK FOR PARSE ERROR           */
*          THEN DO;                 /* YES,                          */
         CL    @10,@CF00032                                     01S0297
         BNE   @RF00297                                         01S0297
*            IF PARSRC=D16          /*   CHK FOR NO-SPACE MESSAGE    */
*              THEN                 /* YES,                          */
         CLC   PARSRC+1236(4,WRKAPTR1),@CF00049                 01S0299
         BNE   @RF00299                                         01S0299
*                ACTSL=CM01SL;      /*   INITIALIZE SEGMENT LIST     */
         L     @10,@CA00125                                     01S0300
         MVC   ACTSL(112,WRKAPTR1),CM01SL-CM01SL(@10)           01S0300
*              ELSE DO;             /* NOT NO-SPACE                  */
         B     @RC00299                                         01S0301
@RF00299 DS    0H                                               01S0302
*                CVD  (PRSMOFF,PRSMCVA);                        01S0302
         LA    @10,1236                                         01S0302
         ALR   @10,WRKAPTR1                                     01S0302
         LH    @06,PRSMOFF(,@10)                                01S0302
         CVD   @06,PRSMCVA(,@10)                                01S0302
*                UNPK (PRSMVAL,PRSMDEC);                        01S0303
         UNPK  PRSMVAL(6,@10),PRSMDEC(4,@10)                    01S0303
*                PARSRC = CH4NOFF;                              01S0304
         MVC   PARSRC(4,@10),@CF00047                           01S0304
*                ACTSL=CM02SL;      /* INITIALIZE SEGMENT LIST       */
         L     @06,@CA00126                                     01S0305
         MVC   ACTSL(112,WRKAPTR1),CM02SL-CM02SL(@06)           01S0305
*                ACTSEG(9) = ADDR(PRSMAR);                      01S0306
         ST    @10,ACTSEG+32(,WRKAPTR1)                         01S0306
*                ACTSEG(1) = ADDR(ACTSEG(5));                   01S0307
         LA    @10,ACTSEG+16(,WRKAPTR1)                         01S0307
         ST    @10,ACTSEG(,WRKAPTR1)                            01S0307
*              END;                                             01S0308
*          END;                                                 01S0309
@RC00299 DS    0H                                               01S0310
*        IF MSGNO = D99             /* CHK FOR ALLOCATION FAILURE    */
*          THEN DO;                 /* YES,                          */
@RF00297 CLC   MSGNO(4,WRKAPTR1),@CF00077                       01S0310
         BNE   @RF00310                                         01S0310
*            R2 = ADDR(DAPL);       /* SET PTR TO DAIR PL            */
         L     R2,DAPLPTR(,WRKAPTR1)                            01S0312
*            R15 = ADDR(DOALLCM);   /* TO INVOKE-DYNALLOC RTN        */
         LA    R15,DOALLCM                                      01S0313
*            GEN FLOWS(DOALLCM) (SYNCH  (R15));  /* DYNALLOC         */
         SYNCH  (R15)
*          END;                                                 01S0315
*        IF MSGNO = D11             /* CHK FOR DS TYPE ERROR         */
*          THEN                     /* YES,                          */
@RF00310 CLC   MSGNO(4,WRKAPTR1),@CF00066                       01S0316
         BNE   @RF00316                                         01S0316
*            CALL DOPODSM;          /* TO SET TYPE ERROR MSG         */
         BAL   @14,DOPODSM                                      01S0317
*        IF MSGNO = D21             /* CHK FOR OPEN FAILURE          */
*          THEN                     /* YES,                          */
@RF00316 CLC   MSGNO(4,WRKAPTR1),@CF00071                       01S0318
         BNE   @RF00318                                         01S0318
*            CALL DOOPENM;          /* TO SET OPEN FAILURE MSG       */
         BAL   @14,DOOPENM                                      01S0319
*        IF MSGNO = D3              /* CHK FOR BLDL FAILURE          */
*          THEN                     /* YES,                          */
@RF00318 CLC   MSGNO(4,WRKAPTR1),@CF00058                       01S0320
         BNE   @RF00320                                         01S0320
*            ACTSL=CM03SL;          /* SET UP BLDL FAILURE MSG       */
         L     @10,@CA00127                                     01S0321
         MVC   ACTSL(112,WRKAPTR1),CM03SL-CM03SL(@10)           01S0321
*        IF MSGNO ^= D88            /* CHK FOR NO TERMINATION FLAG   */
*            &MSGNO ^= ZERO         /*   & FOR SOME ERROR FLAG       */
*          THEN DO;                 /* YES,                          */
@RF00320 L     @10,MSGNO(,WRKAPTR1)                             01S0322
         C     @10,@CF00075                                     01S0322
         BE    @RF00322                                         01S0322
         LTR   @10,@10                                          01S0322
         BZ    @RF00322                                         01S0322
*            R4 = PTPBPTR;          /* SET PUTLINE PARM BLOCK PTR    */
         L     R4,PTPBPTR(,WRKAPTR1)                            01S0324
*            R1 = IOPLPTR;          /* SET PUTLINE PARM LIST PTR     */
         L     R1,IOPLPTR(,WRKAPTR1)                            01S0325
*            R15 = ADDR(DOPUTL);    /* TO INVOKE-PUTLINE RTN         */
         LA    R15,DOPUTL                                       01S0326
*            GEN FLOWS(DOPUTL) (SYNCH  (R15));  /* TO PUTLINE        */
         SYNCH  (R15)
*          END;                                                 01S0328
*        RTNCODE = D12;             /* SET FAILURE CODE              */
@RF00322 L     @10,SAVRBAK(,R13)                                01S0329
         MVC   RTNCODE(4,@10),@CF00068                          01S0329
*      END;                                                     01S0330
*                                                               01S0330
*    RESPECIFY (R14) RSTD;                                      01S0331
@RF00295 DS    0H                                               01S0332
*    /* R14 = ADDR(DOCAL2A);           SET FOR PSEUDO-RETURN  ZA29806*/
*    R14 = ADDR(DOCALLF);           /* SET FOR PSEUDO-RETURN  ZA29806*/
         LA    R14,DOCALLF                                      01S0332
*    RETURN;                        /* RETURN TO CALLER              */
         B     @EL00003                                         01S0333
*    RESPECIFY (R14) UNRSTD;                                    01S0334
*/* * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * * */
