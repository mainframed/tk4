         TITLE 'IKJEFT08 -- TMP-CALL COMMAND PROCESSOR                 *
                        '
IKJEFT08 CSECT ,                                                01S0001
@MAINENT DS    0H                                               01S0001
         USING *,@15                                            01S0001
         B     @PROLOG                                          01S0001
         DC    AL2(@EP00001-@MAINENT)
         DC    AL1(16)                                          01S0001
         DC    C'IKJEFT08  78.177'                              01S0001
DOCALL2  DS    0H                                               01S0001
         USING *,@15                                            01S0001
         B     @PROLOG                                          01S0001
         DC    AL2(@EP00117-DOCALL2)
         ENTRY DOCALL2
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                  01S0001
         BALR  @11,0                                            01S0001
@PSTART  DS    0H                                               01S0001
         USING @PSTART,@11                                      01S0001
         L     @00,@SIZDATD                                     01S0001
         GETMAIN  R,LV=(0)
         LR    @12,@01                                          01S0001
         USING @DATD,@12                                        01S0001
         ST    @13,@SA00001+4                                   01S0001
         LM    @15,@01,16(@13)                                  01S0001
         ST    @12,8(,@13)                                      01S0001
         LR    @13,@12                                          01S0001
         AH    @15,4(,@15)                                      01S0001
         BR    @15                                              01S0001
@EP00001 DS    0H                                               01S0002
JSCBDISP EQU 188
JSCBAOS1 EQU  72
JSCBAOS2 EQU 192
*/* * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * * */
*/*                                                                  */
*/* SEGMENT NAME -- DOCALL                                           */
*/*                                                                  */
*/* DESCRIPTIVE NAME -- N/A                                          */
*/*                                                                  */
*/* FUNCTION -- MAIN ENTRY TO CALL FUNCTION                          */
*/*                                                                  */
*/*   OPERATION --                                                   */
*/*           - EXECUTE STANDARD PROLOGUE, SAVING ENTRY ENVIRONMENT  */
*/*           - PERFORM SEGMENT TINIT8 TO INITIALIZE CONTROLS FOR    */
*/*             DOCALL                                               */
*/*           - PERFORM SEGMENT DOPARS TO SYNTAX CHECK THE COMMAND   */
*/*           - IF PARSE UNSUCCESSFUL (RETURN CODE > 0), SET MSGNO   */
*/*             TO ONE FOR PARSE-FAILURE MESSAGE                     */
*/*           _ IF PARSE SUCCESSFUL --                               */
*/*             -IF MEMBER NAME ENTERED, TURN GET-MEMBER-NOW FLAG ON */
*/*             -SET DAIR RETURN CODE TO 8                           */
*/*             -WHILE PARSRC IS 0 AND DAIRRC IS 8, PERFORM SEGMENT  */
*/*              DOSETCP TO ALLOCATE FOR CP CALLED                   */
*/*             -IF PARSE AND PUTLINE ARE SUCCESSFUL (MSGNO IS 0),   */
*/*              PERFORM SEGMENT DOOPEN TO OPEN CP DCB               */
*/*             -IF OPEN IS SUCCESSFUL (OPENNOW = YES), PERFORM      */
*/*              SEGMENT DOBLDL TO LOCATE CP                         */
*/*             -IF MSGNO IS ZERO, PERFORM SEGMENT DOCALLP TO        */
*/*              ATTACH CP                                           */
*/*             -PERFORM STANDARD EPILOGUE TO RESTORE ENTRY          */
*/*              ENVIRONMENT AND RETURN TO CALLER                    */
*/*           - PERFORM SEGMENT DOMSG TO ISSUE ANY TERMINATION       */
*/*             MESSAGES                                             */
*/*           - PERFORM SEGMENT DOTERM TO TERMINATE CALL ACTIVITY,   */
*/*             RELEASE ALL RESOURCES USED AND SET RETURN CODE       */
*/*           - PERFORM STANDARD EPILOGUE TO RESTORE ENTRY           */
*/*             ENVIRONMENT AND RETURN TO CALLER                     */
*/*                                                                  */
*/* ENTRY --                                                         */
*/*                                                                  */
*/* INPUT --  N/A                                                    */
*/*                                                                  */
*/* OUTPUT -- N/A                                                    */
*/*                                                                  */
*/* EXTERNAL REFERENCES -- N/A                                       */
*/*                                                                  */
*/* EXIT-NORMAL --  RETURN                                           */
*/*                                                                  */
*/*   RETURN CODE -- N/A                                             */
*/*                                                                  */
*/* EXIT-ERROR -- RETURN                                             */
*/*                                                                  */
*/*   CONDITIONS -- N/A                                              */
*/*   RETURN CODE -- N/A                                             */
*/*                                                                  */
*/* * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * * */
*DOCALL:  /* EP (LOGICAL) TO CALL FUNCTION                           */
*                                                               01S0087
*    CALL  TINIT8;                  /* TO INITIALIZE CONTROLS        */
DOCALL   BAL   @14,TINIT8                                       01S0087
*    R15 = ADDR(DOPARS);            /* PTR TO INVOKE-PARSE ROUTINE   */
         LA    R15,DOPARS                                       01S0088
*    GEN FLOWS(DOPARS) (SYNCH  (R15));  /* TO CHK SYNTAX             */
         SYNCH  (R15)
*    IF PARSRC = ZERO               /* CHK FOR SUCCESSFUL PARSE      */
*        & MSGNO = ZERO             /* CHK FOR TERMINATION           */
*      THEN DO;                     /* YES, PROCESS COMMAND          */
         SLR   @14,@14                                          01S0090
         CL    @14,PARSRC+1236(,WRKAPTR1)                       01S0090
         BNE   @RF00090                                         01S0090
         C     @14,MSGNO(,WRKAPTR1)                             01S0090
         BNE   @RF00090                                         01S0090
*        R3SAVE = R3;               /* SAVE ORIGINAL PDL PTR  ZA28716*/
         ST    R3,R3SAVE(,WRKAPTR1)                             01S0092
*        /* ESTABLISH CP NAME AND ALLOCATE ITS DS                    */
*        IF MBRPRES = NO            /* CHK IF MEMBER NAME ENTERED    */
*          THEN                     /* NO,                           */
         TM    MBRPRES(R3),B'10000000'                          01S0093
         BNZ   @RF00093                                         01S0093
*            GMBRNOW = YES;         /* SET GET-MEMBER-NAME FLAG      */
         OI    GMBRNOW(WRKAPTR1),B'00000100'                    01S0094
*          ELSE                     /* YES, MBR NAME PRESENT         */
*            R3 = PDLADDR;          /* PTR TO 1ST PDL                */
         B     @RC00093                                         01S0095
@RF00093 L     R3,PDLADDR(,WRKAPTR1)                            01S0095
*        DAIRRC = D8;               /* FORCE INITIAL ATTEMPT         */
@RC00093 MVC   DAIRRC(4,WRKAPTR1),@CF00038                      01S0096
*        DO WHILE PARSRC = ZERO     /* CHK FOR SUCCESSFUL PARSE      */
*            & DAIRRC = D8          /*   & MEMBER NAME NOT FOUND     */
*            & MSGNO = ZERO;        /*   & NO TERMINATION FLAG       */
         B     @DE00097                                         01S0097
@DL00097 DS    0H                                               01S0098
*          CALL DOSETCP;            /* TO ALLOCATE FOR CP CALLED     */
         BAL   @14,DOSETCP                                      01S0098
*DOCALLA:                           /* FOR PSEUDO-RETURN             */
*        END;                                                   01S0099
DOCALLA  DS    0H                                               01S0099
@DE00097 SLR   @14,@14                                          01S0099
         CL    @14,PARSRC+1236(,WRKAPTR1)                       01S0099
         BNE   @DC00097                                         01S0099
         CLC   DAIRRC(4,WRKAPTR1),@CF00038                      01S0099
         BNE   @DC00097                                         01S0099
         C     @14,MSGNO(,WRKAPTR1)                             01S0099
         BE    @DL00097                                         01S0099
@DC00097 DS    0H                                               01S0100
*        /* OPEN CP DCB, PASS CONTROL TO CP, AND WAIT                */
*        CALL DOOPEN;               /* TO OPEN CP DCB                */
         BAL   @14,DOOPEN                                       01S0100
*DOCALLB:                           /* FOR PSEUDO-RETURN             */
*        /* IF OPEN OK, ATTACH AND WAIT, THEN DETACH                 */
*        IF DSOPEN=YES              /* CHK FOR SUCCESSFUL OPEN       */
*          THEN DO;                 /* YES,                          */
DOCALLB  TM    DSOPEN(WRKAPTR1),B'01000000'                     01S0101
         BNO   @RF00101                                         01S0101
*            CALL  DOBLDL;          /* TO LOCATE CP                  */
         BAL   @14,DOBLDL                                       01S0103
*DOCALLC:    CALL  DOPARM;          /* TO CHK OPERANDS               */
DOCALLC  BAL   @14,DOPARM                                       01S0104
*DOCALLD:    IF MSGNO=ZERO          /* CHK ANY ERROR                 */
*              THEN                 /* NO,                           */
DOCALLD  L     @14,MSGNO(,WRKAPTR1)                             01S0105
         LTR   @14,@14                                          01S0105
         BNZ   @RF00105                                         01S0105
*                CALL  DOCALLP;     /* TO ATTACH                     */
         BAL   @14,DOCALLP                                      01S0106
*DOCALLE:  END;                     /* FOR PSEUDO-RETURN             */
@RF00105 DS    0H                                               01S0107
DOCALLE  DS    0H                                               01S0108
*      END;                                                     01S0108
*                                                               01S0108
@RF00101 DS    0H                                               01S0109
*    IF TPLAECB->ECBPOST = YES      /* CHK IF ATTENTION OCCURRED     */
*      THEN                         /* YES,                          */
@RF00090 L     @14,TPLAECB-1(,WRKAPTR1)                         01S0109
         TM    ECBPOST(@14),B'01000000'                         01S0109
         BNO   @RF00109                                         01S0109
*        DOLIST = YES;              /*   SET TO PROCESS ATTENTION    */
         OI    DOLIST(WRKAPTR1),B'00100000'                     01S0110
*      ELSE                         /* NO ATTENTION,                 */
*        IF MSGNO ^= ZERO           /*   CHK FOR ANY ERROR MESSAGE   */
*          THEN DO;                 /* YES,                   ZA29806*/
         B     @RC00109                                         01S0111
@RF00109 L     @14,MSGNO(,WRKAPTR1)                             01S0111
         LTR   @14,@14                                          01S0111
         BZ    @RF00111                                         01S0111
*            CALL DOMSG;            /* ISSUE MESSAGES         ZA29806*/
         BAL   @14,DOMSG                                        01S0113
*DOCALLF:                           /* FOR PSEUDO-RETURN      ZA29806*/
*            DOGETC = YES;          /* SET TO SKIP WAIT/DETACH       */
DOCALLF  OI    DOGETC(WRKAPTR1),B'00010000'                     01S0114
*          END;                     /*                        ZA29806*/
*                                                               01S0115
*    RETURN;                                                    01S0116
@EL00001 L     @13,4(,@13)                                      01S0116
@EF00001 L     @00,@SIZDATD                                     01S0116
         LR    @01,@12                                          01S0116
         FREEMAIN R,LV=(0),A=(1)
@ER00001 LM    @14,@12,12(@13)                                  01S0116
         BR    @14                                              01S0116
*/* * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * * */
*/*                                                                  */
*/* SEGMENT NAME -- DOCALL2                                          */
*/*                                                                  */
*/* DESCRIPTIVE NAME -- N/A                                          */
*/*                                                                  */
*/* FUNCTION -- INITIALIZE VARIABLES                                 */
*/*   OPERATION --                                                   */
*/*           - INITIALIZE AUTOMATIC COPIES OF MACROS FROM STATIC    */
*/*             COPIES                                               */
*/*           - SET UP SERVICE ROUTINE POINTERS                      */
*/*           - INITIALIZE USER ECB AND COMMUNICATIONS ECB           */
*/*           - SET INITIAL VALUES FOR VARIABLES                     */
*/*                                                                  */
*/* ENTRY -- LINKED BY REGISTER 14                                   */
*/*                                                                  */
*/* INPUT -- ADDRESS OF CPPL IN REGISTER 1                           */
*/*                                                                  */
*/* OUTPUT -- INITIALIZED ENVIRONMENT FOR CALL                       */
*/*                                                                  */
*/* EXTERNAL REFERENCES -- N/A                                       */
*/*                                                                  */
*/* EXIT-NORMAL -- RETURN BY LINK REGISTER 14                        */
*/*                                                                  */
*/*   RETURN CODE -- N/A                                             */
*/*                                                                  */
*/* EXIT-ERROR -- N/A                                                */
*/*                                                                  */
*/*   CONDITIONS -- N/A                                              */
*/*   RETURN CODE -- N/A                                             */
*/*                                                                  */
*/* * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * * */
*                                                               01S0117
*DOCALL2:  /* EP TO CLOSE-OUT-CALL-FUNCTION ROUTINE                  */
*                                                               01S0117
*    ENTRY;                                                     01S0117
@EP00117 DS    0H                                               01S0118
*    RTNCODE = ZERO;                /* INITIALIZE TO SUCCESS RC      */
         L     @14,SAVRBAK(,R13)                                01S0118
         SLR   @10,@10                                          01S0118
         ST    @10,RTNCODE(,@14)                                01S0118
*    CALL  DOMSG;                   /* TO ISSUE ANY TERMINATION MSG  */
         BAL   @14,DOMSG                                        01S0119
*DOCAL2A:                           /* FOR PSEUDO-RETURN             */
*    CALL  DOTERM;                  /* TO CLOSE-OUT CALL             */
*                                                               01S0120
DOCAL2A  BAL   @14,DOTERM                                       01S0120
*/*  END;                           **  (LOGICAL) OF CALL PROCESSOR  */
*MAINX:  /* RETURN FORCES EPILOG FUNCTION                            */
*    RETURN  CODE(RTNCODE);         /* END OF CALL PROCESSOR         */
MAINX    L     @10,SAVRBAK(,R13)                                01S0121
         L     @10,RTNCODE(,@10)                                01S0121
         L     @13,4(,@13)                                      01S0121
         L     @00,@SIZDATD                                     01S0121
         LR    @01,@12                                          01S0121
         FREEMAIN R,LV=(0),A=(1)
         LR    @15,@10                                          01S0121
         L     @14,12(,@13)                                     01S0121
         LM    @00,@12,20(@13)                                  01S0121
         BR    @14                                              01S0121
*/* * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * * */
*/*                                                                  */
*/* SEGMENT NAME -- TINIT8                                           */
*/*                                                                  */
*/* DESCRIPTIVE NAME -- N/A                                          */
*/*                                                                  */
*/* FUNCTION -- INITIALIZE VARIABLES                                 */
*/*   OPERATION --                                                   */
*/*           - SAVE LINK REGISTER                                   */
*/*           - INITIALIZE AUTOMATIC COPIES OF MACROS FROM STATIC    */
*/*             COPIES FOR OPEN AND CLOSE                            */
*/*           - SET UP SERVICE ROUTINE PARAMETERS                    */
*/*           - INITIALIZE USER ECB AND COMMUNICATIONS ECB           */
*/*           - SET INITIAL VALUES FOR VARIABLES                     */
*/*           - RESTORE LINK REGISTER                                */
*/*                                                                  */
*/* ENTRY -- LINKED BY REGISTER 14                                   */
*/*                                                                  */
*/* INPUT -- ADDRESS OF CPPL IN REGISTER 1                           */
*/*                                                                  */
*/* OUTPUT -- INITIALIZED ENVIRONMENT FOR CALL                       */
*/*                                                                  */
*/* EXTERNAL REFERENCES -- N/A                                       */
*/*                                                                  */
*/* EXIT-NORMAL -- RETURN BY LINK REGISTER 14                        */
*/*                                                                  */
*/*   RETURN CODE -- N/A                                             */
*/*                                                                  */
*/* EXIT-ERROR -- N/A                                                */
*/*                                                                  */
*/*   CONDITIONS -- N/A                                              */
*/*   RETURN CODE -- N/A                                             */
*/*                                                                  */
*/* * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * * */
*TINIT8:  /* INITIALIZATION                                          */
*                                                               01S0122
*    PROC   OPTIONS(DONTSAVE,NOSAVEAREA);  /* NO SAVE AREA           */
         B     @EL00001                                         01S0122
TINIT8   DS    0H                                               01S0123
*    RESPECIFY (R14) RSTD;                                      01S0123
*                                                               01S0123
*    CALLSWS = CALLSWS&&CALLSWS;    /* CLEAR FLAGS                   */
         XC    CALLSWS(1,WRKAPTR1),CALLSWS(WRKAPTR1)            01S0124
*    CDCBPTR=ADDR(CDCBSP);          /* SET PTR TO CALL DCB           */
         LA    @10,CDCBSP(,WRKAPTR1)                            01S0125
         ST    @10,CDCBPTR(,WRKAPTR1)                           01S0125
*    CALLDCB = CALLDCBM;            /* INITIALIZE DCB                */
         MVC   CALLDCB(52,WRKAPTR1),CALLDCBM                    01S0126
*    GENERATE;                                                  01S0127
           MVC   OPENPL(OPENPLL,WRKAPTR1),OPENPLM
           MVC   CLOSEPL(CLOSPLL,WRKAPTR1),CLOSPLM
*    PPL(1:12) = IOPL;              /* PARSE PARM LIST               */
         L     @10,PPLPTR(,WRKAPTR1)                            01S0128
         MVC   PPL(12,@10),IOPL(SRPARMLP)                       01S0128
*    DAPL(1:12) = IOPL;             /* DAIR PARM LIST                */
         L     @06,DAPLPTR(,WRKAPTR1)                           01S0129
         MVC   DAPL(12,@06),IOPL(SRPARMLP)                      01S0129
*    PPLPCL = PCLADCN;              /* PTR TO PCL IN PARSE PARM LIST */
         L     @02,PCLADCN                                      01S0130
         ST    @02,PPLPCL(,@10)                                 01S0130
*    PPLANS = ADDR(PDLADDR);        /* PTR TO ANSWER SPACE FOR PARSE */
         LA    @02,PDLADDR(,WRKAPTR1)                           01S0131
         ST    @02,PPLANS(,@10)                                 01S0131
*    PPLUWA = ZERO;                 /* CLEAR                         */
         SLR   @02,@02                                          01S0132
         ST    @02,PPLUWA(,@10)                                 01S0132
*    DAPLPSCB = PSCBPTR;            /* PTR TO PSCB IN DAIR PARM LIST */
         L     @05,PSCBPTR(,WRKAPTR2)                           01S0133
         ST    @05,DAPLPSCB(,@06)                               01S0133
*    DAPLDAPB=ADDR(DAPBSP);         /* ADDRESSABILITY TO DAIR PB     */
         LA    @05,DAPBSP(,WRKAPTR1)                            01S0134
         ST    @05,DAPLDAPB(,@06)                               01S0134
*    RESPECIFY (DAPB08) BASED(DAPLDAPB);                        01S0135
*    TMPCECB = ZERO;                /* INITIALIZE USER ECB           */
         ST    @02,TMPCECB(,WRKAPTR1)                           01S0136
*    R3SAVE = ZERO;                 /* INITIALIZE IKJRLSA POINTER    */
         ST    @02,R3SAVE(,WRKAPTR1)                            01S0137
*    CALWKA=CALWKA&&CALWKA;         /* INITIALIZE/CLEAR WORK AREA    */
         XC    CALWKA(256,WRKAPTR1),CALWKA(WRKAPTR1)            01S0138
*    CALWKB=CALWKB&&CALWKB;         /* INITIALIZE/CLEAR WORK AREA    */
         XC    CALWKB(4,WRKAPTR1),CALWKB(WRKAPTR1)              01S0139
*    PPLCBUF = TPLCBUF;             /* SET PTR TO CMD BUFFER         */
         L     @06,TPLCBUF(,WRKAPTR1)                           01S0140
         ST    @06,PPLCBUF(,@10)                                01S0140
*    R1 = PPLPTR;                   /* SET CMD SCAN PARM LIST PTR    */
*                                                               01S0141
         LR    R1,@10                                           01S0141
*    END TINIT8;                                                01S0142
@EL00002 DS    0H                                               01S0142
@EF00002 DS    0H                                               01S0142
@ER00002 BR    @14                                              01S0142
*    RESPECIFY (R14) UNRSTD;                                    01S0143
*/* * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * * */
*/*                                                                  */
*/* SEGMENT NAME -- DOSETCP                                          */
*/*                                                                  */
*/* DESCRIPTIVE NAME -- N/A                                          */
*/*                                                                  */
*/* FUNCTION -- TO ESTABLISH AND ALLOCATE DS FOR CALLED CP           */
*/*  OPERATION --                                                    */
*/*           - PROCESS DATA SET NAME                                */
*/*           - IF MEMBER NAME NOT ENTERED, ISSUE TEMPNAME MESSAGE   */
*/*             AND RELEASE PRIOR PDL IF NECESSARY                   */
*/*           - ALLOCATE CALLED DATA SET                             */
*/*           - IF DATA SET NOT FOUND, PROMPT FOR MEMBER NAME        */
*/*           - SET ALLOCATION FAILURE INDICATOR IF DAIRRC NOT ZERO  */
*/*                                                                  */
*/* ENTRY -- LINKED BY REGISTER 14                                   */
*/*                                                                  */
*/* INPUT -- PTR TO PPL ANS PLACE IN REGISTER 3                      */
*/*                                                                  */
*/* OUTPUT -- TEMPNAME MESSAGE ISSUED IF MEMBER NAME NOT ENTERED     */
*/*                                                                  */
*/* EXTERNAL REFERENCES -- N/A                                       */
*/*                                                                  */
*/* EXIT-NORMAL --  RETURN BY LINK REGISTER 14                       */
*/*                 MSGNO = 0                                        */
*/*                                                                  */
*/*   RETURN CODE -- N/A                                             */
*/*                                                                  */
*/* EXIT-ERROR -- RETURN BY LINK REGISTER 14                         */
*/*                                                                  */
*/*   CONDITIONS -- ALLOCATION UNSUCCESSFUL (MSGNO = 99)             */
*/*              -- DATA SET TYPE ERROR (MSGNO = 11)                 */
*/*   RETURN CODE -- N/A                                             */
*/*                                                                  */
*/* * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * * */
*DOSETCP:   /* ESTABLISH AND ALLOCATE DS FOR CALLED CP               */
*                                                               01S0144
*    PROC   OPTIONS(DONTSAVE,NOSAVEAREA);  /* NO SAVE AREA           */
*                                                               01S0144
DOSETCP  DS    0H                                               01S0145
*    /* IF NO MEMBER NAME IS CURRENT, ISSUE TEMPNAME MSG             */
*    IF GMBRNOW = YES               /* CHK IF MEMBER NAME ENTERED    */
*      THEN DO;                     /* NO,                           */
         TM    GMBRNOW(WRKAPTR1),B'00000100'                    01S0145
         BNO   @RF00145                                         01S0145
*        MBRSTXT=TEMPMBNM;          /* INITIALIZE TO TEMPNAME        */
         MVC   MBRSTXT(8,WRKAPTR1),TEMPMBNM                     01S0147
*        ACTSL = CM61SL;            /* INITIALIZE SEGMENT LIST       */
         L     @14,@CA00133                                     01S0148
         MVC   ACTSL(112,WRKAPTR1),CM61SL-CM61SL(@14)           01S0148
*        R4 = PTPBPTR;              /* SET PUTLINE PARM BLOCK PTR    */
         L     R4,PTPBPTR(,WRKAPTR1)                            01S0149
*        R1 = IOPLPTR;              /* SET PUTLINE PARM LIST PTR     */
         L     R1,IOPLPTR(,WRKAPTR1)                            01S0150
*        R15 = ADDR(DOPUTL);        /* PTR TO INVOKE-PUTLINE ROUTINE */
         LA    R15,DOPUTL                                       01S0151
*        GEN FLOWS(DOPUTL) (SYNCH  (R15));  /* TO PUTLINE            */
         SYNCH  (R15)
*        BLANKB = YES;              /* INDICATE MEMBR NAME PROCESSED */
*        GMBRNOW = NO;              /* RESET GET MEMBER NOW FLAG     */
         OI    BLANKB(WRKAPTR1),B'00010000'                     01S0154
         NI    GMBRNOW(WRKAPTR1),B'11111011'                    01S0154
*      END;                                                     01S0155
*                                                               01S0155
*    CALL DODSNAM;                  /* TO PROCESS DATA SET NAME      */
@RF00145 BAL   @14,DODSNAM                                      01S0156
*DOSTCPA:                           /* FOR PSEUDO-RETURN             */
*    IF DORELS = YES                /* CHK TO RELEASE PDL NOW        */
*      THEN DO;                     /* YES,                          */
DOSTCPA  TM    DORELS(WRKAPTR1),B'00001000'                     01S0157
         BNO   @RF00157                                         01S0157
*        R3=ADDR(PDLADDR2);         /* PTR TO PARSE PARM LIST        */
         LA    R3,PDLADDR2(,WRKAPTR1)                           01S0159
*        GEN(IKJRLSA  (3));         /* INVOKE PARSE RELEASE          */
         IKJRLSA  (3)
*        DORELS = NO;               /* CLEAR RELEASE-PDL-NOW FLAG    */
         NI    DORELS(WRKAPTR1),B'11110111'                     01S0161
*        R3 = R3SAVE;               /* RESTORE PTR TO PRIOR PDL      */
         L     R3,R3SAVE(,WRKAPTR1)                             01S0162
*      END;                                                     01S0163
*                                                               01S0163
*    IF MSGNO = ZERO                /* CHK FOR TERMINATION           */
*      THEN DO;                     /* NO, CONTINUE                  */
@RF00157 L     @14,MSGNO(,WRKAPTR1)                             01S0164
         LTR   @14,@14                                          01S0164
         BNZ   @RF00164                                         01S0164
*        RESPECIFY (R2,R4) RSTD;                                01S0166
*        R2 = DAPLPTR;              /* SET PTR FOR DAIR PARM LIST    */
         L     R2,DAPLPTR(,WRKAPTR1)                            01S0167
*        R4 = CPPLPTR;              /* SET PTR FOR CMD SCAN PL       */
         L     R4,CPPLPTR(,WRKAPTR1)                            01S0168
*        R15 = ADDR(DODAIR);        /* PTR TO INVOKE-DYNALLOC RTN    */
         LA    R15,DODAIR                                       01S0169
*        GEN FLOWS(DODAIR) (SYNCH  (R15));  /* TO DYNALLOC           */
         SYNCH  (R15)
*        RESPECIFY (R2,R4) UNRSTD;                              01S0171
*        IF MSGNO = ZERO            /* CHK IF ERROR OCCURRED         */
*          THEN DO;                 /* NO,                           */
         L     @14,MSGNO(,WRKAPTR1)                             01S0172
         LTR   @14,@14                                          01S0172
         BNZ   @RF00172                                         01S0172
*            IF DAIRRC = D8         /* CHK FOR DATA-SET-NOT-FOUND    */
*              THEN DO;             /* YES,                          */
         CLC   DAIRRC(4,WRKAPTR1),@CF00038                      01S0174
         BNE   @RF00174                                         01S0174
*                CALL DOGETDS;      /* GO TO PROMPT FOR DSNAME       */
         BAL   @14,DOGETDS                                      01S0176
*DOSTCPB:      END;                 /* FOR PSEUDO-RETURN             */
