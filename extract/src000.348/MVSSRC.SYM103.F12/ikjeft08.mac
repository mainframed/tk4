         MVI   CATCHL24(WRKAPTR1),X'40'                         01S0273
*          CATCHL26 = LENGTH(CATCHL); /* LENGTH OF LIST */      01S0274
         MVC   CATCHL26(2,WRKAPTR1),@CH00899                    01S0274
*          RFY (GPR01P,GPR15P) RSTD;                            01S0275
