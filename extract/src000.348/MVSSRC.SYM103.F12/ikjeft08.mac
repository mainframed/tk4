*/* * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * * */
*/*                                                                  */
*/* SEGMENT NAME -- DODAIR                                           */
*/*                                                                  */
*/* DESCRIPTIVE NAME -- N/A                                          */
*/*                                                                  */
*/* FUNCTION -- TO ALLOCATE THE SPECIFIED DATA SET                   */
*/*   OPERATION --                                                   */
*/*           - SAVE ADDRESS OF DAIR PARM LIST, PSCB AND DSNAME      */
*/*             BUFFER                                               */
*/*           - BLANK DDN, UNIT AND SERIAL                           */
*/*           - ZERO RETURN CODES, PARSE PARAMETERS, STATUS AND      */
*/*             DISPOSITION                                          */
*/*           - SET STATUS OF SHR                                    */
*/*           - SET DISPOSITION OF KEEP                              */
*/*           - PERFORM SEGMENT DODAIR TO ALLOCATE DATA SET          */
*/*           - SAVE DAIR RETURN CODE                                */
*/*           - IF DAIR RETURN CODE IS ZERO, DETERMINE IF DS IS A    */
*/*             PARTITIONED DATA SET:                                */
*/*                  YES - PLACE DDNAME (FROM DAIR) IN DCB           */
*/*                  NO - SET UP DS-TYPE-ERROR MESSAGE               */
*/*                                                                  */
*/* ENTRY -- LINKED BY REGISTER 14                                   */
*/*                                                                  */
*/* INPUT -- N/A                                                     */
*/*                                                                  */
*/* OUTPUT -- DAIR RETURN CODE                                       */
*/*                                                                  */
*/* EXTERNAL REFERENCES -- N/A                                       */
*/*                                                                  */
*/* EXIT-NORMAL -- RETURN VIA SVC 3                                  */
*/*                MSGNO = 0                                         */
*/*                                                                  */
*/*  RETURN CODE -- DAIRRC = 0                                       */
*/*                                                                  */
*/* EXIT-ERROR -- RETURN VIA SVC 3                                   */
*/*                                                                  */
*/*   CONDITIONS -- DATA-SET-TYPE ERROR (MSGNO = 11)                 */
*/*   RETURN CODE -- DAIRRC > 0                                      */
*/*                                                                  */
*/* * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * * */
*DODAIR:    /* DETERMINE WHETHER THE DATA SET IS CURRENTLY ALLOCATED */
*                                                               01S0404
*    ENTRY;                                                     01S0404
DODAIR   DS    0H                                               01S0405
*    RESPECIFY (R4) RSTD;                                       01S0405
*                                                               01S0405
*    RESPECIFY (DAPL) BASED(R2);                                01S0406
*    RESPECIFY (CPPL) BASED(R4);                                01S0407
*    DA08CD = '0008'X;              /* OPERATION CODE                */
         L     @14,DAPLDAPB(,R2)                                01S0408
         MVC   DA08CD(2,@14),@CB02660                           01S0408
*    DAPLPSCB = CPPLPSCB;           /* PTR TO PSCB                   */
         L     @10,CPPLPSCB(,R4)                                01S0409
         ST    @10,DAPLPSCB(,R2)                                01S0409
*    DA08PDSN=ADDR(DSNBUFFR);       /* ADDRESS OF DSNAME BUFFER      */
         LA    @10,DSNBUFFR(,WRKAPTR1)                          01S0410
         ST    @10,DA08PDSN(,@14)                               01S0410
*    BLANK1 = ' ';                  /* BLANK DDN, UNIT AND SERIAL    */
         MVI   BLANK1+1(@14),C' '                               01S0411
         MVC   BLANK1+2(22,@14),BLANK1+1(@14)                   01S0411
         MVI   BLANK1(@14),C' '                                 01S0411
*    ZERO1A=ZERO1A&&ZERO1A;         /* ZERO RETURN CODES             */
         XC    ZERO1A(6,@14),ZERO1A(@14)                        01S0412
*    ZERO1=ZERO1 &&ZERO1;           /* ZERO PARSE PARAMETERS         */
         XC    ZERO1(16,@14),ZERO1(@14)                         01S0413
*    ZERO2=ZERO2 &&ZERO2;           /* ZERO STATUS, DISPOSITION      */
         XC    ZERO2(3,@14),ZERO2(@14)                          01S0414
*    DA08SHR = YES;                 /* STATUS OF SHR                 */
         OI    DA08SHR(@14),B'00001000'                         01S0415
*    DA08KEEP= YES;                 /* DISPOSITION OF KEEP           */
*                                                               01S0416
         OI    DA08KEEP(@14),B'00001000'                        01S0416
*    R1 = ADDR(DAPL);               /* PTR TO SERVICE RTN PB         */
         LR    R1,R2                                            01S0417
*    GEN(LINK EP=IKJEFD00);         /* TO DAIR FOR ALLOCATION OF DS  */
         LINK EP=IKJEFD00
*    DAIRRC = R15;                  /* SAVE DAIR RETURN CODE         */
*                                                               01S0419
         ST    R15,DAIRRC(,WRKAPTR1)                            01S0419
*    IF DAIRRC = ZERO               /* CHK FOR SUCCESSFUL ALLOCATION */
*      THEN DO;                     /* YES                           */
         SLR   @14,@14                                          01S0420
         L     @10,DAIRRC(,WRKAPTR1)                            01S0420
         CLR   @10,@14                                          01S0420
         BNE   @RF00420                                         01S0420
*        IF (DA08DSO & PODS) ^= PODS/* IS DATASET NOT PDS    @ZA14237*/
*            & MSGNO = ZERO         /*     & ANY TERMINATION FLAG    */
*          THEN                     /* NO,                           */
         L     @10,DAPLDAPB(,R2)                                01S0422
         MVC   @TS00001(1),DA08DSO(@10)                         01S0422
         NI    @TS00001,X'02'                                   01S0422
         CLI   @TS00001,X'02'                                   01S0422
         BE    @RF00422                                         01S0422
         C     @14,MSGNO(,WRKAPTR1)                             01S0422
         BNE   @RF00422                                         01S0422
*            MSGNO = D11;           /* DATA SET TYPE ERROR           */
         MVC   MSGNO(4,WRKAPTR1),@CF00066                       01S0423
*                                   /*      DAIR IN THE DCB          */
*      END;                                                     01S0424
@RF00422 DS    0H                                               01S0425
*    GEN(SVC    3);                  /* RETURN TO CALLER             */
@RF00420 DS    0H                                               01S0425
         SVC    3
*    RESPECIFY (DAPL) BASED(DAPLPTR);                           01S0426
*    RESPECIFY (CPPL) BASED(CPPLPTR);                           01S0427
*    RESPECIFY (R4) UNRSTD;                                     01S0428
*    RETURN;                         /* TERMINATE ROUTINE            */
         B     @EL00004                                         01S0429
*/* * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * * */
*/*                                                                  */
*/* SEGMENT NAME -- DOUNALC                                          */
*/*                                                                  */
*/* DESCRIPTIVE NAME -- N/A                                          */
*/*                                                                  */
*/* FUNCTION -- TO UNALLOCATE THE SPECIFIED DATA SET                 */
*/*   OPERATION --                                                   */
*/*           - SAVE ADDRESS OF DAIR PARM LIST, PSCB AND DSNAME      */
*/*             BUFFER                                               */
*/*           - BLANK DDN, UNIT AND SERIAL                           */
*/*           - ZERO RETURN CODES, PARSE PARAMETERS, STATUS AND      */
*/*             DISPOSITION                                          */
*/*           - SET STATUS OF SHR                                    */
*/*           - SET DISPOSITION OF KEEP                              */
*/*           - PERFORM SEGMENT DODAIR TO ALLOCATE DATA SET          */
*/*           - SAVE DAIR RETURN CODE                                */
*/*                                                                  */
*/* ENTRY -- LINKED BY REGISTER 14                                   */
*/*                                                                  */
*/* INPUT -- N/A                                                     */
*/*                                                                  */
*/* OUTPUT -- DAIR RETURN CODE                                       */
*/*                                                                  */
*/* EXTERNAL REFERENCES -- N/A                                       */
*/*                                                                  */
*/* EXIT-NORMAL -- RETURN VIA SVC 3                                  */
*/*                MSGNO = 0                                         */
*/*                                                                  */
*/*  RETURN CODE -- DAIRRC = 0                                       */
*/*                                                                  */
*/* EXIT-ERROR -- RETURN VIA SVC 3                                   */
*/*                                                                  */
*/*   CONDITIONS -- N/A                                              */
*/*   RETURN CODE -- DAIRRC > 0                                      */
*/*                                                                  */
*/* * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * * */
*DOUNALC:   /* UNALLOCATE THE CURRENTLY ALLOCATED SPECIFIED DATA SET */
*                                                               01S0430
*    ENTRY;                                                     01S0430
DOUNALC  DS    0H                                               01S0431
*    RESPECIFY (R4) RSTD;                                       01S0431
*                                                               01S0431
*    RESPECIFY (DAPL) BASED(R2);                                01S0432
*    RESPECIFY (CPPL) BASED(R4);                                01S0433
*    DAPBSP = DAPBSP&&DAPBSP;       /* CLEAR PARM BLOCK              */
         XC    DAPBSP(84,WRKAPTR1),DAPBSP(WRKAPTR1)             01S0434
*    DAPL = IOPL;                   /* INITIALIZE PARM LIST          */
         MVI   DAPL+16(R2),C' '                                 01S0435
         MVC   DAPL+17(3,R2),DAPL+16(R2)                        01S0435
         MVC   DAPL(16,R2),IOPL(SRPARMLP)                       01S0435
*    DAPLPSCB = PSCBPTR;            /* SET PTR TO CURRENT PSCB       */
         L     @14,PSCBPTR(,WRKAPTR2)                           01S0436
         ST    @14,DAPLPSCB(,R2)                                01S0436
*    DAPLDAPB = ADDR(DAPBSP);       /* SET PTR TO PARM BLOCK IN PL   */
         LA    @14,DAPBSP(,WRKAPTR1)                            01S0437
         ST    @14,DAPLDAPB(,R2)                                01S0437
*    RESPECIFY (DAPB18) BASED(DAPLDAPB);                        01S0438
*                                                               01S0438
*    IF SWDOQ = YES THEN            /* D.S. QUALF. FOR ALLOC @ZA20551*/
         TM    SWDOQ(WRKAPTR1),B'00100000'                      01S0439
         BNO   @RF00439                                         01S0439
*     DA18UID = YES;                /* YES-SET FLAG TO FULLY @ZA20551*/
         OI    DA18UID(@14),B'00100000'                         01S0440
*                                   /* QUALIFY DS AT UNALLOC @ZA20551*/
*    DA18CD = '0018'X;              /* OPERATION CODE                */
@RF00439 L     @14,DAPLDAPB(,R2)                                01S0441
         MVC   DA18CD(2,@14),@CB02669                           01S0441
*    DA18PDSN=ADDR(DSNBUFFR);       /* ADDRESS OF DSNAME BUFFER      */
         LA    @10,DSNBUFFR(,WRKAPTR1)                          01S0442
         ST    @10,DA18PDSN(,@14)                               01S0442
*    BLANK2 = ' ';                  /* BLANK DDN, UNIT AND SERIAL    */
*                                                               01S0443
         MVI   BLANK2+1(@14),C' '                               01S0443
         MVC   BLANK2+2(16,@14),BLANK2+1(@14)                   01S0443
         MVI   BLANK2(@14),C' '                                 01S0443
*    R1 = ADDR(DAPL);               /* PTR TO SERVICE RTN PB         */
         LR    R1,R2                                            01S0444
*    GEN(LINK EP=IKJEFD00);         /* TO DAIR FOR ALLOCATION OF DS  */
         LINK EP=IKJEFD00
*    DAIRRC = R15;                  /* SAVE DAIR RETURN CODE         */
*                                                               01S0446
         ST    R15,DAIRRC(,WRKAPTR1)                            01S0446
*    GEN(SVC    3);                  /* RETURN TO CALLER             */
         SVC    3
*    RESPECIFY (DAPL) BASED(DAPLPTR);                           01S0448
*    RESPECIFY (CPPL) BASED(CPPLPTR);                           01S0449
*    RESPECIFY (R4) UNRSTD;                                     01S0450
*    RETURN;                         /* TERMINATE ROUTINE            */
         B     @EL00004                                         01S0451
*/* * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * * */
*/*                                                                  */
*/* SEGMENT NAME -- DOGETDS                                          */
*/*                                                                  */
*/* DESCRIPTIVE NAME -- N/A                                          */
*/*                                                                  */
*/* FUNCTION -- TO PROMPT FOR DATA SET NAME IF DATA SET NAME IS NOT  */
*/*             FOUND                                                */
*/*   OPERATION --                                                   */
*/*           - IF DATA SET NOT FOUND, ISSUE DS-NOT-FOUND MESSAGE    */
*/*             OTHERWISE, SET UP PARSE-ERROR MESSAGE                */
*/*           - SET UP PARAMETERS FOR PARSE                          */
*/*           - CALL SEGMENT DOPARS TO SYNTAX CHECK THE RESPONSE     */
*/*           - IF PARSRC IS ZERO:                                   */
*/*             - IF MBRPRES IS OFF, TURN ON GET-MEMBER-NOW FLAG     */
*/*                                  AND MEMBER-NAME-PROCESSED FLAG  */
*/*           - IF PARSRC IS NOT ZERO, TURN OFF MEMBER-NAME-PRO-     */
*/*             CESSED FLAG                                          */
*/*                                                                  */
*/* ENTRY -- LINKED BY REGISTER 14                                   */
*/*                                                                  */
*/* INPUT -- N/A                                                     */
*/*                                                                  */
*/* OUTPUT -- ISSUES DS-NOT-FOUND MESSAGE, IF NECESSARY              */
*/*                                                                  */
*/* EXTERNAL REFERENCES -- N/A                                       */
*/*                                                                  */
*/* EXIT-NORMAL -- RETURN BY LINK REGISTER 14                        */
*/*                                                                  */
*/*   RETURN CODE -- N/A                                             */
*/*                                                                  */
*/* EXIT-ERROR -- RETURN BY LINK REGISTER 14                         */
*/*                                                                  */
*/*   CONDITIONS -- DATA SET NOT FOUND                               */
*/*              -- PARSE ERROR (MSGNO = 0)                          */
*/*  RETURN CODE -- N/A                                              */
*/*                                                                  */
*/* * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * * */
*DOGETDS:   /* DATA SET NOT FOUND, RETURN TO PARSE FOR DATA SET NAME */
*                                                               01S0452
*    ENTRY;                                                     01S0452
*                                                               01S0452
DOGETDS  DS    0H                                               01S0453
*    IF DA08CTRC >= '0008'X         /* CHK IF DATA SET NAME FOUND    */
*      THEN DO;                     /* NO, INVOKE PARSE FOR PROMPT   */
         L     @14,DAPLPTR(,WRKAPTR1)                           01S0453
         L     @14,DAPLDAPB(,@14)                               01S0453
         LH    @14,DA08CTRC(,@14)                               01S0453
         C     @14,@CF02660                                     01S0453
         BL    @RF00453                                         01S0453
*        ACTSL = CM41SL;            /* INITIALIZE SEGMENT LIST       */
         L     @14,@CA00131                                     01S0455
         MVC   ACTSL(112,WRKAPTR1),CM41SL-CM41SL(@14)           01S0455
*        DSSGOFF = D10;             /* DATA SET OFFSET FOR MESSAGE   */
         MVC   DSSGOFF(2,WRKAPTR1),@CH00064                     01S0456
*        ACTSEG(5)=ADDR(DSSEG);     /* PTR TO DATA SET NAME          */
         LA    @14,DSSEG(,WRKAPTR1)                             01S0457
         ST    @14,ACTSEG+16(,WRKAPTR1)                         01S0457
*        R4 = PTPBPTR;              /* SET PUTLINE PARM BLOCK PTR    */
         L     R4,PTPBPTR(,WRKAPTR1)                            01S0458
*        R1 = IOPLPTR;              /* SET PUTLINE PARM LIST PTR     */
         L     R1,IOPLPTR(,WRKAPTR1)                            01S0459
*        R15 = ADDR(DOPUTL);        /* PTR TO INVOKE-PUTLINE ROUTINE */
         LA    R15,DOPUTL                                       01S0460
*        GEN FLOWS(DOPUTL) (SYNCH  (R15));  /* TO PUTLINE            */
         SYNCH  (R15)
*      END;                                                     01S0462
*    IF MSGNO = ZERO                /* CHK FOR TERMINATION FLAG      */
*      THEN DO;                     /* NO, CONTINUE                  */
@RF00453 L     @14,MSGNO(,WRKAPTR1)                             01S0463
         LTR   @14,@14                                          01S0463
         BNZ   @RF00463                                         01S0463
*        PPLPCL = PCL1ADCN;         /* PUT ADDRESS OF PCL INTO LIST  */
         L     @14,PPLPTR(,WRKAPTR1)                            01S0465
         L     @10,PCL1ADCN                                     01S0465
         ST    @10,PPLPCL(,@14)                                 01S0465
*        PPLANS=ADDR(PDLADDR2);     /* ADDRESS OF ANSWER PLACE       */
         LA    @10,PDLADDR2(,WRKAPTR1)                          01S0466
         ST    @10,PPLANS(,@14)                                 01S0466
*        R1 = PPLPTR;               /* SET CMD SCAN PARM LIST PTR    */
         LR    R1,@14                                           01S0467
*        R15 = ADDR(DOPARS);        /* PTR TO INVOKE-PARSE ROUTINE   */
         LA    R15,DOPARS                                       01S0468
*        GEN FLOWS(DOPARS) (SYNCH  (R15));  /* TO INVOKE PARSE       */
         SYNCH  (R15)
*        IF PARSRC = ZERO           /* CHECK FOR SUCCESSFUL PARSE    */
*          THEN DO;                                             01S0470
         L     @14,PARSRC+1236(,WRKAPTR1)                       01S0470
         LTR   @14,@14                                          01S0470
         BNZ   @RF00470                                         01S0470
*            DORELS = YES;          /* SET DO-PDL-RELEASE-NOW FLAG   */
         OI    DORELS(WRKAPTR1),B'00001000'                     01S0472
*            R3SAVE = R3;           /* RESERVE CURRENT PDL PTR       */
         ST    R3,R3SAVE(,WRKAPTR1)                             01S0473
*            R3 = PDLADDR2;         /* PTR TO NEW DSNAME PDL         */
         L     R3,PDLADDR2(,WRKAPTR1)                           01S0474
*            IF MBRPRES = YES       /* CHK IF MEMBER NAME ENTERED    */
*              THEN                 /* YES,                          */
         TM    MBRPRES(R3),B'10000000'                          01S0475
         BNO   @RF00475                                         01S0475
*                BLANKB = NO;       /*   SET MEMBER-GOTTEN FLAG      */
         NI    BLANKB(WRKAPTR1),B'11101111'                     01S0476
*              ELSE                 /* MEMBER NAME NOT ENTERED,      */
*                GMBRNOW = YES;     /*   SET DO-PARSE-PROMPT FLAG    */
         B     @RC00475                                         01S0477
@RF00475 OI    GMBRNOW(WRKAPTR1),B'00000100'                    01S0477
*          END;                                                 01S0478
*                                                               01S0478
*          ELSE                                                 01S0479
*            IF MSGNO = ZERO        /* CHK FOR TERMINATION FLAG      */
*              THEN                 /* NO, CONTINUE                  */
         B     @RC00470                                         01S0479
@RF00470 L     @14,MSGNO(,WRKAPTR1)                             01S0479
         LTR   @14,@14                                          01S0479
         BNZ   @RF00479                                         01S0479
*                MSGNO = D1;        /* INDICATE PARSE FAILURE        */
         MVC   MSGNO(4,WRKAPTR1),@CF00032                       01S0480
*      END;                                                     01S0481
*                                                               01S0481
@RF00479 DS    0H                                               01S0481
@RC00470 DS    0H                                               01S0482
*    RESPECIFY (R14) RSTD;                                      01S0482
@RF00463 DS    0H                                               01S0483
*    R14 = ADDR(DOSTCPB);           /* SET FOR PSEUDO-RETURN         */
         LA    R14,DOSTCPB                                      01S0483
*    RETURN;                        /* RETURN TO CALLER              */
         B     @EL00004                                         01S0484
*    RESPECIFY (R14) UNRSTD;                                    01S0485
*/* * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * * */
*/*                                                                  */
*/* SEGMENT NAME -- DOGETM                                           */
*/*                                                                  */
*/* DESCRIPTIVE NAME --N/A                                           */
*/*                                                                  */
*/* FUNCTION -- TO ISSUE BLDL                                        */
*/*   OPERATION --                                                   */
*/*           - IF MEMBER NOT FOUND, CALL DOPUTL TO ISSUE MEMBER-    */
*/*             NOT-FOUND MESSAGE                                    */
*/*           - IF PUTLINE SUCCESSFUL : CALL DOPARS TO PROMPT FOR   */
*/*                                      MEMBER NAME                */
*/*                        UNSUCCESSFUL : SET ERROR TERMINATION FLAG */
*/*           - IF PARSE SUCCESSFUL : FREE OLD PDL SPACE             */
*/*                      UNSUCCESSFUL : SET ERROR FLAG               */
*/*           - SAVE BLDL RETURN CODE                                */
*/*                                                                  */
*/* ENTRY -- LINKED BY REGISTER 14                                   */
*/*                                                                  */
*/* INPUT -- N/A                                                      *
*/*                                                                  */
*/* OUTPUT -- BLDL RETURN CODE                                       */
*/*                                                                  */
*/* EXTERNAL REFERENCES -- N/A                                       */
*/*                                                                  */
*/* EXIT-NORMAL -- RETURN BY LINK REGISTER 14                        */
*/*                                                                  */
*/* RETURN CODE -- BLDLRC = 0                                        */
*/*                                                                  */
*/* EXIT-ERROR -- RETURN BY LINK REGISTER 14                         */
*/*                                                                  */
*/*   CONDITIONS -- BLDL FAILURE                                     */
*/*   RETURN CODE -- BLDLRC > 0                                      */
*/*                                                                  */
*/* * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * * */
*DOGETM:    /* MEMBER NAME NON-EXISTANT, PROMPT FOR MEMBER NAME      */
*                                                               01S0486
*    ENTRY;                                                     01S0486
*                                                               01S0486
DOGETM   DS    0H                                               01S0487
*    ACTSL = CM51SL;                /* INITIALIZE SEGMENT LIST       */
         L     @14,@CA00132                                     01S0487
         MVC   ACTSL(112,WRKAPTR1),CM51SL-CM51SL(@14)           01S0487
*    MBRSOFF = D10;                 /* MEMBER OFFSET FOR MESSAGE     */
         LA    @14,10                                           01S0488
         STH   @14,MBRSOFF(,WRKAPTR1)                           01S0488
*    DSSGOFF = D10;                 /* DATA SET OFFSET FOR MESSAGE   */
         STH   @14,DSSGOFF(,WRKAPTR1)                           01S0489
*    ACTSEG(5) = ADDR(MBRSEG);      /* PTR TO MEMBERNAME             */
         LA    @14,MBRSEG(,WRKAPTR1)                            01S0490
         ST    @14,ACTSEG+16(,WRKAPTR1)                         01S0490
*    ACTSEG(7) = ADDR(DSSEG);       /* PTR TO DS NAME                */
         LA    @14,DSSEG(,WRKAPTR1)                             01S0491
         ST    @14,ACTSEG+24(,WRKAPTR1)                         01S0491
*    R4 = PTPBPTR;                  /* SET PUTLINE PARM BLOCK PTR    */
         L     R4,PTPBPTR(,WRKAPTR1)                            01S0492
*    R1 = IOPLPTR;                  /* SET PUTLINE PARM LIST PTR     */
         L     R1,IOPLPTR(,WRKAPTR1)                            01S0493
*    R15 = ADDR(DOPUTL);            /* PTR TO INVOKE-PUTLINE ROUTINE */
         LA    R15,DOPUTL                                       01S0494
*    GEN FLOWS(DOPUTL) (SYNCH  (R15));  /* TO INVOKE PUTLINE         */
         SYNCH  (R15)
*    RESPECIFY (R4) UNRSTD;                                     01S0496
*    MBRSTXT = '';                  /* CLEAR MEMBER NAME BUFFER      */
         MVI   MBRSTXT(WRKAPTR1),C' '                           01S0497
         MVC   MBRSTXT+1(7,WRKAPTR1),MBRSTXT(WRKAPTR1)          01S0497
*    PPLPCL = PCL2ADCN;             /* PTR TO PCL IN PL              */
         L     @14,PPLPTR(,WRKAPTR1)                            01S0498
         L     @10,PCL2ADCN                                     01S0498
         ST    @10,PPLPCL(,@14)                                 01S0498
*    PPLANS = ADDR(PDLADDR2);       /* PTR TO ANSWER                 */
         LA    @10,PDLADDR2(,WRKAPTR1)                          01S0499
         ST    @10,PPLANS(,@14)                                 01S0499
*    R1 = PPLPTR;                   /* SET CMD SCAN PARM LIST PTR    */
         LR    R1,@14                                           01S0500
*    R15 = ADDR(DOPARS);            /* PTR TO INVOKE-PARSE ROUTINE   */
         LA    R15,DOPARS                                       01S0501
*    GEN FLOWS(DOPARS) (SYNCH  (R15));  /* TO DO PARSE               */
         SYNCH  (R15)
*    IF MSGNO = ZERO                /* CHK FOR TERMINATION FLAG      */
*      THEN DO;                     /* NO, SET ERROR FLAG            */
         L     @14,MSGNO(,WRKAPTR1)                             01S0503
         LTR   @14,@14                                          01S0503
         BNZ   @RF00503                                         01S0503
*        MBRSLEN=MPDLLEN+D4;        /* SET SEGMENT LENGTH            */
         L     @14,PDLADDR2(,WRKAPTR1)                          01S0505
         LH    @10,MPDLLEN(,@14)                                01S0505
         LA    @06,4                                            01S0505
         ALR   @06,@10                                          01S0505
         STH   @06,MBRSLEN(,WRKAPTR1)                           01S0505
*        MBRSTXT(1:MPDLLEN)=        /* INSERT MEMBER NAME IN         */
*        MPDLBUF(1:MPDLLEN);        /*   MESSAGE SEGMENT             */
         BCTR  @10,0                                            01S0506
         L     @14,MPDLPTR(,@14)                                01S0506
         EX    @10,@SM02747                                     01S0506
*        RESPECIFY (R2) RESTRICTED;                             01S0507
*        R2 = ADDR(PDLADDR2);       /* PTR TO PDL                    */
         LA    R2,PDLADDR2(,WRKAPTR1)                           01S0508
*        GEN(IKJRLSA  (2));         /* FREE OLD PDL SPACE            */
         IKJRLSA  (2)
*        RESPECIFY (R2) UNRESTRICTED;                           01S0510
*      END;                                                     01S0511
*                                                               01S0511
*    RESPECIFY (R14) RSTD;                                      01S0512
@RF00503 DS    0H                                               01S0513
*    R14 = ADDR(DOBLDLA);           /* SET FOR PSEUDO-RETURN         */
         LA    R14,DOBLDLA                                      01S0513
*    RETURN;                        /* RETURN TO CALLER              */
         B     @EL00004                                         01S0514
*    RESPECIFY (R14) UNRSTD;                                    01S0515
