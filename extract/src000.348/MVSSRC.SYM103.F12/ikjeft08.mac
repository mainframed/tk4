*/*                                                                  */
*/* SEGMENT NAME -- DOPARS                                           */
*/*                                                                  */
*/* DESCRIPTIVE NAME -- N/A                                          */
*/*                                                                  */
*/* FUNCTION -- TO SCAN AND SYNTAX CHECK THE COMMAND                 */
*/*   OPERATION --                                                   */
*/*           - SET UP POINTERS TO COMMAND BUFFER AND PARSE PL       */
*/*           - PARSE THE COMMAND                                    */
*/*           - TURN OFF PDLPRES BIT                                 */
*/*           - IF PARSRC IS ZERO, TURN ON PDLPRES BIT AND PLACE     */
*/*             PDLANSP IN REGISTER 3 (PTR TO ANS PLACE)             */
*/*                                                                  */
*/* ENTRY -- LINKED BY REGISTER 14                                   */
*/*                                                                  */
*/* INPUT --  N/A                                                    */
*/*                                                                  */
*/* OUTPUT -- N/A                                                    */
*/*                                                                  */
*/* EXTERNAL REFERENCES -- N/A                                       */
*/*                                                                  */
*/* EXIT-NORMAL --  RETURN VIA SVC 3                                 */
*/*                                                                  */
*/*   RETURN CODE -- PARSRC = 0                                      */
*/*                                                                  */
*/* EXIT-ERROR -- RETURN VIA SVC 3                                   */
*/*                                                                  */
*/*   CONDITIONS -- PARSE FAILURE                                    */
*/*   RETURN CODE -- PARSRC > 0                                      */
*/*                                                                  */
*/* * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * * */
*DOPARS:    /* CHK SYNTAX OF CALL COMMAND OPERANDS                   */
*                                                               01S0584
*    PROC OPTIONS(DONTSAVE,NOSAVEAREA);  /* NO SAVE AREA             */
DOPARS   DS    0H                                               01S0585
*    RESPECIFY (PPL) BASED(R1);                                 01S0585
*                                                               01S0585
*    IF MSGNO = ZERO                /* CHK FOR TERMINATION FLAG      */
*      THEN DO;                     /* NO, CONTINUE TO PARSE         */
         SLR   @14,@14                                          01S0586
         C     @14,MSGNO(,WRKAPTR1)                             01S0586
         BNE   @RF00586                                         01S0586
*        DO; /*CALLTSSR EP(IKJPARS)*/                           01S0588
*          RESPECIFY GPR01P RSTD;                               01S0589
*          IF CVTPARS = 0                                       01S0590
*            THEN                                               01S0590
         L     @10,CVTPTR                                       01S0590
         C     @14,CVTPARS(,@10)                                01S0590
         BNE   @RF00590                                         01S0590
*              GEN(LINK EP=IKJPARS);                            01S0591
         LINK EP=IKJPARS
*            ELSE                                               01S0592
*              CALL CVTPARS;                                    01S0592
         B     @RC00590                                         01S0592
@RF00590 L     @10,CVTPTR                                       01S0592
         L     @15,CVTPARS(,@10)                                01S0592
         BALR  @14,@15                                          01S0592
*          RESPECIFY GPR01P UNRSTD;                             01S0593
@RC00590 DS    0H                                               01S0594
*        END;     /* TO CHK DOMMAND SYNTAX         */           01S0594
*                                                               01S0594
*        PARSRC = R15;              /* RESERVE PARSE RETURN CODE     */
*                                                               01S0595
         ST    R15,PARSRC+1236(,WRKAPTR1)                       01S0595
*        PDLPRES = NO;              /* NO PDL TO RELEASE             */
         NI    PDLPRES(WRKAPTR1),B'01111111'                    01S0596
*        IF PARSRC = ZERO           /* CHK FOR SUCCESSFUL PARSE      */
*          THEN DO;                 /* YES,                          */
         L     @14,PARSRC+1236(,WRKAPTR1)                       01S0597
         LTR   @14,@14                                          01S0597
         BNZ   @RF00597                                         01S0597
*            R3 = PDLANSP;          /*   SET POINTER TO PDL SPACE    */
         L     @14,PPLANS(,R1)                                  01S0599
         L     R3,PDLANSP(,@14)                                 01S0599
*            PDLPRES = YES;         /*   AND FLAG TO DO IKJRLSA      */
         OI    PDLPRES(WRKAPTR1),B'10000000'                    01S0600
*          END;                                                 01S0601
*          ELSE                     /* INDICATE PARSE FAILURE        */
*            IF MSGNO = ZERO        /* CHK FOR TERMINATION FLAG      */
*              THEN                 /* NO,                           */
         B     @RC00597                                         01S0602
@RF00597 L     @14,MSGNO(,WRKAPTR1)                             01S0602
         LTR   @14,@14                                          01S0602
         BNZ   @RF00602                                         01S0602
*                MSGNO = D1;        /*   SET PARSE FAILURE FLAG      */
         MVC   MSGNO(4,WRKAPTR1),@CF00032                       01S0603
*      END;                                                     01S0604
*                                                               01S0604
@RF00602 DS    0H                                               01S0604
@RC00597 DS    0H                                               01S0605
*    GEN(SVC   3);                  /* RETURN VIA SVC 3              */
@RF00586 DS    0H                                               01S0605
         SVC   3
*    RESPECIFY (PPL) BASED(PPLPTR);                             01S0606
*    RETURN;                        /* TERMINATE ROUTINE             */
@EL00005 DS    0H                                               01S0607
@EF00005 DS    0H                                               01S0607
@ER00005 BR    @14                                              01S0607
*/* * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * * */
*/*                                                                  */
*/* SEGMENT NAME -- DOQUAL                                           */
*/*                                                                  */
*/* DESCRIPTIVE NAME -- N/A                                          */
*/*                                                                  */
*/* FUNCTION -- TO FULLY QUALIFY THE DS NAME                         */
*/*   OPERATION --                                                   */
*/*           - SET DAIR CONTROL FLAG                                */
*/*           - IF DSNAME IS NOT ENTERED, DSNAME IS 'DEFAULT.LOAD'   */
*/*           - IF DSNAME IS ENTERED, AND LAST QUALIFIER IS NOT      */
*/*             ALREADY '.LOAD', APPEND '.LOAD' TO DSNAME AND UPDATE */
*/*             LENGTH                                               */
*/*                                                                  */
*/* ENTRY -- LINKED BY REGISTER 14                                   */
*/*                                                                  */
*/* INPUT --  N/A                                                    */
*/*                                                                  */
*/* OUTPUT -- LENGTH OF FULLY QUALIFIED DSNAME                       */
*/*                                                                  */
*/* EXTERNAL REFERENCES -- N/A                                       */
*/*                                                                  */
*/* EXIT-NORMAL --  RETURN BY LINK REGISTER 14                       */
*/*                                                                  */
*/*   RETURN CODE -- N/A                                             */
*/*                                                                  */
*/* EXIT-ERROR -- RETURN BY LINK REGISTER 14                         */
*/*                                                                  */
*/*   CONDITIONS -- N/A                                              */
*/*   RETURN CODE -- N/A                                             */
*/*                                                                  */
*/* * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * * */
*DOQUAL:    /* ESTABLISH FULL QUALIFICATION OF DSNAME                */
*                                                               01S0608
*    ENTRY;                         /* EP TO ROUTINE                 */
DOQUAL   DS    0H                                               01S0609
*    RESPECIFY (R14) RSTD;                                      01S0609
*                                                               01S0609
*    /* SET STANDARD QUALIFICATION                                   */
*    DA08UID = YES;                 /* SET DAIR CONTROL FLAG         */
         L     @01,DAPLPTR(,WRKAPTR1)                           01S0610
         L     @01,DAPLDAPB(,@01)                               01S0610
         OI    DA08UID(@01),B'00100000'                         01S0610
*    SWDOQ = YES;                   /* SET FLAG INDICATING   @ZA20551*/
         OI    SWDOQ(WRKAPTR1),B'00100000'                      01S0611
*                                   /* DS. WAS FULLY QUALIFY @ZA20551*/
*    IF DSNLENG = ZERO              /* CHK IF DSNAME ENTERED         */
*      THEN DO;                     /* NO,                           */
         LH    @10,DSNLENG(,WRKAPTR1)                           01S0612
         LTR   @10,@10                                          01S0612
         BNZ   @RF00612                                         01S0612
*        DSNBUF = CMPRE;                                        01S0614
         MVI   DSNBUF+4(WRKAPTR1),C' '                          01S0614
         MVC   DSNBUF+5(39,WRKAPTR1),DSNBUF+4(WRKAPTR1)         01S0614
         MVC   DSNBUF(4,WRKAPTR1),CMPRE                         01S0614
*        DSNLENG = D4;                                          01S0615
         MVC   DSNLENG(2,WRKAPTR1),@CH00044                     01S0615
*        DSSGLEN = D8;              /* SET DS LENGTH FOR MSG @ZA18797*/
         MVC   DSSGLEN(2,WRKAPTR1),@CH00038                     01S0616
*        DSSGTXT = CMPRE;           /* SET DS NAME FOR MSG   @ZA18797*/
         MVI   DSSGTXT+4(WRKAPTR1),C' '                         01S0617
         MVC   DSSGTXT+5(39,WRKAPTR1),DSSGTXT+4(WRKAPTR1)       01S0617
         MVC   DSSGTXT(4,WRKAPTR1),CMPRE                        01S0617
*      END;                                                     01S0618
*      ELSE DO;                     /* DSNAME WAS ENTERED            */
         B     @RC00612                                         01S0619
@RF00612 DS    0H                                               01S0620
*        RESPECIFY (X) RSTD;                                    01S0620
*        X = ADDR(DSNBUF);                                      01S0621
         LA    X,DSNBUF(,WRKAPTR1)                              01S0621
*        IF DSNLENG > D3                                        01S0622
*          THEN                                                 01S0622
         LH    @10,DSNLENG(,WRKAPTR1)                           01S0622
         C     @10,@CF00058                                     01S0622
         BNH   @RF00622                                         01S0622
*            X = X+DSNLENG-D5;                                  01S0623
         ALR   @10,X                                            01S0623
         SL    @10,@CF00061                                     01S0623
         LR    X,@10                                            01S0623
*        IF DSNLENG ^> D3                                       01S0624
*             LAST5 ^= QUALIFR                                 01S0624
*          THEN DO;                                             01S0624
@RF00622 LH    @10,DSNLENG(,WRKAPTR1)                           01S0624
         C     @10,@CF00058                                     01S0624
         BNH   @RT00624                                         01S0624
         CLC   LAST5(5,X),QUALIFR                               01S0624
         BE    @RF00624                                         01S0624
