         SVC   8                                                01S0521
*    RESPECIFY (GPR00P,GPR01P) UNRESTRICTED;                    01S0522
*    END;       /*LOAD EP('IKJEFF18') */          /* LOAD DAIRFAIL MODU
*E          */                                                  01S0523
*    RESPECIFY (R1) UNRESTRICTED;   /* R1 CAN NOW BE USED TO         */
*                                   /* CONTAIN THE PARAMETER LIST    */
*                                   /*   ADDRESS FOR CALL            */
*                                                               01S0525
*    RESPECIFY (DAPL) BASED(R2);                                01S0525
*    CALL  IKJEFF18 (DAPL,DAIRRC,ADMSG,CALLERNO);  /* TO DAIRFAIL    */
         ST    R2,@AL00001                                      01S0526
         LA    @14,DAIRRC(,WRKAPTR1)                            01S0526
         ST    @14,@AL00001+4                                   01S0526
         LA    @14,ADMSG                                        01S0526
         ST    @14,@AL00001+8                                   01S0526
         LA    @14,CALLERNO                                     01S0526
         ST    @14,@AL00001+12                                  01S0526
         LR    @15,R0                                           01S0526
         LA    @01,@AL00001                                     01S0526
         BALR  @14,@15                                          01S0526
*    RESPECIFY (R1) RESTRICTED;     /* RESTRICT R1                   */
*    DO;/* DELETE EP(IKJEFF18)*/                                01S0528
*    RESPECIFY (GPR00P)RESTRICTED;                              01S0529
*    GPR00P = ADDR(IKJEFF18);/* ->ENTRY NAME*/                  01S0530
*    SVC    (9);/* DELETE SVC*/                                 01S0531
         SVC   9                                                01S0531
*    RESPECIFY (GPR00P)UNRESTRICTED;                            01S0532
*    END;/* DELETE EP(IKJEFF18)*/           /* DELETE DAIRFAIL MODULE
*     */                                                        01S0533
*                                                               01S0533
*    MSGNO = ZERO;                  /* SET FOR NO FURTHER MSG        */
*                                                               01S0534
         SLR   @14,@14                                          01S0534
         ST    @14,MSGNO(,WRKAPTR1)                             01S0534
*    GEN(SVC    3);                 /* RETURN VIA SVC 3              */
         SVC    3
*    RESPECIFY (R2,R14) UNRSTD;                                 01S0536
