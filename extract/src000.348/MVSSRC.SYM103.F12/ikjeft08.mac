*      RESPECIFY (R5) RSTD;         /* RESTRICT REGISTER 5           */
*      RESPECIFY (R4) RSTD;         /* RESTRICT REGISTER 4           */
*      R4 = ADDR (CALLDCB);         /* ADDRESS OF DCB                */
         LA    R4,CALLDCB(,WRKAPTR1)                            01S0212
*      R5 = ADDR (BLDLLST);         /* ADDRESS OF PARM LIST FOR BLDL */
         LA    R5,BLDLLST(,WRKAPTR1)                            01S0213
*      GENERATE (BLDL (4),(5));     /* BLDL ON LIBRARY               */
         BLDL (4),(5)
*      BLDLRC = R15;                /* SAVE BLDL RETURN CODE         */
         ST    R15,BLDLRC(,WRKAPTR1)                            01S0215
*      RESPECIFY (R4) UNRSTD;       /* UNRESTRICT REGISTER 4         */
*      RESPECIFY (R5) UNRSTD;       /* UNRESTRICT REGISTER 5         */
*      IF BLDLRC = D4               /* CHK FOR MEMBER NOT FOUND      */
*        THEN DO;                   /* YES,                          */
         L     @14,BLDLRC(,WRKAPTR1)                            01S0218
         CL    @14,@CF00044                                     01S0218
         BNE   @RF00218                                         01S0218
*          CALL  DOGETM;            /*   PROMPT FOR MEMBER NAME      */
         BAL   @14,DOGETM                                       01S0220
*DOBLDLA:                           /* FOR PSEUDO-RETURN             */
