*/* SEGMENT NAME -- TSRCHAPF                                         */
*/*                                                                  */
*/* DESCRIPTIVE NAME -- N/A                                          */
*/*                                                                  */
*/* FUNCTION -- TO DETERMINE FROM A LIST OF ELIGIBLE PROGRAM NAMES   */
*/*       IF THE CURRENT PROGRAM NAME SHOULD BE ATTACHED WITH APF    */
*/*       AUTHORIZATION                                              */
*/*   OPERATION --                                                   */
*/*           - SET ATTACH-WITH-AUTHORIZATION FLAG TO NO             */
*/*           - LOOP OVER NAMES (8 BYTE) IN EXTERNAL TABLE, APFPTABL */
*/*           - IF NAME IN LIST IS THE SAME AS THE CURRENT PROGRAM   */
*/*             NAME, THEN EXIT FROM LOOP WITH AUTHORIZATION FLAG,   */
*/*             DRSAPF, SET TO YES                                   */
*/*           - IF A BLANK IS FOUND IN LIST, THEN EXIT FROM LOOP     */
*/*           - STEP SEARCH TO NEXT TABLE ENTRY                      */
*/*                                                                  */
*/* ENTRY -- LINKED BY REGISTER 14                                   */
*/*                                                                  */
*/* INPUT --  (TSRCHNAM)                                             */
*/*                                                                  */
*/* OUTPUT -- (DRSAPF)                                               */
*/*                                                                  */
*/* EXTERNAL REFERENCES -- APFPTABL                                  */
*/*                                                                  */
*/* EXIT-NORMAL --  RETURN BY LINK REGISTER 14                       */
*/*                                                                  */
*/*   RETURN CODE -- N/A                                             */
*/*                                                                  */
*/* EXIT-ERROR -- N/A                                                */
*/*                                                                  */
*/*   CONDITIONS -- N/A                                              */
*/*   RETURN CODE -- N/A                                             */
*/*                                                                  */
*/* * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * * */
*TSRCHAPF:  /* EP TO DETERMINE-ELIGIBILITY-FOR-APF-ATTACH ROUTINE    */
*                                                               01S0555
*    ENTRY;                         /* EP TO ROUTINE                 */
TSRCHAPF DS    0H                                               01S0556
*    RESPECIFY (R1,R14) RSTD;                                   01S0556
*                                                               01S0556
*    /* MODESET TO PSW PROTECT KEY 1                                 */
*    DO; /*MODESET  KEYADDR(PROK1) WORKREG(R2)*/                01S0557
*      R2 = PROK1; /*GET KEY FORM SAVE LOCATION*/               01S0558
         LA    R2,16                                            01S0558
*      SPKA (R2); /*SET PSW KEY*/                               01S0559
         SPKA  0(R2)                                            01S0559
*    END; /*MODESET  KEYADDR(PROK1) WORKREG(R2)*/               01S0560
*    DRSAPF = NO;                   /* PRESUME NO APF ELIGIBILITY    */
         NI    DRSAPF(WRKAPTR2),B'10111111'                     01S0561
*    /* MODESET TO ENTRY PSW PROTECT KEY                             */
*    DO; /*MODESET  KEYADDR(TEPKEY) WORKREG(R2)*/               01S0562
*      R2 = TEPKEY; /*GET KEY FORM SAVE LOCATION*/              01S0563
         L     R2,TEPKEY(,WRKAPTR2)                             01S0563
*      SPKA (R2); /*SET PSW KEY*/                               01S0564
         SPKA  0(R2)                                            01S0564
*    END; /*MODESET  KEYADDR(TEPKEY) WORKREG(R2)*/              01S0565
*                                                               01S0565
*    R1 = ADDR(APFPTABL);           /* LOCATE LIST OF ELIGIBLE NAMES */
*                                                               01S0566
         L     R1,@CV00136                                      01S0566
*    /* SEARCH LIST                                                  */
*    DO WHILE (R1->TE8NAME ^= BLANKS  /* LOOP WHILE NEITHER BLANK    */
*        & DRSAPF = NO);            /* LIST NAME NOR MATCHING NAME   */
         B     @DE00567                                         01S0567
@DL00567 DS    0H                                               01S0568
*      IF R1->TE8NAME = MBRSTXT     /* CHK IF NAMES MATCH            */
*        THEN DO;                   /* YES,                          */
         CLC   TE8NAME(8,R1),MBRSTXT(WRKAPTR1)                  01S0568
         BNE   @RF00568                                         01S0568
*          /* MODESET TO PSW PROTECT KEY 1                           */
*          DO; /*MODESET  KEYADDR(PROK1) WORKREG(R2)*/          01S0570
*            R2 = PROK1; /*GET KEY FORM SAVE LOCATION*/         01S0571
         LA    R2,16                                            01S0571
*            SPKA (R2); /*SET PSW KEY*/                         01S0572
         SPKA  0(R2)                                            01S0572
*          END; /*MODESET  KEYADDR(PROK1) WORKREG(R2)*/         01S0573
*          DRSAPF = YES;            /* SET FOR APF ELIGIBILITY       */
         OI    DRSAPF(WRKAPTR2),B'01000000'                     01S0574
*          /* MODESET TO ENTRY PSW PROTECT KEY                       */
*          DO; /*MODESET  KEYADDR(TEPKEY) WORKREG(R2)*/         01S0575
*            R2 = TEPKEY; /*GET KEY FORM SAVE LOCATION*/        01S0576
         L     R2,TEPKEY(,WRKAPTR2)                             01S0576
*            SPKA (R2); /*SET PSW KEY*/                         01S0577
         SPKA  0(R2)                                            01S0577
*          END; /*MODESET  KEYADDR(TEPKEY) WORKREG(R2)*/        01S0578
*        END;                                                   01S0579
*      R1 = R1+D8;                  /* STEP PTR TO NEXT ENTRY        */
@RF00568 AL    R1,@CF00038                                      01S0580
*    END;                                                       01S0581
*                                                               01S0581
@DE00567 CLC   TE8NAME(8,R1),@CC00039                           01S0581
         BE    @DC00567                                         01S0581
         TM    DRSAPF(WRKAPTR2),B'01000000'                     01S0581
         BZ    @DL00567                                         01S0581
@DC00567 DS    0H                                               01S0582
*    END  DODSNAM;                  /* RETURN TO CALLER              */
         B     @EL00004                                         01S0582
*    RESPECIFY (R1,R14) UNRSTD;                                 01S0583
