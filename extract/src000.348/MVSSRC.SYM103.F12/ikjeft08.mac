*/* SEGMENT NAME -- DOTERM                                           */
*/*                                                                  */
*/* DESCRIPTIVE NAME -- N/A                                          */
*/*                                                                  */
*/* FUNCTION -- TO COMPLETE CALL PROCESSING                          */
*/*   OPERATION --                                                   */
*/*           - FREE PDL IF NECESSARY                                */
*/*           - CLOSE DCB IF NECESSARY                               */
*/*           - IF FAILURE RETURN CODE IS SET, CLEAR STACK AND QUEUE */
*/*           - IF FAILURE RETURN CODE IS NOT SET, RETURN CODE EQUALS*/
*/*             THE USER ECB                                         */
*/*                                                                  */
*/* ENTRY -- LINKED BY REGISTER 14                                   */
*/*                                                                  */
*/* INPUT --  N/A                                                    */
*/*                                                                  */
*/* OUTPUT -- N/A                                                    */
*/*                                                                  */
*/* EXTERNAL REFERENCES -- N/A                                       */
*/*                                                                  */
*/* EXIT-NORMAL --  RETURN BY LINK REGISTER 14                       */
*/*                                                                  */
*/*   RETURN CODE -- RTNCODE = TMPCECB                               */
*/*                                                                  */
*/* EXIT-ERROR -- RETURN BY LINK REGISTER 14                         */
*/*                                                                  */
*/*   CONDITIONS -- N/A                                              */
*/*   RETURN CODE -- N/A                                             */
*/*                                                                  */
*/* * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * * */
*DOTERM:    /* TERMINATE CALL ACTIVITY                               */
*                                                               01S0335
*    ENTRY;                         /* EP TO ROUTINE                 */
*                                                               01S0335
DOTERM   DS    0H                                               01S0336
*    ECTPCMD = CALLE;               /* REPLACE CALL NAME IN ECT      */
*                                                               01S0336
         L     @14,ECTPTR(,WRKAPTR2)                            01S0336
         MVC   ECTPCMD(8,@14),CALLE                             01S0336
*    /* CLOSE CALL DCB                                               */
*    IF DCBOPEN = YES               /* CHK IF DCB OPEN               */
*      THEN DO;                     /* YES                           */
         L     @14,CDCBPTR(,WRKAPTR1)                           01S0337
         TM    DCBOPEN+40(@14),B'00010000'                      01S0337
         BNO   @RF00337                                         01S0337
*        R1 = ADDR(CLOSEPL);        /* PTR TO PARM LIST              */
         LA    R1,CLOSEPL(,WRKAPTR1)                            01S0339
*        R0 = ADDR(CALLDCB);        /* PTR TO DCB AREA               */
         LA    R0,CALLDCB(,WRKAPTR1)                            01S0340
*        GEN(CLOSE  ((R0)),MF=(E,(R1)));                        01S0341
         CLOSE  ((R0)),MF=(E,(R1))
*        DSOPEN = NO;               /* RESET OPEN FLAG               */
         NI    DSOPEN(WRKAPTR1),B'10111111'                     01S0342
*      END;                                                     01S0343
*                                                               01S0343
*    /* UNALLOCATE CALL DATA SET                                     */
*    RESPECIFY (R2,R4) RSTD;                                    01S0344
@RF00337 DS    0H                                               01S0345
*    R2 = DAPLPTR;                  /* SET PTR TO DAIR PL            */
         L     R2,DAPLPTR(,WRKAPTR1)                            01S0345
*    IF MSGNO ^= D99                /* CHK FOR ALLOCATION FAILURE    */
*      THEN DO;                     /* NO,                           */
         CLC   MSGNO(4,WRKAPTR1),@CF00077                       01S0346
         BE    @RF00346                                         01S0346
*        R4 = CPPLPTR;              /* SET PTR TO SERV RTN PL        */
         L     R4,CPPLPTR(,WRKAPTR1)                            01S0348
*        R15 = ADDR(DOUNALC);       /* SET PTR TO UNALLOC ROUTINE    */
         LA    R15,DOUNALC                                      01S0349
*        GEN FLOWS(DOUNALC) (SYNCH   (15));  /* GO TO UNALLOC RTN    */
         SYNCH   (15)
*        RESPECIFY (R4) UNRSTD;                                 01S0351
*      END;                                                     01S0352
*                                                               01S0352
*    /* FREE PARSE DESCRIPTOR LIST                                   */
*    IF PDLPRES = YES               /* CHK FOR PDL TO FREE           */
*      THEN DO;                     /* YES                           */
@RF00346 TM    PDLPRES(WRKAPTR1),B'10000000'                    01S0353
         BNO   @RF00353                                         01S0353
*        R3=ADDR(PDLADDR);          /* PTR TO PDL                    */
         LA    R3,PDLADDR(,WRKAPTR1)                            01S0355
*        GEN(IKJRLSA  (3));                                     01S0356
         IKJRLSA  (3)
*      END;                                                     01S0357
*                                                               01S0357
*    /* IF FAILURE EXIT, CLEAR STACK AND QUEUE                       */
*    IF RTNCODE = D12               /* CHK FOR FAILURE CODE          */
*      THEN DO;                     /* YES,                          */
@RF00353 L     @14,SAVRBAK(,R13)                                01S0358
         CLC   RTNCODE(4,@14),@CF00068                          01S0358
         BNE   @RF00358                                         01S0358
*        RESPECIFY (R2) RSTD;                                   01S0360
*        R2 = STPBPTR;              /* PTR TO STACK PB               */
         L     R2,STPBPTR(,WRKAPTR1)                            01S0361
*        R1 = IOPLPTR;              /* PTR TO STACK PL               */
         L     R1,IOPLPTR(,WRKAPTR1)                            01S0362
*        GEN(STACK  PARM=(R2),DELETE=ALL,MF=(E,(1)));           01S0363
         STACK  PARM=(R2),DELETE=ALL,MF=(E,(1))
*        GEN(TCLEARQ  INPUT);                                   01S0364
         TCLEARQ  INPUT
*        RESPECIFY (R2) UNRSTD;                                 01S0365
*      END;                                                     01S0366
*      ELSE                         /* NO FAILURE CODE               */
*        RTNCODE=TMPCECB;           /* CP RETURN CODE VIA REG 15     */
*                                                               01S0367
         B     @RC00358                                         01S0367
@RF00358 L     @14,SAVRBAK(,R13)                                01S0367
         L     @10,TMPCECB(,WRKAPTR1)                           01S0367
         ST    @10,RTNCODE(,@14)                                01S0367
*    RESPECIFY (R14) RSTD;                                      01S0368
@RC00358 DS    0H                                               01S0369
*    R14 = ADDR(MAINX);             /* SET FOR PSEUDO-RETURN         */
         LA    R14,MAINX                                        01S0369
*    END  DOSETCP;                  /* RETURN TO CALLER              */
         B     @EL00003                                         01S0370
*    RESPECIFY (R14) UNRSTD;                                    01S0371
*/* * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * * */
*/*                                                                  */
*/* SEGMENT NAME -- DODSNAM                                          */
*/*                                                                  */
*/* DESCRIPTIVE NAME -- N/A                                          */
*/*                                                                  */
*/* FUNCTION -- TO ESTABLISH DS NAME FOR DYNAMIC ALLOCATION          */
*/*   OPERATION --                                                   */
*/*           - SET LENGTH FOR DAIR AND MESSAGE                      */
*/*           - IF MEMBER NAME ENTERED, SET MEMBER NAME FOR DAIR     */
*/*           - BLANK MEMBER NAME IN DAIR PARAMETER BLOCK            */
*/*           - INSERT PASSWORD IN DAIR PB IF ENTERED                */
*/*           - CLEAR CONTROL FLAG IN DAIR PB                        */
*/*           - ASSUME NO DATA SET NAME ENTERED                      */
*/*           - IF DATA SET NAME IS ENTERED, SET DSNAME, DSLENGTH,   */
*/*             MSG AND MSGLENGTH FOR DAIR                           */
*/*           - IF DSNAME IS NOT FULLY QUALIFIED, ESTABLISH FULL     */
*/*             QUALIFICATION                                        */
*/*                                                                  */
*/* ENTRY -- LINKED BY REGISTER 14                                   */
*/*                                                                  */
*/* INPUT --  N/A                                                    */
*/*                                                                  */
*/* OUTPUT -- N/A                                                    */
*/*                                                                  */
*/* EXTERNAL REFERENCES -- N/A                                       */
*/*                                                                  */
*/* EXIT-NORMAL --  RETURN BY LINK REGISTER 14                       */
*/*                                                                  */
*/*   RETURN CODE -- N/A                                             */
*/*                                                                  */
*/* EXIT-ERROR -- N/A                                                */
*/*                                                                  */
*/*   CONDITIONS -- N/A                                              */
*/*   RETURN CODE -- N/A                                             */
*/*                                                                  */
*/* * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * * */
*DODSNAM:   /* SET DSNAME FOR DYNAMIC ALLOCATION                     */
*                                                               01S0372
*    PROC   OPTIONS(DONTSAVE,NOSAVEAREA);  /* NO SAVE AREA           */
*                                                               01S0372
DODSNAM  DS    0H                                               01S0373
*    IF MSGNO = ZERO                /* CHK FOR TERMINATION FLAG      */
*      THEN DO;                     /* NO, CONTINUE                  */
         L     @14,MSGNO(,WRKAPTR1)                             01S0373
         LTR   @14,@14                                          01S0373
         BNZ   @RF00373                                         01S0373
*        /* SET UP MEMBER NAME IN MSG SEGMENT AND DAIR PB            */
*        MBRDLEN = D8;              /* SET LENGTH FOR DAIR           */
         MVC   MBRDLEN(2,WRKAPTR1),@CH00038                     01S0375
*        MBRSLEN = D12;             /* SET LENGTH FOR MSG            */
         MVC   MBRSLEN(2,WRKAPTR1),@CH00068                     01S0376
*        IF BLANKB = NO             /*                               */
*          THEN DO;                 /* YES                           */
         TM    BLANKB(WRKAPTR1),B'00010000'                     01S0377
         BNZ   @RF00377                                         01S0377
*            MBRSLEN=MBRLEN+D4;     /* SET LENGTH FOR MSG            */
         LH    @14,MBRLEN(,R3)                                  01S0379
         LA    @10,4                                            01S0379
         ALR   @10,@14                                          01S0379
         STH   @10,MBRSLEN(,WRKAPTR1)                           01S0379
*            MBRSTXT = '' ;         /* INITIALIZE TEXT TO BLANKS     */
         MVI   MBRSTXT(WRKAPTR1),C' '                           01S0380
         MVC   MBRSTXT+1(7,WRKAPTR1),MBRSTXT(WRKAPTR1)          01S0380
*            MBRSTXT(1:MBRLEN) =    /* INSERT MEMBER NAME AS         */
*                MBRBUF(1:MBRLEN);  /*     ENTERED                   */
         BCTR  @14,0                                            01S0381
         L     @10,MBRPTR(,R3)                                  01S0381
         EX    @14,@SM02737                                     01S0381
*            MBRDTXT = MBRSTXT;     /* SET MEMBER NAME FOR DAIR      */
         MVC   MBRDTXT(8,WRKAPTR1),MBRSTXT(WRKAPTR1)            01S0382
*          END;                                                 01S0383
*        DA08MNM = '';              /* BLANK MEMBERNAME IN DAIR PB   */
*                                                               01S0384
@RF00377 L     @14,DAPLPTR(,WRKAPTR1)                           01S0384
         L     @14,DAPLDAPB(,@14)                               01S0384
         MVI   DA08MNM(@14),C' '                                01S0384
         MVC   DA08MNM+1(7,@14),DA08MNM(@14)                    01S0384
*        /* SET UP PASSWORD IN DAIR PB                               */
*        DA08PSWD = '';             /* INITIALIZE TO BLANKS  @ZA20355*/
         MVI   DA08PSWD(@14),C' '                               01S0385
         MVC   DA08PSWD+1(7,@14),DA08PSWD(@14)                  01S0385
*        IF PASSPRES = YES          /* CHK IF PASSWORD ENTERED       */
*          THEN                     /* YES,                          */
         TM    PASSPRES(R3),B'10000000'                         01S0386
         BNO   @RF00386                                         01S0386
*            DA08PSWD(1:PASSLEN)=   /* INSERT PASSWORD AS            */
*                PASSBUF(1:PASSLEN);  /*  ENTERED                    */
         LH    @10,PASSLEN(,R3)                                 01S0387
         BCTR  @10,0                                            01S0387
         L     @06,PASSPTR(,R3)                                 01S0387
         EX    @10,@SM02739                                     01S0387
*        DA08CTL = ZERO;            /* CLEAR CONTROL FLAG IN DAIR PB */
*                                                               01S0388
@RF00386 L     @14,DAPLPTR(,WRKAPTR1)                           01S0388
         L     @14,DAPLDAPB(,@14)                               01S0388
         MVI   DA08CTL(@14),X'00'                               01S0388
*        /* SET UP DATA SET NAME                                     */
*        DSNLENG = ZERO;            /* INITIALIZE TO NO ENTRY        */
         SLR   @14,@14                                          01S0389
         STH   @14,DSNLENG(,WRKAPTR1)                           01S0389
*        IF DSNPRES = YES           /* CHK IF DSNAME ENTERED         */
*          THEN DO;                 /* YES,                          */
         TM    DSNPRES(R3),B'10000000'                          01S0390
         BNO   @RF00390                                         01S0390
*            DSNLENG = DSNLEN;      /* SET DSNAME LENGTH FOR DAIR    */
         LH    @14,DSNLEN(,R3)                                  01S0392
         STH   @14,DSNLENG(,WRKAPTR1)                           01S0392
*            DSSGLEN = DSNLEN+D4;   /* SET MSG SEGMENT LENGTH        */
         LA    @10,4                                            01S0393
         ALR   @10,@14                                          01S0393
         STH   @10,DSSGLEN(,WRKAPTR1)                           01S0393
*            DSNBUF = BUFDSN(1:DSNLEN);/* SET DSN FOR DAIR   @ZA20567*/
         MVI   DSNBUF+1(WRKAPTR1),C' '                          01S0394
         MVC   DSNBUF+2(42,WRKAPTR1),DSNBUF+1(WRKAPTR1)         01S0394
         BCTR  @14,0                                            01S0394
         L     @10,DSNPTR(,R3)                                  01S0394
         EX    @14,@SM02741                                     01S0394
*            DSSGTXT = BUFDSN(1:DSNLEN);/* SET MSG SEG TEXT  @ZA20567*/
         MVI   DSSGTXT+1(WRKAPTR1),C' '                         01S0395
         MVC   DSSGTXT+2(42,WRKAPTR1),DSSGTXT+1(WRKAPTR1)       01S0395
         EX    @14,@SM02743                                     01S0395
*          END;                                                 01S0396
*                                                               01S0396
*        /* ESTABLISH FULLY QUALIFIED NAME                           */
*        IF DSNQUOT ^= YES          /* CHK FULLY QUALIFIED ENTRY     */
*          THEN                     /* NO,                           */
@RF00390 TM    DSNQUOT(R3),B'01000000'                          01S0397
         BO    @RF00397                                         01S0397
*            CALL  DOQUAL;          /* TO SET FULL NAME              */
         BAL   @14,DOQUAL                                       01S0398
*      END;                                                     01S0399
@RF00397 DS    0H                                               01S0400
*    RESPECIFY (R14) RSTD;                                      01S0400
@RF00373 DS    0H                                               01S0401
*    R14 = ADDR(DOSTCPA);           /* SET FOR PSEUDO-RETURN         */
         LA    R14,DOSTCPA                                      01S0401
*    RETURN;                        /* RETURN TO CALLER              */
@EL00004 DS    0H                                               01S0402
@EF00004 DS    0H                                               01S0402
@ER00004 BR    @14                                              01S0402
