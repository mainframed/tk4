         TITLE 'IKJEFT52, CHAIN OUT SUBROUTINE                       '
IKJEFT52 CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'IKJEFT52  73.151'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @12,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@12                                         0001
         L     @00,@SIZDATD                                        0001
         GETMAIN R,LV=(0)
         LR    @11,@01                                             0001
         USING @DATD,@11                                           0001
         ST    @13,@SA00001+4                                      0001
         LM    @00,@01,20(@13)                                     0001
         ST    @11,8(,@13)                                         0001
         LR    @13,@11                                             0001
         MVC   @PC00001(16),0(@01)                                 0001
*                                                                  0011
*   /*****************************************************************/
*   /*                                                               */
*   /* START ECECUTABLE CODE                                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0011
*   ECTPTR=XECTPTR;                 /* SET LOCAL ECT POINTER         */
         L     ECTPTR,XECTPTR(,R1)                                 0011
*   PTPBPTR=XPTPBPTR;               /* SET LOCAL PTPB POINTER        */
         L     PTPBPTR,XPTPBPTR(,R1)                               0012
*   SAVEREG1=R1;                    /* SAVE REGISTER ONE             */
         LR    SAVEREG1,R1                                         0013
*   SAVEPARM=PARMAOUT;              /* SAVE POINTER TO OUTPUT IN PTPB
*                                      SO THE PTPB CAN BE MODIFIED 0014
*                                      AND USED FOR TERMINAL OUTPUT
*                                      OF CHAIN                      */
         L     SAVEPARM,PARMAOUT(,PTPBPTR)                         0014
*   SAVETPUT=PARMOPTN;              /* SAVE USERS TPUT OPTIONS       */
         SR    SAVETPUT,SAVETPUT                                   0015
         IC    SAVETPUT,PARMOPTN(,PTPBPTR)                         0015
*   PARMOPTN=0;                     /* INDICATE DEFAULT TPUT         */
         MVI   PARMOPTN(PTPBPTR),X'00'                             0016
*   PARMAOUT=ECTSMSG;               /* GET CHAIN POINTER             */
         SR    @14,@14                                             0017
         ICM   @14,7,ECTSMSG(ECTPTR)                               0017
         ST    @14,PARMAOUT(,PTPBPTR)                              0017
*   IF ECTMSGF>='80'X×              /* IF 2ND LEVEL CHAIN IS FLAGGED
*                                      FOR DELETION                  */
*       PARMAOUT=0 THEN             /* OR IF NONE EXISTS THEN        */
         CLI   ECTMSGF(ECTPTR),X'80'                               0018
         BNL   @RT00018                                            0018
         LTR   @14,@14                                             0018
         BZ    @RT00018                                            0018
*     GO TO TPUTMSG;                /* INFORM TERMINAL               */
*                                                                  0019
*   /*****************************************************************/
*   /*                                                               */
*   /* SET UP PARMS AND PUT CHAIN OUT                                */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0020
*   PARMMLIN='1'B;                  /* INDICATE MULTI-LIN OPUT       */
         OI    PARMMLIN(PTPBPTR),B'00001000'                       0020
*   CALL IKJEFT56;                  /* GO TO TERMOUT                 */
         L     @15,@CV00126                                        0021
         BALR  @14,@15                                             0021
*   RETURNCD=R15;                   /* SAVE TERMOUTRETRN CODE        */
         LR    RETURNCD,R15                                        0022
*   PARMMLIN='0'B;                  /* TURN OFF MULTI-LINE BIT       */
         NI    PARMMLIN(PTPBPTR),B'11110111'                       0023
*UNCHAIN:                                                          0024
*   CALL IKJEFT53;                  /* DELETE THE CHAIN AND          */
UNCHAIN  L     @15,@CV00129                                        0024
         BALR  @14,@15                                             0024
*   GOTO EXIT;                      /* RETURN                        */
         B     EXIT                                                0025
*TPUTMSG:                                                          0026
*   PARMAOUT=ADDR(NOIFRMSG);        /* POINT TO MESSAGE              */
TPUTMSG  L     @14,@CA00121                                        0026
         ST    @14,PARMAOUT(,PTPBPTR)                              0026
*   CALL IKJEFT56;                  /* PUT OUT NO IFOR MSG           */
         L     @15,@CV00126                                        0027
         BALR  @14,@15                                             0027
*   RETURNCD=R15;                   /* SAVE RETURN CODE              */
         LR    RETURNCD,R15                                        0028
*   IF ECTMSGF>='80'X THEN          /* CHAIN TO BE DELETED           */
         CLI   ECTMSGF(ECTPTR),X'80'                               0029
         BNL   @RT00029                                            0029
*     GOTO UNCHAIN;                                                0030
*EXIT:                                                             0031
*   PARMAOUT=SAVEPARM;              /* RESTOR PTR TO OPUT LINE       */
EXIT     ST    SAVEPARM,PARMAOUT(,PTPBPTR)                         0031
*   PARMOPTN=SAVETPUT;              /* RESTORE TPUT OPTION           */
         STC   SAVETPUT,PARMOPTN(,PTPBPTR)                         0032
*   RETURN CODE(RETURNCD);                                         0033
         L     @13,4(,@13)                                         0033
         L     @00,@SIZDATD                                        0033
         LR    @01,@11                                             0033
         FREEMAIN R,LV=(0),A=(1)
         LR    @15,@04                                             0033
         L     @14,12(,@13)                                        0033
         LM    @00,@12,20(@13)                                     0033
         BR    @14                                                 0033
*   END IKJEFT52                                                   0034
*                                                                  0034
*/* THE FOLLOWING INCLUDE STATEMENTS WERE FOUND IN THIS PROGRAM.     */
*/*%INCLUDE SYSLIB  (IKJUPT  )                                       */
*/*%INCLUDE SYSLIB  (IKJECT  )                                       */
*                                                                  0034
*       ;                                                          0034
@DATA    DS    0H
@DATD    DSECT
         DS    0F
@SA00001 DS    18F
@PC00001 DS    4F
IKJEFT52 CSECT
         DS    0F
@DATD    DSECT
         DS    0D
         ORG   *+1-(*-@DATD)/(*-@DATD) INSURE DSECT DATA
@ENDDATD EQU   *
IKJEFT52 CSECT
         DS    0F
@SIZDATD DC    AL1(1)
         DC    AL3(@ENDDATD-@DATD)
@CA00121 DC    A(NOIFRMSG)
@CV00126 DC    V(IKJEFT56)
@CV00129 DC    V(IKJEFT53)
         DS    0D
@00      EQU   00                      EQUATES FOR REGISTERS 0-15
@01      EQU   01
@02      EQU   02
@03      EQU   03
@04      EQU   04
@05      EQU   05
@06      EQU   06
@07      EQU   07
@08      EQU   08
@09      EQU   09
@10      EQU   10
@11      EQU   11
@12      EQU   12
@13      EQU   13
@14      EQU   14
@15      EQU   15
SAVETPUT EQU   @06
SAVEPARM EQU   @05
RETURNCD EQU   @04
SAVEREG1 EQU   @04
PTPBPTR  EQU   @02
ECTPTR   EQU   @03
R1       EQU   @01
R0       EQU   @00
R15      EQU   @15
         EXTRN NOIFRMSG
XPRMLIST EQU   0
UPTPTR   EQU   XPRMLIST
XECTPTR  EQU   XPRMLIST+4
XPTPBPTR EQU   XPRMLIST+12
UPT      EQU   0
UPTSWS   EQU   UPT+12
ECT      EQU   0
ECTMSGF  EQU   ECT+8
ECTSMSG  EQU   ECT+9
ECTSWS   EQU   ECT+28
PTPB     EQU   0
PARMCNTL EQU   PTPB
PARMMLIN EQU   PARMCNTL
PARMTPUT EQU   PTPB+2
PARMOPTN EQU   PARMTPUT
PARMAOUT EQU   PTPB+4
UPTX     EQU   0
ECTX     EQU   0
ECB      EQU   0
PTPBX    EQU   0
         AGO   .@UNREFD                START UNREFERENCED COMPONENTS
ECBCOMPC EQU   ECB
ECBCBIT  EQU   ECB
ECBWBIT  EQU   ECB
@NM00006 EQU   PARMTPUT+1
PARMFORM EQU   PARMCNTL+1
PARMDMND EQU   PARMCNTL+1
PARMMODE EQU   PARMCNTL+1
PARMPRMT EQU   PARMCNTL
PARMIFOR EQU   PARMCNTL
PARMMLEV EQU   PARMCNTL
PARMSNGL EQU   PARMCNTL
PARMDTMS EQU   PARMCNTL
PARMPUT  EQU   PARMCNTL
PARMPBT0 EQU   PARMCNTL
ECTBKPB  EQU   ECT+36
ECTUSER  EQU   ECT+32
ECTDDNUM EQU   ECT+29
@NM00005 EQU   ECTSWS
ECTBKGRD EQU   ECTSWS
ECTNNOT  EQU   ECTSWS
ECTNMAL  EQU   ECTSWS
ECTLOGF  EQU   ECTSWS
ECTATRM  EQU   ECTSWS
@NM00004 EQU   ECTSWS
ECTNOPD  EQU   ECTSWS
ECTSCMD  EQU   ECT+20
ECTPCMD  EQU   ECT+12
ECTIOWA  EQU   ECT+4
ECTRTCD  EQU   ECT+1
ECTRCDF  EQU   ECT
UPTPREFL EQU   UPT+23
UPTPREFX EQU   UPT+16
@NM00003 EQU   UPT+15
UPTLDEL  EQU   UPT+14
UPTCDEL  EQU   UPT+13
UPTWTP   EQU   UPTSWS
UPTMODE  EQU   UPTSWS
UPTALD   EQU   UPTSWS
UPTPAUS  EQU   UPTSWS
UPTNCOM  EQU   UPTSWS
UPTMID   EQU   UPTSWS
UPTNPRM  EQU   UPTSWS
@NM00002 EQU   UPTSWS
UPTUSER  EQU   UPT+2
@NM00001 EQU   UPT
XECBPTR  EQU   XPRMLIST+8
.@UNREFD ANOP                          END UNREFERENCED COMPONENTS
@RT00018 EQU   TPUTMSG
@RT00029 EQU   UNCHAIN
@ENDDATA EQU   *
         END   IKJEFT52
