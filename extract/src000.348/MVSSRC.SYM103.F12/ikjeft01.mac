TDUMX1   DS    0H                                               01S0553
*    END  TDUMX1;                   /* RETURN TO CALLER              */
@EL00002 DS    0H                                               01S0553
@EF00002 DS    0H                                               01S0553
@ER00002 BR    @14                                              01S0553
*/* * * * * * * * * * * * * * * * **** * * * * * * * * * * * * * * * */
*/*                                                                  */
*/* SEGMENT NAME -- BKGBLDR                                          */
*/*                                                                  */
*/* DESCRIPTIVE NAME -- TOP SEGMENT                                  */
*/*                                                                  */
*/* FUNCTION -- BKGBLDR EXECUTOR ROUTINE                             */
*/*  OPERATION --                                                    */
*/*           - GET SPACE (IN BACKGROUND MODE) FOR UPT, PSCB, RLGB,  */
*/*             AND LWA                                              */
*/*           - INITIALIZE BLOCKS WITH INFORMATION FROM UADS IF IT   */
*/*             IS AVAILABLE, OTHERWISE WITH DEFAULT VALUES          */
*/*                                                                  */
*/* ENTRY -- LINKED BY REGISTER 14                                   */
*/*                                                                  */
*/* INPUT -- REGISTER 1 POINTS TO A FIELD TO RECEIVE THE POINTER TO  */
*/*          THE DEVELOPED PSCB                                      */
*/*                                                                  */
*/* OUTPUT -- INITIALIZED UPT, PSCB, RLGB AND LWA                    */
*/*                                                                  */
*/* EXTERNAL REFERENCES -- N/A                                       */
*/*                                                                  */
*/* EXIT NORMAL -- RETURN BY LINK REGISTER 14                        */
*/*   RETURN CODE -- NORMAL                                          */
*/*          ZERO -- 0, ALL SUCCESSFUL, USERID FOUND                 */
*/*          RCNOUID -- 4, NO USERID AVAILABLE, DEFAULT BLOCKS ARE   */
*/*                     INITIALIZED                                  */
*/*          RCNOTTSO -- 8, USERID SUPPLIED IS NOT TSO USER, DEFAULT */
*/*                     BLOCKS ARE INITIALIZED                       */
*/* EXIT ERROR -- RETURN BY LINK REGISTER 14                         */
*/*   CONDITIONS -- GETMAIN FAILURE 0R I/0 ERROR WITH UADS           */
*/*   RETURN CODE -- ERROR                                           */
*/*          RCUADERR -- 12, ERROR IN ACCESSING UADS                 */
*/*          RCTERM -- 16, FAILURE IN GETMAIN FOR PSCB/UPT/LWA/RLGB  */
*/*                                                                  */
*/* * * * * * * * * * * * * * * * **** * * * * * * * * * * * * * * * */
*                                                               01S0554
*BKGBLDR:   /* EP TO PSCB/UPT/RLGB/LWA CONSTRUCTOR                   */
*    PROCEDURE           ;                                      01S0554
*                                                               01S0554
BKGBLDR  STM   @14,@12,@SA00003                                 01S0554
*    RESPECIFY (TCB)  BASED(TCURTCB);                           01S0555
*    RESPECIFY (IEZJSCB) BASED(TCBJSCB);                        01S0556
*    RESPECIFY (ASCB) BASED(TCUASCB);                           01S0557
*    RESPECIFY (ASXB) BASED(ASCBASXB);                          01S0558
*    RESPECIFY (LWA)  BASED(ASXBLWA);                           01S0559
*    RESPECIFY (RLGB) BASED(PSCBRLGB);                          01S0560
*    RESPECIFY (PSCB) BASED(PSCBPTR);                           01S0561
*    RESPECIFY (UPT)  BASED(UPTPTR);                            01S0562
*                                                               01S0562
*IKJEFLR:   /* EP (LOGICAL) TO PSCB/UPT/RLGB/LWA CONSTRUCTOR         */
*    ENTRY;                                                     01S0563
*                                                               01S0563
         B     @EC00563                                         01S0563
IKJEFLR  STM   @14,@12,@SA00003                                 01S0563
@EC00563 DS    0H                                               01S0564
*    CALL INIT;                      /* INITIALIZATION FOR BKGBLDR   */
*                                                               01S0564
         BAL   @14,INIT                                         01S0564
*    IF ASXBUSER ^= ZEROES           /* CHK IF USERID SUPPLIED       */
*      THEN DO;                      /* YES,                         */
         L     @02,CVTPTR                                       01S0565
         L     @02,CVTTCBP(,@02)                                01S0565
         L     @02,TCUASCB(,@02)                                01S0565
         L     @02,ASCBASXB(,@02)                               01S0565
         CLC   ASXBUSER(7,@02),@CB00154                         01S0565
         BE    @RF00565                                         01S0565
*        CALL CHKUSER;               /*   TO CHK USERID VALIDITY     */
         BAL   @14,CHKUSER                                      01S0567
*        CALL INITUP;                /* INITIALIZE UPT AND PSCB      */
         BAL   @14,INITUP                                       01S0568
*      END;                                                     01S0569
*                                                               01S0569
*    IF ASXBUSER = ZEROES            /* CHK FOR NO USERID SUPPLIED   */
*        STATUS ^= CONTINUE         /*   OR QUIT FLAG               */
*     THEN                           /* YES,                         */
@RF00565 L     @02,CVTPTR                                       01S0570
         L     @02,CVTTCBP(,@02)                                01S0570
         L     @02,TCUASCB(,@02)                                01S0570
         L     @02,ASCBASXB(,@02)                               01S0570
         CLC   ASXBUSER(7,@02),@CB00154                         01S0570
         BE    @RT00570                                         01S0570
         L     @02,STATUS                                       01S0570
         LTR   @02,@02                                          01S0570
         BZ    @RF00570                                         01S0570
@RT00570 DS    0H                                               01S0571
*       CALL  DUMMYUP;               /*   INITIALIZE DEFAULT BLOCKS  */
*                                                               01S0571
         BAL   @14,DUMMYUP                                      01S0571
*   CALL CHAINUP;                    /* CHAIN UPT & PSCB FOR TMP     */
*                                                               01S0572
@RF00570 BAL   @14,CHAINUP                                      01S0572
*   CALL CLEANUP;                    /* CLEANUP BEFORE TERMINATION   */
*                                                               01S0573
         BAL   @14,CLEANUP                                      01S0573
*   RETURN CODE(RETCODE);                                       01S0574
         L     @15,RETCODE                                      01S0574
         L     @14,@SA00003                                     01S0574
         LM    @00,@12,@SA00003+8                               01S0574
         BR    @14                                              01S0574
*/* * * * * * * * * * * * * * * * **** * * * * * * * * * * * * * * * */
*/*                                                                  */
*/* SEGMENT NAME -- INIT                                             */
*/*                                                                  */
*/* DESCRIPTIVE NAME -- BKGBLDR INITIALIZER - PART 1                 */
*/*                                                                  */
*/* FUNCTION -- TO ESTABLISH AND INITIALIZE POINTERS AND CONTROL     */
*/*       AREAS USED BY BKGBLDR                                      */
*/*  OPERATION --                                                    */
*/*           - INITIALIZE MACRO LIST FORMS AND OTHER VARIABLES      */
*/*           - OBTAIN THE NAME OF THE UADS FROM SECURITY CONTROL    */
*/*             BLOCKS                                               */
*/*           - CALL ROUTINE (OPENUADS) TO OPEN THE UADS             */
*/*                                                                  */
*/* ENTRY -- LINKED BY REGISTER 14                                   */
*/*                                                                  */
*/* INPUT -- N/A                                                     */
*/*                                                                  */
*/* OUTPUT -- INITIALIZED ENVIRONMENT FOR BKGBLDR                    */
*/*                                                                  */
*/* EXTERNAL REFERENCES -- N/A                                       */
*/*                                                                  */
*/* EXIT NORMAL -- RETURN BY LINK REGISTER 14                        */
*/*   RETURN CODE -- STATUS = CONTINUE, 0                            */
*/*                                                                  */
*/* EXIT ERROR -- RETURN BY LINK REGISTER 14                         */
*/*   CONDITIONS -- FAILURE IN ALLOCATION OR OPEN                    */
*/*   RETURN CODE -- STATUS = QUIT (NON-ZERO)                        */
*/*                                                                  */
*/* * * * * * * * * * * * * * * * **** * * * * * * * * * * * * * * * */
*INIT:  /* EP TO INITIALIZE FOR BKGBLDR                              */
*                                                               01S0575
*    PROCEDURE   ;                                              01S0575
*                                                               01S0575
*                                                               01S0575
INIT     STM   @14,@12,@SA00004                                 01S0575
*    RETCODE = ZERO;                 /* INITIALIZE RETURN CODE AREA  */
         SLR   @02,@02                                          01S0576
         ST    @02,RETCODE                                      01S0576
*    STATUS   = CONTINUE;            /* STATUS IS CONTINUE           */
         ST    @02,STATUS                                       01S0577
*    UADSBUFP = ZERO;                /* NO UADS BUFFER YET           */
         ST    @02,UADSBUFP                                     01S0578
*    UADSDPTR = ZERO;                /* NO UADS DATA BLOCKS YET      */
         ST    @02,UADSDPTR                                     01S0579
*    CURBLOCK = ZERO;                /* NO UADS BLOCK YET            */
         ST    @02,CURBLOCK                                     01S0580
*    NEWBLOCK = ZERO;                /* NO NEW BLOCK WANTED, YET     */
         ST    @02,NEWBLOCK                                     01S0581
*    USERIDBP = ZERO;                /* PTR TO BLANK AFTER USERID    */
         ST    @02,USERIDBP                                     01S0582
*    USERIDL  = ZERO;                /* LENGTH OF USERID             */
         ST    @02,USERIDL                                      01S0583
*    USERID   = BLANK8;              /* NO USERID YET                */
         MVC   USERID(8),@CC00050                               01S0584
*    DCBPTR = ADDR(DCB);             /* SET PTR TO DCB IN @DATD      */
*                                                               01S0585
         LA    @07,DCB                                          01S0585
         ST    @07,DCBPTR(,WRKAPTR1)                            01S0585
*    $UPT     = ''B;                 /* ZERO OUT THE MODEL UPT       */
         XC    $UPT(24),$UPT                                    01S0586
*    $PSCB    = ''B;                 /* ZERO OUT THE MODEL PSCB      */
         XC    $PSCB(72),$PSCB                                  01S0587
*    RESPECIFY (R3,R4) RSTD;                                    01S0588
*    R3 = ADDR($UPT);                /* GET PTR TO MODEL UPT         */
         LA    R3,$UPT                                          01S0589
*    R4 = ADDR($PSCB);               /* GET PTR TO MODEL PSCB        */
         LA    R4,$PSCB                                         01S0590
*    DO; /*MODESET  KEYADDR(PROK1) WORKREG(R2)*/                01S0591
*      R2 = PROK1; /*GET KEY FORM SAVE LOCATION*/               01S0592
         LA    R2,16                                            01S0592
*      SPKA (R2); /*SET PSW KEY*/                               01S0593
         SPKA  0(R2)                                            01S0593
*    END; /*MODESET  KEYADDR(PROK1) WORKREG(R2)*/               01S0594
*    UPTPTR = R3;                    /* SET PTR TO MODEL UPT         */
         ST    R3,UPTPTR(,WRKAPTR2)                             01S0595
*    PSCBPTR = R4;                   /* SET PTR TO MODEL PSCB        */
         ST    R4,PSCBPTR(,WRKAPTR2)                            01S0596
*    DO; /*MODESET  KEYADDR(TEPKEY) WORKREG(R2)*/               01S0597
*      R2 = TEPKEY; /*GET KEY FORM SAVE LOCATION*/              01S0598
         L     R2,TEPKEY(,WRKAPTR2)                             01S0598
*      SPKA (R2); /*SET PSW KEY*/                               01S0599
         SPKA  0(R2)                                            01S0599
*    END; /*MODESET  KEYADDR(TEPKEY) WORKREG(R2)*/              01S0600
*    RESPECIFY (R3,R4) UNRSTD;                                  01S0601
*    UADSOB   = ''B;                 /* ZERO OUT UADS OFFSET BLOCK   */
         LA    @07,$UADSOB                                      01S0602
         XC    UADSOB(12,@07),UADSOB(@07)                       01S0602
*    CNTLBITS = ''B;                 /* ZERO OUT THE CONTROL BITS    */
*                                                               01S0603
         MVI   CNTLBITS,X'00'                                   01S0603
*    IF CVTRAC = ZERO                /* CHK IF RACF IS IN SYSTEM     */
*         RCVTUADS = ZEROES         /* CHK IF UADS RECOGNIZED       */
*      THEN                          /* NO,                          */
         SLR   @07,@07                                          01S0604
         L     @01,CVTPTR                                       01S0604
         L     @06,CVTRAC(,@01)                                 01S0604
         CR    @06,@07                                          01S0604
         BE    @RT00604                                         01S0604
         CLC   RCVTUADS(7,@06),@CB00154                         01S0604
         BNE   @RF00604                                         01S0604
@RT00604 DS    0H                                               01S0605
*        STATUS = ERRUSER;           /*   SET FLAG FOR DUMMYUP       */
         MVC   STATUS(4),@CF00034                               01S0605
*      ELSE DO;                      /* YES, RACF IS PRESENT         */
         B     @RC00604                                         01S0606
@RF00604 DS    0H                                               01S0607
*        THEUADSL=LENGTH(THEUADSN);  /* LENGTH OF UADS               */
         MVC   THEUADSL(2),@CH01182                             01S0607
*        THEUADSN = RCVTUADS;        /* NAME OF UADS                 */
         L     @01,CVTPTR                                       01S0608
         L     @01,CVTRAC(,@01)                                 01S0608
         MVC   THEUADSN(44),RCVTUADS(@01)                       01S0608
*        OPEN(1:$OPENL)=$OPEN(1:$OPENL);  /* INIT THE OPEN LIST      */
         LH    @07,$OPENL                                       01S0609
         BCTR  @07,0                                            01S0609
         EX    @07,@SM03561                                     01S0609
*        CLOSE(1:$CLOSEL) =          /* INITITIALIZE                 */
*            $CLOSE(1:$CLOSEL);      /*   THE CLOSE LIST             */
         LH    @07,$CLOSEL                                      01S0610
         BCTR  @07,0                                            01S0610
         EX    @07,@SM03563                                     01S0610
*        DCB(1:$DCBL)=$DCB(1:$DCBL);  /* INIT THE DCB                */
         LH    @07,$DCBL                                        01S0611
         BCTR  @07,0                                            01S0611
         EX    @07,@SM03565                                     01S0611
*        READ(1:$READL)=$READ(1:$READL);  /* INIT THE READ LIST      */
         LH    @07,$READL                                       01S0612
         BCTR  @07,0                                            01S0612
         EX    @07,@SM03567                                     01S0612
*        CALL ENQUADS;               /* ENQUEUE ON ACCOUNT UADS NAME */
         BAL   @14,ENQUADS                                      01S0613
*        CALL OPENUADS;              /* OPEN SYS1.UADS               */
         BAL   @14,OPENUADS                                     01S0614
*      END;                                                     01S0615
*/* * * * * * * * * * * * * * * * **** * * * * * * * * * * * * * * * */
*/*                                                                  */
*/* SEGMENT NAME -- (INIT) - PART 2                                  */
*/*                                                                  */
*/* DESCRIPTIVE NAME -- BKGBLDR INITIALIZER - PART 2                 */
*/*                                                                  */
*/* FUNCTION -- N/A                                                  */
*/*                                                                  */
*/* ENTRY -- N/A                                                     */
*/*                                                                  */
*/* INPUT -- N/A                                                     */
*/*                                                                  */
*/* OUTPUT -- N/A                                                    */
*/*                                                                  */
*/* EXTERNAL REFERENCES -- N/A                                       */
*/*                                                                  */
*/* EXIT NORMAL -- N/A                                               */
*/*   RETURN CODE -- N/A                                             */
*/*                                                                  */
*/* EXIT ERROR -- N/A                                                */
*/*   CONDITIONS -- N/A                                              */
*/*   RETURN CODE -- N/A                                             */
*/*                                                                  */
*/* * * * * * * * * * * * * * * * **** * * * * * * * * * * * * * * * */
*                                                               01S0616
*                                                               01S0616
*    IF STATUS = CONTINUE            /* SUCCESSFUL OPEN              */
*      THEN DO;                      /* GET BUFFER FOR UADS READ     */
@RC00604 L     @07,STATUS                                       01S0616
         LTR   @07,@07                                          01S0616
         BNZ   @RF00616                                         01S0616
*        RESPECIFY (GPR01P) RSTD;                               01S0618
*        DO; /*GETMAIN (R) LV(DCBBLKSI)SP(SPUADS)*/             01S0619
*        RESPECIFY (GPR01F,GPR00F) RESTRICTED;                  01S0620
*        GPR00F = DCBBLKSI; /* LENGTH REQUESTED*/               01S0621
         L     @07,DCBPTR(,WRKAPTR1)                            01S0621
         LH    GPR00F,DCBBLKSI+52(,@07)                         01S0621
*        GPR01F = SPUADS; /* SP IN REG 1 */                     01S0622
         LA    GPR01F,1                                         01S0622
*        GEN   (SLL   GPR01F,24)                                01S0623
*        NODEFS NOREFS NOSETS NOEXIT NOFLOWS SEQFLOW;           01S0623
         SLL   GPR01F,24
*        GPR00F = GPR00F  GPR01F; /* SP IN HIGH BYTE */        01S0624
         OR    GPR00F,GPR01F                                    01S0624
*        GPR01F = -1; /* INDICATE A GETMAIN REQUEST */          01S0625
         SLR   GPR01F,GPR01F                                    01S0625
         BCTR  GPR01F,0                                         01S0625
*        SVC(10); /* R-FORM OF GETMAIN*/                        01S0626
         SVC   10                                               01S0626
*        RESPECIFY (GPR01F,GPR00F) UNRESTRICTED;                01S0627
*        END; /*GETMAIN (R) LV(DCBBLKSI)SP(SPUADS)*/            01S0628
*        UADSBUFP = GPR01P;          /* ADDRESS OF UADS BUFFER       */
         ST    GPR01P,UADSBUFP                                  01S0629
*        RESPECIFY (GPR01P) UNRSTD;                             01S0630
*      END;                                                     01S0631
*                                                               01S0631
*    IF ASXBUSER ^= ZEROES                                      01S0632
*      THEN DO;                                                 01S0632
@RF00616 L     @01,CVTPTR                                       01S0632
         L     @01,CVTTCBP(,@01)                                01S0632
         L     @01,TCUASCB(,@01)                                01S0632
         L     @07,ASCBASXB(,@01)                               01S0632
         CLC   ASXBUSER(7,@07),@CB00154                         01S0632
         BE    @RF00632                                         01S0632
*        USERID = ASXBUSER;          /* MAKE COPY OF THE USERID      */
*                                                               01S0634
         MVI   USERID+7,C' '                                    01S0634
         MVC   USERID(7),ASXBUSER(@07)                          01S0634
*        RESPECIFY (GPR01P) RSTD;                               01S0635
*        GPR01P = ZERO;              /* MAY BE CHANGED BY TRT        */
         SLR   GPR01P,GPR01P                                    01S0636
*        TRT (USERID,BLANKS(1));     /* SEARCH FOR A BLANK           */
*                                                               01S0637
         TRT   USERID(8),BLANKS                                 01S0637
*        USERIDBP = GPR01P;          /* PT TO BLANK, WILL BE BLOCK#  */
         ST    GPR01P,USERIDBP                                  01S0638
*        RESPECIFY (GPR01P) UNRSTD;                             01S0639
*                                                               01S0639
*        USERIDL=USERIDBP-ADDR(USERID);  /* LENGTH OF THE USERID     */
*                                                               01S0640
         L     @07,USERIDBP                                     01S0640
         LA    @06,USERID                                       01S0640
         LCR   @06,@06                                          01S0640
         ALR   @06,@07                                          01S0640
         ST    @06,USERIDL                                      01S0640
*        IF USERIDBP = ZERO          /* CHK FOR LENGTH ERROR         */
*          THEN                      /* YES,                         */
         LTR   @07,@07                                          01S0641
         BNZ   @RF00641                                         01S0641
*            STATUS = ERRUSER;       /*   INDICATE USERID ERROR      */
         MVC   STATUS(4),@CF00034                               01S0642
*      END;                                                     01S0643
*    RETURN;                                                    01S0644
@EL00004 DS    0H                                               01S0644
@EF00004 DS    0H                                               01S0644
@ER00004 LM    @14,@12,@SA00004                                 01S0644
         BR    @14                                              01S0644
*/* * * * * * * * * * * * * * * * **** * * * * * * * * * * * * * * * */
*/*                                                                  */
*/* SEGMENT NAME -- CHKUSER                                          */
*/*                                                                  */
*/* DESCRIPTIVE NAME -- N/A                                          */
*/*                                                                  */
*/* FUNCTION -- VERIFY THAT THE USER IS A VALID TSO USER             */
*/*  OPERATION --                                                    */
*/*           - ISSUE FIND ON THE NON-BLANK PORTION OF THE USERID    */
*/*             CONCATENATED WITH A CHARACTER ZERO -- THIS WOULD BE  */
*/*             THE UADS DIRECTORY ENTRY NAME FOR THE FIRST MEMBER   */
*/*             IN UADS CONTAINING DATA PERTAINING TO THE USERID     */
*/*                                                                  */
*/* ENTRY -- LINKED BY REGISTER 14                                   */
*/*                                                                  */
*/* INPUT -- N/A                                                     */
*/*                                                                  */
*/* OUTPUT -- N/A                                                    */
*/*                                                                  */
*/* EXTERNAL REFERENCES -- N/A                                       */
*/*                                                                  */
*/* EXIT NORMAL -- RETURN BY LINK REGISTER 14                        */
*/*   RETURN CODE -- N/A                                             */
*/*                                                                  */
*/* EXIT ERROR -- N/A                                                */
*/*   CONDITIONS -- N/A                                              */
*/*   RETURN CODE -- N/A                                             */
*/*                                                                  */
*/* * * * * * * * * * * * * * * * **** * * * * * * * * * * * * * * * */
*                                                               01S0645
*CHKUSER:     /* EP TO VALIDATE-USERID ROUTINE                       */
*                                                               01S0645
*    ENTRY;                                                     01S0645
*                                                               01S0645
CHKUSER  STM   @14,@12,@SA00004                                 01S0645
*    IF STATUS = CONTINUE                                       01S0646
*      THEN DO;                                                 01S0646
         L     @07,STATUS                                       01S0646
         LTR   @07,@07                                          01S0646
         BNZ   @RF00646                                         01S0646
*        NEWBLOCK = BLOCK0;          /* FIND USERID0 IN SYS1.UADS  */
         LA    @07,240                                          01S0648
         ST    @07,NEWBLOCK                                     01S0648
*        CALL FIND;                  /* FIND USERID  BLOCK#        */
*                                                               01S0649
         BAL   @14,FIND                                         01S0649
*        NEWBLOCK = HEX40;           /* NO NEW BLOCK YET             */
         LA    @07,64                                           01S0650
         ST    @07,NEWBLOCK                                     01S0650
*      END;                                                     01S0651
*                                                               01S0651
*    RETURN;                                                    01S0652
         B     @EL00004                                         01S0652
*/* * * * * * * * * * * * * * * * **** * * * * * * * * * * * * * * * */
*/*                                                                  */
*/* SEGMENT NAME -- INITUP                                           */
*/*                                                                  */
*/* DESCRIPTIVE NAME -- N/A                                          */
*/*                                                                  */
*/* FUNCTION -- INITIALIZE THE PSCB, UPT, RLGB, AND THE LWA          */
*/*  OPERATION --                                                    */
*/*           - FIND THE BLOCK OF UADS DATA AT OFFSET ZERO FOR THIS  */
*/*             USERID.  THIS IS THE HEADER BLOCK CONTAINING DATA    */
*/*             FOR THE PSCB PLUS IT CONTAINS THE OFFSET TO THE UADS */
*/*             UPT DATA BLOCK FOR THIS USERID                       */
*/*           - INITIALIZE THE MODEL PSCB                            */
*/*           - FIND THE BLOCK OF UADS DATA AT UPT-OFFSET.  THIS IS  */
*/*             THE BLOCK OF DATA THAT CONTAINS THE USERS UPT        */
*/*           - INITIALIZE THE MODEL UPT                             */
*/*                                                                  */
*/* ENTRY -- LINKED BY REGISTER 14                                   */
*/*                                                                  */
*/* INPUT -- N/A                                                     */
*/*                                                                  */
*/* OUTPUT -- N/A                                                    */
*/*                                                                  */
*/* EXTERNAL REFERENCES -- N/A                                       */
*/*                                                                  */
*/*   RETURN CODE -- N/A                                             */
*/*                                                                  */
*/* EXIT ERROR -- N/A                                                */
*/*   CONDITIONS -- N/A                                              */
*/*   RETURN CODE -- N/A                                             */
*/*                                                                  */
*/* * * * * * * * * * * * * * * * **** * * * * * * * * * * * * * * * */
*INITUP:   /* EP TO INITIALIZE-PSCB-UPT-LWA-RLGB ROUTINE             */
*                                                               01S0653
*    ENTRY;                                                     01S0653
*                                                               01S0653
INITUP   STM   @14,@12,@SA00004                                 01S0653
*    IF STATUS = CONTINUE                                       01S0654
*      THEN DO;                                                 01S0654
         L     @07,STATUS                                       01S0654
         LTR   @07,@07                                          01S0654
         BNZ   @RF00654                                         01S0654
*        CALL FINDBLOC(ZERO,UADSDPTR);  /* READ DATA AT OFFSET ZERO  */
*                                                               01S0656
         LA    @07,@CF00059                                     01S0656
         ST    @07,@AL00001                                     01S0656
         LA    @07,UADSDPTR                                     01S0656
         ST    @07,@AL00001+4                                   01S0656
         LA    @01,@AL00001                                     01S0656
         BAL   @14,FINDBLOC                                     01S0656
*        IF UADSEXPF = YES           /* CHK FOR EXPANDED FORMAT      */
*          THEN                      /* YES,                         */
         L     @01,UADSBUFP                                     01S0657
         TM    UADSEXPF(@01),B'01000000'                        01S0657
         BNO   @RF00657                                         01S0657
*            UADSFORM = EXPANDED;    /*   SAVE THE FORMAT STATUS     */
*                                                               01S0658
         OI    UADSFORM,B'10000000'                             01S0658
*        PSCBUSER = USERID;          /* COPY USERID TO PSCB          */
@RF00657 L     @07,PSCBPTR(,WRKAPTR2)                           01S0659
         MVC   PSCBUSER(7,@07),USERID                           01S0659
*        PSCBUSRL = USERIDL;         /* SET USERID LENGTH IN PSCB    */
