*                RETCODE = RCNOTTSO; /* SET NOT-TSO-USER RETURN CODE */
         MVC   RETCODE(4),@CF00049                              01S0831
*              ELSE                                             01S0832
*                IF STATUS=GETERROR  /* CHK FOR GETMAIN FAILURE      */
