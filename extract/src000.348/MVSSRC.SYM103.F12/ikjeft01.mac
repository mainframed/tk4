         B     @RC00830                                         01S0832
@RF00830 CLC   STATUS(4),@CF00042                               01S0832
         BNE   @RF00832                                         01S0832
*                    RETCODE=RCTERM; /*   SET SPACE FAILURE RET CODE */
         MVC   RETCODE(4),@CF00063                              01S0833
*                  ELSE              /* NO, IS NOT SPACE FAILURE     */
*                    RETCODE=RCUADERR;  /* SET UADS FAILURE RET CODE */
         B     @RC00832                                         01S0834
@RF00832 MVC   RETCODE(4),@CF00079                              01S0834
*    RESPECIFY (PSCB) BASED(PSCBPTR);                           01S0835
*    RESPECIFY (UPT) BASED(UPTPTR);                             01S0836
*    END  INIT;                                                 01S0837
         B     @EL00004                                         01S0837
