         TITLE 'IKJEFT01 -- TMP INITIALIZATION ROUTINE                 *
                        '
         ENTRY  IKJEFT0A
IKJEFT01 CSECT ,                                                01S0002
@MAINENT DS    0H                                               01S0002
         USING *,@15                                            01S0002
         B     @PROLOG                                          01S0002
         DC    AL1(16)                                          01S0002
         DC    C'IKJEFT01  78.177'                              01S0002
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                  01S0002
         BALR  @10,0                                            01S0002
@PSTART  LA    @11,4095(,@10)                                   01S0002
         USING @PSTART,@10                                      01S0002
         USING @PSTART+4095,@11                                 01S0002
         L     @00,@SIZDATD                                     01S0002
         GETMAIN  R,LV=(0)
         LR    @12,@01                                          01S0002
         USING @DATD,@12                                        01S0002
         ST    @13,@SA00001+4                                   01S0002
         LM    @00,@01,20(@13)                                  01S0002
         ST    @12,8(,@13)                                      01S0002
         LR    @13,@12                                          01S0002
         MVC   @PC00001(8),0(@01)                               01S0002
LWASWAR  EQU   4
LWAMSPE  EQU   16
INITDONE EQU  4
NQUSERID EQU  8
DQUSERID EQU  12
SCHEDULE EQU  16
BCDONE   EQU  20
TERMINAT EQU  24
NQDQ0    EQU  0
NQDQ4    EQU  4
NQDQ8    EQU  8
STARTP2  EQU  16
INITRDY  EQU  20
ENDTASK  EQU  24
JSCBDISP EQU 188
JSCBAOS1 EQU  72
JSCBAOS2 EQU 192
*    /****************************************************************/
*    /* SET POINTERS TO INPUT PARAMETERS AND MODESET TO SUPV STATE   */
*    /****************************************************************/
*                                                               01S0125
*    FMAINP = R1;                   /* SAVE ENTRY PARM PTR           */
         LR    FMAINP,R1                                        01S0125
*    FSTCMDP = ADDR(PARM);          /* ESTABLISH FIRST CMD           */
         L     @06,@PC00001                                     01S0126
         ST    @06,FSTCMDP                                      01S0126
*    RETRYWAP = ADDR(WA2);          /* VALID IF FROM IKJEFT07        */
         L     @06,@PC00001+4                                   01S0127
         ST    @06,RETRYWAP                                     01S0127
*    /* SET SUPERVISOR STATE                                         */
*    DO; /*MODESET  MODE(SUP)*/                                 01S0128
*      I10701 = '00000000'B;/*OR MASK*/                         01S0129
         MVI   I10701,B'00000000'                               01S0129
*      I10702 = '00000000'B;/*AND MASK*/                        01S0130
         MVI   I10702,B'00000000'                               01S0130
*      I10703 = '00000000'B;/*RESERVED*/                        01S0131
         MVI   I10703,B'00000000'                               01S0131
*      I10704 = '00001100'B;                                    01S0132
         MVI   I10704,B'00001100'                               01S0132
*      RESPECIFY GPR01F RESTRICTED;                             01S0133
*      GPR01F = I107; /*MODESET PARMLIST*/                      01S0134
         L     GPR01F,I107                                      01S0134
*      SVC (107);/*ISSUE MODESET SVC*/                          01S0135
         SVC   107                                              01S0135
*      RESPECIFY (GPR01F) UNRESTRICTED;                         01S0136
*    END; /*MODESET  MODE(SUP)*/                                01S0137
*                                                               01S0137
*                                                               01S0137
*    /****************************************************************/
*    /* GET SPACE FOR TMP PRIVATE, PROTECTED WORK AREA, KEY 1        */
*    /****************************************************************/
*                                                               01S0138
*    IF FSTCMDP ^= EFFWD            /* CHK FOR RETRY ENTRY           */
*      THEN DO;                     /* NO, ESTABLISH STAE/STAI WA    */
         CLC   FSTCMDP(4),@CF00088                              01S0138
         BE    @RF00138                                         01S0138
*        /* GET ENTRY PSW PROTECT KEY                                */
*        RESPECIFY (R2) RSTD;                                   01S0140
*        DO; /*MODESET  SAVEKEY(R2) WORKREG(R2)*/               01S0141
*          RESPECIFY (GPR02F) RESTRICTED;                       01S0142
*          R2 = GPR02F; /*SAVE REGISTER 2*/                     01S0143
*          IPK; /*SAVE CURRENT PSW KEY*/                        01S0144
         IPK                                                    01S0144
*          R2 = GPR02F;                                         01S0145
*          GPR02F = R2; /*RESTORE REGISTER 2*/                  01S0146
*          RESPECIFY (GPR02F) UNRESTRICTED;                     01S0147
*        END; /*MODESET  SAVEKEY(R2) WORKREG(R2)*/              01S0148
*        RESPECIFY (R0,R1,R3) RSTD;                             01S0149
*        R3 = R2;                   /* HOLD ENTRY PSW KEY            */
         LR    R3,R2                                            01S0150
*        CGMBLKL=LENGTH(TMPWRKA2);  /* SET LENGTH FOR GETMAIN        */
         MVC   CGMBLKL(3),@CF03531+1                            01S0151
*        CGMBLKN = SPNM230;         /* SET FOR SUBPOOL 230           */
         MVI   CGMBLKN,X'E6'                                    01S0152
*        R0 = CGMBLKA;              /* SET GETMAIN PARAMETERS        */
         L     R0,CGMBLKA                                       01S0153
*        /* SET PSW PROTECT KEY 1                                    */
*        DO; /*MODESET  KEYADDR(SPROK1) WORKREG(R2)*/           01S0154
*          R2 = SPROK1; /*GET KEY FORM SAVE LOCATION*/          01S0155
         L     R2,SPROK1                                        01S0155
*          SPKA (R2); /*SET PSW KEY*/                           01S0156
         SPKA  0(R2)                                            01S0156
*        END; /*MODESET  KEYADDR(SPROK1) WORKREG(R2)*/          01S0157
*        GEN(GETMAIN  R,LV=(0));    /* GETMAIN PROTECTED STORAGE     */
         GETMAIN  R,LV=(0)
*        WRKAPTR2 = R1;             /* SET PTR TO PROTECTED STORAGE  */
         LR    WRKAPTR2,R1                                      01S0159
*        TWRKA2A=TWRKA2A&&TWRKA2A;  /* CLEAR TMP PROTECTED WORK      */
         XC    TWRKA2A(168,WRKAPTR2),TWRKA2A(WRKAPTR2)          01S0160
*        TWRKA2B=TWRKA2B&&TWRKA2B;  /*   AREA                        */
         XC    TWRKA2B(64,WRKAPTR2),TWRKA2B(WRKAPTR2)           01S0161
*      END;                                                     01S0162
*      ELSE DO;                     /* ENTRY IS RETRY ENTRY          */
         B     @RC00138                                         01S0163
@RF00138 DS    0H                                               01S0164
*        WRKAPTR2 = RETRYWAP;       /* RESTORE PTR TO PROTECTED WA   */
         L     WRKAPTR2,RETRYWAP                                01S0164
*        DO; /*MODESET  KEYADDR(SPROK1) WORKREG(R2)*/           01S0165
*          R2 = SPROK1; /*GET KEY FORM SAVE LOCATION*/          01S0166
         L     R2,SPROK1                                        01S0166
*          SPKA (R2); /*SET PSW KEY*/                           01S0167
         SPKA  0(R2)                                            01S0167
*        END; /*MODESET  KEYADDR(SPROK1) WORKREG(R2)*/          01S0168
*        R3 = TEPKEY;               /* SET FOR ENTRY PSW PROTECT KEY */
         L     R3,TEPKEY(,WRKAPTR2)                             01S0169
*      END;                                                     01S0170
*    WRKA2PTR = WRKAPTR2;           /* RESERVE PTR TO PROTECTED STG  */
@RC00138 ST    WRKAPTR2,WRKA2PTR(,WRKAPTR2)                     01S0171
*    TEPKEY = R3;                   /* RESERVE ENTRY PSW KEY         */
         ST    R3,TEPKEY(,WRKAPTR2)                             01S0172
*    /* RESTORE ENTRY PSW PROTECT KEY                                */
*    R2 = R3;                       /* SET FOR MODESET               */
         LR    R2,R3                                            01S0173
*    DO; /*MODESET  KEYADDR(R2) WORKREG(R2)*/                   01S0174
*      R2 = R2; /*GET KEY FORM SAVE LOCATION*/                  01S0175
*      SPKA (R2); /*SET PSW KEY*/                               01S0176
         SPKA  0(R2)                                            01S0176
*    END; /*MODESET  KEYADDR(R2) WORKREG(R2)*/                  01S0177
*    RESPECIFY (R0,R1,R2,R3) UNRSTD;                            01S0178
*    /****************************************************************/
*    /* CALCULATE  SIZE  OF TMP DYNAMIC WORKAREA  AND  GETMAIN  FROM */
*    /*   SUBPOOL 1.   ZERO OUT THE AREA.                            */
*    /****************************************************************/
*                                                               01S0179
*    RESPECIFY (R0,R1) RSTD;                                    01S0179
*    TOTLEN =                       /* CALCULATED LENGTH             */
*        LENGTH(TMPWRKA1)           /* TMPWRKA1 LENGTH               */
*        + STAXPLEN;                /* ADD STAXPARM LENGTH           */
         LA    TOTLEN,1708                                      01S0180
         AH    TOTLEN,STAXPLEN                                  01S0180
*    R0 = SPL1 + TOTLEN;            /* ADD SPL1 IN HIGH ORDER BYTE   */
         LR    R0,TOTLEN                                        01S0181
         AL    R0,@CF00132                                      01S0181
*    GEN(GETMAIN R,LV=(0));         /* GETMAIN FOR DYNAMIC WORKAREA  */
*                                                               01S0182
         GETMAIN R,LV=(0)
*    WRKAPTR1 = R1;                 /* RESERVE PTR TO PP WORK AREA   */
         LR    WRKAPTR1,R1                                      01S0183
*    RESPECIFY (R0) UNRSTD;                                     01S0184
*    /* SET PROTECT KEY 1                                            */
*    DO; /*MODESET  SAVEKEY(R2) KEYADDR(SPROK1) WORKREG(R2)*/   01S0185
*      RESPECIFY (GPR02F) RESTRICTED;                           01S0186
*      R2 = GPR02F; /*SAVE REGISTER 2*/                         01S0187
*      IPK; /*SAVE CURRENT PSW KEY*/                            01S0188
         IPK                                                    01S0188
*      R2 = GPR02F;                                             01S0189
*      GPR02F = R2; /*RESTORE REGISTER 2*/                      01S0190
*      SPKA (SPROK1); /*SET PSW KEY*/                           01S0191
         L     @06,SPROK1                                       01S0191
         SPKA  0(@06)                                           01S0191
*      RESPECIFY (GPR02F) UNRESTRICTED;                         01S0192
*    END; /*MODESET  SAVEKEY(R2) KEYADDR(SPROK1) WORKREG(R2)*/  01S0193
*    WRKA1PTR = WRKAPTR1;           /* SET PTR TO PP WORK AREA       */
         ST    WRKAPTR1,WRKA1PTR(,WRKAPTR2)                     01S0194
*    RESPECIFY (R2) RSTD;                                       01S0195
*    /* RESTORE ENTRY PSW PROTECT KEY                                */
*    DO; /*MODESET  KEYADDR(R2) WORKREG(R2)*/                   01S0196
*      R2 = R2; /*GET KEY FORM SAVE LOCATION*/                  01S0197
*      SPKA (R2); /*SET PSW KEY*/                               01S0198
         SPKA  0(R2)                                            01S0198
*    END; /*MODESET  KEYADDR(R2) WORKREG(R2)*/                  01S0199
*                                                               01S0199
*    CLTMPC = TOTLEN;               /* SET TO TOTAL LENGTH           */
         LR    CLTMPC,TOTLEN                                    01S0200
*    DO WHILE CLTMPC > D255;        /* TO CLEAR BULK OF WORK AREA    */
         B     @DE00201                                         01S0201
@DL00201 DS    0H                                               01S0202
*      TMPWRKA1((CLTMPC-D254):CLTMPC) =                         01S0202
*          TMPWRKA1((CLTMPC-D254):CLTMPC)&&                     01S0202
*          TMPWRKA1((CLTMPC-D254):CLTMPC);                      01S0202
         LR    @06,WRKAPTR1                                     01S0202
         ALR   @06,CLTMPC                                       01S0202
         AL    @06,@CF03542                                     01S0202
         XC    TMPWRKA1(255,@06),TMPWRKA1(@06)                  01S0202
*      CLTMPC = CLTMPC-D255;                                    01S0203
         SL    CLTMPC,@CF00084                                  01S0203
*    END;                                                       01S0204
@DE00201 C     CLTMPC,@CF00084                                  01S0204
         BH    @DL00201                                         01S0204
*    IF CLTMPC > ZERO                                           01S0205
*      THEN TMPWRKA1(1:CLTMPC) = TMPWRKA1(1:CLTMPC)             01S0205
         LTR   CLTMPC,CLTMPC                                    01S0205
         BNP   @RF00205                                         01S0205
*           &&TMPWRKA1(1:CLTMPC);                               01S0206
         LR    @06,CLTMPC                                       01S0206
         BCTR  @06,0                                            01S0206
         EX    @06,@SX03543                                     01S0206
*                                                               01S0207
*    /****************************************************************/
*    /* USE EXTRACT TO OBTAIN POINTERS TO THE ADDRESS OF THE STOP/   */
*    /* MODIFY ECB, OF THE TIME SHARING FLAGS FIELD, AND OF THE PSCB.*/
*    /* THE POINTERS ARE PLACED  IN THREE CONSECUTIVE WORDS  IN THE  */
*    /* TMP WORKAREA.   A POINTER TO THE STOP/MODIFY ECB POINTER IS  */
*    /* LOCATED IN THE FIRST WORD, A POINTER TO THE TS FLAG FIELD IN */
*    /* THE SECOND WORD,  AND  THE PSCBPTR  IS LOCATED IN THE THIRD  */
*    /* WORD.  THE  NXTCMD AREA  IS TEMPORARILY USED  BY THE EXTRACT */
*    /* MACRO.                                                       */
*    /****************************************************************/
*                                                               01S0207
*    R2 = ADDR(TPLMECB);            /* PTR TO EXTRACT ANSWER FIELD   */
@RF00205 LA    R2,TPLMECB(,WRKAPTR1)                            01S0207
*    R1 = ADDR(XTRCLST);            /* TEMPORARILY USE COMMAND NAME
*                                      AREA FOR EXTRACT WORK AREA    */
         LA    R1,XTRCLST(,WRKAPTR1)                            01S0208
*    /* EXTRACT PTRS TO STOP/MODIFY ECB, TSO FLAGS AND PSCB          */
*    GEN(EXTRACT (R2),'S',FIELDS=(COMM,TSO,PSB),MF=(E,(1)));    01S0209
         EXTRACT (R2),'S',FIELDS=(COMM,TSO,PSB),MF=(E,(1))
*    RESPECIFY (R1,R2) UNRSTD;                                  01S0210
*    TPLMECB = MECBPTR;             /* ESTABLISH STOP/MODIFY ECB     */
         L     @06,TPLMECB(,WRKAPTR1)                           01S0211
         L     @06,MECBPTR(,@06)                                01S0211
         ST    @06,TPLMECB(,WRKAPTR1)                           01S0211
*    TPLPSCB = PSCBP;               /* ESTABLISH PSCB                */
*                                                               01S0212
         L     @06,PSCBP(,WRKAPTR1)                             01S0212
         ST    @06,TPLPSCB(,WRKAPTR1)                           01S0212
*    /****************************************************************/
*    /* INITIALIZE POINTERS AND PARAMETER BLOCKS IN THE TMP WORKAREA */
*    /* THIS AREA  WILL BE USED  FOR THE REMAINDER  OF THE TERMINAL  */
*    /* SESSION  AND  IS REFERENCED BY  THE TMP ATTENTION EXIT, THE  */
*    /* TMP STAE EXIT, AND THE TMP STAI EXIT                         */
*    /****************************************************************/
*                                                               01S0213
*    /* ESTABLISH POINTERS FOR PARM BLOCKS                           */
*    SRPLPTR = ADDR(CSPLSP);        /* SERVICE ROUTINE PARM LIST     */
         LA    @06,CSPLSP(,WRKAPTR1)                            01S0213
         ST    @06,SRPLPTR(,WRKAPTR1)                           01S0213
*    CSPLPTR = SRPLPTR;             /* SET PTR TO CMD SCAN PL        */
         ST    @06,CSPLPTR(,WRKAPTR1)                           01S0214
*    IOPLPTR = SRPLPTR;             /* SET PTR TO I/O SERVICE PL     */
         ST    @06,IOPLPTR(,WRKAPTR1)                           01S0215
*    ASRPLPTR=ADDR(CSPLSP2);        /* SRV RTN PARM LIST FOR ATTN    */
         LA    @06,CSPLSP2(,WRKAPTR1)                           01S0216
         ST    @06,ASRPLPTR(,WRKAPTR1)                          01S0216
*    SCANAP = ADDR(CSOASP);         /* SCAN ANSWER AREA              */
         LA    @06,CSOASP(,WRKAPTR1)                            01S0217
         ST    @06,SCANAP(,WRKAPTR1)                            01S0217
*    ASCANAP=ADDR(CSOASP2);         /* SCAN ANSWER AREA FOR ATTN     */
         LA    @06,CSOASP2(,WRKAPTR1)                           01S0218
         ST    @06,ASCANAP(,WRKAPTR1)                           01S0218
*    GTPBPTR = ADDR(GTPBSP);        /* GETLINE PARAMETER BLOCK       */
         LA    @06,GTPBSP(,WRKAPTR1)                            01S0219
         ST    @06,GTPBPTR(,WRKAPTR1)                           01S0219
*    PGPBPTR = ADDR(PGPBSP);        /* PUTGET PARAMETER BLOCK        */
         LA    @06,PGPBSP(,WRKAPTR1)                            01S0220
         ST    @06,PGPBPTR(,WRKAPTR1)                           01S0220
*    PTPBPTR = ADDR(PTPBSP);        /* PUTLINE PARAMETER BLOCK       */
         LA    @06,PTPBSP(,WRKAPTR1)                            01S0221
         ST    @06,PTPBPTR(,WRKAPTR1)                           01S0221
*    ATTCHPTR = ADDR(ATTCHSP);      /* ATTACH LIST                   */
         LA    @06,ATTCHSP(,WRKAPTR1)                           01S0222
         ST    @06,ATTCHPTR(,WRKAPTR1)                          01S0222
*    STPBPTR = ADDR(STPBSP);        /* STACK PARAMETER BLOCK         */
         LA    @06,STPBSP(,WRKAPTR1)                            01S0223
         ST    @06,STPBPTR(,WRKAPTR1)                           01S0223
*    DYNAPPTR = ADDR(MDYNASP);      /* DYNAMIC ALLOCATION PB         */
         LA    @06,MDYNASP(,WRKAPTR2)                           01S0224
         ST    @06,DYNAPPTR(,WRKAPTR1)                          01S0224
*    PPLPTR = ADDR(PPLSP);          /* PARSE PARAMETER LIST          */
         LA    @06,PPLSP(,WRKAPTR1)                             01S0225
         ST    @06,PPLPTR(,WRKAPTR1)                            01S0225
*    DAPLPTR = ADDR(DAPLSP);        /* DAIR PARAMETER LIST           */
         LA    @06,DAPLSP(,WRKAPTR1)                            01S0226
         ST    @06,DAPLPTR(,WRKAPTR1)                           01S0226
*    DAPB0PTR = ADDR(DAPBSP);       /* DAIR PARAMETER BLOCK          */
         LA    @06,DAPBSP(,WRKAPTR1)                            01S0227
         ST    @06,DAPB0PTR(,WRKAPTR1)                          01S0227
*    READYPTR=ADDR(MODEMSP);        /* READY MESSAGE                 */
         LA    @06,MODEMSP(,WRKAPTR1)                           01S0228
         ST    @06,READYPTR(,WRKAPTR1)                          01S0228
*    MODEMLG = RDYLEN-SEVEN;        /* GET LENGTH OF TEXT            */
         LA    @05,7                                            01S0229
         L     @04,@CA00040                                     01S0229
         LH    MODEMLG,RDYLEN-RDYMSG(,@04)                      01S0229
         SLR   MODEMLG,@05                                      01S0229
*    STAXPPTR=(READYPTR+((MODEMLG+SEVEN)&FWD2));  /* STAX PL         */
         ALR   @05,MODEMLG                                      01S0230
         N     @05,@CF00097                                     01S0230
         ALR   @06,@05                                          01S0230
         ST    @06,STAXPPTR(,WRKAPTR1)                          01S0230
*                                                               01S0231
*                                                               01S0231
*    CPPLPTR = ADDR(CPPLSP);        /* SET PTR TO CP PARAMETER LIST  */
         LA    @06,CPPLSP(,WRKAPTR1)                            01S0231
         ST    @06,CPPLPTR(,WRKAPTR1)                           01S0231
*    TPLECT = ADDR(ECTSP);          /* ECT TABLE                     */
         LA    @06,ECTSP(,WRKAPTR1)                             01S0232
         ST    @06,TPLECT(,WRKAPTR1)                            01S0232
*    RESPECIFY (ECT) BASED(TPLECT);                             01S0233
*    ECT = ECT && ECT;              /* SET COMPLETE ECT TO ZERO      */
         XC    ECT(56,@06),ECT(@06)                             01S0234
*    CTLBKSP = CTLBKSP&&CTLBKSP;    /* CLEAR PARAMETERS              */
*                                                               01S0235
         XC    CTLBKSP(12,WRKAPTR1),CTLBKSP(WRKAPTR1)           01S0235
*    ECTBKGRD = FGRDNOW;            /* CLEAR BACKGROUND FLAG         */
         NI    ECTBKGRD(@06),B'11111101'                        01S0236
*    /* SET PROTECT KEY 1                                            */
*    DO; /*MODESET  KEYADDR(SPROK1) WORKREG(R2)*/               01S0237
*      R2 = SPROK1; /*GET KEY FORM SAVE LOCATION*/              01S0238
         L     @05,SPROK1                                       01S0238
         LR    R2,@05                                           01S0238
*      SPKA (R2); /*SET PSW KEY*/                               01S0239
         SPKA  0(R2)                                            01S0239
*    END; /*MODESET  KEYADDR(SPROK1) WORKREG(R2)*/              01S0240
*    BKGMODE = FGRDNOW;             /* CLEAR BACKGROUND FLAG         */
         NI    BKGMODE(WRKAPTR2),B'01111111'                    01S0241
*    /* RESTORE ENTRY PSW PROTECT KEY                                */
*    DO; /*MODESET  KEYADDR(TEPKEY) WORKREG(R2)*/               01S0242
*      R2 = TEPKEY; /*GET KEY FORM SAVE LOCATION*/              01S0243
         L     @04,TEPKEY(,WRKAPTR2)                            01S0243
         LR    R2,@04                                           01S0243
*      SPKA (R2); /*SET PSW KEY*/                               01S0244
         SPKA  0(R2)                                            01S0244
*    END; /*MODESET  KEYADDR(TEPKEY) WORKREG(R2)*/              01S0245
*    IF FGDTSKF = BTSKNOW           /* CHK IF BACKGROUND TASK        */
*      THEN DO;                     /* YES, IS BACKGROUND TASK       */
         L     @02,TSOFP(,WRKAPTR1)                             01S0246
         TM    FGDTSKF(@02),B'10000000'                         01S0246
         BNZ   @RF00246                                         01S0246
*        /* SET PROTECT KEY 1                                        */
*        DO; /*MODESET  KEYADDR(SPROK1) WORKREG(R2)*/           01S0248
*          R2 = SPROK1; /*GET KEY FORM SAVE LOCATION*/          01S0249
         LR    R2,@05                                           01S0249
*          SPKA (R2); /*SET PSW KEY*/                           01S0250
         SPKA  0(R2)                                            01S0250
*        END; /*MODESET  KEYADDR(SPROK1) WORKREG(R2)*/          01S0251
*        BKGMODE = BKGDNOW;         /* SET PROTECTED BACKGROUND FLAG */
         OI    BKGMODE(WRKAPTR2),B'10000000'                    01S0252
*        /* RESTORE ENTRY PSW PROTECT KEY                            */
*        DO; /*MODESET  KEYADDR(TEPKEY) WORKREG(R2)*/           01S0253
*          R2 = TEPKEY; /*GET KEY FORM SAVE LOCATION*/          01S0254
         LR    R2,@04                                           01S0254
*          SPKA (R2); /*SET PSW KEY*/                           01S0255
         SPKA  0(R2)                                            01S0255
*        END; /*MODESET  KEYADDR(TEPKEY) WORKREG(R2)*/          01S0256
*        ECTBKGRD = BKGDNOW;        /* SET BACKGROUND FLAG           */
*                                                               01S0257
         OI    ECTBKGRD(@06),B'00000010'                        01S0257
*        CALL  BKGBLDR;             /* TO BUILD BKGD CONTROL BLOCKS  */
         BAL   @14,BKGBLDR                                      01S0258
*        RCODE = R15;               /* RESERVE RETURN CODE           */
         ST    R15,RCODE(,WRKAPTR1)                             01S0259
*        TPLPSCB = JSCBPSCB;        /* SET PTR TO PSCB               */
         L     @06,CVTPTR                                       01S0260
         L     @06,CVTTCBP(,@06)                                01S0260
         L     @06,TCURTCB(,@06)                                01S0260
         L     @06,TCBJSCB(,@06)                                01S0260
         L     @06,JSCBPSCB(,@06)                               01S0260
         ST    @06,TPLPSCB(,WRKAPTR1)                           01S0260
*      END;                                                     01S0261
*    DCBPTR = ADDR(DCBSP);          /* PTR TO DCB                    */
*                                                               01S0262
*                                                               01S0262
@RF00246 LA    @06,DCBSP(,WRKAPTR1)                             01S0262
         ST    @06,DCBPTR(,WRKAPTR1)                            01S0262
*    /* INITIALIZED PARAMETER BLOCKS ARE COPIED INTO THE TMP         */
*    /*   DYNAMIC WORKAREA USED BY REENTRANT CODE                    */
