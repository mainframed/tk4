         L     @05,ECTPTR(,WRKAPTR2)                            01S0962
         L     @04,TPLCECB(,WRKAPTR1)                           01S0962
         MVC   ECTRTCD(3,@05),ECBCODE(@04)                      01S0962
*    TMPRTRY = NO;                  /* RESET RETRY INDICATORS        */
*    TMPRINIT = NO;                 /* RESET RE-INITIALIZATION FLAG  */
         L     @04,TMPWA2P(,WRKAPTR2)                           01S0964
         NI    TMPRTRY(@04),B'00111111'                         01S0964
