*    ENTRY;                                                     01S0545
*                                                               01S0545
TFRECBF  DS    0H                                               01S0546
*    DOFRECB = NO;                  /* RESET DO-FREE-CMD-BUFFER FLAG */
         NI    DOFRECB(WRKAPTR1),B'10111111'                    01S0546
*    R0 = CPPLCBUF->CBUFLNG+SPL1;   /* GET BUFFER LENGTH AND         */
         L     @14,CPPLPTR(,WRKAPTR1)                           01S0547
         L     @14,CPPLCBUF(,@14)                               01S0547
         L     R0,@CF00093                                      01S0547
         AH    R0,CBUFLNG(,@14)                                 01S0547
*                                   /*   ADD THE SUBPOOL NUMBER      */
*    R1 = CPPLCBUF;                 /* GET BUFFER POINTER            */
         LR    R1,@14                                           01S0548
*    GEN(FREEMAIN R,LV=(0),A=(1));  /* FREE CMD BUFFER               */
*                                                               01S0549
         FREEMAIN R,LV=(0),A=(1)
*    CPPLCBUF = TPLCBUF;            /* RESTORE PREV CMD BUFFER PTR   */
         L     @14,CPPLPTR(,WRKAPTR1)                           01S0550
         L     @06,TPLCBUF(,WRKAPTR1)                           01S0550
         ST    @06,CPPLCBUF(,@14)                               01S0550
*    CPPLPS = TPLPS;                /* RESTORE PREV PARM LIST        */
*                                                               01S0551
         MVC   CPPLPS(12,@14),TPLPS(WRKAPTR1)                   01S0551
*    RESPECIFY (R14) RSTD;                                      01S0552
*    R14 = ADDR(TINVOKB);           /* SET FOR PSEUDO-RETURN         */
         LA    R14,TINVOKB                                      01S0553
*    RETURN;                        /* RETURN TO CALLER              */
         B     @EL00005                                         01S0554
*    RESPECIFY (R14) UNRSTD;                                    01S0555
