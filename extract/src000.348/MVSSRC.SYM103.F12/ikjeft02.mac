*/* SEGMENT NAME -- TATTNCK                                          */
*/*                                                                  */
*/* DESCRIPTIVE NAME -- N/A                                          */
*/*                                                                  */
*/* FUNCTION -- TO SET UP CONTROLS TO PROCESS THE NEW COMMAND NAME   */
*/*       INTRODUCED THROUGH ATTENTION                               */
*/*  OPERATION --                                                    */
*/*           - RESET ATTENTION COMMUNICATION ECB                    */
*/*           - RESET POINTER TO ANY ABENDING TCB                    */
*/*           - CHECK IF INITIAL COMMAND (FROM LOGON PROCEDURE       */
*/*             EXEC PARAMETER FIELD) HAS BEEN EXECUTED              */
*/*             - IF NOT YET EXECUTED, SET TMPCMDW FLAG TO INDICATE  */
*/*               THAT A COMMAND ENTERED THROUGH ATTENTION IS NOW    */
*/*               WAITING TO BE INITIATED, ISSUE STATUS START TO     */
*/*               ALLOW INITIAL COMMAND TO RESUME OPERATION, AND     */
*/*               SET DOWAIT FLAG TO PASS CONTROL TO TMP WAIT        */
*/*             - IF INITIAL COMMAND HAS ALREADY COMPLETED THEN      */
*/*               SET TMPACTRL FLAG TO INDICATE ATTENTION PROCESSING */
*/*               IS IN CONTROL, SET UP COMMAND SCAN PARAMETER LIST  */
*/*               FROM THE ATTENTION INPUT BUFFER, CLEAR THE COMMAND */
*/*               WAITING FLAG, TMPCMDW, CLEAR THE ABEND-PROCESSING- */
*/*               IN-CONTROL FLAG, TMPSCTRL, RESET THE SUB-TASK-     */
*/*               ABEND-EXIT-PROCESSING-IN-CONTROL FLAG AND SET THE  */
*/*               DOSCAN FLAG TO PASS CONTROL TO THE COMMAND SCAN    */
*/*               ROUTINE                                            */
*/*                                                                  */
*/* ENTRY -- LINKED BY REGISTER 14                                   */
*/*                                                                  */
*/* INPUT -- N/A                                                     */
*/*                                                                  */
*/* OUTPUT -- N/A                                                    */
*/*                                                                  */
*/* EXTERNAL REFERENCES -- N/A                                       */
*/*                                                                  */
*/* EXIT NORMAL -- RETURN BY LINK REGISTER 14                        */
*/*                                                                  */
*/*   RETURN CODE -- N/A                                             */
*/*                                                                  */
*/* EXIT ERROR -- N/A                                                */
*/*                                                                  */
*/*   CONDITIONS -- N/A                                              */
*/*                                                                  */
*/*   RETURN CODE -- N/A                                             */
*/*                                                                  */
*/* * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * * */
*TATTNCK:   /* EP TO PROCESS-ATTENTION-INTERRUPT ROUTINE             */
*    ENTRY;                                                     01S0226
TATTNCK  DS    0H                                               01S0227
*    RESPECIFY (R6,R14) RSTD;                                   01S0227
*    R6 = R14;                      /* RESERVE ENTRY REG 14          */
         LR    R6,R14                                           01S0228
*    RESPECIFY (R14) UNRSTD;                                    01S0229
*                                                               01S0229
*    TMPAECB = ZERO;                /* RESET ECB                     */
         SLR   @14,@14                                          01S0230
         ST    @14,TMPAECB(,WRKAPTR1)                           01S0230
*    TPLNTCB = ZERO;                /* RESET PTR TO ABENDING TCB     */
*                                                               01S0231
         ST    @14,TPLNTCB(,WRKAPTR1)                           01S0231
*    IF TMPNFCMD = NO               /* CHK IF FREE COMMAND DONE YET  */
*      THEN DO;                     /* NO,                           */
         TM    TMPNFCMD(WRKAPTR1),B'00100000'                   01S0232
         BNZ   @RF00232                                         01S0232
*        TMPCMDW = YES;             /* REMEMBER COMMAND WAITING      */
         OI    TMPCMDW(WRKAPTR1),B'01000000'                    01S0234
*        GEN(STATUS START);         /* START CP SUBTASK              */
         STATUS START
*        DOWAIT = YES;              /* WAIT FOR FREE COMMAND END     */
         OI    DOWAIT(WRKAPTR1),B'00000100'                     01S0236
*      END;                                                     01S0237
*      ELSE DO;                     /* YES, FREE COMMAND DONE        */
         B     @RC00232                                         01S0238
@RF00232 DS    0H                                               01S0239
*        TMPACTRL = YES;            /* SHOW ATTENTION OCCURRED       */
         OI    TMPACTRL(WRKAPTR1),B'00010000'                   01S0239
*        FRSTEX = NO;               /* RESET IMPLICIT EXEC INDICATOR
*                                                            @ZA17835*/
         NI    FRSTEX(WRKAPTR1),B'11101111'                     01S0240
*        CSPLCBUF=TMPCMDWT;         /* PICK UP WAITING CMD           */
         L     @14,TMPCMDWT(,WRKAPTR1)                          01S0241
         ST    @14,CSPLCBUF(,SRPARMLP)                          01S0241
*        CSPLCBUF->CBUFOFF=ZERO;    /* PTR FOR RESCAN                */
         SLR   @05,@05                                          01S0242
         STH   @05,CBUFOFF(,@14)                                01S0242
*        TMPCMDW = NO;              /* RESET CMD WAIT INDICATOR      */
         NI    TMPCMDW(WRKAPTR1),B'10111111'                    01S0243
*        TMPCMDWT = ZERO;           /* RESET COMMAND-WAITING PTR     */
         ST    @05,TMPCMDWT(,WRKAPTR1)                          01S0244
*        TMPSCTRL = NO;             /* SHOW STAI NOT IN CONTROL      */
         NI    TMPSCTRL(WRKAPTR1),B'11110111'                   01S0245
*        TMPIECB = ZERO;            /* RESET STAI ECB                */
         ST    @05,TMPIECB(,WRKAPTR1)                           01S0246
*        DOSCAN = YES;              /* SET TO DO COMMAND SCAN        */
         OI    DOSCAN(WRKAPTR1),B'00000010'                     01S0247
*      END;                                                     01S0248
*                                                               01S0248
*    RESPECIFY (R14) RSTD;                                      01S0249
@RC00232 DS    0H                                               01S0250
*    R14 = R6;                      /* RESTORE ENTRY REG 14          */
         LR    R14,R6                                           01S0250
*    RETURN;                        /* RETURN TO CALLER              */
         B     @EL00003                                         01S0251
*    RESPECIFY (R6,R14) UNRSTD;                                 01S0252
*/* * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * * */
