*/* SEGMENT NAME -- TCHKAT1                                          */
*/*                                                                  */
*/* DESCRIPTIVE NAME -- N/A                                          */
*/*                                                                  */
*/* FUNCTION -- TO DETACH  OR  TO SET UP DETACH FOR ANY CURRENTLY    */
*/*       INCOMPLETE SUBTASK                                         */
*/*  OPERATION --                                                    */
*/*           - CHECK IF CURRENT TASK IS COMPLETE (IF THE TMP'S      */
*/*             CURRENT IMMEDIATE SUBTASK TCB, LOCATED BY POINTER    */
*/*             TPLCTCB) HAS BEEN CLEARED                            */
*/*             - IF STILL ACTIVE (IMCOMPLETE), THEN --              */
*/*               - SET ABNORMAL-DETACH FLAG, ECTATRM                */
*/*               - CHECK IF ANY CURRENTLY ABENDING TCB (LOCATED     */
*/*                 BY POINTER TPLNTCB) IS THE TCB FOR THE TMP'S     */
*/*                 IMMEDIATE SUBTASK                                */
*/*                 OR IF THAT CONDITION HAS PREVIOUSLY OCCURRED     */
*/*                 (FLAG FRSTLAB IS ON)                             */
*/*                 - IF YES TO EITHER, SET ALLOW-ABEND-COMPLETION   */
*/*                   COMPLETION FLAG, DOPSTRT                       */
*/*                 - IF NEITHER IS TRUE (NO PRIME LEVEL ABEND IS    */
*/*                   PENDING) THEN --                               */
*/*                   - PERFORM DETACH-TMP-SUBTASK ROUTINE, TDETCP   */
*/*                   - IF ABEND PROCESSING IS IN CONTROL, THEN --   */
*/*                     - SET PERFORM WAIT-ON-ECB-LIST ROUTINE FLAG, */
*/*                       DOLIST                                     */
*/*                                                                  */
*/* ENTRY -- LINKED BY REGISTER 14                                   */
*/*                                                                  */
*/* INPUT -- N/A                                                     */
*/*                                                                  */
*/* OUTPUT -- N/A                                                    */
*/*                                                                  */
*/* EXTERNAL REFERENCES -- N/A                                       */
*/*                                                                  */
*/* EXIT NORMAL -- RETURN BY LINK REGISTER 14                        */
*/*                                                                  */
*/*   RETURN CODE -- N/A                                             */
*/*                                                                  */
*/* EXIT ERROR -- N/A                                                */
*/*                                                                  */
*/*   CONDITIONS -- N/A                                              */
*/*                                                                  */
*/*   RETURN CODE -- N/A                                             */
*/*                                                                  */
*/* * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * * */
*TCHKAT1:   /* EP TO CHECK/DETACH-RUNNING-CP ROUTINE                 */
*    ENTRY;                                                     01S0739
*                                                               01S0739
TCHKAT1  DS    0H                                               01S0740
*    IF TPLCTCB ^= ZERO             /* CHK FOR CP CURRENTLY RUNNING  */
*      THEN DO;                     /* YES, DETACH RUNNING CP        */
         L     @14,TPLCTCB(,WRKAPTR1)                           01S0740
         LTR   @14,@14                                          01S0740
         BZ    @RF00740                                         01S0740
*        ECTATRM=YES;               /* SIGNIFY ABNORMAL END          */
         L     @06,ECTPTR(,WRKAPTR2)                            01S0742
         OI    ECTATRM(@06),B'00100000'                         01S0742
*        IF TPLCTCB=TPLNTCB         /* CHK IF IS CURRENT CP OR FOR   */
*             FRSTLAB=YES          /*   PRIOR 1ST LEVEL ABEND       */
*          THEN                     /* YES,                          */
         C     @14,TPLNTCB(,WRKAPTR1)                           01S0743
         BE    @RT00743                                         01S0743
         TM    FRSTLAB(WRKAPTR1),B'00000010'                    01S0743
         BNO   @RF00743                                         01S0743
@RT00743 DS    0H                                               01S0744
*            DOPSTRT=YES;           /* GO TO COMPLETE ABEND          */
         OI    DOPSTRT(WRKAPTR1),B'00100000'                    01S0744
*          ELSE DO;                 /* IS NOT PRIME CP ABEND         */
         B     @RC00743                                         01S0745
@RF00743 DS    0H                                               01S0746
*            CALL  TDETCP;          /* DETACH ABENDED CP             */
         BAL   @14,TDETCP                                       01S0746
*            IF TMPSCTRL = YES      /* IS STAI IN CONTROL            */
*              THEN                 /* YES,                          */
         TM    TMPSCTRL(WRKAPTR1),B'00001000'                   01S0747
         BNO   @RF00747                                         01S0747
*                DOLIST=YES;        /*   TO CHK NEXT CP ACTION       */
         OI    DOLIST(WRKAPTR1),B'00100000'                     01S0748
*          END;                                                 01S0749
@RF00747 DS    0H                                               01S0750
*      END;                                                     01S0750
@RC00743 DS    0H                                               01S0751
*       IF CALLNOW = YES            /* CHK IF CALL ACTIVE     ZA27469*/
*           & TPLCTCB = ZERO        /* NO CURRENT ATTACH      ZA27469*/
*         THEN DO;                  /* YES,                   ZA27469*/
@RF00740 TM    CALLNOW(WRKAPTR1),B'00001000'                    01S0751
         BNO   @RF00751                                         01S0751
         L     @14,TPLCTCB(,WRKAPTR1)                           01S0751
         LTR   @14,@14                                          01S0751
         BNZ   @RF00751                                         01S0751
*           CALLNOW = NO;         /* SET CALL-NOT-ACTIVE FLAG ZA27469*/
         NI    CALLNOW(WRKAPTR1),B'11110111'                    01S0753
*           CALL DOCALL2;           /* GO TO TERMINATION      ZA27469*/
         L     @15,@CV00134                                     01S0754
         BALR  @14,@15                                          01S0754
*         END;                      /*                        ZA27469*/
*    RESPECIFY (R14) RSTD;                                      01S0756
@RF00751 DS    0H                                               01S0757
*    R14 = ADDR(TCHKATA);           /* SET FOR PSEUDO-RETURN         */
         LA    R14,TCHKATA                                      01S0757
*    RETURN;                        /* RETURN TO CALLER              */
         B     @EL00006                                         01S0758
*    RESPECIFY (R14) UNRSTD;                                    01S0759
*/* * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * * */
*/*                                                                  */
*/* SEGMENT NAME -- TDOTEST                                          */
*/*                                                                  */
*/* DESCRIPTIVE NAME -- N/A                                          */
*/*                                                                  */
*/* FUNCTION -- TO SET UP CONDITIONS FOR TEST, PERFORM LINK TO TEST, */
*/*       AND RESTORE MAINLINE PROCESSING AFTER RETURN FROM TEST     */
*/*  OPERATION --                                                    */
*/*           - RE-INITIALIZE FLOW CONTROL FLAGS FOR TEST ACTIVITY   */
*/*           - SET LINK-ACTIVITY-CURRENT FLAG, LINKNOW              */
*/*           - CHECK IF TEST COMMAND WAS WITHOUT OPERANDS AND IF    */
*/*             ANY IF CURRENT TASK IS APF AUTHORIZED                */
*/*             - IF BOTH ARE TRUE, THEN --                          */
*/*               - SET UP TEST REJECTED MESSAGE                     */
*/*               - SYNCH TO PERFORM-PUTLINE ROUTINE, TPUTL          */
*/*             - IF TEST COMMAND INCLUDED OPERANDS OR APF-AUTHORI-  */
*/*               ZATION IS NOT CURRENT, THEN --                     */
*/*               - RESET APF-AUTHORIZATION FLAG, JSCBAUTH, TO       */
*/*                 PERMIT TESTING THE SPECIFIED PROGRAM IN NON-     */
*/*                 AUTHORIZED STATE                                 */
*/*               - PROVIDE TSEVENT WITH NEW PROGRAM NAME            */
*/*               - LOAD TEST                                        */
*/*               - SYNCH TO TEST                                    */
*/*               - DELETE TEST                                      */
*/*             - RESET LINK-ACTIVE FLAG, LINKNOW, AND TEST-PROCES-  */
*/*               SING-IN-CONTROL FLAG, TMPTEST                      */
*/*             - FREEMAIN COMMAND BUFFER FOR COMMAND JUST TESTED,   */
*/*               POINTED TO BY TPLTBUF                              */
*/*             - ISSUE STATUS-START TO START ANY STOPPED SUBTASK    */
*/*             - IF EXTERNALLY-FORCED-QUIT FLAG, ECTLOGF, IS ON,    */
*/*               SET TMP TERMINATION FLAG, DODONE                   */
*/*             - SET CHECK-PROCESSING-STATUS FLAG, DOLIST           */
*/*                                                                  */
*/* ENTRY -- LINKED BY REGISTER 14                                   */
*/*                                                                  */
*/* INPUT -- N/A                                                     */
*/*                                                                  */
*/* OUTPUT -- N/A                                                    */
*/*                                                                  */
*/* EXTERNAL REFERENCES -- N/A                                       */
*/*                                                                  */
*/* EXIT NORMAL -- RETURN BY LINK REGISTER 14                        */
*/*                                                                  */
*/*   RETURN CODE -- N/A                                             */
*/*                                                                  */
*/* EXIT ERROR -- N/A                                                */
*/*                                                                  */
*/*   CONDITIONS -- N/A                                              */
*/*                                                                  */
*/*   RETURN CODE -- N/A                                             */
*/*                                                                  */
*/* * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * * */
*TDOTEST:   /* EP TO LINK-TO-TEST-CP ROUTINE                         */
*    ENTRY;                                                     01S0760
TDOTEST  DS    0H                                               01S0761
*    IF CSOAVNP = YES               /* CHK FOR NO-OPERANDS           */
*        & JSCBAUTH = YES           /*   & FOR APF-AUTHORISED PGM    */
*      THEN DO;                     /* YES, TEST IS PROHIBITED       */
         L     @14,CSPLOA(,SRPARMLP)                            01S0761
         TM    CSOAVNP(@14),B'01000000'                         01S0761
         BNO   @RF00761                                         01S0761
         L     @14,CVTPTR                                       01S0761
         L     @14,CVTTCBP(,@14)                                01S0761
         L     @14,TCURTCB(,@14)                                01S0761
         L     @14,TCBJSCB(,@14)                                01S0761
         TM    JSCBAUTH(@14),B'00000001'                        01S0761
         BNO   @RF00761                                         01S0761
*        ACTSL = TM02SL;            /* INITIALIZE SEGMENT LIST       */
         L     @14,@CA00126                                     01S0763
         MVC   ACTSL(112,WRKAPTR1),TM02SL-TM02SL(@14)           01S0763
*        R2 = PTPBPTR;              /* PTR TO PUTLINE PARM BLOCK     */
         L     R2,PTPBPTR(,WRKAPTR1)                            01S0764
*        R15 = ADDR(TPUTL);         /* PTR TO ISSUE-PUTLINE RTN      */
         LA    R15,TPUTL                                        01S0765
*        GEN FLOWS(TPUTL) (SYNCH  (15));  /* TO DO PUTLINE           */
         SYNCH  (15)
*        DOGETC = YES;              /* SET TO CONTINUE               */
         OI    DOGETC(WRKAPTR1),B'00010000'                     01S0767
*        TMPACTRL = NO;             /* RESET ATTN-IN-CONTROL FLAG    */
         NI    TMPACTRL(WRKAPTR1),B'11101111'                   01S0768
*      END;                                                     01S0769
*      ELSE DO;                     /* TEST IS PERMISSABLE           */
         B     @RC00761                                         01S0770
@RF00761 DS    0H                                               01S0771
*        TMPSCTRL = NO;             /* RESET STAI IN CONTROL         */
         NI    TMPSCTRL(WRKAPTR1),B'11110111'                   01S0771
*        R1 = LNUCMD1;              /* 1ST 4 CHARS OF CMD NAME       */
         L     R1,LNUCMD1(,WRKAPTR1)                            01S0772
*        R15 = LNUCMD2;             /* 2ND 4 CHARS OF CMD NAME       */
         L     R15,LNUCMD2(,WRKAPTR1)                           01S0773
*        GEN(TSEVENT  PPMODE);      /* ISSUE TSEVENT                 */
         TSEVENT  PPMODE
*        LDOKSWT = '0'B;            /* RESET LOAD OK SWT.    @ZA14244*/
         NI    LDOKSWT,B'01111111'                              01S0775
*        GEN(LOAD  EP=TEST,ERRET=TSTNTFND); /* LOAD IN TEST  @ZA14244*/
         LOAD  EP=TEST,ERRET=TSTNTFND
*        LDOKSWT = '1'B;            /* SET LOAD OK SWITCH    @ZA14244*/
         OI    LDOKSWT,B'10000000'                              01S0777
*        R15 = R0;                  /* SET EP TO TEST                */
         LR    R15,R0                                           01S0778
*        R1 = ADDR(TPL);            /* SET PARM LIST PTR             */
         LR    R1,WRKAPTR1                                      01S0779
*        GEN(SYNCH  (15));          /* ACTIVATE TEST                 */
         SYNCH  (15)
*        GEN(DELETE  EP=TEST);      /* REMOVE TEST FUNCTION          */
         DELETE  EP=TEST
*        GEN(STATUS START);         /* START STOPPED SUBTASK         */
         STATUS START
*      END;                                                     01S0783
*TSTNTFND:                          /* TEST NOT FOUND BY LOAD@ZA14244*/
*  IF LDOKSWT = '0'B                /* IF LOAD FAILED        @ZA14244*/
*   THEN DO;                        /* DO MESSAGE            @ZA14244*/
@RC00761 DS    0H                                               01S0784
TSTNTFND TM    LDOKSWT,B'10000000'                              01S0784
         BNZ   @RF00784                                         01S0784
*    MSGSEG(1) = ADDR(MSG56622);    /* SET MSG POINTER       @ZA14244*/
         L     @14,@CA00131                                     01S0786
         ST    @14,MSGSEG(,WRKAPTR1)                            01S0786
*    DOPUTM = YES;                  /* SET TO DO MESSAGE     @ZA14244*/
         OI    DOPUTM(WRKAPTR1),B'10000000'                     01S0787
*   END;                            /*                       @ZA14244*/
*    TMPTEST = '0'B;                /* TEST NO LONGER IN CONTROL     */
@RF00784 NI    TMPTEST(WRKAPTR1),B'01111111'                    01S0789
*    LINKNOW = NO;                  /* RESET LINK IN CONTROL         */
         NI    LINKNOW(WRKAPTR1),B'11011111'                    01S0790
*    R0 = SPL1 + TPLTBUF->CBUFLNG;  /* LEN OF BUFF + SPL1            */
         L     @14,TPLTBUF(,WRKAPTR1)                           01S0791
         L     R0,@CF00093                                      01S0791
         AH    R0,CBUFLNG(,@14)                                 01S0791
*    R1 = TPLTBUF;                  /* ADDR OF BUFFER TO BE FREE     */
         LR    R1,@14                                           01S0792
*    GEN(FREEMAIN R,LV=(0),A=(1));  /* FREE SPACE                    */
         FREEMAIN R,LV=(0),A=(1)
*    TPLTBUF = ZERO;                /* SET FOR NO BUFFER TO FREE     */
         SLR   @14,@14                                          01S0794
         ST    @14,TPLTBUF(,WRKAPTR1)                           01S0794
*    IF ECTLOGF = YES               /* LOGOFF-LOGON REQUESTED?       */
*      THEN                         /* YES,                          */
         L     @14,ECTPTR(,WRKAPTR2)                            01S0795
         TM    ECTLOGF(@14),B'00010000'                         01S0795
         BNO   @RF00795                                         01S0795
*        DODONE = YES;              /* RETURN TO LOGON SCHEDULER     */
         OI    DODONE(WRKAPTR1),B'00001000'                     01S0796
*    DOLIST=YES;                    /* GO CHECK ECB LIST             */
@RF00795 OI    DOLIST(WRKAPTR1),B'00100000'                     01S0797
*    RESPECIFY (R14) RSTD;                                      01S0798
*    IF LDOKSWT = '0'B              /* IF TEST NOT FOUND     @ZA14244*/
*     THEN                          /* SET UP RETURN         @ZA14244*/
         TM    LDOKSWT,B'10000000'                              01S0799
         BNZ   @RF00799                                         01S0799
*      R14 = ADDR(TINVOKB);         /* FOR MESSAGE           @ZA14244*/
         LA    R14,TINVOKB                                      01S0800
*    ELSE                           /*                       @ZA14244*/
*      R14 = ADDR(TCHKATB);         /* SET FOR PSEUDO-RETURN         */
         B     @RC00799                                         01S0801
@RF00799 LA    R14,TCHKATB                                      01S0801
*    RETURN;                        /* RETURN TO CALLER              */
         B     @EL00006                                         01S0802
*    RESPECIFY (R14) UNRSTD;                                    01S0803
