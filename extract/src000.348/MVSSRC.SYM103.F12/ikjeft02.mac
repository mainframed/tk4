*/* * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * * */
*                                                               01S0106
*TMPNTRY2:  /* EP (LOGICAL)  TO TMP MAINLINE EXTERNAL PROCEDURE      */
*/*         DO:      START (EFFECTIVE) OF TMP MAINLINE PROCESSING    */
*                                                               01S0106
*           CALL TINIT2;            /* INITIALIZE FLOW CONTROL/DATA  */
*                                                               01S0106
TMPNTRY2 BAL   @14,TINIT2                                       01S0106
*TSCAN:     DO WHILE DOSCAN = YES;  /* CONTINUE TMP PROCESSING       */
TSCAN    B     @DE00107                                         01S0107
@DL00107 DS    0H                                               01S0108
*             CALL TIDNTC;          /* TO VALIDATE CMD NAME ENTERED  */
*                                                               01S0108
         BAL   @14,TIDNTC                                       01S0108
*TINVOKE:     DO WHILE DOINVOK=YES; /* CONTINUE TMP PROCESSING       */
TINVOKE  B     @DE00109                                         01S0109
@DL00109 DS    0H                                               01S0110
*               CALL TINVOK;        /* TO PERFORM COMMAND FUNCTION   */
*                                                               01S0110
         BAL   @14,TINVOK                                       01S0110
*TCHKCP:        DO WHILE DOCHKCP=YES;  /* CONTINUE TMP PROCESSING    */
TCHKCP   B     @DE00111                                         01S0111
@DL00111 DS    0H                                               01S0112
*                 DOCHKCP = NO;     /* RESET DO-ATTN-PROCESSING FLAG */
         NI    DOCHKCP(WRKAPTR1),B'11111101'                    01S0112
*                 IF DOATTNF1=NO4   /* CHK TO BYPASS ATTENTION       */
*                     & DOWAIT = NO   /*  PROCESSING                 */
*                   THEN            /* NO,                           */
         TM    DOATTNF1(WRKAPTR1),B'00111100'                   01S0113
         BNZ   @RF00113                                         01S0113
         TM    DOWAIT(WRKAPTR1),B'00000100'                     01S0113
         BNZ   @RF00113                                         01S0113
*                     CALL TATTNCK; /* TO PROCESS ATTENTION INTPT    */
         BAL   @14,TATTNCK                                      01S0114
*                 DOWAIT = YES;     /* TO FORCE FIRST PASS           */
*                                                               01S0115
@RF00113 OI    DOWAIT(WRKAPTR1),B'00000100'                     01S0115
*TWAIT:           DO WHILE DOWAIT=YES; /* CONTINUE TMP PROCESSING    */
TWAIT    B     @DE00116                                         01S0116
@DL00116 DS    0H                                               01S0117
*                   CALL  TWAITX;   /* TO PERFORM WAIT FOR CP END    */
         BAL   @14,TWAITX                                       01S0117
*                   DOLIST = NO;    /* RESET DO-TASK-STATUS-CHK FLAG */
         NI    DOLIST(WRKAPTR1),B'11011111'                     01S0118
*                   IF DOTSKF1=NO4  /* CHK TO BYPASS CHK-TASK-STATUS */
*                     THEN          /* NO,                           */
         TM    DOTSKF1(WRKAPTR1),B'00011110'                    01S0119
         BNZ   @RF00119                                         01S0119
*                       CALL  TTSKCHK;  /* TO CHECK TASK STATUS      */
         BAL   @14,TTSKCHK                                      01S0120
*TRETC:                            /* FOR PSEUDO-RETURN       ZA26167*/
*                  CALL  TCALLND;  /* CLOSE-OUT CALL FUNCTION ZA26167*/
@RF00119 DS    0H                                               01S0121
TRETC    BAL   @14,TCALLND                                      01S0121
*TRETD:                             /* FOR PSEUDO-RETURN             */
*                   DOGETC = YES;   /* TO FORCE FIRST PASS           */
*                                                               01S0122
TRETD    OI    DOGETC(WRKAPTR1),B'00010000'                     01S0122
*TGETCMD:           DO WHILE DOGETC=YES;  /* CONTINUE TMP PROCESSING */
TGETCMD  B     @DE00123                                         01S0123
@DL00123 DS    0H                                               01S0124
*                     DOGETC = NO;  /* RESET GET-NEXT-COMMAND FLAG   */
         NI    DOGETC(WRKAPTR1),B'11101111'                     01S0124
*                     IF DOGTCDF1=NO3   /* CHK TO BYPASS GET-COMMAND */
*                         & DOGTCDF2=NO5  /*   NOW                   */
*                       THEN        /* NO,                           */
         TM    DOGTCDF1(WRKAPTR1),B'00001110'                   01S0125
         BNZ   @RF00125                                         01S0125
         TM    DOGTCDF2(WRKAPTR1),B'00111110'                   01S0125
         BNZ   @RF00125                                         01S0125
*                         CALL  TGETCDX;  /* ACQUIRE NEXT COMMAND    */
         BAL   @14,TGETCDX                                      01S0126
*TRETE:                             /* FOR PSEUDO-RETURN             */
*                     CALL TDONE1;  /* CHECK FOR END NOW             */
@RF00125 DS    0H                                               01S0127
TRETE    BAL   @14,TDONE1                                       01S0127
*TDONE:               ;                                         01S0128
TDONE    DS    0H                                               01S0129
*                   END  TGETCMD;                               01S0129
@DE00123 TM    DOGETC(WRKAPTR1),B'00010000'                     01S0129
         BO    @DL00123                                         01S0129
*                 END  TWAIT;                                   01S0130
@DE00116 TM    DOWAIT(WRKAPTR1),B'00000100'                     01S0130
         BO    @DL00116                                         01S0130
*               END  TCHKCP;                                    01S0131
@DE00111 TM    DOCHKCP(WRKAPTR1),B'00000010'                    01S0131
         BO    @DL00111                                         01S0131
*             END  TINVOKE;                                     01S0132
@DE00109 TM    DOINVOK(WRKAPTR1),B'00000100'                    01S0132
         BO    @DL00109                                         01S0132
*           END  TSCAN;                                         01S0133
*                                                               01S0133
@DE00107 TM    DOSCAN(WRKAPTR1),B'00000010'                     01S0133
         BO    @DL00107                                         01S0133
*           CALL  TTERM;            /* CLOSE-OUT TMP PROCESSING      */
*                                                               01S0134
         BAL   @14,TTERM                                        01S0134
*/*         END:     (EFFECTIVE) OF TMP MAINLINE PROCESSING          */
*TMAINX2:   RETURN TO TEXIT  CODE(RETURNC);  /* END OF TMP MAINLINE  */
TMAINX2  L     @06,RETURNC                                      01S0135
         L     @13,4(,@13)                                      01S0135
         L     @00,@SIZDATD                                     01S0135
         LR    @01,@12                                          01S0135
         FREEMAIN R,LV=(0),A=(1)
         LR    @15,@06                                          01S0135
         LA    @14,TEXIT                                        01S0135
         LM    @00,@12,20(@13)                                  01S0135
         BR    @14                                              01S0135
*TEXIT:     GEN(SVC   3);           /* RETURN TO SYSTEM              */
TEXIT    SVC   3
