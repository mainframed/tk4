*/* SEGMENT NAME -- TTERM                                            */
*/*                                                                  */
*/* DESCRIPTIVE NAME -- N/A                                          */
*/*                                                                  */
*/* FUNCTION -- TO CLOSE OUT TMP ACTIVITY BEFORE RETURNING CONTROL   */
*/*       TO THE SYSTEM                                              */
*/*  OPERATION --                                                    */
*/*           - INITIALIZE TMP TASK RETURN CODE TO SUCCESSFUL CODE   */
*/*           - CHECK IF TMP TASK IS BACKGROUND TASK                 */
*/*             - IF YES, THEN --                                    */
*/*               - SET TMP TASK RETURN CODE FROM LATEST COMMAND     */
*/*                 TASK COMPLETION CODE (FROM ECT)                  */
*/*               - CALL STACK ROUTINE (TSTACKC) TO CLOSE THE BACK-  */
*/*                 GROUND SYSTSIN, SYSTSPRT DATA SETS               */
*/*               - CALL UPT/PSCB BUILD ROUTINE (IKJEFLR) TO FREE    */
*/*                 THE SPACE FOR THE UPT AND THE PSCB               */
*/*               - RESET JSCB POINTER TO THE PSCB (JSCBPSCB) TO     */
*/*                 ZERO                                             */
*/*           - RESTORE THE TMP TASK TCB PROTECT KEY (TCBPKF) TO     */
*/*             THE ORIGINAL ENTRY KEY (TSKPKF)                      */
*/*                                                                  */
*/* ENTRY -- LINKED BY REGISTER 14                                   */
*/*                                                                  */
*/* INPUT -- N/A                                                     */
*/*                                                                  */
*/* OUTPUT -- N/A                                                    */
*/*                                                                  */
*/* EXTERNAL REFERENCES -- IKJEFLR (UPT/PSCB BUILD ROUTINE)          */
*/*                                                                  */
*/* EXIT NORMAL -- RETURN BY LINK REGISTER 14                        */
*/*                                                                  */
*/*   RETURN CODE -- N/A                                             */
*/*                                                                  */
*/* EXIT ERROR -- N/A                                                */
*/*                                                                  */
*/*   CONDITIONS -- N/A                                              */
*/*                                                                  */
*/*   RETURN CODE -- N/A                                             */
*/*                                                                  */
*/* * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * * */
*TTERM:     /* EP TO CLOSE-TMP-ACTIVITY ROUTINE                      */
*    ENTRY;                                                     01S0365
*                                                               01S0365
TTERM    DS    0H                                               01S0366
*    RETURNC = TMPDONEC;            /* SET GOOD RETURN CODE          */
*                                                               01S0366
         SLR   @14,@14                                          01S0366
         ST    @14,RETURNC                                      01S0366
*    IF BKGMODE = YES               /* CHK IF BACKGROUND OPERATION   */
*      THEN DO;                     /* YES,                          */
*                                                               01S0367
         TM    BKGMODE(WRKAPTR2),B'10000000'                    01S0367
         BNO   @RF00367                                         01S0367
*        RETURNC = ECTRTCD;         /* SAVE USER COMPLETION CODE     */
*                                                               01S0369
         L     @14,ECTPTR(,WRKAPTR2)                            01S0369
         L     @14,ECTRTCD-1(,@14)                              01S0369
         LA    @14,0(,@14)                                      01S0369
         ST    @14,RETURNC                                      01S0369
*        /* STACK TO CLOSE SYSIN/SYSPRINT                            */
*        R1 = IOPLPTR;              /* SET STACK PARM LIST PTR       */
         L     R1,IOPLPTR(,WRKAPTR1)                            01S0370
*        IOPLIOPB = STPBPTR;        /* SET STACK PARM BLOCK PTR      */
         L     @14,STPBPTR(,WRKAPTR1)                           01S0371
         ST    @14,IOPLIOPB(,SRPARMLP)                          01S0371
*        R15 = ADDR(TSTACKC);       /* GO TO INVOKE-STACK-CLOSE      */
         LA    R15,TSTACKC                                      01S0372
*        GEN FLOWS(TSTACKC) (SYNCH  (15));  /* TO STACKC             */
*                                                               01S0373
         SYNCH  (15)
*        IF JSCBPSCB ^= ZERO        /* CHK IF PSCB EXISTS            */
*          THEN DO;                 /* YES                           */
         L     @14,CVTPTR                                       01S0374
         L     @14,CVTTCBP(,@14)                                01S0374
         L     @14,TCURTCB(,@14)                                01S0374
         L     @14,TCBJSCB(,@14)                                01S0374
         L     @14,JSCBPSCB(,@14)                               01S0374
         LTR   @14,@14                                          01S0374
         BZ    @RF00374                                         01S0374
*            RESPECIFY (R2) RSTD;                               01S0376
*            DO; /*MODESET  KEYADDR(SPROK0) WORKREG(R2)*/       01S0377
*              R2 = SPROK0; /*GET KEY FORM SAVE LOCATION*/      01S0378
         L     R2,SPROK0                                        01S0378
*              SPKA (R2); /*SET PSW KEY*/                       01S0379
         SPKA  0(R2)                                            01S0379
*            END; /*MODESET  KEYADDR(SPROK0) WORKREG(R2)*/      01S0380
*            /* FREEMAIN ANY UPT/RLGB SPACE ACQUIRED                 */
*            DO; /*FREEMAIN (EC) LV(LENGTH(UPT)) A(PSCBUPT) SP(SPNM252)
*/           RESPECIFY (GPR01P) RESTRICTED;                     01S0381
*            I005 = I005&&I005; /* INIT LIST */                 01S0383
         XC    I005(10),I005                                    01S0383
*            I00505='20'X; /*SET TYPE*/                         01S0384
         MVI   I00505,X'20'                                     01S0384
*            I00502=LENGTH(UPT); /*LENGTH*/                     01S0385
         MVC   I00502(3),@CF00123+1                             01S0385
*            I00504=ADDR(PSCBUPT);  /*ADDR OF ADDR LIST*/       01S0386
         LA    @14,PSCBUPT(,@14)                                01S0386
         STCM  @14,7,I00504                                     01S0386
*            I00506=SPNM252; /*SUBPOOL VALUE*/                  01S0387
         MVI   I00506,X'FC'                                     01S0387
*            GPR01P = ADDR(I005); /* REG1 POINTS TO LIST */     01S0388
         LA    GPR01P,I005                                      01S0388
*            SVC(5); /*STANDARD-FORM OF GETMAIN*/               01S0389
         SVC   5                                                01S0389
*            RESPECIFY (GPR01P) UNRESTRICTED;                   01S0390
*            END; /*FREEMAIN (EC) LV(LENGTH(UPT)) A(PSCBUPT) SP(SPNM252
**/                                                             01S0391
*            DO; /*FREEMAIN (EC) LV(LENGTH(RLGB)) A(PSCBRLGB) SP(SPRLGB
**/          RESPECIFY (GPR01P) RESTRICTED;                     01S0392
*            I005 = I005&&I005; /* INIT LIST */                 01S0394
         XC    I005(10),I005                                    01S0394
*            I00505='20'X; /*SET TYPE*/                         01S0395
         MVI   I00505,X'20'                                     01S0395
*            I00502=LENGTH(RLGB); /*LENGTH*/                    01S0396
         MVC   I00502(3),@CF03086+1                             01S0396
*            I00504=ADDR(PSCBRLGB);  /*ADDR OF ADDR LIST*/      01S0397
         L     @14,CVTPTR                                       01S0397
         L     @14,CVTTCBP(,@14)                                01S0397
         L     @14,TCURTCB(,@14)                                01S0397
         L     @14,TCBJSCB(,@14)                                01S0397
         L     @14,JSCBPSCB(,@14)                               01S0397
         LA    @14,PSCBRLGB(,@14)                               01S0397
         STCM  @14,7,I00504                                     01S0397
*            I00506=SPRLGB; /*SUBPOOL VALUE*/                   01S0398
         MVI   I00506,X'E6'                                     01S0398
*            GPR01P = ADDR(I005); /* REG1 POINTS TO LIST */     01S0399
         LA    GPR01P,I005                                      01S0399
*            SVC(5); /*STANDARD-FORM OF GETMAIN*/               01S0400
         SVC   5                                                01S0400
*            RESPECIFY (GPR01P) UNRESTRICTED;                   01S0401
*            END; /*FREEMAIN (EC) LV(LENGTH(RLGB)) A(PSCBRLGB) SP(SPRLG
*)*/                                                            01S0402
*            /* FREEMAIN ANY PSCB/LWA SPACE ACQUIRED                 */
*            DO; /*FREEMAIN (EC) LV(LENGTH(PSCB)) A(JSCBPSCB) SP(SPNM25
*)*/         RESPECIFY (GPR01P) RESTRICTED;                     01S0403
*            I005 = I005&&I005; /* INIT LIST */                 01S0405
         XC    I005(10),I005                                    01S0405
