*TIMPLEX:   /* EP TO SET-UP-IMPLICIT-EXEC ROUTINE                    */
*                                                               01S0517
*    ENTRY;                                                     01S0517
*                                                               01S0517
TIMPLEX  DS    0H                                               01S0518
*    IF DOIMPLX = YES               /* CHK TO DO IMPLI-EXEC NOW      */
*      THEN DO;                     /* YES,                          */
*                                                               01S0518
         TM    DOIMPLX(WRKAPTR1),B'01000000'                    01S0518
         BNO   @RF00518                                         01S0518
*        DOIMPLX = NO;              /* RESET DO IMPLI-EXEC FLAG      */
         NI    DOIMPLX(WRKAPTR1),B'10111111'                    01S0520
*        IF CSOAEXEC = YES          /* CHK FOR EXPL/IMPLEXEC         */
*            & FRSTEX = NO          /*    & FIRST EXEC NOW           */
*          THEN DO;                 /* YES,                          */
         L     @14,CSPLOA(,SRPARMLP)                            01S0521
         TM    CSOAEXEC(@14),B'00000100'                        01S0521
         BNO   @RF00521                                         01S0521
         TM    FRSTEX(WRKAPTR1),B'00010000'                     01S0521
         BNZ   @RF00521                                         01S0521
*            FRSTEX=YES;            /* INDICATE FIRST IMPLICIT EXEC  */
         OI    FRSTEX(WRKAPTR1),B'00010000'                     01S0523
*            R0=SPL1+CBUFLNG;       /* LEN OF BUFFER + SPL1          */
         L     @14,TPLCBUF(,WRKAPTR1)                           01S0524
         L     R0,@CF00093                                      01S0524
         AH    R0,CBUFLNG(,@14)                                 01S0524
*            R1 = TPLCBUF;          /* ADDR OF BUFFER TO BE FREED    */
         LR    R1,@14                                           01S0525
*            GEN(FREEMAIN R,LV=(0),A=(1));                      01S0526
         FREEMAIN R,LV=(0),A=(1)
*            TPLCBUF = CSPLCBUF;                                01S0527
         L     @14,CSPLCBUF(,SRPARMLP)                          01S0527
         ST    @14,TPLCBUF(,WRKAPTR1)                           01S0527
*          END;                                                 01S0528
*          ELSE                     /* NO, ALREADY DONE              */
*            FRSTEX=NO;             /* INDICATE NOT FIRST EXEC       */
*                                                               01S0529
         B     @RC00521                                         01S0529
@RF00521 NI    FRSTEX(WRKAPTR1),B'11101111'                     01S0529
*        IF CSOACNM->CMDNAME(1:LENGTH(EXECCMD))=EXECCMD  /*          */
*            & CSOALNM=LENGTH(EXECCMD)  /* CHK FOR EXEC CMD          */
*            & FRSTEX=NO            /*   & NOT FIRST IMPLICIT EXEC   */
*          THEN DO;                 /* NO EXEC CMD PROCESSOR FOUND   */
@RC00521 L     @14,CSPLOA(,SRPARMLP)                            01S0530
         L     @06,CSOACNM(,@14)                                01S0530
         CLC   CMDNAME(4,@06),EXECCMD                           01S0530
         BNE   @RF00530                                         01S0530
         CLC   CSOALNM(2,@14),@CH00045                          01S0530
         BNE   @RF00530                                         01S0530
         TM    FRSTEX(WRKAPTR1),B'00010000'                     01S0530
         BNZ   @RF00530                                         01S0530
*            MSGSEG(1)=ADDR(MSG56622);  /* SET MSG PTR               */
         L     @14,@CA00131                                     01S0532
         ST    @14,MSGSEG(,WRKAPTR1)                            01S0532
*            DOPUTM = YES;          /* SET TO DO MESSAGE             */
         OI    DOPUTM(WRKAPTR1),B'10000000'                     01S0533
*          END;                                                 01S0534
*          ELSE DO;                 /* YES, EXEC COMMAND FOUND       */
         B     @RC00530                                         01S0535
@RF00530 DS    0H                                               01S0536
*            CSPLCBUF->CBUFOFF=ZERO;  /* RESET BUFFER OFFSET         */
         L     @14,CSPLCBUF(,SRPARMLP)                          01S0536
         SLR   @06,@06                                          01S0536
         STH   @06,CBUFOFF(,@14)                                01S0536
*            CSOACNM=ADDR(EXECCMD);   /* PTR TO CMD NAME             */
         L     @14,CSPLOA(,SRPARMLP)                            01S0537
         LA    @06,EXECCMD                                      01S0537
         ST    @06,CSOACNM(,@14)                                01S0537
*            CSOALNM=LENGTH(EXECCMD); /* SET NAME LENGTH             */
         MVC   CSOALNM(2,@14),@CH00045                          01S0538
*          END;                                                 01S0539
*      END;                                                     01S0540
*                                                               01S0540
@RC00530 DS    0H                                               01S0541
*    RESPECIFY (R14) RSTD;                                      01S0541
@RF00518 DS    0H                                               01S0542
*    R14 = ADDR(TINVOKA);           /* SET FOR PSEUDO-RETURN         */
         LA    R14,TINVOKA                                      01S0542
*    RETURN;                        /* RETURN TO CALLER              */
         B     @EL00005                                         01S0543
*    RESPECIFY (R14) UNRSTD;                                    01S0544
