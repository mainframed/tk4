*TSRCHAPF:  /* EP TO DETERMINE-ELIGIBLITY-FOR-APF-ATTACH ROUTINE     */
*    ENTRY;                                                     01S0992
TSRCHAPF DS    0H                                               01S0993
*    RESPECIFY (R4,R6,R14) RSTD;                                01S0993
*    R6 = R14;                      /* RESERVE ENTRY REG 14          */
         LR    R6,R14                                           01S0994
*    RESPECIFY (R14) UNRSTD;                                    01S0995
*                                                               01S0995
*    DO; /*MODESET  KEYADDR(SPROK1) WORKREG(R2)*/               01S0996
*      R2 = SPROK1; /*GET KEY FORM SAVE LOCATION*/              01S0997
         L     R2,SPROK1                                        01S0997
*      SPKA (R2); /*SET PSW KEY*/                               01S0998
         SPKA  0(R2)                                            01S0998
*    END; /*MODESET  KEYADDR(SPROK1) WORKREG(R2)*/              01S0999
*    DRSAPF = NO;                   /* PRESUME NO APF ELIGIBILITY    */
         NI    DRSAPF(WRKAPTR2),B'10111111'                     01S1000
*    DO; /*MODESET  KEYADDR(TEPKEY) WORKREG(R2)*/               01S1001
*      R2 = TEPKEY; /*GET KEY FORM SAVE LOCATION*/              01S1002
         L     R2,TEPKEY(,WRKAPTR2)                             01S1002
*      SPKA (R2); /*SET PSW KEY*/                               01S1003
         SPKA  0(R2)                                            01S1003
*    END; /*MODESET  KEYADDR(TEPKEY) WORKREG(R2)*/              01S1004
*    R4 = ADDR(APFCTABL);           /* LOCATE LIST OF APF NAMES      */
*                                                               01S1005
         L     R4,@CV00135                                      01S1005
*    /* SEARCH LIST                                                  */
*    DO WHILE (R4->TE2NAME ^= BLANKS  /* LOOP WHILE NEITHER BLANK    */
*        & DRSAPF = NO);            /* LIST NAME NOR MATCHING NAME   */
         B     @DE01006                                         01S1006
@DL01006 DS    0H                                               01S1007
*      IF R4->TE2NAME = TSRCHNAM    /* CHK IF NAMES MATCH            */
*        THEN DO;                   /* YES,                          */
         CLC   TE2NAME(8,R4),TSRCHNAM                           01S1007
         BNE   @RF01007                                         01S1007
*          DO; /*MODESET  KEYADDR(SPROK1) WORKREG(R2)*/         01S1009
*            R2 = SPROK1; /*GET KEY FORM SAVE LOCATION*/        01S1010
         L     R2,SPROK1                                        01S1010
*            SPKA (R2); /*SET PSW KEY*/                         01S1011
         SPKA  0(R2)                                            01S1011
*          END; /*MODESET  KEYADDR(SPROK1) WORKREG(R2)*/        01S1012
*          DRSAPF = YES;            /*   AUTHORIZE NEXT ATTACH       */
         OI    DRSAPF(WRKAPTR2),B'01000000'                     01S1013
*          DO; /*MODESET  KEYADDR(TEPKEY) WORKREG(R2)*/         01S1014
*            R2 = TEPKEY; /*GET KEY FORM SAVE LOCATION*/        01S1015
         L     R2,TEPKEY(,WRKAPTR2)                             01S1015
*            SPKA (R2); /*SET PSW KEY*/                         01S1016
         SPKA  0(R2)                                            01S1016
*          END; /*MODESET  KEYADDR(TEPKEY) WORKREG(R2)*/        01S1017
*        END;                                                   01S1018
*      R4 = R4+D8;                  /* STEP PTR TO NEXT ENTRY        */
@RF01007 AL    R4,@CF00041                                      01S1019
*    END;                                                       01S1020
*                                                               01S1020
@DE01006 CLC   TE2NAME(8,R4),@CC00042                           01S1020
         BE    @DC01006                                         01S1020
         TM    DRSAPF(WRKAPTR2),B'01000000'                     01S1020
         BZ    @DL01006                                         01S1020
@DC01006 DS    0H                                               01S1021
*    RESPECIFY (R14) RSTD;                                      01S1021
*    R14 = R6;                      /* RESTORE ENTRY REG 14          */
         LR    R14,R6                                           01S1022
*    END  TDETCP;                   /* RETURN TO CALLER              */
         B     @EL00010                                         01S1023
*    RESPECIFY (R4,R14) UNRSTD;                                 01S1024
