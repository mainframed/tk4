*/* * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * * */
*TINVOK:    /* EP TO ACTIVATE-COMMAND-CP ROUTINE                     */
*    ENTRY;                                                     01S0203
*                                                               01S0203
TINVOK   DS    0H                                               01S0204
*    DOINVOK = NO;                  /* RESET DO-INVOKE-NOW FLAG      */
         NI    DOINVOK(WRKAPTR1),B'11111011'                    01S0204
*    DOCHKCP = YES;                 /* TO FORCE FIRST PASS           */
         OI    DOCHKCP(WRKAPTR1),B'00000010'                    01S0205
*    IF DOMPLXF1 = NO2              /* CHK FOR BYPASS-IMPLICIT-EXEC- */
*        & DOMPLXF2 = NO5           /*    PROCESSING FLAGS           */
*      THEN                         /* NONE,                         */
         TM    DOMPLXF1(WRKAPTR1),B'00011000'                   01S0206
         BNZ   @RF00206                                         01S0206
         TM    DOMPLXF2(WRKAPTR1),B'11111000'                   01S0206
         BNZ   @RF00206                                         01S0206
*        CALL  TIMPLEX;             /*   CHK/PERFORM IMPLI-EXEC      */
*                                                               01S0207
         BAL   @14,TIMPLEX                                      01S0207
*TINVOKA:                           /* FOR PSEUDO-RETURN             */
*    IF DOFRECB = YES               /* CHK TO FREE CMD-SCAN BUFFER   */
*      THEN                         /* YES,                          */
@RF00206 DS    0H                                               01S0208
TINVOKA  TM    DOFRECB(WRKAPTR1),B'01000000'                    01S0208
         BNO   @RF00208                                         01S0208
*        CALL  TFRECBF;             /*   PERFORM FREEMAIN            */
*                                                               01S0209
         BAL   @14,TFRECBF                                      01S0209
*TINVOKB:                           /* FOR PSEUDO-RETURN             */
*    IF DOPUTM = YES                /* CHK IF MESSAGE IS PENDING     */
*      THEN                         /* YES,                          */
@RF00208 DS    0H                                               01S0210
TINVOKB  TM    DOPUTM(WRKAPTR1),B'10000000'                     01S0210
         BNO   @RF00210                                         01S0210
*        CALL  TNOCMDF;             /*   ISSUE MESSAGE               */
*                                                               01S0211
         BAL   @14,TNOCMDF                                      01S0211
*TINVOKC:                           /* FOR PSEUDO-RETURN             */
*    IF DOSTCDF1 = NO2              /* CHK FOR BYPASS-NEXT-COMMAND-  */
*        & DOSTCDF2 = NO3           /*   SET-UP FLAGS                */
*      THEN                         /* NONE,                         */
@RF00210 DS    0H                                               01S0212
TINVOKC  TM    DOSTCDF1(WRKAPTR1),B'00011000'                   01S0212
         BNZ   @RF00212                                         01S0212
         TM    DOSTCDF2(WRKAPTR1),B'00111000'                   01S0212
         BNZ   @RF00212                                         01S0212
*        CALL TSETCD;               /*   PERFORM SET-UP              */
*                                                               01S0213
         BAL   @14,TSETCD                                       01S0213
*TINVOKD:                           /* FOR PSEUDO-RETURN             */
*    DOCHKAT = NO;                  /* RESET DO-ATTACH-CHK-NOW FLAG  */
@RF00212 DS    0H                                               01S0214
TINVOKD  NI    DOCHKAT(WRKAPTR1),B'11110111'                    01S0214
*    IF DOCHKAF1 = NO2              /* CHK FOR BYPASS-CHK-TASK-      */
*        & DOCHKAF2 = NO2           /*   STATUS FLAGS                */
*      THEN                         /* NONE,                         */
         TM    DOCHKAF1(WRKAPTR1),B'00011000'                   01S0215
         BNZ   @RF00215                                         01S0215
         TM    DOCHKAF2(WRKAPTR1),B'00110000'                   01S0215
         BNZ   @RF00215                                         01S0215
*        CALL  TCHKAT;              /*   PERFORM TASK STATUS CHECK   */
*                                                               01S0216
         BAL   @14,TCHKAT                                       01S0216
*TINVOKF:                           /* FOR PSEUDO-RETURN             */
*    IF DOPSTRT = YES               /* CHK TO START STOPPED SUBTASKS */
*      THEN                         /* YES,                          */
@RF00215 DS    0H                                               01S0217
TINVOKF  TM    DOPSTRT(WRKAPTR1),B'00100000'                    01S0217
         BNO   @RF00217                                         01S0217
*        CALL TSTRTAB;              /*   PERFORM STATUS START        */
*                                                               01S0218
         BAL   @14,TSTRTAB                                      01S0218
*    DOACTV = NO;                   /* RESET DO-ACTIVATE FLAG        */
@RF00217 NI    DOACTV(WRKAPTR1),B'11101111'                     01S0219
*    IF DOACTF1 = NO3               /* CHK FOR BYPASS-COMMAND FLAGS  */
*      THEN                         /* NONE,                         */
         TM    DOACTF1(WRKAPTR1),B'00111000'                    01S0220
         BNZ   @RF00220                                         01S0220
*        CALL  TACTV;               /*   INITIATE CP ACTION          */
*                                                               01S0221
         BAL   @14,TACTV                                        01S0221
*    RESPECIFY (R14) RSTD;                                      01S0222
@RF00220 DS    0H                                               01S0223
*TINVOKH:                           /* FOR PSEUDO-RETURN             */
*    R14 = ADDR(TCHKCP);            /* SET FOR PSEUDO-RETURN         */
TINVOKH  LA    R14,TCHKCP                                       01S0223
*    RETURN;                        /* RETURN TO CALLER              */
         B     @EL00003                                         01S0224
*    RESPECIFY (R14) UNRSTD;                                    01S0225
