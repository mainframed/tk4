         CLC   CMDNAME(4,@04),@CC02951                          01S0586
         BNE   @RF00586                                         01S0586
         C     @06,@CF00045                                     01S0586
         BNE   @RF00586                                         01S0586
         TM    CSOAVNP(@14),B'01000000'                         01S0586
         BNO   @RF00586                                         01S0586
*            DOSETTB = YES;         /* DO NOT FLUSH STACK            */
*            DOSETBF = NO;          /* DO NOT FREE COMMAND BUFFER    */
         OI    DOSETTB(WRKAPTR1),B'00001000'                    01S0589
         NI    DOSETBF(WRKAPTR1),B'11101111'                    01S0589
*          END;                                                 01S0590
*          ELSE DO;                 /* NOT TEST, FLUSH STACK         */
         B     @RC00586                                         01S0591
@RF00586 DS    0H                                               01S0592
*            IF TMPACTRL = YES      /* CHK IF ATTN IN CONTROL        */
*              THEN                 /* YES,                          */
         TM    TMPACTRL(WRKAPTR1),B'00010000'                   01S0592
         BNO   @RF00592                                         01S0592
*                SPBFLUSH = YES;    /* SET FLUSH ENABLED             */
         L     @14,STPBPTR(,WRKAPTR1)                           01S0593
         OI    SPBFLUSH(@14),B'00001000'                        01S0593
*            R1 = ADDR(IOPL);       /* SET STACK PARM LIST PTR       */
@RF00592 LR    R1,SRPARMLP                                      01S0594
*            IOPLIOPB = STPBPTR;    /* SET STACK PARM BLOCK PTR      */
         L     @14,STPBPTR(,WRKAPTR1)                           01S0595
         ST    @14,IOPLIOPB(,SRPARMLP)                          01S0595
*            R15 = ADDR(TSTACKA);   /* PTR TO ISSUE-STACK RTN        */
         LA    R15,TSTACKA                                      01S0596
*            GEN FLOWS(TSTACKA) (SYNCH  (15));  /* TO TSTACKA        */
         SYNCH  (15)
*            SPBFLUSH = NO;         /* SET FLUSH INHIBITED           */
         L     @14,STPBPTR(,WRKAPTR1)                           01S0598
