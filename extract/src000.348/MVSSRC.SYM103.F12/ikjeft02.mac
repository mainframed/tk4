*/*                                                                  */
*/* SEGMENT NAME -- TTSKCHK                                          */
*/*                                                                  */
*/* FUNCTION -- TO CHECK THE STATUS OF THE ATTACHED-CP TASK AND SET  */
*/*       UP CONSEQUENT PROCESSING                                   */
*/*  OPERATION --                                                    */
*/*           - IF ATTACHED TASK IS POSTED COMPLETE, CALL TDETCP,    */
*/*             THE ROUTINE TO DETACH THAT TASK                      */
*/*           - RESET THE SUBCOMMAND-BEING-ATTACHED FLAG             */
*/*           - IF THE ATTENTION ECB HAS BEEN POSTED OR THERE IS A   */
*/*             COMMAND WAITING TO BE INITIATED (TMPCMDWT FLAG),     */
*/*             SET THE DO-ATTENTION-PROCESSING FLAG, DOCHKCP        */
*/*           - IF NEITHER ATTENTION NOR COMMAND-WAITING, CHECK IF   */
*/*             THE ABEND ECB HAS BEEN POSTED                        */
*/*             - IF ABEND ECB IS POSTED, CALL THE ABEND PROCESSING  */
*/*               ROUTINE, TSTAIPST                                  */
*/*             - IF NOT ABEND, CHECK IF CURRENT SUBTASK IS STILL    */
*/*               ACTIVE                                             */
*/*               - IF STILL ACTIVE, SET CONTINUE-WAIT FLAG, DOWAIT  */
*/*               - IF NOT STILL ACTIVE, CHECK IF SYSTEM-FORCED-     */
*/*                 TERMINATION IS INDICATED (STOP-MODIFY ECB IS     */
*/*                 POSTED)                                          */
*/*                 - IF POSTED, CLEAR ALL TMP FLOW FLAGS AND SET    */
*/*                   TERMINATION FLAG, DODONE                       */
*/*                 - IF TERMINATION NOT INDICATED --                */
*/*                   - CHECK IF ABEND PROCESSING IS CURRENTLY       */
*/*                     ACTIVE (TMPSCTRL FLAG)                       */
*/*                     - IF YES, THEN --                            */
*/*                       - RESET ABEND-PROCESSING FLAG, TMPSCTRL    */
*/*                       - SET DO-CHECK-ATTENTION-NOW FLAG, DOCHKAT */
*/*                         AND PERFORM-NEW-COMMAND-PROCESSING FLAG, */
*/*                         DOINVOK, FOR NEXT TMP CYCLE              */
*/*                       - IF TEST IS CURRENTLY ACTIVE (TMPTEST     */
*/*                         FLAG ON), SET PERFORM-TEST-PROCESSING-   */
*/*                         NOW FLAG, DOTEST, FOR NEXT CYCLE         */
*/*                                                                  */
*/* ENTRY -- LINKED BY REGISTER 14                                   */
*/*                                                                  */
*/* INPUT -- N/A                                                     */
*/*                                                                  */
*/* OUTPUT -- N/A                                                    */
*/*                                                                  */
*/* EXTERNAL REFERENCES -- N/A                                       */
*/*                                                                  */
*/* EXIT NORMAL -- RETURN BY LINK REGISTER 14                        */
*/*   RETURN CODE -- N/A                                             */
*/*                                                                  */
*/* EXIT ERROR -- N/A                                                */
*/*   CONDITIONS -- N/A                                              */
*/*   RETURN CODE -- N/A                                             */
*/*                                                                  */
*/* * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * * */
*TTSKCHK:   /* EP TO CHECK-ATTACHED-CP-TASK-STATUS ROUTINE           */
*    ENTRY;                                                     01S0265
*                                                               01S0265
TTSKCHK  DS    0H                                               01S0266
*    IF TPLCECB->ECBPOST=YES        /* CHK FOR ATTACHED CP COMPLETE  */
*      THEN                         /* YES,                          */
         L     @14,TPLCECB(,WRKAPTR1)                           01S0266
         TM    ECBPOST(@14),B'01000000'                         01S0266
         BNO   @RF00266                                         01S0266
*        CALL TDETCP;               /*   DETACH CURRENT CP           */
*                                                               01S0267
         BAL   @14,TDETCP                                       01S0267
*    ATCHNOW = NO;                  /* CLEAR SUB-CP-ATTACHED FLAG    */
*                                                               01S0268
@RF00266 NI    ATCHNOW(WRKAPTR1),B'01111111'                    01S0268
*    IF TPLAECB->ECBPOST=YES        /* CHK FOR ATTENTION ACTION      */
*          TMPCMDWT^=ZERO          /*     OR COMMAND WAITING        */
*      THEN                         /* YES,                          */
         L     @14,TPLAECB-1(,WRKAPTR1)                         01S0269
         TM    ECBPOST(@14),B'01000000'                         01S0269
         BO    @RT00269                                         01S0269
         L     @14,TMPCMDWT(,WRKAPTR1)                          01S0269
         LTR   @14,@14                                          01S0269
         BZ    @RF00269                                         01S0269
@RT00269 DS    0H                                               01S0270
*        DOCHKCP = YES;             /*   PROCESS ATTENTION           */
         OI    DOCHKCP(WRKAPTR1),B'00000010'                    01S0270
*      ELSE                         /* NO, IS NOT ATTENTION          */
*        IF TPLIECB->ECBPOST=YES    /* FOR STAI-ABEND ACTION         */
*          THEN DO;                 /* YES,                          */
         B     @RC00269                                         01S0271
@RF00269 L     @14,TPLIECB(,WRKAPTR1)                           01S0271
         TM    ECBPOST(@14),B'01000000'                         01S0271
         BNO   @RF00271                                         01S0271
*            CALL  TSTAIPST;        /*   PROCESS ABEND               */
         BAL   @14,TSTAIPST                                     01S0273
*TTSKCKA:  END;                     /* FOR PSEUDO-RETURN             */
*          ELSE                     /* NO, IS NOT ABEND              */
*            IF TPLCTCB^=ZERO       /* CHK FOR CP STILL ATTACHED     */
*              THEN                 /* YES,                          */
         B     @RC00271                                         01S0275
@RF00271 L     @14,TPLCTCB(,WRKAPTR1)                           01S0275
         LTR   @14,@14                                          01S0275
         BZ    @RF00275                                         01S0275
*                DOWAIT=YES;        /* CP ATTACHED                   */
         OI    DOWAIT(WRKAPTR1),B'00000100'                     01S0276
*              ELSE                 /* NO, CP IS NOT ACTIVE          */
*                IF TPLMECB->ECBPOST=YES  /* STOP/MODIFY             */
*                  THEN DO;         /* YES,                          */
         B     @RC00275                                         01S0277
@RF00275 L     @14,TPLMECB(,WRKAPTR1)                           01S0277
         TM    ECBPOST(@14),B'01000000'                         01S0277
         BNO   @RF00277                                         01S0277
*                    FLOFLGS=ZERO;  /* CLEAR ALL CURRENT             */
         SLR   @14,@14                                          01S0279
         ST    @14,FLOFLGS(,WRKAPTR1)                           01S0279
*                    FLOFLGS2=ZERO; /* ACTIVITY FLAGS                */
         MVI   FLOFLGS2(WRKAPTR1),X'00'                         01S0280
*                    DODONE = YES;  /* TO RETURN TO LOGOFF           */
         OI    DODONE(WRKAPTR1),B'00001000'                     01S0281
*                  END;                                         01S0282
*                  ELSE             /* NO, IS NOT STOP               */
*                    IF TMPSCTRL=YES   /* CHK STAI IN CONTROL        */
*                      THEN DO;        /* YES,                       */
         B     @RC00277                                         01S0283
@RF00277 TM    TMPSCTRL(WRKAPTR1),B'00001000'                   01S0283
         BNO   @RF00283                                         01S0283
*                        TMPSCTRL=NO;  /* RESET STAI FLAG            */
         NI    TMPSCTRL(WRKAPTR1),B'11110111'                   01S0285
*                        DOCHKAT=YES;  /* RETURN TO CHECK            */
         OI    DOCHKAT(WRKAPTR1),B'00001000'                    01S0286
*                        DOINVOK=YES;  /* FOR NEXT COMMAND           */
         OI    DOINVOK(WRKAPTR1),B'00000100'                    01S0287
*                        IF TMPTEST=YES  /* CHK TEST NOW             */
*                          THEN        /* YES,                       */
         TM    TMPTEST(WRKAPTR1),B'10000000'                    01S0288
         BNO   @RF00288                                         01S0288
*                            DOTEST=YES; /* ACTIVATE TEST            */
         OI    DOTEST(WRKAPTR1),B'00100000'                     01S0289
*                      END;                                     01S0290
*                                                               01S0290
@RF00288 DS    0H                                               01S0291
*    RESPECIFY (R14) RSTD;                                      01S0291
@RF00283 DS    0H                                               01S0291
@RC00277 DS    0H                                               01S0291
@RC00275 DS    0H                                               01S0291
@RC00271 DS    0H                                               01S0291
@RC00269 DS    0H                                               01S0292
*    R14 = ADDR(TRETC);             /* SET FOR PSEUDO-RETURN  ZA26167*/
         LA    R14,TRETC                                        01S0292
*    RETURN;                        /* RETURN TO CALLER              */
         B     @EL00003                                         01S0293
*    RESPECIFY (R14) UNRSTD;                                    01S0294
