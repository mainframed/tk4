*      THEN                         /* YES,                          */
         TM    DOSETBF(WRKAPTR1),B'00010000'                    01S0602
         BNO   @RF00602                                         01S0602
*        CALL  TSETCD1;             /*   GO TO CHECK ABEND 806       */
         BAL   @14,TSETCD1                                      01S0603
*TSETCDA:                           /* FOR PSEUDO-RETURN             */
*    IF DOSETTB = YES               /* CHK DO-TEST-NOW FLAG          */
*      THEN                         /* YES,                          */
@RF00602 DS    0H                                               01S0604
TSETCDA  TM    DOSETTB(WRKAPTR1),B'00001000'                    01S0604
         BNO   @RF00604                                         01S0604
*        CALL  TSETCD2;             /*   GO TO SET UP TEST           */
         BAL   @14,TSETCD2                                      01S0605
*    RESPECIFY (R14) RSTD;                                      01S0606
@RF00604 DS    0H                                               01S0607
*    R14 = ADDR(TINVOKD);           /* SET FOR PSEUDO-RETURN         */
         LA    R14,TINVOKD                                      01S0607
*    RETURN;                        /* RETURN TO CALLER              */
         B     @EL00005                                         01S0608
*    RESPECIFY (R14) UNRSTD;                                    01S0609
