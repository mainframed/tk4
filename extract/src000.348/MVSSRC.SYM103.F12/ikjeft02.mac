*TACTV1:    /* EP TO INVOKE-CALL-FUNCTION  OUTINE                    */
*    ENTRY;                                                     01S0804
*                                                               01S0804
TACTV1   DS    0H                                               01S0805
*    CALLNOW = YES;                 /* SET CALL-ACTIVE FLAG          */
         OI    CALLNOW(WRKAPTR1),B'00001000'                    01S0805
*    CALL  IKJEFT08;                /* TO PERFORM CALL FUNCTION      */
*                                                               01S0806
         L     @15,@CV00138                                     01S0806
         BALR  @14,@15                                          01S0806
*    RESPECIFY (R14) RSTD;                                      01S0807
*    R14 = ADDR(TACTVA);            /* SET FOR PSEUDO-RETURN         */
         LA    R14,TACTVA                                       01S0808
*    RETURN;                        /* RETURN TO CALLER              */
         B     @EL00006                                         01S0809
*    RESPECIFY (R14) UNRSTD;                                    01S0810
*/* * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * * */
*/*                                                                  */
*/* SEGMENT NAME -- TACTV2                                           */
*/*                                                                  */
*/* DESCRIPTIVE NAME -- N/A                                          */
*/*                                                                  */
*/* FUNCTION -- TO SEARCH PAGEABLE-LPA FOR COMMAND NAME ENTERED      */
*/*  OPERATION --                                                    */
*/*           - LOAD COMMAND NAME ACROSS REGISTERS 0 AND 1           */
*/*           - RESERVE REGISTERS 5 THROUGH 9 (USED BY IEAVVMSR)     */
*/*           - GO TO EXTERNAL ROUTINE IEAVVMSR WHICH SEARCHES       */
*/*             THROUGH THE PAGEABLE LINK PACK AREA FOR NEW COMMAND  */
*/*             NAME                                                 */
*/*           - IF IEAVVMSR FINDS THE NAME, IT RETURN CONTROL TO THE */
*/*             NEXT SEQUENTIAL INSTRUCTION FOLLOWING ITS LINK EXIT  */
*/*             AND REGISTER 14 CONTAINS A POINTER TO THE NAMED      */
*/*             PROGRAM.  THIS NEXT SEQUENTIAL INSTRUCTION IN THE    */
*/*             TMP SETS REGISTER 14 TO ZERO TO INDICATE NAME FOUND. */
*/*           - IF IEAVVMSR FAILS TO FIND THE COMMAND NAME IN LPA,   */
*/*             CONTROL IS RETURNED TO SECOND SEQUENTIAL INSTRUC-    */
*/*             TION FOLLOWING THE LINK EXIT AND REGISTER 14 STILL   */
*/*             HOLDS THE LINK ADDRESS OF IEAVVMSR                   */
*/*           - RESTORE RESERVED REGISTERS 5 THROUGH 9               */
*/*           - IF COMMAND NAME WAS NOT FOUND (REGISTER 14 NOT ZERO) */
*/*             THEN GO TO ROUTINE TACTV3 TO CONTINUE SEARCH         */
*/*                                                                  */
*/* ENTRY -- LINKED BY REGISTER 14                                   */
*/*                                                                  */
*/* INPUT -- N/A                                                     */
*/*                                                                  */
*/* OUTPUT -- N/A                                                    */
*/*                                                                  */
*/* EXTERNAL REFERENCES -- IEAVVMSR (SEARCH PAGEABLE LPA ROUTINE)    */
*/*                                                                  */
*/* EXIT NORMAL -- RETURN BY LINK REGISTER 14                        */
*/*                                                                  */
*/*   RETURN CODE -- N/A                                             */
*/*                                                                  */
*/* EXIT ERROR -- N/A                                                */
*/*                                                                  */
*/*   CONDITIONS -- N/A                                              */
*/*                                                                  */
*/*   RETURN CODE -- N/A                                             */
*/*                                                                  */
*/* * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * * */
*TACTV2:    /* EP TO SEARCH-PAGEABLE-LPA ROUTINE                     */
*    ENTRY;                                                     01S0811
*                                                               01S0811
TACTV2   DS    0H                                               01S0812
*    /* SEARCH LPA FOR COMMAND NAME                                  */
*    LM(R0,R1,NXTCMD);              /* SET TARGET NAME               */
         LM    R0,R1,NXTCMD(WRKAPTR1)                           01S0812
*    RESPECIFY (R3,R5,R6,R7,R8,R9,R14) RSTD;                    01S0813
*    STM(R5,R9,SVT042A);            /* RESERVE REGISTERS             */
         L     @04,TMPWA2P(,WRKAPTR2)                           01S0814
         STM   R5,R9,SVT042A(@04)                               01S0814
*    R5 = ADDR(TMPWA2);             /* RESERVE TEMPORARY AREA PTR    */
         LR    R5,@04                                           01S0815
*    R3 = CVTPTR;                   /* SET CVT POINTER               */
         L     @04,CVTPTR                                       01S0816
         LR    R3,@04                                           01S0816
*    CALL  IEAVVMSR;                /* GO TO SEARCH ROUTINE          */
         L     @15,CVTLPDSR(,@04)                               01S0817
         BALR  @14,@15                                          01S0817
*    GEN SETS(R14) (LA    R14,0(0,0));  /* SET FOUND                 */
         LA    R14,0(0,0)
*    RESPECIFY (TMPWA2) BASED(R5);                              01S0819
*    LM(R5,R9,SVT042A);             /* RESTORE REGISTERS             */
         LM    R5,R9,SVT042A(R5)                                01S0820
*    RESPECIFY (TMPWA2) BASED(TMPWA2P);                         01S0821
*    RESPECIFY (R3,R5,R6,R7,R8,R9) UNRSTD;                      01S0822
*                                                               01S0822
*    /* CHECK IF FOUND IN LPA                                        */
*    /* NO, CONTINUE SEARCH IN ACTIVE LPA                            */
*    IF R14 ^= ZERO                 /* CHK FOR FOUND RETURN CODE     */
*      THEN                         /* NO,                           */
         LTR   R14,R14                                          01S0823
         BZ    @RF00823                                         01S0823
*        CALL TACTV3;               /*    TO CONTINUE SEARCH         */
*                                                               01S0824
         BAL   @14,TACTV3                                       01S0824
*    RESPECIFY (R14) RSTD;                                      01S0825
@RF00823 DS    0H                                               01S0826
*TACTV2A:                           /* FOR PSEUDO-RETURN             */
*    R14 = ADDR(TACTVB);            /* SET FOR PSEUDO-RETURN         */
TACTV2A  LA    R14,TACTVB                                       01S0826
*    RESPECIFY (R14) UNRSTD;                                    01S0827
*    END  TSETCD1;                  /* RETURN TO CALLER              */
         B     @EL00006                                         01S0828
*/* * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * * */
*/*                                                                  */
*/* SEGMENT NAME -- TACTV3                                           */
*/*                                                                  */
*/* DESCRIPTIVE NAME -- N/A                                          */
*/*                                                                  */
*/* FUNCTION -- TO SEARCH ACTIVE-LPA FOR COMMAND NAME ENTERED        */
*/*  OPERATION --                                                    */
*/*           - ISSUE IDENTIFY MACRO TO SEARCH ACTIVE LPA FOR NEW    */
*/*             COMMAND NAME                                         */
*/*           - IF IDENTIFY RETURN CODE IS UNSATISFACTORY (OTHER     */
*/*             THAN 8, 12, OR 20) ABEND WITH DUMP TO INDICATE ERROR */
*/*           - IF IDENTIFY RETURNS THE NOT-FOUND CODE (12), GO TO   */
*/*             ROUTINE, TACTV4, TO CONTINUE SEARCH (WITH BLDL)      */
*/*                                                                  */
*/* ENTRY -- LINKED BY REGISTER 14                                   */
*/*                                                                  */
*/* INPUT -- N/A                                                     */
*/*                                                                  */
*/* OUTPUT -- N/A                                                    */
*/*                                                                  */
*/* EXTERNAL REFERENCES -- N/A                                       */
*/*                                                                  */
*/* EXIT NORMAL -- RETURN BY LINK REGISTER 14                        */
*/*                                                                  */
*/*   RETURN CODE -- N/A                                             */
*/*                                                                  */
*/* EXIT ERROR -- N/A                                                */
*/*                                                                  */
*/*   CONDITIONS -- N/A                                              */
*/*                                                                  */
*/*   RETURN CODE -- N/A                                             */
*/*                                                                  */
*/* * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * * */
*TACTV3:    /* EP TO SEARCH-ACTIVE-LPA ROUTINE                       */
*    PROC OPTIONS(DONTSAVE,NOSAVEAREA);                         01S0829
*                                                               01S0829
TACTV3   DS    0H                                               01S0830
*    /* SEARCH ACTIVE=LPA FOR COMMAND NAME                           */
*    R0 = ADDR(NXTCMD);             /* PTR TO TARGET NAME            */
         LA    R0,NXTCMD(,WRKAPTR1)                             01S0830
*    R1 = ZERO;                     /* NULL ENTRY PTR                */
         SLR   R1,R1                                            01S0831
*    GEN SETS(R0,R1) (IDENTIFY  EPLOC=(R0),ENTRY=(R1));         01S0832
         IDENTIFY  EPLOC=(R0),ENTRY=(R1)
*    /* CHECK FOR ERROR IN SEARCH                                    */
*    IF R15 ^= D8                   /* CHK FOR FOUND RC              */
*        & R15 ^= D12               /* CHK FOR NOT-FOUND RC          */
*        & R15 ^= D20               /* CHK FOR PRIOR-FOUND RC        */
*      THEN                         /* NONE OF ABOVE,                */
         C     R15,@CF00041                                     01S0833
         BE    @RF00833                                         01S0833
         C     R15,@CF00054                                     01S0833
         BE    @RF00833                                         01S0833
         C     R15,@CF00056                                     01S0833
         BE    @RF00833                                         01S0833
*        GEN(ABEND 208,DUMP);       /* INDICATE IDENTIFY ERROR       */
         ABEND 208,DUMP
*    /* CHK IF FOUND IN ACTIVE-LPA                                   */
*    /* NO, CONTINUE SEARCH IN LINK LIST                             */
*    IF R15 = D12                   /* CHK FOR NOT-FOUND RC          */
*      THEN                         /* YES, NOT FOUND                */
@RF00833 C     R15,@CF00054                                     01S0835
         BNE   @RF00835                                         01S0835
*        CALL  TACTV4;              /* CONTINUE SEARCH               */
*                                                               01S0836
         BAL   @14,TACTV4                                       01S0836
*    RESPECIFY (R14) RSTD;                                      01S0837
@RF00835 DS    0H                                               01S0838
*TACTV3A:                           /* FOR PSEUDO-RETURN             */
*    R14 = ADDR(TACTV2A);           /* SET FOR PSEUDO-RETURN         */
TACTV3A  LA    R14,TACTV2A                                      01S0838
*    RETURN;                        /* RETURN TO CALLER              */
@EL00007 DS    0H                                               01S0839
@EF00007 DS    0H                                               01S0839
@ER00007 BR    @14                                              01S0839
*    RESPECIFY (R14) UNRSTD;                                    01S0840
*/* * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * * */
*/*                                                                  */
*/* SEGMENT NAME -- TACTV6                                           */
*/*                                                                  */
*/* DESCRIPTIVE NAME -- N/A                                          */
*/*                                                                  */
*/* FUNCTION -- TO ATTACH THE COMMAND PROCESSOR WHOSE NAME WAS       */
*/*       LOCATED IN LPA                                             */
*/*  OPERATION --                                                    */
*/*           - IF NO ATTACH HAS ALREADY BEEN DONE AND CALL-         */
*/*             PROCESSING IS NOT CURRENTLY ACTIVE, THEN --          */
*/*             - RESET APF-AUTHORIZATION FLAG (JSCBAUTH)            */
*/*             - SET TCB PROTECTION KEY TO PROBLEM PROGRAM KEY (8)  */
*/*             - SET ATTACH-CURRENTLY-ACTIVE FLAG, ATCHNOW          */
*/*             - INITIALIZE PARAMETER LISTS AND POINTERS FOR ATTACH */
*/*             - ISSUE ATTACH (EPLOC) WITH ESTAI (TO IKJEFT04), AND */
*/*               WITH REQUEST FOR SPECIAL APF HANDLING (RSAPF)      */
*/*             - ON RETURN FROM ATTACH, SAVE POINTER TO SUBTASK TCB */
*/*               (IN REGISTER 1) IN AREA TPLCTCB                    */
*/*           - IF ATTACH RETURN CODE IS UNSATISFACTORY (NOT ZERO)   */
*/*             THEN ABEND WITH DUMP TO INDICATE ATTACH FAILURE      */
*/*           - SUPPLY TSEVENT WIHT NAME OF JUST ATTACHED COMMAND    */
*/*             PROCESSOR PROGRAM                                    */
*/*           - SET DO-WAIT-FOR-SUBTASK-COMPLETION FLAG, DOWAIT      */
*/*                                                                  */
*/* ENTRY -- LINKED BY REGISTER 14                                   */
*/*                                                                  */
*/* INPUT -- N/A                                                     */
*/*                                                                  */
*/* OUTPUT -- N/A                                                    */
*/*                                                                  */
*/* EXTERNAL REFERENCES -- N/A                                       */
*/*                                                                  */
*/* EXIT NORMAL -- RETURN BY LINK REGISTER 14                        */
*/*                                                                  */
*/*   RETURN CODE -- N/A                                             */
*/*                                                                  */
*/* EXIT ERROR -- N/A                                                */
*/*                                                                  */
*/*   CONDITIONS -- N/A                                              */
*/*                                                                  */
*/*   RETURN CODE -- N/A                                             */
*/*                                                                  */
*/* * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * * */
*TACTV6:    /* EP TO ATTACH-CP-FOUND-IN-LPA ROUTINE                  */
*    ENTRY;                                                     01S0841
*                                                               01S0841
TACTV6   DS    0H                                               01S0842
*    /* CHK IF CP NAME FOUND IN LPA OR ACTIVE-LPA                    */
*    IF ATCHNOW = NO                /* CHK FOR BLDL-ATTACH           */
*        & CALLNOW = NO             /*   OR IF CALL IS ACTIVE        */
*      THEN DO;                     /* NO, DO DIRECT ATTACH          */
         TM    ATCHNOW(WRKAPTR1),B'10001000'                    01S0842
         BNZ   @RF00842                                         01S0842
*        CALL TSRCHAPF;             /* DETERMINE APF ELIGIBILITY     */
         BAL   @14,TSRCHAPF                                     01S0844
*        ATCHNOW = YES;             /* ATTACH-ACTIVE BIT             */
         OI    ATCHNOW(WRKAPTR1),B'10000000'                    01S0845
*        R1 = ADDR(CPPL);           /* PTR TO CP PARM LIST           */
         L     @14,CPPLPTR(,WRKAPTR1)                           01S0846
         LR    R1,@14                                           01S0846
*        CPPL = TPL;                /* INITIALIZE PARM LIST          */
         MVC   CPPL(16,@14),TPL(WRKAPTR1)                       01S0847
*        RESPECIFY (R3,R4,R5) RSTD;                             01S0848
*        R3 = TPLCECB;              /* SET PTR TO CP ECB             */
         L     R3,TPLCECB(,WRKAPTR1)                            01S0849
*        R4 = TMPT04;               /* PTR TO ABEND STAI EXIT        */
         L     R4,TMPT04(,WRKAPTR2)                             01S0850
*        R5 = R8;                   /* SET PTR TO PASSED PL          */
         LR    R5,R8                                            01S0851
*        R15 = ATTCHPTR;            /* SET PTR TO ATTACH PL          */
         L     R15,ATTCHPTR(,WRKAPTR1)                          01S0852
*        IF DRSAPF = YES            /* CHK FOR ATTACH WITH APF       */
*          THEN DO;                 /* YES,                          */
         TM    DRSAPF(WRKAPTR2),B'01000000'                     01S0853
         BNO   @RF00853                                         01S0853
*            GENERATE  SETS(R14);                               01S0855
          LA    R14,NXTCMD(WRKAPTR1)   SET PTR TO NEXT  NAME
          ATTACH  EPLOC=(R14),ECB=(R3),ESTAI=((R4),(R5)),RSAPF=YES,    *
               MF=(E,(1)),SF=(E,(15))
*          END;                                                 01S0856
*          ELSE DO;                 /* DO NOT ATTACH WITH APF        */
         B     @RC00853                                         01S0857
@RF00853 DS    0H                                               01S0858
*            R15 = ADDR(TATACHB);   /* SET PTR TO ATTACH    @Z403333 */
         LA    R15,TATACHB                                      01S0858
*            GEN FLOWS(TATACHB) (SYNCH  (15));  /* TO RTN   @Z403333 */
         SYNCH  (15)
*          END;                                                 01S0860
*        RCODE = R15;               /* SAVE RETURN CODE              */
@RC00853 ST    R15,RCODE(,WRKAPTR1)                             01S0861
*        TPLCTCB = R1;              /* SAVE TCB POINTER              */
         ST    R1,TPLCTCB(,WRKAPTR1)                            01S0862
*      END;                                                     01S0863
*                                                               01S0863
*    RESPECIFY (R3,R4,R5) UNRSTD;                               01S0864
@RF00842 DS    0H                                               01S0865
*    IF RCODE ^= ATTDONE            /* CHK FOR SATISFACTORY ATTACH   */
*      THEN                         /* NO,                           */
         L     @14,RCODE(,WRKAPTR1)                             01S0865
         LTR   @14,@14                                          01S0865
         BZ    @RF00865                                         01S0865
*        GEN(ABEND 201,DUMP);       /*   INDICATE ATTACH FAILURE     */
*                                                               01S0866
         ABEND 201,DUMP
*    R1 = ANUCMD1;                  /* 1ST 4 CHARS OF CMD NAME       */
@RF00865 L     R1,ANUCMD1(,WRKAPTR1)                            01S0867
*    R15 = ANUCMD2;                 /* 2ND 4 CHARS OF CMD NAME       */
         L     R15,ANUCMD2(,WRKAPTR1)                           01S0868
*    GEN(TSEVENT   PPMODE);         /* ISSUE TSEVENT                 */
         TSEVENT   PPMODE
*    DOWAIT = YES;                                              01S0870
*                                                               01S0870
         OI    DOWAIT(WRKAPTR1),B'00000100'                     01S0870
*    RESPECIFY (R14) RSTD;                                      01S0871
*    R14 = ADDR(TACTVC);            /* SET FOR PSEUDO-RETURN         */
         LA    R14,TACTVC                                       01S0872
*    RESPECIFY (R14) UNRSTD;                                    01S0873
*    END  TACTV3;                   /* RETURN TO CALLER              */
         B     @EL00007                                         01S0874
*/* * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * * */
*/*                                                                  */
*/* SEGMENT NAME -- TACTV4                                           */
*/*                                                                  */
*/* DESCRIPTIVE NAME -- N/A                                          */
*/*                                                                  */
*/* FUNCTION -- TO SEARCH LINK-LIST FOR NEW COMMAND NAME (WITH BLDL) */
*/*  OPERATION --                                                    */
*/*           - INSERT NEW COMMAND NAME IN BLDL PARAMETER BLOCK      */
*/*           - ISSUE BLDL                                           */
*/*           - IF BLDL RETURN CODE IS OTHER THAN NAME FOUND (0) OR  */
*/*             NOT-FOUND (4), ABEND (202) WITH DUMP TO INDICATE     */
*/*             BLDL FAILURE                                         */
*/*           - IF BLDL RETURN CODE IS NOT-FOUND CODE (4), THEN --   */
*/*             - SET PERFORM IMPLICIT-EXEC FUNCTION FLAG, DOIMPLX   */
*/*             - SET PERFORM-NEXT-COMMAND-INVOCATION FUNCTION FLAG, */
*/*               DOINVOK                                            */
*/*             - RESET CHECK-CURRENT-COMMAND-TASK-STATUS FLAG,      */
*/*               DOCHKCP                                            */
*/*           - IF BLDL RETURN CODE IS NAME-FOUND CODE (0), THEN GO  */
*/*             TO ATTACH (DE) ROUTINE, TACTV5                       */
*/*                                                                  */
*/* ENTRY -- LINKED BY REGISTER 14                                   */
*/*                                                                  */
*/* INPUT -- N/A                                                     */
*/*                                                                  */
*/* OUTPUT -- N/A                                                    */
*/*                                                                  */
*/* EXTERNAL REFERENCES -- N/A                                       */
*/*                                                                  */
*/* EXIT NORMAL -- RETURN BY LINK REGISTER 14                        */
*/*                                                                  */
*/*   RETURN CODE -- N/A                                             */
*/*                                                                  */
*/* EXIT ERROR -- N/A                                                */
*/*                                                                  */
*/*   CONDITIONS -- N/A                                              */
*/*                                                                  */
*/*   RETURN CODE -- N/A                                             */
*/*                                                                  */
*/* * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * * */
*TACTV4:    /* EP TO SEARCH-FOR-CP-WITH-BLDL ROUTINE                 */
*    PROC OPTIONS(DONTSAVE,NOSAVEAREA);                         01S0875
*                                                               01S0875
TACTV4   DS    0H                                               01S0876
*    /* SEARCH LINK LIST                                             */
*    BLDLNAME = NXTCMD;             /* TARGET NAME                   */
         MVC   BLDLNAME(8,WRKAPTR1),NXTCMD(WRKAPTR1)            01S0876
*    R1 = ZERO;                     /* INDICATE NO DCB               */
         SLR   R1,R1                                            01S0877
*    R0 = ADDR(BLDLLST);            /* BUILD LIST                    */
         LA    R0,BLDLLST(,WRKAPTR1)                            01S0878
*    GEN(BLDL  (1),(0));            /* ISSUE BLDL                    */
         BLDL  (1),(0)
*    /* CHK FOR BLDL ERROR                                           */
*    IF R15 ^= BLDLNFND             /* NOT-FOUND                     */
*        & R15 ^= BLDLFND           /* & FOUND RC                    */
*      THEN                         /* NEITHER,                      */
         C     R15,@CF00045                                     01S0880
         BE    @RF00880                                         01S0880
         LTR   R15,R15                                          01S0880
         BZ    @RF00880                                         01S0880
*        GEN(ABEND 202,DUMP);       /*   INDICATE BLDL ERROR         */
         ABEND 202,DUMP
*    /* CHK IF NAME NOT-FOUND                                        */
*    IF R15 = BLDLNFND              /* CHK FOR FOUND RETURN CODE     */
*      THEN DO;                     /* YES, TRY IMPLICIT             */
@RF00880 C     R15,@CF00045                                     01S0882
         BNE   @RF00882                                         01S0882
*        DOIMPLX = YES;             /* SET DO-IMPLICIT-EXEC FLAG     */
         OI    DOIMPLX(WRKAPTR1),B'01000000'                    01S0884
*        DOINVOK = YES;             /* SET DO-NEXT-CMD-LOOP FLAG     */
         OI    DOINVOK(WRKAPTR1),B'00000100'                    01S0885
*        DOCHKCP = NO;              /* TO RETURN TO DO-EXEC-CMD      */
         NI    DOCHKCP(WRKAPTR1),B'11111101'                    01S0886
*      END;                                                     01S0887
*      ELSE                         /* CP NAME FOUND,                */
*        CALL  TACTV5;              /*   TO PERFORM ATTACH-CP        */
*                                                               01S0888
         B     @RC00882                                         01S0888
@RF00882 BAL   @14,TACTV5                                       01S0888
*    RESPECIFY (R14) RSTD;                                      01S0889
@RC00882 DS    0H                                               01S0890
*TACTV4A:                           /* FOR PSEUDO-RETURN             */
*    R14 = ADDR(TACTV3A);           /* SET FOR PSEUDO-RETURN         */
TACTV4A  LA    R14,TACTV3A                                      01S0890
*    RESPECIFY (R14) UNRSTD;                                    01S0891
*    END  TACTV4;                   /* RETURN TO CALLER              */
@EL00008 DS    0H                                               01S0892
@EF00008 DS    0H                                               01S0892
@ER00008 BR    @14                                              01S0892
*/* * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * * */
*/*                                                                  */
*/* SEGMENT NAME -- TACTV5                                           */
*/*                                                                  */
*/* DESCRIPTIVE NAME -- N/A                                          */
*/*                                                                  */
*/* FUNCTION -- TO ATTACH THE COMMAND PROCESSOR WHOSE NAME WAS       */
*/*       LOCATED BY BLDL                                            */
*/*  OPERATION --                                                    */
*/*           - RESET APF-AUTHORIZATION FLAG, JSCBAUTH               */
*/*           - SET ATTACH-CURRENTLY-ACTIVE FLAG, ATCHNOW            */
*/*           - SET TMP-TASK TCB PROTECTION KEY TO PROBLEM PROGRAM   */
*/*             PROTECTION KEY (8)                                   */
*/*           - INITIALIZE PARAMETER LISTS AND POINTERS FOR ATTACH   */
*/*           - ISSUE ATTACH (DE), WITH ESTAI (TO IKJEFT04), AND     */
*/*             WITH REQUEST FOR SPECIAL APF HANDLING (RSAPF)        */
*/*           - ON RETURN FROM ATTACH SAVE POINTER TO SUBTASK TCB    */
*/*             (IN REGISTER 1) IN AREA, TPLCTCB                     */
*/*           - SAVE ATTACH RETURN CODE IN AREA, RCODE               */
*/*                                                                  */
*/* ENTRY -- LINKED BY REGISTER 14                                   */
*/*                                                                  */
*/* INPUT -- N/A                                                     */
*/*                                                                  */
*/* OUTPUT -- N/A                                                    */
*/*                                                                  */
*/* EXTERNAL REFERENCES -- N/A                                       */
*/*                                                                  */
*/* EXIT NORMAL -- RETURN BY LINK REGISTER 14                        */
*/*                                                                  */
*/*   RETURN CODE -- N/A                                             */
*/*                                                                  */
*/* EXIT ERROR -- N/A                                                */
*/*                                                                  */
*/*   CONDITIONS -- N/A                                              */
*/*                                                                  */
*/*   RETURN CODE -- N/A                                             */
*/*                                                                  */
*/* * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * * */
*TACTV5:    /* EP TO ATTACH-CP-FOUND-BY-BLDL ROUTINE                 */
*    PROC OPTIONS(DONTSAVE,NOSAVEAREA);                         01S0893
*                                                               01S0893
TACTV5   DS    0H                                               01S0894
*    /* NAME FOUND, ATTACH CP IN PROBLEM PROGRAM STATE, PP KEY       */
*    ATCHNOW = YES;                 /* ATTACH-ACTIVE                 */
         OI    ATCHNOW(WRKAPTR1),B'10000000'                    01S0894
*    CALL TSRCHAPF;                 /* DETERMINE APF ELIGIBILITY     */
*                                                               01S0895
         BAL   @14,TSRCHAPF                                     01S0895
*    R1 = ADDR(CPPL);               /* PTR TO CP PARMS               */
         L     @14,CPPLPTR(,WRKAPTR1)                           01S0896
         LR    R1,@14                                           01S0896
*    CPPL = TPL;                    /* INITIALIZE PL                 */
         MVC   CPPL(16,@14),TPL(WRKAPTR1)                       01S0897
*    RESPECIFY (R3,R4,R5,R14) RSTD;                             01S0898
*    R3 = TPLCECB;                  /* PTR TO CP ECB                 */
         L     R3,TPLCECB(,WRKAPTR1)                            01S0899
*    R5 = R8;                       /* PTR TO PASSED PARM LIST       */
         LR    R5,R8                                            01S0900
*    IF DRSAPF = YES                /* CHK FOR ATTACH WITH APF       */
*      THEN DO;                     /* YES, WITH APF                 */
