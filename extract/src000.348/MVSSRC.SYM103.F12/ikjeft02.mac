*/*                                                                  */
*/* FUNCTION -- TO CLOSE OUT TMP-CALL FUNCTION                       */
*/*  OPERATION --                                                    */
*/*           - IF CALL IS CURRENTLY ACTIVE, CALLNOW FLAG, --        */
*/*             - ACTIVATE THE TERMINATE-CALL-ACTIVITY ROUTINE,      */
*/*               DOCALL2                                            */
*/*             - SET CALL-INACTIVE FLAG, CALLNOW EQUALS NO          */
*/*             - RESTORE ATTACH PARAMETER LIST FOR TMP              */
*/*                                                                  */
*/* ENTRY -- LINKED BY REGISTER 14                                   */
*/*                                                                  */
*/* INPUT -- N/A                                                     */
*/*                                                                  */
*/* OUTPUT -- N/A                                                    */
*/*                                                                  */
*/* EXTERNAL REFERENCES -- N/A                                       */
*/*                                                                  */
*/* EXIT NORMAL -- RETURN BY LINK REGISTER 14                        */
*/*                                                                  */
*/*   RETURN CODE -- N/A                                             */
*/*                                                                  */
*/* EXIT ERROR -- N/A                                                */
*/*                                                                  */
*/*   CONDITIONS -- N/A                                              */
*/*                                                                  */
*/*   RETURN CODE -- N/A                                             */
*/*                                                                  */
*/* * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * * */
*TCALLND:   /* EP TO CLOSE-OUT-CALL-FUNCTION ROUTINE                 */
*    ENTRY;                                                     01S0295
*                                                               01S0295
TCALLND  DS    0H                                               01S0296
*    IF CALLNOW = YES               /* CHK IF CALL ACTIVE            */
*        & TPLCTCB = ZERO           /* NO CURRENT ATTACH      ZA26167*/
*      THEN DO;                     /* YES,                          */
         TM    CALLNOW(WRKAPTR1),B'00001000'                    01S0296
         BNO   @RF00296                                         01S0296
         L     @14,TPLCTCB(,WRKAPTR1)                           01S0296
         LTR   @14,@14                                          01S0296
         BNZ   @RF00296                                         01S0296
*        CALLNOW = NO;              /*   SET CALL-NOT-ACTIVE FLAG    */
         NI    CALLNOW(WRKAPTR1),B'11110111'                    01S0298
*        CALL DOCALL2;              /* GO TO TERMINATION             */
         L     @15,@CV00134                                     01S0299
         BALR  @14,@15                                          01S0299
*      END;                                                     01S0300
*                                                               01S0300
*    RESPECIFY (R14) RSTD;                                      01S0301
@RF00296 DS    0H                                               01S0302
*    R14 = ADDR(TRETD);             /* SET FOR PSEUDO-RETURN  ZA26167*/
         LA    R14,TRETD                                        01S0302
*    RETURN;                        /* RETURN TO CALLER              */
         B     @EL00003                                         01S0303
*    RESPECIFY (R14) UNRSTD;                                    01S0304
