*/* SEGMENT NAME -- TGETCDX                                          */
*/*                                                                  */
*/* DESCRIPTIVE NAME -- N/A                                          */
*/*                                                                  */
*/* FUNCTION -- TO GET NEW COMMAND ENTRY FROM INPUT SOURCE           */
*/*  OPERATION --                                                    */
*/*           - (SAVE LINK REGISTER)                                 */
*/*           - CHECK IF ATTENTION-PROCESSING IS ACTIVE, DOCHKCP     */
*/*             FLAG --                                              */
*/*             - IF NO, THEN --                                     */
*/*               - CHECK IF PUTGET RETURN SIGNIFIES AN ENTRY BUFFER */
*/*                 WAS RETRIEVED                                    */
*/*               - IF YES, THEN --                                  */
*/*                 - COPY THE POINTER TO THE BUFFER RETURNED BY     */
*/*                   PGPBIBUF, INTO THE COMMAND SCAN PARAMETER      */
*/*                   CSPLCBUF                                       */
*/*                 - SET THE PERFORM-COMMAND-SCAN-NOW FLAG, DOSCAN  */
*/*               - IF NO BUFFER WAS RETRIEVED BY PUTGET, THEN --    */
*/*                 - IF PUTGET RETURN CODE SIGNIFIES PUTGET ERROR,  */
*/*                   INDICATE BY FORCING AN ABEND 204               */
*/*                 - IF NOT PUTGET ERROR, THEN --                   */
*/*                   - IF RETURN CODE SIGNIFIES A 2ND LEVEL MESSAGE */
*/*                     EXISTS, SET MESSAGE-DELETE FLAG IN ECT,      */
*/*                     ECTMSGF                                      */
*/*                   - SET DO-GET-COMMAND-NOW FLAG, DOGETC, TO      */
*/*                     CAUSE THE PUTGET TO BE REISSUED              */
*/*           - (RESTORE LINK REGISTER)                              */
*/*                                                                  */
*/* ENTRY -- LINKED BY REGISTER 14                                   */
*/*                                                                  */
*/* INPUT -- N/A                                                     */
*/*                                                                  */
*/* OUTPUT -- N/A                                                    */
*/*                                                                  */
*/* EXTERNAL REFERENCES -- N/A                                       */
*/*                                                                  */
*/* EXIT NORMAL -- RETURN BY LINK REGISTER 14                        */
*/*                                                                  */
*/*   RETURN CODE -- N/A                                             */
*/*                                                                  */
*/* EXIT ERROR -- N/A                                                */
*/*                                                                  */
*/*   CONDITIONS -- N/A                                              */
*/*                                                                  */
*/*   RETURN CODE -- N/A                                             */
*/*                                                                  */
*/* * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * * */
*TGETCDX:   /* EP TO GET-NEW-COMMAND ROUTINE                         */
*    ENTRY;                                                     01S0305
*                                                               01S0305
TGETCDX  DS    0H                                               01S0306
*    RESPECIFY (R2) RSTD;                                       01S0306
*    R2 = PGPBPTR;                  /* PTR TO PUTGET PARM BLOCK      */
         L     R2,PGPBPTR(,WRKAPTR1)                            01S0307
*    R1 = ADDR(IOPL);               /* SET PTR TO PARM LIST          */
         LR    R1,SRPARMLP                                      01S0308
*    R15 = ADDR(TGETCD1);           /* PTR TO ISSUE-PUTGET ROUTINE   */
         LA    R15,TGETCD1                                      01S0309
*    GEN FLOWS(TGETCD1) (SYNCH  (15));  /* TO PUTGET ROUTINE         */
         SYNCH  (15)
*    RCODE = R15;                   /* RESERVE PURGET RETURN CODE    */
         ST    R15,RCODE(,WRKAPTR1)                             01S0311
*    RESPECIFY (R2) UNRSTD;                                     01S0312
*                                                               01S0312
*    IF DOCHKCP = NO                /* CHK FOR ATTENTION             */
*      THEN DO;                     /* NO,                           */
         TM    DOCHKCP(WRKAPTR1),B'00000010'                    01S0313
         BNZ   @RF00313                                         01S0313
*        IF RCODE<=PTGTDONE         /* CHK FOR COMMAND RETRIEVAL     */
*          THEN DO;                 /* YES, COMMAND RETURNED         */
         L     @14,RCODE(,WRKAPTR1)                             01S0315
         C     @14,@CF00045                                     01S0315
         BH    @RF00315                                         01S0315
*            CSPLCBUF=PGPBIBUF;     /* ESTABLISH NEW CMD BUFFER      */
         L     @14,PGPBPTR(,WRKAPTR1)                           01S0317
         L     @14,PGPBIBUF(,@14)                               01S0317
         ST    @14,CSPLCBUF(,SRPARMLP)                          01S0317
*            DOSCAN=YES;            /* SET TO SCAN THE COMMAND       */
         OI    DOSCAN(WRKAPTR1),B'00000010'                     01S0318
*          END;                                                 01S0319
*          ELSE DO;                 /* NO COMMAND RETRIEVED          */
         B     @RC00315                                         01S0320
@RF00315 DS    0H                                               01S0321
*            IF RCODE>PTGTNPS       /* CHK FOR PUTGET ERROR          */
*              THEN                 /* YES,                          */
         CLC   RCODE(4,WRKAPTR1),@CF00054                       01S0321
         BNH   @RF00321                                         01S0321
*                GEN(ABEND 204,DUMP);  /* PUTGET ERROR               */
         ABEND 204,DUMP
*              ELSE DO;             /* CHK FOR 2ND LEVEL MESSAGE     */
         B     @RC00321                                         01S0323
@RF00321 DS    0H                                               01S0324
*                IF RCODE=PTGTNPS   /* COMMAND RETRIEVAL             */
*                  THEN             /* NO,                           */
         CLC   RCODE(4,WRKAPTR1),@CF00054                       01S0324
         BNE   @RF00324                                         01S0324
*                    ECTMSGF=YES;   /* DELETE 2ND LEVEL MESSAGE      */
         L     @14,ECTPTR(,WRKAPTR2)                            01S0325
         MVI   ECTMSGF(@14),B'10000000'                         01S0325
*                DOGETC=YES;        /* SET RE-DO GET COMMAND         */
@RF00324 OI    DOGETC(WRKAPTR1),B'00010000'                     01S0326
*              END;                                             01S0327
*          END;                                                 01S0328
@RC00321 DS    0H                                               01S0329
*      END;                                                     01S0329
*                                                               01S0329
@RC00315 DS    0H                                               01S0330
*    RESPECIFY (R14) RSTD;                                      01S0330
@RF00313 DS    0H                                               01S0331
*    R14 = ADDR(TRETE);             /* SET FOR PSEUDO-RETURN         */
         LA    R14,TRETE                                        01S0331
*    RESPECIFY (R14) UNRSTD;                                    01S0332
*    END  TINIT2;                   /* RETURN TO CALLER              */
         B     @EL00003                                         01S0333
