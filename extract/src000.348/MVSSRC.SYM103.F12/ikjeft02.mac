*TSETCD:    /* EP TO SET-UP-NEXT-COMMAND-INVOCATION ROUTINE          */
*    ENTRY;                                                     01S0581
*                                                               01S0581
TSETCD   DS    0H                                               01S0582
*    DOSETBF = YES;                 /* SET TO CHK FOR IMPLI-EXEC     */
         OI    DOSETBF(WRKAPTR1),B'00010000'                    01S0582
*    NXTCMD = CSOACNM->CMDNAME(1:CSOALNM);  /* SAVE CMD              */
         MVI   NXTCMD+1(WRKAPTR1),C' '                          01S0583
         MVC   NXTCMD+2(6,WRKAPTR1),NXTCMD+1(WRKAPTR1)          01S0583
         L     @14,CSPLOA(,SRPARMLP)                            01S0583
         LH    @06,CSOALNM(,@14)                                01S0583
         LR    @05,@06                                          01S0583
         BCTR  @05,0                                            01S0583
         L     @04,CSOACNM(,@14)                                01S0583
         EX    @05,@SM03118                                     01S0583
