         SPKA  0(R2)                                            01S0976
*    END; /*MODESET  KEYADDR(SPROK0) WORKREG(R2)*/              01S0977
*    JSCBAUTH = NO;                 /* RESET APF-AUTH-PGM FLAG       */
         L     @03,CVTPTR                                       01S0978
         L     @03,CVTTCBP(,@03)                                01S0978
         L     @03,TCURTCB(,@03)                                01S0978
         L     @03,TCBJSCB(,@03)                                01S0978
         NI    JSCBAUTH(@03),B'11111110'                        01S0978
*    DRSAPF = NO;                   /* RESET ATTACH-WITH-APF FLAG    */
         NI    DRSAPF(WRKAPTR2),B'10111111'                     01S0979
*    DO; /*MODESET  KEYADDR(TEPKEY) WORKREG(R2)*/               01S0980
