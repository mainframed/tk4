         TM    ABND806(WRKAPTR1),B'00000100'                    01S0564
         BNO   @RF00564                                         01S0564
         TM    BKGMODE(WRKAPTR2),B'10000000'                    01S0564
         BNO   @RF00564                                         01S0564
*        DODONE = YES;              /* TERMINATE                     */
         OI    DODONE(WRKAPTR1),B'00001000'                     01S0565
