         NI    DOPUTM(WRKAPTR1),B'01111111'                     01S0557
*    NUMSEG = D1;                   /* SET NUMBER OF SEGMENTS        */
*                                                               01S0558
         MVC   NUMSEG(4,WRKAPTR1),@CF00032                      01S0558
