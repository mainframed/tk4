*/* SEGMENT NAME -- TSETCD1                                          */
*/*                                                                  */
*/* DESCRIPTIVE NAME -- N/A                                          */
*/*                                                                  */
*/* FUNCTION -- TO INDICATE ABEND PROCESSING IS IN CONTROL IF THE    */
*/*       COMMAND-NOT-FOUND CONDITION IS CURRENT,  TO EITHER LINK    */
*/*       TO TEST IF THAT IS THE NEW COMMAND NAME OR TO SET UP THE   */
*/*       NEW COMMAND BUFFER FOR THE NEXT COMMAND ATTACH             */
*/*  OPERATION --                                                    */
*/*           - RESET THE DO-IMPLICIT-EXEC-CHECK FLAG, DOSETBF       */
*/*           - IF THE ABEND-FOR-FAILURE-TO-FIND-COMMAND-NAME FLAG,  */
*/*             ABND806, IS ON THEN SET ABEND-PROCESSING-IN-CONTROL  */
*/*             FLAG, TMPSCTRL                                       */
*/*           - CHECK IF COMMAND-NOT-FOUND FLAG, ABND806, IS ON AND  */
*/*             IF CURRENT COMMAND IS EXEC AND IS RESULT OF IMPLICIT */
*/*             EXEC HANDLING                                        */
*/*             - IF BOTH ARE NOT TRUE, THEN NEW COMMAND IS PENDING  */
*/*               - CHECK IF NEW COMMAND NAME IS TEST                */
*/*                 - IF YES, THEN SET LINK-TO-TEST FLAG, DOSETTB    */
*/*                 - IF NOT TEST, THEN DO --                        */
*/*                   - FREEMAIN PREVIOUS COMMAND BUFFER             */
*/*                   - SET POINTER TO NEW COMMAND ENTRY (NOW IN     */
*/*                     COMMAND-SCAN BUFFER POINTER, CSPLCBUF) INTO  */
*/*                     TEST PARAMETER LIST AREA, TPLCBUF            */
*/*                                                                  */
*/* ENTRY -- LINKED BY REGISTER 14                                   */
*/*                                                                  */
*/* INPUT -- N/A                                                     */
*/*                                                                  */
*/* OUTPUT -- N/A                                                    */
*/*                                                                  */
*/* EXTERNAL REFERENCES -- N/A                                       */
*/*                                                                  */
*/* EXIT NORMAL -- RETURN BY LINK REGISTER 14                        */
*/*                                                                  */
*/*   RETURN CODE -- N/A                                             */
*/*                                                                  */
*/* EXIT ERROR -- N/A                                                */
*/*                                                                  */
*/*   CONDITIONS -- N/A                                              */
*/*                                                                  */
*/*   RETURN CODE -- N/A                                             */
*/*                                                                  */
*/* * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * * */
*TSETCD1:   /* EP TO CHK-FOR-806-CONDITION/TEST/SET-BUFFER ROUTINE   */
*    PROC  OPTIONS(DONTSAVE,NOSAVEAREA);                        01S0712
*                                                               01S0712
TSETCD1  DS    0H                                               01S0713
*    DOSETBF = NO;                  /* RESET DO-IMPLI-EXEC-CHK FLAG  */
         NI    DOSETBF(WRKAPTR1),B'11101111'                    01S0713
*    IF ABND806 = YES               /* CHK FOR CMD-NOT-FND FLAG      */
*      THEN                         /* YES,                          */
         TM    ABND806(WRKAPTR1),B'00000100'                    01S0714
         BNO   @RF00714                                         01S0714
*        TMPSCTRL = YES;            /* INDICATE STAI IN CONTROL      */
         OI    TMPSCTRL(WRKAPTR1),B'00001000'                   01S0715
*    IF ABND806 ^= YES              /* CMD-NOT-FND OR                */
*        & CSOACNM^=ADDR(EXECCMD)   /* IMPLICIT EXEC                 */
*      THEN DO;                     /* NEITHER,                      */
@RF00714 TM    ABND806(WRKAPTR1),B'00000100'                    01S0716
         BO    @RF00716                                         01S0716
         L     @14,CSPLOA(,SRPARMLP)                            01S0716
         L     @06,CSOACNM(,@14)                                01S0716
         LA    @05,EXECCMD                                      01S0716
         CR    @06,@05                                          01S0716
         BE    @RF00716                                         01S0716
*        IF CSOACNM->CMDNAME(1:4)='TEST'  /* CHK FOR TEST            */
*            & CSOALNM=D4           /*   BY NAME AND LENGTH          */
*          THEN                     /* YES, IS TEST                  */
         CLC   CMDNAME(4,@06),@CC02951                          01S0718
         BNE   @RF00718                                         01S0718
         CLC   CSOALNM(2,@14),@CH00045                          01S0718
         BNE   @RF00718                                         01S0718
*            DOSETTB = YES;         /* GO TO LINK TO TEST            */
         OI    DOSETTB(WRKAPTR1),B'00001000'                    01S0719
*          ELSE DO;                 /* NO, IS NOT TEST               */
         B     @RC00718                                         01S0720
@RF00718 DS    0H                                               01S0721
*            R0=SPL1+CBUFLNG;       /* LEN OF BUFFER + SPL1          */
         L     @14,TPLCBUF(,WRKAPTR1)                           01S0721
         L     R0,@CF00093                                      01S0721
         AH    R0,CBUFLNG(,@14)                                 01S0721
*            R1 = TPLCBUF;          /* ADDR OF BUFFER TO BE FREED    */
         LR    R1,@14                                           01S0722
*            GEN(FREEMAIN R,LV=(0),A=(1));                      01S0723
         FREEMAIN R,LV=(0),A=(1)
*            TPLCBUF=CSPLCBUF;      /* SET UP COMMAND BUFFER         */
         L     @14,CSPLCBUF(,SRPARMLP)                          01S0724
         ST    @14,TPLCBUF(,WRKAPTR1)                           01S0724
*          END;                                                 01S0725
*      END;                                                     01S0726
*                                                               01S0726
@RC00718 DS    0H                                               01S0727
*    RESPECIFY (R14) RSTD;                                      01S0727
@RF00716 DS    0H                                               01S0728
*    R14 = ADDR(TSETCDA);           /* SET FOR PSEUDO-RETURN         */
         LA    R14,TSETCDA                                      01S0728
*    RETURN;                        /* RETURN TO CALLER              */
@EL00006 DS    0H                                               01S0729
@EF00006 DS    0H                                               01S0729
@ER00006 BR    @14                                              01S0729
*    RESPECIFY (R14) UNRSTD;                                    01S0730
