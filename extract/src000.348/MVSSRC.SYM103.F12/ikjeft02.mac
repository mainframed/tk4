         LA    R15,TSTACKA                                      01S0571
*        GEN(SYNCH   (15));         /* GO TO INVOKE STACK            */
         SYNCH   (15)
*        SPBFLUSH = NO;             /* SET FLUSH INHIBITABLE FLAG    */
*                                                               01S0573
         L     @14,STPBPTR(,WRKAPTR1)                           01S0573
         NI    SPBFLUSH(@14),B'11110111'                        01S0573
*        GEN(TCLEARQ INPUT);        /* CLEAR REMAINING INPUT         */
*                                                               01S0574
         TCLEARQ INPUT
*        DOGETC = YES;              /* SET TO GET NEXT COMMAND       */
         OI    DOGETC(WRKAPTR1),B'00010000'                     01S0575
