*/* SEGMENT NAME -- TWAITX                                           */
*/*                                                                  */
*/* DESCRIPTIVE NAME -- N/A                                          */
*/*                                                                  */
*/* FUNCTION -- TO WAIT FOR COMPLETION OF THE ATTACHED SUB-COMMAND   */
*/*       TASK OR INTERCEPT THE INTERRUPTIONS OF THAT TASK BY ABEND, */
*/*       ATTENTION, OR SYSTEM STOP-MODIFY                           */
*/*  OPERATION --                                                    */
*/*           - RESET THE PERFORM-WAIT-NOW FLAG, DOWAIT              */
*/*           - IF NO BYPASS-WAIT FLAGS ARE ON, ISSUE WAIT ON THE    */
*/*             TMP WAIT LIST                                        */
*/*             WHEN WAIT IS ENDED, SET THE PSW PROTECT KEY TO ZERO  */
*/*             AND THE TCB PROTECT KEY TO 8                         */
*/*                                                                  */
*/* ENTRY -- LINKED BY REGISTER 14                                   */
*/*                                                                  */
*/* INPUT -- N/A                                                     */
*/*                                                                  */
*/* OUTPUT -- N/A                                                    */
*/*                                                                  */
*/* EXTERNAL REFERENCES -- N/A                                       */
*/*                                                                  */
*/* EXIT NORMAL -- RETURN BY LINK REGISTER 14                        */
*/*                                                                  */
*/*   RETURN CODE -- N/A                                             */
*/*                                                                  */
*/* EXIT ERROR -- N/A                                                */
*/*                                                                  */
*/*   CONDITIONS -- N/A                                              */
*/*                                                                  */
*/*   RETURN CODE -- N/A                                             */
*/*                                                                  */
*/* * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * * */
*TWAITX:    /* EP TO WAIT-FOR-CP-COMPLETION ROUTINE                  */
*    ENTRY;                                                     01S0253
TWAITX   DS    0H                                               01S0254
*    RESPECIFY (R6,R14) RSTD;                                   01S0254
*    R6 = R14;                      /* RESERVE ENTRY REG 14          */
*                                                               01S0255
         LR    R6,R14                                           01S0255
*    DOWAIT = NO;                   /* RESET DO-WAIT-NOW FLAG        */
         NI    DOWAIT(WRKAPTR1),B'11111011'                     01S0256
*    IF DOWATF1 = NO5               /* CHK TO BYPASS WAIT            */
*      THEN DO;                     /* NO, PERFORM WAIT              */
         TM    DOWATF1(WRKAPTR1),B'00111110'                    01S0257
         BNZ   @RF00257                                         01S0257
*        R1 = ADDR(TPLECBL);        /* SET PTR TO WAIT LIST          */
         LA    R1,TPLECBL(,WRKAPTR1)                            01S0259
*        GEN(WAIT ECBLIST=(1),LONG=YES);  /* FOR NEXT ACTION         */
         WAIT ECBLIST=(1),LONG=YES
*      END;                                                     01S0261
*                                                               01S0261
*    R14 = R6;                      /* RESTORE ENTRY REG 14          */
@RF00257 LR    R14,R6                                           01S0262
*    RETURN;                        /* RETURN TO CALLER              */
         B     @EL00003                                         01S0263
*    RESPECIFY (R6,R14) UNRSTD;                                 01S0264
