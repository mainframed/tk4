*/* SEGMENT NAME -- IKJEFT0B                                         */
*/*                                                                  */
*/* DESCRIPTIVE NAME -- N/A                                          */
*/*                                                                  */
*/* FUNCTION -- TO RECEIVE THE ASYNCHRONOUS EXIT FROM THE STAE-RETRY */
*/*       ROUTINE, SET UP MESSAGE IKJ56622I AND THE CONTROLS TO      */
*/*       RE-ESTABLISH TMP PROCESSING -- OR TO SET UP TERMINATION    */
*/*       EXIT (DETACHING ANY EXISTING SUBTASK)                      */
*/*  OPERATION --                                                    */
*/*           - (IKJEFT0B ENTRY) --                                  */
*/*             - IF LINK ACTIVITY IS CURRENT (LINKNOW FLAG) --      */
*/*               - RESET LINK-ACTIVITY-CURRENT FLAG, LINKNOW        */
*/*               - SET UP MESSAGE FOR COMMAND-NOT-FOUND             */
*/*               - SET FLOW FLAG, DOINVOK, TO RE-ENTER PERFORM-     */
*/*                 COMMAND-ENTERED LOOP                             */
*/*               - SET LIN REGISTER TO FORCE PROCESSING             */
*/*             - IF LINK ACTIVITY IS NOT CURRENT --                 */
*/*               (ALSO TDONE1 LABEL, INTERNAL ENTRY POINT)          */
*/*               - SAVE LINK REGISTER                               */
*/*               - RESET TERMINATION FLAG, DODONE                   */
*/*               - IF NO CONTINUE-PROCESSING FLAGS, DOLIST, DOGETC, */
*/*                 DOSCAN, DOCHKCP, OR DOWAIT, ARE ON --            */
*/*                 - IF THE LATEST COMMAND ENTERED HAS NOT YET      */
*/*                   COMPLETED, TPLCTCB NOT ZERO, THEN --           */
*/*                   - SET ABNORMAL-DETACH FLAG, ECTATRM            */
*/*                   - CALL DETACH ROUTINE, TDETCP                  */
*/*                   - SET ECT TERMINATION FLAG, ECTLOGF            */
*/*                   - SET FLAG FOR CHECK-STATUS-OF-SUBTASK, DOLIST */
*/*                                                                  */
*/* ENTRY -- EXTERNAL ENTRY, IKJEFT0B                                */
*/*            ENTERED FROM THE STAE RETRY ROUTINE, IKJEFT07         */
*/*        - INTERNAL ENTRY, TDONE                                   */
*/*            ENTERED FROM IKJEFT0B SEQUENCE, OR LINKED BY REGISTER */
*/*            14 FROM ROUTINE TDONE WHEN DODONE TERMINATION FLAG    */
*/*            HAS BEEN SET                                          */
*/*                                                                  */
*/* INPUT -- N/A                                                     */
*/*                                                                  */
*/* OUTPUT -- N/A                                                    */
*/*                                                                  */
*/* EXTERNAL REFERENCES -- N/A                                       */
*/*                                                                  */
*/* EXIT NORMAL -- RETURN BY LINK REGISTER 14                        */
*/*                                                                  */
*/*   RETURN CODE -- N/A                                             */
*/*                                                                  */
*/* EXIT ERROR -- N/A                                                */
*/*                                                                  */
*/*   CONDITIONS -- N/A                                              */
*/*                                                                  */
*/*   RETURN CODE -- N/A                                             */
*/*                                                                  */
*/* * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * * */
*IKJEFT0B:  /* EP TO EOF-READ/ABEND-RETRY ROUTINE                    */
*                                                               01S0334
*    PROC  OPTIONS(NOSAVE,NOSAVEAREA,NORETREG);                 01S0334
*                                                               01S0334
IKJEFT0B DS    0H                                               01S0335
*    RESPECIFY (R13) RSTD;                                      01S0335
*    SRPARMLP = SRPLPTR;            /* RESTORE PTR TO SRPL AREA      */
*                                                               01S0336
         L     SRPARMLP,SRPLPTR(,WRKAPTR1)                      01S0336
*    IF LINKNOW = YES               /* CHK IF TEST WAS ATTEMPTED     */
*      THEN DO;                     /* YES,                          */
         TM    LINKNOW(WRKAPTR1),B'00100000'                    01S0337
         BNO   @RF00337                                         01S0337
*        LINKNOW = NO;              /*   RESET TEST ATTEMPT FLAG     */
         NI    LINKNOW(WRKAPTR1),B'11011111'                    01S0339
*        MSGSEG(1)=ADDR(MSG56622);  /* SET MESSAGE PTR               */
         L     @14,@CA00131                                     01S0340
         ST    @14,MSGSEG(,WRKAPTR1)                            01S0340
*        DOPUTM = YES;              /* SET TO DO MESSAGE             */
         OI    DOPUTM(WRKAPTR1),B'10000000'                     01S0341
*        DOINVOK = YES;             /* TO RE-ENTER COMMAND LOOP      */
         OI    DOINVOK(WRKAPTR1),B'00000100'                    01S0342
*      END;                                                     01S0343
*      ELSE DO;                     /* LINK WAS NOT ATTEMPTED        */
         B     @RC00337                                         01S0344
@RF00337 DS    0H                                               01S0345
*TDONE1: ENTRY;                                                 01S0345
         B     @EC00345                                         01S0345
TDONE1   DS    0H                                               01S0345
@EC00345 DS    0H                                               01S0346
*        DODONE = NO;               /* RESET TERMINATION FLAG        */
         NI    DODONE(WRKAPTR1),B'11110111'                     01S0346
*        IF DOLIST = NO             /*                               */
*            & DOGETC = NO          /*                               */
*            & DOSCAN = NO          /*                               */
*            & DOCHKCP = NO         /*                               */
*            & DOWAIT = NO          /*                               */
*          THEN                                                 01S0347
         TM    DOLIST(WRKAPTR1),B'00110010'                     01S0347
         BNZ   @RF00347                                         01S0347
         TM    DOCHKCP(WRKAPTR1),B'00000110'                    01S0347
         BNZ   @RF00347                                         01S0347
*            IF TPLCTCB^=ZERO       /* CHK IF ANY CP CURRENT         */
*              THEN DO;             /* YES,                          */
         L     @14,TPLCTCB(,WRKAPTR1)                           01S0348
         LTR   @14,@14                                          01S0348
         BZ    @RF00348                                         01S0348
*                ECTATRM=YES;       /* SIGNIFY ABNORMAL END          */
         L     @14,ECTPTR(,WRKAPTR2)                            01S0350
         OI    ECTATRM(@14),B'00100000'                         01S0350
*                CALL  TDETCP;      /* DETACH ABENDING CP            */
         BAL   @14,TDETCP                                       01S0351
*                ECTLOGF=YES;       /* SET TERMINATION FLAG          */
         L     @14,ECTPTR(,WRKAPTR2)                            01S0352
         OI    ECTLOGF(@14),B'00010000'                         01S0352
*                DOLIST=YES;        /* WAIT FOR END OF CP            */
         OI    DOLIST(WRKAPTR1),B'00100000'                     01S0353
*              END;                                             01S0354
*       IF CALLNOW = YES            /* CHK IF CALL ACTIVE     ZA26167*/
*           & TPLCTCB = ZERO        /* NO CURRENT ATTACH      ZA26167*/
*         THEN DO;                  /* YES,                   ZA26167*/
@RF00348 DS    0H                                               01S0355
@RF00347 TM    CALLNOW(WRKAPTR1),B'00001000'                    01S0355
         BNO   @RF00355                                         01S0355
         L     @14,TPLCTCB(,WRKAPTR1)                           01S0355
         LTR   @14,@14                                          01S0355
         BNZ   @RF00355                                         01S0355
*           CALLNOW = NO;         /* SET CALL-NOT-ACTIVE FLAG ZA26167*/
         NI    CALLNOW(WRKAPTR1),B'11110111'                    01S0357
*           CALL DOCALL2;           /* GO TO TERMINATION      ZA26167*/
         L     @15,@CV00134                                     01S0358
         BALR  @14,@15                                          01S0358
*         END;                      /*                        ZA26167*/
*      END;                                                     01S0360
*                                                               01S0360
@RF00355 DS    0H                                               01S0361
*    RESPECIFY (R14) RSTD;                                      01S0361
@RC00337 DS    0H                                               01S0362
*    R14 = ADDR(TDONE);             /* SET FOR PSEUDO-RETURN         */
         LA    R14,TDONE                                        01S0362
*    RESPECIFY (R14) UNRSTD;                                    01S0363
*    RETURN;                        /* RETURN TO CALLER              */
@EL00004 DS    0H                                               01S0364
@EF00004 DS    0H                                               01S0364
@ER00004 BR    @14                                              01S0364
