*TDETCP:    /* EP TO DETACH-CURRENT-CP-SUBTASK ROUTINE               */
*    PROC  OPTIONS(DONTSAVE,NOSAVEAREA);                        01S0918
TDETCP   DS    0H                                               01S0919
*    RESPECIFY (R1,R2,R14,SRPARMLP,WRKAPTR1,WRKAPTR2) RSTD;     01S0919
*    DO; /*MODESET  KEYADDR(SPROK1) WORKREG(R2)*/               01S0920
*      R2 = SPROK1; /*GET KEY FORM SAVE LOCATION*/              01S0921
         L     R2,SPROK1                                        01S0921
*      SPKA (R2); /*SET PSW KEY*/                               01S0922
         SPKA  0(R2)                                            01S0922
*    END; /*MODESET  KEYADDR(SPROK1) WORKREG(R2)*/              01S0923
*    SAVLNKG = R14;                 /* RESERVE LINK ADDRESS          */
         ST    R14,SAVLNKG(,WRKAPTR2)                           01S0924
*    DO; /*MODESET  KEYADDR(TEPKEY) WORKREG(R2)*/               01S0925
*      R2 = TEPKEY; /*GET KEY FORM SAVE LOCATION*/              01S0926
         L     R2,TEPKEY(,WRKAPTR2)                             01S0926
*      SPKA (R2); /*SET PSW KEY*/                               01S0927
         SPKA  0(R2)                                            01S0927
*    END; /*MODESET  KEYADDR(TEPKEY) WORKREG(R2)*/              01S0928
*    RESPECIFY (R2,R14) UNRSTD;                                 01S0929
*                                                               01S0929
*    IF TMPSCTRL = YES              /* CHK IF STAI IS IN CONTROL     */
*      THEN DO;                     /* YES,                          */
         TM    TMPSCTRL(WRKAPTR1),B'00001000'                   01S0930
         BNO   @RF00930                                         01S0930
*        R1 = TPLNECB;              /* PTR TO POSTEE                 */
         L     R1,TPLNECB(,WRKAPTR1)                            01S0932
*        GEN(POST   (1));           /* POST ABENDEE                  */
         POST   (1)
*        GEN(STATUS   START);       /* START SUBTASK(S)              */
         STATUS   START
*      END;                                                     01S0935
*                                                               01S0935
*    ATCHNOW = NO;                  /* RESET ATTACH-ACTIVE FLAG      */
@RF00930 NI    ATCHNOW(WRKAPTR1),B'01111111'                    01S0936
*    DYNASP = MDYNASP;              /* INIITIALIZE PARM LIST         */
         MVC   DYNASP(32,WRKAPTR1),MDYNASP(WRKAPTR2)            01S0937
*    R1 = ADDR(DYNASP);             /* SET PTR TO PARM LIST          */
*                                                               01S0938
         LA    R1,DYNASP(,WRKAPTR1)                             01S0938
*    /*MACDATE Y-2 73082*/                                      01S0939
*    SVC (99);                      /* INVOKE DYNAMIC ALLOCATION     */
         SVC   99                                               01S0939
*    RCODE = R15;                   /* SAVE THE RETURN CODE          */
*                                                               01S0940
         ST    R15,RCODE(,WRKAPTR1)                             01S0940
*    IF RCODE ^= ZERO               /* CHK IF DYNALLOC SUCCESSFUL    */
*      THEN                         /* NO,                           */
         L     @14,RCODE(,WRKAPTR1)                             01S0941
         LTR   @14,@14                                          01S0941
         BZ    @RF00941                                         01S0941
*        GEN(ABEND 203,DUMP);       /*   INDICATE DYNALLOC ERROR     */
*                                                               01S0942
         ABEND 203,DUMP
*    IF ECTATRM = YES               /* CHK FOR COMMAND PROCESSOR     */
*         TPLCTCB->TCBFT=YES       /*     ABEND                     */
*      THEN                         /* YES,                          */
@RF00941 L     @14,ECTPTR(,WRKAPTR2)                            01S0943
         TM    ECTATRM(@14),B'00100000'                         01S0943
         BO    @RT00943                                         01S0943
         L     @14,TPLCTCB(,WRKAPTR1)                           01S0943
         TM    TCBFT(@14),B'00000100'                           01S0943
         BNO   @RF00943                                         01S0943
@RT00943 DS    0H                                               01S0944
*        ECTRCDF = '80'X;           /*   INDICATE CP ABEND           */
         L     @14,ECTPTR(,WRKAPTR2)                            01S0944
         MVI   ECTRCDF(@14),X'80'                               01S0944
*      ELSE                         /* NO, NOT SUBTASK ABEND         */
*        ECTRCDF = '00'X;           /* SET NORMAL CP RETURN FLAG     */
*                                                               01S0945
         B     @RC00943                                         01S0945
@RF00943 L     @14,ECTPTR(,WRKAPTR2)                            01S0945
         MVI   ECTRCDF(@14),X'00'                               01S0945
*    R1 = ADDR(TPLCTCB);            /* SET PTR TO ATTACHED CP TCB    */
@RC00943 LA    R1,TPLCTCB(,WRKAPTR1)                            01S0946
*    IF TMPACTRL = YES              /* CHK IF ATTENTION IN CONTROL   */
*      THEN DO;                     /* YES, GIVE CONTROL TO CP STAE  */
         TM    TMPACTRL(WRKAPTR1),B'00010000'                   01S0947
         BNO   @RF00947                                         01S0947
*        GEN(DETACH (1),STAE=YES);  /* DETACH WITH STAE              */
         DETACH (1),STAE=YES
*        ECTRCDF = '80'X;           /* INDICATE ABEND CODE PRESENT   */
         L     @14,ECTPTR(,WRKAPTR2)                            01S0950
         MVI   ECTRCDF(@14),X'80'                               01S0950
*      END;                                                     01S0951
*      ELSE                         /* NO, ATTENTION NOT IN CONTROL  */
*        GEN(DETACH (1),STAE=NO);   /* DETACH WITHOUT STAE           */
*                                                               01S0952
         B     @RC00947                                         01S0952
@RF00947 DS    0H                                               01S0952
         DETACH (1),STAE=NO
*    CALL TDETCP2;                  /* TO RESET CONTROL FLGS  ZA26167*/
@RC00947 BAL   @14,TDETCP2                                      01S0953
*    CALL  IKJEFTA2 (TMPWRKA1,TMPWRKA2);  /* TO AUTH-CHKR   @Z403333 */
*                                                               01S0954
         ST    WRKAPTR1,@AL00001                                01S0954
         ST    WRKAPTR2,@AL00001+4                              01S0954
         L     @15,@CV00137                                     01S0954
         LA    @01,@AL00001                                     01S0954
         BALR  @14,@15                                          01S0954
*    RESPECIFY (R14) RSTD;                                      01S0955
*    R14 = SAVLNKG;                 /* RESTORE LINK REGISTER         */
         L     R14,SAVLNKG(,WRKAPTR2)                           01S0956
*    RESPECIFY (R14) UNRSTD;                                    01S0957
*    RETURN;                        /* RETURN TO CALLER              */
@EL00010 DS    0H                                               01S0958
@EF00010 DS    0H                                               01S0958
@ER00010 BR    @14                                              01S0958
