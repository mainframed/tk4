*/* * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * * */
*/*                                                                  */
*/* SEGMENT NAME -- TINIT2                                           */
*/*                                                                  */
*/* DESCRIPTIVE NAME -- N/A                                          */
*/*                                                                  */
*/* FUNCTION -- TO ESTABLISH AND INITIALIZE POINTERS AND CONTROL     */
*/*       AREAS USED BY THE TMP                                      */
*/*  OPERATION --                                                    */
*/*           - SET POINTERS TO TMP WORK AREAS                       */
*/*           - SET PSW PROTECT KEY 0                                */
*/*           - SET POINTER TO SERVICE ROUTINE PARAMETER LIST        */
*/*           - SAVE TMP-MAINLINE CODE ADDRESSABILITY                */
*/*           - SET FLAG TO FORCE SYNTAX CHECK OF FIRST COMMAND NAME */
*/*                                                                  */
*/* ENTRY -- LINKED BY REGISTER 14                                   */
*/*                                                                  */
*/* INPUT -- N/A                                                     */
*/*                                                                  */
*/* OUTPUT -- INITIALIZED ENVIRONMENT FOR TMP                        */
*/*                                                                  */
*/* EXTERNAL REFERENCES -- N/A                                       */
*/*                                                                  */
*/* EXIT NORMAL -- RETURN BY LINK REGISTER 14                        */
*/*                                                                  */
*/*   RETURN CODE -- N/A                                             */
*/*                                                                  */
*/* EXIT ERROR -- N/A                                                */
*/*                                                                  */
*/*   CONDITIONS -- N/A                                              */
*/*                                                                  */
*/*   RETURN CODE -- N/A                                             */
*/*                                                                  */
*/* * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * * */
*                                                               01S0139
*                                                               01S0139
*TINIT2:    /* EP TO INITIALIZE-FOR-MAINLINE-PROCESSING ROUTINE      */
*                                                               01S0139
*    PROC  OPTIONS(NOSAVE,NOSAVEAREA);                          01S0139
*                                                               01S0139
TINIT2   DS    0H                                               01S0140
*    RESPECIFY (R14) RSTD;                                      01S0140
*    WRKAPTR2 = R1;                 /* SET PTR TO TMP SUP WA         */
         LR    WRKAPTR2,R1                                      01S0141
*    WRKAPTR1 = WRKA1PTR;           /* SET PTR TO TMP PP WA          */
*                                                               01S0142
         L     WRKAPTR1,WRKA1PTR(,WRKAPTR2)                     01S0142
*    SRPARMLP = SRPLPTR;            /* ESTABLISH PTR TO SRPL AREA    */
*                                                               01S0143
         L     SRPARMLP,SRPLPTR(,WRKAPTR1)                      01S0143
*    RESPECIFY (R2,R6,R10,R11,R12) RSTD;                        01S0144
*    R6 = R14;                      /* RESERVE ENTRY REG 14          */
         LR    R6,R14                                           01S0145
*    /* SET SCHEDULER PSW KEY                                        */
*    DO; /*MODESET  KEYADDR(SPROK1) WORKREG(R2)*/               01S0146
*      R2 = SPROK1; /*GET KEY FORM SAVE LOCATION*/              01S0147
         L     R2,SPROK1                                        01S0147
*      SPKA (R2); /*SET PSW KEY*/                               01S0148
         SPKA  0(R2)                                            01S0148
*    END; /*MODESET  KEYADDR(SPROK1) WORKREG(R2)*/              01S0149
*    SAVLNKA = R6;                  /* RESERVE ENTRY REG 14          */
         ST    R6,SAVLNKA(,WRKAPTR2)                            01S0150
*    STM(R10,R12,RTRYSA);           /* RESERVE ADDRESSABILITY        */
         STM   R10,R12,RTRYSA(WRKAPTR2)                         01S0151
*    /* SET PSW PROTECT KEY 0                                        */
*    DO; /*MODESET  KEYADDR(SPROK0) WORKREG(R2)*/               01S0152
*      R2 = SPROK0; /*GET KEY FORM SAVE LOCATION*/              01S0153
         L     R2,SPROK0                                        01S0153
*      SPKA (R2); /*SET PSW KEY*/                               01S0154
         SPKA  0(R2)                                            01S0154
*    END; /*MODESET  KEYADDR(SPROK0) WORKREG(R2)*/              01S0155
*    JSCBAUTH = NO;                 /* RESET APF AUTHORIZATION FLAG  */
         L     @03,CVTPTR                                       01S0156
         L     @03,CVTTCBP(,@03)                                01S0156
         L     @03,TCURTCB(,@03)                                01S0156
         L     @03,TCBJSCB(,@03)                                01S0156
         NI    JSCBAUTH(@03),B'11111110'                        01S0156
*    /* RESTORE PP PSW KEY                                           */
*    DO; /*MODESET  KEYADDR(TEPKEY) WORKREG(R2)*/               01S0157
*      R2 = TEPKEY; /*GET KEY FORM SAVE LOCATION*/              01S0158
         L     R2,TEPKEY(,WRKAPTR2)                             01S0158
*      SPKA (R2); /*SET PSW KEY*/                               01S0159
         SPKA  0(R2)                                            01S0159
*    END; /*MODESET  KEYADDR(TEPKEY) WORKREG(R2)*/              01S0160
*                                                               01S0160
*    DOSCAN = YES;                  /* TO FORCE FIRST PASS           */
*                                                               01S0161
         OI    DOSCAN(WRKAPTR1),B'00000010'                     01S0161
*    R14 = SAVLNKA;                 /* RESTORE ENTRY REG 14          */
         L     @03,SAVLNKA(,WRKAPTR2)                           01S0162
         LR    R14,@03                                          01S0162
*    RETURN;                        /* RETURN TO CALLER              */
@EL00003 DS    0H                                               01S0163
@EF00003 DS    0H                                               01S0163
@ER00003 BR    @14                                              01S0163
*    RESPECIFY (R2,R6,R10,R11,R12,R14) UNRSTD;                  01S0164
*/* * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * * */
*/*                                                                  */
*/* SEGMENT NAME -- TIDNTC                                           */
*/*                                                                  */
*/* DESCRIPTIVE NAME -- N/A                                          */
*/*                                                                  */
*/* FUNCTION -- TO SYNTAX-CHECK COMMAND NAME AND ACTIVATE SPECIAL    */
*/*       PROCESSING IF REQUIRED                                     */
*/*  OPERATION --                                                    */
*/*           - RESET PERFORM-COMMAND-SCAN FLAG                      */
*/*           - IF TERMINATION FLAG, DODONE, IS NOT ON --            */
*/*             - SET ACTIVATE-NEW=COMMAND FLAG TO FORCE FIRST PASS  */
*/*             - SET UP COMMAND SCAN INTERFACE AND SYNCH TO ROUTINE */
*/*               TO INVOKE COMMAND SCAN                             */
*/*             - IF COMMAND SCAN RETURNS UNSATISFACTORY CODE,       */
*/*               IDENTIFY FAILURE WITH USER ABEND 206               */
*/*             - IF LINE CONTAINED NON-VALID SYNTAX CALL ROUTINE TO */
*/*               PROCESS INVALID LINE, TIDNTC1                      */
*/*             - IF LINE WAS VALID,                                 */
*/*               - IF BACKGROUND MODE IS CURRENT, CALL ROUTINE TO   */
*/*                 CHECK IF COMMAND IS SUPPORTED, TNSCHK            */
*/*               - IF TERMINATION IS NOT BEING FORCED (DODONE FLAG),*/
*/*                 CHECK IF COMMAND IS TIME AND IF SO, CALL ROUTINE */
*/*                 TO INVOKE TIME PROCESSOR, TIDNTC2.  IF NOT TIME  */
*/*                 COMMAND,   CHECK  IF  COMMAND SCAN  RETURNED THE */
*/*                 EXPLICIT-IMPLICIT-EXEC FLAG AND IF SO SET TMP    */
*/*                 FLAG, DOIMPLX                                    */
*/*                                                                  */
*/* ENTRY -- LINKED BY REGISTER 14                                   */
*/*                                                                  */
*/* INPUT -- N/A                                                     */
*/*                                                                  */
*/* OUTPUT -- N/A                                                    */
*/*                                                                  */
*/* EXTERNAL REFERENCES -- N/A                                       */
*/*                                                                  */
*/* EXIT NORMAL -- RETURN BY LINK REGISTER                           */
*/*                                                                  */
*/*   RETURN CODE -- N/A                                             */
*/*                                                                  */
*/* EXIT ERROR -- N/A                                                */
*/*                                                                  */
*/*   CONDITIONS -- N/A                                              */
*/*                                                                  */
*/*   RETURN CODE -- N/A                                             */
*/*                                                                  */
*/* * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * * */
*TIDNTC:    /* EP TO COMMAND-VALIDATION ROUTINE                      */
*    ENTRY;                                                     01S0165
TIDNTC   DS    0H                                               01S0166
*    DOSCAN = NO;                   /* RESET DO SCAN NOW FLAG        */
         NI    DOSCAN(WRKAPTR1),B'11111101'                     01S0166
*    IF DODONE = NO                 /* CHK FOR TERMINATION FLAG      */
*      THEN DO;                     /* NO, CONTINUE                  */
         TM    DODONE(WRKAPTR1),B'00001000'                     01S0167
         BNZ   @RF00167                                         01S0167
*        DOINVOK = YES;             /* SET TO FORCE FIRST PASS       */
         OI    DOINVOK(WRKAPTR1),B'00000100'                    01S0169
*        CSPLOA = SCANAP;           /* SET UP BUFFER FOR SCAN        */
         L     @14,SCANAP(,WRKAPTR1)                            01S0170
         ST    @14,CSPLOA(,SRPARMLP)                            01S0170
*        CSPLFLG=ADDR(SCANFLG);     /* GET FLAG AREA POINTER         */
         LA    @14,SCANFLG(,WRKAPTR1)                           01S0171
         ST    @14,CSPLFLG(,SRPARMLP)                           01S0171
*        R15=ADDR(TDOSCAN);         /* PTR TO COMMAND SCAN EXIT RTN  */
         LA    R15,TDOSCAN                                      01S0172
*        GEN FLOWS(TDOSCAN) (SYNCH  (15));  /* CALL CMD-SCAN         */
         SYNCH  (15)
*        IF RCODE^=SCNDONE          /* CHECK FOR SUCCESSFUL SCAN     */
*          THEN                     /* SCAN ERROR DETECTED           */
         L     @14,RCODE(,WRKAPTR1)                             01S0174
         LTR   @14,@14                                          01S0174
         BZ    @RF00174                                         01S0174
*            GEN(ABEND 206,DUMP);   /* INDICATE SCAN ERROR           */
         ABEND 206,DUMP
*        IF CSOACNM = ZERO          /* CHK IF SYNTAX VALID           */
*          THEN DO;                 /* NO,                           */
@RF00174 L     @14,CSPLOA(,SRPARMLP)                            01S0176
         L     @14,CSOACNM(,@14)                                01S0176
         LTR   @14,@14                                          01S0176
         BNZ   @RF00176                                         01S0176
*            CALL  TIDNTC1;         /* TO PROCESS INVALID LINE       */
         BAL   @14,TIDNTC1                                      01S0178
*TIDNTCA:  END;                     /* FOR PSEUDO-RETURN             */
*          ELSE DO;                 /* YES, IS VALID LINE            */
         B     @RC00176                                         01S0180
@RF00176 DS    0H                                               01S0181
*            EXITRCDE = ZERO;       /* CLEAR PCF CHECKER RC @Z403333 */
         SLR   @14,@14                                          01S0181
         ST    @14,EXITRCDE                                     01S0181
*            CALL  IKJEFTB2 (TMPWRKA1,TMPWRKA2);  /* TO PCF @Z403333 */
         ST    WRKAPTR1,@AL00001                                01S0182
         ST    WRKAPTR2,@AL00001+4                              01S0182
         L     @15,@CV00136                                     01S0182
         LA    @01,@AL00001                                     01S0182
         BALR  @14,@15                                          01S0182
*            EXITRCDE = R15;        /* RESERVE RETURN CODE  @Z403333 */
         ST    R15,EXITRCDE                                     01S0183
*            IF EXITRCDE > D4       /* CHK FOR PCF COMMAND  @Z403333 */
*              THEN                 /* NO,                  @Z403333 */
         L     @14,EXITRCDE                                     01S0184
         C     @14,@CF00045                                     01S0184
         BNH   @RF00184                                         01S0184
*                CSOAEXEC = YES;    /*   SET TO DO EXEC     @Z403333 */
         L     @14,CSPLOA(,SRPARMLP)                            01S0185
         OI    CSOAEXEC(@14),B'00000100'                        01S0185
*            IF BKGMODE = BKGDNOW   /* CHK FOR BACKGROUND MODE       */
*              THEN DO;             /* YES,                          */
@RF00184 TM    BKGMODE(WRKAPTR2),B'10000000'                    01S0186
         BNO   @RF00186                                         01S0186
*                CALL  TNSCHK;      /* CHK IF UNSUPPORTED CP NAME    */
         BAL   @14,TNSCHK                                       01S0188
*TIDNTCB:      END;                 /* FOR PSEUDO-RETURN             */
TIDNTCB  DS    0H                                               01S0190
*            IF DODONE = NO         /* CHK FOR TERMINATION           */
*              THEN                 /* NO,                           */
@RF00186 TM    DODONE(WRKAPTR1),B'00001000'                     01S0190
         BNZ   @RF00190                                         01S0190
*                IF CSOACNM->CMDNAME(1:4)=NMTIME /* CHK TIME         */
*                    & CSOALNM=D4   /*  AND TIME NAME LENGTH         */
*                  THEN DO;         /* YES, IS TIME                  */
         L     @14,CSPLOA(,SRPARMLP)                            01S0191
         L     @06,CSOACNM(,@14)                                01S0191
         CLC   CMDNAME(4,@06),@CC00078                          01S0191
         BNE   @RF00191                                         01S0191
         CLC   CSOALNM(2,@14),@CH00045                          01S0191
         BNE   @RF00191                                         01S0191
*                    CALL  TIDNTC2;    /* CALL TIME PROCESSOR        */
         BAL   @14,TIDNTC2                                      01S0193
*TIDNTCC:          END;             /* FOR PSEUDO-RETURN             */
*                  ELSE             /* IS NOT TIME NAME              */
*                    IF CSOAEXEC=YES   /* IF EXPL/IMPLICIT EXEC      */
*                      THEN         /* YES,                          */
         B     @RC00191                                         01S0195
@RF00191 L     @14,CSPLOA(,SRPARMLP)                            01S0195
         TM    CSOAEXEC(@14),B'00000100'                        01S0195
         BNO   @RF00195                                         01S0195
*                        DOIMPLX=YES;  /* SET UP EXEC BUFFER         */
         OI    DOIMPLX(WRKAPTR1),B'01000000'                    01S0196
*          END;                                                 01S0197
@RF00195 DS    0H                                               01S0197
@RC00191 DS    0H                                               01S0197
@RF00190 DS    0H                                               01S0198
*      END;                                                     01S0198
@RC00176 DS    0H                                               01S0199
*    RESPECIFY (R14) RSTD;                                      01S0199
@RF00167 DS    0H                                               01S0200
*    R14 = ADDR(TINVOKE);           /* SET FOR PSEUDO-RETURN         */
         LA    R14,TINVOKE                                      01S0200
*    RETURN;                        /* RETURN TO CALLER              */
         B     @EL00003                                         01S0201
*    RESPECIFY (R14) UNRSTD;                                    01S0202
