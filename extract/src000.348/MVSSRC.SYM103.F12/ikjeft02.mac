         TITLE 'IKJEFT02 -- TMP MAINLINE CONTROL PROGRAM               *
                        '
IKJEFT02 CSECT ,                                                01S0001
@MAINENT DS    0H                                               01S0001
         USING *,@15                                            01S0001
         B     @PROLOG                                          01S0001
         DC    AL1(16)                                          01S0001
         DC    C'IKJEFT02  78.177'                              01S0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                  01S0001
         BALR  @10,0                                            01S0001
@PSTART  LA    @11,4095(,@10)                                   01S0001
         USING @PSTART,@10                                      01S0001
         USING @PSTART+4095,@11                                 01S0001
         L     @00,@SIZDATD                                     01S0001
         GETMAIN  R,LV=(0)
         LR    @12,@01                                          01S0001
         USING @DATD,@12                                        01S0001
         ST    @13,@SA00001+4                                   01S0001
         LM    @00,@01,20(@13)                                  01S0001
         ST    @12,8(,@13)                                      01S0001
         LR    @13,@12                                          01S0001
         ENTRY  IKJEFT0B
JSCBDISP EQU 188
JSCBAOS1 EQU  72
JSCBAOS2 EQU 192
LWASWAR  EQU   4
LWAMSPE  EQU   16
INITDONE EQU  4
