*/* DESCRIPTIVE NAME - BLANK SCAN ROUTINE                            */
*/*                                                                  */
*/* FUNCTION - THIS ROUTINE IS INVOKED WHEN IT IS NECESSARY TO RETURN*/
*/*            THE DDNAME, DSNAME OR MEMBERNAME AND ITS LENGTH.      */
*/*                                                                  */
*/* OPERATION - TO DETERMINE THE LENGTH OF A NAME, THE FIELD IS      */
*/*             SEARCHED FOR A BLANK, COUNTING THE NON-BLANK         */
*/*             CHARACTERS UNTIL A BLANK IS FOUND OR THE MAXIMUM     */
*/*             LENGTH OF THE FIELD HAS BEEN SCANNED. THE NAME AND   */
*/*             LENGTH ARE STORED IN THE APPROPRIATE TEXT UNIT.      */
*/*                                                                  */
*/* ENTRY - BLNKSCAN (INTERNAL PROCEDURE)                            */
*/*                                                                  */
*/* PURPOSE - SEE FUNCTION                                           */
*/*                                                                  */
*/* INPUT DATA - LIMIT - THE MAXIMUM SIZE OF FIELD TO BE SCANNED     */
*/*              CHARPTR - POINTER TO FIELD TO BE SCANNED            */
*/*              TEXTPTR - POINTER TO TEXT UNIT TO STORE INTO        */
*/*                                                                  */
*/* EXITS - ALL ARE TO NEXT SEQUENTIAL INSTRUCTION                   */
*/*                                                                  */
*/*  NORMAL - RETURN VIA REG 14                                      */
*/*                                                                  */
*/*  ERROR - NONE                                                    */
*/*                                                                  */
*/* OUTPUT DATA - NAME AND LENGTH STORED INTO TEXT UNIT              */
*/*                                                                  */
*/********************************************************************/
*BLNKSCAN: PROC;                                                02S0246
BLNKSCAN STM   @14,@12,12(@13)                                  02S0246
*CHARCNT = ZERO;                       /* INIT CHAR COUNT TO 0       */
         SLR   CHARCNT,CHARCNT                                  02S0247
*DO WHILE (CHARCNT < LIMIT & DATA ^= ' '); /* SCAN FIELD FOR A BLANK */
         B     @DE00248                                         02S0248
@DL00248 DS    0H                                               02S0249
*  CHARCNT = CHARCNT + 1;              /* INCREMENT CHARACTER COUNT  */
         LA    @10,1                                            02S0249
         ALR   CHARCNT,@10                                      02S0249
*  CHARPTR = CHARPTR + 1;              /* INCREMENT PTR TO NEXT CHAR */
         ALR   CHARPTR,@10                                      02S0250
*END;                                  /* END CHARACTER SCAN         */
@DE00248 CR    CHARCNT,LIMIT                                    02S0251
         BNL   @DC00248                                         02S0251
         CLI   DATA(CHARPTR),C' '                               02S0251
         BNE   @DL00248                                         02S0251
@DC00248 DS    0H                                               02S0252
*IF CHARCNT > 0 THEN                   /* WAS NON-BLANK FOUND        */
         LTR   CHARCNT,CHARCNT                                  02S0252
         BNP   @RF00252                                         02S0252
*  TEXTUPAR(1:CHARCNT) = DATFIELD(1:CHARCNT);/*MOVE DATA TO TEXT UNIT*/
         LR    @10,CHARCNT                                      02S0253
         BCTR  @10,0                                            02S0253
         LR    @05,CHARPTR                                      02S0253
         SLR   @05,CHARCNT                                      02S0253
         EX    @10,@SM01727                                     02S0253
*ELSE;                                 /* NOTHING TO MOVE            */
@RF00252 DS    0H                                               02S0255
*TEXTULNG = CHARCNT;                   /* STORE LENGTH EVEN IF ZERO  */
         STCM  CHARCNT,3,TEXTULNG(TEXTPTR)                      02S0255
*END BLNKSCAN;                                                  02S0256
@EL00002 DS    0H                                               02S0256
@EF00002 DS    0H                                               02S0256
@ER00002 LM    @14,@12,12(@13)                                  02S0256
         BR    @14                                              02S0256
*END IEFDB470                                                   01S0257
