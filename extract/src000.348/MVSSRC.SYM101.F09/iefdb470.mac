         TITLE 'IEFDB470 - DYNAMIC INFORMATION RETRIEVAL               *
                        '
IEFDB470 CSECT ,                                                01S0001
@MAINENT DS    0H                                               01S0001
         USING *,@15                                            01S0001
         B     @PROLOG                                          01S0001
         DC    AL1(16)                                          01S0001
         DC    C'IEFDB470  78.089'                              01S0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                  01S0001
         BALR  @12,0                                            01S0001
@PSTART  DS    0H                                               01S0001
         USING @PSTART,@12                                      01S0001
         L     @00,@SIZDATD                                     01S0001
         GSPACE   R,LV=(0)
         LR    @11,@01                                          01S0001
         USING @DATD,@11                                        01S0001
         ST    @13,@SA00001+4                                   01S0001
         LM    @00,@01,20(@13)                                  01S0001
         ST    @11,8(,@13)                                      01S0001
         LR    @13,@11                                          01S0001
         MVC   @PC00001(24),0(@01)                              01S0001
JSCBDISP EQU 188
JSCBAOS1 EQU  72
JSCBAOS2 EQU 192
*/********************************************************************/
*/*                                                                  */
*/* SEGMENT NAME - DINSYNCK                                          */
*/*                                                                  */
*/* DESCRIPTIVE NAME - DYNAMIC INFORMATION RETRIEVAL SYNTAX CHECK    */
*/*                                                                  */
*/* FUNCTION - THIS ROUTINE DETERMINES IF THE TEXT ENTERED IS VALID  */
*/*                                                                  */
*/* OPERATION - IEFBD4FF IS INVOKED TO CHECK TEXT UNIT SYNTAX        */
*/*                                                                  */
*/* ENTRY - DINSYNCK (INCLUDED SEGMENT)                              */
*/*                                                                  */
*/* PURPOSE - SEE FUNCTION                                           */
*/*                                                                  */
*/* INPUT DATA - TEXT UNITS                                          */
*/*                                                                  */
*/* EXITS - ALL ARE TO NEXT SEQUENTIAL INSTRUCTION                   */
*/*                                                                  */
*/*  NORMAL - SYNTAX IS GOOD AND ERRCODE IS SET TO ZERO              */
*/*                                                                  */
*/*  ERROR  - ERRCODE IS ONE OF THE FOLLOWING                        */
*/*           1. RCINVKEY - INVALID KEY SPECIFIED IN TEXT            */
*/*           2. RCINNUMB - INVALID NUMBER OF KEY PARAMETERS         */
*/*           3. RCDUPKEY - DUPLICATE KEYS SPECIFIED IN TEXT         */
*/*           4. RCINKEYL - INVALID KEY PARAMETER LENGTH             */
*/*           5. RCKEYREQ - REQUIRED KEY NOT SPECIFIED               */
*/*                                                                  */
*/* OUTPUT DATA - KEY TABLE FILLED IN WITH TEXT UNIT ADDRESSES       */
*/*                                                                  */
*/********************************************************************/
*KEYTABLE = KEYTABLE && KEYTABLE;      /* ZERO KEYTABLE              */
         XC    KEYTABLE(56),KEYTABLE                            02S0073
*MASKS = MASKS && MASKS;               /* ZERO MUTUAL & DUPE MASKS   */
         XC    MASKS(4),MASKS                                   02S0074
*CALL IEFDB4FF                         /* CALL SYNTAX CHECKER        */
*             (DINRTVTP,ADDR(KEYDICT),ADDR(DUPEMASK),ADDR(MUTEMASK),
*              ADDR(KEYTABLE),DINRTRN);                         02S0075
         L     @10,@PC00001                                     02S0075
         ST    @10,@AL00001                                     02S0075
         LA    @10,KEYDICT                                      02S0075
         ST    @10,@TF00001                                     02S0075
         LA    @10,@TF00001                                     02S0075
         ST    @10,@AL00001+4                                   02S0075
         LA    @10,DUPEMASK                                     02S0075
         ST    @10,@TF00002                                     02S0075
         LA    @10,@TF00002                                     02S0075
         ST    @10,@AL00001+8                                   02S0075
         LA    @10,MUTEMASK                                     02S0075
         ST    @10,@TF00003                                     02S0075
         LA    @10,@TF00003                                     02S0075
         ST    @10,@AL00001+12                                  02S0075
         LA    @10,KEYTABLE                                     02S0075
         ST    @10,@TF00004                                     02S0075
         LA    @10,@TF00004                                     02S0075
         ST    @10,@AL00001+16                                  02S0075
         L     @10,@PC00001+4                                   02S0075
         ST    @10,@AL00001+20                                  02S0075
         L     @15,@CV00126                                     02S0075
         LA    @01,@AL00001                                     02S0075
         BALR  @14,@15                                          02S0075
*IF ERRCODE = ZERO THEN                /* WAS SYNTAX OK ?            */
         SLR   @10,@10                                          02S0076
         L     @15,@PC00001+4                                   02S0076
         SLR   @14,@14                                          02S0076
         ICM   @14,3,ERRCODE(@15)                               02S0076
         CR    @10,@14                                          02S0076
         BNE   @RF00076                                         02S0076
*  DO;                                 /* YES                        */
*    IF DDNAMTUP = ZERO & DSNAMTUP = ZERO & RELNOTUP = ZERO THEN
         C     @10,DDNAMTUP                                     02S0078
         BNE   @RF00078                                         02S0078
         C     @10,DSNAMTUP                                     02S0078
         BNE   @RF00078                                         02S0078
         C     @10,RELNOTUP                                     02S0078
         BNE   @RF00078                                         02S0078
*      ERRCODE = RCKEYREQ;             /* SET ERRCODE IF REQUIRED KEY
*                                         NOT SPECIFIED              */
         MVC   ERRCODE(2,@15),@CB01331                          02S0079
*  END;                                /* END TEST FOR REQUIRED KEY  */
@RF00078 DS    0H                                               02S0081
*IF ERRCODE = ZERO THEN                /* IS ERRCODE ZERO ?          */
@RF00076 SLR   @10,@10                                          01S0081
         L     @15,@PC00001+4                                   01S0081
         SLR   @14,@14                                          01S0081
         ICM   @14,3,ERRCODE(@15)                               01S0081
         CR    @10,@14                                          01S0081
         BNE   @RF00081                                         01S0081
*  DO;                                 /* YES, REQUIRED KEYS ARE
*                                         PRESENT AND SYNTAX IS GOOD */
*    DSABPTR = DSQFRSTP;               /* GET FIRST DSAB ADDRESS     */
         L     @15,@PC00001+8                                   01S0083
         L     @01,DINJSCBP(,@15)                               01S0083
         L     @01,JSCDSABQ(,@01)                               01S0083
         L     @15,DSQFRSTP(,@01)                               01S0083
         ST    @15,DSABPTR                                      01S0083
*/********************************************************************/
*/*                                                                  */
*/* SEGMENT NAME - DINRTSRC                                          */
*/*                                                                  */
*/* DESCRIPTIVE NAME - DYNAMIC INFORMATION RETRIEVAL SEARCH ROUTINE  */
*/*                                                                  */
*/* FUNCTION - THIS ROUTINE SEARCHES THE DSAB CHAIN BY DDNAME, DSNAME*/
*/*            OR BY RELATIVE NUMBER, DEPENDING ON THE TEXT UNIT     */
*/*            PRESENT, TO FIND A DSAB THAT CORRESPONDS TO           */
*/*            THE REQUEST.                                          */
*/*                                                                  */
*/* OPERATION - THIS ROUTINE IS ENTERED ONLY IF THE SYNTAX IS VALID  */
*/*            AND THERE ARE NO MUTUALLY EXCLUSIVE KEYS PRESENT.     */
*/*            THEREFORE, THE SEARCH WILL BE DONE IN ONE OF THREE    */
*/*            WAYS, DEPENDING ON THE TEXT UNIT POINTER PRESENT      */
*/*            IN THE KEY TABLE.                                     */
*/*                                                                  */
*/*            1. IF THE DDNAME TEXT UNIT PTR IS NON-ZERO, THEN      */
*/*               IEFDB4FC IS INVOKED TO SEARCH FOR THE SPECIFIED    */
*/*               DDNAME.                                            */
*/*            2. IF THE DSNAME TEXT UNIT PTR IS NON-ZERO, THEN      */
*/*               IEFDB4FA IS INVOKED TO SEARCH THE SIOTS            */
*/*               AND JFCB'S FOR THE SPECIFIED DSNAME.               */
*/*            3. THE DSAB CHAIN IS SEARCHED, STARTING FROM THE      */
*/*               FIRST DSAB, UNTIL THE RELATIVE ENTRY IS FOUND OR   */
*/*               THE END OF THE DSAB CHAIN IS REACHED.              */
*/*                                                                  */
*/* ENTRY - DINRTSRC(INCLUDED SEGMENT)                               */
*/*                                                                  */
*/* PURPOSE - SEE FUNCTION                                           */
*/*                                                                  */
*/* INPUT DATA - DSABPTR,JSCBPTR,TEXT PTRS                           */
*/*                                                                  */
*/* EXITS - ALL ARE TO NEXT SEQUENTIAL INSTRUCTION                   */
*/*                                                                  */
*/*  NORMAL - DDNAME,DSNAME OR RELATIVE ENTRY FOUND                  */
*/*                                                                  */
*/*  ERROR - DDNAME,DSNAME OR RELATIVE ENTRY NOT FOUND               */
*/*                                                                  */
*/* OUTPUT - IF NO ERROR, THE DSABPTR HAS THE ADDRESS OF THE DSAB    */
*/*          THAT CORRESPONDS TO THE SPECIFIED DDNAME, DSNAME OR     */
*/*          RELATIVE ENTRY NUMBER.                                  */
*/*                                                                  */
*/*          IF THE SEARCH WAS DONE BY DSNAME, THEN FSTDSBSW IS ON   */
*/*          INDICATING SIOTS AND JFCBS HAVE BEEN READ STARTING AT   */
*/*          THE FIRST DSAB IN THE CHAIN.                            */
*/*                                                                  */
*/*          IF ERROR, ERRCODE IS SET TO:                            */
*/*             1. RCDDNFND IF DDNAME NOT FOUND                      */
*/*             2. RCDSNFND IF DSNAME NOT FOUND                      */
*/*             3. RCRELNNF IF RELATIVE ENTRY NOT FOUND              */
*/*                                                                  */
*/********************************************************************/
*    SWITCHES = ''B;                   /* CLEAR SWITCHES             */
         MVI   SWITCHES,X'00'                                   02S0084
*    IF DDNAMTUP ^= ZERO THEN          /* IS DDNAME TEXT UNIT PRESENT*/
         L     @15,DDNAMTUP                                     02S0085
         CR    @15,@10                                          02S0085
         BE    @RF00085                                         02S0085
*      DO;                             /* YES, SEARCH DSABS BY DDNAME*/
*        TEXTPTR = DDNAMTUP;           /* GET ADDRESS OF TEXT UNIT   */
         LR    TEXTPTR,@15                                      02S0087
*        DDNAREA = '';                 /* BLANK DDNAME AREA          */
         MVI   DDNAREA,C' '                                     02S0088
         MVC   DDNAREA+1(7),DDNAREA                             02S0088
*        DDNAREA(1:TEXTULNG) = TEXTUPAR(1:TEXTULNG); /* MOVE DDNAME  */
         ICM   @03,12,TEXTULNG(TEXTPTR)                         02S0089
         SRA   @03,16                                           02S0089
         BCTR  @03,0                                            02S0089
         EX    @03,@SM01719                                     02S0089
*        DDNAMEP = ADDR(DDNAREA);      /* GET ADDR OF DDNAME AREA    */
         LA    @15,DDNAREA                                      02S0090
         ST    @15,DDNAMEP                                      02S0090
*        RTNADDR = ZERO;               /* ZERO RETURN AREA ADDRESS   */
         ST    @10,RTNADDR                                      02S0091
*        CALL IEFDB4FC                 /* CALL DDNAME SEARCH         */
*                     (DSABPTR,DDNAMEP,ADDR(RTNADDR));          02S0092
         LA    @10,DSABPTR                                      02S0092
         ST    @10,@AL00001                                     02S0092
         LA    @10,DDNAMEP                                      02S0092
         ST    @10,@AL00001+4                                   02S0092
         LA    @10,RTNADDR                                      02S0092
         ST    @10,@TF00001                                     02S0092
         LA    @10,@TF00001                                     02S0092
         ST    @10,@AL00001+8                                   02S0092
         L     @15,@CV00124                                     02S0092
         LA    @01,@AL00001                                     02S0092
         BALR  @14,@15                                          02S0092
*        IF RTNADDR = ZERO THEN        /* WAS DDNAME FOUND IN DSABS ?*/
         L     @10,RTNADDR                                      02S0093
         LTR   @10,@10                                          02S0093
         BNZ   @RF00093                                         02S0093
*          ERRCODE = RCDDNFND;         /* NO, SET DD NOT FND ERR CODE*/
         L     @10,@PC00001+4                                   02S0094
         MVC   ERRCODE(2,@10),@CB01355                          02S0094
*        ELSE                          /* YES, DDNAME WAS FOUND      */
*          DSABPTR = RTNADDR;          /* POINT TO PROPER DSAB       */
         B     @RC00093                                         02S0095
@RF00093 L     @10,RTNADDR                                      02S0095
         ST    @10,DSABPTR                                      02S0095
*      END;                            /* END DDNAME SEARCH          */
*    ELSE;                             /* FALL THROUGH-DDNAME NOT
*                                         SPECIFIED                  */
@RF00085 DS    0H                                               02S0098
*    IF DSNAMTUP ^= ZERO THEN          /* IS DSNAME TEXT UNIT PRESENT*/
@RC00085 L     @10,DSNAMTUP                                     02S0098
         LTR   @10,@10                                          02S0098
         BZ    @RF00098                                         02S0098
*      DO;                             /* YES,SEARCH BY DSNAME       */
*        RESPECIFY S99TUNIT BASED(DSNAMTUP);                    02S0100
*        IF S99TUPAR(1:2) = '&&' THEN  /* IS DSN OF FORM,&&NAME      */
         CLC   S99TUPAR(2,@10),@CC01704                         02S0101
         BNE   @RF00101                                         02S0101
*          DO;                         /* YES, MAKE TU OF FORM,&NAME */
*            DSNAMKEY = S99TUKEY;      /* MOVE IN DSNAME KEY         */
         MVC   DSNAMKEY(2),S99TUKEY(@10)                        02S0103
*            DSNAMNUM = S99TUNUM;      /* MOVE IN NUMBER OF PARMS    */
         MVC   DSNAMNUM(2),S99TUNUM(@10)                        02S0104
*            DSNAMLEN = S99TULNG - 1;  /* SET LENGTH FOR &NAME       */
         ICM   @15,12,S99TULNG(@10)                             02S0105
         SRA   @15,16                                           02S0105
         LR    @14,@15                                          02S0105
         BCTR  @14,0                                            02S0105
         STH   @14,DSNAMLEN                                     02S0105
*            DSNAMPAR = S99TUPAR(2:S99TULNG); /* MOVE &NAME TO TU    */
         MVI   DSNAMPAR+1,C' '                                  02S0106
         MVC   DSNAMPAR+2(42),DSNAMPAR+1                        02S0106
         BCTR  @15,0                                            02S0106
         BCTR  @15,0                                            02S0106
         EX    @15,@SM01721                                     02S0106
*            DSNAMTUP = ADDR(DSNAMTU); /* SET PTR TO NEW TEXT UNIT   */
         LA    @10,DSNAMTU                                      02S0107
         ST    @10,DSNAMTUP                                     02S0107
*          END;                                                 02S0108
*        TEXTPTR = DSNAMTUP;           /* GET ADDRESS OF TEXT UNIT   */
@RF00101 L     TEXTPTR,DSNAMTUP                                 02S0109
*        DSNPTR = ADDR(TEXTUENT);      /* GET ADDR OF DSNAME INFO    */
         LA    @10,TEXTUENT(,TEXTPTR)                           02S0110
         ST    @10,DSNPTR                                       02S0110
*        FUNCTION = FUNCTION && FUNCTION; /* ZERO FUNCTION MAP       */
         XC    FUNCTION(2),FUNCTION                             02S0111
*        JFCBALNM = ON;                /* INDICATE SEARCH JFCB AND
*                                         ALTERNATE NAME             */
         OI    JFCBALNM,B'00100000'                             02S0112
*        RTNADDR = ZERO;               /* ZERO RETURN AREA           */
         SLR   @10,@10                                          02S0113
         ST    @10,RTNADDR                                      02S0113
*        CALL IEFDB4FA                 /* CALL DSNAME SEARCH ROUTINE */
*                     (DSABPTR,DSNPTR,RTNADDR,FUNCTION);        02S0114
         LA    @10,DSABPTR                                      02S0114
         ST    @10,@AL00001                                     02S0114
         LA    @10,DSNPTR                                       02S0114
         ST    @10,@AL00001+4                                   02S0114
         LA    @10,RTNADDR                                      02S0114
         ST    @10,@AL00001+8                                   02S0114
         LA    @10,FUNCTION                                     02S0114
         ST    @10,@AL00001+12                                  02S0114
         L     @15,@CV00123                                     02S0114
         LA    @01,@AL00001                                     02S0114
         BALR  @14,@15                                          02S0114
*        IF RTNADDR = ZERO THEN        /* WAS DSNAME FOUND ?         */
         L     @10,RTNADDR                                      02S0115
         LTR   @10,@10                                          02S0115
         BNZ   @RF00115                                         02S0115
*          ERRCODE = RCDSNFND;         /* SET DSNAME NOT FOUND ERRCDE*/
         L     @10,@PC00001+4                                   02S0116
         MVC   ERRCODE(2,@10),@CB01359                          02S0116
*        ELSE                          /* OTHERWISE, DSNAME FOUND    */
*          DSABPTR = RTNADDR;          /* POINT TO PROPER DSAB       */
         B     @RC00115                                         02S0117
@RF00115 L     @10,RTNADDR                                      02S0117
         ST    @10,DSABPTR                                      02S0117
*      END;                            /* END DSNAME TEXT UNIT PRESENT
*                                         PROCESSING                 */
*    ELSE;                             /* FALL THROUGH - DSNAME NOT
*                                         SPECIFIED                  */
@RF00098 DS    0H                                               02S0120
*    IF RELNOTUP ^= ZERO THEN          /* IS RELATIVE # TEXT PRESENT */
@RC00098 L     @10,RELNOTUP                                     02S0120
         SLR   @15,@15                                          02S0120
         CR    @10,@15                                          02S0120
         BE    @RF00120                                         02S0120
*      DO;                             /* YES, SEARCH BY RELATIVE #  */
*        TEXTPTR = RELNOTUP;           /* GET ADDRESS OF TEXT UNIT   */
         LR    TEXTPTR,@10                                      02S0122
*        IF TEXTUPAR(1:2) <= ZERO × DSABPTR = ZERO THEN /* IS RELATIVE
*                                       # <= 0 OR NO DSABS PRESENT ? */
         SLR   @10,@10                                          02S0123
         ICM   @10,3,TEXTUPAR(TEXTPTR)                          02S0123
         CR    @15,@10                                          02S0123
         BNL   @RT00123                                         02S0123
         C     @15,DSABPTR                                      02S0123
         BNE   @RF00123                                         02S0123
@RT00123 DS    0H                                               02S0124
*          ERRCODE = RCRELNNF;         /* YES SET ERRCODE            */
         L     @10,@PC00001+4                                   02S0124
         MVC   ERRCODE(2,@10),@CB01361                          02S0124
*        CONTINUE = ON;                /* INITIALIZE SWITCH          */
@RF00123 OI    CONTINUE,B'10000000'                             02S0125
*        DSABNBR = 1;                  /* SET DSAB COUNT TO 1        */
         LA    DSABNBR,1                                        02S0126
*        DO WHILE (ERRCODE = ZERO & CONTINUE = ON); /* LOOP CONTROL  */
         B     @DE00127                                         02S0127
@DL00127 DS    0H                                               02S0128
*          IF DSABNBR = TEXTUPAR(1:2) THEN /*WAS RELATIVE DSAB FOUND?*/
         SLR   @10,@10                                          02S0128
         ICM   @10,3,TEXTUPAR(TEXTPTR)                          02S0128
         CR    DSABNBR,@10                                      02S0128
         BNE   @RF00128                                         02S0128
*            CONTINUE = OFF;           /* YES, STOP PROCESSING       */
         NI    CONTINUE,B'01111111'                             02S0129
*          ELSE                        /* OTHERWISE, CONTINUE SEARCH */
*            DO;                       /* CONTINUE DSAB SEARCH       */
         B     @RC00128                                         02S0130
@RF00128 DS    0H                                               02S0131
*              IF DSABFCHN ^= ZERO THEN /* END OF DSAB CHAIN ?       */
         L     @10,DSABPTR                                      02S0131
         L     @10,DSABFCHN(,@10)                               02S0131
         LTR   @10,@10                                          02S0131
         BZ    @RF00131                                         02S0131
*                DO;                   /* NO, UPDATE POINTER & COUNT */
*                  DSABPTR = DSABFCHN; /* UPDATE DSABPTR             */
         ST    @10,DSABPTR                                      02S0133
*                  DSABNBR = DSABNBR + 1; /* INCREMENT DSAB COUNT    */
         AL    DSABNBR,@CF00084                                 02S0134
*                END;                  /* END OF PTR UPDATE          */
*              ELSE                    /* OTHERWISE, END OF DSAB CHN */
*                ERRCODE = RCRELNNF;   /* SET DSAB NOT FOUND ERRCODE */
         B     @RC00131                                         02S0136
@RF00131 L     @10,@PC00001+4                                   02S0136
         MVC   ERRCODE(2,@10),@CB01361                          02S0136
*            END;                      /* END DSAB SEARCH            */
@RC00131 DS    0H                                               02S0138
*        END;                          /* END OF LOOP CONTROL        */
@RC00128 DS    0H                                               02S0138
@DE00127 L     @10,@PC00001+4                                   02S0138
         ICM   @15,3,ERRCODE(@10)                               02S0138
         BNZ   @DC00127                                         02S0138
         TM    CONTINUE,B'10000000'                             02S0138
         BO    @DL00127                                         02S0138
@DC00127 DS    0H                                               02S0139
*      END;                            /* END RELATIVE # PROCESSING  */
*    ELSE;                             /* RELATIVE # NOT SPECIFIED   */
@RF00120 DS    0H                                               02S0141
*  END;                                /* END SEARCH FOR PROPER DSAB */
*IF ERRCODE = ZERO THEN                /* IF ERRCODE = 0, THEN PROPER
*                                         DSAB FOUND                 */
@RF00081 SLR   @10,@10                                          01S0142
         L     @15,@PC00001+4                                   01S0142
         SLR   @14,@14                                          01S0142
         ICM   @14,3,ERRCODE(@15)                               01S0142
         CR    @10,@14                                          01S0142
         BNE   @RF00142                                         01S0142
*  DO;                                 /* 0 ERRCODE, RETRIEVE DATA   */
*/********************************************************************/
*/*                                                                  */
*/* SEGMENT NAME - RETRIEVE                                          */
*/*                                                                  */
*/* DESCRIPTIVE NAME - RETRIEVE REQUESTED INFORMATION                */
*/*                                                                  */
*/* FUNCTION - THIS ROUTINE DETERMINES WHAT INFORMATION HAS BEEN     */
*/*            REQUESTED AND EXAMINES CONTROL BLOCKS IN ORDER TO     */
*/*            OBTAIN THE NECESSARY DATA. THE DATA IS STORED         */
*/*            INTO THE TEXT UNIT PARM FIELDS.                       */
*/*                                                                  */
*/* OPERATION - THE KEY TABLE IS INTERROGATED TO DETERMINE WHICH KEYS*/
*/*             HAVE BEEN SPECIFIED.  THE DATA IS THEN STORED INTO   */
*/*             THE ASSOCIATED TEXT UNITS, AFTER RETRIEVING IT       */
*/*             FROM THE APPROPRIATE CONTROL BLOCKS.                 */
*/*                                                                  */
*/*             IF RETURN DSORG (KEY DINRTORG) HAS BEEN SPECIFIED,   */
*/*             IEFDB4FE IS INVOKED ONLY IF THE DSORG FIELD IN       */
*/*             THE DSAB IS 0.                                       */
*/*                                                                  */
*/* ENTRY - RETRIEVE (INCLUDED SEGMENT)                              */
*/*                                                                  */
*/* PURPOSE - SEE FUNCTION                                           */
*/*                                                                  */
*/* INPUT DATA - TEXT UNITS,KEY TABLE                                */
*/*                                                                  */
*/* EXITS - ALL ARE TO NEXT SEQUENTIAL INSTRUCTION                   */
*/*                                                                  */
*/*  NORMAL - ERRCODE IS ZERO AND ALL REQUESTED DATA RETURNED        */
*/*                                                                  */
*/*  ERROR - NONE                                                    */
*/*                                                                  */
*/* OUTPUT DATA - TEXT UNITS CONTAIN DATA REQUESTED.                 */
*/*                                                                  */
*/********************************************************************/
*    IF RTDDNTUP  ^= ZERO THEN         /* RETURN DDNAME REQUESTED ?  */
         L     @15,RTDDNTUP                                     02S0144
         CR    @15,@10                                          02S0144
         BE    @RF00144                                         02S0144
*      DO;                             /* YES                        */
*        TEXTPTR = RTDDNTUP;           /* GET TEXT UNIT POINTER      */
         LR    TEXTPTR,@15                                      02S0146
*        LIMIT = 8;                    /* SET MAX # OF CHAR TO SCAN  */
         LA    LIMIT,8                                          02S0147
*        CHARPTR = ADDR(TIOEDDNM);     /* ADDR OF FIELD TO SCAN      */
         L     @10,DSABPTR                                      02S0148
         L     CHARPTR,DSABTIOT(,@10)                           02S0148
         LA    CHARPTR,TIOEDDNM(,CHARPTR)                       02S0148
*        CALL BLNKSCAN;                /* CALL ROUTINE TO SCAN FOR
*                                         BLANKS AND MOVE FIELD      */
         BAL   @14,BLNKSCAN                                     02S0149
*      END;                            /* END RETURN DDNAME REQUEST  */
*                                                               02S0150
*    IF RTDSNTUP ^= ZERO THEN          /* RETURN DSNAME REQUESTED?   */
@RF00144 L     @10,RTDSNTUP                                     02S0151
         LTR   @10,@10                                          02S0151
         BZ    @RF00151                                         02S0151
*      DO;                             /* YES                        */
*        TEXTPTR = RTDSNTUP;           /* GET TEXT UNIT POINTER      */
         LR    TEXTPTR,@10                                      02S0153
*        LIMIT = 44;                   /* SET MAX # OF CHAR TO SCAN  */
         LA    LIMIT,44                                         02S0154
*        CHARPTR = ADDR(JFCBDSNM);     /* GET ADDRESS OF DSNAME FIELD*/
         L     @10,DSABPTR                                      02S0155
         L     @10,DSABSIOT(,@10)                               02S0155
         L     CHARPTR,SJFCBPTR(,@10)                           02S0155
*        CALL BLNKSCAN;                /* CALL ROUTINE TO SCAN FOR
*                                         BLANKS AND MOVE FIELD      */
         BAL   @14,BLNKSCAN                                     02S0156
*      END;                            /* END RETURN DSNAME REQUEST  */
*                                                               02S0157
*    IF RTMEMTUP ^= ZERO THEN          /* RETURN MEMBERNAME REQUESTED*/
@RF00151 L     @10,RTMEMTUP                                     02S0158
         LTR   @10,@10                                          02S0158
         BZ    @RF00158                                         02S0158
*      DO;                             /* YES                        */
*        TEXTPTR = RTMEMTUP;           /* GET TEXT UNIT POINTER      */
         LR    TEXTPTR,@10                                      02S0160
*        LIMIT = 8;                    /* SET MAX # OF CHAR TO SCAN  */
         LA    LIMIT,8                                          02S0161
*        CHARPTR = ADDR(JFCBELNM);     /* ADDR OF FIELD TO SCAN      */
         L     @10,DSABPTR                                      02S0162
         L     @10,DSABSIOT(,@10)                               02S0162
         L     CHARPTR,SJFCBPTR(,@10)                           02S0162
