@RF00174 DS    0H                                               01S0397
*      END;                            /*END NO INCLUCHK ERROR       */
*    ELSE;                             /*ELSE REQUIRED KEY MISSING   */
@RF00144 DS    0H                                               01S0399
*  END;                                /*END NO SYNTAX CHECKER ERROR */
*ELSE;                                 /*ERROR IN SYNTAX CHECKER     */
@RF00094 DS    0H                                               01S0401
*RETURN CODE(0);                                                01S0401
         L     @13,4(,@13)                                      01S0401
         L     @00,@SIZDATD                                     01S0401
         LR    @01,@10                                          01S0401
         FSPACE   R,LV=(0),A=(1)
         SLR   @15,@15                                          01S0401
         L     @14,12(,@13)                                     01S0401
         LM    @00,@12,20(@13)                                  01S0401
         BR    @14                                              01S0401
*CHARSCAN: PROC;                                                02S0402
CHARSCAN STM   @14,@12,@SA00002                                 02S0402
*/********************************************************************/
*/*                                                                  */
*/* SEGMENT NAME - CHARSCAN                                          */
*/*                                                                  */
*/* DESCRIPTIVE NAME - SUBROUTINE SCAN OF CHARACTER PARM VALUES      */
*/*                                                                  */
*/* FUNCTION - CALLED BY THE CHARCHK SEGMENT TO SCAN THE PARM        */
*/*            IN A TEXT UNIT FOR INVALID CHARACTERS                 */
*/*                                                                  */
*/* OPERATION  -  IF THE FIRST CHARACTER OF THE TEXT UNIT PARM MUST  */
*/*               BE ALPHANATIONAL, A CHECK IS MADE TO ENSURE THIS   */
*/*               CHARACTER IS NOT NUMERIC, THE ALPHACHK BIT IS      */
*/*               TURNED OFF, AND THE REMAINING NECESSARY CHECKS     */
*/*               FOLLOW.                                            */
*/*                                                                  */
*/*               THIS SEGMENT USES THE BAL TRT INSTRUCTION TO SCAN  */
*/*               THE PARM FOR THE LENGTH SPECIFIED IN THE TEXT UNIT.*/
*/*               CHARMASK(A 256 BYTE MASK WITH ZEROS IN THE         */
*/*               POSITIONS CORRESPONDING TO @, #, $, A-Z, 0-9, AND  */
*/*               THE CORRESPONDING HEX VALUES IN ALL THE OTHER      */
*/*               POSITIONS) IS USED IN THE TRT TO DETERMINE VALID   */
*/*               CHARACTERS. (NOTE: FF IS IN THE POSITION           */
*/*               CORRESPONDING TO '00'X TO PREVENT THIS FROM BEING  */
*/*               TAKEN AS A VALID VALUE) IF REGISTER 1 IS ZERO AFTER*/
*/*               THE TRT, ALL CHARACTERS ARE VALID. OTHERWISE REG. 1*/
*/*               HAS THE ADDRESS OF THE INVALID CHARACTER, AND REG.2*/
*/*               THE CHARACTER ITSELF AS FOUND IN CHARMASK. SPECIAL */
*/*               CHECKS ARE THEN MADE AS FOLLOWS TO SEE IF THE      */
*/*               INVALID CHARACTER IS ALLOWABLE                     */
*/*                                                                  */
*/*                 - A BLANK IS ALLOWED AS LONG AS THE BLANK IS NOT */
*/*                   IN THE FIRST POSITION, DOES NOT FOLLOW A PERIOD*/
*/*                   IN DSN, AND ALL REMAINING CHARACTERS ARE BLANK.*/
*/*                                                                  */
*/*                 - HYPHENS ARE ALLOWED IN VOLUME SERIALS          */
*/*                                                                  */
*/*                 - HYPHENS AND 12-0 PUNCHES ARE ALLOWED IN DSN,   */
*/*                   AS LONG AS NOT IN THE FIRST POSITION, OR       */
*/*                   FOLLOWING A PERIOD.                            */
*/*                                                                  */
*/*                 - PERIOD IS ALLOWED IN DSN AS LONG AS NOT IN     */
*/*                   FIRST OR LAST POSITION, THERE ARE 1-8          */
*/*                   CHARACTERS BETWEEN PERIODS, THE DSN IS NOT     */
*/*                   TEMPORARY, AND THE CHARACTER FOLLOWING THE     */
*/*                   PERIOD IS ALPHANATIONAL.                       */
*/*                                                                  */
*/*               IF THE CHARACTER IS TO BE ALLOWED, THE TRT PROCESS */
*/*               IS THEN REPEATED FOR THE REMAINING CHARACTERS IN   */
*/*               THE PARM UNTIL ALL HAVE BEEN LOOKED AT OR AN ERROR */
*/*               HAS BEEN DETERMINED. A FINAL CHECK IS MADE TO      */
*/*               ENSURE THAT NO MORE THAN EIGHT CHARACTERS WERE     */
*/*               SPECIFIED IN A DSN WITHOUT A PERIOD. IF AT ANY     */
*/*               TIME AN ERROR IS DETERMINED IN THE PROCESS THE     */
*/*               ERRORSW BIT IS SET ON AND NO FURTHER VALIDITY      */
*/*               CHECKING IS DONE.                                  */
*/*                                                                  */
*/* ENTRY -                                                          */
*/*                                                                  */
*/*   INPUT DATA - UNITPTR  - PTR TO TEXTUNIT WHOSE PARM             */
*/*                           IS TO BE SCANNED                       */
*/*                ALPHACHK - BIT SET ON IF FIRST CHARACTER          */
*/*                           MUST BE ALPHANATIONAL                  */
*/*                AMPDSN   - BIT SET ON IF PARM BEING SCANNED       */
*/*                           IS AN &DSNAME                          */
*/*                ERRORSW  - BIT SET ON IF ERROR IS FOUND           */
*/*                           (ZERO ON ENTRY)                        */
*/*                                                                  */
*/* EXIT -                                                           */
*/*                                                                  */
*/*   OUTPUT DATA - ERRORSW  - TURNED ON IF ERROR IS FOUND           */
*/*                 ALPHACHK - TURNED OFF IF IT WAS SET ON ENTRY     */
*/*                            AND THE PARM IS VALID                 */
*/*                                                                  */
*/********************************************************************/
*RFY (TRTSTART,TRTLEN,INVALPTR,INVALCHR) RESTRICTED;            02S0403
*                                      /*RESTRICT WORK REGISTERS     */
*RFY TEXTUNIT BASED(UNITPTR);          /*RESET TEXT UNIT MAPPING BASE*/
*DSNCNTR=ADDR(TEXTUPAR);               /*INIT CNTR FOR DSN KEY       */
         LA    @04,TEXTUPAR(,UNITPTR)                           02S0405
         LR    DSNCNTR,@04                                      02S0405
*BLNKADDR=0;                           /*INIT ADDR OF 1ST PARM BLANK */
         SLR   BLNKADDR,BLNKADDR                                02S0406
*TRTSTART=ADDR(TEXTUPAR);              /*POINT TO PARM FOR TRT       */
         LR    TRTSTART,@04                                     02S0407
*TRTLEN=TEXTULNG-1;                    /*LENGTH LESS 1 FOR EXECUTE   */
         ICM   TRTLEN,12,TEXTULNG(UNITPTR)                      02S0408
         SRA   TRTLEN,16                                        02S0408
         BCTR  TRTLEN,0                                         02S0408
*DO WHILE (ERRORSW='0'B & TRTLEN>=0);  /*SCAN TILL ERROR OR PARM END */
         B     @DE00409                                         02S0409
@DL00409 DS    0H                                               02S0410
*  IF ALPHACHK='1'B THEN               /*MUST 1ST CHAR BE ALPHANAT'L.*/
         TM    ALPHACHK,B'01000000'                             02S0410
         BNO   @RF00410                                         02S0410
*    IF TRTSTART->ALPCKCHR > 'Z' THEN  /*YES,IS CHAR > EBCDIC 'Z'?   */
         CLI   ALPCKCHR(TRTSTART),C'Z'                          02S0411
         BNH   @RF00411                                         02S0411
*      ERRORSW='1'B;                   /*YES,THEN ERROR              */
         OI    ERRORSW,B'10000000'                              02S0412
*    ELSE                              /*NO,THEN CHAR IS NOT NUMERIC */
*      ALPHACHK='0'B;                  /*TURN OFF ALPHANAT'L CHECK   */
         B     @RC00411                                         02S0413
@RF00411 NI    ALPHACHK,B'10111111'                             02S0413
*  ELSE;                               /*ALPHANAT'L. CHECK NOT NEEDED*/
*                                                               02S0414
@RF00410 DS    0H                                               02S0415
*  IF ERRORSW='0'B THEN                /*HAS ERROR BEEN DETECTED     */
@RC00410 TM    ERRORSW,B'10000000'                              02S0415
         BNZ   @RF00415                                         02S0415
*    DO;                               /*NO, CONTINUE CHECKS         */
*      INVALPTR=0;                     /*INIT TRT RESULT REG TO 0    */
         SLR   INVALPTR,INVALPTR                                02S0417
*      INVALCHR=0;                     /*INIT TRT RESULT REG TO 0    */
         SLR   INVALCHR,INVALCHR                                02S0418
*      GEN (EX TRTLEN,CHARTST)         /*EXECUTE TRT FOR VALID CHARS */
*          REFS(INVALPTR,INVALCHR,TRTSTART,TRTLEN)              02S0419
*          SETS(INVALPTR,INVALCHR);                             02S0419
         EX TRTLEN,CHARTST
*      IF INVALPTR ^= 0 THEN           /*WAS AN INVALID CHAR FOUND   */
         LTR   INVALPTR,INVALPTR                                02S0420
         BZ    @RF00420                                         02S0420
*        DO;                           /*YES,MAKE EXCEPTION CHECKS   */
*          BADCHAR=INVALCHR;           /*SAVE INVALID CHARACTER      */
         STC   INVALCHR,BADCHAR                                 02S0422
*          PRMINDX=INVALPTR-ADDR(TEXTUPAR)+1;                   02S0423
         LA    @03,TEXTUPAR(,UNITPTR)                           02S0423
         LR    PRMINDX,INVALPTR                                 02S0423
         SLR   PRMINDX,@03                                      02S0423
         AL    PRMINDX,@CF00063                                 02S0423
*                                      /*FIND ITS INDEX WITHIN PARM  */
*                                                               02S0424
*          IF BADCHAR=' ' THEN         /*IS IT A BLANK ?             */
         CLI   BADCHAR,C' '                                     02S0424
         BNE   @RF00424                                         02S0424
*            IF (INVALPTR=ADDR(TEXTUPAR)  ×  /*YES,IS IT 1ST IN PARM */
*                TEXTUPAR(PRMINDX-1)='.') THEN/*OR AFTER A DSN PERIOD*/
         CR    INVALPTR,@03                                     02S0425
         BE    @RT00425                                         02S0425
         LR    @03,UNITPTR                                      02S0425
         ALR   @03,PRMINDX                                      02S0425
         CLI   TEXTUPAR-2(@03),C'.'                             02S0425
         BNE   @RF00425                                         02S0425
@RT00425 DS    0H                                               02S0426
*              ERRORSW='1'B;           /*YES,THEN ERROR              */
         OI    ERRORSW,B'10000000'                              02S0426
*            ELSE                      /*NO,THEN CONTINUE CHECKS     */
*              DO;                     /*ENSURE REST OF PARM IS BLANK*/
         B     @RC00425                                         02S0427
@RF00425 DS    0H                                               02S0428
*                BLNKADDR=INVALPTR;    /*SAVE ADDR OF 1ST BLANK      */
         LR    BLNKADDR,INVALPTR                                02S0428
*                DO WHILE (PRMINDX<=TEXTULNG & /*DO UNTIL END OF PARM*/
*                          ERRORSW='0'B);      /*OR NON-BLANK ERROR  */
         B     @DE00429                                         02S0429
@DL00429 DS    0H                                               02S0430
*                  IF TEXTUPAR(PRMINDX)^=' ' THEN /*IS CHAR A BLANK  */
         LR    @03,UNITPTR                                      02S0430
         ALR   @03,PRMINDX                                      02S0430
         CLI   TEXTUPAR-1(@03),C' '                             02S0430
         BE    @RF00430                                         02S0430
*                    ERRORSW='1'B;     /*NO,THEN ERROR               */
         OI    ERRORSW,B'10000000'                              02S0431
*                  ELSE                /*YES,THEN CONTINUE           */
*                    PRMINDX=PRMINDX+1;/*INCREMENT TO NEXT PARM CHAR.*/
         B     @RC00430                                         02S0432
@RF00430 AL    PRMINDX,@CF00063                                 02S0432
*                END;                  /*END OF BLANK CHECKS         */
@RC00430 DS    0H                                               02S0433
@DE00429 ICM   @03,12,TEXTULNG(UNITPTR)                         02S0433
         SRA   @03,16                                           02S0433
         CR    PRMINDX,@03                                      02S0433
         BH    @DC00429                                         02S0433
         TM    ERRORSW,B'10000000'                              02S0433
         BZ    @DL00429                                         02S0433
@DC00429 DS    0H                                               02S0434
*                TRTLEN=-1;            /*GET OUT OF OUTER SCAN LOOP  */
         SLR   TRTLEN,TRTLEN                                    02S0434
         BCTR  TRTLEN,0                                         02S0434
*              END;                    /*END OF BLANK PROCESSING     */
*          ELSE                        /*ILLEGAL CHAR IS NOT A BLANK */
*                                                               02S0436
*            IF TEXTUKEY=DALVLSER THEN /*IS THIS VOL SER TEXT KEY    */
         B     @RC00424                                         02S0436
@RF00424 CLC   TEXTUKEY(2,UNITPTR),@CB01196                     02S0436
         BNE   @RF00436                                         02S0436
*              IF BADCHAR='-' THEN     /*YES,IS ILLEGAL CHAR A HYPHEN*/
         CLI   BADCHAR,C'-'                                     02S0437
         BNE   @RF00437                                         02S0437
*                DO;                   /*YES,HYPHEN ALLOWED IN VOLSER*/
*                  TRTSTART=INVALPTR+1;  /*POINT TO NEXT CHAR FOR TRT*/
         LA    TRTSTART,1                                       02S0439
         ALR   TRTSTART,INVALPTR                                02S0439
*                  TRTLEN=TEXTULNG-PRMINDX-1; /* LENGTH LEFT TO CHECK*/
         ICM   TRTLEN,12,TEXTULNG(UNITPTR)                      02S0440
         SRA   TRTLEN,16                                        02S0440
         SLR   TRTLEN,PRMINDX                                   02S0440
         BCTR  TRTLEN,0                                         02S0440
*                END;                  /*END OF HYPHEN PROCESSING    */
*              ELSE                    /*CHAR IS NOT HYPHEN          */
*                ERRORSW='1'B;         /*THEREFORE ERROR             */
         B     @RC00437                                         02S0442
@RF00437 OI    ERRORSW,B'10000000'                              02S0442
*            ELSE                      /*KEY IS NOT VOL SER,CONTINUE */
*                                                               02S0443
*              IF TEXTUKEY=DALDSNAM THEN /*IS KEY DSNAME ?           */
         B     @RC00436                                         02S0443
@RF00436 CLC   TEXTUKEY(2,UNITPTR),@CB00126                     02S0443
         BNE   @RF00443                                         02S0443
*                IF (INVALPTR=ADDR(TEXTUPAR) ×                  02S0444
*                    TEXTUPAR(PRMINDX-1)='.' ) THEN             02S0444
         LA    @03,TEXTUPAR(,UNITPTR)                           02S0444
         CR    INVALPTR,@03                                     02S0444
         BE    @RT00444                                         02S0444
         LR    @03,UNITPTR                                      02S0444
         ALR   @03,PRMINDX                                      02S0444
         CLI   TEXTUPAR-2(@03),C'.'                             02S0444
         BNE   @RF00444                                         02S0444
@RT00444 DS    0H                                               02S0445
*                                      /*YES, IS ILLEGAL CHAR 1ST IN */
*                                      /*PARM OR AFTER A PERIOD ?    */
*                  ERRORSW='1'B;       /*YES,THEN ERROR              */
         OI    ERRORSW,B'10000000'                              02S0445
*                ELSE                  /*NO,POSITION IS OK           */
*                  IF (BADCHAR='C0'X × /*IS THE CHAR A 12-0 PUNCH    */
*                      BADCHAR='-') THEN  /*OR A HYPHEN              */
         B     @RC00444                                         02S0446
@RF00444 CLI   BADCHAR,X'C0'                                    02S0446
         BE    @RT00446                                         02S0446
         CLI   BADCHAR,C'-'                                     02S0446
         BNE   @RF00446                                         02S0446
@RT00446 DS    0H                                               02S0447
*                    DO;               /*YES THESE ARE ALLOWED IN DSN*/
*                      TRTSTART=INVALPTR+1; /*START TRT W. NEXT CHAR */
         LA    TRTSTART,1                                       02S0448
         ALR   TRTSTART,INVALPTR                                02S0448
*                      TRTLEN=TEXTULNG-PRMINDX-1; /*GET LENGTH LEFT  */
         ICM   TRTLEN,12,TEXTULNG(UNITPTR)                      02S0449
         SRA   TRTLEN,16                                        02S0449
         SLR   TRTLEN,PRMINDX                                   02S0449
         BCTR  TRTLEN,0                                         02S0449
*                    END;              /*END HYPHEN, 12-0 PROCESSING */
*                  ELSE                /*NEITHER HYPHEN OR 12-0 PUNCH*/
*                    IF BADCHAR='.' THEN  /*IS THE CHAR A PERIOD     */
         B     @RC00446                                         02S0451
@RF00446 CLI   BADCHAR,C'.'                                     02S0451
         BNE   @RF00451                                         02S0451
*                      IF (AMPDSN='1'B      × /*YES,IS IT AN &DSN OR */
*                          PRMINDX=TEXTULNG × /*PERIOD LAST CHAR OR  */
*                          INVALPTR-DSNCNTR > 8) THEN           02S0452
         TM    AMPDSN,B'00100000'                               02S0452
         BO    @RT00452                                         02S0452
         ICM   @03,12,TEXTULNG(UNITPTR)                         02S0452
         SRA   @03,16                                           02S0452
         CR    PRMINDX,@03                                      02S0452
         BE    @RT00452                                         02S0452
         LR    @03,INVALPTR                                     02S0452
         SLR   @03,DSNCNTR                                      02S0452
         C     @03,@CF00142                                     02S0452
         BNH   @RF00452                                         02S0452
@RT00452 DS    0H                                               02S0453
*                                      /* > 8 CHARS BETWEEN PERIODS  */
*                        ERRORSW='1'B; /*YES,THEN ERROR              */
         OI    ERRORSW,B'10000000'                              02S0453
*                      ELSE            /*NO,THEN PERIOD IS VALID     */
*                        DO;           /*SET UP TO CONTINUE CHECKING */
         B     @RC00452                                         02S0454
@RF00452 DS    0H                                               02S0455
*                          ALPHACHK='1'B;/*NEXT CHAR TO BE ALPHHAT'L.*/
         OI    ALPHACHK,B'01000000'                             02S0455
*                          TRTSTART=INVALPTR+1;  /*NEXT CHAR FOR TRT */
         LA    TRTSTART,1                                       02S0456
         ALR   TRTSTART,INVALPTR                                02S0456
*                          TRTLEN=TEXTULNG-PRMINDX-1; /*ADJUST LENGTH*/
         ICM   TRTLEN,12,TEXTULNG(UNITPTR)                      02S0457
         SRA   TRTLEN,16                                        02S0457
         SLR   TRTLEN,PRMINDX                                   02S0457
         BCTR  TRTLEN,0                                         02S0457
*                          DSNCNTR=TRTSTART;  /*RESET DSN PERIOD CNTR*/
         LR    DSNCNTR,TRTSTART                                 02S0458
*                        END;          /*END VALID DSN PERIOD        */
*                    ELSE              /*ILLEGAL CHAR IS NOT PERIOD  */
*                      ERRORSW='1'B;   /*THEN ERROR                  */
         B     @RC00451                                         02S0460
@RF00451 OI    ERRORSW,B'10000000'                              02S0460
*              ELSE                    /*KEY IS NOT DSN              */
*                                                               02S0461
*                ERRORSW='1'B;         /*THEN ERROR                  */
         B     @RC00443                                         02S0461
@RF00443 OI    ERRORSW,B'10000000'                              02S0461
*        END;                          /*END EXCEPTION CHECKS        */
*      ELSE                            /*NO INVALID CHAR FOUND       */
*        TRTLEN=-1;                    /*GET OUT OF SCAN LOOP        */
         B     @RC00420                                         02S0463
@RF00420 SLR   TRTLEN,TRTLEN                                    02S0463
         BCTR  TRTLEN,0                                         02S0463
*    END;                              /*END NO ALPHANATL CHECK ERROR*/
*  ELSE;                               /*ERROR IN ALPHANATL CHECK    */
@RF00415 DS    0H                                               02S0466
*END;                                  /*END SCAN LOOP               */
*                                                               02S0466
@RC00415 DS    0H                                               02S0466
@DE00409 TM    ERRORSW,B'10000000'                              02S0466
         BNZ   @DC00409                                         02S0466
         LTR   TRTLEN,TRTLEN                                    02S0466
         BNM   @DL00409                                         02S0466
@DC00409 DS    0H                                               02S0467
*IF TEXTUKEY=DALDSNAM THEN             /*IS TEXT KEY DSN             */
         CLC   TEXTUKEY(2,UNITPTR),@CB00126                     02S0467
         BNE   @RF00467                                         02S0467
*  IF ERRORSW='0'B THEN                /*YES,HAS ERROR BEEN FOUND    */
         TM    ERRORSW,B'10000000'                              02S0468
         BNZ   @RF00468                                         02S0468
*    IF BLNKADDR^=0 THEN               /*NO,WAS A BLANK FOUND        */
         LTR   BLNKADDR,BLNKADDR                                02S0469
         BZ    @RF00469                                         02S0469
*      IF BLNKADDR-DSNCNTR > 8 THEN    /*YES,>8 CHARS BEFORE IT W/O .*/
         LR    @04,BLNKADDR                                     02S0470
         SLR   @04,DSNCNTR                                      02S0470
         C     @04,@CF00142                                     02S0470
         BNH   @RF00470                                         02S0470
*        ERRORSW='1'B;                 /*YES, THEN ERROR             */
         OI    ERRORSW,B'10000000'                              02S0471
*      ELSE;                           /*NO, THEN NO ERROR           */
@RF00470 DS    0H                                               02S0473
*    ELSE                              /*NO TRAILING BLANK           */
*      IF (ADDR(TEXTUPAR) + TEXTULNG   /*MORE THAN 8 CHARS AT END OF */
*          -DSNCNTR  > 8) THEN         /*PARM W/O A PERIOD ?         */
         B     @RC00469                                         02S0473
@RF00469 LA    @09,TEXTUPAR(,UNITPTR)                           02S0473
         ICM   @04,12,TEXTULNG(UNITPTR)                         02S0473
         SRA   @04,16                                           02S0473
         ALR   @09,@04                                          02S0473
         SLR   @09,DSNCNTR                                      02S0473
         C     @09,@CF00142                                     02S0473
         BNH   @RF00473                                         02S0473
*        ERRORSW='1'B;                 /*YES, THEN ERROR             */
         OI    ERRORSW,B'10000000'                              02S0474
*RETURN;                               /*RETURN TO INVOKER           */
@EL00002 DS    0H                                               02S0475
@EF00002 DS    0H                                               02S0475
@ER00002 LM    @14,@12,@SA00002                                 02S0475
         BR    @14                                              02S0475
*CHARTST: GEN(TRT 0(0,TRTSTART),CHARMASK);/*VALID CHAR CHK - EXECUTED*/
CHARTST  TRT 0(0,TRTSTART),CHARMASK
*RFY TEXTUNIT BASED(TXTPLENT);         /*RESET TEXT UNIT MAP BASE    */
*RFY (TRTSTART,TRTLEN,INVALPTR,INVALCHR) UNRESTRICTED;          02S0478
*                                      /*RELEASE WORK REGISTERS      */
*END CHARSCAN;                                                  02S0479
         B     @EL00002                                         02S0479
*                                      /*CHARACTERS                  */
*END IEFDB412                                                   01S0480
