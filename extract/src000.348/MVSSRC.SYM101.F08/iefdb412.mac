         TITLE 'IEFDB412 - DYN ALLOC FUNCTION - VALIDITY CHECKER       *
                        '
IEFDB412 CSECT ,                                                01S0001
@MAINENT DS    0H                                               01S0001
         USING *,@15                                            01S0001
         B     @PROLOG                                          01S0001
         DC    AL1(16)                                          01S0001
         DC    C'IEFDB412  78.089'                              01S0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                  01S0001
         BALR  @11,0                                            01S0001
@PSTART  LA    @12,4095(,@11)                                   01S0001
         USING @PSTART,@11                                      01S0001
         USING @PSTART+4095,@12                                 01S0001
         L     @00,@SIZDATD                                     01S0001
         GSPACE   R,LV=(0)
         LR    @10,@01                                          01S0001
         USING @DATD,@10                                        01S0001
         ST    @13,@SA00001+4                                   01S0001
         LM    @00,@01,20(@13)                                  01S0001
         ST    @10,8(,@13)                                      01S0001
         LR    @13,@10                                          01S0001
         MVC   @PC00001(12),0(@01)                              01S0001
*REG2 = ADDR(LKEYDICT);                /*OUTPUT ADDRES REG   @G29AN2F*/
         LA    REG2,LKEYDICT                                    01S0072
*REG3 = LENGTH(KEYDICT);               /*OUTPUT LENGTH REG   @G29AN2F*/
         LA    REG3,1994                                        01S0073
*REG4 = ADDR(KEYDICT);                 /*INPUT ADDRESS REG   @G29AN2F*/
         LA    REG4,KEYDICT                                     01S0074
*REG5 = REG3;                          /*INPUT LENGTH REG    @G29AN2F*/
         LR    REG5,REG3                                        01S0075
*MVCL(REG2,REG4);                      /*COPY KEY DICT       @G29AN2F*/
         MVCL  REG2,REG4                                        01S0076
*RFY (REG2,REG3,REG4,REG5) UNRSTD;     /*RELEASE REGS        @G29AN2F*/
*IF SU29BIT = OFF THEN                 /*NOT SU29 SYSTEM?    @G29AN2F*/
         L     @06,CVTPTR                                       01S0078
         L     @06,CVTIHASU(,@06)                               01S0078
         TM    SU29BIT(@06),B'00000100'                         01S0078
         BNZ   @RF00078                                         01S0078
*  DO;                                 /*YES, DISALLOW KEYS  @G29AN2F*/
*    KEYPTR = ADDR(LKEYDICT) + SSNMKYOF; /*SSNM KEY POINTER  @G29AN2F*/
         LA    @06,LKEYDICT                                     01S0080
         LH    KEYPTR,SSNMKYOF                                  01S0080
         ALR   KEYPTR,@06                                       01S0080
*    KEYAREA = NOKEY;                  /*INHIBIT SSNM KEY    @G29AN2F*/
         MVC   KEYAREA(2,KEYPTR),NOKEY                          01S0081
*    KEYPTR = ADDR(LKEYDICT) + SSPRKYOF; /*SSPRM KEY POINTER @G29AN2F*/
         AH    @06,SSPRKYOF                                     01S0082
         LR    KEYPTR,@06                                       01S0082
*    KEYAREA = NOKEY;                  /*INHIBIT SSPR KEY    @G29AN2F*/
         MVC   KEYAREA(2,KEYPTR),NOKEY                          01S0083
*  END;                                /*END SU29 PROCESS    @G29AN2F*/
*IF SU32BIT=OFF THEN                   /*NOT SU32 SYSTEM?    @G32HPPJ*/
@RF00078 L     @06,CVTPTR                                       01S0085
         L     @06,CVTIHASU(,@06)                               01S0085
         TM    SU32BIT(@06),B'10000000'                         01S0085
         BNZ   @RF00085                                         01S0085
*  DO;                                 /* YES, INVALIDATE PROTECT
*                                                            @G32HPPJ*/
*    KEYPTR = ADDR(LKEYDICT)+PROTKYOF; /*PROTECT KEY POINTER @G32HPPJ*/
         LA    KEYPTR,LKEYDICT                                  01S0087
         AH    KEYPTR,PROTKYOF                                  01S0087
*    KEYAREA = NOKEY;                  /*INHIBIT PROTECT KEY @G32HPPJ*/
         MVC   KEYAREA(2,KEYPTR),NOKEY                          01S0088
*  END;                                /*                    @G32HPPJ*/
*ELSE;                                 /*                    @G32HPPJ*/
@RF00085 DS    0H                                               01S0091
*DUPMASK=DUPMASK && DUPMASK;           /*ZERO OUT DUP KEY MASK       */
         XC    DUPMASK(16),DUPMASK                              01S0091
*MUTEXMSK=MUTEXMSK && MUTEXMSK;        /*ZERO OUT MUT EX KEY MASK    */
         XC    MUTEXMSK(16),MUTEXMSK                            01S0092
*CALL IEFDB4FF(TEXTPLP,ADDR(LKEYDICT),ADDR(DUPMASK), /*      @G29AN2F*/
*              ADDR(MUTEXMSK),KEYTABP,RSCODE);                  01S0093
         L     @06,@PC00001+4                                   01S0093
         ST    @06,@AL00001                                     01S0093
         LA    @06,LKEYDICT                                     01S0093
         ST    @06,@TF00001                                     01S0093
         LA    @06,@TF00001                                     01S0093
         ST    @06,@AL00001+4                                   01S0093
         LA    @06,DUPMASK                                      01S0093
         ST    @06,@TF00002                                     01S0093
         LA    @06,@TF00002                                     01S0093
         ST    @06,@AL00001+8                                   01S0093
         LA    @06,MUTEXMSK                                     01S0093
         ST    @06,@TF00003                                     01S0093
         LA    @06,@TF00003                                     01S0093
         ST    @06,@AL00001+12                                  01S0093
         L     @06,@PC00001+8                                   01S0093
         ST    @06,@AL00001+16                                  01S0093
         L     @06,@PC00001                                     01S0093
         ST    @06,@AL00001+20                                  01S0093
         L     @15,@CV00061                                     01S0093
         LA    @01,@AL00001                                     01S0093
         BALR  @14,@15                                          01S0093
*                                      /*INVOKE SYNTAX CHECKER       */
*IF ERRCODE = 0 THEN                   /*DOES TEXT HAVE VALID SYNTAX */
         L     @06,@PC00001                                     01S0094
         LH    @06,ERRCODE(,@06)                                01S0094
         LTR   @06,@06                                          01S0094
         BNZ   @RF00094                                         01S0094
*  DO;                                 /*YES,CONTINUE VALIDITY CHECKS*/
*/********************************************************************/
*/*                                                                  */
*/* SEGMENT NAME - INCLUCHK                                          */
*/*                                                                  */
*/* DESCRIPTIVE NAME - INCLUSIVE KEY CHECK                           */
*/*                                                                  */
*/* FUNCTION - CHECK THAT ANY KEY SPECIFIED, REQUIRING THE PRESENCE  */
*/*            OF OTHER KEYS, HAS THESE OTHER KEYS SPECIFIED ALSO.   */
*/*                                                                  */
*/* OPERATION - INCLUCHK WORKS AROUND ITS OWN INTERNAL TABLE         */
*/*             (INCLUTAB) IN ITS INCLUSIVE KEY PROCESSING. EACH     */
*/*             ENTRY CONTAINS IN ORDER:                             */
*/*                                                                  */
*/*             . A NUMBER WHICH, WHEN THE KEY TABLE OF TEXT PTRS    */
*/*               IS TREATED AS AN ARRAY, SERVES AS AN INDEX         */
*/*               INTO THE ARRAY FOR A KEY THAT IS REQUIRED          */
*/*               BY ONE OR MORE DEPENDENT KEYS.                     */
*/*             . THE NUMBER OF THESE DEPENDANT KEYS                 */
*/*             . THE INDEXES OF THESE KEYS INTO THE KEY TABLE       */
*/*             (NOTE THAT THE INDEX FOR EACH KEY CORRESPONDS TO ITS */
*/*              HEX KEY VALUE AND IS THEREFORE DEPENDENT ON THE     */
*/*              RELATIONSHIP BETWEEN THE HEX VALUE FOR THE KEY AND  */
*/*              ITS POSITION IN THE KEY TABLE.)                     */
*/*                                                                  */
*/*             IF THE KEYTABLE ENTRY FOR A REQUIRED KEY IS NON-ZERO,*/
*/*             THEN THE KEY WAS SPECIFIED AND THE KEYS DEPENDENT    */
*/*             ON IT NEED NOT BE CHECKED. HOWEVER, IF THE REQUIRED  */
*/*             KEY WAS NOT SPECIFIED, EACH DEPENDENT KEY MUST ALSO  */
*/*             NOT BE SPECIFIED AND THEREFORE, MUST BE CHECKED FOR  */
*/*             AN INCLUSIVE KEY ERROR. EACH ENTRY IN INCLUTAB IS    */
*/*             PROCESSED IN THIS MANNER UNTIL EITHER THE LAST ENTRY */
*/*             IS FOUND (ZEROS IN REQUIRED KEY FIELD) AND ALL KEYS  */
*/*             ARE VALID, OR AN ERROR IS DETECTED. A CHECK IS ALSO  */
*/*             MADE TO ENSURE THAT IF NEITHER PRIMARY NOR SECONDARY */
*/*             SPACE WERE SPECIFIED, NONE OF TRK, CYL, OR AVERAGE   */
*/*             BLOCK LENGTH WERE SPECIFIED EITHER. IN ADDITION, A   */
*/*             SPECIAL CHECK IS ALSO MADE FOR THE PRIMARY SPACE KEY */
*/*             WHICH IS DEPENDENT ON THE PRESENCE OF ONE OF THESE   */
*/*             THREE KEYS. IF THIS KEY IS SPECIFIED, EITHER TRK,    */
*/*             CYL, OR AVERAGE BLOCK SIZE MUST ALSO BE SPECIFIED.   */
*/*                                                                  */
*/*             IF AN ERROR IS FOUND, AN INCLUSIVE KEY ERROR         */
*/*             CODE IS SET, THE KEY CAUSING THE ERROR IS PLACED IN  */
*/*             THE INFO CODE FIELD, AND THIS SEGMENT'S PROCESSING   */
*/*             IS COMPLETED.                                        */
*/*                                                                  */
*/* ENTRY -                                                          */
*/*                                                                  */
*/*   INPUT DATA - RSCODE - REASON CODE ERRCODE AND INFOCODE FIELDS  */
*/*                         (ZERO ON ENTRY)                          */
*/*                KEYTABLE - TABLE OF TEXT UNIT PTRS                */
*/*                                                                  */
*/* EXITS -                                                          */
*/*                                                                  */
*/*   OUTPUT DATA - ERRCODE  - SET TO RCMINCLU IF A KEY REQUIRED BY  */
*/*                            THE SPECIFICATION OF ANOTHER KEY IS   */
*/*                            NOT PRESENT.                          */
*/*                 INFOCODE - HAS SPECIFIED KEY CAUSING INCLUSIVE   */
*/*                            KEY ERROR IF ONE IS FOUND             */
*/*                            (MACRO IEFZB4DA HAS HEX VALUES FOR    */
*/*                             ERRORCODE MNEMONICS.)                */
*/*                                                                  */
*/********************************************************************/
*ENTRYPTR = ADDR(INCLUTAB);            /*POINT TO 1ST ENTRY IN TABLE */
         LA    ENTRYPTR,INCLUTAB                                02S0096
*DO WHILE (KEYINDEX^=ENDTINDX &        /*PROCESS LOOP UNTIL LAST     */
*          ERRCODE=0);                 /*ENTRY CHECKED OR ERROR FOUND*/
         B     @DE00097                                         02S0097
@DL00097 DS    0H                                               02S0098
*  IF KYTABMAP(KEYINDEX)=0 THEN        /*WAS KEY FOR INCLUSIVE TABLE
*                                        ENTRY SPECIFIED             */
         SLR   @06,@06                                          02S0098
         ICM   @06,3,KEYINDEX(ENTRYPTR)                         02S0098
         SLA   @06,2                                            02S0098
         L     @03,@PC00001+8                                   02S0098
         L     @03,KEYTABP(,@03)                                02S0098
         AL    @03,@CF03094                                     02S0098
         L     @06,KYTABMAP(@06,@03)                            02S0098
         LTR   @06,@06                                          02S0098
         BNZ   @RF00098                                         02S0098
*    DO;                               /*NO,CHECK THAT KEYS REQUIRING
*                                        THIS KEY ARE ALSO NOT SPEC'D*/
*      NUMCNT = KEYNUM;                /*INIT CNTR OF KEYS TO CHECK  */
         SLR   NUMCNT,NUMCNT                                    02S0100
         ICM   NUMCNT,3,KEYNUM(ENTRYPTR)                        02S0100
*      DO WHILE (NUMCNT>0);            /*LOOP CHECK OF KEYS          */
         B     @DE00101                                         02S0101
@DL00101 DS    0H                                               02S0102
*        IF KYTABMAP(KEYS(NUMCNT))^=0 THEN  /*IS KEY SPECIFIED THAT
*                                        REQUIRES THIS INCLUSIVE KEY */
         LR    @06,NUMCNT                                       02S0102
         ALR   @06,@06                                          02S0102
         ALR   @06,ENTRYPTR                                     02S0102
         SLR   @15,@15                                          02S0102
         ICM   @15,3,KEYS-2(@06)                                02S0102
         LR    @06,@15                                          02S0102
         SLA   @06,2                                            02S0102
         L     @14,@PC00001+8                                   02S0102
         L     @01,KEYTABP(,@14)                                02S0102
         AL    @01,@CF03094                                     02S0102
         L     @06,KYTABMAP(@06,@01)                            02S0102
         LTR   @06,@06                                          02S0102
         BZ    @RF00102                                         02S0102
*          DO;                         /*YES, INCLUSIVE KEY ERROR    */
*            ERRCODE = RCMINCLU;       /*SET INCLUSIVE KEY ERROR CODE*/
         L     @06,@PC00001                                     02S0104
         MVC   ERRCODE(2,@06),@CH02779                          02S0104
*            INFOCODE = KEYS(NUMCNT);  /*RETURN ERROR KEY            */
         STH   @15,INFOCODE(,@06)                               02S0105
*            NUMCNT = 0;               /*GET OUT OF INNER LOOP       */
         SLR   NUMCNT,NUMCNT                                    02S0106
*          END;                        /*END ERROR PROCESS           */
*        ELSE                          /*NO INCLUSIVE KEY ERROR      */
*          NUMCNT = NUMCNT-1;          /*DECREMENT CNTR OF KEYS      */
         B     @RC00102                                         02S0108
@RF00102 BCTR  NUMCNT,0                                         02S0108
*      END;                            /*END LOOP CHECK OF KEYS      */
@RC00102 DS    0H                                               02S0109
@DE00101 LTR   NUMCNT,NUMCNT                                    02S0109
         BP    @DL00101                                         02S0109
*    END;                              /*END INCLUS. TABLE KEY SPEC'D*/
*  ELSE;                               /*INCLUS. TABLE KEY SPECD, NEED
*                                        NOT CHECK KEYS REQUIRING IT */
@RF00098 DS    0H                                               02S0112
*  ENTRYPTR = ENTRYPTR +LENGTH(KEYINDEX)+                       02S0112
*             LENGTH(KEYNUM) +(KEYNUM*LENGTH(KEYS));            02S0112
         LA    @06,4                                            02S0112
         ALR   @06,ENTRYPTR                                     02S0112
         SLR   @03,@03                                          02S0112
         ICM   @03,3,KEYNUM(ENTRYPTR)                           02S0112
         ALR   @03,@03                                          02S0112
         ALR   @06,@03                                          02S0112
         LR    ENTRYPTR,@06                                     02S0112
*                                      /*POINT TO NEXT INCLUTAB ENTRY*/
*END;                                  /*END INCLUTAB CHECK LOOP     */
@DE00097 CLC   KEYINDEX(2,ENTRYPTR),ENDTINDX                    02S0113
         BE    @DC00097                                         02S0113
         L     @06,@PC00001                                     02S0113
         LH    @06,ERRCODE(,@06)                                02S0113
         LTR   @06,@06                                          02S0113
         BZ    @DL00097                                         02S0113
@DC00097 DS    0H                                               02S0114
*IF ERRCODE=0 THEN                     /*IF NO ERROR FOUND IN TABLE  */
         SLR   @06,@06                                          02S0114
         L     @15,@PC00001                                     02S0114
         CH    @06,ERRCODE(,@15)                                02S0114
         BNE   @RF00114                                         02S0114
*  IF KYTABMAP(DALPRIME)=0 &           /*IF PRIMARY SPACE NOT SPEC'D.*/
*     KYTABMAP(DALSECND)=0 THEN        /*AND SECONDARY NOT SPECIFIED */
         L     @14,@PC00001+8                                   02S0115
         L     @14,KEYTABP(,@14)                                02S0115
         C     @06,KYTABMAP+36(,@14)                            02S0115
         BNE   @RF00115                                         02S0115
         C     @06,KYTABMAP+40(,@14)                            02S0115
         BNE   @RF00115                                         02S0115
*    IF KYTABMAP(DALTRK)^=0 THEN       /*THEN IF TRK WAS SPECIFIED   */
         C     @06,KYTABMAP+24(,@14)                            02S0116
         BE    @RF00116                                         02S0116
*      DO;                             /*ERROR-TRK NEEDS ONE OF THESE*/
*        ERRCODE=RCMINCLU;             /*SET INCLUSIVE KEY ERROR CODE*/
         MVC   ERRCODE(2,@15),@CH02779                          02S0118
*        INFOCODE=DALTRK;              /*RETURN THE KEY CAUSING ERROR*/
         MVC   INFOCODE(2,@15),@CH00239                         02S0119
*      END;                            /*END IF TRK WAS SPECIFIED    */
*    ELSE                              /*ELSE TRK WAS NOT SPECIFIED  */
*      IF KYTABMAP(DALCYL)^=0 THEN     /*IF CYL WAS SPECIFIED        */
         B     @RC00116                                         02S0121
@RF00116 L     @06,@PC00001+8                                   02S0121
         L     @06,KEYTABP(,@06)                                02S0121
         L     @06,KYTABMAP+28(,@06)                            02S0121
         LTR   @06,@06                                          02S0121
         BZ    @RF00121                                         02S0121
*        DO;                           /*ERROR-CYL NEEDS ONE OF THESE*/
*          ERRCODE=RCMINCLU;           /*SET INCLUSIVE KEY ERROR CODE*/
         L     @06,@PC00001                                     02S0123
         MVC   ERRCODE(2,@06),@CH02779                          02S0123
*          INFOCODE=DALCYL;            /*RETURN THE KEY CAUSING ERROR*/
         MVC   INFOCODE(2,@06),@CH00192                         02S0124
*        END;                          /*END IF CYL WAS SPECIFIED    */
*      ELSE                            /*ELSE CYL WAS NOT SPECIFIED  */
*        IF KYTABMAP(DALBLKLN)^=0 THEN /*IF BLOCK LENGTH IS SPECIFIED*/
         B     @RC00121                                         02S0126
@RF00121 L     @06,@PC00001+8                                   02S0126
         L     @06,KEYTABP(,@06)                                02S0126
         L     @06,KYTABMAP+32(,@06)                            02S0126
         LTR   @06,@06                                          02S0126
         BZ    @RF00126                                         02S0126
*          DO;                         /*ERROR-IT NEEDS ONE OF THESE */
*            ERRCODE=RCMINCLU;         /*SET INCLUSIVE KEY ERROR CODE*/
         L     @06,@PC00001                                     02S0128
         MVC   ERRCODE(2,@06),@CH02779                          02S0128
*            INFOCODE=DALBLKLN;        /*RETURN THE KEY CAUSING ERROR*/
         MVC   INFOCODE(2,@06),@CH01186                         02S0129
*          END;                        /*END IF BLOCK LENGTH SPEC'D. */
*        ELSE;                         /*ELSE BLOCK LENGTH NOT SPEC'D*/
@RF00126 DS    0H                                               02S0132
*  ELSE;                               /*ELSE PRIMARY OR SECONDARY
*                                        WAS SPEC'D.-CHECK NOT NEEDED*/
@RF00115 DS    0H                                               02S0133
*ELSE;                                 /*ELSE ERROR FOUND BY TABLE   */
@RF00114 DS    0H                                               02S0134
*IF ERRCODE=0 THEN                     /*IF NO ERROR FOUND ABOVE     */
@RC00114 SLR   @06,@06                                          02S0134
         L     @15,@PC00001                                     02S0134
         CH    @06,ERRCODE(,@15)                                02S0134
         BNE   @RF00134                                         02S0134
*  IF KYTABMAP(DALPRIME)^=0 THEN       /*WAS PRIMARY SPACE SPECIFIED */
         L     @14,@PC00001+8                                   02S0135
         L     @14,KEYTABP(,@14)                                02S0135
         C     @06,KYTABMAP+36(,@14)                            02S0135
         BE    @RF00135                                         02S0135
*    IF KYTABMAP(DALTRK)=0 &           /*YES, WAS EITHER TRK OR      */
*       KYTABMAP(DALCYL)=0 &           /*CYL, OR                     */
*       KYTABMAP(DALBLKLN)=0 THEN      /*AVERAGE BLK SIZE SPECIFIED ?*/
         C     @06,KYTABMAP+24(,@14)                            02S0136
         BNE   @RF00136                                         02S0136
         C     @06,KYTABMAP+28(,@14)                            02S0136
         BNE   @RF00136                                         02S0136
         C     @06,KYTABMAP+32(,@14)                            02S0136
         BNE   @RF00136                                         02S0136
*         DO;                          /*NO, INCLUSIVE KEY ERROR     */
*           ERRCODE = RCMINCLU;        /*INCLUSIVE KEY ERROR CODE    */
         MVC   ERRCODE(2,@15),@CH02779                          02S0138
*           INFOCODE = DALPRIME;       /*RETURN ERROR KEY            */
         MVC   INFOCODE(2,@15),@CH01187                         02S0139
*         END;                         /*END ERROR PROCESS           */
*    ELSE;                             /*ONE OF THREE WAS SPECIFIED  */
@RF00136 DS    0H                                               02S0142
*  ELSE;                               /*PRIMARY SPACE NOT SPECIFIED */
@RF00135 DS    0H                                               02S0143
*ELSE;                                 /*ERROR ALREADY FOUND         */
@RF00134 DS    0H                                               02S0144
*    IF ERRCODE=0 THEN                 /*NECESSARY KEYS PRESENT?     */
         L     @06,@PC00001                                     01S0144
         LH    @06,ERRCODE(,@06)                                01S0144
         LTR   @06,@06                                          01S0144
         BNZ   @RF00144                                         01S0144
*      DO;                             /*YES,CONTINUE VALIDITY CHECKS*/
*/********************************************************************/
*/*                                                                  */
*/* SEGMENT NAME - VALUECHK                                          */
*/*                                                                  */
*/* DESCRIPTIVE NAME - CHECK FOR SPECIFIC VALID PARM VALUES          */
*/*                                                                  */
*/* FUNCTION - THIS SEGMENT CHECKS THAT VALID PARM VALUES WERE       */
*/*            ENTERED FOR THOSE KEYS SPECIFIED WHICH ALLOW ONLY A   */
*/*            CERTAIN NUMBER OF POSSIBLE VALID VALUES               */
*/*                                                                  */
*/* OPERATION - VALUECHK IS DRIVEN BY VALUETAB, A TABLE WITH ENTRIES */
*/*             FOR EACH KEY ALLOWING ONLY ONE OF N POSSIBLE PARM    */
*/*             VALUES. FOR EACH KEY IN THIS TABLE, A CHECK IS MADE  */
*/*             OF THE KEY TABLE OF TEXT POINTERS TO SEE IF THIS KEY */
*/*             WAS SPECIFIED. (A SPECIFIED KEY HAS A NON-ZERO TEXT  */
*/*             PTR. IN THE KEY TABLE.) IF THE KEY WAS NOT SPECIFIED,*/
*/*             THE NEXT VALUETAB ENTRY IS LOOKED AT. IF THE KEY WAS */
*/*             SPECIFIED, THE PARM FIELD OF THE TEXT UNIT IS CHECKED*/
*/*             AGAINST THE POSSIBLE VALID VALUES FOR THIS KEY, AS   */
*/*             LISTED IN THE VALUETAB ENTRY. IF THE PARM IS VALID,  */
*/*             THE NEXT ENTRY IN VALUETAB IS LOOKED AT, IF INVALID, */
*/*             AN ERROR CODE IS RETURNED, ALONG WITH THE            */
*/*             INVALID KEY. THIS PROCESSING IS CONTINUED UNTIL      */
*/*             EITHER AN ERROR HAS BEEN FOUND, OR ALL SPECIFIED KEYS*/
*/*             WITH ENTRIES IN VALUETAB HAVE BEEN CHECKED AND HAVE  */
*/*             VALID PARMS. (NOTE: THE LENGTH FIELD OF THE TEXT UNIT*/
*/*             IS USED IN THE PARM FIELD COMPARES, AND THIS SEGMENT */
*/*             ASSUMES VALID LENGTHS HAVE BEEN DETERMINED PRIOR TO  */
*/*             THIS POINT BY THE SYNTAX CHECKER.)                   */
*/*                                                                  */
*/* ENTRY -                                                          */
*/*                                                                  */
*/*   INPUT DATA - RSCODE   - REASON CODE WITH ERROR AND INFO        */
*/*                           CODE FIELDS. (ZERO ON ENTRY)           */
*/*                KEYTABLE - TABLE OF TEXT UNIT PTRS                */
*/*                                                                  */
*/* EXIT -                                                           */
*/*                                                                  */
*/*   OUTPUT DATA - ERRCODE  - SET TO RCINPARM IF AN INVALID PARM    */
*/*                            VALUE WAS SPECIFIED.                  */
*/*                 INFOCODE - KEY WITH INVALID PARM VALUE IF        */
*/*                            ONE IS FOUND                          */
*/*                            (MACRO IEFZB4DA HAS HEX VALUES FOR    */
*/*                             ERRCODE MNEMONICS.)                  */
*/*                                                                  */
*/********************************************************************/
*RFY TEXTUNIT BASED(UNITPTR);          /*RESET TEXT UNIT BASE PTR    */
*ENTRYPTR=ADDR(VALUETAB);              /*POINT TO 1ST VAL TABLE ENTRY*/
         LA    ENTRYPTR,VALUETAB                                02S0147
*DO WHILE (VTABKEY ^= VLASTKEY  &      /*PROCESS LOOP UNTIL LAST     */
*          ERRCODE = 0);               /*ENTRY CHECKED OR ERROR FOUND*/
         B     @DE00148                                         02S0148
@DL00148 DS    0H                                               02S0149
*  IF KYTABMAP(VTABKEY) ^= 0 THEN      /*WAS KEY IN TABLE ENTRY SPECD*/
         SLR   @06,@06                                          02S0149
         ICM   @06,3,VTABKEY(ENTRYPTR)                          02S0149
         SLA   @06,2                                            02S0149
         L     @03,@PC00001+8                                   02S0149
         L     @03,KEYTABP(,@03)                                02S0149
         AL    @03,@CF03094                                     02S0149
         L     @06,KYTABMAP(@06,@03)                            02S0149
         LTR   @06,@06                                          02S0149
         BZ    @RF00149                                         02S0149
*    DO;                               /*YES,CHECK VALID PARM VALUES */
*      UNITPTR=KYTABMAP(VTABKEY);      /*POINT TO KEY'S  TEXT UNIT   */
         LR    UNITPTR,@06                                      02S0151
*      FOUND='0'B;                     /*INIT LOOP CTL SWITCH        */
         NI    FOUND,B'01111111'                                02S0152
*      NUMCNT=1;                       /*INIT LOOP CTL COUNTER       */
         LA    NUMCNT,1                                         02S0153
*      VALUEPTR=ADDR(VTABVALS);        /*POINT TO 1ST VALID VALUE    */
         LA    VALUEPTR,VTABVALS(,ENTRYPTR)                     02S0154
*      DO WHILE (NUMCNT <= VTABNUM &   /*CHECK TILL LAST VALID VALUE */
*                FOUND = '0'B);        /*OR TILL MATCH IS FOUND      */
         B     @DE00155                                         02S0155
@DL00155 DS    0H                                               02S0156
*        IF TEXTUPAR(1:TEXTULNG) ^= KEYVALS(1:TEXTULNG) THEN    02S0156
         ICM   @06,12,TEXTULNG(UNITPTR)                         02S0156
         SRA   @06,16                                           02S0156
         LR    @05,@06                                          02S0156
         BCTR  @05,0                                            02S0156
         EX    @05,@SC03096                                     02S0156
         BE    @RF00156                                         02S0156
*                                      /*SPECD PARM = THIS VALID ONE?*/
*          DO;                         /*NO,CHECK NEXT VALID VALUE   */
*            NUMCNT=NUMCNT+1;          /*INCREMENT VALID PARMS CNTR  */
         AL    NUMCNT,@CF00063                                  02S0158
