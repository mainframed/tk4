*IF COPYDSAB->DSABSIOT->SIOTDMND=ON THEN /*COPIED DD DEMAND REQ 02S0170
*                                                            @YM06172*/
         TM    SIOTDMND(@06),B'10000000'                        02S0170
         BNO   @RF00170                                         02S0170
*  DO;                                   /*YES,CHECK OPT BITS@YM06172*/
*    IF SIO3DACC = ON THEN             /*DIRECT ACCESS DEVICE@YM05476*/
         TM    SIO3DACC(@08),B'00100000'                        02S0172
         BNO   @RF00172                                         02S0172
*      DO;                             /*TURN OFF OPTION BITS@YM05476*/
