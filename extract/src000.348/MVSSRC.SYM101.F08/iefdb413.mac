         NI    SIOUBYT2(@08),X'DC'                              02S0174
*        IF SIOUBYT4 = DA3340 THEN     /*IF A 3340 DEVICE    @YM05476*/
         CLI   SIOUBYT4(@08),X'0A'                              02S0175
         BNE   @RF00175                                         02S0175
*          SIOUBYT2 = SIOUBYT2 & RPSOFF; /*RPS BIT OFF ALSO  @YM05476*/
         NI    SIOUBYT2(@08),X'EF'                              02S0176
*      END;                            /*END IF DIRECT ACCESS@YM05476*/
@RF00175 DS    0H                                               02S0178
*    IF SIO3TAPE = ON THEN             /*TAPE DEVICE         @YM05477*/
@RF00172 L     @08,@PC00001+24                                  02S0178
         L     @08,FSIOTPTR(,@08)                               02S0178
         TM    SIO3TAPE(@08),B'10000000'                        02S0178
         BNO   @RF00178                                         02S0178
*      SIOUBYT2 = SIOUBYT2 & OPTIONS;  /*OPTIONS BITS OFF    @ZA04104*/
         NI    SIOUBYT2(@08),X'F4'                              02S0179
*    IF SIO3UREC = ON THEN             /*UNIT RECORD DEVICE  @YM05476*/
@RF00178 L     @08,@PC00001+24                                  02S0180
         L     @08,FSIOTPTR(,@08)                               02S0180
