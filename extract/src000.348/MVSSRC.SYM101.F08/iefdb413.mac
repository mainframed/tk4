*/* FUNCTION - INTERFACE TO SMF TCTIOT UPDATE ROUTINE.               */
*/*                                                                  */
*/*   OPERATION -                                                    */
*/*     IF MORE THAN ONE SIOT IS ASSOCIATED WITH THE DYNAMIC         */
*/*     ALLOCATION REQUEST (USUAL CASE), CORE FROM THE PL/S DSECT    */
*/*     IS USED FOR A PARM LIST TO THE TCTIOT UPDATE ROUTINE,        */
*/*     IEFDB4F9. OTHERWISE, CORE FOR THE PARM LIST IS OBTAINED      */
*/*     VIA THE GETMAIN MACRO. THE PARM LIST IS SET UP AND THE       */
*/*     TCTIOT UPDATE ROUTINE IS INVOKED. IF CORE WAS GETMAIN'D,     */
*/*     THE FREEMAIN MACRO IS USED TO RELEASE THE CORE USED FOR      */
*/*     THE PARM LIST.                                               */
*/*                                                                  */
*/* INPUT -                                                          */
*/*   COUNTSIO - NUMBER OF SIOTS ASSOCIATED WITH THE REQUEST.        */
*/*                                                                  */
*/********************************************************************/
*                                                               03S0587
*IF COUNTSIO = 1 THEN                  /*ONLY ONE SIOT TO PROCESS?   */
         C     COUNTSIO,@CF00103                                03S0587
         BNE   @RF00587                                         03S0587
*  SMFLISTP = ADDR(SMFCORE);           /*YES,USE PLS CORE            */
         LA    SMFLISTP,SMFCORE                                 03S0588
*ELSE                                  /*NO,MORE THAN ONE SIOT       */
*  DO;                                 /*GET CORE FOR PARM LIST      */
         B     @RC00587                                         03S0589
@RF00587 DS    0H                                               03S0590
*    SMFLSTLN = COUNTSIO*LENGTH(SMFLSTEL(1)); /*AMOUNT OF CORE       */
         LR    SMFLSTLN,COUNTSIO                                03S0590
         MH    SMFLSTLN,@CH00493                                03S0590
*    R0 = SMFLSTLN;                    /*SET REGISTER FOR GETMAIN    */
         LR    R0,SMFLSTLN                                      03S0591
*    R0 = R0 × SUBPOLNO;               /*GET FROM CORRECT SUBPOOL    */
         O     R0,@CF00119                                      03S0592
*    GEN (GETMAIN R,LV=(0));           /*GET CORE                    */
         GETMAIN R,LV=(0)
*    SMFLISTP = R1;                    /*SET PTR TO CORE             */
         LR    SMFLISTP,R1                                      03S0594
*  END;                                                         03S0595
*WRKSIOTP = FSIOTPTR;                  /*WORK PTR TO LOOP THRU SIOT'S*/
@RC00587 L     @10,@PC00001+24                                  03S0596
         L     WRKSIOTP,FSIOTPTR(,@10)                          03S0596
*RFY INDMSIOT BASED(WRKSIOTP);         /*BASE FOR SIOT MAPPING MACRO */
*DO I = 1 TO COUNTSIO;                 /*PROCESS SIOT'S JUST ALLOC'D */
         LA    I,1                                              03S0598
         B     @DE00598                                         03S0598
@DL00598 DS    0H                                               03S0599
*  SMFLSTEL(I) = SMFLSTEL(I) && SMFLSTEL(I); /*CLEAR PARMLIST ELEMENT*/
         LR    @10,I                                            03S0599
         MH    @10,@CH00493                                     03S0599
         ST    @10,@TF00001                                     03S0599
         ALR   @10,SMFLISTP                                     03S0599
         AL    @10,@CF03524                                     03S0599
         XC    SMFLSTEL(5,@10),SMFLSTEL(@10)                    03S0599
*  SMFTIOTP(I) = DSABTIOT;             /*SET TIOT ENTRY PTR          */
         L     @10,SIOTETIO(,WRKSIOTP)                          03S0600
         L     @08,DSABTIOT(,@10)                               03S0600
         LR    @04,SMFLISTP                                     03S0600
         AL    @04,@TF00001                                     03S0600
         AL    @04,@CF03524                                     03S0600
         STCM  @08,15,SMFTIOTP(@04)                             03S0600
*  IF DSABVAM=ON THEN                  /*IF DSAB IS VAM THEN @ZA02961*/
         TM    DSABVAM(@10),B'00100000'                         03S0601
         BNO   @RF00601                                         03S0601
*    SMFVAM=ON;                        /*VAM ON IN SMFLIST   @ZA02961*/
         OI    SMFVAM(SMFLISTP),B'01000000'                     03S0602
*  WRKSIOTP = SIOTNPTR;                /*GET NEXT SIOT ADDRESS       */
@RF00601 L     WRKSIOTP,SIOTNPTR(,WRKSIOTP)                     03S0603
*END;                                                           03S0604
         AL    I,@CF00103                                       03S0604
@DE00598 CR    I,COUNTSIO                                       03S0604
         BNH   @DL00598                                         03S0604
*SMFLAST(COUNTSIO) = ON;               /*SET LAST ELEMENT INDICATOR  */
         LR    @10,COUNTSIO                                     03S0605
         MH    @10,@CH00493                                     03S0605
         ALR   @10,SMFLISTP                                     03S0605
         BCTR  @10,0                                            03S0605
         OI    SMFLAST-4(@10),B'10000000'                       03S0605
*RFY INDMSIOT BASED(FSIOTPTR);         /*RESET SIOT MAPPING BASE     */
*CALL IEFDB4F9 (VERBCODE,TCBPTR,SMFLIST,DEPARMS,JSCDSABQ,SMFPARMS);
         LA    @10,@CB00392                                     03S0607
         ST    @10,@AL00001                                     03S0607
         L     @10,@PC00001+4                                   03S0607
         LA    @08,TCBPTR(,@10)                                 03S0607
         ST    @08,@AL00001+4                                   03S0607
         ST    SMFLISTP,@AL00001+8                              03S0607
         L     @08,@PC00001+24                                  03S0607
         ST    @08,@AL00001+12                                  03S0607
         L     @10,JSCBPTR(,@10)                                03S0607
         LA    @10,JSCDSABQ(,@10)                               03S0607
         ST    @10,@AL00001+16                                  03S0607
         L     @10,@PC00001+28                                  03S0607
         ST    @10,@AL00001+20                                  03S0607
         L     @15,@CV00389                                     03S0607
         LA    @01,@AL00001                                     03S0607
         BALR  @14,@15                                          03S0607
*                                      /*SMF ROUTINE         @ZA02706*/
*IF SMFLISTP ^= ADDR(SMFCORE) THEN     /*WAS CORE GETMAIN'D FOR PARMS*/
         LA    @10,SMFCORE                                      03S0608
         CR    SMFLISTP,@10                                     03S0608
         BE    @RF00608                                         03S0608
*  DO;                                 /*YES,FREE IT                 */
*    R0 = SMFLSTLN;                    /*REGISTER FOR FREEMAIN       */
         LR    R0,SMFLSTLN                                      03S0610
*    R0 = R0 × SUBPOLNO;               /*FREE FROM CORRECT SUBPOOL   */
         O     R0,@CF00119                                      03S0611
*    R1 = SMFLISTP;                    /*REGISTER FOR FREEMAIN       */
         LR    R1,SMFLISTP                                      03S0612
*    GEN (FREEMAIN R,LV=(0),A=(1));    /*FREE PARM LIST CORE         */
         FREEMAIN R,LV=(0),A=(1)
*  END;                                                         03S0614
*  END;                                                         02S0615
@RF00608 DS    0H                                               02S0616
*  END;                                                         01S0616
@RF00520 DS    0H                                               01S0617
*IF ERRCODE^=ZERO THEN                 /*ANY ERROR?                  */
@RF00443 L     @10,@PC00001+16                                  01S0617
         ICM   @08,3,ERRCODE(@10)                               01S0617
         BZ    @RF00617                                         01S0617
*  CALL IEFDB418 (DEPARMS);            /*YES,DO ERROR CLEAN-UP       */
         L     @10,@PC00001+24                                  01S0618
         ST    @10,@AL00001                                     01S0618
         L     @15,@CV00101                                     01S0618
         LA    @01,@AL00001                                     01S0618
         BALR  @14,@15                                          01S0618
*RETURN CODE(ZERO);                    /*RETURN CODE OF ZERO         */
@RF00617 L     @13,4(,@13)                                      01S0619
         L     @00,@SIZDATD                                     01S0619
         LR    @01,@11                                          01S0619
         FSPACE   R,LV=(0),A=(1)
         SLR   @15,@15                                          01S0619
         L     @14,12(,@13)                                     01S0619
         LM    @00,@12,20(@13)                                  01S0619
         BR    @14                                              01S0619
*/********************************************************************/
*/*                                                                  */
*/* SEGMENT NAME - SETRCODE                                          */
*/*                                                                  */
*/* DESCRIPTIVE NAME - SET DYNAMIC ALLOC ERROR REASON CODE           */
*/*                                                                  */
*/* FUNCTION - TRANSLATE THE REASON CODES FROM JFCB HOUSEKEEPING AND */
*/*   COMMON ALLOCATION INTO A DYNAMIC ALLOCATION ERROR REASON CODE. */
*/*                                                                  */
*/*   OPERATION - IF A NON-SIOT RELATED REASON CODE IS RETURNED FROM */
*/*     HOUSEKEEPING OR ALLOCATION, SET THE DYNAMIC ALLOCATION REASON*/
*/*     CODE AS DEFINED BY THE ARRAY, NSRSNCOD. IF A         @G29AN2F*/
*/*     SUBSYSTEM RETURNED AN INFORMATION REASON CODE IN THE @G29AN2F*/
*/*     SUBSYSTEM ERROR BLOCK, THE INFO CODE FROM THE        @G29AN2F*/
*/*     SUBSYSTEM ERROR BLOCK IS PLACED IN THE DYNAMIC INFO  @G29AN2F*/
*/*     REASON CODE FIELD.   FOR SIOT RELATED                @G29AN2F*/
*/*     REASON CODES, IF THE REASON CODE IS 'INVALID GDG SINGLE      */
*/*     RELATIVE NUMBER', SET THE DYNAMIC ERROR CODE TO THE VALUE FOR*/
*/*     'INVALID TEXT UNIT PARM', AND SET THE DYNAMIC INFO REASON    */
*/*     TO THE VALUE FOR MEMBER NAME KEY. (THE INFO CODE IS SET TO BE*/
*/*     CONSISTANT WITH THE WAY THE 'INVALID TEXT UNIT PARM' REASON  */
*/*     CODE IS RETURNED BY IEFDB412.) OTHERWISE USE THE ARRAY OF    */
*/*     SIOT RELATED REASON CODES, SIRSNCOD, TO SET THE DYNAMIC ERROR*/
*/*     REASON CODE. IF THE SIOT IS FOR A SUBSYSTEM DATA SET @G29AN2F*/
*/*     THE DYNAMIC INFO REASON CODE IS SET WITH THE VALUE   @G29AN2F*/
*/*     OR THE INDICATOR FOR AN INFORMATION CODE IS ON,      @G24LPSJ*/
*/*     OF FIELD SIOTSSIC.                                   @G29AN2F*/
*/*                                                                  */
*/* INPUT -                                                          */
*/*   NSIOTERR - NON-SIOT RELATED REASON CODE FROM HOUSEKEEPING OR   */
*/*              ALLOCATION                                          */
*/*   FSIOTPTR - PTR TO FIRST SIOT OF REQUEST                        */
*/*                                                                  */
*/* OUTPUT -                                                         */
*/*   ERRCODE/INFOCODE - DYNAMIC ALLOC REASON CODES FILLED IN        */
*/*                                                                  */
*/********************************************************************/
*SETRCODE: PROC;                                                02S0620
@EL00001 L     @13,4(,@13)                                      02S0620
@EF00001 L     @00,@SIZDATD                                     02S0620
         LR    @01,@11                                          02S0620
         FSPACE   R,LV=(0),A=(1)
@ER00001 LM    @14,@12,12(@13)                                  02S0620
         BR    @14                                              02S0620
SETRCODE STM   @14,@12,12(@13)                                  02S0620
*IF NSIOTERR ^= ZERO THEN              /*NON-SIOT RELATED ERROR?     */
         LH    @10,NSIOTERR                                     02S0621
         SLR   @08,@08                                          02S0621
         CR    @10,@08                                          02S0621
         BE    @RF00621                                         02S0621
*  DO;                                                          02S0622
*    ERRCODE = NSRSNCOD(NSIOTERR);     /*YES,SET REASON CODE         */
         L     @15,@PC00001+16                                  02S0623
         ALR   @10,@10                                          02S0623
         LA    @14,NSRSNCOD-2(@10)                              02S0623
         MVC   ERRCODE(2,@15),0(@14)                            02S0623
*    IF SSEINCDE ^= ZERO THEN          /*DID A SUBSYSTEM RETURN AN
*                                        INFO CODE ?         @G29AN2F*/
         LH    @10,SSEINCDE                                     02S0624
         CR    @10,@08                                          02S0624
         BE    @RF00624                                         02S0624
*      INFOCODE = SSEINCDE;            /*YES, RETURN IT      @G29AN2F*/
         STCM  @10,3,INFOCODE(@15)                              02S0625
*  END;                                                         02S0626
*ELSE                                  /*NO,ERROR FOR PARTICULAR SIOT*/
*  IF SIOTRSNC = 52 THEN               /*GDG SINGLE REL # ERROR?     */
         B     @RC00621                                         02S0627
@RF00621 L     @10,@PC00001+24                                  02S0627
         L     @10,FSIOTPTR(,@10)                               02S0627
         CLC   SIOTRSNC(2,@10),@CH03492                         02S0627
         BNE   @RF00627                                         02S0627
*    DO;                                                        02S0628
*      ERRCODE = RCINPARM;             /*YES,SET BAD PARM VALUE CODE */
         L     @10,@PC00001+16                                  02S0629
         MVC   ERRCODE(2,@10),@CB01048                          02S0629
*      INFOCODE = DALMEMBR;            /*MEMBER KEY CAUSED ERROR     */
         MVC   INFOCODE(2,@10),@CB00692                         02S0630
*    END;                                                       02S0631
*  ELSE                                /*NOT GDG SINGLE REL # ERROR  */
*    DO;                                                        02S0632
         B     @RC00627                                         02S0632
@RF00627 DS    0H                                               02S0633
*      ERRSIOTP = FSIOTPTR;            /*INIT PTR TO SIOT WITH ERROR */
         L     @10,@PC00001+24                                  02S0633
         L     ERRSIOTP,FSIOTPTR(,@10)                          02S0633
*      DO WHILE(ERRSIOTP->SIOTRSNC = ZERO); /*FIND SIOT WITH ERROR   */
         B     @DE00634                                         02S0634
@DL00634 DS    0H                                               02S0635
*        ERRSIOTP = ERRSIOTP->SIOTNPTR; /*NOT ERR SIOT,GET NEXT SIOT */
         L     ERRSIOTP,SIOTNPTR(,ERRSIOTP)                     02S0635
*      END;                                                     02S0636
@DE00634 LH    @10,SIOTRSNC(,ERRSIOTP)                          02S0636
         LTR   @10,@10                                          02S0636
         BZ    @DL00634                                         02S0636
*      ERRCODE = SIRSNCOD(ERRSIOTP->SIOTRSNC); /*SET DYN ERROR CODE  */
         L     @08,@PC00001+16                                  02S0637
         ALR   @10,@10                                          02S0637
         LA    @03,SIRSNCOD-2(@10)                              02S0637
         MVC   ERRCODE(2,@08),0(@03)                            02S0637
*      IF ERRSIOTP->SIOTSSGP = ON ×    /*WAS THE REQUEST FOR    02S0638
*                                        SSNAME KEY?         @G24LPSJ*/
*         ERRSIOTP->SIOTINFC = ON THEN /* OR IF THERE IS AN INFO
*                                         CODE               @G24LPSJ*/
         TM    SIOTSSGP(ERRSIOTP),B'10010000'                   02S0638
         BZ    @RF00638                                         02S0638
*        INFOCODE = SIOTSSIC;          /*YES, SET INFO CODE  @G29AN2F*/
         L     @10,@PC00001+24                                  02S0639
         L     @10,FSIOTPTR(,@10)                               02S0639
         MVC   INFOCODE(2,@08),SIOTSSIC(@10)                    02S0639
*    END;                                                       02S0640
*END SETRCODE;                                                  02S0641
@EL00002 DS    0H                                               02S0641
@EF00002 DS    0H                                               02S0641
@ER00002 LM    @14,@12,12(@13)                                  02S0641
         BR    @14                                              02S0641
*END IEFDB413                                                   01S0642
