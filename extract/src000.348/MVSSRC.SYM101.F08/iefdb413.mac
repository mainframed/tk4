*TIOMCCAT = ON;                        /*INDICATE CONCAT FUNCTION    */
         OI    TIOMCCAT(@14),B'00000100'                        03S0408
*TIOMJSCP = JSCBPTR;                   /*SET JSCB PTR                */
         L     @08,@PC00001+4                                   03S0409
         L     @08,JSCBPTR(,@08)                                03S0409
         ST    @08,TIOMJSCP(,@14)                               03S0409
*CCATSIOT = FSIOTPTR;                  /*INIT WORK SIOT PTR          */
         LR    CCATSIOT,@06                                     03S0410
*DO WHILE(CCATSIOT ^= ZERO);           /*LOOP THRU SIOTS             */
         B     @DE00411                                         03S0411
@DL00411 DS    0H                                               03S0412
*  TIOMNDSA = TIOMNDSA + 1;            /*# OF DSABS TO CONCATENATE   */
