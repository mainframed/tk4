*      ELSE                            /*3211 UNIT RECORD DEV@YM05476*/
*        SIOUBYT2 = SIOUBYT2 & UCSOPMSK; /*ALLOW UCS BIT     @YM05476*/
         B     @RC00181                                         02S0183
@RF00181 L     @08,@PC00001+24                                  02S0183
         L     @08,FSIOTPTR(,@08)                               02S0183
         NI    SIOUBYT2(@08),X'80'                              02S0183
