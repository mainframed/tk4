*/*     INVOKE CATALOG MANAGEMENT. IF AN ERROR OCCURS DURING COMMON  */
*/*     UNALLOCATION'S PROCESSING OR CATALOG MANAGEMENT'S PROCESSING,*/
*/*     A DYNAMIC ALLOCATION ERROR REASON CODE IS SET. IF SUCCESSFUL */
*/*     A BIT IS SET TO INDICATE TO COMMON UNALLOCATION, WHEN IT IS  */
*/*     CALLED FROM EITHER DYNAMIC OR STEP UNALLOCATION TO UNALLOCATE*/
*/*     THE DATA SET, THAT THE DATA SET HAS BEEN CATALOGED AT        */
*/*     ALLOCATION AND TO UNCATALOG THE DATA SET IF THE DATA SET IS  */
*/*     DELETED.                                                     */
*/*                                                                  */
*/********************************************************************/
*                                                               03S0491
*RFY CMUNFMAP BASED(ADDR(UNALFMAP));   /*BASE FOR UNALLOC FUNC. MAP  */
*CMUNFMAP = CMUNFMAP && CMUNFMAP;      /*CLEAR FUNCTION MAP          */
         LA    @15,UNALFMAP                                     03S0492
         XC    CMUNFMAP(4,@15),CMUNFMAP(@15)                    03S0492
*CMDISPRC = ON;                        /*INDICATE TO PROCESS DISP    */
         OI    CMDISPRC(@15),B'00100000'                        03S0493
*CMTIOTNQ = ON;                        /*INDICATE TIOT ENQ'D ON      */
         OI    CMTIOTNQ(@15),B'00000001'                        03S0494
*RFY CMUNRB BASED(ADDR(UNALREQB));     /*COMMON UNALLOC REQ BLK BASE */
*CMUNRB = CMUNRB && CMUNRB;            /*CLEAR REQUEST BLOCK         */
         LA    @14,UNALREQB                                     03S0496
         XC    CMUNRB(16,@14),CMUNRB(@14)                       03S0496
*CMSIOPTR = FSIOTPTR;                  /*SET PTR TO SIOT             */
         ST    @08,CMSIOPTR(,@14)                               03S0497
*CMNMDISP = ON;                        /*INDICATE TO USE NORMAL DISP */
         OI    CMNMDISP(@14),B'01000000'                        03S0498
*UNALCARY = UNALCARY && UNALCARY;      /*CLEAR RETURN CODE ARRAY     */
         XC    UNALCARY(8),UNALCARY                             03S0499
*CALL IEFAB4A0                         /*CATALOG BY COMMON UNALLOC   */
*              (CMUNFMAP,ADDR(CMUNRB),JSCBPTR,0,0,              03S0500
*               0,0,UNALCARY,0);       /*                    @Z40LPPK*/
         MVC   @AL00001+12(24),@AL00500                         03S0500
         ST    @15,@AL00001                                     03S0500
         ST    @14,@TF00001                                     03S0500
         LA    @08,@TF00001                                     03S0500
         ST    @08,@AL00001+4                                   03S0500
         L     @08,@PC00001+4                                   03S0500
         ST    @08,@AL00001+8                                   03S0500
         LA    @08,UNALCARY                                     03S0500
         ST    @08,@AL00001+28                                  03S0500
         L     @15,@CV00387                                     03S0500
         LA    @01,@AL00001                                     03S0500
         BALR  @14,@15                                          03S0500
*IF RSNCDARY(4) ^= ZERO THEN           /*ESTAE ERROR?                */
         ICM   @08,3,RSNCDARY+6                                 03S0501
         BZ    @RF00501                                         03S0501
*  ERRCODE = RCESTAER;                 /*YES, SET REASON CODE        */
         L     @08,@PC00001+16                                  03S0502
         MVC   ERRCODE(2,@08),@CB01112                          03S0502
*ELSE                                  /*OR                          */
*  IF CMINTERR = ON THEN               /*NO,NO CORE FOR VOL LIST ERR?*/
         B     @RC00501                                         03S0503
@RF00501 LA    @08,UNALREQB                                     03S0503
         TM    CMINTERR(@08),B'00010000'                        03S0503
         BNO   @RF00503                                         03S0503
*    ERRCODE = RCNOCORE;               /*YES, SET REASON CODE        */
         L     @08,@PC00001+16                                  03S0504
         MVC   ERRCODE(2,@08),@CB01027                          03S0504
*  ELSE                                /*OR                          */
*    IF CMCATERR = ON THEN             /*NO, ERROR FROM CATALOG?     */
         B     @RC00503                                         03S0505
@RF00503 LA    @08,UNALREQB                                     03S0505
         TM    CMCATERR(@08),B'01000000'                        03S0505
         BNO   @RF00505                                         03S0505
*      ERRCODE = RCCTLGER × CMRETCDE;  /*YES, SET REASON CODE        */
         L     @15,@PC00001+16                                  03S0506
         L     @14,@CF01158                                     03S0506
         LH    @08,CMRETCDE(,@08)                               03S0506
         OR    @14,@08                                          03S0506
         STCM  @14,3,ERRCODE(@15)                               03S0506
*    ELSE                              /*NO, CATALOG WAS SUCCESSFUL  */
*      DO;                                                      03S0507
         B     @RC00505                                         03S0507
@RF00505 DS    0H                                               03S0508
*        JFCCAT = ON;                  /*INDICATE TO UNCAT IF DELETE */
         L     @08,@PC00001+24                                  03S0508
         L     @08,FSIOTPTR(,@08)                               03S0508
         L     @01,SJFCBPTR(,@08)                               03S0508
         OI    JFCCAT(@01),B'10000000'                          03S0508
*        SIOTCALC = ON;                /*INDICATE CATALOGED AT ALLOC */
         OI    SIOTCALC(@08),B'00010000'                        03S0509
*      END;                                                     03S0510
*  END;                                                         02S0511
*                                                               02S0511
@RC00505 DS    0H                                               02S0511
@RC00503 DS    0H                                               02S0511
@RC00501 DS    0H                                               02S0512
*IF NQEXCLSW = ON                      /*SHR TO EXCL ENQ TO BE DONE? */
*  & ERRCODE = ZERO THEN               /*AND NO ERROR ABOVE?         */
@RF00489 L     @08,@PC00001+20                                  02S0512
         TM    NQEXCLSW(@08),B'00001000'                        02S0512
         BNO   @RF00512                                         02S0512
         L     @08,@PC00001+16                                  02S0512
         ICM   @15,3,ERRCODE(@08)                               02S0512
         BNZ   @RF00512                                         02S0512
*  DO;                                                          02S0513
*    DSRRFMAP = DSRRFMAP && DSRRFMAP;  /*YES,CLEAR DSENQ RTN FLAGS   */
         XC    DSRRFMAP(2),DSRRFMAP                             02S0514
*    ADDRPASS = ON;                    /*ADDRESS OF DSNQ ENTRY PASSED*/
*    ADCHENQ  = ON;                    /*CHANGE ENTRY AND ENQ        */
*    ENQEXCL  = ON;                    /*EXCLUSIVE USE               */
         OI    ADDRPASS,B'10100001'                             02S0517
*    CALL IEFAB4DC                     /*INVOKE DSENQ ROUTINE@YM03580*/
*                  (DSRRFMAP,0,0,LASTEPAP,ENQTCBP,SCTSNUMB,NQRETPS,
*                   0,REASONCD,0);                              02S0518
         LA    @15,DSRRFMAP                                     02S0518
         ST    @15,@AL00001                                     02S0518
         LA    @15,@CF00125                                     02S0518
         ST    @15,@AL00001+4                                   02S0518
         LA    @15,@CF00125                                     02S0518
         ST    @15,@AL00001+8                                   02S0518
         L     @15,@PC00001+8                                   02S0518
         ST    @15,@AL00001+12                                  02S0518
         LA    @15,ENQTCBP                                      02S0518
         ST    @15,@AL00001+16                                  02S0518
         L     @15,@PC00001+4                                   02S0518
         L     @01,SCTPTR(,@15)                                 02S0518
         LA    @15,SCTSNUMB(,@01)                               02S0518
         ST    @15,@AL00001+20                                  02S0518
         L     @15,@PC00001+24                                  02S0518
         LA    @15,NQRETPS(,@15)                                02S0518
         ST    @15,@AL00001+24                                  02S0518
         LA    @15,@CF00125                                     02S0518
         ST    @15,@AL00001+28                                  02S0518
         ST    @08,@AL00001+32                                  02S0518
         LA    @08,@CF00125                                     02S0518
         ST    @08,@AL00001+36                                  02S0518
         L     @15,@CV00098                                     02S0518
         LA    @01,@AL00001                                     02S0518
         BALR  @14,@15                                          02S0518
*  END;                                                         02S0519
*                                                               02S0519
*IF ERRCODE = ZERO THEN                /*SUCCESSFUL ABOVE?           */
@RF00512 SLR   @08,@08                                          02S0520
         L     @15,@PC00001+16                                  02S0520
         SLR   @14,@14                                          02S0520
         ICM   @14,3,ERRCODE(@15)                               02S0520
         CR    @08,@14                                          02S0520
         BNE   @RF00520                                         02S0520
*  DO;                                 /*YES,COMPLETE PROCESSING     */
*    SAVFEPAP = FSJXEPAP;              /*SAVE EPA PTR TO ASSIGNED BLK*/
         L     @15,@PC00001+24                                  02S0522
         L     SAVFEPAP,FSJXEPAP(,@15)                          02S0522
*    FSJXEPAP = ZERO;                  /*CLEAR EPA PTR IN DEPARMS    */
         ST    @08,FSJXEPAP(,@15)                               02S0523
*    DEQDSNSW = OFF;                   /*RESET DEQ DSNAME ON ERROR SW*/
         L     @08,@PC00001+20                                  02S0524
         NI    DEQDSNSW(@08),B'11011111'                        02S0524
*    HSKPDQSW = OFF;                   /*RESET HSKP DEQ SW   @YM06112*/
         NI    HSKPDQSW(@08),B'11110111'                        02S0525
*    UNALOCSW = OFF;                   /*RESET UNALLOC ON ERROR SW   */
*                                                               02S0526
         NI    UNALOCSW(@08),B'11101111'                        02S0526
*/********************************************************************/
*/*                                                                  */
*/* SEGMENT NAME - CHAINSIO                                          */
*/*                                                                  */
*/* DESCRIPTIVE NAME - CHAIN SIOT(S)                                 */
*/*                                                                  */
*/* FUNCTION - CHAIN SIOT(S) FOR THE REQUEST INTO THE STEP'S SIOT    */
*/*   CHAIN.                                                         */
*/*                                                                  */
*/*   OPERATION -                                                    */
*/*     IF SIOTS EXIST FOR THE STEP, THE CURRENT LAST SIOT IS        */
*/*     READ IN VIA THE SWA MANAGER INTERFACE RTN. THIS IS   @YA00061*/
*/*     NECESSARY AS DSABS MAY NOT EXIST, OR THE SIOT        @YA00061*/
*/*     CORRESPONDING TO THE LAST DSAB MAY NOT BE THE LAST           */
*/*     SIOT IN THE CHAIN.                                           */
*/*                                                                  */
*/*     WRITE EPA CHAINING IS DONE FOR UPDATED/CREATED       @YA00061*/
*/*     RECORDS, PLACING THESE EPAS ON THE INPUT EPA CHAIN.  @YA00061*/
*/*     (SEE CHAINEPA SEGMENT)                               @YA00061*/
*/*                                                                  */
*/*     THE LAST SIOT'S SVA AND IN-CORE NEXT POINTERS ARE UPDATED TO */
*/*     THOSE OF THE FIRST SIOT ASSOCIATED WITH THE REQUEST. IF      */
*/*     SIOTS DID NOT EXIST FOR THE STEP, THE SCT FIRST SIOT SVA     */
*/*     IS UPDATED. THE SCT LAST SIOT SVA IS UPDATED IN BOTH CASES   */
*/*     TO THE SVA OF THE LAST SIOT ASSOCIATED WITH THE REQUEST.     */
*/*                                                                  */
*/*     (NOTE THAT THE WRITE EPA CHAINING IS DONE BEFORE     @YA00061*/
*/*     THE CHAINING OF RECORDS THEMSELVES TO ENSURE THAT IN @YA00061*/
*/*     THE EVENT OF PROGRAM INTERRUPTION, ASSIGNED RECORDS  @YA00061*/
*/*     WILL BE WRITTEN BY ESTAE PROCESSING AND THEREFORE    @YA00061*/
*/*     CAN BE READ IN SUBSEQUENT PROCESSING, E.G.           @YA00061*/
*/*     UNALLOCATION.)                                       @YA00061*/
*/*                                                                  */
*/********************************************************************/
*                                                               03S0527
*OLDLSISW = OFF;                       /*WORK SW,OLD LAST SIOT UPDATE*/
         NI    OLDLSISW,B'01111111'                             03S0527
*IF SCTLSIOT ^= ZERO THEN              /*SIOTS ALREADY ON SIOT CHAIN?*/
         L     @08,@PC00001+4                                   03S0528
         L     @08,SCTPTR(,@08)                                 03S0528
         SLR   @04,@04                                          03S0528
         ICM   @04,7,SCTLSIOT(@08)                              03S0528
         LTR   @04,@04                                          03S0528
         BZ    @RF00528                                         03S0528
*  DO;                                 /*YES, READ LAST ONE  @YA00061*/
*    SWACTLS = SWACTLS && SWACTLS;     /*ZERO FUNCTION MAP   @YA00061*/
         LH    @08,SWACTLS                                      03S0530
         N     @08,@CF03511                                     03S0530
         LR    @15,@08                                          03S0530
         XR    @15,@08                                          03S0530
         STH   @15,SWACTLS                                      03S0530
*    SWREAD = ON;                      /*REQUEST READ FN     @YA00061*/
*    SWNEW = ON;                       /*CREATE EPAS         @YA00061*/
*    SWFREE = ON;                      /*FREE EPAS WHEN DONE @YA00061*/
         OI    SWREAD,B'00101100'                               03S0533
*    SMISVA(1) = SCTLSIOT;             /*SVA OF LAST SIOT    @YA00061*/
         STCM  @04,7,SMISVA                                     03S0534
*    CALL IEFAB4F7(SWACTLS,0,0,ADDR(SMISVAID),ADDR(OLDLSIOT),   03S0535
*                  RCORDLEN,1);        /*SWA MGR I/F FOR READ@YA00061*/
         MVC   @AL00001+4(24),@AL00535                          03S0535
         LA    @08,SWACTLS                                      03S0535
         ST    @08,@AL00001                                     03S0535
         LA    @08,SMISVAID                                     03S0535
         ST    @08,@TF00001                                     03S0535
         LA    @08,@TF00001                                     03S0535
         ST    @08,@AL00001+12                                  03S0535
         LA    @08,OLDLSIOT                                     03S0535
         ST    @08,@TF00002                                     03S0535
         LA    @08,@TF00002                                     03S0535
         ST    @08,@AL00001+16                                  03S0535
         L     @15,@CV00099                                     03S0535
         LA    @01,@AL00001                                     03S0535
         BALR  @14,@15                                          03S0535
*    OLDLSISW = ON;                    /*OLD LAST SIOT TO BE UPDATED
*                                                            @YA00061*/
         OI    OLDLSISW,B'10000000'                             03S0536
*  END;                                                         03S0537
*ELSE;                                 /*NO SIOTS ON CHAIN YET       */
@RF00528 DS    0H                                               03S0539
*/********************************************************************/
*/*                                                                  */
*/* SEGMENT NAME - CHAINEPA                                          */
*/*                                                                  */
*/* DESCRIPTIVE NAME - CHAIN EPA'S FOR THE SCT AND OLD LAST SIOT     */
*/*                                                                  */
*/* FUNCTION - COMPLETE EPA CHAIN FOR THE REQUEST.                   */
*/*                                                                  */
*/*   OPERATION -                                                    */
*/*     IF EPA'S FOR THE SCT AND OLD LAST SIOT (IF ONE       @YA00061*/
*/*     EXISTS) ARE NOT ALREADY ON THE                       @YA00061*/
*/*     EPA CHAIN, THEY ARE CREATED. THESE EPA'S                     */
*/*     ARE CHAINED TO THE EPA'S FOR THE SIOT(S), JFCB(S),           */
*/*     AND JFCBX(S) OF THE REQUEST, AND THEN ALL OF THE EPA'S ARE   */
*/*     CHAINED TO THE INPUT EPA CHAIN. (THIS METHOD OF CHAINING IS  */
*/*     DONE FOR ESTAE PROCESSING, SO THAT EITHER ALL OR NONE OF THE */
*/*     EPA'S ARE ON THE EPA CHAIN, AND THEREFORE, ALL OR NONE OF    */
*/*     THE RECORDS WILL BE WRITTEN BY THE ESTAE EXIT.)              */
*/*                                                                  */
*/* INPUT -                                                          */
*/*   OLDLSISW - INDICATES TO CHAINEPA SEGMENT IF OLD LAST SIOT      */
*/*              WILL BE UPDATED.                            @YA00061*/
*/*   OLDLSIOT - ADDRESS OF OLD LAST SIOT.                           */
*/*                                                                  */
*/********************************************************************/
*RFY (EPACOREL,EPACOREP) RESTRICTED;   /*RESTRICT R0,R1 RESPECTIVELY */
*EPACOREL = ZERO;                      /*INIT AMT OF CORE NEEDED     */
         SLR   EPACOREL,EPACOREL                                04S0540
*IF EPASCTSW = OFF THEN                /*EPA FOR SCT ALREADY CHAINED?*/
         L     @08,@PC00001+20                                  04S0541
         TM    EPASCTSW(@08),B'10000000'                        04S0541
         BNZ   @RF00541                                         04S0541
*   EPACOREL = EPACOREL+LENGTH(SWAEPA); /*NO, GET CORE FOR EPA       */
         AL    EPACOREL,@CF00069                                04S0542
*IF EPALSISW = OFF                     /*EPA FOR LAST SIOT CHAINED?  */
*   & OLDLSISW = ON THEN               /*NO, LAST SIOT TO BE UPDATED?
*                                                            @YA00061*/
@RF00541 L     @08,@PC00001+20                                  04S0543
         TM    EPALSISW(@08),B'01000000'                        04S0543
         BNZ   @RF00543                                         04S0543
         TM    OLDLSISW,B'10000000'                             04S0543
         BNO   @RF00543                                         04S0543
*   EPACOREL=EPACOREL+LENGTH(SWAEPA);   /*YES, GET CORE FOR EPA      */
         AL    EPACOREL,@CF00069                                04S0544
*IF EPACOREL > ZERO THEN               /*EPA'S TO BE CREATED/CHAINED?*/
@RF00543 LTR   EPACOREL,EPACOREL                                04S0545
         BNP   @RF00545                                         04S0545
* DO;                                                           04S0546
*    R0 = EPACOREL;                    /*LENGTH OF EPA'S             */
*    R0=R0 × SUBPOLNO;                 /*GET FROM CORRECT SUBPOOL    */
         O     R0,@CF00119                                      04S0548
*    GEN (GETMAIN R,LV=(0));           /*YES, GET CORE FOR EPA'S     */
         GETMAIN R,LV=(0)
*    SWEPAPTR = EPACOREP;              /*SET EPA MAP BASE            */
         LR    SWEPAPTR,EPACOREP                                04S0550
*    IF EPASCTSW = OFF THEN            /*CORE GOTTEN FOR SCT EPA?    */
         L     @08,@PC00001+20                                  04S0551
         TM    EPASCTSW(@08),B'10000000'                        04S0551
         BNZ   @RF00551                                         04S0551
*      DO;                             /*YES, SET UP EPA FOR SCT     */
*        SWBLKPTR = SCTPTR;            /*SET BLOCK PTR               */
         L     @08,@PC00001+4                                   04S0553
         L     @08,SCTPTR(,@08)                                 04S0553
         ST    @08,SWBLKPTR(,SWEPAPTR)                          04S0553
*        SWVA = SCTDISKA;              /*SET SVA                     */
         MVC   SWVA(3,SWEPAPTR),SCTDISKA(@08)                   04S0554
*        SWBLKID = SCTTBLID;           /*SET BLOCK ID                */
         MVC   SWBLKID(1,SWEPAPTR),SCTTBLID(@08)                04S0555
*        SWLNGTH=LENGTH(INSMSCT);      /*SET BLOCK LENGTH            */
         MVC   SWLNGTH(4,SWEPAPTR),@CF00166                     04S0556
*      END;                                                     04S0557
*    IF EPALSISW = OFF                                          04S0558
*      & OLDLSISW = ON THEN            /*CORE FOR OLD LAST SIOT EPA? */
@RF00551 L     @08,@PC00001+20                                  04S0558
         TM    EPALSISW(@08),B'01000000'                        04S0558
         BNZ   @RF00558                                         04S0558
         TM    OLDLSISW,B'10000000'                             04S0558
         BNO   @RF00558                                         04S0558
*      DO;                             /*YES, SET UP LAST SIOT EPA   */
*        IF EPASCTSW = OFF THEN        /*EPA FOR SCT CREATED ABOVE?  */
         TM    EPASCTSW(@08),B'10000000'                        04S0560
         BNZ   @RF00560                                         04S0560
*          DO;                         /*YES,CHAIN IT TO THIS EPA    */
*            SWCHNPTR = SWEPAPTR+LENGTH(SWAEPA); /*SET CHAIN FIELD   */
         LA    @08,16                                           04S0562
         ALR   @08,SWEPAPTR                                     04S0562
         ST    @08,SWCHNPTR(,SWEPAPTR)                          04S0562
*            SWEPAPTR = SWCHNPTR;      /*POINT TO SIOT EPA           */
         LR    SWEPAPTR,@08                                     04S0563
*          END;                                                 04S0564
*        ELSE;                         /*NO,EPA PTR ALREADY SET UP   */
@RF00560 DS    0H                                               04S0566
*        SWBLKPTR = OLDLSIOT;          /*SET BLOCK PTR               */
         L     @08,OLDLSIOT                                     04S0566
         ST    @08,SWBLKPTR(,SWEPAPTR)                          04S0566
*        SWVA = OLDLSIOT->SIOTDSKA;    /*SET SVA                     */
         MVC   SWVA(3,SWEPAPTR),SIOTDSKA(@08)                   04S0567
*        SWBLKID = OLDLSIOT->SIOTTYPE; /*SET BLOCK ID                */
         MVC   SWBLKID(1,SWEPAPTR),SIOTTYPE(@08)                04S0568
*        SWLNGTH = SIOTLNTH;           /*SET BLOCK LENGTH            */
         MVC   SWLNGTH(4,SWEPAPTR),@CF00166                     04S0569
*      END;                                                     04S0570
*    /* AT THIS POINT, EPACOREP POINTS TO THE FIRST EPA GETMAIN'D,   */
*    /* AND SWEPAPTR POINTS TO THE LAST EPA GETMAIN'D.               */
*    SWEPAPTR->SWCHNPTR = SAVFEPAP;    /*CHAIN EPA'S FOR ASSIGNED
*                                        RECORDS TO ABOVE EPA'S      */
@RF00558 ST    SAVFEPAP,SWCHNPTR(,SWEPAPTR)                     04S0571
*    LASTEPAP->SWCHNPTR = EPACOREP;    /*CHAIN EPA'S TO INPUT CHAIN  */
         L     @08,@PC00001+8                                   04S0572
         L     @08,LASTEPAP(,@08)                               04S0572
         ST    EPACOREP,SWCHNPTR(,@08)                          04S0572
*    EPASCTSW = ON;                    /*INDICATE EPA FOR SCT ON CHN */
         L     @08,@PC00001+20                                  04S0573
         OI    EPASCTSW(@08),B'10000000'                        04S0573
*    IF OLDLSISW = ON THEN             /*OLD LAST SIOT TO BE UPDATED?
*                                                            @YA00061*/
         TM    OLDLSISW,B'10000000'                             04S0574
         BNO   @RF00574                                         04S0574
*      EPALSISW = ON;                  /*YES INDICATE EPA ON CHAIN   */
         OI    EPALSISW(@08),B'01000000'                        04S0575
*  END;                                                         04S0576
*ELSE                                  /*CORE NOT GOTTEN ABOVE       */
*  LASTEPAP->SWCHNPTR = SAVFEPAP;      /*CHAIN EPA'S FOR ASSIGNED BLK
*                                        TO INPUT EPA CHAIN          */
         B     @RC00545                                         04S0577
@RF00545 L     @08,@PC00001+8                                   04S0577
         L     @08,LASTEPAP(,@08)                               04S0577
         ST    SAVFEPAP,SWCHNPTR(,@08)                          04S0577
*LASTEPAP = LSJXEPAP;                  /*UPDATE LAST EPA POINTER     */
@RC00545 L     @08,@PC00001+8                                   04S0578
         L     @15,@PC00001+24                                  04S0578
         L     @14,LSJXEPAP(,@15)                               04S0578
         ST    @14,LASTEPAP(,@08)                               04S0578
*RFY (EPACOREL,EPACOREP) UNRESTRICTED; /*RELEASE R0 AND R1           */
*IF SCTLSIOT ^= ZERO THEN              /*SIOTS ALREADY EXIST?@YA00061*/
         L     @08,@PC00001+4                                   03S0580
         L     @08,SCTPTR(,@08)                                 03S0580
         ICM   @14,7,SCTLSIOT(@08)                              03S0580
         BZ    @RF00580                                         03S0580
*  DO;                                 /*YES,CHAIN INTO LAST @YA00061*/
*    OLDLSIOT->SCTPSIOT = SIOTDSKA;    /*NEW SVA INTO OLD LAST  03S0582
*                                                            @YA00061*/
         L     @08,OLDLSIOT                                     03S0582
         L     @15,FSIOTPTR(,@15)                               03S0582
         MVC   SCTPSIOT(3,@08),SIOTDSKA(@15)                    03S0582
*    OLDLSIOT->SIOTNPTR = FSIOTPTR;    /*NEW IN-CORE PTR ALSO@YA00061*/
         ST    @15,SIOTNPTR(,@08)                               03S0583
*  END;                                /*                    @YA00061*/
*ELSE                                  /*NO SIOTS IN CHAIN   @YA00061*/
*  SCTFSIOT = SIOTDSKA;                /*NEW SIOTS SVA INTO SCT 03S0585
*                                                            @YA00061*/
         B     @RC00580                                         03S0585
@RF00580 L     @08,@PC00001+4                                   03S0585
         L     @08,SCTPTR(,@08)                                 03S0585
         L     @15,@PC00001+24                                  03S0585
         L     @01,FSIOTPTR(,@15)                               03S0585
         MVC   SCTFSIOT(3,@08),SIOTDSKA(@01)                    03S0585
*SCTLSIOT = NEWLSIOT->SIOTDSKA;        /*SET NEW SCT LAST SIOT SVA   */
@RC00580 L     @08,@PC00001+4                                   03S0586
         L     @08,SCTPTR(,@08)                                 03S0586
         MVC   SCTLSIOT(3,@08),SIOTDSKA(NEWLSIOT)               03S0586
*                                                               02S0587
*/********************************************************************/
