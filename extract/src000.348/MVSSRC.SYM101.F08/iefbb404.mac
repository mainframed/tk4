*/********************************************************************/
*/*                                                                  */
*/* SEGMENT NAME - SETFUNMP                                          */
*/*                                                                  */
*/* DESCRIPTIVE NAME - SETUP FUNCTION MAPS                           */
*/*                                                                  */
*/* FUNCTION -                                                       */
*/*   THE PURPOSE OF THIS CODE SEGMENT IS TO SET UP THE FUNCTION     */
*/*   MAPS FOR JFCB HOUSEKEEPING AND COMMON ALLOCATION.              */
*/*                                                                  */
*/*   THE VARIABLE FIELDS IN THESE FUNCTION  MAPS ARE SET ON OR      */
*/*   OFF DEPENDING ON THE FUNCTION MAP FOR THE STEP ALLOCATION      */
*/*   CONTROL MODULE AND (IN ONE CASE) ON A FIELD IN THE JCT(INCMALL)*/
*/*                                                                  */
*/* ENTRY - THIS CODE IS AN IN-LINE SUBROUTINE WHICH IS ENTERED      */
*/*         AT THE TOP.                                              */
*/*                                                                  */
*/*   PUPPOSE - SEE FUNCTION                                         */
*/*                                                                  */
*/*   INPUT -                                                        */
*/*            JCT - JOB CONTROL TABLE                               */
*/*            INFUNMAP - FUNCTION MAP FOR THE STEP ALLOCATION       */
*/*                       CONTROL ROUTINE.                           */
*/*                                                                  */
*/* EXITS - THIS CODE IS AN IN-LINE SUBROUTINE WHICH EXITS           */
*/*         FROM THE BOTTOM.                                         */
*/*                                                                  */
*/*   CONDITION - (NORMAL). FUNCTION MAPS ARE BUILT FOR JFCB         */
*/*               HOUSEKEEPING AND COMMON ALLOCATION PROCESSING.     */
*/*                                                                  */
*/*   OUTPUT -                                                       */
*/*            ALFUNMAP - FUNCTION MAP FOR COMMON ALLOCATION         */
*/*            JHFUNMAP - FUNCTION MAP FOR JFCB HOUSEKEEPING         */
*/*                                                                  */
*/*   RETURN CODE - (NONE)                                           */
*/*                                                                  */
*/********************************************************************/
*JHFUNMAP = JHINITFM;                  /*INITIALIZE JFCB HSKPING
*                                        FUNCTION MAP                */
         MVC   JHFUNMAP(2),@CB00442                             02S0047
*JHRETMAP = JHRETMAP&&JHRETMAP;        /*CLEAR HSKP RETURN FUNC MAP
*                                                            @G24LPSJ*/
         XC    JHRETMAP(1),JHRETMAP                             02S0048
*RFY FUNCMAP BASED(ADDR(ALFUNMAP));                             02S0049
*FUNCMAP = ALINITFM;                   /*INITIALIZE ALLOCATION  02S0050
*                                        FUNCTION MAP                */
         LA    @10,ALFUNMAP                                     02S0050
         MVC   FUNCMAP(2,@10),@CB00438                          02S0050
*ALCSUBWT = ON;                        /*OK TO WAIT FOR SUB-SYSTEM
*                                        DATA SET                    */
*SSEMSGSW = ON;                        /*SUB-SYSTEM TO RETURN ERROR
*                                        MESSAGES            @G29AN2F*/
         OI    ALCSUBWT(@10),B'10000100'                        02S0052
*IF INUWAISW = CANWAIT THEN            /*ARE WAITS FOR UNITS OK      */
         L     @07,@PC00001                                     02S0053
         TM    INUWAISW(@07),B'01000000'                        02S0053
         BNO   @RF00053                                         02S0053
*  DO;                                 /*YES,INDICATE TO JFCB HSKPING
*                                        AND ALLOCATION              */
*    JUWAITSW = CANWAIT;               /*SET IN JFCB HSKP FUNC MAP   */
         OI    JUWAITSW,B'00010000'                             02S0055
*    ALCWTUNT = CANWAIT;               /*SET IN ALLOCATION FUNC MAP  */
         OI    ALCWTUNT(@10),B'00100000'                        02S0056
*  END;                                                         02S0057
*ELSE;                                 /*NO UNIT WAITS, USE     02S0058
*                                        INITIALIZED FUNCTION MAPS   */
@RF00053 DS    0H                                               02S0059
*IF INVWAISW = CANWAIT THEN            /*ARE WAITS FOR VOLS + DSNS OK*/
         L     @10,@PC00001                                     02S0059
         TM    INVWAISW(@10),B'10000000'                        02S0059
         BNO   @RF00059                                         02S0059
*   DO;                                /*YES,INDICATE TO JFCB HSKPING
*                                        AND ALLOCATION              */
*     JVWAITSW = CANWAIT;              /*SET IN JFCB HSKP FUNC MAP   */
         OI    JVWAITSW,B'00000010'                             02S0061
*     ALCWTVOL = CANWAIT;              /*SET IN ALLOCATION FUNC MAP  */
         LA    @10,ALFUNMAP                                     02S0062
         OI    ALCWTVOL(@10),B'00010000'                        02S0062
*   END;                                                        02S0063
*ELSE;                                 /*NO VOL OR DSN WAITS, USE
*                                        INITIALIZED FUNCTION MAPS   */
@RF00059 DS    0H                                               02S0065
*IF USEOFFSW = ON THEN                 /*ARE OFFLINES TO BE USED?    */
         L     @10,@PC00001                                     02S0065
         TM    USEOFFSW(@10),B'00001000'                        02S0065
         BNO   @RF00065                                         02S0065
*  DO;                                 /*YES,INDICATE TO JFCB HSKPING
*                                        AND ALLOCATION              */
*    JHOFFSW = ON;                     /*SET IN JFCB HSKP FUNC MAP   */
         OI    JHOFFSW,B'10000000'                              02S0067
*    ALCOFFDV = ON;                    /*SET IN ALLOCATION FUNC MAP  */
         LA    @10,ALFUNMAP                                     02S0068
         OI    ALCOFFDV(@10),B'00000100'                        02S0068
*  END;                                                         02S0069
*ELSE;                                 /*DONT USE OFFLINES, USE 02S0070
*                                        INITIALIZED FUNCTION MAPS   */
@RF00065 DS    0H                                               02S0071
*IF INCMALL = ON THEN                  /*IS ALLOCATION MSGLEVEL=1    */
         L     @10,@PC00001+12                                  02S0071
         L     @10,JCTPTR(,@10)                                 02S0071
         TM    INCMALL(@10),B'10000000'                         02S0071
         BNO   @RF00071                                         02S0071
*  DO;                                 /*YES,INDICATE TO ALLOCATION  */
*    ALCMSGLV = ON;                    /*SET MSGLEVEL=1 IN FUNC MAP  */
         LA    @10,ALFUNMAP                                     02S0073
         OI    ALCMSGLV(@10),B'01000000'                        02S0073
*    ALCHDMSG = ON;                    /*INDICATE WRITE HDR MSG      */
         OI    ALCHDMSG(@10),B'01000000'                        02S0074
*  END;                                                         02S0075
*ELSE;                                 /*NO,KEEP MSGLEVEL=0 IN MAP   */
@RF00071 DS    0H                                               02S0077
*IF MONJOBSW = ON THEN                 /*IS MONITOR JOBNAMES ON      */
         L     @10,@PC00001                                     02S0077
         TM    MONJOBSW(@10),B'00000100'                        02S0077
         BNO   @RF00077                                         02S0077
*  DO;                                 /*YES, INDICATE TO ALLOCATION */
*    ALCJOBNM = ON;                    /*SET MONITOR JOBNAMES ON     */
         LA    @10,ALFUNMAP                                     02S0079
         OI    ALCJOBNM(@10),B'00100000'                        02S0079
*  END;                                                         02S0080
*ELSE;                                 /*NO, KEEP NO MTRJOBNAMES     */
@RF00077 DS    0H                                               02S0082
*IF INNDSISW = ON THEN                 /*DATA SET INTEGRITY TO BE
*                                        DONE IF JOBLIB OR STEPLIB
*                                        EXISTS?                     */
         L     @10,@PC00001                                     02S0082
         TM    INNDSISW(@10),B'00000010'                        02S0082
         BNO   @RF00082                                         02S0082
*  JHNDSISW = ON;                      /*NO, TELL HSKP'NG SO         */
         OI    JHNDSISW,B'00000100'                             02S0083
*IF SIOT1PTR ^= ZERO THEN              /*ARE THERE ANY DD REQUESTS   */
@RF00082 L     @10,@PC00001+32                                  01S0084
         ICM   @07,15,SIOT1PTR(@10)                             01S0084
         BZ    @RF00084                                         01S0084
*  DO;                                 /*YES DD REQUESTS, COMPLETE   */
*    CALL IEFAB451                     /*CALL JFCB HOUSEKEEPSING     */
*     (JHFUNMAP,SIOT1PTR,STEPNUM,JCTPTR,SCTPTR,LASTEPAP,0,      01S0086
*      INTJSCBP,IOSLUTP,FAILRSC,                                01S0086
*      JHRETMAP);                      /*AREA FOR HOUSEKEEPING TO SET
*                                        SWITCHES            @G24LPSJ*/
         LA    @07,JHFUNMAP                                     01S0086
         ST    @07,@AL00001                                     01S0086
         ST    @10,@AL00001+4                                   01S0086
         L     @10,@PC00001+16                                  01S0086
         ST    @10,@AL00001+8                                   01S0086
         L     @10,@PC00001+12                                  01S0086
         ST    @10,@AL00001+12                                  01S0086
         L     @10,@PC00001+20                                  01S0086
         ST    @10,@AL00001+16                                  01S0086
         L     @10,@PC00001+36                                  01S0086
         ST    @10,@AL00001+20                                  01S0086
         LA    @10,@CF00131                                     01S0086
         ST    @10,@AL00001+24                                  01S0086
         L     @10,@PC00001+4                                   01S0086
         ST    @10,@AL00001+28                                  01S0086
         L     @10,@PC00001+40                                  01S0086
         ST    @10,@AL00001+32                                  01S0086
         L     @10,@PC00001+28                                  01S0086
         ST    @10,@AL00001+36                                  01S0086
         LA    @10,JHRETMAP                                     01S0086
         ST    @10,@AL00001+40                                  01S0086
         L     @15,@CV00480                                     01S0086
         LA    @01,@AL00001                                     01S0086
         BALR  @14,@15                                          01S0086
*    RTNCODE = REG15;                  /*SAVE RETURN CODE            */
         LR    RTNCODE,REG15                                    01S0087
*    IF JHMSSTRM = ON THEN             /*IF MSS TERM NEEDED  @G24LPSJ*/
         TM    JHMSSTRM,B'10000000'                             01S0088
         BNO   @RF00088                                         01S0088
*      ALCMSSTM = ON;                  /*TURN ON MSS TERM FLAG FOR
*                                        CMN ALLOCATION      @G24LPSJ*/
         LA    @10,ALFUNMAP                                     01S0089
         OI    ALCMSSTM(@10),B'00001000'                        01S0089
*  END;                                                         01S0090
*ELSE;                                 /*NO DD REQUESTS              */
@RF00084 DS    0H                                               01S0092
*IF RTNCODE = SUCCESS THEN             /*ANY ERRORS                  */
@RC00084 LTR   RTNCODE,RTNCODE                                  01S0092
         BNZ   @RF00092                                         01S0092
*  DO;                                 /*NO,ALLOCATE THE JOB/STEP    */
*    SOMEALC = ON;                     /*SOME DDS MAY BE ALLOCATED   */
         L     @10,@PC00001+52                                  01S0094
         L     @10,JAEXITP(,@10)                                01S0094
         OI    SOMEALC(@10),B'10000000'                         01S0094
*    CALL IEFAB421(ALFUNMAP,SIOT1PTR,  /*COMMON ALLOCATION           */
*      PPJSCBP,IOSLUTP,FAILRSC,TCBPTR,TIOTP,                    01S0095
*      CANCECBP,STEPNUM,ADDR(SSERRBLK)); /*                  @G29AN2F*/
         LA    @10,ALFUNMAP                                     01S0095
         ST    @10,@AL00001                                     01S0095
         L     @10,@PC00001+32                                  01S0095
         ST    @10,@AL00001+4                                   01S0095
         L     @10,@PC00001+8                                   01S0095
         ST    @10,@AL00001+8                                   01S0095
         L     @10,@PC00001+40                                  01S0095
         ST    @10,@AL00001+12                                  01S0095
         L     @10,@PC00001+28                                  01S0095
         ST    @10,@AL00001+16                                  01S0095
         L     @10,@PC00001+24                                  01S0095
         ST    @10,@AL00001+20                                  01S0095
         L     @10,@PC00001+48                                  01S0095
         ST    @10,@AL00001+24                                  01S0095
         L     @10,@PC00001+44                                  01S0095
         ST    @10,@AL00001+28                                  01S0095
         L     @10,@PC00001+16                                  01S0095
         ST    @10,@AL00001+32                                  01S0095
         L     @10,@PC00001+56                                  01S0095
         ST    @10,@AL00001+36                                  01S0095
         L     @15,@CV00479                                     01S0095
         LA    @01,@AL00001                                     01S0095
         BALR  @14,@15                                          01S0095
*    RTNCODE = REG15;                  /*SAVE RETURN CODE            */
         LR    RTNCODE,REG15                                    01S0096
*    IF JHMSSTRM = ON &                /*IF MSS TERMINATION NEEDED   */
*       RTNCODE ^= SUCCESS THEN        /*AND CMN ALLOC NOT SUCCESSFUL
*                                                            @G24LPSJ*/
         TM    JHMSSTRM,B'10000000'                             01S0097
         BNO   @RF00097                                         01S0097
         LTR   RTNCODE,RTNCODE                                  01S0097
         BZ    @RF00097                                         01S0097
*      INMSSTRM = ON;                  /*TURN ON MSS TERM FOR   01S0098
*                                       ALLOC/INIT INTERFACE @G24LPSJ*/
         L     @10,@PC00001+60                                  01S0098
         OI    INMSSTRM(@10),B'10000000'                        01S0098
*    IF RTNCODE = SUCCESS & SIOT1PTR ^= ZERO THEN  /*IS THERE A TIOT */
@RF00097 SLR   @10,@10                                          01S0099
         CR    RTNCODE,@10                                      01S0099
         BNE   @RF00099                                         01S0099
         L     @07,@PC00001+32                                  01S0099
         L     @07,SIOT1PTR(,@07)                               01S0099
         CR    @07,@10                                          01S0099
         BE    @RF00099                                         01S0099
*      DO;                             /*YES, CALL THE TIOT MGR      */
*        TIORBPTR = ADDR(UPTIOTRB);    /*ADDRESS RB MAPPING MACRO    */
         LA    @10,UPTIOTRB                                     01S0101
         ST    @10,TIORBPTR                                     01S0101
*        TIOMFMAP = UPDTFCN;           /*INDICATE UPDATE FUNCTION    */
         MVC   TIOMFMAP(2,@10),@CB00133                         01S0102
*        TIOMJSCP = PPJSCBP;           /*PUT PTR TO JSCB IN RB       */
         L     @05,@PC00001+8                                   01S0103
         MVC   TIOMJSCP(4,@10),PPJSCBP(@05)                     01S0103
*        TIOMSIOP = SIOT1PTR;          /*PUT PTR TO FIRST SIOT IN RB */
         ST    @07,TIOMSIOP(,@10)                               01S0104
*        TIOMRETP = ADDR(FAILRSC);     /*POINT TO REASON CODE AREA   */
         L     @07,@PC00001+28                                  01S0105
         ST    @07,TIOMRETP(,@10)                               01S0105
*        CALL IEFAB4FC(TIORBPTR);      /*REORDER AND COMPRESS TIOT   */
         LA    @10,TIORBPTR                                     01S0106
         ST    @10,@AL00001                                     01S0106
         L     @15,@CV00476                                     01S0106
         LA    @01,@AL00001                                     01S0106
         BALR  @14,@15                                          01S0106
*        RTNCODE = REG15;              /*SAVE RETURN CODE            */
         LR    RTNCODE,REG15                                    01S0107
*      END;                                                     01S0108
*    ELSE;                             /*NO TIOT                     */
@RF00099 DS    0H                                               01S0110
*  END;                                                         01S0110
*ELSE;                                 /*ERROR FOUND IN JFCB HSKPING */
@RF00092 DS    0H                                               01S0112
*IF RTNCODE ^= SUCCESS THEN            /*ANY ERRORS                  */
         LTR   RTNCODE,RTNCODE                                  01S0112
         BZ    @RF00112                                         01S0112
*  DO;                                 /*YES, PROCESS THESE ERRORS   */
*/********************************************************************/
*/*                                                                  */
*/* SEGMENT NAME - DDERRMSG                                          */
*/*                                                                  */
*/* DESCRIPTIVE NAME - BUILD DD-RELATED ERROR MESSAGES               */
*/*                                                                  */
*/* FUNCTION -                                                       */
*/*   THE PURPOSE OF THIS CODE SEGMENT IS TO BUILD AN ERROR MESSAGE  */
*/*   FOR EACH DD REQUEST WHOSE SIOT INDICATES AN ERROR HAS OCCURRED */
*/*   WHILE PROCESSING THIS REQUEST. IF A SIOT IN ERROR HAS  @G29AN2F*/
*/*   AN ADDITIONAL ERROR MESSAGE ASSOCIATED WITH IT, THIS   @G29AN2F*/
*/*   MESSAGE WILL ALSO BE ISSUED.                           @G29AN2F*/
*/*   ALL SIOTS FOR THE STEP ARE                             @G29AN2F*/
*/*   SCANNED SINCE MORE THAN ONE DD REQUEST MAY HAVE AN ASSOCIATED  */
*/*   ERROR. MODULE IEFAB4FD (SYSTEM MESSAGE INTERFACE RTN) IS       */
*/*   CALLED TO WRITE EACH ERROR MESSAGE NEEDED. THE TEXT FOR        */
*/*   THE SYSTEM MESSAGES ISSUED BY THIS ROUTINE IS          @G29AN2F*/
*/*   CONTAINED IN MESSAGE MODULE IEFBB4M3. THE TEXT FOR THE @G29AN2F*/
*/*   ADDITIONAL MESSAGES IS PASSED BACK FROM COMMON         @G29AN2F*/
*/*   ALLOCATION.                                            @G29AN2F*/
*/*                                                                  */
*/*   OPERATION -                                                    */
*/*     THE ROUTINE DOES THE FOLLOWING FOR EACH SIOT ON THE INPUT    */
*/*     CHAIN:                                                       */
*/*       .IF THE DDNAME IS NOT CONCATENATED THE DDNAME      @YM07646*/
*/*        IS SAVED AND A RELATIVE CONCATENATION POSITION    @YM07646*/
*/*        COUNTER IS SET TO ZERO. IF THE NAME HAS BEEN      @YM07646*/
*/*        CONCATENATED THE COUNTER IS INCREMENTED BY ONE.   @YM07646*/
*/*                                                                  */
*/*       .FOR UNALLOCATED SIOTS THE REASON CODE, IF NON-ZERO, IS    */
*/*        USED TO INDEX INTO THE MESSAGE OFFSET TABLE OF THE MESSAGE*/
*/*        MODULE, IEFBB4M3. IF THE REASON CODE IS VALID THE OFFSET  */
*/*        IS USED TO SET THE POINTER TO THE PROPER MESSAGE IN THE   */
*/*        TEXT PORTION OF THE MESSAGE MODULE.                       */
*/*                                                                  */
*/*       .THE MESSAGE TEXT IS MOVED TO A BUFFER AND THE LAST SAVED  */
*/*        DDNAME IS INSERTED. THE JOBNAME, STEPNAME, AND PROCSTEP   */
*/*        ARE ALSO INSERTED. IF THE CONCATENATION POSITION COUNTER  */
*/*        IS NOT ZERO IT IS CONVERTED TO EBCDIC AND INSERTED IN THE */
*/*        MESSAGE.                                                  */
*/*                                                                  */
*/*       .SIOT INFORMATION CODES ARE CONVERTED AND INSERTED @G24LPSJ*/
*/*        INTO THE MESSAGE TEXT.                            @G24LPSJ*/
*/*                                                                  */
*/*       .A MESSAGE COMPRESSION ROUTINE, IEFAB4F3, IS INVOKED TO    */
*/*        REMOVE ANY CONSECUTIVE OR TRAILING BLANKS IN THE MESSAGE. */
*/*        THE NEW MESSAGE LENGTH IS RETURNED.                       */
*/*                                                                  */
*/*       .THE SYSTEM MESSAGE INTERFACE ROUTINE, IEFAB4FD, IS CALLED */
*/*        TO ISSUE THE MESSAGE.                                     */
*/*                                                                  */
*/*       .IF BIT SIOTSSMG IS ON THEN THE INTERNAL DD NUMBER @G29AN2F*/
*/*        IN THE SIOT IS USED TO LOCATE THE ADDITIONAL      @G29AN2F*/
*/*        MESSAGE IN THE MESSAGE AREA RETURNED BY           @G29AN2F*/
*/*        COMMON ALLOCATION CONTROL.  THE MESSAGE IS MOVED  @G29AN2F*/
*/*        TO THE MESSAGE BUFFER AND THE MESSAGE COMPRESSION @G29AN2F*/
*/*        ROUTINE, IEFAB4F3, IS INVOKED TO REMOVE ANY       @G29AN2F*/
*/*        CONSECUTIVE OR TRAILING BLANKS IN THE MESSAGE.    @G29AN2F*/
*/*        THE NEW MESSAGE LENGTH IS RETURNED AND THE SYSTEM @G29AN2F*/
*/*        MESSAGE INTERFACE ROUTINE, IEFAB4FD, IS CALLED TO @G29AN2F*/
*/*        ISSUE THE MESSAGE.                                @G29AN2F*/
*/*                                                                  */
*/* ENTRY -                                                          */
*/*                                                                  */
*/*   PURPOSE - SEE FUNCTION                                         */
*/*                                                                  */
*/*   INPUT -                                                        */
*/*     SIOT1PTR - POINTER TO FIRST SIOT                             */
*/*       SIOTRSNC - FIELD IN SIOT CONTAING REASON CODE OR 0         */
*/*       SIOTINFC - SIOT INFORMATION CODE INDICATOR         @G24LPSJ*/
*/*       SIOTSSIC - SIOT INFORMATION CODE FIELD             @G24LPSJ*/
*/*       SIOTCCAT - CONCATENATION INDICATOR                 @YM07646*/
*/*       SIOTSSMG - SUBSYSTEM MESSAGE INDICATOR             @G29AN2F*/
*/*       SCTDDINO - INTERNAL NUMBER OF THE DD STATEMENT     @G29AN2F*/
*/*     SSMSGBPR - POINTER TO SUBSYSTEM MESSAGE BLOCK        @G29AN2F*/
*/*     PPJSCBP - PTR TO PROBLEM PROGRAN JSCB                        */
*/*     TIOTP   - PTR TO TIOT                                        */
*/*                                                                  */
*/* EXITS -                                                          */
*/*                                                                  */
*/*   CONDITION - ALL DD-RELATED ERROR MESSAGES ISSUED               */
*/*                                                                  */
*/*     OUTPUT - MESSAGE(S) WRITTEN                                  */
*/*                                                                  */
*/*     RETURN CODE - (NONE)                                         */
*/*                                                                  */
*/********************************************************************/
*RETCODE = SUCCESS;                    /*INITIALIZE RETURN CODE      */
         SLR   RETCODE,RETCODE                                  02S0114
*SIOTPTR = SIOT1PTR;                   /*PT TO FIRST SIOT OF STEP    */
         L     @10,@PC00001+32                                  02S0115
         L     SIOTPTR,SIOT1PTR(,@10)                           02S0115
*DO WHILE(SIOTPTR ^= ZERO              /*SCAN ALL SIOTS ...          */
*         & RETCODE = SUCCESS);        /*... UNLESS ERROR OCCURS     */
         B     @DE00116                                         02S0116
@DL00116 DS    0H                                               02S0117
*  IF (SIOTJSCT = ON & SCTPCAT(1:3) = SIOTDSKA)/*FIRST JOBCAT/STEPCAT
*                                                IN STEP?    @YM08122*/
*    × (SIOTCCAT = OFF                 /*OR NOT CONCATENATED DD?
*                                        NOTE: SIOTCCAT NOT ON FOR
*                                        CONCATENATE JOB/STEPCAT
*                                                            @YM08122*/
*      & SIOTJSCT = OFF) THEN          /*AND NOT CONCATENATED   02S0117
*                                        JOB/STEPCAT         @YM08122*/
         TM    SIOTJSCT(SIOTPTR),B'00000001'                    02S0117
         BNO   @GL00003                                         02S0117
         L     @10,@PC00001+20                                  02S0117
         L     @10,SCTPTR(,@10)                                 02S0117
         CLC   SCTPCAT(3,@10),SIOTDSKA(SIOTPTR)                 02S0117
         BE    @RT00117                                         02S0117
@GL00003 TM    SIOTCCAT(SIOTPTR),B'00100001'                    02S0117
         BNZ   @RF00117                                         02S0117
@RT00117 DS    0H                                               02S0118
*    DO;                               /*YES,PREPARE FOR CONCAT'N    */
*      MSGDDNAM = SCTDDNAM;            /*SAVE DD NAME                */
         MVC   MSGDDNAM(8),SCTDDNAM(SIOTPTR)                    02S0119
*      CCATCTR = ZERO;                 /*INIT RELATIVE POS CTR       */
         SLR   CCATCTR,CCATCTR                                  02S0120
*    END;                                                       02S0121
*  ELSE                                /*CONCATENATED DD             */
*    CCATCTR = CCATCTR + ONE;          /*UPDATE RELATIVE POS. CTR    */
         B     @RC00117                                         02S0122
@RF00117 LA    CCATCTR,1(,CCATCTR)                              02S0122
*  IF SIOTALCD = NO THEN               /*WAS DD ALLOCATED?           */
@RC00117 TM    SIOTALCD(SIOTPTR),B'00000010'                    02S0123
         BNZ   @RF00123                                         02S0123
*    DO;                               /*NO, PREPARE TO ISSUE MSG    */
*      MSGBUFFR = '';                  /*BLANK OUT MSG BUFFER        */
         MVI   MSGBUFFR,C' '                                    02S0125
         MVC   MSGBUFFR+1(119),MSGBUFFR                         02S0125
*      IF SIOTRSNC ^= ZERO THEN        /*REASON CODE GIVEN?          */
         SLR   @10,@10                                          02S0126
         LH    @07,SIOTRSNC(,SIOTPTR)                           02S0126
         CR    @07,@10                                          02S0126
         BE    @RF00126                                         02S0126
*        IF MSGOFFS(SIOTRSNC) ^= INVLDCDE THEN /*VALID REASON CODE   */
         SLA   @07,2                                            02S0127
         L     @02,@CA00468                                     02S0127
