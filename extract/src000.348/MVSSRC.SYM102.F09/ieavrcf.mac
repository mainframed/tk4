         TITLE '***** REAL STORAGE RECONFIGURATION ROUTINE IEAVRCF *****
               **       '
IEAVRCF CSECT
         USING *,15
         MODID
IEAVRCF2 DS    0H
         ENTRY IEAVRCF2
E1  STM   @14,@12,12(@13)         CALLER PROVIDES SAVE AREA FOR VRCF
    BALR  @15,0
    USING *,@15
    B     COMMON
    DC    AL2(EP1-E1-6)
IEAVRCFI  STM  @00,@15,PVTWSA3(PVTPTR)  SAVE AREA FOR INTECEPT
    BALR  @15,0
    USING *,@15
    B     COMMON
    DC    AL2(EP2-IEAVRCFI-6)
    ENTRY IEAVRCFI
IEAVRCFC  BALR  @15,0             COMPLETE DOES NOT SAVE REGS
    USING *,@15
    B     COMMON
    DC    AL2(EP3-IEAVRCFC-2)
    ENTRY IEAVRCFC
COMMON  BALR  @12,0               SET UP REG12 AS BASE
    USING *,@12
    AH    @15,4(@15)
    BR    @15
*                                                                  0077
*/********************************************************************/
*/*IEAVRCF IS ENTERED FOR ONE OF SIX REASONS: 1) ONLINE REQUEST,     */
*/*2) OFFLINE REQUEST, 3) STORAGE KEY ERROR RECOVERY REQUEST,        */
*/*4) STORAGE DATA ERROR RECOVERY REQUEST, 5) FRAME STATUS REQUEST,  */
*/*6) CANCEL OFFLINE REQUEST.  EACH OF THESE FUNCTNS IS A SUBROUTINE */
*/*WHICH GETS CALLED AFTER SAVING REGISTERS AND OBTAINING THE SALLOC */
*/*LOCK, IF NEEDED.  WHEN THE SUBROUTINE COMPLETES AND RETURNS TO    */
*/*MAINLINE, THE LOCK, IF HELD, WILL BE RELEASED AND REGISTERS       */
*/*RESTORED BEFORE RETURNING CONTROL TO THE CALLER.                  */
*/********************************************************************/
*                                                                  0077
*EP1:                                                              0077
*   ;                                                              0077
EP1      DS    0H                                                  0078
*   PARMPTR=REG1;                   /* PARAMETER ADDRESS             */
         LR    PARMPTR,REG1                                        0078
*   PVTPTR=CVTPVTP;                                                0079
         L     @05,CVTPTR                                          0079
         L     PVTPTR,CVTPVTP(,@05)                                0079
*   REG0=PVTPRCV;                   /* SET UP RECOVERY               */
         L     REG0,PVTPRCV(,PVTPTR)                               0080
*   RFY                                                            0081
*     REG2 RSTD;                                                   0081
*   GENERATE;                                                      0082
   SETFRR  A,FRRAD=(REG0),WRKREGS=(REG1,REG2),PARMAD=(REG2)
*   RESPECIFY                                                      0083
*     RCA BASED(REG2);              /*                       @ZA09185*/
*   RCACSECT=RCACRECF;                                             0084
         MVI   RCACSECT(REG2),X'10'                                0084
*   RCARCF='1'B;                                                   0085
         OI    RCARCF(REG2),B'00001000'                            0085
*   IF SALREQ='0'B                  /* IF 0, THEN OBTAIN SALLOC      */
*     THEN                                                         0086
         TM    SALREQ(PARMPTR),B'00000010'                         0086
         BNZ   @RF00086                                            0086
*     DO;                                                          0087
*       RCASAL='1'B;                                               0088
         OI    RCASAL(REG2),B'10000000'                            0088
*       REG0=REG12;                 /* SETLOCK CLOBBERS 11 - 14      */
         LR    REG0,REG12                                          0089
*       REG1=REG13;                                                0090
         LR    REG1,REG13                                          0090
*       GENERATE;                                                  0091
OB       SETLOCK OBTAIN,TYPE=SALLOC,MODE=UNCOND,                      **
               RELATED=(RSM,IEAVRCF(RE))
*       REG12=REG0;                                                0092
         LR    REG12,REG0                                          0092
*       REG13=REG1;                                                0093
         LR    REG13,REG1                                          0093
*     END;                                                         0094
*                                                                  0094
*   /*****************************************************************/
*   /*                                                               */
*   /* SAVE THE OLD RACA POINTER FROM PVTPRCA, AND REPLACE IT WITH   */
*   /* RCA POINTER JUST OBTAINED SO THAT OTHER RSM ROUTINES WILL     */
*   /* REFERENCE THE CORRECT RCA. THE OLD POINTER WILL BE RESTORED   */
*   /* UPON EXIT                                             @ZA09185*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0095
*   RCAPSAVE=PVTPRCA;               /*                       @ZA09185*/
@RF00086 MVC   RCAPSAVE(3,REG13),PVTPRCA+1(PVTPTR)                 0095
*   PVTPRCA=REG2;                   /*                       @ZA09185*/
         ST    REG2,PVTPRCA(,PVTPTR)                               0096
*   RESPECIFY                                                      0097
*     RCA BASED(PVTPRCA);           /*                       @ZA09185*/
*   RESPECIFY                                                      0098
*     REG2 UNRSTD;                                                 0098
*   RSMHDPTR=ASCBRSM;                                              0099
*                                                                  0099
         L     @05,ASCBPTR                                         0099
         L     RSMHDPTR,ASCBRSM(,@05)                              0099
*   /*****************************************************************/
*   /*                                                               */
*   /* IF ALL FRAMES ARE WITHIN SYSTEM FRAME LIMITS THEN PROCESS REQ.*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0100
*   RFY                                                            0100
*     HIRBN RSTD;                                                  0100
*   HIRBN=REQRBN+((FCNT-1)*16);                                    0101
         LH    @05,REQRBN(,PARMPTR)                                0101
         N     @05,@CF02384                                        0101
         LH    HIRBN,FCNT(,PARMPTR)                                0101
         BCTR  HIRBN,0                                             0101
         SLA   HIRBN,4                                             0101
         ALR   HIRBN,@05                                           0101
*   IF PVTLPFN>=HIRBN               /* IF >=, THEN TOO LARGE         */
*     THEN                                                         0102
         LH    @08,PVTLPFN(,PVTPTR)                                0102
         N     @08,@CF02384                                        0102
         CR    HIRBN,@08                                           0102
         BH    @RF00102                                            0102
*     DO;                                                          0103
*       RESPECIFY                                                  0104
*         PFTEPTR RSTD;                                            0104
*       PFTEPTR=PVTPFTP+REQRBN;                                    0105
         AL    @05,PVTPFTP(,PVTPTR)                                0105
         LR    PFTEPTR,@05                                         0105
*       S15=0;                      /* INIT RETURN CODE TO 0         */
         SLR   @08,@08                                             0106
         ST    @08,S15(,REG13)                                     0106
*       IF ONREQ='1'B               /* ONLINE REQUEST                */
*         THEN                                                     0107
         TM    ONREQ(PARMPTR),B'10000000'                          0107
         BNO   @RF00107                                            0107
*         CALL ONLINE;              /* ON LINE REQUEST               */
         BAL   @14,ONLINE                                          0108
*       IF OFFREQ='1'B              /* OFFLINE REQUEST               */
*         THEN                                                     0109
@RF00107 TM    OFFREQ(PARMPTR),B'01000000'                         0109
         BNO   @RF00109                                            0109
*         CALL OFFLINE;             /* OFFLINE                       */
         BAL   @14,OFFLINE                                         0110
*       IF STGREQ='1'B              /* STORAGE DATA ERROR            */
*         THEN                                                     0111
@RF00109 TM    STGREQ(PARMPTR),B'00100000'                         0111
         BNO   @RF00111                                            0111
*         CALL STORAGE;             /* STORAGE REQUEST               */
         BAL   @14,STORAGE                                         0112
*       IF KEYREQ='1'B              /* STORAGE KEY ERROR             */
*         THEN                                                     0113
@RF00111 TM    KEYREQ(PARMPTR),B'00010000'                         0113
         BNO   @RF00113                                            0113
*         CALL KEY;                 /* KEY REQUEST                   */
         BAL   @14,KEY                                             0114
*       IF STAREQ='1'B              /* FRAME STATUS                  */
*         THEN                                                     0115
@RF00113 TM    STAREQ(PARMPTR),B'00001000'                         0115
         BNO   @RF00115                                            0115
*         CALL STATUS;              /* STATUS REQUEST                */
         BAL   @14,STATUS                                          0116
*       IF CANREQ='1'B              /* CANCEL OFFLINE                */
*         THEN                                                     0117
@RF00115 TM    CANREQ(PARMPTR),B'00000001'                         0117
         BNO   @RF00117                                            0117
*         CALL CANCEL;              /* CANCEL REQUEST                */
         BAL   @14,CANCEL                                          0118
*       ELSE                                                       0119
*         S15=20;                   /* NO REQUEST                    */
         B     @RC00117                                            0119
@RF00117 MVC   S15(4,REG13),@CF00109                               0119
*     END;                                                         0120
*   ELSE                                                           0121
*     S15=12;                       /* HIGHEST FRAME RBN TOO HIGH    */
*                                                                  0121
         B     @RC00102                                            0121
@RF00102 MVC   S15(4,REG13),@CF00032                               0121
*   /*****************************************************************/
*   /*                                                               */
*   /* COMMON EXIT CODE FOR ALL RECONFIGURATION REQUESTS.            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0122
*ALLEXIT:                                                          0122
*   ;                                                              0122
@RC00102 DS    0H                                                  0122
ALLEXIT  DS    0H                                                  0123
*   PVTPRCA=RCAPSAVE;               /* RESTORE PVTPRCA TO VALUE ON 0123
*                                      ENTRY TO IEAVRCF      @ZA09185*/
         L     @08,RCAPSAVE-1(,REG13)                              0123
         LA    @08,0(,@08)                                         0123
         ST    @08,PVTPRCA(,PVTPTR)                                0123
*   IF SALREQ='0'B                  /* IS SALLOC LOCK HELD           */
*     THEN                                                         0124
         TM    SALREQ(PARMPTR),B'00000010'                         0124
         BNZ   @RF00124                                            0124
*     DO;                                                          0125
*       REG0=REG12;                                                0126
         LR    REG0,REG12                                          0126
*       REG1=REG13;                                                0127
         LR    REG1,REG13                                          0127
*       GENERATE;                                                  0128
RE       SETLOCK RELEASE,TYPE=SALLOC,RELATED=(RSM,IEAVRCR(OB))
*       REG12=REG0;                                                0129
         LR    REG12,REG0                                          0129
*       REG13=REG1;                                                0130
         LR    REG13,REG1                                          0130
*     END;                                                         0131
*   GENERATE;                                                      0132
@RF00124 DS    0H                                                  0132
   SETFRR  D,WRKREGS=(REG1,REG2)
   LM   @14,@12,12(@13)              /* DELETE FRR AND RETURN */
   BR   @14
*ONLINE:                                                           0133
*   PROC OPTIONS(DONTSAVE,NOSAVEAREA);                             0133
*                                                                  0133
         B     @PB00002                                            0133
ONLINE   DS    0H                                                  0134
*/********************************************************************/
*/*ONLINE IS CALLED TO TURN ALL REQUESTED FRAMES ONLINE. AT ENTRY THE*/
*/*OFFLINE WAIT PCBRS ARE SEARCHED FOR AN OVERLAP.  IF FOUND AND NOT */
*/*EXACT, RETURN CODE OF 16.  IF FOUND AND EXACT OVERLAP WITH        */
*/*PCBRFAIL ON, REQUEST IS HONORED.  PCBRFAIL BIT IS SET BY CANCEL.  */
*/*IT SET IT SINCE THE PCBR CANNOT BE DEQUEUED UNTIL COMPLETE POSTS  */
*/*THE ECB WITH 4.  THEREFORE, IF THE PCBRFAIL BIT IS OFF, THE       */
*/*OVERLAP, EVEN THOUGH EXACT, IS ILLEGAL AND RETURN CODE 16 IS GIVEN*/
*/*IF OVERLAP IS NOT FOUND OR THERE WAS AN EXACT OVERLAP WITH        */
*/*PCBRFAIL BIT ON  EACH FRAME IN THE REQUEST IS CHECKED FOR ON OR   */
*/*OFF LINE STATUS.  IF THE FRAME IS PRESENTLY OFFLINE, IT IS BROUGT */
*/*ONLINE AND SENT TO THE AFQ.  IF THE FRAME IS ALREADY ONLINE, NO   */
*/*FURTHER ACTION IS NEEDED.                                         */
*/********************************************************************/
*                                                                  0134
*   RESPECIFY                                                      0134
*     PCBRPTR RSTD;                                                0134
*   PCBRPTR=PVTOROOT;                                              0135
         L     PCBRPTR,PVTOROOT(,PVTPTR)                           0135
*ONPCB:                                                            0136
*   IF PCBRPTR^=0                   /* SEARCH PCBR WAIT QUEUE        */
*     THEN                                                         0136
ONPCB    LTR   PCBRPTR,PCBRPTR                                     0136
         BZ    @RF00136                                            0136
*     DO;                                                          0137
*                                                                  0137
*       /*************************************************************/
*       /*                                                           */
*       /* IF EXACT OVERLAP AND PCBRFAIL BIT ON, THEN CANCEL REQUEST */
*       /* HAS RUN. THEREFORE, IT IS OK TO HONOR ONLINE REQUEST SINCE*/
*       /* PCB DOES NOT HAVE TO BE CHANGED. COMPLETE HAS BEEN        */
*       /* SCHEDULED TO POST ECB WITH 4 FOR CANCEL REQUEST.          */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0138
*       IF LORANGE=REQRBN&HIRANGE=HIRBN&PCBRFAIL='1'B THEN         0138
         LH    @07,REQRBN(,PARMPTR)                                0138
         N     @07,@CF02384                                        0138
         C     @07,LORANGE(,PCBRPTR)                               0138
         BNE   @RF00138                                            0138
         C     HIRBN,HIRANGE(,PCBRPTR)                             0138
         BNE   @RF00138                                            0138
         TM    PCBRFAIL(PCBRPTR),B'00001000'                       0138
         BO    @RT00138                                            0138
*         GOTO ONTEST;              /* IF EXACT AND PCBFAIL ON, THEN
*                                      OK                            */
*       IF HIRANGE<REQRBN×LORANGE>HIRBN/* NO OVERLAP, IF < OR >      */
*         THEN                                                     0140
@RF00138 LH    @07,REQRBN(,PARMPTR)                                0140
         N     @07,@CF02384                                        0140
         C     @07,HIRANGE(,PCBRPTR)                               0140
         BH    @RT00140                                            0140
         C     HIRBN,LORANGE(,PCBRPTR)                             0140
         BNL   @RF00140                                            0140
@RT00140 DS    0H                                                  0141
*         DO;                                                      0141
*           PCBRPTR=NEXTPCB;                                       0142
         L     PCBRPTR,NEXTPCB(,PCBRPTR)                           0142
*           GOTO ONPCB;             /* CONTINUE SEARCH               */
         B     ONPCB                                               0143
*         END;                                                     0144
*       S15=16;                                                    0145
@RF00140 MVC   S15(4,REG13),@CF00077                               0145
*       GOTO ALLEXIT;               /* COMMON EXIT ROUTINE           */
         B     ALLEXIT                                             0146
*     END;                                                         0147
*                                                                  0147
*   /*****************************************************************/
*   /*                                                               */
*   /* IF THE FRAME IS OFLINE PREVENT RECLAIM AND SEND IT TO THE AFQ */
*   /* WHERE THE OFFLINE FLAG WILL BE RESET. INCREMENT THE COUNT OF  */
*   /* ONLINE FRAMES. IF IT IS ALREADY ONLINE BUT INTERCEPTED FOR    */
*   /* OFFLINE, RESET THE INTERCEPT FLAG AND INCREMENT THE COUNT OF  */
*   /* ONLINE FRAMES. DURING VARY OFFLINE, THE COUNT WAS DECREMENTED */
*   /* FOR EVERY FRAME IN THE RANGE, WHETHER IT COULD ACTUALLY BE    */
*   /* MARKED OFFLINE OR COULD ONLY BE MARKED AS INTERCEPTED.        */
*   /*                                                       @ZA08772*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0148
*ONTEST:                                                           0148
*   ;                                                              0148
@RF00136 DS    0H                                                  0148
ONTEST   DS    0H                                                  0149
*   RFY                                                            0149
*     REG0 RSTD;                                                   0149
*   DO REG0=REQRBN TO HIRBN BY 16;                                 0150
         LH    REG0,REQRBN(,PARMPTR)                               0150
         N     REG0,@CF02384                                       0150
         B     @DE00150                                            0150
@DL00150 DS    0H                                                  0151
*     IF PFTOFFLN='1'B              /* IF ON, THEN OFFLINE FRAME     */
*       THEN                                                       0151
         TM    PFTOFFLN(PFTEPTR),B'01000000'                       0151
         BNO   @RF00151                                            0151
*       DO;                                                        0152
*         PFTPGID='0'B;             /* THIS IS TO PREVENT POSSIBLE 0153
*                                      RECLAIM OF THE FRAME FOR THE
*                                      USER WHICH LAST HAD THE FRAME
*                                      BEFORE THE FRAME WENT OFFLINE.
*                                                            @YM06715*/
         SLR   @07,@07                                             0153
         ST    @07,PFTPGID(,PFTEPTR)                               0153
*         PFTIRRG='0'B;                                            0154
*                                                                  0154
         NI    PFTIRRG(PFTEPTR),B'11101111'                        0154
*         /***********************************************************/
*         /*                                                         */
*         /* REG0 CONTAINS RBN OF FRAME                              */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0155
*         CALL IEAVPFTE;            /* SEND TO THE AFQ               */
         L     @15,PVTPFTE(,PVTPTR)                                0155
         BALR  @14,@15                                             0155
*         PVTPOOL=PVTPOOL+1;                                       0156
         LA    @07,1                                               0156
         AH    @07,PVTPOOL(,PVTPTR)                                0156
         STH   @07,PVTPOOL(,PVTPTR)                                0156
*       END;                                                       0157
*     ELSE                                                         0158
*       DO;                                                        0158
         B     @RC00151                                            0158
@RF00151 DS    0H                                                  0159
*         IF PFTOFINT='1'B THEN     /*                       @ZA09185*/
         TM    PFTOFINT(PFTEPTR),B'00000001'                       0159
         BNO   @RF00159                                            0159
*           DO;                     /* FRAME IS OFFLINE INTERCEPTED
*                                                            @ZA08772*/
*             PFTOFINT='0'B;                                       0161
         NI    PFTOFINT(PFTEPTR),B'11111110'                       0161
*             PVTPOOL=PVTPOOL+1;    /*                       @ZA08772*/
         LA    @07,1                                               0162
         AH    @07,PVTPOOL(,PVTPTR)                                0162
         STH   @07,PVTPOOL(,PVTPTR)                                0162
*           END;                    /*                       @ZA08772*/
*         PFTBADPG='0'B;                                           0164
@RF00159 NI    PFTBADPG(PFTEPTR),B'11111011'                       0164
*       END;                                                       0165
*     PFTEPTR=PFTEPTR+LENGTH(PFTE);                                0166
@RC00151 AL    PFTEPTR,@CF00077                                    0166
*   END;                                                           0167
         AL    REG0,@CF00077                                       0167
@DE00150 CR    REG0,HIRBN                                          0167
         BNH   @DL00150                                            0167
*   RFY                                                            0168
*     REG0 UNRSTD;                                                 0168
*   PVTMAXFX=PVTPERFX*PVTPOOL/100;  /* CALCULATE THE FIXED FRAME   0169
*                                      THRESHOLD             @YA01572*/
         LA    @07,100                                             0169
         LH    @05,PVTPOOL(,PVTPTR)                                0169
         LH    @11,PVTPERFX(,PVTPTR)                               0169
         MR    @10,@05                                             0169
         DR    @10,@07                                             0169
         STH   @11,PVTMAXFX(,PVTPTR)                               0169
*   PVTFIXOK=PVTPOOL*PVTPEROK/100;  /* DETERMINE THE NUMBER OF FIXED
*                                      FRAMES ACCEPTABLE FOR NORMAL
*                                      SYSTEM PROCESSING     @YA01572*/
         LR    @11,@05                                             0170
         LH    @05,PVTPEROK(,PVTPTR)                               0170
         MR    @10,@05                                             0170
         DR    @10,@07                                             0170
         STH   @11,PVTFIXOK(,PVTPTR)                               0170
*   RESPECIFY                                                      0171
*     REG7 RSTD;                                                   0171
*   REG7=REG13;                                                    0172
         LR    REG7,REG13                                          0172
*   REG13=ADDR(PVTSAVE);            /* INFORM SRM OF AVAILABLE FRAMES*/
         LA    REG13,PVTSAVE(,PVTPTR)                              0173
*   GENERATE REFS(PVTPTR,PVTPOOL,PVTAFCLO) SETS(REG1);             0174
*                                                       /*  @ZA08772*/
   ICM  REG1,12,PVTPOOL(PVTPTR)                             @ZA08772
   ICM  REG1,3,PVTAFCLO(PVTPTR)                             @ZA08772
   SYSEVENT  RSMCNSTS,ENTRY=BRANCH
*   REG13=REG7;                                                    0175
         LR    REG13,REG7                                          0175
*   CALL STATUS;                    /* PROVIDE STATUS FOR REQUEST  0176
*                                      FRAMES                        */
         BAL   @14,STATUS                                          0176
*   RESPECIFY                                                      0177
*     REG7 UNRSTD;                                                 0177
*   END ONLINE;                                                    0178
@EL00002 DS    0H                                                  0178
@EF00002 DS    0H                                                  0178
@ER00002 BR    @14                                                 0178
*OFFLINE:                                                          0179
*   PROC OPTIONS(DONTSAVE,NOSAVEAREA);                             0179
*                                                                  0179
OFFLINE  DS    0H                                                  0180
*/********************************************************************/
*/*OFFLINE IS CALLED TO TAKE REQUESTED FRAMES OFFLINE.  IF THE FRAMES*/
*/*ARE PART OF THE PERMANENTLY RESIDENT STORAGE, THEY CANNOT BE TAKEN*/
*/*OFFLINE.  IF THE FRAMES ARE IN USE FOR SQA,LSQA,VEQR,OR FIXED,    */
*/*THE OFFLINE INTERCEPT BIT IS TURNED ON INORDER TO INTERCEPT THEN  */
*/*WHEN THEY BECOME AVAILABLE.  IF THE FRAMES ARE ON THE AVAILABLE   */
*/*FRAME QUEUE OR IN THE CURRENT ADDRESS SPACE,THEN THE FRAMES CAN   */
*/*BE TAKEN OFF IMMEDIATELY. OFFLINE WILL DO A GETCELL FOR THE SRB   */
*/*SPACE NEEDED TO SCHEDULE IEFVRCFC.                       @Z30GNYD */
*/********************************************************************/
*                                                                  0180
*   PCBRPTR=PVTOROOT;                                              0180
         L     PCBRPTR,PVTOROOT(,PVTPTR)                           0180
*   PRCNT=0;                                                       0181
         SLR   @07,@07                                             0181
         ST    @07,PRCNT(,REG13)                                   0181
*OFFPCB:                                                           0182
*   ;                                                              0182
OFFPCB   DS    0H                                                  0183
*   IF PCBRPTR^=0                   /* IF ^= 0, THEN CHECK IF OVERLAP*/
*     THEN                                                         0183
         LTR   PCBRPTR,PCBRPTR                                     0183
         BZ    @RF00183                                            0183
*     DO;                                                          0184
*       IF HIRANGE<REQRBN×LORANGE>HIRBN/* IF < OR >, NO OVERLAP      */
*         THEN                                                     0185
         LH    @07,REQRBN(,PARMPTR)                                0185
         N     @07,@CF02384                                        0185
         C     @07,HIRANGE(,PCBRPTR)                               0185
         BH    @RT00185                                            0185
         C     HIRBN,LORANGE(,PCBRPTR)                             0185
         BNL   @RF00185                                            0185
@RT00185 DS    0H                                                  0186
*         DO;                                                      0186
*           PCBRPTR=NEXTPCB;                                       0187
         L     PCBRPTR,NEXTPCB(,PCBRPTR)                           0187
*           GOTO OFFPCB;            /* REPEAT PROCESS                */
         B     OFFPCB                                              0188
*         END;                                                     0189
*       ELSE                                                       0190
*         DO;                                                      0190
@RF00185 DS    0H                                                  0191
*           S15=16;                                                0191
         MVC   S15(4,REG13),@CF00077                               0191
*           GOTO ALLEXIT;           /* RETURN                        */
         B     ALLEXIT                                             0192
*         END;                                                     0193
*     END;                                                         0194
*   RESPECIFY                                                      0195
*     PCBRPTR UNRSTD;                                              0195
@RF00183 DS    0H                                                  0196
*   IF REQRBN>=PVTFPFN              /* FIRST FRAME ABOVE PERMANENT 0196
*                                      RESIDENT AREA                 */
*     THEN                                                         0196
         CLC   REQRBN(2,PARMPTR),PVTFPFN(PVTPTR)                   0196
         BL    @RF00196                                            0196
*     DO RBN=REQRBN TO HIRBN BY 16;                                0197
         LH    @08,REQRBN(,PARMPTR)                                0197
         N     @08,@CF02384                                        0197
         ST    @08,RBN(,REG13)                                     0197
         B     @DE00197                                            0197
@DL00197 DS    0H                                                  0198
*       IF PFTOFFLN='0'B            /* IF 0, FRAME IS ONLINE         */
*         THEN                                                     0198
         TM    PFTOFFLN(PFTEPTR),B'01000000'                       0198
         BNZ   @RF00198                                            0198
*         DO;                                                      0199
*           PVTPOOL=PVTPOOL-1;                                     0200
         LH    @08,PVTPOOL(,PVTPTR)                                0200
         BCTR  @08,0                                               0200
         STH   @08,PVTPOOL(,PVTPTR)                                0200
*           IF PFTONAVQ='1'B        /* ON AVAILABLE FRAME QUEUE      */
*             THEN                                                 0201
         TM    PFTONAVQ(PFTEPTR),B'10000000'                       0201
         BNO   @RF00201                                            0201
*             DO;                                                  0202
*               REG0=PFTNQN;                                       0203
         LA    REG0,255                                            0203
*               GEN( SLL  REG0,16) ;                               0204
          SLL  REG0,16
*               GEN( ICM REG0,3,RBN+2(REG13)) ;                    0205
          ICM REG0,3,RBN+2(REG13)
*               CALL IEAVPFTE;      /* PFTE MANAGER                  */
         L     @15,PVTPFTE(,PVTPTR)                                0206
         BALR  @14,@15                                             0206
*               PFTOFFLN='1'B;                                     0207
         OI    PFTOFFLN(PFTEPTR),B'01000000'                       0207
*             END;                                                 0208
*           ELSE                                                   0209
*             DO;                                                  0209
         B     @RC00201                                            0209
@RF00201 DS    0H                                                  0210
