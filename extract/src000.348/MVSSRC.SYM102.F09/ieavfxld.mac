         TITLE 'IEAVFXLD: PAGE FIX/LOAD'
IEAVFXLD CSECT
         USING *,15
         MODID BR=NO
         ENTRY IEAVFXL2
IEAVFXL2 DS    0H
         ST    @14,CIWRETRN(CIWAPTR)        SAVE RETURN REGISTER
         BALR  @09,@00                      ESTABLISH ADDRESSABILITY
         USING *,@09
*   RCAFXLD='1'B;                   /* SET ENTRY POINT FLAG          */
         L     @14,RCAPTR(,PVTPTR)                                 0096
         OI    RCAFXLD(@14),B'00000100'                            0096
*   RCACSECT=RCACPGFL;              /* SET MODULE ID                 */
         MVI   RCACSECT(@14),X'16'                                 0097
*                                                                  0098
*   /*****************************************************************/
*   /*                                                               */
*   /* LOOPVSL --- THIS IS THE LARGEST LOOP OF FIX/LOAD. THE FIRST   */
*   /* VSL ENTRY TO BE PROCESSED IS IN THE CIWVSL. THE START AND END */
*   /* ADDRESSES ARE COMPARED, AND THE LOOP BROKEN WHEN START IS     */
*   /* GREATER THAN END. THE LAST PAGE TO BE PROCESSED, CIWLSTPG, IS */
*   /* CALCULATED BY TRUNCATING CIWEND1A - 1 TO A PAGE BOUNDRY. THE  */
*   /* START ADDRESS IS TRUNCATED TO A PAGE BOUNDRY AND STORED IN    */
*   /* CIWCURPG. CONTROL THEN FALLS TO NEXTPGLP. CONTROL WILL RETURN */
*   /* TO VSLLOOP WHEN THE NEXTVSL MODULE IS CALLED AND RETURNS A    */
*   /* CODE OF ZERO.                                                 */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0098
*LOOPVSL:                                                          0098
*   IF CIWSTRTA>=CIWEND1A THEN      /* IF STARTING ADDRESS IS GREATER
*                                      THEN ENDING ADDRESS           */
LOOPVSL  CLC   CIWSTRTA(3,CIWAPTR),CIWEND1A(CIWAPTR)               0098
         BL    @RF00098                                            0098
*     DO;                                                          0099
*       CIWRETC=16;                 /* SET CIW RETURN CODE TO 16     */
         MVI   CIWRETC(CIWAPTR),X'10'                              0100
*       GOTO ERREXITA;              /* ERROR-START GREATER THEN END  */
         B     ERREXITA                                            0101
*     END;                                                         0102
*   RFY                                                            0103
*    (WORKREG,                                                     0103
*     TESTADDR,                                                    0103
*     REALPGR) RSTD;                                               0103
@RF00098 DS    0H                                                  0104
*   WORKREG=CIWEND1A-1;             /* LOAD THE LAST PAGE POINTER    */
         SLR   WORKREG,WORKREG                                     0104
         ICM   WORKREG,7,CIWEND1A(CIWAPTR)                         0104
         BCTR  WORKREG,0                                           0104
*   WORKREG=WORKREG&TRUNCWD;        /* TRUNCATE TO PAGE BOUNDRY      */
         L     @14,@CF02248                                        0105
         NR    WORKREG,@14                                         0105
*   CIWLSTPG=WORKREG;               /* STORE ROUNDED PAGE ADDRESS    */
         ST    WORKREG,CIWLSTPG(,CIWAPTR)                          0106
*   TESTADDR=CIWSTRTA;              /* LOAD START ADDRESS OF PAGE    */
         SLR   TESTADDR,TESTADDR                                   0107
         ICM   TESTADDR,7,CIWSTRTA(CIWAPTR)                        0107
*   TESTADDR=TESTADDR&TRUNCWD;      /* TRUNCATE TO PAGE BOUNDRY      */
         NR    TESTADDR,@14                                        0108
*                                                                  0109
*   /*****************************************************************/
*   /*                                                               */
*   /* NEXTPGLP --- THIS LOOP PROCESSES THE PAGE ADDRESSED IN        */
*   /* CIWCURPG. AFTER THE CURRENT PAGE IS PROCESSED, CONTROL WILL   */
*   /* RETURN TO THIS POINT FROM PAGELOOP IF THERE IS ANOTHER PAGE TO*/
*   /* PROCESS. NEXTPGLP PROCESSING FIRST DETERMINES IF THE PAGE IS  */
*   /* RESIDENT IN STOREAGE OR NOT, THE LRA INSTRUCTION PERFORMING   */
*   /* THIS TEST. IF THE PAGE IS IN AN INVALID SEGMENT, A USER ERROR */
*   /* IS INDICATED. IF NOT INVALID, FINDPAGE IS CALLED TO OBTAIN    */
*   /* PGTE AND XPTE ADDRESSES. THE PGTPAM FLAG IS TESTED TO INSURE  */
*   /* THAT THE PAGE IS GETMAINED. THE RESIDENCY OF THE PAGE IS THEN */
*   /* TESTED. IF AN IN STORAGE PAGE, ( PGTPVM=0 ), PROCESSING FALLS */
*   /* THROUGH TO THE IN STORAGE CODE, ELSE CONTROL IS PASSED TO THE */
*   /* OUTOFSTG ROUTINE.                                             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0109
*NEXTPGLP:                                                         0109
*   CIWCURPG=TESTADDR;              /* STORE CURRENT PAGE ADDRESS    */
NEXTPGLP ST    TESTADDR,CIWCURPG(,CIWAPTR)                         0109
*   GENERATE REFS(TESTADDR) SETS(REALPGR);                         0110
         LRA    REALPGR,0(TESTADDR)    DETERMINE IF SEGMENT IS VALID
*   RFY                                                            0111
*    (WORKREG,                                                     0111
*     REALPGR) UNRSTD;                                             0111
*   BC(5,ERREXIT);                  /* PAGE RESIDES IN INVALID     0112
*                                      SEGMENT OR SEGMENT LENGTH   0112
*                                      VIOLATION                     */
         BC    5,ERREXIT                                           0112
*   RFY                                                            0113
*    (R0,                                                          0113
*     R1) RSTD;                                                    0113
*   R1=TESTADDR;                    /* LOAD ADDRESS FOR FINDPAGE. REG
*                                      1 IS A CONVENTION AND MUST NOT
*                                      CHANGE                        */
         LR    R1,TESTADDR                                         0114
*   CALL IEAVFP2;                   /* GET PGTBL AND XPGTBL PTRS     */
         L     @15,PVTPFP2(,PVTPTR)                                0115
         BALR  @14,@15                                             0115
*   IF RETCODR^=0 THEN              /* IF FINDPAGE RETURNS A NONZERO
*                                      RETURN CODE                   */
         LTR   RETCODR,RETCODR                                     0116
         BZ    @RF00116                                            0116
*     DO;                                                          0117
*       RCARCRD='01'X;              /* SET RECOVERY CODE             */
         L     @14,RCAPTR(,PVTPTR)                                 0118
         MVI   RCARCRD(@14),X'01'                                  0118
*       GENERATE SETS(R1);                                         0119
         ABEND  X'C0D',,,SYSTEM        ISSUE ABEND FOR FINDPAGE FAIL
*       GOTO ERREXIT;               /* ERROR-FINDPAGE FAILED         */
         B     ERREXIT                                             0120
*     END;                                                         0121
*   RFY                                                            0122
*    (PTEPTR,                                                      0122
*     XPTEPTR) RSTD;                                               0122
@RF00116 DS    0H                                                  0123
*   PTEPTR=R0;                      /* LOAD PAGE TABLE PTR AS      0123
*                                      RETURNED BY FINDPAGE          */
         LR    PTEPTR,R0                                           0123
*   XPTEPTR=R1;                     /* LOAD EXT PAGE TABLE PTR AS  0124
*                                      RETURNED BY FINDPAGE          */
         LR    XPTEPTR,R1                                          0124
*   RFY                                                            0125
*    (R0,                                                          0125
*     R1) UNRSTD;                                                  0125
*   IF PGTPAM='0'B THEN             /* IF PAGE IS NOT GM ASSIGNED    */
         TM    PGTPAM(PTEPTR),B'00000001'                          0126
         BZ    @RT00126                                            0126
*     GOTO ERREXIT;                 /* ERROR-PAGE NOT GM ASSIGNED    */
*   IF PGTPVM='1'B THEN             /* IF PAGE IS NOT IN REAL CORE   */
         TM    PGTPVM(PTEPTR),B'00001000'                          0128
         BO    @RT00128                                            0128
*     GOTO OUTOFSTG;                /* GO TO THE OUT OF STG ROUTINE  */
*                                                                  0130
*   /*****************************************************************/
*   /*                                                               */
*   /* IN STORAGE PROCESSING --- AN IMMEDIATE TEST IS MADE TO        */
*   /* DETERMINE IF A FIX OR A LOAD IS REQUESTED. FOR LOAD REQUESTS, */
*   /* NO IN-STORAGE PROCESSING IS REQUIRED. FOR FIX REQUESTS, THE   */
*   /* PAGE MUST BE MADE NON-PAGEABLE. IF THE VIRTUAL ADDRESS IS     */
*   /* BELOW THE PRIVATE AREA IT IS ASSUMED TO BE IN THE NUCLEUS AND */
*   /* IS ALREADY FIXED, OTHERWISE THE PFTE MUST BE LOCATED TO       */
*   /* DETERMINE IF THE PAGE IS IN SQA, LSQA, OR IF IT IS A V=R      */
*   /* ALLOCATED PAGE. IF SO, IT IS NON-PAGEABLE AND FIXING IS NOT   */
*   /* REQUIRED. IF A FIX IS REQUESTED FOR A VAM PAGE, AN ERROR      */
*   /* CONDITION EXISTS. IF THE PAGE IS FIXABLE AND NOT ALREADY FIXED*/
*   /* AND BACKED BY A NON-PREFERRED PAGE FURTHER CHECKS ARE MADE TO */
*   /* DETERMINE IF THE PAGE SHOULD BE FIXED IN THE PREFERRED AREA.  */
*   /* PAGES SHOULD BE FIXED IN THE PREFERRED AREA IF IT IS (1) A    */
*   /* COMMON AREA PAGE OR (2) IT IS A PRIVATE AREA PAGE AND THE USER*/
*   /* IS A SECOND LEVEL PREFERRED USER OR (3) THE REQUEST IS FOR A  */
*   /* LONG-TERM PRIVATE AREA FIX AND USER IS A FIRST LEVEL PREFERRED*/
*   /* USER.                                                 @ZA16816*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0130
*   IF CIWFIX='1'B THEN             /* IF THIS IS A FIX REQUEST, IN
*                                      STORAGE PROCESSING IS REQUIRED*/
         TM    CIWFIX(CIWAPTR),B'01000000'                         0130
         BNO   @RF00130                                            0130
*     DO;                                                          0131
*       IF CURVBN<PVTLPRIV THEN     /* IF PAGE IS WITHIN NUCLEUS     */
         CLC   CURVBN(2,CIWAPTR),PVTLPRIV(PVTPTR)                  0132
         BL    @RT00132                                            0132
*         GOTO PAGELOOP;            /* NO FIXING REQUIRED            */
*       RFY                                                        0134
*        (PFTEPTR) RSTD;                                           0134
*       PFTEPTR=PGTRSA;             /* LOAD INDEX FOR PFTE           */
         SLR   PFTEPTR,PFTEPTR                                     0135
         ICM   PFTEPTR,3,PGTRSA(PTEPTR)                            0135
*       PFTEPTR=PFTEPTR&ANDWORD;    /* FORCE LOW FOUR BITS TO ZERO   */
         N     PFTEPTR,@CF02250                                    0136
*       SAVWRK2=PFTEPTR;            /* SAVE RBN                      */
         ST    PFTEPTR,SAVWRK2(,PVTPTR)                            0137
*       PFTEPTR=PFTEPTR+PVTPFTP;    /* CALCULATE FULL ADDRESS OF PFTE*/
         AL    PFTEPTR,PVTPFTP(,PVTPTR)                            0138
*       IF PFTLSQA='1'B×PFTVRALC='1'B THEN/* IF THE PAGE IS IN SQA OR
*                                      LSQA, OR IF THE PAGE IS A V=R
*                                      ALLOCATED PAGE                */
         TM    PFTLSQA(PFTEPTR),B'00100010'                        0139
         BNZ   @RT00139                                            0139
*         GOTO PAGELOOP;            /* NO FIXING REQUIRED            */
*       IF XPTVIO='1'B THEN         /* VIO WINDOW PAGES CANNOT BE  0141
*                                      FIXED                 @Z40WPYD*/
         TM    XPTVIO(XPTEPTR),B'00001000'                         0141
         BO    @RT00141                                            0141
*         GOTO ERREXIT;             /* ERROR - CANT FIX VIO PAGES    */
*       IF PFTPREF='0'B&            /* FRAME IS NON-PREFERRED      0143
*                                                            @ZA16816*/
*           PFTFXCT=0 THEN          /* AND NOT ALREADY FIXED @ZA16816*/
         TM    PFTPREF(PFTEPTR),B'00000100'                        0143
         BNZ   @RF00143                                            0143
         ICM   @14,3,PFTFXCT(PFTEPTR)                              0143
         BNZ   @RF00143                                            0143
*         DO;                       /* CHECK FURTHER---      @ZA16816*/
*           IF CURVBN>=PVTLCSA THEN /* IF COMMON AREA, THEN  @ZA16816*/
         CLC   CURVBN(2,CIWAPTR),PVTLCSA(PVTPTR)                   0145
         BNL   @RT00145                                            0145
*             GOTO OUTOFSTG;        /* FIX IN PREFERRED AREA @ZA16816*/
*           IF ASCB1LPU='1'B THEN   /* IF LEV 1 USER, FIX IN @ZA16816*/
         L     @14,ASCBPTR                                         0147
         TM    ASCB1LPU(@14),B'01000000'                           0147
         BNO   @RF00147                                            0147
*             IF CIWLONG='1'B×      /* PREF AREA IF LONG-FIX @ZA16816*/
*                 ASCB2LPU='1'B THEN/* OR IF LEV 2 USER, ANY @ZA16816*/
         TM    CIWLONG(CIWAPTR),B'00000010'                        0148
         BO    @RT00148                                            0148
         TM    ASCB2LPU(@14),B'10000000'                           0148
         BO    @RT00148                                            0148
*               GOTO OUTOFSTG;      /* FIX GOES IN PREFERRED @ZA16816*/
*         END;                      /*                       @ZA16816*/
@RF00147 DS    0H                                                  0151
*       PFTFXCT=PFTFXCT+1;          /* INCREMENT FXCT        @ZA16816*/
@RF00143 LA    @14,1                                               0151
         AH    @14,PFTFXCT(,PFTEPTR)                               0151
         STH   @14,PFTFXCT(,PFTEPTR)                               0151
*       RFY                                                        0152
*        (INPUTVBN) RSTD;                                          0152
*       INPUTVBN=CURVBN;            /* LOAD PARM FOR FOEADD          */
         SLR   INPUTVBN,INPUTVBN                                   0153
         ICM   INPUTVBN,3,CURVBN(CIWAPTR)                          0153
*       CALL FOEADD;                /* CALL ROUTINE TO ADD FOE TO  0154
*                                      LIST                          */
         BAL   @14,FOEADD                                          0154
*       IF RETCODR^=0 THEN          /* IF FOEADD HAD GETMAIN FAILURE */
         LTR   RETCODR,RETCODR                                     0155
         BZ    @RF00155                                            0155
*         DO;                                                      0156
*           PFTFXCT=PFTFXCT-1;      /* RESTORE FIX COUNT     @ZA16816*/
         LH    @14,PFTFXCT(,PFTEPTR)                               0157
         BCTR  @14,0                                               0157
         STH   @14,PFTFXCT(,PFTEPTR)                               0157
*           CIWRETC=20;             /* MARK ENVIRONMENTAL ERROR      */
         MVI   CIWRETC(CIWAPTR),X'14'                              0158
*           GOTO ERREXITA;          /* ERROR-FOE GETMAIN FAILED      */
         B     ERREXITA                                            0159
*         END;                                                     0160
*       IF PFTFXCT=1 THEN           /*                       @YA01572*/
@RF00155 LA    @14,1                                               0161
         CH    @14,PFTFXCT(,PFTEPTR)                               0161
         BNE   @RF00161                                            0161
*         DO;                       /*                       @YA01572*/
*           IF PFTQNDX<PFTLFQN THEN /* IS IT A GLOBAL Q      @YA01572*/
         CLI   PFTQNDX(PFTEPTR),X'80'                              0163
         BNL   @RF00163                                            0163
*             PVTCOMFX=PVTCOMFX+1;  /* INCREMENT COUNT       @YA01572*/
         AH    @14,PVTCOMFX(,PVTPTR)                               0164
         STH   @14,PVTCOMFX(,PVTPTR)                               0164
*           ELSE                    /* NO, IT IS LOCAL Q     @YA01572*/
*             RSMCNTFX=RSMCNTFX+1;  /* INCREMENT COUNT       @YA01572*/
         B     @RC00163                                            0165
@RF00163 LA    @14,1                                               0165
         AH    @14,RSMCNTFX(,RSMHDPTR)                             0165
         STH   @14,RSMCNTFX(,RSMHDPTR)                             0165
*           PVTCNTFX=PVTCNTFX+1;    /* INCREMENT PVTCNTFX    @YA01572*/
@RC00163 LA    @14,1                                               0166
         AH    @14,PVTCNTFX(,PVTPTR)                               0166
         STH   @14,PVTCNTFX(,PVTPTR)                               0166
*           IF PVTCNTFX=PVTMAXFX THEN/* CHECK THRESHOLD      @YA01572*/
         CH    @14,PVTMAXFX(,PVTPTR)                               0167
         BNE   @RF00167                                            0167
*             DO;                   /*                       @YA01572*/
*               RESPECIFY                                          0169
*                (R1,                                              0169
*                 R13) RESTRICTED;                                 0169
*               R13=ADDR(PVTSAVE);  /* SAVE AREA FOR SRM     @YA01572*/
         LA    R13,PVTSAVE(,PVTPTR)                                0170
*               R1=MAXSYS;          /* CODE FOR MAX FIX-COUNT      0171
*                                      THRESHOLD REACHED     @YA01572*/
         LA    R1,4                                                0171
*               GENERATE NODEFS REFS(CVTPTR,CVTOPTE,CVT) NOSETS NOFLOWS
*                   ;                                              0172
              SYSEVENT  AVQLOW,ENTRY=BRANCH
*               RESPECIFY                                          0173
*                (R1,                                              0173
*                 R13) UNRESTRICTED;                               0173
*             END;                  /* THRESHOLD REACHED     @YA01572*/
*           ELSE                                                   0175
*             ;                     /* NOT REACHED           @YA01572*/
@RF00167 DS    0H                                                  0176
*         END;                      /* END PFTFXCT= 0        @YA01572*/
*       ELSE                                                       0177
*         ;                         /* PFTFXCT NOT ZERO      @YA01572*/
@RF00161 DS    0H                                                  0178
*       RFY                                                        0178
*        (PFTEPTR,                                                 0178
*         INPUTVBN) UNRSTD;                                        0178
*     END;                                                         0179
*                                                                  0180
*   /*****************************************************************/
*   /*                                                               */
*   /* PAGELOOP --- THIS ROUTINE IS NOT REALLY A LOOP, BUT A         */
*   /* CALCULATION AND TEST THAT EITHER RETURNS TO LOOPVSL OR        */
*   /* NEXTPGLP AS REQUIRED, OR PASSES CONTROL TO THE ERREXIT OR     */
*   /* NORMEXIT AS REQUIRED. IF ONE PAGE PAST THE CURRENT PAGE IS NOT*/
*   /* GREATER THAN CIWLSTPG, THEN NEXTPGLP IS GIVEN CONTROL TO      */
*   /* PROCESS THE NEXT PAGE. OTHERWISE THE NEXTVSL ROUTINE IS       */
*   /* CALLED. IF NEXTVSL RETURNS ANOTHER VSL LIST CONTROL IS PASSED */
*   /* TO VSLLOOP. IF NEXTVSL RETURNS AN ERROR CODE THE ERREXITA     */
*   /* ROUTINE IS CALLED. IF NO ERROR AND NO VSL LIST IS RETURNED,   */
*   /* THE NORMEXIT ROUTINE IS CALLED.                               */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0180
*PAGELOOP:                                                         0180
*   TESTADDR=CIWCURPG+BLOCKSIZ;     /* UPDATE TO NEXT PAGE           */
@RF00130 DS    0H                                                  0180
PAGELOOP LH    TESTADDR,@CH02252                                   0180
         AL    TESTADDR,CIWCURPG(,CIWAPTR)                         0180
*   IF TESTADDR^>CIWLSTPG THEN      /* IF THIS PAGE IS TO BE       0181
*                                      PROCESSED                     */
         C     TESTADDR,CIWLSTPG(,CIWAPTR)                         0181
         BNH   @RT00181                                            0181
*     GOTO NEXTPGLP;                /* GO TO PAGE PROCESSING LOOP    */
*   IF CIWINVSL^=0 THEN             /* IF NOT A REGISTER FORM ENTRY  */
         SLR   @14,@14                                             0183
         C     @14,CIWINVSL(,CIWAPTR)                              0183
         BE    @RF00183                                            0183
*     DO;                                                          0184
*       CIWCURPG=0;                 /* CLEAR                         */
         ST    @14,CIWCURPG(,CIWAPTR)                              0185
*       CIWLSTPG=0;                 /* CLEAR                         */
         ST    @14,CIWLSTPG(,CIWAPTR)                              0186
*       CALL NEXTVSL;               /* CALL TO GET A NEW VSL ENTRY   */
         L     @15,@CV02299                                        0187
         BALR  @14,@15                                             0187
*       IF RETCODR=0 THEN           /* IF NEXTVSL RETURNED A ZERO  0188
*                                      R.C.                          */
         LTR   RETCODR,RETCODR                                     0188
         BZ    @RT00188                                            0188
*         GOTO LOOPVSL;             /* PROCESS A NEW GROUP           */
*       IF RETCODR^=8 THEN          /* R.C. FROM NEXTVSL WAS NOT 0,
*                                      AND IS NOT 8                  */
         CH    RETCODR,@CH00059                                    0190
         BE    @RF00190                                            0190
*         DO;                                                      0191
*           CIWRETC=16;             /* SET RETURN CODE TO 16         */
         MVI   CIWRETC(CIWAPTR),X'10'                              0192
*           GOTO ERREXITA;          /* ERROR-BAD RETURN CODE FROM  0193
*                                      NEXTVSL                       */
         B     ERREXITA                                            0193
*         END;                                                     0194
*     END;                                                         0195
*   GOTO NORMEXIT;                  /* NO MORE TO PROCESS, GO TO EXIT*/
         B     NORMEXIT                                            0196
*                                                                  0197
*   /*****************************************************************/
*   /*                                                               */
*   /* OUTOFSTG --- THIS ROUTINE IS PERFORMED FOR BOTH PGFIX AND     */
*   /* PGLOAD IF THE REQUESTED PAGES ARE NOT IN REAL STOREAGE. IF A  */
*   /* FIX HAS BEEN REQUESTED FOR A VIO WINDOW PAGE, CONTROL PASSES  */
*   /* TO THE ERROR ROUTINE, AS WINDOW PAGES CANNOT BE FIXED. IF A   */
*   /* PCB EXISTS ON THE INTERNAL QUEUE (CIWWRK1), CONTROL PASSES TO */
*   /* PAGELOOP FOR LOAD REQUESTS. FOR FIX REQUESTS, AN FOE IS ADDED */
*   /* TO THE LIST BEFORE GOING TO PAGELOOP. IF NO PCB EXISTS, ONE   */
*   /* MUST BE CREATED. A CHECK IS MADE TO DETERMINE IF A LONG FIX   */
*   /* WAS REQUESTED, AS THIS CODE IS USED FOR PCB CREATION FOR PAGES*/
*   /* ALREADY IN REAL CORE BUT IN POTENTIAL V=R FRAMES. THE PCB IS  */
*   /* ASSOCIATED WITH A ROOT PCB IF THE REQUESTOR SUPPLIED AN ECB.  */
*   /* IN THE CASE OF USER REQUESTED LOADS, PROCESSING WILL TERMINATE*/
*   /* IF THE PCB FREE QUEUE BECOMES CRITICALLY LOW (REACHES         */
*   /* THRESHOLD), AND PREVIOUSLY PROCESSED PCBS WILL BE BACKED OUT. */
*   /* IN CASE THE PCB MANAGER RETURNS AN ERROR CODE, THIS ROUTINE   */
*   /* WILL SET RETURN CODE OF 20 FOR FIX REQUESTS, OR ZERO FOR LOAD */
*   /* REQUESTS. OTHERWISE THE PCB IS ADDED TO THE QUEUE             */
*   /* (CIWWRK1-CIWWRK2).                                            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0197
*   RFY                                                            0197
*    (PCBPTR) RSTD;                                                0197
*OUTOFSTG:                                                         0198
*   IF CIWFIX='1'B&XPTVIO='1'B THEN /* VIO WINDOW PAGES CANT BE FIXED
*                                                            @Z40WPYD*/
OUTOFSTG TM    CIWFIX(CIWAPTR),B'01000000'                         0198
         BNO   @RF00198                                            0198
         TM    XPTVIO(XPTEPTR),B'00001000'                         0198
         BO    @RT00198                                            0198
*     GOTO ERREXIT;                 /* ERROR - CANT FIX WINDOW PAGES */
*   PCBPTR=CIWWRK1;                 /* SET PCBPTR TO FIRST ON QUE    */
*                                                                  0200
@RF00198 L     PCBPTR,CIWWRK1(,CIWAPTR)                            0200
*   /*****************************************************************/
*   /*                                                               */
*   /* TRY TO FIND THE ASSOCIATED PCB                                */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0201
*   DO WHILE PCBPTR^=0;             /* LOOP AS LONG AS THERE IS A PCB*/
         B     @DE00201                                            0201
@DL00201 DS    0H                                                  0202
*     IF PCBVBN=CURVBN THEN         /* IF THIS PCB IS FOR CURRENT  0202
*                                      PAGE                          */
         CLC   PCBVBN(2,PCBPTR),CURVBN(CIWAPTR)                    0202
         BNE   @RF00202                                            0202
*       DO;                                                        0203
*         IF CIWFIX='0'B THEN       /* IF THIS IS A LOAD REQUEST     */
         TM    CIWFIX(CIWAPTR),B'01000000'                         0204
         BZ    @RT00204                                            0204
*           GOTO PAGELOOP;          /* NO FURTHER OUT OF PAGE      0205
*                                      PROCESSING IS REQUIRED, RETURN
*                                      TO PAGELOOP                   */
*         PCBFXC=PCBFXC+1;          /* INCREMENT FIX COUNT IN PCB    */
         LA    @14,1                                               0206
         SLR   @12,@12                                             0206
         IC    @12,PCBFXC(,PCBPTR)                                 0206
         ALR   @14,@12                                             0206
         STC   @14,PCBFXC(,PCBPTR)                                 0206
*         RFY                                                      0207
*          (INPUTVBN) RSTD;                                        0207
*         INPUTVBN=CURVBN;          /* LOAD PARM FOR FOEADD          */
         SLR   INPUTVBN,INPUTVBN                                   0208
         ICM   INPUTVBN,3,CURVBN(CIWAPTR)                          0208
*         CALL FOEADD;              /* CALL TO ADD FOE TO CHAIN      */
         BAL   @14,FOEADD                                          0209
*         RFY                                                      0210
*          (INPUTVBN) UNRSTD;                                      0210
*         IF RETCODR^=0 THEN        /* IF FOEADD HAD GETMAIN FAILURE */
         LTR   RETCODR,RETCODR                                     0211
         BZ    @RF00211                                            0211
*           DO;                                                    0212
*             CIWRETC=20;           /* MARK ENVIRONMENTAL ERROR      */
         MVI   CIWRETC(CIWAPTR),X'14'                              0213
*             GOTO ERREXITA;        /* ERROR-FOE GETMAIN FAILED      */
         B     ERREXITA                                            0214
*           END;                                                   0215
*         GOTO PAGELOOP;            /* NO FURTHER OUT OF STG       0216
*                                      PROCESSING REQUIRED           */
         B     PAGELOOP                                            0216
*       END;                                                       0217
*     PCBPTR=PCBFQPA;               /* PICK UP NEXT PCB ON CHAIN     */
@RF00202 SLR   @14,@14                                             0218
         ICM   @14,7,PCBFQPA(PCBPTR)                               0218
         LR    PCBPTR,@14                                          0218
