*   /*                                                               */
*   /* FOEDEL --- THIS SUBROUTINE IS USED BY THIS MODULE AND BY      */
*   /* PGFREE. IT SEARCHES FOR AN FOE FOR THE PAGE BEING PROCESSED,  */
*   /* AND DECREMENTS THE FIX COUNT BY 1 IF NOT ALREADY ZERO. IF     */
*   /* ALREADY ZERO, THE FOE IS DEQUEUED FROM THE TCB AND INSERTED ON*/
*   /* THE AVAILABLE QUEUE (RSMFOEQ). THE RETURN CODE IS THEN SET TO */
*   /* 4. IF THE FIX COUNT IS ZERO AS A RESULT OF DECREMENTING, THE  */
*   /* FOE IS DEQUEUED AND INSERTED ON RSMFOEQ BUT THE RETURN CODE IS*/
*   /* SET TO ZERO. INPUT TO THIS ROUTINE IS THE VBN SUPPLIED BY THE */
*   /* CALLER IN REG 1. REGISTERS ARE SAVED AND RESTORED FOR THE     */
*   /* CALLER. IF NO FOE IS FOUND, A RETURN CODE OF 4 IS ISSUED. RSM */
*   /* POINTERS ARE NOT MAINTAINED IN THIS ROUTINE, BUT ARE RESTORED */
*   /* PRIOR TO RETURN TO THE CALLER.                                */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0438
*FOEDEL:                                                           0438
*   PROC OPTIONS(NOSAVE,NOSAVEAREA,RETREG);                        0438
FOEDEL   DS    0H                                                  0439
*   RFY                                                            0439
*    (OLDFOE,                                                      0439
*     CURFOE,                                                      0439
*     INPUTVBN,                                                    0439
*     PTRSAVR) RSTD;                                               0439
*   GENERATE;                                                      0440
         ENTRY  FOEDEL
         STM    @00,@14,PVTWSA2(PVTPTR)   STORE CALLERS REGS
         BALR   @09,@00                   ESTABLISH ADDRESSABILITY
         USING  *,@09
*   SAV15=0;                        /* CLEAR THE RET CODE FOR LATER  */
         SLR   @14,@14                                             0441
         ST    @14,SAV15(,PVTPTR)                                  0441
*   IF CIWTCB=0 THEN                /* IF THERE IS NO TCB            */
         C     @14,CIWTCB(,CIWAPTR)                                0442
         BNE   @RF00442                                            0442
*     DO;                                                          0443
*       SAV15=4;                    /* SET RETURN CODE TO 4          */
         MVC   SAV15(4,PVTPTR),@CF00053                            0444
*       GOTO ENDDEL;                /* GOING BACK TO CALLER          */
         B     ENDDEL                                              0445
*     END;                                                         0446
*   OLDFOE=ADDR(TCBFOE);            /* GET THE QUEUE POINTER ADDRESS */
@RF00442 L     @14,TCBPTR(,CIWAPTR)                                0447
         LA    OLDFOE,TCBFOE(,@14)                                 0447
*   CURFOE=TCBFOEA;                 /* GET POINTER TO FIRST FOE      */
         SLR   CURFOE,CURFOE                                       0448
         ICM   CURFOE,7,TCBFOEA(@14)                               0448
*LASTCK:                                                           0449
*   IF CURFOE=0×FOEVINDX>INPUTVBN THEN/* IF CHAIN IS EMPTY, OR IF NO
*                                      FOE FOR THIS PAGE             */
