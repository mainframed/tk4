IEAVFXL  STM      @00,@15,PVTWSA7(PVTPTR)   STORE CALLERS REGS
         BALR     @09,@00                   ESTABLISH ADDRESSABILITY
         USING    *,@09
*   RCACSECT=RCACPGFL;              /* SET MODULE ID                 */
         L     @14,RCAPTR(,PVTPTR)                                 0476
         MVI   RCACSECT(@14),X'16'                                 0476
*   RCAFXL='1'B;                    /* SET ENTRY POINT FLAG          */
         OI    RCAFXL(@14),B'01000000'                             0477
*   PCBRPTR=R1;                     /* PICK UP ROOT POINTER FROM PARM*/
