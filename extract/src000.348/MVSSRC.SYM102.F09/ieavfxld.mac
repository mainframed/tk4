         BZ    @RT00449                                            0449
         SLR   @14,@14                                             0449
         ICM   @14,3,FOEVINDX(FOEPTR)                              0449
         CR    INPUTVBN,@14                                        0449
         BNL   @RF00449                                            0449
@RT00449 DS    0H                                                  0450
*     DO;                                                          0450
*       SAV15=4;                    /* NO VALID FOE RESULTS IN R.C.4 */
         MVC   SAV15(4,PVTPTR),@CF00053                            0451
*       GOTO ENDDEL;                /* GOING BACK TO CALLER          */
         B     ENDDEL                                              0452
*     END;                                                         0453
*   IF FOEVINDX^=INPUTVBN THEN      /* IF VBNS DO NOT MATCH          */
@RF00449 SLR   @14,@14                                             0454
         ICM   @14,3,FOEVINDX(FOEPTR)                              0454
         CR    INPUTVBN,@14                                        0454
         BE    @RF00454                                            0454
*     DO;                                                          0455
*       OLDFOE=CURFOE;              /* MOVE DOWN THE CHAIN           */
         LR    OLDFOE,CURFOE                                       0456
*       CURFOE=FOEFLINK;            /* GET NEXT FOE POINTER          */
         SLR   @14,@14                                             0457
         ICM   @14,7,FOEFLINK(FOEPTR)                              0457
         LR    CURFOE,@14                                          0457
*       GOTO LASTCK;                /* REPEAT TESTS FOR THIS FOE     */
         B     LASTCK                                              0458
*     END;                                                         0459
