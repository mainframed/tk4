*                                      ALREADY 0                     */
         MVC   SAV15(4,PVTPTR),@CF00053                            0461
*   ELSE                            /* IF THE FIX COUNT IS NOT ZERO  */
*     DO;                                                          0462
         B     @RC00460                                            0462
@RF00460 DS    0H                                                  0463
*       FOEFXCT=FOEFXCT-1;          /* DECREMENT FIX COUNT           */
         LH    @14,FOEFXCT(,FOEPTR)                                0463
         BCTR  @14,0                                               0463
         STH   @14,FOEFXCT(,FOEPTR)                                0463
