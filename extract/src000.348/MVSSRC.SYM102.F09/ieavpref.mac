*               END;                /* END NON-IRREG PROCESS @ZA16816*/
*           END;                    /* END ACCEPTABLE FRAME  @ZA16816*/
*         ELSE                      /* IF NOT ACCEPTABLE THEN      0303
*                                                            @ZA16816*/
*           RBN=PFTFQPTR;           /* GET NEXT PFTE RBN     @ZA16816*/
         B     @RC00218                                            0303
@RF00218 LH    @14,PFTFQPTR(,FRAMEPTR)                             0303
         N     @14,@CF01844                                        0303
         LR    RBN,@14                                             0303
*       END;                        /* END STEALABLE FRAME   @ZA16816*/
*     ELSE                          /* IF NOT STEALABLE THEN @ZA16816*/
*       RBN=PFTFQPTR;               /* GET THE NEXT PFTE RBN @ZA16816*/
         B     @RC00201                                            0305
@RF00201 LH    @14,PFTFQPTR(,FRAMEPTR)                             0305
         N     @14,@CF01844                                        0305
         LR    RBN,@14                                             0305
