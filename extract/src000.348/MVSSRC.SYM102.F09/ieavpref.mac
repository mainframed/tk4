*                     /***********************************************/
*                                                                  0282
*                     IF ASCBPTR^=0 THEN/* IF ASCB NOT ZERO, @ZA16816*/
STOLEN   LTR   ASCBPTR,ASCBPTR                                     0282
         BZ    @RF00282                                            0282
*                       DO;         /* TRAS BACK AND DEQ     @ZA16816*/
*                         REG1=PSAAOLD;/* GET CUR ASCB       @ZA16816*/
         L     REG1,PSAAOLD                                        0284
*                         GENERATE CODE REFS(ASCBSTOR);            0285
                    LCTL 1,1,ASCBSTOR(REG1) -----TRAS BACK-----
*                         LCCAASID=0;/* CLEAR RTM TRAS IND.  @ZA16816*/
         SLR   @14,@14                                             0286
         L     @11,LCCAPTR                                         0286
         ST    @14,LCCAASID(,@11)                                  0286
*                         PVTTASID=0;/* CLEAR RCV TRAS IND.  @ZA16816*/
         STH   @14,PVTTASID(,PVTPTR)                               0287
*                         REG0=FRAMEPTR-PVTPFTP;/* GET THE RBN     0288
*                                                            @ZA16816*/
         LR    REG0,FRAMEPTR                                       0288
         SL    REG0,PVTPFTP(,PVTPTR)                               0288
*                         REG0=REG0×(65536*PFTNQN);/* INDICATE DEQ 0289
*                                                            @ZA16816*/
         O     REG0,@CF01839                                       0289
