         TITLE 'IEAVPREF - PREFERRED AREA ALLOCATION STEAL             *
                        '
*                         /*ENTRY CODE*/
IEAVPREF CSECT
         MODID BR=NO
         ENTRY IEAVPRE2
IEAVPRE2 DS    0H
         STM   REG0,REG14,MYSAVE(PVTPTR) SAVE THE INPUT REGS (CHANGE)
         BALR  BASEREG,0
         USING *,BASEREG
*   RBN=REG1;                       /* GET RBN (CONVERT) OR ZERO   0067
*                                                            @ZA16816*/
         LR    RBN,REG1                                            0067
*   RESPECIFY                                                      0068
*    (REG1) UNRSTD;                 /*                       @ZA16816*/
*   IF RBN=0 THEN                   /* IF STEAL OPTION THEN  @ZA16816*/
         LTR   RBN,RBN                                             0069
         BNZ   @RF00069                                            0069
*     DO;                           /* TRY TO STEAL A FRAME  @ZA16816*/
*       PASS=REG0;                  /* GET THE PASS NUMBER   @ZA16816*/
         LR    PASS,REG0                                           0071
*       RESPECIFY                                                  0072
*        (REG0) UNRSTD;             /*                       @ZA16816*/
*       RSMHDPTR=PSAAOLD->ASCBRSM;  /* SET TO CURRENT RSMHD  @ZA16816*/
         L     @09,PSAAOLD                                         0073
         L     RSMHDPTR,ASCBRSM(,@09)                              0073
*       RESPECIFY                                                  0074
*        (FRAMEPTR,                                                0074
*         ASCBPTR) RSTD;            /*                       @ZA16816*/
*       SAVER15='00000004'X;        /* SET RC=4 INITIALLY    @ZA16816*/
         MVC   SAVER15(4,PVTPTR),@CB01815                          0075
*START:                             /*                       @ZA16816*/
*                                                                  0076
*       /*************************************************************/
*       /*                                                           */
*       /* START SEARCH WITH THE CURRENT LOCAL FRAME QUEUE   @ZA16816*/
*       /* IF NOT IN TRAS MODE.                              @ZA29043*/
*       /*                                                           */
*       /*************************************************************/
*                                                                  0076
*       IF LCCAASID=0 THEN          /* IN TRAS MODE ?        @ZA29043*/
START    L     @06,LCCAPTR                                         0076
         L     @06,LCCAASID(,@06)                                  0076
         LTR   @06,@06                                             0076
         BNZ   @RF00076                                            0076
*         DO;                       /* NO, SEARCH CURRENT LFQ      0077
*                                                            @ZA29043*/
*           ASCBPTR=0;              /* NO ASCB FOR CURRENT LFQ     0078
*                                                            @ZA16816*/
         SLR   ASCBPTR,ASCBPTR                                     0078
*           RBN=RSMLFQF;            /* GET 1ST PFTE RBN ON CUR LFQ 0079
*                                                            @ZA16816*/
         LH    RBN,RSMLFQF(,RSMHDPTR)                              0079
         N     RBN,@CF01844                                        0079
*           CALL QSCAN;             /* SEARCH THE CURRENT LFQ      0080
*                                                            @ZA16816*/
         BAL   @14,QSCAN                                           0080
*         END;                      /*                       @ZA29043*/
*       ELSE                        /* WE ARE IN TRAS MODE   @ZA29043*/
*         REG15=4;                  /* ENSURE WE SEARCH CFQ  @ZA29043*/
         B     @RC00076                                            0082
@RF00076 LA    REG15,4                                             0082
*       IF REG15^=0 THEN            /* IF A FRAME WAS NOT FOUND,   0083
*                                                            @ZA16816*/
@RC00076 LTR   REG15,REG15                                         0083
         BZ    @RF00083                                            0083
*         DO;                       /* CONTINUE THE SEARCH   @ZA16816*/
*/*  *****************************************************************/
*/*                                                                  */
*/*          SEARCH THE COMMON FRAME QUEUE SECOND            @ZA16816*/
*/*                                                                  */
*/*                  NOTE WELL...........                    @ZA29043*/
*/*                                                                  */
*/*     THE VIRTUAL ADDRESS OF THE SEGMENT TABLE MUST BE     @ZA29043*/
*/*     IDENTICAL FOR ALL ADDRESS SPACES.  IF THEY ARE       @ZA29043*/
*/*     NOT, UNPREDICTABLE RESULTS MAY OCCUR WHEN THE        @ZA29043*/
*/*     SYSTEM IS IN TRAS MODE.                              @ZA29043*/
*/*  *****************************************************************/
*                                                                  0085
*           RBN=PVTCFQF;            /* GET 1ST PFTE RBN ON CFQ     0085
*                                                            @ZA16816*/
         LH    RBN,PVTCFQF(,PVTPTR)                                0085
         N     RBN,@CF01844                                        0085
*           CALL QSCAN;             /* SEARCH THE CFQ        @ZA16816*/
         BAL   @14,QSCAN                                           0086
*           IF REG15^=0             /* IF A FRAME NOT FOUND, @ZA16816*/
*               &LCCAASID=0 THEN    /* AND NOT IN TRAS MODE, @ZA29043*/
         SLR   @14,@14                                             0087
         CR    REG15,@14                                           0087
         BE    @RF00087                                            0087
         L     @06,LCCAPTR                                         0087
         CL    @14,LCCAASID(,@06)                                  0087
         BNE   @RF00087                                            0087
*             DO;                   /* CONTINUE THE SEARCH   @ZA16816*/
*               RESPECIFY                                          0089
*                (REG0) RSTD;       /*                       @ZA16816*/
*                                                                  0089
*               /*****************************************************/
*               /*                                                   */
*               /* SEARCH ALL FOREIGN LOCAL FRAME QUEUES 3RD @ZA16816*/
*               /* IF NOT IN TRAS MODE.                      @ZA29043*/
*               /*                                                   */
*               /*****************************************************/
*                                                                  0090
*               REG0=BASEREG;       /* SAVE BASE REGISTER    @ZA16816*/
         LR    REG0,BASEREG                                        0090
*               RFY                                                0091
*                (REG13) RSTD;      /*                       @ZA28519*/
*               GENERATE REFS(PSALITA,PSA,PSACPULA,PSAHLHI);       0092
         SETLOCK OBTAIN,TYPE=DISP,MODE=UNCOND,RELATED=(PREF)
*               BASEREG=REG0;       /* RESTORE BASE REGISTER @ZA16816*/
         LR    BASEREG,REG0                                        0093
*               IF REG13=0 THEN     /* DISP LOCK OBTAINED ?  @ZA28519*/
         LTR   REG13,REG13                                         0094
         BNZ   @RF00094                                            0094
*                 RCADISP='1'B;     /* YES, NOTE NEED FOR RELEASE  0095
*                                                            @ZA28519*/
         L     @01,RCAPTR(,PVTPTR)                                 0095
         OI    RCADISP(@01),B'00000010'                            0095
*               RFY                                                0096
*                (REG13) UNRSTD;    /*                       @ZA28519*/
@RF00094 DS    0H                                                  0097
*               ASCBPTR=CVTASCBH;   /* GET 1ST ASCB ON READY QUEUE 0097
*                                                            @ZA16816*/
         L     @06,CVTPTR                                          0097
         L     ASCBPTR,CVTASCBH(,@06)                              0097
*               RESPECIFY                                          0098
*                (REG15) RSTD;      /*                       @ZA16816*/
*               DO UNTIL REG15=0;   /* SEARCH UNTIL HIT OR END OF  0099
*                                      ASCBS                 @ZA16816*/
@DL00099 DS    0H                                                  0100
*                 IF ASCBPTR=0 THEN /* IF NO MORE ASCBS THEN @ZA16816*/
         LTR   ASCBPTR,ASCBPTR                                     0100
         BNZ   @RF00100                                            0100
*                   REG15=0;        /* CLEAR R15 TO EXIT LOOP      0101
*                                                            @ZA16816*/
         SLR   REG15,REG15                                         0101
*                 ELSE              /* IF ANOTHER ASCB THEN  @ZA16816*/
*                   DO;             /* SCAN ITS LOCAL FRAME QUEUE  0102
*                                                            @ZA16816*/
         B     @RC00100                                            0102
@RF00100 DS    0H                                                  0103
*                     RBN=ASCBPTR->ASCBRSM->RSMLFQF;/*       @ZA16816*/
         L     @01,ASCBRSM(,ASCBPTR)                               0103
         LH    RBN,RSMLFQF(,@01)                                   0103
         N     RBN,@CF01844                                        0103
*                     CALL QSCAN;   /* SEARCH THE LFQ        @ZA16816*/
         BAL   @14,QSCAN                                           0104
*                     ASCBPTR=ASCBFWDP;/* GET NEXT ASCB ON RETRY QUE
*                                                            @ZA16816*/
         L     ASCBPTR,ASCBFWDP(,ASCBPTR)                          0105
*                   END;            /*                       @ZA16816*/
*               END;                /*                       @ZA16816*/
@RC00100 DS    0H                                                  0107
@DE00099 LTR   REG15,REG15                                         0107
         BNZ   @DL00099                                            0107
*               RESPECIFY                                          0108
*                (REG15) UNRSTD;    /*                       @ZA16816*/
*               IF RCADISP='1'B THEN/* RELEASE DISP LOCK?    @ZA28519*/
         L     @01,RCAPTR(,PVTPTR)                                 0109
         TM    RCADISP(@01),B'00000010'                            0109
         BNO   @RF00109                                            0109
*                 DO;               /* YES                   @ZA28519*/
*                   REG0=BASEREG;   /* SAVE BASE REGISTER    @ZA16816*/
         LR    REG0,BASEREG                                        0111
*                   GENERATE REFS(PSALITA,PSA,PSACPULA,PSAHLHI);   0112
         SETLOCK RELEASE,TYPE=DISP,RELATED=(PREF)
*                   BASEREG=REG0;   /* RESTORE BASE REGISTER @ZA16816*/
         LR    BASEREG,REG0                                        0113
*                   RCADISP='0'B;   /* NOTE DISP LOCK RELEASED     0114
*                                                            @ZA28519*/
         L     @01,RCAPTR(,PVTPTR)                                 0114
         NI    RCADISP(@01),B'11111101'                            0114
*                 END;              /* END RELEASE OF DISP LOCK    0115
*                                                            @ZA28519*/
*               RESPECIFY                                          0116
*                (REG0) UNRSTD;     /*                       @ZA16816*/
@RF00109 DS    0H                                                  0117
*               IF RC^=0&           /* IF STILL NO PFTE FOUND      0117
*                                                            @ZA16816*/
*                   PVTRSBQF^=0 THEN/* AND RSB Q NOT EMPTY,  @ZA16816*/
         CLI   RC(PVTPTR),0                                        0117
         BE    @RF00117                                            0117
         LH    @06,PVTRSBQF(,PVTPTR)                               0117
         N     @06,@CF01844                                        0117
         LTR   @06,@06                                             0117
         BZ    @RF00117                                            0117
*                 DO;               /* THEN TRY THE RSB QUEUE      0118
*                                                            @ZA16816*/
*                                                                  0118
*                   /*************************************************/
*                   /*                                               */
*                   /* SEARCH REAL STORAGE BUFFER QUEUE FOURTH       */
*                   /*                                       @ZA16816*/
*                   /*                                               */
*                   /*************************************************/
*                                                                  0119
*                   RESPECIFY                                      0119
*                    (REG1) RSTD;   /*                       @ZA16816*/
*                   REG1=PASS;      /* GET PASS NO FOR PRSB  @ZA16816*/
         LR    REG1,PASS                                           0120
*                   CALL IEAVPRSS;  /* TRY TO STEAL A FRAME  @ZA16816*/
         L     @15,@CV01797                                        0121
         BALR  @14,@15                                             0121
*                   RESPECIFY                                      0122
*                    (REG1) UNRSTD; /*                       @ZA16816*/
*                   RESPECIFY                                      0123
*                    (REG15) RSTD;  /*                       @ZA16816*/
*                   IF REG15^=0 THEN/* IF SUCCESSFUL         @ZA16816*/
         LTR   REG15,REG15                                         0124
         BZ    @RF00124                                            0124
*                     DO;           /* THEN                  @ZA16816*/
*                       SAVER0=REG15;/* SAVE ADDRESS OF PFTE @ZA16816*/
         ST    REG15,SAVER0(,PVTPTR)                               0126
*                       RC=0;       /* SET FINAL RC TO ZERO  @ZA16816*/
         MVI   RC(PVTPTR),X'00'                                    0127
*                       REG15->PFTSTEAL='0'B;/* INITIALIZE FLAG    0128
*                                                            @ZA16816*/
         NI    PFTSTEAL(REG15),B'11110111'                         0128
*                       REG15->PFTUIC=0;/* INITIALIZE UIC    @ZA16816*/
         MVI   PFTUIC(REG15),X'00'                                 0129
*                     END;          /*                       @ZA16816*/
*                   RESPECIFY                                      0131
*                    (REG15) UNRSTD;/*                       @ZA16816*/
@RF00124 DS    0H                                                  0132
*                 END;              /* END RSB QUEUE PROCESS @ZA16816*/
*             END;                  /* END FOREIGN LFQ SEARCH      0133
*                                                            @ZA16816*/
@RF00117 DS    0H                                                  0134
*         END;                      /* END CFQ SEARCH        @ZA16816*/
@RF00087 DS    0H                                                  0135
*       RESPECIFY                                                  0135
*        (FRAMEPTR,                                                0135
*         ASCBPTR) UNRSTD;          /*                       @ZA16816*/
*                                                                  0135
@RF00083 DS    0H                                                  0136
*       /*************************************************************/
*       /*                                                           */
*       /* AT THIS POINT, PREF HAS (1) FOUND THE DESIRED FRAME-      */
*       /* (SAVER15=0 AND SAVER0=PFTE ADDR) OR (2) IF PASS 1, FOUND  */
*       /* CHANGED PREFERRED FRAME - (SAVER15=8 AND SAVER0=PFTE ADDR)*/
*       /* OR (3) NOT FOUND A SUITABLE FRAME - (SAVER15=4)   @ZA16816*/
*       /*                                                           */
*       /*************************************************************/
*                                                                  0136
*       IF PASS=3&RC=4 THEN         /* NO LUCK ON PASS 3 THEN      0136
*                                                            @ZA16816*/
         C     PASS,@CF00187                                       0136
         BNE   @RF00136                                            0136
         CLI   RC(PVTPTR),4                                        0136
         BNE   @RF00136                                            0136
*         DO;                       /* RESTART SEARCH (PASS=4)     0137
*                                                            @ZA16816*/
*           PASS=4;                 /* LOOKING FOR A V=R     @ZA16816*/
         LA    PASS,4                                              0138
*           GOTO START;             /* INTERCEPTED FRAME     @ZA16816*/
         B     START                                               0139
*         END;                      /*                       @ZA16816*/
*       IF PASS=4&RC=0 THEN         /* IF SUCCESS ON PASS 4, @ZA16816*/
@RF00136 C     PASS,@CF00085                                       0141
         BNE   @RF00141                                            0141
         CLI   RC(PVTPTR),0                                        0141
         BNE   @RF00141                                            0141
*         DO;                       /* CANCEL THE V=R JOB    @ZA16816*/
*                                                                  0142
*           /*********************************************************/
*           /*                                                       */
*           /* SCAN THE PCBR V=R WAIT QUEUE IN SEARCH OF THE PCBR    */
*           /* WAITING FOR THE STOLEN V=R INTERCEPTED FRAME. WHEN    */
*           /* FOUND, CALL IEAVEQRI TO CANCEL THE ASSOCIATED V=R JOB.*/
*           /*                                               @ZA16816*/
*           /*                                                       */
*           /*********************************************************/
*                                                                  0143
*           RESPECIFY                                              0143
*            (PCBRPTR) RSTD;        /*                       @ZA16816*/
*           PCBRPTR=PVTVROOT;       /* GET 1ST V=R PCBR      @ZA16816*/
         L     PCBRPTR,PVTVROOT(,PVTPTR)                           0144
*           RBN=SAVER0-PVTPFTP;     /* GET RBN SEARCH ARGUMENT     0145
*                                                            @ZA16816*/
         L     RBN,SAVER0(,PVTPTR)                                 0145
         SL    RBN,PVTPFTP(,PVTPTR)                                0145
*PCBRLOOP:                          /*                       @ZA16816*/
*           IF PCBRPTR=0 THEN       /* IF ZERO, AN ERROR -   @ZA16816*/
PCBRLOOP LTR   PCBRPTR,PCBRPTR                                     0146
         BNZ   @RF00146                                            0146
*             DO;                   /* RECORD ERROR          @ZA16816*/
*               RCARCRD='08'X;      /* SET REASON CODE       @ZA16816*/
         L     @06,RCAPTR(,PVTPTR)                                 0148
         MVI   RCARCRD(@06),X'08'                                  0148
*               GENERATE;                                          0149
              ABEND X'C0D',,,SYSTEM      ABEND CODE
*               SAVER0->PFTVRINT='0'B;/* CLEAR INTERCEPT FLAG      0150
*                                                            @ZA16816*/
         L     @06,SAVER0(,PVTPTR)                                 0150
         NI    PFTVRINT(@06),B'10111111'                           0150
*             END;                  /*                       @ZA16816*/
*           ELSE                    /* HAVE A PCBR TO LOOK AT      0152
*                                                            @ZA16816*/
*                                                                  0152
*             /*******************************************************/
*             /*                                                     */
*             /* DETERMINE IF THE STOLEN FRAME IS WITHIN THE REGION  */
*             /* DEFINED BY THIS ROOT PCB. PCBRWRK1 AND PCBRWRK2     */
*             /* CONTAIN THE START AND END RBNS OF THE V=R REGION    */
*             /* REPRESENTED BY THIS ROOT PCB.               @ZA16816*/
*             /*                                                     */
*             /*******************************************************/
*                                                                  0152
*             IF RBN^<PCBRWRK1&     /* IF THE CORRECT        @ZA16816*/
*                 RBN^>PCBRWRK2 THEN/* ROOT PCBR IS FOUND    @ZA16816*/
         B     @RC00146                                            0152
@RF00146 C     RBN,PCBRWRK1(,PCBRPTR)                              0152
         BL    @RF00152                                            0152
         C     RBN,PCBRWRK2(,PCBRPTR)                              0152
         BH    @RF00152                                            0152
*               DO;                 /* CANCEL THE V=R JOB    @ZA16816*/
*                 PCBRFAIL='1'B;    /* SET CANCEL FLAG       @ZA16816*/
         OI    PCBRFAIL(PCBRPTR),B'00001000'                       0154
*                 IF PCBRINT='0'B THEN/* IF NOT PREVIOUSLY   @ZA16816*/
         TM    PCBRINT(PCBRPTR),B'10000000'                        0155
         BNZ   @RF00155                                            0155
*                   DO;             /* TERMINATED, CALL EQRI @ZA16816*/
*                     RESPECIFY                                    0157
*                      (REG1,                                      0157
*                       REG6) RSTD; /*                       @ZA16816*/
*                     REG1=RBN;     /* R1=RBN FOR IEAVEQRI   @ZA16816*/
         LR    REG1,RBN                                            0158
*                     REG6=SAVER0;  /* R6=PFTE ADDR FOR EQRI @ZA16816*/
         L     REG6,SAVER0(,PVTPTR)                                0159
*                     CALL IEAVEQRI;/* CANCEL THE V=R JOB    @ZA16816*/
         L     @15,PVTPVRIN(,PVTPTR)                               0160
         BALR  @14,@15                                             0160
*                     RESPECIFY                                    0161
*                      (REG1,                                      0161
*                       REG6) UNRSTD;/*                      @ZA16816*/
*                   END;            /*                       @ZA16816*/
*               END;                /*                       @ZA16816*/
*             ELSE                  /* HERE IF WRONG PCBR    @ZA16816*/
*               DO;                 /*                       @ZA16816*/
         B     @RC00152                                            0164
@RF00152 DS    0H                                                  0165
*                 PCBRPTR=PCBRWRK3; /* GET THE NEXT PCBR     @ZA16816*/
         L     PCBRPTR,PCBRWRK3(,PCBRPTR)                          0165
*                 GOTO PCBRLOOP;    /* GO CHECK ITS RANGE    @ZA16816*/
         B     PCBRLOOP                                            0166
*               END;                /*                       @ZA16816*/
*         END;                      /*                       @ZA16816*/
*     END;                          /* END OF STEAL OPTION   @ZA16816*/
*   ELSE                            /* WANT TO CONVERT A UNIT TO   0170
*                                      PREFERRED                     */
*     DO;                           /* DO THE CONVERSION             */
         B     @RC00069                                            0170
@RF00069 DS    0H                                                  0171
*       RESPECIFY                                                  0171
*        (MASK) RSTD;                                              0171
*       MASK=((PVTRSUS-1)&&ALLFOXES)*16;/* CONVERT THE # OF FRAMES IN
*                                      A STORAGE UNIT TO A MASK TO BE
*                                      USED TO ROUND A RBN TO A BOX
*                                      BOUNDARY                      */
         LH    @06,PVTRSUS(,PVTPTR)                                0172
         N     @06,@CF01844                                        0172
         BCTR  @06,0                                               0172
         LR    MASK,@06                                            0172
         X     MASK,@CF01805                                       0172
         SLL   MASK,4                                              0172
*       RBN=RBN&MASK;               /* ROUND THE INPUT RBN TO A LOWER
*                                      BOX BOUNDARY                  */
         NR    RBN,MASK                                            0173
*       RFY                                                        0174
*        (MASK) UNRSTD;                                            0174
*       RFY                                                        0175
*        (ENDPFTE,                                                 0175
*         PFTEPTR) RSTD;                                           0175
*       ENDPFTE=PVTPFTP+(RBN+((PVTRSUS-1)*LENGTH(PFTE)));/* GET END
*                                      OF LOOP. IS THE HIGHEST PFTE
*                                      IN A STORAGE UNIT             */
         L     @05,PVTPFTP(,PVTPTR)                                0176
         SLA   @06,4                                               0176
         ALR   @06,RBN                                             0176
         ALR   @06,@05                                             0176
         LR    ENDPFTE,@06                                         0176
*       DO PFTEPTR=PVTPFTP+RBN TO ENDPFTE BY LENGTH(PFTE);/* PROCESS
*                                      EACH PFTE IN THE STORAGE BOX  */
         ALR   @05,RBN                                             0177
         LR    PFTEPTR,@05                                         0177
         B     @DE00177                                            0177
@DL00177 DS    0H                                                  0178
*         IF PFTVR='0'B THEN        /* IF NOT A V=R FRAME,   @ZA16816*/
         TM    PFTVR(PFTEPTR),B'00100000'                          0178
         BNZ   @RF00178                                            0178
*           PFTPREF='1'B;           /* INDICATE PFTE IS PREFERRED    */
         OI    PFTPREF(PFTEPTR),B'00000100'                        0179
*       END;                                                       0180
@RF00178 AL    PFTEPTR,@CF00060                                    0180
@DE00177 CR    PFTEPTR,ENDPFTE                                     0180
         BNH   @DL00177                                            0180
*       RFY                                                        0181
*        (PFTEPTR,                                                 0181
*         ENDPFTE) UNRSTD;                                         0181
*       PVTPMSG='1'B;               /* INDICATE MSG ISSUED   @ZA16816*/
         OI    PVTPMSG(PVTPTR),B'10000000'                         0182
*       RFY                                                        0183
*        (MSGPTR,                                                  0183
*         MSGLEN) RSTD;                                            0183
*       MSGPTR=ADDR(PREFMSG);       /* GET ADDR OF THE MSG           */
         LA    MSGPTR,PREFMSG                                      0184
*       MSGLEN=LENGTH(PREFMSG);     /* GET MSG LENGTH                */
         LA    MSGLEN,4                                            0185
*       RFY                                                        0186
*        (REG13) RSTD;                                             0186
*       REG13=ADDR(PVTWSA14);       /* PASS AN UNUSED SAVE AREA      */
         LA    REG13,PVTWSA14(,PVTPTR)                             0187
*       GEN CODE;                                                  0188
*                                                                  0188
*                        /*ISSUE CONVERSION MESSAGE*/
         RECORD TYPE=WTO,RCVRY=SETFRR,DATAADR=(MSGPTR),BUFFER=NO,LENGTHH
               =(MSGLEN)
*       /*************************************************************/
*       /*                                                           */
*       /* DO NOT BOTHER TO CHECK RETURN CODE. IF BAD R.C., THERE IS */
*       /* NOTHING THAT CAN BE DONE BECAUSE LOGREC IS NOT AVAILABLE  */
*       /* TO RECORD AN ERROR.                                       */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0189
*       RFY                                                        0189
*        (MSGPTR,                                                  0189
*         MSGLEN,                                                  0189
*         REG13) UNRSTD;                                           0189
*     END;                          /* END OF CONVERT OPTION @ZA16816*/
*   GENERATE CODE;                                                 0191
@RC00069 DS    0H                                                  0191
         LM REG0,REG15,MYSAVE(PVTPTR)
         BR REG14         RETURN TO CALLER
*                                                                  0192
*/* ******************************************************************/
*/*                                                                  */
*/*            QSCAN SUBROUTINE  -  USED BY THE STEAL OPTION         */
*/*                                                                  */
*/*               FUNCTION - QSCAN WILL SCAN THE PFTE'S OF A GIVEN   */
*/*                          PFTE QUEUE IN SEARCH OF A STEALABLE     */
*/*                          FRAME THAT MEETS THE INPUT PASS CRITERIA*/
*/*                                                                  */
*/*               INPUT- RBN      = RBN OF 1ST PFTE ON A QUEUE       */
*/*                      PASS     = PASS NUMBER                      */
*/*                      PVTPTR   = ADDRESS OF PVT                   */
*/*                      RSMHDPTR = CURRENT ADDRESS SPACE RSMHD      */
*/*                                 ADDRESS                          */
*/*                      ASCBPTR  = ASCB PTR IF FOREIGN LFQ-ELSE 0   */
*/*                      REG14    = RETURN ADDRESS                   */
*/*                                                                  */
*/*               OUTPUT- (1) IF DESIRED PFTE FOUND                  */
*/*                               A. REG15=0                         */
*/*                               B. SAVER0=ADDRESS OF PFTE          */
*/*                               C. SAVER15=0                       */
*/*                       (2) IF PASS=1 AND CHANGED PREF PFTE FOUND  */
*/*                               A. REG15=4                         */
*/*                               B. SAVER0=ADDRESS OF PFTE          */
*/*                               C. SAVER1=0 (IF CURRENT LFQ OR     */
*/*                                            CFQ)  OR              */
*/*                                  SAVER1=ASCB ADDR                */
*/*                               D. SAVER15=8                       */
*/*                       (3) IF NO ACCEPTABLE PFTE FOUND            */
*/*                               A. REG15=4                         */
*/*                               B. SAVER15=AS ON INPUT             */
*/*                                                          @ZA16816*/
*/* ******************************************************************/
*                                                                  0192
*QSCAN:                                                            0192
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0192
QSCAN    DS    0H                                                  0193
*   RESPECIFY                                                      0193
*    (REG6,                                                        0193
*     FRAMEPTR,                                                    0193
*     PASS,                                                        0193
*     ASCBPTR,                                                     0193
*     REG14) RSTD;                  /*                       @ZA16816*/
*   REG6=REG14;                     /* SAVE RETURN ADDRESS   @ZA16816*/
         LR    REG6,REG14                                          0194
*   RESPECIFY                                                      0195
*    (REG14) UNRSTD;                /*                       @ZA16816*/
*   RESPECIFY                                                      0196
*    (REG15) RSTD;                  /*                       @ZA16816*/
*   REG15=4;                        /* INITIALIZE TO RC=4    @ZA16816*/
         LA    REG15,4                                             0197
*   DO WHILE RBN^=0;                /* EXAMINE EACH PFTE ON Q      0198
*                                                            @ZA16816*/
         B     @DE00198                                            0198
@DL00198 DS    0H                                                  0199
*     FRAMEPTR=PVTPFTP+RBN;         /* GET PFTE ADDRESS      @ZA16816*/
*                                                                  0199
         L     @14,PVTPFTP(,PVTPTR)                                0199
         LR    @11,RBN                                             0199
         ALR   @11,@14                                             0199
         LR    FRAMEPTR,@11                                        0199
*     /***************************************************************/
*     /*                                                             */
*     /* A PFTE IS STEALABLE IF IT IS (1) NOT FIXED, (2) NOT         */
*     /* ASSOCIATED WITH ANY PCB, (3) NOT ALLOCATED TO A V=R JOB, (4)*/
*     /* NOT DEFERRED RELEASED, (5) NOT A BAD PAGE, AND (6) NOT      */
*     /* OFFLINE INTERCEPTED                                 @ZA16816*/
*     /*                                                             */
*     /***************************************************************/
*                                                                  0200
*     RESPECIFY                                                    0200
*       PFTE BASED(FRAMEPTR);       /*                       @ZA16816*/
*     IF PFTFXCT=0&                 /* FIRST CHECK TO SEE IF @ZA16816*/
*         PFTPCBSI='0'B&            /* THE PFTE IS STEALABLE @ZA16816*/
*         PFTVRALC='0'B&            /*                       @ZA16816*/
*         PFTOFINT='0'B&            /* IF STEALABLE, THEN    @ZA16816*/
*         PFTBADPG='0'B&            /*                       @ZA16816*/
*         PFTDFRLS='0'B THEN        /*                       @ZA16816*/
         LH    @11,PFTFXCT(,FRAMEPTR)                              0201
         LTR   @11,@11                                             0201
         BNZ   @RF00201                                            0201
         TM    PFTPCBSI(FRAMEPTR),B'00001111'                      0201
         BNZ   @RF00201                                            0201
         TM    PFTDFRLS(FRAMEPTR),B'10000000'                      0201
         BNZ   @RF00201                                            0201
*       DO;                         /* DO FURTHER CHECKS     @ZA16816*/
*         IF PFTIRRG='0'B THEN      /* IF NOT AN IRRG FRAME, @ZA16816*/
         TM    PFTIRRG(FRAMEPTR),B'00010000'                       0203
         BNZ   @RF00203                                            0203
*           DO;                     /* ENSURE PAGE IS UNCHNGD      0204
*                                                            @ZA16816*/
*             RESPECIFY                                            0205
*              (REG0,                                              0205
*               REG1) RSTD;         /*                       @ZA16816*/
*             REG1=256*(FRAMEPTR-PVTPFTP);/* R1 =RSA 1ST 2K  @ZA16816*/
         LCR   @14,@14                                             0206
         ALR   @14,FRAMEPTR                                        0206
         SLA   @14,8                                               0206
         LR    REG1,@14                                            0206
*             GENERATE CODE REFS(CHBITMSK);                        0207
           ISK REG0,REG1          GET STG KEY FOR 1ST 2K
           N   REG0,CHBITMSK      ISOLATE THE CHANGE BIT - FIRST 2K
           BNZ PREFTST1           TO PREFTST1 IF BLOCK CHANGED
           LA  REG1,2048(REG1)    R1 =RSA OF 2ND 2K
           ISK REG0,REG1          GET STG KEY FOR 2ND 2K - SECOND 2K
           N   REG0,CHBITMSK      ISOLATE THE CHANGE BIT
           BZ  PREFTST2           TO PREFTST2 IF NOT CHANGED
*             RESPECIFY                                            0208
*              (REG0,                                              0208
*               REG1) UNRSTD;                                      0208
*PREFTST1:                          /* HERE IF CHANGED PAGE  @ZA16816*/
*             IF PFTPREF='1'B&      /* SAVE THE PFTE IF IT IS      0209
*                                                            @ZA16816*/
*                 RC=4&             /* A PREF, PASS=1,       @ZA16816*/
*                 PASS=1&           /* NONE SAVED PREVIOUSLY,      0209
*                                                            @ZA16816*/
*                 PVTRSMGM='0'B THEN/* AND NOT RSM GETMAIN   @ZA16816*/
PREFTST1 TM    PFTPREF(FRAMEPTR),B'00000100'                       0209
         BNO   @RF00209                                            0209
         CLI   RC(PVTPTR),4                                        0209
         BNE   @RF00209                                            0209
         C     PASS,@CF00038                                       0209
         BNE   @RF00209                                            0209
         TM    PVTRSMGM(PVTPTR),B'10000000'                        0209
         BNZ   @RF00209                                            0209
*               DO;                 /*                       @ZA16816*/
*                 SAVER0=FRAMEPTR;  /* SAVE PFTE PTR         @ZA16816*/
         ST    FRAMEPTR,SAVER0(,PVTPTR)                            0211
*                 SAVER1=ASCBPTR;   /* SAVE ASCB ADDRESS     @ZA16816*/
         ST    ASCBPTR,SAVER1(,PVTPTR)                             0212
*                 RC=8;             /* SET FOR RC=8          @ZA16816*/
         MVI   RC(PVTPTR),X'08'                                    0213
*               END;                /*                       @ZA16816*/
*             RBN=PFTFQPTR;         /* GET NEXT PFTE RBN AND @ZA16816*/
@RF00209 LH    @14,PFTFQPTR(,FRAMEPTR)                             0215
         N     @14,@CF01844                                        0215
         LR    RBN,@14                                             0215
*             GOTO LOOPEND;         /* CONTINUE THE SEARCH   @ZA16816*/
         B     LOOPEND                                             0216
*           END;                    /*                       @ZA16816*/
*PREFTST2:                          /* HERE IF PAGE UNCHANGED      0218
*                                                            @ZA16816*/
*                                                                  0218
*         /***********************************************************/
*         /*                                                         */
*         /* NOW HAVE A STEALABLE FRAME BACKING AN UNCHANGED PAGE. IT*/
*         /* IS ACCEPTABLE IF (1) IT IS A PREFERRED FRAME, OR (2)    */
*         /* PASS=2,3 OR 4 AND THE FRAME IS NON-V=R, OR (3) IT IS    */
*         /* PASS 3 OR 4 AND THE FRAME IS NOT V=R INTERCEPTED, OR (4)*/
*         /* IT IS PASS 4.                                   @ZA16816*/
*         /*                                                         */
*         /***********************************************************/
*                                                                  0218
*         IF PFTPREF='1'B×          /*                       @ZA16816*/
*             (PASS>1&(PFTVR='0'B×  /*                       @ZA16816*/
*             (PASS>2&(PFTVRINT='0'B×/*                      @ZA16816*/
*             PASS=4)))) THEN       /* IF ACCEPTABLE FRAME   @ZA16816*/
@RF00203 DS    0H                                                  0218
PREFTST2 TM    PFTPREF(FRAMEPTR),B'00000100'                       0218
         BO    @RT00218                                            0218
         C     PASS,@CF00038                                       0218
         BNH   @RF00218                                            0218
         TM    PFTVR(FRAMEPTR),B'00100000'                         0218
         BZ    @RT00218                                            0218
         C     PASS,@CF00217                                       0218
         BNH   @RF00218                                            0218
         TM    PFTVRINT(FRAMEPTR),B'01000000'                      0218
         BZ    @RT00218                                            0218
         C     PASS,@CF00085                                       0218
         BNE   @RF00218                                            0218
@RT00218 DS    0H                                                  0219
*           DO;                     /* TRY TO STEAL IT       @ZA16816*/
*             IF PFTIRRG='1'B THEN  /* IF AN IRRG FRAME THEN @ZA16816*/
         TM    PFTIRRG(FRAMEPTR),B'00010000'                       0220
         BNO   @RF00220                                            0220
*               DO;                 /* STEAL IT DIRECTLY     @ZA16816*/
*                 IF ASCBPTR^=0 THEN/* IF THERE IS AN ASCB,  @ZA16816*/
         LTR   ASCBPTR,ASCBPTR                                     0222
         BZ    @RF00222                                            0222
*                   DO;             /* DEQUEUE THE PFTE NOW  @ZA16816*/
*                     RESPECIFY                                    0224
*                      (REG0) RSTD; /*                       @ZA16816*/
*                     REG0=FRAMEPTR-PVTPFTP;/* GET RBN       @ZA16816*/
         LR    REG0,FRAMEPTR                                       0225
         SL    REG0,PVTPFTP(,PVTPTR)                               0225
*                     REG0=REG0×(65536*PFTNQN);/* INDICATE DEQ     0226
*                                                            @ZA16816*/
         O     REG0,@CF01839                                       0226
*                     RSMHDPTR=ASCBRSM;/* FROM RSMHD         @ZA16816*/
         L     RSMHDPTR,ASCBRSM(,ASCBPTR)                          0227
*                     CALL IEAVPFTE;/* DEQ THE PFTE          @ZA16816*/
         L     @15,PVTPFTE(,PVTPTR)                                0228
         BALR  @14,@15                                             0228
*                     RSMHDPTR=PSAAOLD->ASCBRSM;/* RESTORE RSMHD   0229
*                                                            @ZA16816*/
         L     @14,PSAAOLD                                         0229
         L     RSMHDPTR,ASCBRSM(,@14)                              0229
*                     RESPECIFY                                    0230
*                      (REG0) UNRSTD;/*                      @ZA16816*/
*                   END;            /*                       @ZA16816*/
*                                                                  0231
*                 /***************************************************/
*                 /*                                                 */
*                 /* INITIALIZE THE PFTE (IRRG) FOR USE BY THE CALLER*/
*                 /* AND INDICATE THAT THE PFTE WAS STOLEN.  @ZA16816*/
*                 /*                                                 */
*                 /***************************************************/
*                                                                  0232
*                 PFTIRRG='0'B;     /* CLEAR IRRG FLAG       @ZA16816*/
@RF00222 DS    0H                                                  0233
*                 PFTSTEAL='0'B;    /* CLEAR STEAL FLAG      @ZA16816*/
         NI    PFTIRRG(FRAMEPTR),B'11100111'                       0233
*                 PFTUIC=0;         /* INITIALIZE UIC        @ZA16816*/
         MVI   PFTUIC(FRAMEPTR),X'00'                              0234
*                 SAVER0=FRAMEPTR;  /* SAVE PTR TO PFTE      @ZA16816*/
         ST    FRAMEPTR,SAVER0(,PVTPTR)                            0235
*                 RC=0;             /* SET FINAL RC=0        @ZA16816*/
         MVI   RC(PVTPTR),X'00'                                    0236
*                 RBN=0;            /* KILL DO WHILE LOOP    @ZA16816*/
         SLR   RBN,RBN                                             0237
*                 REG15=0;          /* SET QSCAN RC TO 0     @ZA16816*/
         SLR   REG15,REG15                                         0238
*               END;                /* END OF IRRG PROCESSING      0239
*                                                            @ZA16816*/
*             ELSE                  /* IF FRAME NOT IRRG THEN      0240
*                                                            @ZA16816*/
*               DO;                 /* TRY TO STEAL IT.      @ZA16816*/
         B     @RC00220                                            0240
@RF00220 DS    0H                                                  0241
*                 RESPECIFY                                        0241
*                  (REG0,                                          0241
*                   REG1) RSTD;     /*                       @ZA16816*/
*                 REG1=PFTVBN*256;  /* R1=VSA FOR FINDPAGE   @ZA16816*/
         LH    REG1,PFTVBN(,FRAMEPTR)                              0242
         N     REG1,@CF01844                                       0242
         SLA   REG1,8                                              0242
*                 IF ASCBPTR^=0 THEN/* IF A FOREIGN LFQ, THEN      0243
*                                                            @ZA16816*/
         LTR   ASCBPTR,ASCBPTR                                     0243
         BZ    @RF00243                                            0243
*                   DO;             /* TRAS TO OTHER ADDR SP.      0244
*                                                            @ZA16816*/
*                     PVTTASID=ASCBASID;/* SET TO ASID FOR RCV     0245
*                                                            @ZA16816*/
         MVC   PVTTASID(2,PVTPTR),ASCBASID(ASCBPTR)                0245
*                     LCCAASID=ASCBASID;/* SET TO ASID FOR RTM     0246
*                                                            @ZA16816*/
         L     @14,LCCAPTR                                         0246
         XC    LCCAASID+2(2,@14),LCCAASID+2(@14)                   0246
         MVC   LCCAASID(2,@14),ASCBASID(ASCBPTR)                   0246
*                     GENERATE CODE REFS(ASCBSTOR);                0247
                LCTL 1,1,ASCBSTOR(ASCBPTR)  -----TRAS -----
*                     RSMHDPTR=ASCBRSM;/* GET RSMHD FOR FINDPAGE   0248
*                                                            @ZA16816*/
         L     RSMHDPTR,ASCBRSM(,ASCBPTR)                          0248
*                   END;            /*                       @ZA16816*/
*                 CALL IEAVFP2;     /* INVALIDATE THE PAGE   @ZA16816*/
@RF00243 L     @15,PVTPFP2(,PVTPTR)                                0250
         BALR  @14,@15                                             0250
*                 RSMHDPTR=PSAAOLD->ASCBRSM;/* RESET RSMHD PTR     0251
*                                                            @ZA16816*/
         L     @14,PSAAOLD                                         0251
         L     RSMHDPTR,ASCBRSM(,@14)                              0251
*                 IF REG15^=0 THEN  /* IF FINDPAGE FAILED,   @ZA16816*/
         SLR   @11,@11                                             0252
         CR    REG15,@11                                           0252
         BE    @RF00252                                            0252
*                   DO;             /* C0D AND TRY NEXT PFTE @ZA16816*/
*                     IF ASCBPTR^=0 THEN/* IF DID A TRAS, THEN     0254
*                                                            @ZA16816*/
         CR    ASCBPTR,@11                                         0254
         BE    @RF00254                                            0254
*                       DO;         /* TRAS BACK TO CURRENT  @ZA16816*/
*                         REG1=PSAAOLD;/* GET CURRENT ASCB   @ZA16816*/
         LR    REG1,@14                                            0256
*                         GENERATE CODE REFS(ASCBSTOR);            0257
                     LCTL 1,1,ASCBSTOR(REG1)  -----TRAS BACK-----
*                         LCCAASID=0;/* CLEAR RTM TRAS IND.  @ZA16816*/
         SLR   @14,@14                                             0258
         L     @11,LCCAPTR                                         0258
         ST    @14,LCCAASID(,@11)                                  0258
*                         PVTTASID=0;/* CLEAR RCV TRAS IND.  @ZA16816*/
         STH   @14,PVTTASID(,PVTPTR)                               0259
*                       END;        /*                       @ZA16816*/
*                     RCARCRD='01'X;/* SET REASON CODE       @ZA16816*/
@RF00254 L     @14,RCAPTR(,PVTPTR)                                 0261
         MVI   RCARCRD(@14),X'01'                                  0261
*                     GEN NOFLOWS SETS(REG1)(ABEND X'C0D',,,SYSTEM);
         ABEND X'C0D',,,SYSTEM
*                     RBN=PFTFQPTR; /* GET NEXT PFTE RBN     @ZA16816*/
         LH    @14,PFTFQPTR(,FRAMEPTR)                             0263
         N     @14,@CF01844                                        0263
         LR    RBN,@14                                             0263
*                     REG15=4;      /* INSURE RC STILL INITIALIZED 0264
*                                                            @ZA16816*/
         LA    REG15,4                                             0264
*                   END;            /* END FINDPAGE FAILURE  @ZA16816*/
*                 ELSE              /* SUCCESSFUL FINDPAGE   @ZA16816*/
*                   DO;             /* NOW INVALIDATE PAGE   @ZA16816*/
         B     @RC00252                                            0266
@RF00252 DS    0H                                                  0267
*                     REG1=REG0;    /* SET R1=PGTE ADDR      @ZA16816*/
         LR    REG1,REG0                                           0267
*                     CALL IEAVINV; /* INVALIDATE THE PAGE   @ZA16816*/
*                                                                  0268
         L     @15,PVTPINV(,PVTPTR)                                0268
         BALR  @14,@15                                             0268
*                     /***********************************************/
*                     /*                                             */
*                     /* PAGE IS INVALIDATED. NOW MUST RECHECK TO    */
*                     /* ENSURE PAGE IS STILL UNCHANGED. THIS IS     */
*                     /* NECESSARY IF MP                     @ZA16816*/
*                     /*                                             */
*                     /***********************************************/
*                                                                  0269
*                     REG15=256*(FRAMEPTR-PVTPFTP);/* RSA 1ST 2K   0269
*                                                            @ZA16816*/
         LR    REG15,FRAMEPTR                                      0269
         SL    REG15,PVTPFTP(,PVTPTR)                              0269
         SLA   REG15,8                                             0269
*                     GENERATE CODE REFS(CHBITMSK);                0270
                  ISK REG0,REG15       GET STG KEY FOR 1ST 2K BLOCK
                  N   REG0,CHBITMSK    ISOLATE THE CHANGE BIT - 1ST 2K
                  BNZ REVALID          TO REVALID IF BLOCK CHANGED
                  LA  REG15,2048(REG15) GET    RSA OF 2ND 2K BLOCK
                  ISK REG0,REG15       GET STG KEY FOR 2ND 2K BLOCK
                  N   REG0,CHBITMSK    ISOLATE THE CHANGE BIT - 2ND 2K
                  BZ  STOLEN           TO STOLEN IF 2ND 2K UNCHANGED
*REVALID:                                                          0271
*                     REG1->PGTPVM='0'B;/* REVALIDATE THE PAGE     0271
*                                                            @ZA16816*/
REVALID  NI    PGTPVM(REG1),B'11110111'                            0271
*                     IF ASCBPTR^=0 THEN/* IF DID A TRAS, THEN     0272
*                                                            @ZA16816*/
         LTR   ASCBPTR,ASCBPTR                                     0272
         BZ    @RF00272                                            0272
*                       DO;         /* TRAS BACK TO CURRENT  @ZA16816*/
*                         REG1=PSAAOLD;/* GET CURRENT ASCB   @ZA16816*/
         L     REG1,PSAAOLD                                        0274
*                         GENERATE CODE REFS(ASCBSTOR);            0275
                     LCTL 1,1,ASCBSTOR(REG1) -----TRAS BACK-----
*                         LCCAASID=0;/* CLEAR RTM TRAS IND.  @ZA16816*/
         SLR   @14,@14                                             0276
         L     @11,LCCAPTR                                         0276
         ST    @14,LCCAASID(,@11)                                  0276
*                         PVTTASID=0;/* CLEAR RCV TRAS IND.  @ZA16816*/
         STH   @14,PVTTASID(,PVTPTR)                               0277
*                       END;                                       0278
*                     RBN=FRAMEPTR-PVTPFTP;/* RESTORE ORIGINAL RBN 0279
*                                                            @ZA16816*/
@RF00272 LR    @14,FRAMEPTR                                        0279
         SL    @14,PVTPFTP(,PVTPTR)                                0279
         LR    RBN,@14                                             0279
*                     REG15=4;      /* INSURE RC STILL INITIALIZED 0280
*                                                            @ZA16816*/
         LA    REG15,4                                             0280
*                     GOTO LOOPEND; /* CONTINUE SEARCH WITH THE SAME
*                                      PFTE                  @ZA16816*/
         B     LOOPEND                                             0281
*STOLEN:                                                           0282
*                                                                  0282
*                     /***********************************************/
*                     /*                                             */
*                     /* NON-IRRG FRAME CAN BE STOLEN. IF FROM A     */
*                     /* FOREIGN LOCAL FRAME QUEUE, DEQUEUE THE PFTE */
*                     /* NOW.                                @ZA16816*/
