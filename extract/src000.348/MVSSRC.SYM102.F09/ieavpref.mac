@RF00282 NI    PFTSTEAL(FRAMEPTR),B'11110111'                      0294
*                     PFTUIC=0;     /* INITIALIZE UIC        @ZA16816*/
         MVI   PFTUIC(FRAMEPTR),X'00'                              0295
*                     SAVER0=FRAMEPTR;/* SAVE PTR TO PFTE    @ZA16816*/
         ST    FRAMEPTR,SAVER0(,PVTPTR)                            0296
*                     RC=0;         /* SET RINAL RC=0        @ZA16816*/
         MVI   RC(PVTPTR),X'00'                                    0297
