         BNE   @RT00545                                            0545
*           GOTO LBKEPSEG;          /* PAGE NOT FREEMAINED YET       */
*         ELSE                                                     0547
*           ;                       /* PAGE FREEMAINED, CONTINUE LOOP*/
*       END;                        /* END PGTE SEARCH               */
         AL    PTEPTR,@CF00081                                     0548
@DE00544 LA    @12,30                                              0548
         ALR   @12,RLPARMRG                                        0548
         CR    PTEPTR,@12                                          0548
         BNH   @DL00544                                            0548
*       RLPARMR0=WSA6SEGN;          /* IF ALL PAGES HAVE BEEN      0549
