         MVI   RCACSECT(@12),X'07'                                 0551
*       RLPARMR0=RLPARMRG;          /* PUT THE ADDRESS RETURNED BY 0552
*                                      DESTROY SEGMENT IN REG 0      */
         LR    RLPARMR0,RLPARMRG                                   0552
*       GOTO LBEXIT0A;              /* SKIP SETTING OF REG 0         */
         B     LBEXIT0A                                            0553
*       RESPECIFY                                                  0554
*         STEPTR UNRESTRICTED;                                     0554
*     END;                          /* END PRIVATE AREA PROCESSING   */
*   ELSE                                                           0556
