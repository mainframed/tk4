*                                      ENTRY DCL FOR INTERFACE       */
         L     @15,PVTPCB(,PVTPTR)                                 0815
         BALR  @14,@15                                             0815
*   IF PCBPTR^=MZERO THEN           /* DID A RELATED PCB EXIST       */
         LTR   PCBPTR,PCBPTR                                       0816
         BZ    @RF00816                                            0816
*     DO;                           /* A RELATED PCB EXISTS TO BE  0817
*                                      PROCESSED AND MUST BE SET TO
*                                      APPEAR AS A NORMAL PCB FOR THE
*                                      CALL TO THE PCB MANAGER       */
*       PCBFQP=RLINITRL;            /* SET CURRENT Q NUMBER TO THE 0818
*                                      UNQUEUED VALUE AND THE FORWARD
*                                      Q PTR FIELD TO ZERO           */
         L     @14,RLINITRL                                        0818
         ST    @14,PCBFQP(,PCBPTR)                                 0818
