*                                      PFTE ENQ/DEQ TO MOVE THE PFTE
*                                      FOR THE REAL STORAGE TO AFQ   */
         MVC   PGTRSA(2,PTEPTR),RLCLRPGT                           0368
*                         WSA6HIGH='1'B;/* PFTE TO FRONT OF AFQ      */
         OI    WSA6HIGH(PVTPTR),B'10000000'                        0369
*                         WSA6TQID=PFTAFQN;/* AFQ Q IDENTIFIER       */
         MVI   WSA6TQID(PVTPTR),X'00'                              0370
*                         RLPARMR0=WSA6PARM;/* RSN ALREADY IN WSA    */
         L     RLPARMR0,WSA6PARM(,PVTPTR)                          0371
*                         PVTSQAFX=PVTSQAFX-MONE;/* DECREMENT SQA  0372
*                                      COUNT                 @YA01572*/
         LH    @12,PVTSQAFX(,PVTPTR)                               0372
         BCTR  @12,0                                               0372
         STH   @12,PVTSQAFX(,PVTPTR)                               0372
*                         PVTCNTFX=PVTCNTFX-MONE;/* DECREMENT SYSTEM
*                                      COUNT                 @YA01572*/
         LH    @12,PVTCNTFX(,PVTPTR)                               0373
         BCTR  @12,0                                               0373
         STH   @12,PVTCNTFX(,PVTPTR)                               0373
*                         CALL EPENQDEQ;/* REG 1 NOT SET UP BECAUSE
*                                      PFTE ENQ/DEQ DOESN'T USE. --
*                                      SEE ENTRY DCL FOR INTERFACE   */
         L     @15,PVTPFTE(,PVTPTR)                                0374
         BALR  @14,@15                                             0374
*                         GOTO LBEXIT0;/* EXIT TO THE CALLER         */
         B     LBEXIT0                                             0375
*                       END;        /* END SQA PROCESSING            */
*                     ELSE          /* PAGE NOT SQA                  */
*                       DO;                                        0377
@RF00363 DS    0H                                                  0378
*                         IF PFTASID^=RSMASCB->ASCBASID&PFTVBN^=WSA6VBN
*                           THEN    /* TEST FOR A PAGE USED FOR    0378
*                                      CREATE ADDRESS SPACE          */
         L     @12,RSMASCB(,RSMHDPTR)                              0378
         CLC   PFTASID(2,PFTEPTR),ASCBASID(@12)                    0378
         BE    @RF00378                                            0378
         CLC   PFTVBN(2,PFTEPTR),WSA6VBN(PVTPTR)                   0378
         BE    @RF00378                                            0378
*                           DO;     /* THE PAGE TO BE RELEASED WAS 0379
*                                      USED TO CREATE AN ADDRESS   0379
*                                      SPACE. THE ONLY PROCESSING  0379
