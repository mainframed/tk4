         TITLE 'IEAVRELS -- PAGE RELEASE                               *
                        '
IEAVRELS CSECT
         USING *,15
         MODID BR=NO
IEAVREL2 DS    0H
         ENTRY IEAVREL2
         ENTRY IEAVRELV
         ENTRY IEAVRELF
LBSETBAS BALR  RLBASERG,0
         USING *,RLBASERG
*   CIWRETRN=RLRETREG;              /* SAVE PSI RETURN ADDRESS       */
         ST    RLRETREG,CIWRETRN(,CIWAPTR)                         0109
*   RCACSECT=RCACPGRL;              /* SET CSECT ID IN RCA           */
         L     @12,RCAPTR(,PVTPTR)                                 0110
         MVI   RCACSECT(@12),X'07'                                 0110
*   RCARELS='1'B;                   /* SET ENTRY POINT FLAG IN RCA   */
         OI    RCARELS(@12),B'00000010'                            0111
*                                                                  0112
*   /*****************************************************************/
*   /*                                                               */
*   /* THE MAIN PART OF THE IEAVRELS ENTRY LOGIC CONSISTS OF TWO     */
*   /* LOOPS. THE OUTER LOOP CONTROLS PROCESSING OF VSL ENTRIES. FOR */
*   /* REGISTER FORM REQUESTS (CIWINVSL=0), ONLY ONE VSL ENTRY EXISTS*/
*   /* TO BE PROCESSED. FOR LIST FORM REQUESTS MANY VSL ENTRIES MAY  */
*   /* EXIST. EACH NEW VSL ENTRY IS OBTAINED BY CALLING THE NEXTVSL  */
*   /* SUBROUTINE OF IEAVPSI.                                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0112
*LBVSLOOP:                                                         0112
*   RLTESTAD=CIWSTRTA+MPSZMIN1;     /* REDUCE THE START ADDR IN THE
*                                      CIWA TO THE NEXT HIGHER PAGE
*                                      BOUNDARY                      */
LBVSLOOP LA    RLTESTAD,4095                                       0112
         L     @12,CIWSTRTA-1(,CIWAPTR)                            0112
         LA    @12,0(,@12)                                         0112
         ALR   RLTESTAD,@12                                        0112
*   RLTESTAD=RLTESTAD&MPGBDYMK;                                    0113
         N     RLTESTAD,@CF02426                                   0113
*   RLNUMBYT=CIWEND1A-RLTESTAD;     /* SET UP DO WHILE CONTROL VALUE */
*                                                                  0114
         L     RLNUMBYT,CIWEND1A-1(,CIWAPTR)                       0114
         LA    RLNUMBYT,0(,RLNUMBYT)                               0114
         SLR   RLNUMBYT,RLTESTAD                                   0114
*   /*****************************************************************/
*   /*                                                               */
*   /* THE INNER DO WHILE LOOP CHECKS EACH PAGE IN A VSL ENTRY FOR   */
*   /* PGRLSE ELIGIBILITY AND THEN CALLS THE INTERNAL PAGE PROCESSOR */
*   /* SUBROUTINE (SUBPAGE) TO PERFORM ACTUAL RELEASE PROCESSING. A  */
*   /* PAGE IS ELIGIBLE FOR RELEASE PROCESSING IF IT IS 1) IN A VALID*/
*   /* SEGMENT, 2) IS NOT IN THE QUICK START AREA (QSA), AND 3) IS IN*/
*   /* THE KEY OF THE CALLER IF THE CALLER'S KEY IS NOT ZERO.        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0115
*   DO WHILE(RLNUMBYT>=MPGSIZE);    /* THIS LOOP IS EXITED WHEN THE
*                                      NUMBER OF BYTES TO RELEASE  0115
*                                      FALLS BELOW 4096              */
         B     @DE00115                                            0115
@DL00115 DS    0H                                                  0116
*     RELSVSA=RLTESTAD;             /* STORE VSA IN CIWA             */
         ST    RLTESTAD,RELSVSA(,CIWAPTR)                          0116
*     CIWWRK1=RLNUMBYT;             /* SAVE LOOP CONTROL VALUE       */
         ST    RLNUMBYT,CIWWRK1(,CIWAPTR)                          0117
*     RLPARMRG=RELSVSA;             /* GET PGTE AND XPTE ADDRESSES   */
         L     @12,RELSVSA(,CIWAPTR)                               0118
         LR    RLPARMRG,@12                                        0118
*     CALL EPFINDPG;                /* SEE ENTRY DCL FOR INTERFACE   */
         L     @15,PVTPFP2(,PVTPTR)                                0119
         BALR  @14,@15                                             0119
*     IF RLRETCOD^=MZERO THEN       /* WHAT IS RETURN CODE           */
         LTR   RLRETCOD,RLRETCOD                                   0120
         BZ    @RF00120                                            0120
*       DO;                         /* RETURN CODE IS NOT ZERO       */
*         IF RLRETCOD=8 THEN        /* WHICH ERROR RETURN CODE       */
         C     RLRETCOD,@CF00039                                   0122
         BNE   @RF00122                                            0122
*           DO;                     /* A RETURN CODE 8 MEANS AN    0123
*                                      INTERNAL RSM ERROR            */
*             RCARCRD='01'X;        /* SET REASON CODE IN RCA        */
         L     @12,RCAPTR(,PVTPTR)                                 0124
         MVI   RCARCRD(@12),X'01'                                  0124
*             GEN(ABEND X'C0D',,,SYSTEM) ;/* THE RSM FRR IS GIVEN  0125
*                                      CONTROL TO RECORD THE ERROR 0125
*                                      BEFORE CONTINUING             */
         ABEND X'C0D',,,SYSTEM
*           END;                                                   0126
*         ELSE                                                     0127
*           ;                       /* RETURN CODE 4 MEANS USER ERROR*/
@RF00122 DS    0H                                                  0128
*         GOTO LBERROR;             /* GO TO ERROR EXIT PROCESSING   */
         B     LBERROR                                             0128
*       END;                                                       0129
*     ELSE                                                         0130
*       ;                           /* RETURN CODE WAS ZERO, CONTINUE*/
@RF00120 DS    0H                                                  0131
*     PTEPTR=RLPARMR0;              /* PLACE PGTE AND                */
         LR    PTEPTR,RLPARMR0                                     0131
*     XPTEPTR=RLPARMRG;             /* XPTE ADDRESSES IN BASE REGS   */
         LR    XPTEPTR,RLPARMRG                                    0132
*     IF RELSVBN>=PVTLCSA THEN      /* IS PAGE IN COMMON AREA        */
         CLC   RELSVBN(2,CIWAPTR),PVTLCSA(PVTPTR)                  0133
         BL    @RF00133                                            0133
*       DO;                         /* IF CURRENT PAGE IN COMMON   0134
*                                      AREA, CHECK FOR A QSA PAGE    */
*         IF RELSVBN>=PVTLQSA&RELSVBN<PVTHQSA THEN/* IS PAGE A QSA 0135
*                                      PAGE                          */
         CLC   RELSVBN(2,CIWAPTR),PVTLQSA(PVTPTR)                  0135
         BL    @RF00135                                            0135
         CLC   RELSVBN(2,CIWAPTR),PVTHQSA(PVTPTR)                  0135
         BNL   @RF00135                                            0135
*           DO;                                                    0136
*             RLNUMBYT=CIWWRK1;     /* IF PAGE A QSA PAGE, RESTORE 0137
*                                      LOOP CONTROL VALUE            */
         L     RLNUMBYT,CIWWRK1(,CIWAPTR)                          0137
*             GOTO LBNXTPAG;        /* SKIP ALL PROCESSING FOR PAGE  */
         B     LBNXTPAG                                            0138
*           END;                                                   0139
*         ELSE                                                     0140
*           ;                       /* PAGE NOT IN QSA, CONTINUE     */
@RF00135 DS    0H                                                  0141
*       END;                        /* END COMMON AREA PROCESSING    */
*     ELSE                          /* CURRENT PAGE IN PRIVATE AREA  */
*       DO;                                                        0142
         B     @RC00133                                            0142
@RF00133 DS    0H                                                  0143
*         IF CIWKEY^=MZERO THEN     /* FOR PRIVATE AREA PAGES, IF THE
*                                      CALLERS KEY IS NOT ZERO, THE
*                                      KEY OF THE PAGE MUST EQUAL  0143
*                                      THAT OF THE REQUESTOR         */
         CLI   CIWKEY(CIWAPTR),0                                   0143
         BE    @RF00143                                            0143
*           DO;                                                    0144
*             IF PGTPVM='0'B THEN   /* IF THE PAGE IS IN STORAGE, THE
*                                      KEY OF THE REAL FRAME IS    0145
*                                      TESTED. ONLY THE KEY OF THE 0145
*                                      FIRST 2K BLOCK OF THE FRAME IS
*                                      TESTED BECAUSE RSM ALWAYS SETS
*                                      THE KEYS FOR BOTH 2K PARTS OF
*                                      THE FRAME TO THE SAME VALUE.
*                                      NOTE THE REAL ADDRESS IN    0145
*                                      RLWRKRG7 WAS PLACED THERE BY
*                                      THE LRA INSTRUCTION ABOVE.    */
         TM    PGTPVM(PTEPTR),B'00001000'                          0145
         BNZ   @RF00145                                            0145
*               DO;                                                0146
*                 RESPECIFY                                        0147
*                  (RLWRKRG7,                                      0147
*                   RLWRKRG8) RESTRICTED;                          0147
*                 RLWRKRG7=PGTRSA;  /* GET RBN TO FORM REAL ADDR     */
         SLR   RLWRKRG7,RLWRKRG7                                   0148
         ICM   RLWRKRG7,3,PGTRSA(PTEPTR)                           0148
*                 GEN SETS(RLWRKRG8,RLWRKRG7);                     0149
          SRL   RLWRKRG7,4        REMOVE LOW 4 BITS
          SLL   RLWRKRG7,12       CREATE 24 BIT REAL ADDR
          ISK   RLWRKRG8,RLWRKRG7 GET KEY OF REAL STORAGE
*                 RLWRKRG8=RLWRKRG8&RLISOKEY;/* ISOLATE KEY, ZEROING
*                                      FETCH PROTECT AND R/C BITS FOR
*                                      COMPARE                       */
         N     RLWRKRG8,RLISOKEY                                   0150
*                 IF RLWRKRG8=CIWKEY THEN/* ARE KEYS EQUAL           */
         SLR   @12,@12                                             0151
         IC    @12,CIWKEY(,CIWAPTR)                                0151
         CR    RLWRKRG8,@12                                        0151
         BE    @RT00151                                            0151
*                   GOTO LBCALPAG;  /* IF THE KEYS MATCH, CONTINUE   */
*                 ELSE                                             0153
*                   ;               /* OTHERWISE, ERROR CONDITION    */
*                 RESPECIFY                                        0154
*                  (RLWRKRG7,                                      0154
*                   RLWRKRG8) UNRESTRICTED;                        0154
*               END;                /* END ISK TEST                  */
*             ELSE                  /* TEST KEY IN XPTE FOR PAGES NOT
*                                      IN REAL STORAGE               */
*               DO;                                                0156
         B     @RC00145                                            0156
@RF00145 DS    0H                                                  0157
*                 IF CIWKEY=(XPTPROT&RLISOKY4) THEN/* ARE KEYS EQUAL */
         SLR   @12,@12                                             0157
         IC    @12,XPTPROT(,XPTEPTR)                               0157
         SLR   @11,@11                                             0157
         IC    @11,RLISOKY4                                        0157
         NR    @12,@11                                             0157
         SLR   @11,@11                                             0157
         IC    @11,CIWKEY(,CIWAPTR)                                0157
         CR    @12,@11                                             0157
         BE    @RT00157                                            0157
*                   GOTO LBCALPAG;  /* IF THE KEYS MATCH, CONTINUE   */
*                 ELSE                                             0159
*                   ;               /* OTHERWISE, ERROR CONDITION    */
*               END;                /* END XPTE KEY TEST             */
*LBERROR:                           /* ERROR CONDITION PROCESSING    */
*             CIWRETC=MERRORCD;     /* ERROR CODE IN THE CIWA        */
@RC00145 DS    0H                                                  0161
LBERROR  MVI   CIWRETC(CIWAPTR),X'04'                              0161
*             RLNUMBYT=MPGSIZE;     /* FORCE EXIT FROM THIS LOOP,  0162
*                                      PROCESSING WILL BE TERMINATED */
         L     RLNUMBYT,@CF02411                                   0162
*             GOTO LBNXTPAG;                                       0163
         B     LBNXTPAG                                            0163
*           END;                    /* END NON KEY ZERO PROCESSING   */
*         ELSE                                                     0165
*           ;                       /* CALLERS KEY IS ZERO, CONTINUE */
@RF00143 DS    0H                                                  0166
*       END;                        /* END PRIVATE AREA PROCESSING   */
*LBCALPAG:                                                         0167
*     CALL SUBPAGE;                 /* CALL THE PAGE PROCESSOR SUBRTN*/
@RC00133 DS    0H                                                  0167
LBCALPAG BAL   @14,SUBPAGE                                         0167
*     RESPECIFY                                                    0168
*       RLRETCOD RESTRICTED;                                       0168
*     IF RLRETCOD^=MZERO THEN       /* CHECK SUBPAGE RETURN CODE     */
         LTR   RLRETCOD,RLRETCOD                                   0169
         BZ    @RF00169                                            0169
*       DO;                         /* THE PAGE PROCESSOR HAS FOUND
*                                      AN ERROR, PROCESSING MUST BE
*                                      TERMINATED                    */
*         CIWRETC=MERRORCD;         /* SET ERROR RETURN CODE         */
         MVI   CIWRETC(CIWAPTR),X'04'                              0171
*         RLNUMBYT=MPGSIZE;         /* FORCE EXIT FROM LOOP          */
         L     RLNUMBYT,@CF02411                                   0172
*       END;                                                       0173
*     ELSE                          /* SUBPAGE RETURN CODE IS ZERO   */
*       DO;                         /* NO ERROR WAS FOUND            */
         B     @RC00169                                            0174
@RF00169 DS    0H                                                  0175
*         RLNUMBYT=CIWWRK1;         /* RESTORE LOOP CONTROL VALUE    */
         L     RLNUMBYT,CIWWRK1(,CIWAPTR)                          0175
*       END;                                                       0176
*LBNXTPAG:                                                         0177
*     RLTESTAD=RELSVSA+MPGSIZE;     /* ADJUST LOOP VALUES            */
@RC00169 DS    0H                                                  0177
LBNXTPAG L     @14,@CF02411                                        0177
         L     RLTESTAD,RELSVSA(,CIWAPTR)                          0177
         ALR   RLTESTAD,@14                                        0177
*     RLNUMBYT=RLNUMBYT-MPGSIZE;    /* FOR NEXT PAGE                 */
         SLR   RLNUMBYT,@14                                        0178
*   END;                            /* END RLNUMBYT DO WHILE         */
@DE00115 C     RLNUMBYT,@CF02411                                   0179
         BNL   @DL00115                                            0179
*                                                                  0180
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS IS THE MAIN PART OF THE OUTER LOOP WHERE ADDITIONAL VSL  */
*   /* ENTRIES ARE SET UP TO BE PROCESSED IF THEY EXIST. ONCE ALL    */
*   /* EXISTING VSL ENTRIES HAVE BEEN PROCESSED, OR IF AN ERROR      */
*   /* EXISTS, EXIT IS MADE TO PSI.                                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0180
*   IF CIWRETC=MZERO THEN                                          0180
         CLI   CIWRETC(CIWAPTR),0                                  0180
         BNE   @RF00180                                            0180
*     DO;                           /* IF AN ERROR WAS NOT DETECTED
*                                      IN THE ABOVE LOOP, GET THE  0181
*                                      NEXT VSL FOR LIST REQUESTS, 0181
*                                      EXIT FOR REGISTER REQUESTS    */
*       IF CIWINVSL^=MZERO THEN     /* IS REQUEST LIST FORM          */
         L     @14,CIWINVSL(,CIWAPTR)                              0182
         LTR   @14,@14                                             0182
         BZ    @RF00182                                            0182
*         DO;                       /* CALL PSI SUBROUTINE TO GET  0183
*                                      NEXT VSL ENTRY OF A LIST    0183
*                                      REQUEST                       */
*           CALL NEXTVSL;           /* SEE ENTRY DCL FOR INTERFACE   */
         L     @15,@CV02455                                        0184
         BALR  @14,@15                                             0184
*           IF RLRETCOD=MZERO THEN  /* MORE VSL ENTRIES TO PROCESS   */
         LTR   RLRETCOD,RLRETCOD                                   0185
         BZ    @RT00185                                            0185
*             GOTO LBVSLOOP;        /* IF MORE VSL ENTRIES EXIST TO
*                                      BE PROCESSED, PROCESS THEM    */
*           ELSE                    /* RETURN CODE NOT ZERO          */
*             DO;                   /* OTHERWISE, CHECK FOR ERRORS   */
*               IF RLRETCOD^=LISTDONE THEN/* WAS ERROR DETECTED      */
         C     RLRETCOD,@CF00039                                   0188
         BE    @RF00188                                            0188
*                 DO;               /* AN ERROR WAS DETECTED         */
*                   CIWRETC=MPARMERR;/* ERROR CODE IN CIWA           */
         MVI   CIWRETC(CIWAPTR),X'10'                              0190
*                 END;                                             0191
*               ELSE                                               0192
*                 ;                 /* NO ERROR, REQUEST IS COMPLETE */
@RF00188 DS    0H                                                  0193
*             END;                                                 0193
*         END;                                                     0194
*       ELSE                                                       0195
*         ;                         /* REQUEST IS REGISTER FORM      */
@RF00182 DS    0H                                                  0196
*     END;                          /* END CONTINUATION PROCESSING   */
*   ELSE                            /* AN ERROR WAS DETECTED         */
*     DO;                           /* IF AN ERROR WAS DETECTED IN 0197
*                                      PAGE PROCESSING, THE ERROR  0197
*                                      FLAG MUST BE SET IN A VSL   0197
*                                      ENTRY FOR LIST FORM ENTRIES   */
         B     @RC00180                                            0197
@RF00180 DS    0H                                                  0198
*       IF CIWINVSL^=MZERO THEN     /* IS REQUEST LIST FORM          */
         L     @14,CIWINVSL(,CIWAPTR)                              0198
         LTR   @14,@14                                             0198
         BZ    @RF00198                                            0198
*         DO;                       /* YES                           */
*           CIWIERR='1'B;           /* SET ERROR FLAG IN CIWA COPY   */
         OI    CIWIERR(CIWAPTR),B'00010000'                        0200
*           VSL=CIWVSL;             /* STORE CIWA COPY OF VSL OVER 0201
*                                      USER SUPPLIED COPY            */
         L     @14,VSLPTR(,CIWAPTR)                                0201
         MVC   VSL(8,@14),CIWVSL(CIWAPTR)                          0201
*         END;                                                     0202
*       ELSE                                                       0203
*         ;                         /* REQUEST IS REGISTER FORM      */
@RF00198 DS    0H                                                  0204
*     END;                                                         0204
*   RCARELS='0'B;                   /* RESET THE RCA ENTRY POINT FLAG*/
@RC00180 L     @14,RCAPTR(,PVTPTR)                                 0205
         NI    RCARELS(@14),B'11111101'                            0205
*   RLRETREG=CIWRETRN;              /* RESTORE RETURN ADDRESS TO PSI */
         L     RLRETREG,CIWRETRN(,CIWAPTR)                         0206
*   RESPECIFY                                                      0207
*     RLRETCOD UNRESTRICTED;                                       0207
*   GEN(BR    RLRETREG) ;           /* RETURN TO PSI                 */
         BR    RLRETREG
*                                                                  0209
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS IS THE SPECIAL FREEMAIN RELEASE ENTRY POINT. CONTROL IS  */
*   /* RECEIVED ONLY FROM VSM WITH THE SALLOC LOCK HELD. THE         */
*   /* OWNERSHIP OF THE LOCAL LOCK IS UNCERTAIN. THIS ENTRY SAVES    */
*   /* REGISTERS, ESTABLISHES THE RECOVERY ENVIRONMENT, AND          */
*   /* INITIALIZES THE STANDARD RSM REGISTERS. THIS SEGMENT ALSO     */
*   /* PREPARES THE INPUT ADDRESSES IN REGISTERS 0 AND 1 FOR THE LOOP*/
*   /* IN THE SECOND SEGMENT.                                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0209
*IEAVRELV:                                                         0209
*   ENTRY;                                                         0209
*   GENERATE REFS(PVTPTR) SETS(PVTWSA5,RLBASERG,RLWRKRG7,PVTPRCA); 0210
IEAVRELV STM   RLOWADDR,RLRETREG,PVTWSA5(PVTPTR)
*                                   SAVE CALLERS REGISTERS
LBRELVBS BALR  RLBASERG,0           SET UP THE BASE REG FOR ENTRY
         LA    RLWRKRG7,LBRELVBS-LBSETBAS  ADJUST BASE REG TO VALUE
         SR    RLBASERG,RLWRKRG7           USED THROUGHOUT MODULE
         SETFRR A,FRRAD=PVTPRCV(PVTPTR),PARMAD=PVTPRCA(,PVTPTR),      **
               WRKREGS=(RLWRKRG7,RLWRKRG8) ESTABLISH RSM FRR
*   RCACSECT=RCACPGRL;              /* SET MODULE CSECT ID           */
         L     @12,RCAPTR(,PVTPTR)                                 0211
         MVI   RCACSECT(@12),X'07'                                 0211
*   RCARELV='1'B;                   /* SET ENTRY POINT FLAG          */
         OI    RCARELV(@12),B'01000000'                            0212
*   RSMHDPTR=ASCBRSM;               /* PLACE RSM HEADER ADDRESS IN 0213
*                                      STANDARD RSM REGISTER         */
         L     RSMHDPTR,ASCBRSM(,ASCBPTR)                          0213
*   WSA5FREE=MZERO;                 /* INITIALIZE THE SAVE AREA FOR
*                                      FREE PGT/XPT SPACE            */
         SLR   @12,@12                                             0214
         ST    @12,WSA5FREE(,PVTPTR)                               0214
*   RLTESTAD=RLOWADDR+MPSZMIN1;     /* REDUCE THE INPUT LOW ADDR TO
*                                      THE NEXT HIGHER PAGE BOUNDARY */
         LA    RLTESTAD,4095                                       0215
         ALR   RLTESTAD,RLOWADDR                                   0215
*   RLTESTAD=RLTESTAD&MPGBDYMK;                                    0216
         N     RLTESTAD,@CF02426                                   0216
*   RLNUMBYT=RLHIADDR-RLTESTAD;     /* SET UP LOOP CONTROL VALUE     */
         LR    RLNUMBYT,RLHIADDR                                   0217
         SLR   RLNUMBYT,RLTESTAD                                   0217
*   WSA5VSA=RLTESTAD;               /* STORE ADDR FIRST PAGE IN WSA  */
         ST    RLTESTAD,WSA5VSA(,PVTPTR)                           0218
*   RESPECIFY                                                      0219
*     RLRETCOD RESTRICTED;          /* PROTECT REG 15 FROM COMPILER  */
*   RLRETCOD=MZERO;                 /* INITIALIZE RETURN CODE TO ZERO
*                                      BEFORE BEGINNING DO WHILE LOOP*/
         SLR   RLRETCOD,RLRETCOD                                   0220
*                                                                  0221
*   /*****************************************************************/
*   /*                                                               */
*   /* A LOOP IS NOW EXECUTED THAT IS CONTROLLED BY THE NUMBER OF    */
*   /* BYTES THERE ARE TO RELEASE. THE FREEMAIN PAGE PROCESSOR IS    */
*   /* CALLED AS A SUBROUTINE. THIS SEGMENT ALSO MAINTAINS A CHAIN OF*/
*   /* AREAS TO BE FREEMAINED THAT WILL EVENTUALLY BE RETURNED TO    */
*   /* VSM.                                                          */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0221
*   DO WHILE(RLNUMBYT>=MPGSIZE);    /* THE LOOP IS EXITED WHEN LESS
*                                      THAN 4096 BYTES REMAIN TO BE
*                                      FREEMAINED.                   */
         B     @DE00221                                            0221
@DL00221 DS    0H                                                  0222
*     IF(WSA5VBN>=PVTLQSA&WSA5VBN<PVTHQSA) THEN/* IF THE PAGE IS IN
*                                      THE QUICK START AREA, A     0222
*                                      FREEMAIN ERROR HAS OCCURRED.  */
         CLC   WSA5VBN(2,PVTPTR),PVTLQSA(PVTPTR)                   0222
         BL    @RF00222                                            0222
         CLC   WSA5VBN(2,PVTPTR),PVTHQSA(PVTPTR)                   0222
         BNL   @RF00222                                            0222
*       DO;                                                        0223
*         RLNUMBYT=MPGSIZE;         /* FORCE EXIT FROM THE LOOP      */
         L     RLNUMBYT,@CF02411                                   0224
*         RLRETCOD=MERRORCD;        /* SET THE RETURN CODE = 4       */
         LA    RLRETCOD,4                                          0225
*       END;                                                       0226
*     ELSE                          /* OTHERWISE, CALL THE FREEMAIN
*                                      PAGE PROCESSOR                */
*       DO;                                                        0227
         B     @RC00222                                            0227
@RF00222 DS    0H                                                  0228
*         RLPARMRG=WSA5VSA;         /* SET UP THE PARAMETER REG      */
         L     RLPARMRG,WSA5VSA(,PVTPTR)                           0228
*         WSA5NUM=RLNUMBYT;         /* SAVE NUMBER OF BYTES TO RLSE  */
         ST    RLNUMBYT,WSA5NUM(,PVTPTR)                           0229
*         CALL SUBFMPAG;            /* CALL FREEMAIN PAGE PROCESSOR  */
         BAL   @14,SUBFMPAG                                        0230
*         IF RLRETCOD=MDFRLSCD THEN /* WAS RELEASE DEFERED           */
         C     RLRETCOD,@CF00039                                   0231
         BNE   @RF00231                                            0231
*           DO;                     /* A DEFERED RELEASE CONDITION 0232
*                                      HAS BEEN DETECTED.            */
*             WSA5DRLS=WSA5VSA;     /* SAVE THE ADDRESS OF THE     0233
*                                      DEFERED RELEASE PAGE          */
         L     @14,WSA5VSA(,PVTPTR)                                0233
         ST    @14,WSA5DRLS(,PVTPTR)                               0233
*             RLNUMBYT=MPGSIZE;     /* FORCE EXIT FROM LOOP          */
         L     RLNUMBYT,@CF02411                                   0234
*           END;                                                   0235
*         ELSE                      /* NO DEFERED RELEASE            */
*           DO;                                                    0236
         B     @RC00231                                            0236
@RF00231 DS    0H                                                  0237
*             IF RLRETCOD^=MGOODCD THEN/* WAS ERROR DETECTED         */
         LTR   RLRETCOD,RLRETCOD                                   0237
         BZ    @RF00237                                            0237
*               DO;                 /* AN ERROR WAS DETECTED         */
*                 RLNUMBYT=MPGSIZE; /* FORCE EXIT FROM LOOP          */
         L     RLNUMBYT,@CF02411                                   0239
*               END;                                               0240
*             ELSE                  /* OTHERWISE, A SUCCESSFUL     0241
*                                      RELEASE WAS EXECUTED          */
*               DO;                                                0241
         B     @RC00237                                            0241
@RF00237 DS    0H                                                  0242
*                 IF RLRETSPC^=MZERO THEN/* WAS PGT/XPT SPACE      0242
*                                      RETURNED                      */
         SLR   @14,@14                                             0242
         CR    RLRETSPC,@14                                        0242
         BE    @RF00242                                            0242
*                   DO;             /* PGT/XPT SPACE HAS BEEN      0243
*                                      RETURNED TO BE FREEMAINED. THE
*                                      ADDRESS OF THIS AREA IS SAVED
*                                      AS EITHER THE FIRST AREA IN 0243
*                                      THE REGISTER 2 SAVEAREA OF THE
*                                      WSA OR ADDED TO A CHAIN OF  0243
*                                      AREAS.                        */
*                     IF WSA5FREE=MZERO THEN/* IS THIS FIRST AREA    */
         C     @14,WSA5FREE(,PVTPTR)                               0244
         BNE   @RF00244                                            0244
*                       DO;         /* FIRST AREA TO BE SAVED        */
*                         WSA5FREE=RLRETSPC;/* SAVE AREA ADDR        */
         ST    RLRETSPC,WSA5FREE(,PVTPTR)                          0246
*                         FRECHAIN=MZERO;/* FIRST WORD OF AREA=0     */
         L     @12,WSA5FREE(,PVTPTR)                               0247
         ST    @14,FRECHAIN(,@12)                                  0247
*                       END;                                       0248
*                     ELSE          /* NOT FIRST AREA RETURNED       */
*                       DO;         /* ATTACH AREA TO END OF EXISTING
*                                      CHAIN                         */
         B     @RC00244                                            0249
@RF00244 DS    0H                                                  0250
*                         RESPECIFY                                0250
*                           RLWRKRG7 RESTRICTED;                   0250
*                         RLWRKRG7=WSA5FREE;/* GET ADDR 1ST AREA     */
         L     RLWRKRG7,WSA5FREE(,PVTPTR)                          0251
*                         RESPECIFY                                0252
*                           FREESPAC BASED(RLWRKRG7);              0252
*                         DO WHILE(FRECHAIN^=MZERO);/* THIS DO WHILE
*                                      LOOP GOES DOWN THE AREA CHAIN
*                                      UNTIL THE LAST AREA IS FOUND  */
         B     @DE00253                                            0253
@DL00253 DS    0H                                                  0254
*                           RLWRKRG7=FRECHAIN;                     0254
         L     RLWRKRG7,FRECHAIN(,RLWRKRG7)                        0254
*                         END;      /* END FRECHAIN DO WHILE         */
@DE00253 SLR   @14,@14                                             0255
         C     @14,FRECHAIN(,RLWRKRG7)                             0255
         BNE   @DL00253                                            0255
*                         FRECHAIN=RLRETSPC;/* PLACE NEW AREA ADDR IN
*                                      LAST AREA                     */
         ST    RLRETSPC,FRECHAIN(,RLWRKRG7)                        0256
*                         RLWRKRG7=RLRETSPC;                       0257
         LR    RLWRKRG7,RLRETSPC                                   0257
*                         FRECHAIN=MZERO;/* FIRST WORD OF NEW AREA=0 */
         ST    @14,FRECHAIN(,RLWRKRG7)                             0258
*                         RESPECIFY                                0259
*                           RLWRKRG7 UNRESTRICTED;                 0259
*                       END;        /* END ADD TO CHAIN GROUP        */
*                   END;            /* END SAVE PGT/XPTSPACE GROUP   */
*                 ELSE                                             0262
*                   ;               /* IT MUST BE ASSURED THAT REG 15
*                                      CONTAINS A ZERO AS EXIT FROM
*                                      THE LOOP MAY OCCUR            */
@RF00242 DS    0H                                                  0263
*                 RLNUMBYT=WSA5NUM; /* RESTORE CORRECT NUMBER OF   0263
*                                      BYTES TO RELEASE.             */
@RC00242 L     RLNUMBYT,WSA5NUM(,PVTPTR)                           0263
*               END;                /* END SUCCESSFUL PROCESSING     */
*           END;                    /* END NON DEFERED RELEASE CODE  */
@RC00237 DS    0H                                                  0266
*       END;                        /* END PAGE PROCESSOR GROUP      */
@RC00231 DS    0H                                                  0267
*     WSA5VSA=WSA5VSA+MPGSIZE;      /* TESTADDR IS NEXT PAGE IN SEQ. */
@RC00222 L     @14,@CF02411                                        0267
         L     @12,WSA5VSA(,PVTPTR)                                0267
         ALR   @12,@14                                             0267
         ST    @12,WSA5VSA(,PVTPTR)                                0267
*     RLNUMBYT=RLNUMBYT-MPGSIZE;    /* REDUCE NUMBER OF BYTES TO FREE*/
         SLR   RLNUMBYT,@14                                        0268
*     RESPECIFY                                                    0269
*       RLRETCOD UNRESTRICTED;                                     0269
*   END;                            /* END THE DO WHILE LOOP         */
@DE00221 C     RLNUMBYT,@CF02411                                   0270
         BNL   @DL00221                                            0270
*                                                                  0271
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS SEGMENT GETS CONTROL UPON EXIT FROM THE DO WHILE LOOP.   */
*   /* REGISTER 15 IS ALREADY SET WITH THE PROPER RETURN CODE. ALL   */
*   /* THAT MUST BE DONE IS TO CANCEL THE RSM FRR AND RESTORE        */
*   /* REGISTERS. IT SHOULD BE NOTED THAT WITH THE RESTORING OF      */
*   /* REGISTERS, THE PROPER OUTPUT PARAMETERS ARE ALSO SET          */
*   /* AUTOMATICALLY SINCE THE CORRECT VALUES HAVE BEEN PLACED IN THE*/
*   /* REGISTER SAVE AREA BY THE LAST SEGMENT.                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0271
*   GENERATE REFS(PVTWSA5);                                        0271
         SETFRR D,WRKREGS=(RLWRKRG7,RLWRKRG8)
         LM    RLOWADDR,RLRETREG,PVTWSA5(PVTPTR)
*   GOTO RLRETREG;                  /* RETURN TO THE CALLER          */
         BR    RLRETREG                                            0272
*                                                                  0273
*   /*****************************************************************/
*   /*                                                               */
*   /* THE DEFERED RELEASE ENTRY POINT RECEIVES CONTROL FROM ANY RSM */
*   /* MODULE WHO DETECTS THE DEFERED RELEASE CONDITION WHEN A PFTE  */
*   /* FIX COUNT GOES TO ZERO. INPUT IS A VIRTUAL ADDRESS IN REGISTER*/
*   /* 1 AND THE STANDARD RSM VALUES IN REGISTERS 2 AND 3. THE       */
*   /* CALLERS REGISTERS ARE SAVED AND SUBFMPAG CALLED TO COMPLETE   */
*   /* THE DEFERED RELEASE. UPON RETURN, IF A PGT/XPT IS TO BE FREED,*/
*   /* IT IS FREEMAINED AND RETURN MADE TO THE CALLER WITH A ZERO    */
*   /* ALWAYS IN REGISTER 0.                                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0273
*IEAVRELF:                                                         0273
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0273
IEAVRELF DS    0H                                                  0274
*   GENERATE SETS(PVTWSA6,RLBASERG,RLWRKRG7);                      0274
         STM   RLPARMRG,RLRETREG,PVTWSA6(PVTPTR) SAVE REGISTERS
LBRELFBS BALR  RLBASERG,0           SET UP THE BASE REG FOR ENTRY
         LA    RLWRKRG7,LBRELFBS-LBSETBAS ADJUST BASE REG TO VALUE
         SR    RLBASERG,RLWRKRG7          USED THROUGHOUT MODULE
*   RCACSECT=RCACPGRL;              /* INIT RCA WITH CSECT ID        */
         L     @12,RCAPTR(,PVTPTR)                                 0275
         MVI   RCACSECT(@12),X'07'                                 0275
*   RCARELF='1'B;                   /* ENTRY POINT FLAG SET          */
         OI    RCARELF(@12),B'10000000'                            0276
*   CALL SUBFMPAG;                  /* THE FREEMAIN PAGE PROCESSOR IS
*                                      CALLED TO COMPLETE THE DEFERED
*                                      RELEASE                       */
         BAL   @14,SUBFMPAG                                        0277
*   RESPECIFY                                                      0278
*     RLRETCOD RESTRICTED;                                         0278
*   IF RLRETCOD=0 THEN              /* IF THE RETURN CODE IS ZERO, 0279
*                                      FREEMAIN ANY SPACE POINTED TO
*                                      BY REGISTER 0                 */
         SLR   @14,@14                                             0279
         CR    RLRETCOD,@14                                        0279
         BNE   @RF00279                                            0279
*     DO;                                                          0280
*       IF RLRETSPC^=0 THEN         /* IS THERE SPACE TO FREE        */
         CR    RLRETSPC,@14                                        0281
         BE    @RF00281                                            0281
*         DO;                                                      0282
*           RESPECIFY                                              0283
*            (RLFMRCAS,                                            0283
*             RLFMPVTS,                                            0283
*             RLFMTCB,                                             0283
*             RLWRKRG7) RESTRICTED;                                0283
*           RLPARMRG=RLRETSPC;      /* XFER REG 0 TO REG 1           */
         LR    RLPARMRG,RLRETSPC                                   0284
*           RLPARMR0=(LENGTH(PGTPTE)+LENGTH(XPTE))*16;/* CALCULATE 0285
*                                      LENGTH OF PGT/XPT SPACE TO BE
*                                      FREEMAINED                    */
         LA    RLPARMR0,224                                        0285
*           RLFMRCAS=PVTPRCA;       /* SAVE RCA ADDRESS AS FREEMAIN
*                                      MAY REENTER RSM               */
         L     RLFMRCAS,PVTPRCA(,PVTPTR)                           0286
*           RLFMPVTS=PVTPTR;        /* SAVE PVT ADDR AS FREEMAIN USES
*                                      REG 3                         */
         LR    RLFMPVTS,PVTPTR                                     0287
*           RLFMTCB=0;              /* SET FREEMAIN TCB ADDR TO ZERO */
         SLR   RLFMTCB,RLFMTCB                                     0288
*           RLWRKRG7=PSAAOLD;       /* SET ASCB ADDR IN REG 7        */
         L     RLWRKRG7,PSAAOLD                                    0289
*           GENERATE REFS(RLPARMR0,RLPARMRG,RLFMTCB,RLWRKRG7) SETS(
*               PVTPTR,RLRETCOD);                                  0290
         FREEMAIN RC,LV=(0),A=(1),BRANCH=YES,SP=255,KEY=0,            **
               RELATED=(PGTXPT,IEAVGM00('GET PGT/XPT SPACE'))
*           PVTPTR=RLFMPVTS;        /* RESTORE PVT ADDR REG          */
         LR    PVTPTR,RLFMPVTS                                     0291
*           PVTPRCA=RLFMRCAS;       /* RESTORE RCA ADDR IN PVT       */
         ST    RLFMRCAS,PVTPRCA(,PVTPTR)                           0292
*           RESPECIFY                                              0293
*            (RLFMRCAS,                                            0293
*             RLFMPVTS,                                            0293
*             RLFMTCB,                                             0293
*             RLWRKRG7) UNRESTRICTED;                              0293
*           IF RLRETCOD^=0 THEN     /* WAS FREEMAIN SUCCESSFUL       */
         LTR   RLRETCOD,RLRETCOD                                   0294
         BZ    @RF00294                                            0294
*             DO;                   /* IF FREEMAIN NOT SUCCESSFUL, 0295
*                                      RECORD ERROR AND CONTINUE     */
*               RCARCRD='03'X;      /* SET REASON CODE IN RCA        */
         L     @14,RCAPTR(,PVTPTR)                                 0296
         MVI   RCARCRD(@14),X'03'                                  0296
*               GEN(ABEND X'C0D',,,SYSTEM) ;                       0297
         ABEND X'C0D',,,SYSTEM
*               RLRETCOD=0;         /* FORCE RETURN CODE TO ZERO     */
         SLR   RLRETCOD,RLRETCOD                                   0298
*               RESPECIFY                                          0299
*                 RLRETCOD UNRESTRICTED;                           0299
*             END;                                                 0300
*           ELSE                                                   0301
*             ;                     /* FREEMAIN WAS SUCCESSFUL       */
@RF00294 DS    0H                                                  0302
*         END;                                                     0302
*       ELSE                                                       0303
*         ;                         /* NO SPACE TO FREEMAIN          */
@RF00281 DS    0H                                                  0304
*     END;                          /* END RETURN CODE 0 PROCESSING  */
*   ELSE                                                           0305
*     ;                             /* DEFERED RELEASE WAS NOT     0305
*                                      COMPLETED                     */
@RF00279 DS    0H                                                  0306
*   RLPARMR0=0;                     /* REG 0 ALWAYS SET TO ZERO FOR
*                                      THE RETURN TO THE CALLER.     */
         SLR   RLPARMR0,RLPARMR0                                   0306
*   RCARELF='0'B;                   /* TURN OFF ENTRY POINT FLAG     */
         L     @12,RCAPTR(,PVTPTR)                                 0307
         NI    RCARELF(@12),B'01111111'                            0307
*   GENERATE REFS(PVTWSA6);                                        0308
         LM    RLPARMRG,RLRETREG,PVTWSA6(PVTPTR)   RESTORE REGS
*   END;                            /* END IEAVRELF SUBPROCEDURE     */
@EL00002 DS    0H                                                  0309
@EF00002 DS    0H                                                  0309
@ER00002 BR    @14                                                 0309
*                                                                  0310
*   /*****************************************************************/
*   /*                                                               */
*   /* THE FREEMAIN PAGE PROCESSOR RECEIVES CONTROL FROM THE IEAVRELV*/
*   /* OR IEAVRELF ENTRY POINTS WITH THE STANDARD RSM POINTERS IN    */
*   /* REGISTERS 2 AND 3. THIS SEGMENT SAVES THE CALLERS RETURN      */
*   /* ADDRESS AND DOES PRELIMINARY SET UP AND ERROR CHECKING BEFORE */
*   /* CONTROL GOES TO THE IN-STORAGE OR OUT-OF-STORAGE SEGMENTS.    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0310
*SUBFMPAG:                                                         0310
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0310
SUBFMPAG DS    0H                                                  0311
*   WSA6RETS=RLRETREG;              /* SAVE RETURN ADDRESS           */
         ST    RLRETREG,WSA6RETS(,PVTPTR)                          0311
*   WSA6VSA=RLPARMRG;               /* STORE INPUT VSA IN WSA FOR USE*/
         ST    RLPARMRG,WSA6VSA(,PVTPTR)                           0312
*   WSA6LOWB=WSA6LOWB&MVBNMASK;     /* ZERO LOW ORDER 4 BITS OF VBN
*                                      PORTION OF VSA TO MAKE A VBN  */
         NI    WSA6LOWB(PVTPTR),X'F0'                              0313
*   IF WSA6VBN<PVTLPRIV THEN        /* IS PAGE IN NUCLEUS            */
         CLC   WSA6VBN(2,PVTPTR),PVTLPRIV(PVTPTR)                  0314
         BNL   @RF00314                                            0314
*     DO;                                                          0315
*       GOTO LBEXIT0;               /* IGNORE RELEASE REQUESTS FOR 0316
*                                      NUCLEUS PAGES                 */
         B     LBEXIT0                                             0316
*     END;                                                         0317
*   ELSE                                                           0318
*     ;                             /* PAGE NOT IN NUCLEUS, CONTINUE */
@RF00314 DS    0H                                                  0319
*   CALL EPFINDPG;                  /* FINDPAGE IS CALLED TO OBTAIN
*                                      PGTE AND XPTE ADDRS -- SEE  0319
*                                      ENTRY DCL FOR INTERFACE       */
         L     @15,PVTPFP2(,PVTPTR)                                0319
         BALR  @14,@15                                             0319
*   IF RLRETCOD^=MZERO THEN         /* IS RETURN CODE ZERO           */
         LTR   RLRETCOD,RLRETCOD                                   0320
         BZ    @RF00320                                            0320
*     IF RLRETCOD=8 THEN            /* NO, IS ERROR USER OR RSM      */
         C     RLRETCOD,@CF00039                                   0321
         BE    @RT00321                                            0321
*       GOTO LBRFRR01;              /* IF A RETURN CODE 8 IS ISSUED
*                                      BY FINDPAGE, AN INTERNAL ERROR
*                                      HAS OCCURRED IN RSM.          */
*     ELSE                          /* ERROR IS USER ERROR           */
*       GOTO LBEXIT4;               /* GO TO ERROR EXIT              */
         B     LBEXIT4                                             0323
*   PTEPTR=RLPARMR0;                /* OTHERWISE, THE PGTE AND       */
@RF00320 LR    PTEPTR,RLPARMR0                                     0324
*   XPTEPTR=RLPARMRG;               /* XPTE ADDRESSES ARE PLACED IN
*                                      THEIR CORRECT REGISTERS       */
         LR    XPTEPTR,RLPARMRG                                    0325
*   RESPECIFY                                                      0326
*     PFTEPTR RESTRICTED;                                          0326
*   PFTEPTR=PGTRSA;                 /* BUILD PFTE ADDRESS USING THE
*                                      RSN IN THE PGTE.              */
         SLR   PFTEPTR,PFTEPTR                                     0327
         ICM   PFTEPTR,3,PGTRSA(PTEPTR)                            0327
*   PFTEPTR=PFTEPTR&MREALMSK;       /* AND OFF PGTE FLAG BITS FOR A
*                                      PFTE INDEX.                   */
         N     PFTEPTR,@CF02422                                    0328
*   WSA6RSN=PFTEPTR;                /* SAVE THE INDEX IN THE WSA     */
         STH   PFTEPTR,WSA6RSN(,PVTPTR)                            0329
*   RLSAVPCB=0;                     /* CLEAR WORD TO BE USED AT LABEL
*                                      LBXPTPRC FOR XPTE PROCESSING.
*                                      WORD CONTAINS THE ADDRESS OF
*                                      THE PCB WITH VBN MATCHING THE
*                                      INPUT VBN             @Z40WPYD*/
         SLR   @12,@12                                             0330
         ST    @12,RLSAVPCB(,PVTPTR)                               0330
*   PFTEPTR=PFTEPTR+PVTPFTP;        /* GET FULL PFTE ADDRESS         */
         AL    PFTEPTR,PVTPFTP(,PVTPTR)                            0331
*   IF PGTPVM='0'B THEN             /* THE PGTE INVALID FLAG IS    0332
*                                      TESTED TO DETERMINE WHICH   0332
*                                      SEGMENT BELOW RECEIVES CNTRL  */
         TM    PGTPVM(PTEPTR),B'00001000'                          0332
         BNZ   @RF00332                                            0332
*                                                                  0333
*     /***************************************************************/
*     /*                                                             */
*     /* THE IN-STORAGE SEGMENT PROCESSES ALL PAGES THAT ARE IN REAL */
*     /* STORAGE. A NUMBER OF SPECIAL CONDITION CHECKS ARE MADE AND  */
*     /* PROCESSED FIRST. IF NONE OF THE CONDITIONS ARISE PROCESSING */
*     /* IS CONCLUDED WITH NORMAL FREEING OF THE REAL STORAGE AND    */
*     /* INVALIDATION OF THE PAGE                                    */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0333
*     DO;                                                          0333
*                                                                  0333
*       /*************************************************************/
*       /*                                                           */
*       /* SPECIAL CONDITION PROCESSING                              */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0334
*       IF PFTDFRLS='1'B THEN       /* IS A DEFERED RELEASE PENDING  */
         TM    PFTDFRLS(PFTEPTR),B'10000000'                       0334
         BNO   @RF00334                                            0334
*         DO;                       /* A DEFERED RELEASE IS TO BE  0335
*                                      COMPLETED.                    */
*           IF PFTFXCT^=MZERO THEN  /* IS FIX COUNT ZERO             */
         CH    @12,PFTFXCT(,PFTEPTR)                               0336
         BNE   @RT00336                                            0336
*             GOTO LBRFRR0A;        /* IF THE FIX COUNT IS NOT ZERO,
*                                      WE HAVE AN INTERNAL RSM ERROR */
*           ELSE                    /* FIX COUNT IS ZERO             */
*             DO;                   /* OTHERWISE, VSM IS NOTIFIED  0338
*                                      THAT THE VIRTUAL PAGE IS NOW
*                                      FREE                          */
*               PFTDFRLS='0'B;      /* RESET THE DEFERED RELEASE FLAG*/
         NI    PFTDFRLS(PFTEPTR),B'01111111'                       0339
*               RLPARMRG=WSA6VSA;   /* SET UP PARAMETER TO VSM       */
         L     RLPARMRG,WSA6VSA(,PVTPTR)                           0340
*               IF WSA6VBN<PVTLCSA THEN/* IS PAGE IN PRIVATE AREA    */
         CLC   WSA6VBN(2,PVTPTR),PVTLCSA(PVTPTR)                   0341
         BNL   @RF00341                                            0341
*                 DO;               /* FOR PRIVATE AREA PAGES, SET 0342
*                                      REG 0 TO ZERO                 */
*                   RLPARMR0=0;                                    0343
         SLR   RLPARMR0,RLPARMR0                                   0343
*                 END;                                             0344
*               ELSE                /* PAGE IN COMMON AREA           */
*                 DO;               /* FOR COMMON AREA PAGES, SET  0345
*                                      REGISTER 0 TO 1               */
         B     @RC00341                                            0345
@RF00341 DS    0H                                                  0346
*                   RLPARMR0=1;                                    0346
         LA    RLPARMR0,1                                          0346
*                 END;                                             0347
*               CALL MRELEASR;      /* CALL VSM AT A SPECIAL ENTRY --
*                                      SEE ENTRY DCL FOR INTERFACE   */
@RC00341 L     @15,@CV02454                                        0348
         BALR  @14,@15                                             0348
*             END;                  /* END VSM NOTIFICATION          */
*         END;                      /* END INPUT DEFERED RELEASE COND*/
*       ELSE                        /* NO PENDING DEFERED RELEASE    */
*         DO;                       /* TEST FOR A V=R ALLOCATED PAGE */
         B     @RC00334                                            0351
@RF00334 DS    0H                                                  0352
*           IF PFTVRALC='1'B THEN   /* IS PAGE V=R ALLOCATED         */
