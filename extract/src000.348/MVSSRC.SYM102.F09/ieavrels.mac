*   /*****************************************************************/
*   /*                                                               */
*   /* THE I/O ACTIVE Q PROCESSOR SUBROUTINE IS CALLED FROM THE Q    */
*   /* SEARCH SUBROUTINE OR THE IEAVRELF ENTRY POINT AND HAS NO SAVE */
*   /* AREA. INPUT IS THE ADDRESS OF A QUEUED PCB IN REGISTER 1, THE */
*   /* XPTE ADDRESS INREGISTER 6, AND THE STANDARD RSM REGISTERS 2   */
*   /* AND 3. IT MUST NOT DESTROY REGISTERS 2, 3, 4, 5, 6, 10, AND   */
*   /* 11.                                                           */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0824
*SUBIOACT:                                                         0824
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0824
SUBIOACT DS    0H                                                  0825
*   RESPECIFY                                                      0825
*     RLRETREG RESTRICTED;                                         0825
*   PCBPTR=RLPARMRG;                /* GET ADDR OF INPUT PCB         */
         LR    PCBPTR,RLPARMRG                                     0826
*   XPTPOINP='0'B;                  /* SET PAGE-OUT IN PROGRESS FLAG
*                                      OFF                   @Z40WPYD*/
         NI    XPTPOINP(XPTEPTR),B'11011111'                       0827
*   DO WHILE(PCBPTR^=MZERO);        /* THE FREE REAL FLAG IS SET AND
*                                      THE VBN SET TO ZERO IN ALL  0828
*                                      PCBS EXCEPT PCBS WITH NON ZERO
*                                      FIX COUNTS                    */
         B     @DE00828                                            0828
@DL00828 DS    0H                                                  0829
*     IF PCBFXC=MZERO THEN          /* IS PCB FOR A PGFIX            */
         CLI   PCBFXC(PCBPTR),0                                    0829
         BNE   @RF00829                                            0829
*       DO;                         /* NO                            */
*         PCBVBN=MZERO;             /* THE VBN IS ZEROED TO PREVENT
*                                      DUPLICATE PCB'S FROM APPEARING
*                                      ON AN I/O ACTIVE QUEUE.       */
         SLR   @12,@12                                             0831
         STH   @12,PCBVBN(,PCBPTR)                                 0831
*         PCBFREAL='1'B;            /* FRAME WILL BE FREED WHEN I/O
*                                      COMPLETES.                    */
         OI    PCBFREAL(PCBPTR),B'10000000'                        0832
*       END;                                                       0833
*     PCBPTR=PCBRLPA;               /* GET RELATED PTR IN CURRENT PCB*/
@RF00829 L     @12,PCBRLPA-1(,PCBPTR)                              0834
         LA    @12,0(,@12)                                         0834
         LR    PCBPTR,@12                                          0834
*   END;                            /* END DO WHILE                  */
@DE00828 LTR   PCBPTR,PCBPTR                                       0835
         BNZ   @DL00828                                            0835
*   END;                            /* END SUBIOACT PROC             */
@EL00008 DS    0H                                                  0836
@EF00008 DS    0H                                                  0836
@ER00008 BR    @14                                                 0836
