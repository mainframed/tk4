LBRFRR01 L     @14,RCAPTR(,PVTPTR)                                 0561
         MVI   RCARCRD(@14),X'01'                                  0561
*   GOTO LBRSMFRR;                  /* RECORD THE ERROR              */
         B     LBRSMFRR                                            0562
