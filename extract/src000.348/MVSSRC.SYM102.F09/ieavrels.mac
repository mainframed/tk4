*       CALL EPDSEG;                /* SEE ENTRY DCL FOR INTERFACE   */
         L     @15,PVTPDSEG(,PVTPTR)                               0550
         BALR  @14,@15                                             0550
*       RCACSECT=RCACPGRL;          /* RESET CSECT ID IN RCA         */
