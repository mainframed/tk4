*                                                                  0702
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS SEGMENT SEARCHES THE PCB Q SET UP IN THE FIRST SEGMENT   */
*   /* FOR PCBS WITH MATCHING VBNS AND ASCB ADDRESSES WHERE          */
*   /* NECESSARY. THE VBN TEST IS ALWAYS PERFORMED FIRST BECAUSE THIS*/
*   /* FIELD WILL BE ZERO IN ALREADY PURGED PCB'S AND BECAUSE IT IS  */
*   /* THE ONLY TEST NECESSARY FOR INPUT PAGES IN THE COMMON AREA.   */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0702
*LBPCBLOP:                          /* THIS IS THE BEGINNING OF LOOP */
*   IF PCBPTR^=MZERO THEN                                          0702
@RC00680 DS    0H                                                  0702
LBPCBLOP LTR   PCBPTR,PCBPTR                                       0702
         BZ    @RF00702                                            0702
*     DO;                           /* IF THE LAST PCB HAS NOT BEEN
*                                      TESTED, THE NEXT PCB IS TESTED*/
*       IF RLPARMRG=PCBVBN THEN     /* IS PCB FOR PAGE               */
         LH    @14,PCBVBN(,PCBPTR)                                 0704
         N     @14,@CF02622                                        0704
         CR    RLPARMRG,@14                                        0704
         BNE   @RF00704                                            0704
*         DO;                       /* IF THE VBNS MATCH, THE ASCB 0705
*                                      ADDRESSES ARE TESTED FOR    0705
*                                      PRIVATE AREA VBNS ONLY        */
*           IF RLPARMRG<PVTLCSA THEN/* IS PAGE IN PRIVATE AREA       */
         LH    @14,PVTLCSA(,PVTPTR)                                0706
         N     @14,@CF02622                                        0706
         CR    RLPARMRG,@14                                        0706
         BNL   @RF00706                                            0706
*             DO;                   /* YES                           */
*               IF RSMASCB^=PCBASCB THEN/* IS PCB FOR CURRENT ADDRESS
*                                      SPACE                         */
         CLC   RSMASCB(4,RSMHDPTR),PCBASCB(PCBPTR)                 0708
         BNE   @RT00708                                            0708
*                 GOTO LBNXTPCB;    /* NO, TEST NEXT PCB             */
*               ELSE                                               0710
*                 ;                 /* PCB IS FOR CURRENT A.S.       */
*             END;                  /* END ASCB TEST PROCESSING      */
*           ELSE                                                   0712
*             ;                     /* PAGE IS IN COMMON AREA        */
@RF00706 DS    0H                                                  0713
*           RLSAVPCB=PCBPTR;        /* SAVE ADDR OF FOUND PCB        */
         ST    PCBPTR,RLSAVPCB(,PVTPTR)                            0713
