*           ELSE                    /* PAGE NOT V=R ALLOCATED        */
*             IF PFTFXCT^=MZERO THEN/* IS PAGE STILL FIXED           */
         LH    @12,PFTFXCT(,PFTEPTR)                               0354
         LTR   @12,@12                                             0354
         BZ    @RF00354                                            0354
*               DO;                 /* A PAGE TO BE RELEASED IS STILL
*                                      PGFIXED, CREATING A DEFERED 0355
*                                      RELEASE CONDITION. IMMEDIATE
*                                      RETURN IS MADE TO THE CALLER  */
*                 PFTDFRLS='1'B;    /* SET DEFERED RELEASE FLAG      */
         OI    PFTDFRLS(PFTEPTR),B'10000000'                       0356
*                 RLRETCOD=MDFRLSCD;/* SET PROPER RETURN CODE        */
         LA    RLRETCOD,8                                          0357
*                 GOTO LBEXIT;      /* GO TO EXIT CODE               */
         B     LBEXIT                                              0358
*               END;                /* END DEFERED RELEASE CREATION  */
*                                                                  0360
*           /*********************************************************/
*           /*                                                       */
*           /* NORMAL RELEASE PROCESSING IS PERFORMED AT THIS POINT. */
*           /* THIS INCLUDES SQA, LSQA, PAGEABLE AND CREATE ADDRESS  */
*           /* SPACE PAGES. SQA AND LSQA PAGES ARE SEPARATED BY THE  */
*           /* ASID FIELD IN THE PFTE WHICH IS 'FFFF' FOR SQA. CREATE*/
*           /* A.S. PAGES ARE LSQA PAGES WITH AN ASID NOT EQUAL TO   */
*           /* THE CURRENT ASID AND A VBN NOT EQUAL TO THE VBN BEING */
*           /* PROCESSED.                                            */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0360
*             ELSE                                                 0360
*               DO;                                                0360
@RF00354 DS    0H                                                  0361
*                 IF PFTLSQA='1'B THEN/* TEST FOR AN SQA OR LSQA PAGE*/
         TM    PFTLSQA(PFTEPTR),B'00100000'                        0361
         BNO   @RF00361                                            0361
*                   DO;             /* SQA OR LSQA                   */
*                     IF PFTASID=MCOMASID THEN/* TEST FOR SQA        */
         CLC   PFTASID(2,PFTEPTR),@CH02428                         0363
         BNE   @RF00363                                            0363
*                       DO;                                        0364
*                                                                  0364
*                         /*******************************************/
*                         /*                                         */
*                         /* SQA RELEASE PROCESSING INCLUDES PAGE    */
*                         /* INVALID ATION AND MOVING OF THE PFTE TO */
*                         /* THE AFQ, BUT NO X PTE PROCESSING IS     */
*                         /* NECESSARY AS THERE ARE NONE.            */
*                         /*                                         */
*                         /*******************************************/
*                                                                  0365
*                         PFTLSQA='0'B;/* RESET THE SQA/LSQA FLAG    */
         NI    PFTLSQA(PFTEPTR),B'11011111'                        0365
*                         RLPARMRG=PTEPTR;/* CALL PAGE INVALIDATION
*                                      TO INVALIDATE THE PGTE AND  0366
*                                      PURGE THE HARDWARE TRANSLATION
*                                      TABLE                         */
         LR    RLPARMRG,PTEPTR                                     0366
*                         CALL EPPGINV;/* SEE ENTRY DCL FOR INTERFACE*/
         L     @15,PVTPINV(,PVTPTR)                                0367
         BALR  @14,@15                                             0367
*                         PGTRSA=RLCLRPGT;/* SET THE PGTE FOR THE  0368
*                                      INPUT PAGE TO ZERO, PRESERVING
*                                      THE INVALID FLAG ALREADY SET
