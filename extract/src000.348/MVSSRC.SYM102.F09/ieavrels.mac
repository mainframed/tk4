*       IF RLRETCOD^=MZERO THEN     /* IS RETURN CODE NON-ZERO       */
         LTR   RLRETCOD,RLRETCOD                                   0541
         BNZ   @RT00541                                            0541
*         GOTO LBRFRR01;            /* IF A NON-ZERO RETURN CODE IS
*                                      RETURNED, AN INTERNAL ERROR 0542
*                                      HAS OCCURRED                  */
*       ELSE                                                       0543
*         ;                         /* RETURN CODE WAS ZERO, CONTINUE*/
*       DO PTEPTR=RLPARMRG BY LENGTH(PGTPTE) TO(RLPARMRG+LENGTH(PGTPTE)
*             *MFIFTEEN);           /* TEST THE GETMAIN ASSIGNED FLAG
*                                      IN EACH PGTE UNTIL ONE IS   0544
*                                      FOUND. THIS LOOP DEPENDS ON 0544
*                                      TRV RETURNING A ZERO IN THE 0544
*                                      HIGH ORDER BYTE OF REG 1.     */
