*   SRBPARM=R11;                    /* SET PARM VALUE AS REQUESTED 0401
*                                                            @Z30GNYC*/
         ST    R11,SRBPARM(,SRBPTR)                                0401
*   GENERATE;                                                      0402
*                                /* SCHEDULE THE SRB        @Z30GNYC*/
        SCHEDULE SRB=(1),SCOPE=LOCAL
*   RETURN;                         /* RETURN TO CALLER      @Z30GNYC*/
