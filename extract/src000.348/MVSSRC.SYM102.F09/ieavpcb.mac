*   SRBASCB=ASCBPTR;                /* SET ASCB ADDRESS      @Z30GNYC*/
         ST    ASCBPTR,SRBASCB(,SRBPTR)                            0395
*   SRBEP=R0;                       /* SET ENTRY POINT       @Z30GNYC*/
         ST    R0,SRBEP(,SRBPTR)                                   0396
