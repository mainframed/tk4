         TITLE ' -- IEAVPCB: PCB MANAGER --                            *
                        '
*                                                                  0076
IEAVPCB  CSECT
         USING  *,15
         MODID  BR=NO
IEAVPCB2 DS     0H
         ENTRY  IEAVPCB2
         STM     R0,R14,PVTWSA13(PVTPTR)     SAVE REGISTERS
BASE1    BALR    R9,R0             ESTABLISH ENTRY POINT VALUE
         USING   *,R9
*/********************************************************************/
*/* THIS IS THE START OF THE MAIN BODY OF CODE FOR THE PCB MANAGER.  */
*/* IF R1 IS ZERO, THE BUILD OPTION IS SELECTED AND CONTROL TRANSFERS*/
*/* TO THE BUILD ROUTINE. IF R1 IS NOT ZERO, THE ENTRY FLAG IS  SET  */
*/* AND THE PCBPTR LOADED FOR THE ENQ/DEQ OPERATION.                 */
*/********************************************************************/
*                                                                  0077
*MGRCODE:                                                          0077
*   IF R1^=0 THEN                   /* IF PARAMETER REG1 EQUALS 0, 0077
*                                      THE BUILD OPTION HAS BEEN   0077
*                                      CHOSEN                        */
MGRCODE  LTR   R1,R1                                               0077
         BZ    @RF00077                                            0077
*     DO;                                                          0078
*       RCAPTR=PVTPRCA;             /* LOAD POINTER REGISTER         */
         L     RCAPTR,PVTPRCA(,PVTPTR)                             0079
*       RCAPCB='1'B;                /* SET ENTRY POINT FLAG IN RCA   */
         OI    RCAPCB(RCAPTR),B'00010000'                          0080
*       PCBPTR=R1;                  /* ESTABLISH POINTER FOR PCB     */
         LR    PCBPTR,R1                                           0081
*                                                                  0082
*/********************************************************************/
*/* DEQUEUE OPERATION - THE FOLLOWING CODE IS EXECUTED WHEN IEAVPCB  */
*/* IS ENTERED WITH A REQUEST FOR A DEQUEUE OPERATION, THAT IS WHEN  */
*/* THE PCB POINTER IS NOT ZERO AND THE PCBCQN IS NOT EQUAL TO       */
*/* PCBDENQ. THE QUEUE ON WHICH THE DEQUEUE IS TO BE PERFORMED IS    */
*/* DETERMINED BY COMPARING THE PCBCQN TO QUEUE NUMBERS. ONLY ONE PCB*/
*/* MAY BE DEQUEUED PER REQUEST. AFTER THE PCB HAS BEEN DEQUEUED, THE*/
*/* PCBCQN IS SET TO INDICATE THAT IT IS DEQUEUED (EQUAL TO PCBDENQ).*/
*/********************************************************************/
*                                                                  0082
*       IF PCBCQN^=PCBDEQN THEN     /* IF PCBCQN IS NOT EQUAL TO   0082
*                                      PCBDENQ, DEQUEUE IS REQUIRED  */
         CLI   PCBCQN(PCBPTR),255                                  0082
         BE    @RF00082                                            0082
*         DO;                       /* NOT EQUAL, DEQUEUE IS REQUIRED*/
*           IF PCBCQN<LOCALCON THEN /* IF CQN IS LESS THAN LOCAL   0084
*                                      QUEUE NUMBERS, IT IS A GLOBAL
*                                      QUEUE                         */
         CLI   PCBCQN(PCBPTR),128                                  0084
         BNL   @RF00084                                            0084
*             QFSTPTR=ADDR(PVTPCBQS)+PCBCQN;/* GLOBAL QUEUE, SET UP
*                                      POINTER               @G33VPOD*/
         LA    QFSTPTR,PVTPCBQS(,PVTPTR)                           0085
         SLR   @12,@12                                             0085
         IC    @12,PCBCQN(,PCBPTR)                                 0085
         ALR   QFSTPTR,@12                                         0085
*           ELSE                    /* LOCAL QUEUE, SET UP POINTER   */
*             QFSTPTR=ADDR(RSMQS)+(PCBCQN-LOCALCON);               0086
         B     @RC00084                                            0086
@RF00084 LA    QFSTPTR,RSMQS(,RSMHDPTR)                            0086
         SLR   @12,@12                                             0086
         IC    @12,PCBCQN(,PCBPTR)                                 0086
         SL    @12,@CF01689                                        0086
         ALR   QFSTPTR,@12                                         0086
*           IF PCBBQPA=0 THEN       /* IF 0, THIS IS FIRST PCB ON  0087
*                                      QUEUE                         */
@RC00084 L     @12,PCBBQPA(,PCBPTR)                                0087
         LTR   @12,@12                                             0087
         BNZ   @RF00087                                            0087
*             QFIRST=PCBFQPA;       /* FIRST PCB, SO ADJUST THE QUEUE
*                                      FIRST POINTER                 */
         L     @12,PCBFQPA-1(,PCBPTR)                              0088
         LA    @12,0(,@12)                                         0088
         ST    @12,QFIRST(,QFSTPTR)                                0088
*           ELSE                    /* IF NOT 0, THIS IS NOT FIRST 0089
*                                      PCB                           */
*             PCBBQPA->PCBFQPA=PCBFQPA;/* ADJUST PREVIOUS POINTER    */
         B     @RC00087                                            0089
@RF00087 L     @12,PCBBQPA(,PCBPTR)                                0089
         MVC   PCBFQPA(3,@12),PCBFQPA(PCBPTR)                      0089
*           IF PCBFQPA=0 THEN       /* IF 0 THIS IS THE LAST PCB ON
*                                      QUEUE                         */
@RC00087 L     @12,PCBFQPA-1(,PCBPTR)                              0090
         LA    @12,0(,@12)                                         0090
         LTR   @12,@12                                             0090
         BNZ   @RF00090                                            0090
*             QLAST=PCBBQPA;        /* LAST PCB, SO ADJUST LAST    0091
*                                      POINTER                       */
         L     @12,PCBBQPA(,PCBPTR)                                0091
         ST    @12,QLAST(,QFSTPTR)                                 0091
*           ELSE                    /* IF NOT 0, THIS PCB IS NOT LAST*/
*             PCBFQPA->PCBBQPA=PCBBQPA;/* NOT LAST PCB, SO ADJUST  0092
*                                      BACK POINTER IN NEXT PCB      */
         B     @RC00090                                            0092
@RF00090 L     @12,PCBFQPA-1(,PCBPTR)                              0092
         L     @08,PCBBQPA(,PCBPTR)                                0092
         ST    @08,PCBBQPA(,@12)                                   0092
*           PCBCQN=PCBDEQN;         /* SET PCBCQN TO INDICATE NOT ON
*                                      A QUEUE                       */
@RC00090 MVI   PCBCQN(PCBPTR),X'FF'                                0093
*           PCBFQPA=0;              /* ZERO THE FORWARD POINTER      */
         SLR   @12,@12                                             0094
         STCM  @12,7,PCBFQPA(PCBPTR)                               0094
*           PCBBQPA=0;              /* ZERO THE BACKWARD POINTER     */
         ST    @12,PCBBQPA(,PCBPTR)                                0095
*         END;                                                     0096
*                                                                  0097
*/********************************************************************/
*/* ENQUEUE OPERATION - THE FOLLOWING CODE IS EXECUTED WHEN IEAVPCB  */
*/* IS ENTERED WITH A ENQUEUE REQUEST, THAT IS THE PCBPTR IS NOT ZERO*/
*/* AND MGRTQN IS NOT EQUAL TO PCBDENQ. THE QUEUE ON WHICH THE PCB IS*/
*/* TO BE ENQUEUED IS DETERMINED BY COMPARING THE MGRTQN TO THE QUEUE*/
*/* NUMBERS. IF MORE THAN ONE PCB IS TO BE ENQUEUED, IT IS ASSUMED   */
*/* THAT THE CALLER HAS MADE SURE THAT THE CHAIN IS NOT BROKEN, AND  */
*/* AND THAT THE END IS MARKED BY A ZERO FORWARD POINTER. THE PCBCQN */
*/* IS SET TO THE VALUE OF THE QUEUE NUMBER WHEN IT IS ENQUEUED, AND */
*/* IF THE ENQUEUE IS TO THE FREE QUEUE THE FREE QUEUE DEPTH IS      */
*/* ADJUSTED IN THE PVT.                                             */
*/********************************************************************/
*                                                                  0097
*       IF MGRTQN^=PCBDEQN THEN     /* IF NOT EQUAL, ENQUEUE       0097
*                                      REQUESTED                     */
@RF00082 CLI   MGRTQN(PVTPTR),255                                  0097
         BE    @RF00097                                            0097
*         DO;                       /* NOT EQUAL, EXECUTE ENQUEUE  0098
*                                      CODE                          */
*           IF MGRTQN<LOCALCON THEN /* IF LESS THEN LOCAL QUEUE    0099
*                                      NUMBER IT IS A GLOBAL QUEUE   */
         CLI   MGRTQN(PVTPTR),128                                  0099
         BNL   @RF00099                                            0099
*             QFSTPTR=ADDR(PVTPCBQS)+MGRTQN;/* LOAD ADDRESS OF QUEUE
*                                      ANCHOR OF GLOBAL QUEUE      0100
*                                                            @G33VPOD*/
         LA    QFSTPTR,PVTPCBQS(,PVTPTR)                           0100
         SLR   @12,@12                                             0100
         IC    @12,MGRTQN(,PVTPTR)                                 0100
         ALR   QFSTPTR,@12                                         0100
*           ELSE                    /* NOT LESS MEANS LOCAL QUEUE    */
*             QFSTPTR=ADDR(RSMQS)+(MGRTQN-LOCALCON);/* LOAD ADDRESS
*                                      OF QUEUE ANCHOR OF LOCAL QUE  */
         B     @RC00099                                            0101
@RF00099 LA    QFSTPTR,RSMQS(,RSMHDPTR)                            0101
         SLR   @12,@12                                             0101
         IC    @12,MGRTQN(,PVTPTR)                                 0101
         SL    @12,@CF01689                                        0101
         ALR   QFSTPTR,@12                                         0101
*           IF QLAST=0 THEN         /* IF ZERO THE QUEUE IS EMPTY    */
@RC00099 SLR   @12,@12                                             0102
         C     @12,QLAST(,QFSTPTR)                                 0102
         BNE   @RF00102                                            0102
*             DO;                   /* EMPTY QUEUE                   */
*               QFIRST=PCBPTR;      /* FILL ANCHOR FIRST POINTER     */
         ST    PCBPTR,QFIRST(,QFSTPTR)                             0104
*               PCBBQPA=0;          /* SET PCB BACKWARD PTR TO 0     */
         ST    @12,PCBBQPA(,PCBPTR)                                0105
*             END;                                                 0106
*           ELSE                    /* IF NOT 0, QUEUE IS NOT EMPTY  */
*             DO;                                                  0107
         B     @RC00102                                            0107
@RF00102 DS    0H                                                  0108
*               QLAST->PCBFQPA=PCBPTR;/* NOT EMPTY SO SET FORWARD  0108
*                                      POINTER OF LAST CURRENT PCB TO
*                                      POINT TO PCB BEING ADDED      */
         L     @12,QLAST(,QFSTPTR)                                 0108
         STCM  PCBPTR,7,PCBFQPA(@12)                               0108
*               PCBBQPA=QLAST;      /* SET NEW PCB BACKWARD POINTER
*                                      TO POINT TO PRECEEDING PCB    */
         ST    @12,PCBBQPA(,PCBPTR)                                0109
*             END;                                                 0110
*MGRENQLP:                                                         0111
*           PCBCQN=MGRTQN;          /* SET CQN TO CURRENT QUEUE    0111
*                                      NUMBER THAT PCB IS TAGGED TO  */
@RC00102 DS    0H                                                  0111
MGRENQLP IC    @12,MGRTQN(,PVTPTR)                                 0111
         STC   @12,PCBCQN(,PCBPTR)                                 0111
*           IF MGRTQN=PCBFREQN THEN /* IF EQUAL, PCB HAS BEEN PUT ON
*                                      THE FREE QUEUE                */
         CLI   MGRTQN(PVTPTR),16                                   0112
         BNE   @RF00112                                            0112
*             DO;                                                  0113
*CHAINFRQ:                          /* BEGINNING OF LOOP PLACING   0114
*                                      PCB'S ON THE FREE QUEUE.    0114
*                                                            @Z40WPYD*/
*                                                                  0114
*               /*****************************************************/
*               /*                                                   */
*               /* THE FOLLOWING SETS THE TRACE CSECT ID, IN THE PCB,*/
*               /* OF THE MODULE WHICH IS FREEING THE PCB. WHEN THE  */
*               /* TRACE IS ACTIVE, THE BRANCH INSTRUCTION MUST BE   */
*               /* NOPED.                                    @Z40WPYD*/
*               /*                                                   */
*               /*****************************************************/
*                                                                  0114
*TRACEZP3:                                                         0114
*               GENERATE;                                          0114
CHAINFRQ DS    0H                                                  0114
TRACEZP3 DS    0H                                                  0114
         B   TRACEZP3+10  NOPED WHEN TRACE ACTIVE
         MVC 65(1,PCBPTR),RCACSECT(RCAPTR)     MOVE IN CSECT ID OF
*                                              MODULE FREEING PCB
*               PVTPCBCT=PVTPCBCT+1;/* UPDATE FREE QUEUE COUNT       */
         LA    @12,1                                               0115
         AH    @12,PVTPCBCT(,PVTPTR)                               0115
         STH   @12,PVTPCBCT(,PVTPTR)                               0115
*               IF PCBFQPA=0 THEN   /* IF 0 THIS IS LAST PCB ON CHAIN*/
         L     @12,PCBFQPA-1(,PCBPTR)                              0116
         LA    @12,0(,@12)                                         0116
         LTR   @12,@12                                             0116
         BNZ   @RF00116                                            0116
*                 QLAST=PCBPTR;     /* SET LAST POINTER IN ANCHOR TO
*                                      POINT TO LAST PCB             */
         ST    PCBPTR,QLAST(,QFSTPTR)                              0117
*               ELSE                /* IF NOT 0, THIS IS NOT THE LAST
*                                      PCB ON CHAIN BEING ENQUEUED   */
*                 DO;                                              0118
         B     @RC00116                                            0118
@RF00116 DS    0H                                                  0119
*                   PCBPTR=PCBFQPA; /* UPDATE PCBPTR TO NEXT ON CHAIN
*                                                            @Z40WPYD*/
         L     @12,PCBFQPA-1(,PCBPTR)                              0119
         LA    @12,0(,@12)                                         0119
         LR    PCBPTR,@12                                          0119
*                   PCBCQN=MGRTQN;  /* SET CQN TO CURRENT QUEUE, FREE
*                                      QUEUE                 @Z40WPYD*/
         IC    @12,MGRTQN(,PVTPTR)                                 0120
         STC   @12,PCBCQN(,PCBPTR)                                 0120
*                   GOTO CHAINFRQ;  /* LOOP TO PROCESS EACH PCB ON 0121
*                                      THE FREE QUEUE        @Z40WPYD*/
         B     CHAINFRQ                                            0121
*                 END;                                             0122
*             END;                                                 0123
*           ELSE                                                   0124
*             DO;                                                  0124
         B     @RC00112                                            0124
@RF00112 DS    0H                                                  0125
*               IF PCBFQPA=0 THEN                                  0125
         L     @12,PCBFQPA-1(,PCBPTR)                              0125
         LA    @12,0(,@12)                                         0125
         LTR   @12,@12                                             0125
         BNZ   @RF00125                                            0125
*                 QLAST=PCBPTR;     /* IF ZERO, THIS IS LAST PCB ON
*                                      CHAIN                 @Z40WPYD*/
         ST    PCBPTR,QLAST(,QFSTPTR)                              0126
*               ELSE                                               0127
*                 DO;                                              0127
         B     @RC00125                                            0127
@RF00125 DS    0H                                                  0128
*                   PCBPTR=PCBFQPA; /* UPDATE PCBPTR TO NEXT ON CHAIN*/
         L     @12,PCBFQPA-1(,PCBPTR)                              0128
         LA    @12,0(,@12)                                         0128
         LR    PCBPTR,@12                                          0128
*                   GOTO MGRENQLP;  /* LOOP TO PROCESS EACH PCB ON 0129
*                                      THE CHAIN BEING ENQUEUED      */
         B     MGRENQLP                                            0129
*                 END;                                             0130
*             END;                                                 0131
@RC00125 DS    0H                                                  0132
*         END;                                                     0132
@RC00112 DS    0H                                                  0133
*                                                                  0133
*/********************************************************************/
*/* CODE COMMON TO ENQUEUE/DEQUEUE - IF EITHER THE ENQUEUE OPTION OR */
*/* THE DEQUEUE OPTION WERE REQUESTED THIS CODE WILL BE EXECUTED.    */
*/* IT MERELY SETS THE RETURN CODE TO ZERO AND THEN PASSES CONTROL   */
*/* TO THE RETURN PREPERATION ROUTINE VIA A BRANCH.                  */
*/********************************************************************/
*                                                                  0133
*       RETCODR=0;                  /* RETURN CODE IS ZERO AT THIS 0133
*                                      POINT IF ANY COMBINATION OF 0133
*                                      ENQ/DEQ FUNCTIONS WAS       0133
*                                      PERFORMED                     */
@RF00097 SLR   RETCODR,RETCODR                                     0133
*       GOTO MGRRTNCD;              /* AFTER ENQUEUE AND DEQUEUE   0134
*                                      DECISIONS ARE MADE, GO TO CODE
*                                      TO RESTORE REGISTERS AND    0134
*                                      RETURN TO CALLER              */
         B     MGRRTNCD                                            0134
*     END;                                                         0135
*                                                                  0136
*/********************************************************************/
*/* BUILD ROUTINE - THIS CODE MAKES UP THE BUILD ROUTINE. CONTROL IS */
*/* PASSED TO THIS POINT IF UPON ENTRY TO IEAVPCB THE PCBPTR IS ZERO */
*/* SPECIFYING THE BUILD OPTION. AN IMMEDIATE CHECK IS MADE TO SEE   */
*/* IF THE BUILD REQUEST IS FOR ZERO (0) PCBS. IF IT IS A ZERO       */
*/* REQUEST, CONTROL IMMEDIATELY TRANSFERS TO CODE FOR THAT PURPOSE. */
*/* IF IT IS NOT A ZERO REQUEST, AND THE NUMBER OF PCBS ON THE FREE  */
*/* QUEUE IS SUFFICIENT TO SATISFY THE REQUEST, THE CHAIN IS BUILT.  */
*/* IF ENOUGH PCBS ARE NOT AVAILABLE, IEAVPCB2 WILL BE CALLED TO     */
*/* CREATE MORE PCBS BEFORE THIS BUILD TAKES PLACE.                  */
*/*   THE PCBPTR AND MGRWD IN THE SAVE AREA ARE SET TO POINT TO THE  */
*/* FIRST PCB ON THE CHAIN BEING BUILT. SETTING THE MGRWD PROVIDES   */
*/* THE CALLER WITH THE CHAIN POINTER UPON RETURN. THE PCBCQN OF ALL */
*/* PCBS ON THE CHAIN ARE SET TO INDICATE DEQUEUED, AND THE PCBS ARE */
*/* CLEARED WITH THE EXCEPTION OF THE FORWARD AND BACKWARD POINTERS. */
*/* THE RETURN CODE IS SET TO ZERO. AFTER THE CHAIN HAS BEEN BUILT   */
*/* THE FREE QUEUE IS CHECKED FOR MINIMUM DEPTH LEVEL. IF PCBS ARE   */
*/* REQUIRED, THE REPLENISH ROUTINE IS CALLED IF POSSIBLE, AND THE   */
*/* RETURN PREPERATION ROUTINE IS GIVEN CONTROL VIA A BRANCH.        */
*/********************************************************************/
*                                                                  0136
*MGRBLD:                                                           0136
*   IF MGRQTY^=0 THEN               /* IF THIS IS NOT A ZERO BUILD 0136
*                                      REQ                           */
@RF00077 DS    0H                                                  0136
MGRBLD   L     @12,MGRQTY(,PVTPTR)                                 0136
         LTR   @12,@12                                             0136
         BZ    @RF00136                                            0136
*     DO;                                                          0137
*       RCAPTR=PVTPRCA;             /* LOAD POINTER REGISTER         */
         L     RCAPTR,PVTPRCA(,PVTPTR)                             0138
*       RCAPCB='1'B;                /* SET THE ENTERED FLAG          */
         OI    RCAPCB(RCAPTR),B'00010000'                          0139
*       IF PVTPCBCT^<MGRQTY THEN    /* IF FREE QUEUE HAS ENOUGH PCBS
*                                      BUILD THE CHAIN               */
         CH    @12,PVTPCBCT(,PVTPTR)                               0140
         BH    @RF00140                                            0140
*         DO;                                                      0141
*MGRBLD2:                                                          0142
*           PCBPTR=PVTFPCBF;        /* SET PCBPTR TO PCB ADDRESS     */
MGRBLD2  L     PCBPTR,PVTFPCBF(,PVTPTR)                            0142
*           MGRWD=PCBPTR;           /* PCB ADDRESS WILL BE PASSED TO
*                                      THE CALLER AS A PARAMETER. IT
*                                      MUST POINT TO THE PCB CHAIN   */
         ST    PCBPTR,MGRWD(,PVTPTR)                               0143
*           TEMPCTR=MGRQTY;         /* INITIALIZE COUNTER TO NUMBER
*                                      OF PCB'S TO BE PUT ON THE   0144
*                                      CHAIN                         */
         L     TEMPCTR,MGRQTY(,PVTPTR)                             0144
*MGRCHBLD:                                                         0145
*           PCBCQN=PCBDEQN;         /* PCB IS DEQUEUED NOW SO THE CQN
*                                      CODE MUST INDICATE SUCH       */
*                                                                  0145
MGRCHBLD MVI   PCBCQN(PCBPTR),X'FF'                                0145
*           /*********************************************************/
*           /*                                                       */
*           /* WHEN TRACE IS ACTIVE, THE INSTRUCTION ZEROING THE PCB */
*           /* AFTER THE QUEUE POINTERS MUST BE ZAPPED TO CLEAR NEW  */
*           /* PCB SIZE                                      @Z40WPYD*/
*           /*                                                       */
*           /*********************************************************/
*                                                                  0146
*TRACEZP4:                                                         0146
*           MGRCLEAR(9:LENGTH(PCB))=''B;/* ZERO PCB AFTER QUEUE    0146
*                                      POINTERS                      */
TRACEZP4 XC    MGRCLEAR+8(56,PCBPTR),MGRCLEAR+8(PCBPTR)            0146
*TRACEZP2:                                                         0147
*           GENERATE;                                              0147
TRACEZP2 DS    0H                                                  0147
         B   TRACEZP2+10                    NOPED WHEN TRACE ACTIVE
         MVC 64(1,PCBPTR),RCACSECT(RCAPTR)  MOVE IN CSECT ID OF MODULE
*                                           REQUESTING PCB BUILD
*           TEMPCTR=TEMPCTR-1;      /* PCB HAS BEEN ADDED TO THE   0148
*                                      CHAIN                         */
         BCTR  TEMPCTR,0                                           0148
*           IF TEMPCTR^=0 THEN      /* IF NOT ZERO, CHAIN IS       0149
*                                      INCOMPLETE                    */
         LTR   TEMPCTR,TEMPCTR                                     0149
         BZ    @RF00149                                            0149
*             DO;                                                  0150
*               PCBPTR=PCBFQPA;     /* POINT TO NEXT PCB ON CHAIN    */
         L     @12,PCBFQPA-1(,PCBPTR)                              0151
         LA    @12,0(,@12)                                         0151
         LR    PCBPTR,@12                                          0151
*               GOTO MGRCHBLD;      /* BUILD ANOTHER PCB ON THE CHAIN*/
         B     MGRCHBLD                                            0152
*             END;                                                 0153
*           PVTPCBCT=PVTPCBCT-MGRQTY;/* TAKE NUMBER OF PCBS REMOVED
*                                      FROM THE FREE QUEUE OFF IT'S
*                                      COUNT                         */
@RF00149 LH    @12,PVTPCBCT(,PVTPTR)                               0154
         SL    @12,MGRQTY(,PVTPTR)                                 0154
         STH   @12,PVTPCBCT(,PVTPTR)                               0154
*           PVTFPCBF=PCBFQPA;       /* SET FREE QUEUE FIRST POINTER
*                                      TO POINT TO THE NEW FIRST PCB */
         L     @12,PCBFQPA-1(,PCBPTR)                              0155
         LA    @12,0(,@12)                                         0155
         ST    @12,PVTFPCBF(,PVTPTR)                               0155
*           IF PVTFPCBF=0 THEN      /* IF FREE QUEUE INDICATES EMPTY */
         SLR   @08,@08                                             0156
         CR    @12,@08                                             0156
         BNE   @RF00156                                            0156
*             PVTFPCBL=0;           /* EMPTY QUEUE SO LAST POINTER =
*                                      0                             */
         ST    @08,PVTFPCBL(,PVTPTR)                               0157
*           ELSE                    /* IF FREE QUEUE NOT EMPTY       */
*             PCBFQPA->PCBBQPA=0;   /* QUEUE IS NOT EMPTY SO FIRST 0158
*                                      PCB ON CHAIN MUST HAVE A ZERO
*                                      BACK PTR                      */
         B     @RC00156                                            0158
@RF00156 L     @12,PCBFQPA-1(,PCBPTR)                              0158
         SLR   @08,@08                                             0158
         ST    @08,PCBBQPA(,@12)                                   0158
*           PCBFQPA=0;              /* SET FORWARD POINTER OF LAST 0159
*                                      PCB ON CHAIN TO ZERO          */
@RC00156 SLR   @12,@12                                             0159
         STCM  @12,7,PCBFQPA(PCBPTR)                               0159
*           RETCODR=0;              /* SUCCESSFUL OPERATION CODE     */
         SLR   RETCODR,RETCODR                                     0160
*           IF PVTPCBCT^<PVTPCBLO THEN/* IF REMAINING PCB COUNT IS 0161
*                                      ABOVE MINIMUM, RETURN TO    0161
*                                      CALLER                        */
         LH    @12,PVTPCBCT(,PVTPTR)                               0161
         CH    @12,PVTPCBLO(,PVTPTR)                               0161
         BNL   @RT00161                                            0161
*             GOTO MGRRTNCD;        /* GO TO RETURN ROUTINE          */
*           IF PVTBGMS='0'B THEN    /* IF GETMAIN IS NOT INHIBITED   */
         TM    PVTBGMS(PVTPTR),B'01000000'                         0163
         BNZ   @RF00163                                            0163
*             DO;                                                  0164
*               QTYRQDR=PVTPCBNO;   /* LOAD THE NUMBER OF PCBS TO BE
*                                      AQUIRED                       */
         SLR   QTYRQDR,QTYRQDR                                     0165
         IC    QTYRQDR,PVTPCBNO(,PVTPTR)                           0165
*               CALL IEAVPCB3;      /* CALL INTERNAL PROC TO GET   0166
*                                      PCB'S                         */
         BAL   @14,IEAVPCB3                                        0166
*               RETCODR=0;          /* CALLERS REQUEST WAS SATISFIED,
*                                      SO FORCE RETURN CODE TO ZERO
*                                      REGARDLESS OF OUTCOME OF    0167
*                                      REPLENISH                     */
         SLR   RETCODR,RETCODR                                     0167
*             END;                                                 0168
*           GOTO MGRRTNCD;          /* GO TO RETURN ROUTINE          */
         B     MGRRTNCD                                            0169
*         END;                                                     0170
*                                                                  0171
*/********************************************************************/
*/* THIS CODE WILL BE EXECUTED WHEN THE BUILD OPTION IS SELECTED AND */
*/* THE FREE QUEUE DOES NOT HAVE ENOUGH PCB'S ON THE CHAIN. IF       */
*/* GETMAIN IS NOT INHIBITED, THE NUMBER OF PCBS NEEDED TO FILL THE  */
*/* REQUEST IS ADDED TO THE NUMBER OF PCBS NORMALLY ACQUIRED DURING  */
*/* A REPLENISH, AND THE REPLENISH ROUTINE IS CALLED. IF GETMAIN IS  */
*/* INHIBITED A RETURN CODE OF FOUR IS SET.                          */
*/********************************************************************/
*                                                                  0171
*       ELSE                                                       0171
*         IF PVTBGMS^='1'B THEN     /* GET MORE SPACE IF GETMAINS ARE
*                                      NOT INHIBITED                 */
@RF00140 TM    PVTBGMS(PVTPTR),B'01000000'                         0171
         BO    @RF00171                                            0171
*           DO;                                                    0172
*             QTYRQDR=PVTPCBNO+(MGRQTY-PVTPCBCT);/* SET NUMBER OF  0173
*                                      PCBS TO BE ACQUIRED           */
         L     QTYRQDR,MGRQTY(,PVTPTR)                             0173
         SH    QTYRQDR,PVTPCBCT(,PVTPTR)                           0173
         SLR   @12,@12                                             0173
         IC    @12,PVTPCBNO(,PVTPTR)                               0173
         ALR   QTYRQDR,@12                                         0173
*             CALL IEAVPCB3;        /* CALL REPLENISH ROUTINE        */
         BAL   @14,IEAVPCB3                                        0174
*             IF RETCODR=0 THEN     /* IF REPLENISH WAS SUCCESSFUL   */
         LTR   RETCODR,RETCODR                                     0175
         BZ    @RT00175                                            0175
*               GOTO MGRBLD2;       /* SHOULD HAVE ENOUGH PCB'S NOW,
*                                      SO GO BACK TO BUILD ROUTINE   */
*             GOTO MGRRTNCD;        /* PREPARE TO RETURN TO CALLER   */
         B     MGRRTNCD                                            0177
*           END;                                                   0178
*       RETCODR=4;                  /* RETURN CODE 4, GM INHIBITED   */
@RF00171 LA    RETCODR,4                                           0179
*       GOTO MGRRTNCD;              /* PREPARE TO RETURN TO CALLER   */
         B     MGRRTNCD                                            0180
*     END;                                                         0181
*                                                                  0182
*/********************************************************************/
*/* THIS CODE IS EXECUTED ONLY ON A BUILD REQUEST FOR ZERO PCBS. A   */
*/* CHECK IS MADE TO SEE IF IT IS A REQUEST FROM NIP. IF SO, THE     */
*/* CALLS TO IEAVPCB2 AND IEAVREP3 ARE MADE,                         */
*/* OTHERWISE A RETURN CODE OF FOUR IS                               */
*/* SET. IF IEAVPCB2 RETURNS A CODE 0F 4,                            */
*/* THE PCBS FOR NIP FLAG IS NOT SET AND THE RETURN CODE             */
*/*  IS PASSED TO THE CALLER OF THE PCB MANAGER                      */
*/* IF THE RETURN CODE IS ZERO THE PVTPCBLT FLAG IS SET.             */
*/********************************************************************/
*                                                                  0182
*   IF PVTPCBLT='0'B THEN           /* IF THIS IS A NIP BUILD REQ    */
@RF00136 TM    PVTPCBLT(PVTPTR),B'00010000'                        0182
         BNZ   @RF00182                                            0182
*     DO;                                                          0183
*       QTYRQDR=PVTPCBS;            /* LOAD NIP QUANTITY INTO      0184
*                                      REGISTER                      */
         SLR   QTYRQDR,QTYRQDR                                     0184
         IC    QTYRQDR,PVTPCBS(,PVTPTR)                            0184
*       CALL IEAVPCB3;              /* CALL PCB REPLENISH ROUTINE    */
         BAL   @14,IEAVPCB3                                        0185
*       IF RETCODR=0 THEN           /* IF THE BUILD WAS SUCCESSFUL   */
         LTR   RETCODR,RETCODR                                     0186
         BNZ   @RF00186                                            0186
*         DO;                                                      0187
*           CALL IEAVREP3;          /* CALL SRB REPLENISH ROUTINE  0188
*                                                            @Z30GNYC*/
         L     @15,@CA01698                                        0188
         BALR  @14,@15                                             0188
*           IF RETCODR=0 THEN       /* WAS REPLENISH SUCCESFUL     0189
*                                                            @Z30GNYC*/
         LTR   RETCODR,RETCODR                                     0189
         BNZ   @RF00189                                            0189
*             DO;                   /* YES                   @Z30GNYC*/
*               PVTPCBLT='1'B;      /* TURN ON PCBS FOR NIP  @Z30GNYC*/
         OI    PVTPCBLT(PVTPTR),B'00010000'                        0191
*               GOTO RTNNIP;        /* DO NOT ALLOW RCA ACCESSING  0192
*                                                            @Z30GNYC*/
         B     RTNNIP                                              0192
*             END;                                                 0193
*           ELSE                                                   0194
*             ;                     /* REPLENISH UNSUCCESSFUL      0194
*                                                            @Z30GNYC*/
@RF00189 DS    0H                                                  0195
*         END;                                                     0195
*     END;                                                         0196
@RF00186 DS    0H                                                  0197
*   RETCODR=4;                      /* IF NOT A REQUEST FROM NIP,  0197
*                                      SIGNAL AN ERROR               */
@RF00182 LA    RETCODR,4                                           0197
*   GOTO RTNNIP;                    /* NO RCA POINTER, NO FLAGS SET  */
         B     RTNNIP                                              0198
*/********************************************************************/
*/* RETURN PREPERATION - THIS IS MERELY A COMMON RETURN ROUTINE FOR  */
*/* ALL OF THE ROUTINES OF IEAVPCB. NIP REQUESTS DO NOT RESET RCA.   */
*/********************************************************************/
*                                                                  0199
*MGRRTNCD:                                                         0199
*   RCAPCB='0'B;                    /* TURN OFF IN USE FLAG          */
MGRRTNCD NI    RCAPCB(RCAPTR),B'11101111'                          0199
*RTNNIP:                            /* LABEL FOR NIP TIME EXIT       */
*   GENERATE;                                                      0200
RTNNIP   DS    0H                                                  0200
        LM    R0,R14,PVTWSA13(PVTPTR)
        BR    R14                  RETURN TO CALLER
*                                                                  0201
*/********************************************************************/
*/* REPLENISH CODE PROCEDURE--                                       */
*/* THIS CODE CALCULATES THE AMOUNT OF CORE NEEDED TO BUILD PCBS     */
*/* THIS VALUE IS THEN USED TO PRIME THE GETMAIN MACRO. WHEN         */
*/* THE MACRO RETURNS THE CONDITION CODE IS CHECKED TO SEE IF THE    */
*/* OPERATION WAS SUCCESSFUL. IF UNSUCCESSFUL, THE CODE WILL TRY TO  */
*/* ACQUIRE A LESSER NUMBER OF PCBS. IF UNABLE TO ASK FOR LESS, OR   */
*/* IF GETMAIN IS STILL UNSUCCESSFUL, RETURN IS MADE TO THE CALLER   */
*/* WITH A RETURN CODE OF FOUR. IF THE GETMAIN WAS SUCCESSFUL,       */
*/* THE CORE IS FORMATTED AND THE PCBS CHAINED TO THE FREE QUEUE.    */
*/********************************************************************/
*                                                                  0201
*IEAVPCB3:                                                         0201
*   PROC OPTIONS(NOSAVE,NOSAVEAREA,RETREG);                        0201
         B     @PB00002                                            0201
IEAVPCB3 DS    0H                                                  0202
*   MGRRRTN=R14;                    /* BACK UP THE RETURN REGISTER   */
         ST    R14,MGRRRTN(,PVTPTR)                                0202
*   RFY                                                            0203
*    (R14) UNRSTD;                                                 0203
*   FGMFAILF='0'B;                  /* INIT GETMAIN FAIL FLAG TO OFF
*                                                            @YM08254*/
*                                                                  0204
         NI    FGMFAILF(PVTPTR),B'01111111'                        0204
*   /*****************************************************************/
*   /*                                                               */
*   /* WHEN TRACE IS ACTIVE, THE CONSTANT LABELLED PCBTRACE MUST BE  */
*   /* ZAPPED TO THE LARGER PCB SIZE                         @Z40WPYD*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0205
*REPLCALC:                                                         0205
*   GENERATE;                                                      0205
REPLCALC DS    0H                                                  0205
         LR    CORELNR,QTYRQDR    TOTAL AMOUNT OF CORE EQUALS THE
         MH    CORELNR,PCBTRACE   QUANTITY DESIRED TIMES PCB LENGTH
*   R5=PVTPTR;                      /* SAVE PVTPTR ACROSS MACRO      */
         LR    R5,PVTPTR                                           0206
*/********************************************************************/
*/* THE GETMAIN MACRO REFERANCES CORELENR AS REG 0 AND RETURNS THE   */
*/* STARTING ADDRESS OF THE CORE BLOCK IN REG 1(BEGINPTR). REG 1     */
*/* IS SUBSEQUENTLY USED BY BLDCPOOL AND MUST NOT BE CHANGED PRIOR   */
*/* TO THAT MACRO.                                                   */
*/********************************************************************/
*                                                                  0207
*   PVTRSMGM='1'B;                  /* INDICATE RSM GETMAIN TO PREF
*                                                            @ZA16816*/
         OI    PVTRSMGM(PVTPTR),B'10000000'                        0207
*   RFY                                                            0208
*     BEGINPTR RSTD;                                               0208
*   GENERATE NODEFS REFS(R0) SETS(R1,R3,R4,R15) NOFLOWS;           0209
SELF     GETMAIN RC,LV=(0),BNDRY=DBLWD,BRANCH=(YES,GLOBAL),SP=245,    **
               RELATED=(RSM,IEAVPCB(SELF)'CORE IS FREED ON NEXT IPL')
*   PVTPTR=R5;                      /* RESTORE PVTPTR AFTER MACRO    */
         LR    PVTPTR,R5                                           0210
*   PVTRSMGM='0'B;                  /* RESET RSM GETMAIN INDICATOR 0211
*                                                            @ZA16816*/
         NI    PVTRSMGM(PVTPTR),B'01111111'                        0211
*   PVTPRCA=RCAPTR;                 /* RESTORE PVTPRCA AFTER MACRO --
*                                      NOTE -- WHEN THIS MODULE IS 0212
*                                      CALLED BY NIP, THE VALUE    0212
*                                      STORED IN THIS FIELD IS     0212
*                                      IRRELEVANT, AS THE RCA HAS NOT
*                                      YET BEEN ESTABLISHED          */
         ST    RCAPTR,PVTPRCA(,PVTPTR)                             0212
*   IF R15^=0 THEN                  /* IF RETURN CODE FROM GETMAIN IS
*                                      NOT ZERO                      */
         LTR   R15,R15                                             0213
         BZ    @RF00213                                            0213
*     DO;                                                          0214
*       IF PVTPCBLT='0'B×FGMFAILF='1'B×QTYRQDR^>PVTPCBNO THEN/* IF A
*                                      NIP REQUEST OR THIS IS THE  0215
*                                      SECOND GETMAIN TO FAIL OR THE
*                                      QUANTITY REQUESTED IS NOT   0215
*                                      GREATER THEN THE NORMAL     0215
*                                      REQUEST NUMBER, GIVE AN ERROR
*                                      RETURN                @YM08254*/
         TM    PVTPCBLT(PVTPTR),B'00010000'                        0215
         BZ    @RT00215                                            0215
         TM    FGMFAILF(PVTPTR),B'10000000'                        0215
         BO    @RT00215                                            0215
         SLR   @14,@14                                             0215
         IC    @14,PVTPCBNO(,PVTPTR)                               0215
         CR    QTYRQDR,@14                                         0215
         BH    @RF00215                                            0215
@RT00215 DS    0H                                                  0216
*         DO;                                                      0216
*           RFY                                                    0217
*            (R14) RSTD;                                           0217
*           RETCODR=4;              /* SET THE RETURN CODE           */
         LA    RETCODR,4                                           0218
*           R14=MGRRRTN;            /* RESTORE RETURN REGISTER       */
         L     R14,MGRRRTN(,PVTPTR)                                0219
*           RETURN;                 /* RETURN TO CALLER              */
@EL00002 DS    0H                                                  0220
@EF00002 DS    0H                                                  0220
@ER00002 BR    @14                                                 0220
*           RFY                                                    0221
*            (R14) UNRSTD;                                         0221
*         END;                                                     0222
*       QTYRQDR=QTYRQDR-PVTPCBNO;   /* IF THIS WAS A REQUEST FOR MORE
*                                      THAN THE NORMAL NUMBER OF   0223
*                                      PCBS, SET QUANTITY TO THAT  0223
*                                      ACTUALLY NEEDED TO SATISFY THE
*                                      CURRENT REQUEST               */
@RF00215 SLR   @14,@14                                             0223
         IC    @14,PVTPCBNO(,PVTPTR)                               0223
         SLR   QTYRQDR,@14                                         0223
*       FGMFAILF='1'B;              /* THE FIRST GETMAIN FAILED, SET
*                                      A FLAG TO PREVENT A THIRD TRY
*                                                            @YM08254*/
         OI    FGMFAILF(PVTPTR),B'10000000'                        0224
*       GOTO REPLCALC;              /* RECALCULATE CORE AMOUNT, AND
*                                      TRY THE GETMAIN AGAIN         */
         B     REPLCALC                                            0225
*     END;                                                         0226
*                                                                  0227
*/********************************************************************/
*/* PCB BUILD ROUTINE- THIS CODE ZERO'S, FORMATS, AND CHAINS NEW PCBS*/
*/* TOGETHER. WHEN THE ENTIRE BLOCK OF CORE HAS BEEN FORMATTED,      */
*/* CONTROL FALLS THROUGH TO THE SUCCEEDING CODE.                    */
*/********************************************************************/
*                                                                  0227
*   /*****************************************************************/
*   /*                                                               */
*   /* WHEN TRACE IS ACTIVE, THE CONSTANT LABELLED PCBTRACE MUST BE  */
*   /* ZAPPED TO THE NEW PCB SIZE                            @Z40WPYD*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0227
*   GENERATE;                                                      0227
@RF00213 DS    0H                                                  0227
         LR    CORELNR,QTYRQDR    TOTAL AMOUNT OF CORE EQUALS THE
         MH    CORELNR,PCBTRACE   QUANTITY DESIRED TIMES PCB LENGTH
*   PCBPTR=BEGINPTR;                /* POINT TO NEW PCB BEING BUILT  */
*                                                                  0228
         LR    PCBPTR,BEGINPTR                                     0228
*   /*****************************************************************/
*   /*                                                               */
*   /* WHEN TRACE IS ACTIVE, THE INSTRUCTION CLEARING THE PCB MUST BE*/
*   /* ZAPPED TO HANDLE THE NEW PCB SIZE                     @Z40WPYD*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0229
*TRACEZP5:                                                         0229
*   ;                                                              0229
TRACEZP5 DS    0H                                                  0230
*REPLBLD:                                                          0230
*   PCB=PCB&&PCB;                   /* CLEAR THE NEW PCB             */
REPLBLD  XC    PCB(64,PCBPTR),PCB(PCBPTR)                          0230
*   PCBCQN=PCBFREQN;                /* MARK PCB ON THE FREE QUEUE    */
*                                                                  0231
         MVI   PCBCQN(PCBPTR),X'10'                                0231
*   /*****************************************************************/
*   /*                                                               */
*   /* WHEN TRACE IS ACTIVE, THE LA INSTRUCTION OF THE PCB SIZE MUST */
*   /* BE ZAPPED TO X'41E00048'                              @Z40WPYD*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0232
*TRACEZP6:                                                         0232
*   PCBFQPA=PCBPTR+LENGTH(PCB);     /* POINT TO NEXT PCB TO BE     0232
*                                      CHAINED                       */
TRACEZP6 LA    @14,64                                              0232
         LR    @12,PCBPTR                                          0232
         ALR   @12,@14                                             0232
         STCM  @12,7,PCBFQPA(PCBPTR)                               0232
*   PCBBQPA=PCBPTR-LENGTH(PCB);     /* POINT BACK TO PREVIOUS PCB    */
         LR    @08,PCBPTR                                          0233
         SLR   @08,@14                                             0233
         ST    @08,PCBBQPA(,PCBPTR)                                0233
*   IF PCBFQPA^=BEGINPTR+CORELNR THEN/* IF THIS IS NOT THE LAST PCB
*                                      TO BE BUILT                   */
         LR    @08,BEGINPTR                                        0234
         ALR   @08,CORELNR                                         0234
         CR    @12,@08                                             0234
         BE    @RF00234                                            0234
*     DO;                                                          0235
*       PCBPTR=PCBPTR+LENGTH(PCB);  /* UPDATE PCB POINTER TO NEXT PCB
*                                      ON CHAIN                      */
         ALR   PCBPTR,@14                                          0236
*       GOTO REPLBLD;               /* LOOP UNTIL ALL CORE IS      0237
*                                      FORMATTED                     */
         B     REPLBLD                                             0237
*     END;                                                         0238
*                                                                  0239
*/********************************************************************/
*/* THIS CODE ADJUSTS THE FORWARD AND BACKWARD POINTERS IN THE FREE  */
*/* QUEUE ANCHOR AS REQUIRED, TO ADD THE NEW PCB CHAIN TO THE QUEUE. */
*/* THE FREE QUEUE COUNT IS UPDATED AND RETURN IS MADE TO THE CALLER.*/
*/********************************************************************/
*                                                                  0239
*   PCBFQPA=0;                      /* LAST NEW PCB FORWARD POINTER =
*                                      0                             */
@RF00234 SLR   @14,@14                                             0239
         STCM  @14,7,PCBFQPA(PCBPTR)                               0239
*   IF PVTFPCBF=0 THEN              /* IF FREE QUEUE IS EMPTY        */
         C     @14,PVTFPCBF(,PVTPTR)                               0240
         BNE   @RF00240                                            0240
*     DO;                                                          0241
*       PVTFPCBF=BEGINPTR;          /* FREE QUEUE FIRST POINTER    0242
*                                      POINTS TO NEW PCB CHAIN       */
         ST    BEGINPTR,PVTFPCBF(,PVTPTR)                          0242
*       BEGINPTR->PCBBQPA=0;        /* FIRST NEW PCB BACKWARD PTR  0243
*                                      POINTS TO 0                   */
         ST    @14,PCBBQPA(,BEGINPTR)                              0243
*     END;                                                         0244
*   ELSE                            /* IF FREE QUEUE IS NOT EMPTY    */
*     DO;                                                          0245
         B     @RC00240                                            0245
@RF00240 DS    0H                                                  0246
*       PVTFPCBL->PCBFQPA=BEGINPTR; /* CHAIN NEW PCBS TO OLD CHAIN   */
         L     @14,PVTFPCBL(,PVTPTR)                               0246
         STCM  BEGINPTR,7,PCBFQPA(@14)                             0246
*       BEGINPTR->PCBBQPA=PVTFPCBL; /* BACK CHAIN THE FIRST NEW PCB  */
         ST    @14,PCBBQPA(,BEGINPTR)                              0247
*     END;                                                         0248
*   PVTFPCBL=PCBPTR;                /* SET FREE QUEUE LAST POINTER   */
@RC00240 ST    PCBPTR,PVTFPCBL(,PVTPTR)                            0249
*   PVTPCBCT=PVTPCBCT+QTYRQDR;      /* UPDATE FREE QUEUE COUNT       */
         LR    @14,QTYRQDR                                         0250
         AH    @14,PVTPCBCT(,PVTPTR)                               0250
         STH   @14,PVTPCBCT(,PVTPTR)                               0250
*   RFY                                                            0251
*    (R14) RSTD;                                                   0251
*   R14=MGRRRTN;                    /* RESTORE RETURN REGISTER       */
         L     R14,MGRRRTN(,PVTPTR)                                0252
*   RETCODR=0;                                                     0253
         SLR   RETCODR,RETCODR                                     0253
*   END IEAVPCB3;                                                  0254
         B     @EL00002                                            0254
@PB00002 DS    0H                                                  0254
*                                                                  0255
*/********************************************************************/
*/*                                                                  */
*/* SUBROUTINE - IEAVREP1 (ENTRY IEAVREP1, IEAVREP2 AND IEAVREP3)    */
*/*                                                                  */
*/* FUNCTION   - REPLENISH THE SRB POOL USED BY RSM ROUTINES. WHEN   */
*/*              ENTERED AT IEAVREP1, THIS ROUTINE WILL ALSO SCAN    */
*/*              THE RSMHDR'S FOR EACH MEMORY TO DETERMINE IF        */
*/*              IEAVIOCP SHOULD BE SCHEDULE. IF RSMCPNU OR RSMCPNC  */
*/*              IS SET TO 1, IEAVIOCP IS SCHEDULED TO THE MEMORY    */
*/*              REPRESENTED BY THE RSMHDR.                          */
*/*              SEE MODULE PROLOGUE FOR A DESCRIPTION OF EACH       */
*/*              ENTRY POINT                                         */
*/*                                                                  */
*/* INPUT      - SEE MODULE PROLOGUE                                 */
*/*                                                                  */
*/* OUTPUT     - SEE MODULE PROLOGUE                                 */
*/*                                                                  */
*/********************************************************************/
*                                                                  0255
*   GEN DEFS(IEAVREP1,BASE2) SETS(R9,R8);                          0255
        ENTRY IEAVREP1                                      @Z30GNYC
IEAVREP1 DS   0H                                            @Z30GNYC
BASE2    BALR R9,0                                          @Z30GNYC
        LA    R8,BASE2-BASE1     GET AMOUNT TO ADJUST BY    @Z30GNYC
        SR    R9,R8              ADJUST BASE REG            @Z30GNYC
*   PVTPTR=CVTPVTP;                 /* SET PVT BASE          @Z30GNYC*/
         L     @12,CVTPTR                                          0256
         L     PVTPTR,CVTPVTP(,@12)                                0256
*   RESPECIFY                                                      0257
*    (R8) RESTRICTED;                                              0257
*   R8=R14;                         /* SAVE RETURN ADDRESS   @Z30GNYC*/
*                                                                  0258
         LR    R8,R14                                              0258
*   /*****************************************************************/
*   /*                                                               */
*   /* SET FRR                                               @Z30GNYC*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0259
*   GENERATE NODEFS REFS(R3,PVTPRCV,PSACSTK,FRRSCURR,FRRSLAST,FRRSELEN,
*       FRRSENTR,FRRSFRRA,FRRSPARM) SETS(R4,R5,RCAPTR,R15) NOFLOWS;
*                                                                  0259
         SETFRR  A,FRRAD=PVTPRCV(R3),PARMAD=(RCAPTR),WRKREGS=(R4,R5), **
               RELATED=(RSM,IEAVREP1(EXIT))
*   /*****************************************************************/
*   /*                                                               */
*   /* OBTAIN SALLOC LOCK                                    @Z30GNYC*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0260
*GETLOCK:                                                          0260
*   GENERATE NODEFS REFS(PSALITA,FLC) SETS(R11,R12,R13,R14) NOFLOWS;
GETLOCK  DS    0H                                                  0260
         SETLOCK OBTAIN,TYPE=SALLOC,MODE=UNCOND,                      **
               RELATED=(RSM,IEAVREP1(FREELOCK))
*   PVTPRCA=RCAPTR;                 /* RECOVERY COMM. AREA   @Z30GNYC*/
         ST    RCAPTR,PVTPRCA(,PVTPTR)                             0261
*   RCAREP1='1'B;                   /* IEAVREP1 ENTERED      @Z30GNYC*/
         OI    RCAREP1(RCAPTR),B'00000001'                         0262
*   RCASAL='1'B;                    /* SALLOC LOCK OBTAINED  @Z30GNYC*/
         OI    RCASAL(RCAPTR),B'10000000'                          0263
*   RCACSECT=RCACREP;               /* CSECT IS IEAVPCB      @Z30GNYC*/
         MVI   RCACSECT(RCAPTR),X'0C'                              0264
*   PVTSRBIU='0'B;                  /* THIS SRB NO LONGER IN USE   0265
*                                                            @Z30GNYC*/
         NI    PVTSRBIU(PVTPTR),B'11011111'                        0265
*   SCANHEAD='1'B;                  /* RSM HEADERS IN ALL MEMORIES 0266
*                                      WILL BE SEARCHED TO DETERMINE
*                                      IF IEAVIOCP SHOULD BE       0266
*                                      SCHEDULED             @Z30GNYC*/
         OI    SCANHEAD(PVTPTR),B'10000000'                        0266
*   QTYRQDR=PVTSRBNO;               /* INDICATE THE NUMBER OF SRBS TO
*                                      BE INCLUDED IN POOL   @Z30GNYC*/
         SLR   QTYRQDR,QTYRQDR                                     0267
         IC    QTYRQDR,PVTSRBNO(,PVTPTR)                           0267
*   GOTO GETSPACE;                  /* GET STORAGE FOR SRB'S @Z30GNYC*/
         B     GETSPACE                                            0268
*   GEN DEFS(IEAVREP2,BASE3) SETS(R9,R8,PVTWSA17) REFS(R0,R14);    0269
*                                                                  0269
        ENTRY IEAVREP2                                      @Z30GNYC
IEAVREP2 DS   0H                                            @Z30GNYC
        STM   R0,R14,PVTWSA17(R3)   SAVE CALLERS REG        @Z30GNYC
BASE3   BALR  R9,0                                          @Z30GNYC
        LA    R8,BASE3-BASE1     GET AMOUNT TO ADJUST BY    @Z30GNYC
        SR    R9,R8              ADJUST BASE REG            @Z30GNYC
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS IS THE BRANCH ENTRY FOR IEAVEQR AND IEAVRCF. THESE       */
*   /* ROUTINES MUST ENTER WITH SALLOC HELD                          */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0270
*   RCAPTR=PVTPRCA;                 /* SET BASE FOR RCA      @Z30GNYC*/
         L     RCAPTR,PVTPRCA(,PVTPTR)                             0270
*   QTYRQDR=PVTSRBNO;               /* NUMBER OF SRB'S TO BE INCLUDED
*                                      IN POOL               @Z30GNYC*/
         SLR   QTYRQDR,QTYRQDR                                     0271
         IC    QTYRQDR,PVTSRBNO(,PVTPTR)                           0271
*   SCANHEAD='0'B;                  /* RSMHDR'S WILL NOT BE SCANNED
*                                                            @Z30GNYC*/
         NI    SCANHEAD(PVTPTR),B'01111111'                        0272
*   GOTO GETSPACE;                  /* GET STORAGE FOR SRB'S @Z30GNYC*/
         B     GETSPACE                                            0273
*IEAVREP3:                                                         0274
*   ENTRY;                          /*                       @Z30GNYC*/
*   GEN;                                                           0275
*                                                                  0275
          ENTRY IEAVREP3                                    @Z30GNYC
IEAVREP3  DS    0H                                          @Z30GNYC
*   /*****************************************************************/
*   /*                                                               */
*   /* ENTERED HERE FROM IEAVPCB ON NIP CALL FOR PRIMARY ALLOCATION  */
*   /* OF SRB POOL                                                   */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0276
*   STM(R0,R14,PVTWSA17);           /* SAVE CALLERS REG      @Z30GNYC*/
         STM   R0,R14,PVTWSA17(PVTPTR)                             0276
*   QTYRQDR=PVTSRBS;                /* NUMBER OF SRB'S TO BE INCLUDED
*                                      IN SRB POOL           @Z30GNYC*/
         SLR   QTYRQDR,QTYRQDR                                     0277
         IC    QTYRQDR,PVTSRBS(,PVTPTR)                            0277
*   RCAPTR=PVTPRCA;                 /* SET BASE FOR RCA      @Z30GNYC*/
         L     RCAPTR,PVTPRCA(,PVTPTR)                             0278
*   SCANHEAD='0'B;                  /* RSMHDR'S ARE NOT TO BE      0279
*                                      SEARCHED              @Z30GNYC*/
         NI    SCANHEAD(PVTPTR),B'01111111'                        0279
*GETSPACE:                                                         0280
*   ERRLOOP='0'B;                   /* ERROR LOOP CONTROL SWITCH - 0280
*                                      WILL ONLY PERMIT ONE FAILURE
*                                      OF GETMAIN OF SRB'S   @Z30GNYC*/
GETSPACE NI    ERRLOOP(PVTPTR),B'10111111'                         0280
*   RESPECIFY                                                      0281
*    (R0,                                                          0281
*     R1,                                                          0281
*     R15,                                                         0281
*     R11) RESTRICTED;                                             0281
*RETRY:                                                            0282
*   R0=(QTYRQDR*(LENGTH(SRB)+4)+LENGTH(CPAB));/* COMPUTE SIZE OF   0282
*                                      GETMAIN               @Z30GNYC*/
RETRY    LR    R0,QTYRQDR                                          0282
         MH    R0,@CH00983                                         0282
         AL    R0,@CF00107                                         0282
*   R11=PVTPTR;                     /* SAVE PVTPTR           @Z30GNYC*/
         LR    R11,PVTPTR                                          0283
*   PVTRSMGM='1'B;                  /* INDICATE RSM GETMAIN TO PREF
