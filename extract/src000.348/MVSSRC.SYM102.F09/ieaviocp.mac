         BNE   @RF00341                                            0341
*                   DO;             /* YES                           */
*                     FOEFXCT=FOEFXCT-PCBFXC;                      0343
         LH    @12,FOEFXCT(,FOEPTR)                                0343
         SLR   @11,@11                                             0343
         IC    @11,PCBFXC(,CPCURPCB)                               0343
         SLR   @12,@11                                             0343
         STH   @12,FOEFXCT(,FOEPTR)                                0343
*                     GOTO LBPFTFXC;/* WHEN AN FOE IS FOUND, EXIT  0344
*                                      LOOP                          */
         B     LBPFTFXC                                            0344
*                   END;                                           0345
*                 ELSE              /* VBNS NOT EQUAL, KEEP LOOKING  */
*                   FOEPTR=FOEFLINK;/* GET NEXT FOE                  */
@RF00341 L     @12,FOEFLINK-1(,FOEPTR)                             0346
         LA    @12,0(,@12)                                         0346
         LR    FOEPTR,@12                                          0346
*                 RESPECIFY                                        0347
*                   FOEPTR UNRESTRICTED;                           0347
*               END;                /* END FOE DO WHILE              */
@DE00340 LTR   FOEPTR,FOEPTR                                       0348
         BZ    @DC00340                                            0348
         CLC   FOEVINDX(2,FOEPTR),PCBVBN(CPCURPCB)                 0348
         BNH   @DL00340                                            0348
@DC00340 DS    0H                                                  0349
*               RCARCRD='10'X;      /* IF NO FOE IS FOUND, SET THE 0349
*                                      REASON CODE AND ISSUE ABEND TO
*                                      RECORD ERROR BEFORE         0349
*                                      CONTINUING.                   */
         MVI   RCARCRD(RCAPTR),X'10'                               0349
*               GEN(ABEND X'C0D',,,SYSTEM) ;/* GO TO FRR TO RECORD 0350
*                                      ERROR                         */
         ABEND X'C0D',,,SYSTEM
*             END;                  /* END NON-ZERO TCB PROCESSING   */
*           ELSE                                                   0352
*             ;                     /* TCB ADDR IN ROOT WAS ZERO     */
@RF00336 DS    0H                                                  0353
*LBPFTFXC:                          /* REGARDLESS OF TCB ADDR, THE 0353
*                                      PFTE FIX COUNT IS DECREMENTED
*                                      BY THE VALUE OF PCBFXC AND  0353
*                                      PCBFXC THEN SET TO ZERO.      */
*           RESPECIFY                                              0353
*             PFTEPTR RESTRICTED;                                  0353
LBPFTFXC DS    0H                                                  0354
*           PFTEPTR=PVTPFTP+PCBRBN; /* DEVELOP THE ADDR OF THE PFTE  */
         LH    PFTEPTR,PCBRBN(,CPCURPCB)                           0354
         N     PFTEPTR,@CF02180                                    0354
         AL    PFTEPTR,PVTPFTP(,PVTPTR)                            0354
*           PFTFXCT=PFTFXCT-PCBFXC;                                0355
         LH    @12,PFTFXCT(,PFTEPTR)                               0355
         SLR   @11,@11                                             0355
         IC    @11,PCBFXC(,CPCURPCB)                               0355
         SLR   @12,@11                                             0355
         STH   @12,PFTFXCT(,PFTEPTR)                               0355
*           RESPECIFY                                              0356
*             PFTEPTR UNRESTRICTED;                                0356
*           PCBFXC=0;                                              0357
         MVI   PCBFXC(CPCURPCB),X'00'                              0357
*           IF PFTFXCT=0 THEN       /* IS FRAME FIX COUNT NOW EQUAL
*                                      ZERO                          */
         LTR   @12,@12                                             0358
         BNZ   @RF00358                                            0358
*             DO;                   /* YES, DECREMENT COUNTS @YA01572*/
*               IF PCBVBN>=PVTLCSA THEN/* IS FRAME IN COMMON AREA    */
         CLC   PCBVBN(2,CPCURPCB),PVTLCSA(PVTPTR)                  0360
         BL    @RF00360                                            0360
*                 PVTCOMFX=PVTCOMFX-1;/* YES, COMMON AREA, DECREMENT
*                                      COMMON AREA COUNT     @YA01572*/
         LH    @12,PVTCOMFX(,PVTPTR)                               0361
         BCTR  @12,0                                               0361
         STH   @12,PVTCOMFX(,PVTPTR)                               0361
*               ELSE                /* NOT COMMON AREA               */
*                 RSMCNTFX=RSMCNTFX-1;/* DECREMENT COUNT FOR THIS  0362
*                                      ADDRESS SPACE         @YA01572*/
         B     @RC00360                                            0362
@RF00360 LH    @12,RSMCNTFX(,RSMHDPTR)                             0362
         BCTR  @12,0                                               0362
         STH   @12,RSMCNTFX(,RSMHDPTR)                             0362
*               PVTCNTFX=PVTCNTFX-1;/* DECREMENT SYSTEM COUNT      0363
*                                                            @YA01572*/
@RC00360 LH    @12,PVTCNTFX(,PVTPTR)                               0363
         BCTR  @12,0                                               0363
         STH   @12,PVTCNTFX(,PVTPTR)                               0363
*             END;                                                 0364
*           ELSE                                                   0365
*             ;                     /* FRAME FIX COUNT IS NOT ZERO   */
@RF00358 DS    0H                                                  0366
*         END;                      /* END NON-ZERO PCBFXC PROCESSING*/
*       ELSE                                                       0367
*         ;                         /* THE PCB FIX CNT WAS ZERO      */
@RF00328 DS    0H                                                  0368
*     END;                          /* END ROOT PCB PROCESSING       */
*   ELSE                                                           0369
*     ;                             /* NO ROOT EXISTED, NO SPECIAL 0369
*                                      PCB PROCESSING IS NECESSARY   */
@RF00320 DS    0H                                                  0370
*   CALL SNOTIFY;                   /* CALL SNOTIFY FOR ALL INPT   0370
*                                      PCB'S                         */
*                                                                  0370
