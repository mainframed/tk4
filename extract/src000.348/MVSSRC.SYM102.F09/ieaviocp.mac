         BNO   @RF00323                                            0323
*         DO;                                                      0324
*           PCBRPB='1'B;            /* SET SPECIAL FLAG FOR STAGE 1  */
         OI    PCBRPB(@12),B'00100000'                             0325
*           GOTO LBPFTFXC;          /* BYPASS FOE PROCESSING         */
         B     LBPFTFXC                                            0326
*         END;                                                     0327
*       IF PCBFXC^=0 THEN           /* IF THE PCB IS FOR A PGFIX   0328
*                                      REQ., FOE PROCESSING MUST BE
*                                      DONE                          */
@RF00323 CLI   PCBFXC(CPCURPCB),0                                  0328
         BE    @RF00328                                            0328
*         DO;                                                      0329
*           IF CPLLHELD='0'B THEN   /* NO FIX PCB PROCESSING IS DONE
*                                      UNLESS THE LOCAL LOCK IS HELD
*                                      IN ORDER THAT ANY PFTE DEFERED
*                                      RELEASE PROCESSING WILL BE  0330
*                                      DONE ONLY WITH THE LOCAL LOCK
*                                      HELD AND TO GUARANTEE TCB   0330
*                                      SERIALIZATION.                */
         TM    CPLLHELD(PVTPTR),B'10000000'                        0330
         BNZ   @RF00330                                            0330
*             DO;                                                  0331
*               CPKEEPCB='1'B;      /* SET INDICATOR ON TO KEEP PCB  */
         OI    CPKEEPCB(PVTPTR),B'01000000'                        0332
*               GOTO LBERRETN;      /* BYPASS ALL PCB PROCESSING     */
         B     LBERRETN                                            0333
*             END;                                                 0334
*           ELSE                                                   0335
*             ;                     /* CONTINUE IF LOCAL LOCK HELD   */
@RF00330 DS    0H                                                  0336
*           IF PCBRTCBA^=0 THEN     /* FOE PROCESSING IS DONE ONLY IF
