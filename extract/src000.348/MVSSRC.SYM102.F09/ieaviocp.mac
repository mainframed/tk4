*               CPKEEPCB='1'B;      /* TELL CALLER NOT TO FREE PCB   */
         OI    CPRCODE(PVTPTR),B'01010000'                         0392
*               GOTO LBNRMEXT;      /* GO TO EXIT PROCESSING         */
         B     LBNRMEXT                                            0393
*             END;                                                 0394
*           ELSE                    /* OTHERWISE, RESET MAY BE CALLED*/
*             DO;                                                  0395
@RF00389 DS    0H                                                  0396
*               CPRSETR4=PCBRTPA;   /* REG 4 GETS THE TCB ADDR FOR 0396
*                                      NON-SRB PCB'S                 */
         L     CPRSETR4,PCBRTPA-1(,CPCURPCB)                       0396
