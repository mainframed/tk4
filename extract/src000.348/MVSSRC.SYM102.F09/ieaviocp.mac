         TITLE 'IEAVIOCP--PAGE I/O COMPLETION PROCESSOR                *
                        '
IEAVIOCP CSECT
         USING *,15                REG 15 CONTAINS ENTRY PT. ADDR
         MODID BRANCH=NO
IEAVIOC2 DS    0H
         ENTRY IEAVIOC2
         BALR  CPBASE10,0
         USING *,CPBASE10
*   PVTPTR=CVTPVTP;                 /* GET THE ADDR OF PVT IN REG 3  */
         L     @12,CVTPTR                                          0067
         L     PVTPTR,CVTPVTP(,@12)                                0067
*   GENERATE REFS(PVTPRCV,CPWRKRG4,CPWRKRG5) SETS(RCAPTR);         0068
LBGETFRR SETFRR A,FRRAD=PVTPRCV(PVTPTR),PARMAD=(RCAPTR),              **
               WRKREGS=(CPWRKRG4,CPWRKRG5),                           **
               RELATED=(RSM,IEAVIOCP(LBFREFRR))
*   RCACSECT=RCACCOMP;              /* SET CSECT ID IN RCA           */
         MVI   RCACSECT(RCAPTR),X'14'                              0069
*   RCAIOCP='1'B;                   /* SET IEAVIOCP ENTRY POINT FLAG */
         OI    RCAIOCP(RCAPTR),B'10000000'                         0070
*   RESPECIFY                                                      0071
*     CPWRKRG5 RESTRICTED;                                         0071
*   CPWRKRG5=CPARMRG1;              /* SAVE INPUT PARM ACCROSS     0072
*                                      FREEING OF SRB                */
         LR    CPWRKRG5,CPARMRG1                                   0072
*   CPARMRG1=CPARMRG0;              /* TRANSFER SRB ADDR TO REG 1    */
         LR    CPARMRG1,CPARMRG0                                   0073
*   CPARMRG0=PVTSRBID;              /* PLACE CELL POOL ID IN REG 0   */
         L     CPARMRG0,PVTSRBID(,PVTPTR)                          0074
*   GEN(FREECELL CPID=(0),CELL=(1),BRANCH=YES,SAVE=NO)             0075
*   REFS(CPARMRG0,CPARMRG1);        /* RETURN SRB TO CELL POOL. THIS
*                                      MACRO DESTROYS REGISTERS    0075
*                                      0-4,14,15                     */
         FREECELL CPID=(0),CELL=(1),BRANCH=YES,SAVE=NO
*   PVTPTR=CVTPVTP;                 /* RESTORE PVT REGISTER          */
         L     @12,CVTPTR                                          0076
         L     PVTPTR,CVTPVTP(,@12)                                0076
*   RESPECIFY                                                      0077
*    (CPARMRG0,                                                    0077
*     CPARMRG1) UNRESTRICTED;                                      0077
*   IF CPWRKRG5^=0 THEN             /* IF INPUT PARM NOT 0, ASK FOR
*                                      LOCAL LOCK UNCONDITIONALLY    */
         LTR   CPWRKRG5,CPWRKRG5                                   0078
         BZ    @RF00078                                            0078
*     DO;                                                          0079
*       GENERATE;                                                  0080
LBGETLL1 SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,                       **
               RELATED=(RSM,IEAVIOCP(LBFREELL))
*       RCALL='1'B;                 /* THE LOCAL LOCK HAS BEEN     0081
*                                      OBTAINED WHEN AND IF THIS   0081
*                                      INSTRUCTION IS EXECUTED       */
         OI    RCALL(RCAPTR),B'01000000'                           0081
*     END;                                                         0082
*   ELSE                                                           0083
*     ;                             /* OTHERWISE, NO LOCK REQUIRED   */
@RF00078 DS    0H                                                  0084
*   GENERATE;                                                      0084
LBGTSALC SETLOCK OBTAIN,TYPE=SALLOC,MODE=UNCOND,                      **
               RELATED=(RSM,IEAVIOCP(LBFRSALC))
*   RCASAL='1'B;                    /* IF THIS MODULE REMAINS IN   0085
*                                      CONTROL, THE SALLOC LOCK HAS
*                                      BEEN OBTAINED.                */
         OI    RCASAL(RCAPTR),B'10000000'                          0085
*   PVTPRCA=RCAPTR;                 /* THE RCA ADDR IS STORED IN PVT */
         ST    RCAPTR,PVTPRCA(,PVTPTR)                             0086
*   RSMHDPTR=ASCBRSM;               /* REG 2 IS INITIALIZED WITH THE
*                                      CORRECT RSM HEADER ADDRESS    */
         L     @12,ASCBPTR                                         0087
         L     RSMHDPTR,ASCBRSM(,@12)                              0087
*   CPFLAGS=CPINITFL;               /* THE INTERNAL FLAGS ARE SET OFF*/
         MVI   CPFLAGS(PVTPTR),X'00'                               0088
*   IF CPWRKRG5^=0 THEN             /* IF THE LOCAL LOCK WAS       0089
*                                      OBTAINED, THE INTERNAL LOCAL
*                                      LOCK HELD FLAG IS SET AND THE
*                                      OBTAIN LOCAL LOCK FLAG IN   0089
*                                      RSMHD IS RESET                */
         LTR   CPWRKRG5,CPWRKRG5                                   0089
         BZ    @RF00089                                            0089
*     DO;                                                          0090
*       CPLLHELD='1'B;                                             0091
         OI    CPLLHELD(PVTPTR),B'10000000'                        0091
*       RSMIOCPU='0'B;                                             0092
         NI    RSMIOCPU(RSMHDPTR),B'01111111'                      0092
*     END;                                                         0093
*   ELSE                            /* OTHERWISE, THE LOCAL LOCK WAS
*                                      NOT REQUESTED AND THE RUN   0094
*                                      WITHOUT LOCAL LOCK FLAG IN  0094
*                                      RSMHD IS RESET                */
*     DO;                                                          0094
         B     @RC00089                                            0094
@RF00089 DS    0H                                                  0095
*       RSMIOCPC='0'B;                                             0095
         NI    RSMIOCPC(RSMHDPTR),B'10111111'                      0095
*     END;                                                         0096
*   RESPECIFY                                                      0097
*     CPWRKRG5 UNRESTRICTED;                                       0097
@RC00089 DS    0H                                                  0098
*                                                                  0098
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS SECOND SEGMENT CALLS THE SQSEARCH SUBROUTINE TO PROCESS  */
*   /* ANY AND ALL PCB'S THAT CAN BE PROCESSED ON THIS CALL TO THE   */
*   /* MODULE. ANY RETURN CODE SET BY PCB PROCESSING IS IGNORED. THE */
*   /* LOCKS OBTAINED IN THE FIRST SEGMENT ARE FREED, THE RECOVERY   */
*   /* ENVIRONMENT CANCELLED AND EXIT MADE TO THE DISPATCHER USING   */
*   /* THE RETURN ADDRESS PROVIDED IN THE CVT.                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0098
*   CALL SQSEARCH;                  /* THE SQSEARCH SUBRTN. IS CALLED*/
         BAL   @14,SQSEARCH                                        0098
*   GENERATE;                                                      0099
LBFRSALC SETLOCK RELEASE,TYPE=SALLOC,                                 **
               RELATED=(RSM,IEAVIOCP(LBGTSALC))
*   IF RCALL='1'B THEN              /* IF THE LOCAL LOCK WAS       0100
*                                      OBTAINED, FREE IT.            */
         TM    RCALL(RCAPTR),B'01000000'                           0100
         BNO   @RF00100                                            0100
*     GENERATE;                                                    0101
LBFREELL SETLOCK RELEASE,TYPE=LOCAL,                                  **
               RELATED=(RSM,IEAVIOCP(LBGETLL1))
*   ELSE                                                           0102
*     ;                             /* DO NOTHING IF LOCAL LOCK NOT
*                                      REQUESTED                     */
@RF00100 DS    0H                                                  0103
*   GENERATE REFS(CPWRKRG4,CPWRKRG5);                              0103
LBFREFRR SETFRR D,WRKREGS=(CPWRKRG4,CPWRKRG5),                        **
               RELATED=(RSM,IEAVIOCP(LBGETFRR))
*   CPRETREG=CVTSRBRT;              /* GET EXIT ADDR FROM CVT        */
         L     @12,CVTPTR                                          0104
         L     CPRETREG,CVTSRBRT(,@12)                             0104
*   GOTO CPRETREG;                  /* EXIT TO THE DISPATCHER        */
         BR    CPRETREG                                            0105
*                                                                  0106
*   /*****************************************************************/
*   /*                                                               */
*   /* THE SQSEARCH SUBROUTINE IS CALLED FROM BOTH ENTRY POINTS. ITS */
*   /* ONLY JOB IS TO FIND PCB'S ON THE LOCAL I/O ACTIVE Q FOR THE   */
*   /* CURRENT ADDRESS SPACE OR THE COMMON I/O ACTIVE Q THAT HAVE    */
*   /* BEEN MARKED I/O COMPLETE AND PASS THESE PCB'S TO THE SUBNORML */
*   /* OR SUBIOERR SUBROUTINES. THE QUEUES ARE SEARCHED USING THE    */
*   /* BACK POINTERS OF EACH PCB BECAUSE PROCESSING OF A PCB MAY     */
*   /* CAUSE A RELATED PCB TO BE ENQUEUED TO THE END OF THE Q BEING  */
*   /* PROCESSED. BY FOLLOWING THE BACK Q POINTERS, WE ARE ASSURED OF*/
*   /* NOT PROCESSING NEWLY ENQUEUED PCB'S AGAIN AFTER THEY HAVE     */
*   /* ALREADY BEEN PROCESSED.                                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0106
*SQSEARCH:                                                         0106
*   PROC OPTIONS(NOSAVE NOSAVEAREA);                               0106
SQSEARCH DS    0H                                                  0107
*   CPSBRET1=CPRETREG;              /* SAVE THE RETURN ADDRESS       */
         ST    CPRETREG,CPSBRET1(,PVTPTR)                          0107
*   PCBPTR=RSMLIOQL;                /* INIT THE PCB REG WITH THE ADDR
*                                      OF THE LAST PCB ON THE LOCAL
*                                      I/O ACTIVE QUEUE.             */
         L     PCBPTR,RSMLIOQL(,RSMHDPTR)                          0108
*   DO WHILE(PCBPTR^=0);            /* PROCESS ALL PCB'S ON Q MARKED
*                                      I/O COMPLETE                  */
         B     @DE00109                                            0109
@DL00109 DS    0H                                                  0110
*     IF PCBIOCMP='1'B THEN         /* IS I/O COMPLETE FOR PCB       */
         TM    PCBIOCMP(PCBPTR),B'00000001'                        0110
         BNO   @RF00110                                            0110
*       DO;                         /* YES                           */
*         CPBAKSAV=PCBBQPA;         /* SAVE BACK PTR TO NEXT PCB     */
         L     @12,PCBBQPA(,PCBPTR)                                0112
         ST    @12,CPBAKSAV(,PVTPTR)                               0112
*         IF PCBIOERR='0'B THEN     /* CALL THE I/O ERROR OR NORMAL
*                                      PCB PROCESSOR SUBROUTINE    0113
*                                      ACCORDING TO THE I/O ERROR PCB
*                                      FLAG                          */
         TM    PCBIOERR(PCBPTR),B'00100000'                        0113
         BNZ   @RF00113                                            0113
*           CALL SUBNORML;          /* NORMAL PROCESSOR GETS CONTROL */
         BAL   @14,SUBNORML                                        0114
*         ELSE                      /* I/O ERROR OCCURRED            */
*           CALL SUBIOERR;          /* ERROR PROCESSOR GETS CONTROL  */
         B     @RC00113                                            0115
@RF00113 BAL   @14,SUBIOERR                                        0115
*         PCBPTR=CPBAKSAV;          /* PREPARE FOR NEXT PCB          */
@RC00113 L     PCBPTR,CPBAKSAV(,PVTPTR)                            0116
*       END;                                                       0117
*     ELSE                          /* IF I/O IS NOT COMPLETE FOR  0118
*                                      PCB, GET ADDR OF NEXT PCB TO
*                                      PROCESS                       */
*       PCBPTR=PCBBQPA;                                            0118
         B     @RC00110                                            0118
@RF00110 L     PCBPTR,PCBBQPA(,PCBPTR)                             0118
*   END;                            /* END LOCAL Q DO WHILE          */
@RC00110 DS    0H                                                  0119
@DE00109 LTR   PCBPTR,PCBPTR                                       0119
         BNZ   @DL00109                                            0119
*   PCBPTR=PVTCIOQL;                /* ONCE THE LOCAL Q IS PROCESSED,
*                                      GET THE ADDR OF THE LAST PCB
*                                      ON THE COMMON I/O Q AND     0120
*                                      PROCESS THAT Q                */
         L     PCBPTR,PVTCIOQL(,PVTPTR)                            0120
*   DO WHILE(PCBPTR^=0);            /* PROCESS THOSE PCB'S THAT ARE
*                                      FOR THE ADDRESS SPACE WE ARE
*                                      RUNNING IN AND ARE MARKED I/O
*                                      COMPLETE                      */
         B     @DE00121                                            0121
@DL00121 DS    0H                                                  0122
*     IF PCBASCB=PSAAOLD&PCBIOCMP='1'B THEN/* PCB FOR CURRENT A.S. 0122
*                                      AND I/O COMPLETE              */
         CLC   PCBASCB(4,PCBPTR),PSAAOLD                           0122
         BNE   @RF00122                                            0122
         TM    PCBIOCMP(PCBPTR),B'00000001'                        0122
         BNO   @RF00122                                            0122
*       DO;                         /* YES                           */
*         CPBAKSAV=PCBBQPA;         /* SAVE BACK PTR TO NEXT PCB     */
         L     @12,PCBBQPA(,PCBPTR)                                0124
         ST    @12,CPBAKSAV(,PVTPTR)                               0124
*         IF PCBIOERR='0'B THEN     /* CALL THE I/O ERROR OR NORMAL
*                                      PCB PROCESSOR SUBROUTINE    0125
*                                      ACCORDING TO THE I/O ERROR PCB
*                                      FLAG                          */
         TM    PCBIOERR(PCBPTR),B'00100000'                        0125
         BNZ   @RF00125                                            0125
*           CALL SUBNORML;          /* NORMAL PROCESSOR GETS CONTROL */
         BAL   @14,SUBNORML                                        0126
*         ELSE                      /* I/O ERROR OCCURRED            */
*           CALL SUBIOERR;          /* ERROR PROCESSOR GETS CONTROL  */
         B     @RC00125                                            0127
@RF00125 BAL   @14,SUBIOERR                                        0127
*         PCBPTR=CPBAKSAV;          /* PREPARE FOR NEXT PCB          */
@RC00125 L     PCBPTR,CPBAKSAV(,PVTPTR)                            0128
*       END;                                                       0129
*     ELSE                          /* OTHERWISE, GET ADDR OF NEXT 0130
*                                      PCB TO PROCESS                */
*       PCBPTR=PCBBQPA;                                            0130
         B     @RC00122                                            0130
@RF00122 L     PCBPTR,PCBBQPA(,PCBPTR)                             0130
*   END;                            /* END COMMON Q DO WHILE         */
@RC00122 DS    0H                                                  0131
@DE00121 LTR   PCBPTR,PCBPTR                                       0131
         BNZ   @DL00121                                            0131
*   CPRETREG=CPSBRET1;              /* RESTORE RETURN ADDRESS AND  0132
*                                      RETURN TO THE CALLER WHEN BOTH
*                                      QUEUES HAVE BEEN PROCESSED    */
         L     CPRETREG,CPSBRET1(,PVTPTR)                          0132
*   END;                            /* END QSEARCH INTERNAL PROC     */
@EL00002 DS    0H                                                  0133
@EF00002 DS    0H                                                  0133
@ER00002 BR    @14                                                 0133
*                                                                  0134
*   /*****************************************************************/
*   /*                                                               */
*   /* THE SUBNORML SUBROUTINE IS CALLED BY THE SQSEARCH SUBROUTINE. */
*   /* IT PROCESSES ALL PCB'S FOR WHICH I/O HAS COMPLETED            */
*   /* SUCCESSFULLY. THE INPUT PCB ADDRESS IS PASSED IN THE STANDARD */
*   /* PCB REGISTER FOR THIS MODULE. FIRST, ANY SPECIAL SWAP-IN      */
*   /* PROCESSING IS PERFORMED IF NECESSARY TO NORMALIZE THE PGTE FOR*/
*   /* ALL PROCESSING BELOW. FOR STAGE 2 SWAP-IN PCB'S, THE CORRECT  */
*   /* RBN IS PLACED IN THE PGTE FOR THE PAGE AND THE FORCE RELATE   */
*   /* FLAG RESET IN THE XPTE. NOTE THAT THIS DONE ONLY FOR PCB'S    */
*   /* WITH NON-ZERO VBN'S AND THE FREE REAL FLAG OFF AS THAT IS THE */
*   /* ONLY TIME THE PGTE/XPTE ADDRESSES IN THE PCB ARE VALID. NOTE  */
*   /* ALSO THAT STAGE 2 PCB'S CAN APPEAR ONLY AS QUEUED PCB'S, NEVER*/
*   /* AS RELATED PCB'S. THE SWAP-IN TEST ASSUMES IEAVPIOP REMOVES   */
*   /* ALL STAGE 1 PCB'S FROM A Q AS SOON AS THEIR PAGE-IN COMPLETES.*/
*   /* ONCE PGTE NORMALIZATION IS COMPLETE, ANY AND ALL RELATED PCB'S*/
*   /* ARE PROCESSED FIRST BY CALLING THE SPCBNORM SUBROUTINE FOR    */
*   /* EACH PCB. UPON RETURN, IF A PCB IS NOT TO BE FREED, IT IS     */
*   /* REMOVED FROM ITS RELATED Q AND SENT TO THE I/O ACTIVE Q ITS   */
*   /* PRIMARY PCB (INPUT PCB) IS ON. THE VBN FIELD IN THE PCB IS SET*/
*   /* TO ZERO AND THE FREE REAL FRAME FLAG TURNED OFF AS IT NO      */
*   /* LONGER HAS ANY MEANING. PIOCP IS SCHEDULED TO RUN AT A LATER  */
*   /* TIME WHEN THE LOCAL LOCK CAN BE OBTAINED BY CALLING A         */
*   /* SUBROUTINE OF PIOP. THIS SUBROUTINE ASSURES THAT MULTIPLE     */
*   /* SCHEDULES ARE NOT PERFORMED WHERE NOT NECESSARY. OTHERWISE,   */
*   /* THE RELATED PCB IS RETURNED TO THE FREE Q. PROCESSING         */
*   /* CONTINUES AT THE NEXT SEGMENT WITH THE PRIMARY PCB. NOTE THAT */
*   /* UNQUEUED PCB'S PASSED ON A BRANCH ENTRY (IEAVCPBR) MUST SET   */
*   /* PCB'S TO ALWAYS ALLOW THE PCB TO BE DISPOSED OF AS STATED IN  */
*   /* THE DEPENDENCY SECTION OF THE PROLOGUE.                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0134
*SUBNORML:                                                         0134
*   PROC OPTIONS(NOSAVE NOSAVEAREA);                               0134
SUBNORML DS    0H                                                  0135
*   CPSBRET2=CPRETREG;              /* SAVE THE RETURN ADDRESS       */
*                                                                  0135
         ST    CPRETREG,CPSBRET2(,PVTPTR)                          0135
*   /*****************************************************************/
*   /*                                                               */
*   /* WHEN THE PCB TRACE IS ACTIVE, THE BRANCH INSTRUCTION BELOW    */
*   /* MUST BE NOOPED                                        @Z40WPYD*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0136
*TRACEZC1:                                                         0136
*   GENERATE REFS(PCBPTR) SETS(CPWRKRG5);                          0136
TRACEZC1 DS    0H                                                  0136
         B     TRACEZC1+16
         L     CPWRKRG5,64(PCBPTR)
*   CPWRKRG5=CPWRKRG5+MYID;                                        0137
         AL    CPWRKRG5,@CF02093                                   0137
*   GENERATE;                                                      0138
         ST    CPWRKRG5,64(PCBPTR)
*   IF PCBSWPS2='1'B&PCBFREAL='0'B&PCBVBN^=0 THEN/* IS PCB FOR STAGE
*                                      2 SWAP-IN AND UNPURGED      0139
*                                                            @Z40WPYD*/
         TM    PCBSWPS2(PCBPTR),B'00000001'                        0139
         BNO   @RF00139                                            0139
         TM    PCBFREAL(PCBPTR),B'10000000'                        0139
         BNZ   @RF00139                                            0139
         LH    @12,PCBVBN(,PCBPTR)                                 0139
         N     @12,@CF02180                                        0139
         LTR   @12,@12                                             0139
         BZ    @RF00139                                            0139
*     DO;                           /* YES                           */
*       PGTRSA=CPGTFLGS;            /* THE PGTE IS INITIALIZED TO  0141
*                                      ZERO, INVALID, AND GETMAIN  0141
*                                      ASSIGNED                      */
         L     @12,PTEPTR(,PCBPTR)                                 0141
         MVC   PGTRSA(2,@12),@CB02091                              0141
*       PGTRSA=PGTRSA×PCBRBN;       /* THE REMAINDER OF THE PGTE IS
*                                      THEN SET EQUAL TO THE PCBRBN
*                                      FIELD                         */
         OC    PGTRSA(2,@12),PCBRBN(PCBPTR)                        0142
*     END;                                                         0143
*   ELSE                                                           0144
*     ;                             /* DO NOTHING IF PCB IS NOT    0144
*                                      SWAP-IN OR SWAP-IN PCB WAS  0144
*                                      PURGED                        */
@RF00139 DS    0H                                                  0145
*   CPFREREL='1'B;                  /* SET INTERNAL FLAG TO FREE   0145
*                                      FRAME VALUE                   */
         OI    CPFREREL(PVTPTR),B'00100000'                        0145
*   DO WHILE(PCBRLPA^=0);           /* PROCESS EACH RELATED PCB      */
         B     @DE00146                                            0146
@DL00146 DS    0H                                                  0147
*     RESPECIFY                                                    0147
*       CPCURPCB RESTRICTED;        /* THIS REG WILL BE THE PARM REG
*                                      FOR CPNORMP                   */
*     CPCURPCB=PCBRLPA;             /* INIT PARM REG FOR CPNORMP     */
         L     CPCURPCB,PCBRLPA-1(,PCBPTR)                         0148
         LA    CPCURPCB,0(,CPCURPCB)                               0148
*     CALL SPCBNORM;                /* PROCESS A RELATED PCB         */
         BAL   @14,SPCBNORM                                        0149
*     PCBRLPA=CPCURPCB->PCBRLPA;    /* REMOVE PROCESSED PCB FROM THE
*                                      RELATED CHAIN.                */
         MVC   PCBRLPA(3,PCBPTR),PCBRLPA(CPCURPCB)                 0150
*     CPCURPCB->PCBFQP=CPRPFRST;    /* RESET THE FIRST WORD OF THE 0151
*                                      RELATED PCB TO ZERO WITH A Q
*                                      NUMBER INDICATING THE UNQUEUED
*                                      STATE                         */
         L     @12,CPRPFRST                                        0151
         ST    @12,PCBFQP(,CPCURPCB)                               0151
*     RESPECIFY                                                    0152
*      (CPARMRG0,                                                  0152
*       CPARMRG1) RESTRICTED;                                      0152
*     IF CPKEEPCB='1'B THEN         /* IF THE PCB IS TO BE KEPT,   0153
*                                      SCHEDULE PIOCP TO RUN LATER 0153
*                                      AND SET THE TO Q NUMBER FOR 0153
*                                      PCB MANAGER TO THE Q NUMBER IN
*                                      THE PRIMARY PCB               */
         TM    CPKEEPCB(PVTPTR),B'01000000'                        0153
         BNO   @RF00153                                            0153
*       DO;                                                        0154
*         CPCURPCB->PCBVBN=0;       /* ZERO THE PCBVBN SO PFTE AND 0155
*                                      PGTE PROCESSING WON'T BE    0155
*                                      ATTEMPTED AGAIN BY THIS     0155
*                                      MODULE.                       */
         SLR   @12,@12                                             0155
         STH   @12,PCBVBN(,CPCURPCB)                               0155
*         CPCURPCB->PCBFREAL='0'B;  /* THIS FLAG NO LONGER HAS     0156
*                                      MEANING                       */
         NI    PCBFREAL(CPCURPCB),B'01111111'                      0156
*         CPCURPCB->PCBRLPA=0;      /* ZERO THE RELATED PTR IN THE 0157
*                                      PCB TO BE ENQUEUED TO AN I/O
*                                      Q.                            */
         STCM  @12,7,PCBRLPA(CPCURPCB)                             0157
*         CPARMRG1=CPCURPCB;        /* SET UP PARM FOR SCHED SUBRTN. */
         LR    CPARMRG1,CPCURPCB                                   0158
*         CALL EPSCHED;             /* CALL SCHEDULE SUBRTN OF PIOP  */
         L     @15,PVTPOPBR(,PVTPTR)                               0159
         BALR  @14,@15                                             0159
*         CPARMRG0=PCBCQN;          /* INIT Q NUMBER PARM TO PCB MGR */
         SLR   CPARMRG0,CPARMRG0                                   0160
         IC    CPARMRG0,PCBCQN(,PCBPTR)                            0160
*       END;                                                       0161
*     ELSE                          /* OTHERWISE, SET Q NUMBER FOR 0162
*                                      THE FREE Q                    */
*       DO;                                                        0162
         B     @RC00153                                            0162
@RF00153 DS    0H                                                  0163
*         CPARMRG0=PCBFREQN;                                       0163
         LA    CPARMRG0,16                                         0163
*       END;                                                       0164
*     CPARMRG1=CPCURPCB;            /* SET REG 1 FOR CALL            */
@RC00153 LR    CPARMRG1,CPCURPCB                                   0165
*     CALL EPPCBMGR;                /* CALL THE PCB MANAGER TO PLACE
*                                      THE PCB ON THE PROPER Q       */
         L     @15,PVTPCB(,PVTPTR)                                 0166
         BALR  @14,@15                                             0166
*     RESPECIFY                                                    0167
*      (CPARMRG0,                                                  0167
*       CPARMRG1) UNRESTRICTED;                                    0167
*   END;                            /* END THE RELATED PCB DO WHILE  */
@DE00146 L     @12,PCBRLPA-1(,PCBPTR)                              0168
         LA    @12,0(,@12)                                         0168
         LTR   @12,@12                                             0168
         BNZ   @DL00146                                            0168
*                                                                  0169
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS SEGMENT PROCESSES THE INPUT PCB ONCE ALL RELATED PCB'S   */
*   /* HAVE BEEN PROCESSED. THE SPCBNORM SUBROUTINE IS CALLED TO     */
*   /* PROCESS THIS PCB. ONCE ALL PCB'S HAVE BEEN PROCESSED, THE     */
*   /* DISPOSITION OF THE REAL FRAME ASSIGNED TO THE VIRTUAL PAGE    */
*   /* REPRESENTED IN THE PCB IS DETERMINED. IF THE INTERNAL FLAG IS */
*   /* SET TO FREE THE REAL FRAME, IT IS FREED BY SENDING IT TO THE  */
*   /* AVAILABLE FRAME QUEUE (AFQ). THE PROPER FRAME COUNT IS ALSO   */
*   /* DECREMENTED. OTHERWISE, THE VIRTUAL PAGE HAS BEEN VALIDATED.  */
*   /* IF THE PAGE HAS BEEN VALIDATED, ALL PGTE AND STORAGE KEY      */
*   /* SETTING HAS BEEN DONE IN SPCBNORM. NOTE THAT THE PAGE MAY     */
*   /* ALREADY BE VALID ON ENTRY TO THIS MODULE. IN THIS CASE, THE   */
*   /* VBN IN THE PCB WAS ZERO AND THE PCBFREAL FLAG WAS OFF CAUSING */
*   /* THE INTERNAL FREE REAL FLAG TO BE TURNED OFF.                 */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0169
*   CPCURPCB=PCBPTR;                /* SET PARM REG FOR CPNORMP      */
         LR    CPCURPCB,PCBPTR                                     0169
*   CALL SPCBNORM;                  /* PROCESS THE PRIMARY PCB       */
         BAL   @14,SPCBNORM                                        0170
*   RESPECIFY                                                      0171
*     CPCURPCB UNRESTRICTED;                                       0171
*   IF CPFREREL='1'B THEN           /* IS FRAME FOR PCB TO BE FREED  */
         TM    CPFREREL(PVTPTR),B'00100000'                        0172
         BNO   @RF00172                                            0172
*     DO;                           /* YES                           */
*       RESPECIFY                                                  0174
*         PFTEPTR RESTRICTED;                                      0174
*       PFTEPTR=PVTPFTP+PCBRBN;     /* OBTAIN THE ADDRESS OF THE PFTE
*                                      ASSIGNED TO THE VIRTUAL PAGE  */
         LH    @12,PCBRBN(,PCBPTR)                                 0175
         N     @12,@CF02180                                        0175
         L     PFTEPTR,PVTPFTP(,PVTPTR)                            0175
         ALR   PFTEPTR,@12                                         0175
*       PFTPCBSI='0'B;              /* TURN OFF THE PCB DEFINED FLAG */
         NI    PFTPCBSI(PFTEPTR),B'11110111'                       0176
*       RESPECIFY                                                  0177
*         PFTEPTR UNRESTRICTED;                                    0177
*       CPHI2BYT=CPINITHI;          /* THE PFTE IS FREED. PARM REG 0
*                                      IS CONSTRUCTED IN THE WORK  0178
*                                      AREA TO SEND THE PFTE TO THE
*                                      FRONT OF THE AFQ              */
         LH    @11,CPINITHI                                        0178
         N     @11,@CF02180                                        0178
         STH   @11,CPHI2BYT(,PVTPTR)                               0178
*       CPRBNPRM=PCBRBN;            /* THE PFTE INDEX IS PLACED IN 0179
*                                      THE LOW 2 BYTES OF REG 0      */
         STH   @12,CPRBNPRM(,PVTPTR)                               0179
*       RESPECIFY                                                  0180
*        (CPARMRG0,                                                0180
*         CPARMRG1) RESTRICTED;                                    0180
*       CPARMRG0=CPENQDEQ;          /* REG 0 IS INITIALIZED FROM THE
*                                      WORK AREA                     */
         L     CPARMRG0,CPENQDEQ(,PVTPTR)                          0181
*       CPARMRG1=PSAAOLD;           /* REG 1 CONTAINS THE ASCB ADDR
*                                      FOR THE CURRENT ADDRESS SPACE */
         L     CPARMRG1,PSAAOLD                                    0182
*       CALL EPFTENDQ;              /* PFTE ENQ/DEQ IS CALLED        */
         L     @15,PVTPFTE(,PVTPTR)                                0183
         BALR  @14,@15                                             0183
*     END;                          /* END FREE FRAME PROCESSING     */
*   ELSE                                                           0185
*     ;                             /* DO NOTHING IF FRAME IS NOT TO
*                                      BE FREED                      */
@RF00172 DS    0H                                                  0186
*                                                                  0186
*   /*****************************************************************/
*   /*                                                               */
*   /* FINALLY, THE DISPOSITION OF THE INPUT PCB IS DETERMINED AND   */
*   /* THE PCB SENT TO THE FREE Q IF NECESSARY. EXIT IS THEN MADE TO */
*   /* THE CALLER. IF THE PCB IS TO BE KEPT, PIOCP IS SCHEDULED AS   */
*   /* DISCUSSED IN THE FIRST SEGMENT. EXIT IS THEN MADE TO THE      */
*   /* CALLER.                                                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0186
*LBFINPCB:                                                         0186
*   IF CPKEEPCB='0'B THEN           /* IF CPNORMP INDICATED THE PCB
*                                      IS TO BE FREED, IT IS FREED   */
LBFINPCB TM    CPKEEPCB(PVTPTR),B'01000000'                        0186
         BNZ   @RF00186                                            0186
*     DO;                                                          0187
*       CPARMRG0=PCBFREQN;          /* SET Q NUMBER PARM TO FREE Q   */
         LA    CPARMRG0,16                                         0188
*       CPARMRG1=PCBPTR;            /* PUT PCB ADDR IN REG 1         */
         LR    CPARMRG1,PCBPTR                                     0189
*       CALL EPPCBMGR;              /* THE PCB MANAGER IS CALLED     */
         L     @15,PVTPCB(,PVTPTR)                                 0190
         BALR  @14,@15                                             0190
*     END;                                                         0191
*   ELSE                            /* THE PCB IS TO BE KEPT, THE VBN
*                                      IS SET TO ZERO, AND PCBFREAL
*                                      SET OFF. THE SCHEDULE       0192
*                                      SUBROUTINE OF PIOP CALLED TO
*                                      SCHEDULE PIOCP TO RUN LATER.  */
*     DO;                                                          0192
         B     @RC00186                                            0192
@RF00186 DS    0H                                                  0193
*       PCBVBN=0;                   /* ZERO THE PCBVBN SO PFTE AND 0193
*                                      PGTE PROCESSING WON'T BE    0193
*                                      ATTEMPTED AGAIN BY THIS MODULE*/
         SLR   @12,@12                                             0193
         STH   @12,PCBVBN(,PCBPTR)                                 0193
*       PCBFREAL='0'B;              /* THIS FLAG NO LONGER HAS     0194
*                                      MEANING                       */
         NI    PCBFREAL(PCBPTR),B'01111111'                        0194
*       CPARMRG1=PCBPTR;            /* SET UP PARM FOR SCHED SUBRTN. */
         LR    CPARMRG1,PCBPTR                                     0195
*       CALL EPSCHED;               /* CALL SCHEDULE SUBRTN OF PIOP  */
         L     @15,PVTPOPBR(,PVTPTR)                               0196
         BALR  @14,@15                                             0196
*       RESPECIFY                                                  0197
*        (CPARMRG0,                                                0197
*         CPARMRG1) UNRESTRICTED;                                  0197
*     END;                                                         0198
*   CPRETREG=CPSBRET2;              /* RESTORE CALLER'S RETURN ADDR
*                                      AND RETURN TO THE CALLER      */
@RC00186 L     CPRETREG,CPSBRET2(,PVTPTR)                          0199
*   END;                            /* END SUBNORML INTERNAL PROC    */
@EL00003 DS    0H                                                  0200
@EF00003 DS    0H                                                  0200
@ER00003 BR    @14                                                 0200
*                                                                  0201
*   /*****************************************************************/
*   /*                                                               */
*   /* THE I/O ERROR SUBROUTINE IS CALLED BY THE SQSEARCH SUBROUTINE.*/
*   /* IT GETS ALL PCB'S FOR WHICH A PAGE-IN HAS NOT COMPLETED       */
*   /* SUCCESSFULLY PROCESSED BY CALLING THE SPCBERR SUBROUTINE.     */
*   /* SUBIOERR WORKS A LITTLE DIFFERENTLY THAN SUBNORML IN THAT     */
*   /* RELATED PCB'S THAT CAN NOT BE PROCESSED ARE RETAINED AND NOT  */
*   /* ENQUEUED SEPARATELY. THE REAL FRAME ASSOCIATED WITH PAGE-IN   */
*   /* OPERATION IS NOT FREED UNTIL THE LAST PCB HAS BEEN PROCESSED. */
*   /* INPUT TO THIS SUBROUTINE IS THE ADDRESS OF THE QUEUED OR      */
*   /* PRIMARY PCB TO BE PROCESSED. THIS FIRST SEGMENT PROCESSES ALL */
*   /* RELATED PCB'S, LEAVING THOSE THAT WERE NOT FOR THE CURRENT    */
*   /* ADDRESS SPACE OR THAT SPCBERR COULD NOT PROCESS QUEUED AS     */
*   /* RELATED PCB'S. PROCESSING CONTINUES WITH THE SECOND SEGMENT   */
*   /* ONCE AN ATTEMPT HAS BEEN MADE TO PROCESS EACH RELATED PCB.    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0201
*SUBIOERR:                                                         0201
*   PROC OPTIONS(NOSAVE NOSAVEAREA);                               0201
SUBIOERR DS    0H                                                  0202
*   CPSBRET2=CPRETREG;              /* SAVE RETURN ADDRESS           */
         ST    CPRETREG,CPSBRET2(,PVTPTR)                          0202
*   CPREVREL=PCBPTR;                /* INIT BASE PCB ADDR FOR RELATED
*                                      LOOP THAT FOLLOWS             */
         STCM  PCBPTR,7,CPREVREL(PVTPTR)                           0203
*   DO WHILE(CPREVREL->PCBRLPA^=0); /* ATTEMPT TO PROCESS EACH     0204
*                                      RELATED PCB.                  */
         B     @DE00204                                            0204
@DL00204 DS    0H                                                  0205
*     RESPECIFY                                                    0205
*       CPCURPCB RESTRICTED;                                       0205
*     CPCURPCB=CPREVREL->PCBRLPA;   /* GET ADDR OF PCB TO BE       0206
*                                      PROCESSED                     */
         L     @12,CPREVREL-1(,PVTPTR)                             0206
         L     CPCURPCB,PCBRLPA-1(,@12)                            0206
         LA    CPCURPCB,0(,CPCURPCB)                               0206
*     IF CPCURPCB->PCBASCB=PCBASCB THEN/* IS RELATED PCB FOR SAME  0207
*                                      ADDRESS SPACE                 */
         CLC   PCBASCB(4,CPCURPCB),PCBASCB(PCBPTR)                 0207
         BNE   @RF00207                                            0207
*       DO;                         /* PROCESS ONLY PCB'S FOR THE  0208
*                                      CURRENT ADDRESS               */
*         CALL SPCBERR;             /* PROCESS THE RELATED PCB       */
         BAL   @14,SPCBERR                                         0209
*         IF CPKEEPCB='0'B THEN     /* IF THE PCB CAN BE FREED,    0210
*                                      REMOVE THE PCB FROM THE     0210
*                                      RELATED CHAIN AND RETURN IT TO
*                                      THE FREE Q                    */
         TM    CPKEEPCB(PVTPTR),B'01000000'                        0210
         BNZ   @RF00210                                            0210
*           DO;                                                    0211
*             CPREVREL->PCBRLPA=CPCURPCB->PCBRLPA;                 0212
         L     @12,CPREVREL-1(,PVTPTR)                             0212
         MVC   PCBRLPA(3,@12),PCBRLPA(CPCURPCB)                    0212
*             CPCURPCB->PCBFQP=CPRPFRST;/* RESET THE FIRST WORD OF 0213
*                                      THE RELATED PCB TO ZERO WITH A
*                                      Q NUMBER INDICATING THE     0213
*                                      UNQUEUED STATE                */
         L     @12,CPRPFRST                                        0213
         ST    @12,PCBFQP(,CPCURPCB)                               0213
*             RESPECIFY                                            0214
*              (CPARMRG0,                                          0214
*               CPARMRG1) RESTRICTED;                              0214
*             CPARMRG0=PCBFREQN;                                   0215
         LA    CPARMRG0,16                                         0215
*             CPARMRG1=CPCURPCB;                                   0216
         LR    CPARMRG1,CPCURPCB                                   0216
*             CALL EPPCBMGR;        /* CALL THE PCB MANAGER TO SEND
*                                      PCB TO THE FREE Q             */
         L     @15,PVTPCB(,PVTPTR)                                 0217
         BALR  @14,@15                                             0217
*             RESPECIFY                                            0218
*              (CPARMRG0,                                          0218
*               CPARMRG1) UNRESTRICTED;                            0218
*           END;                                                   0219
*         ELSE                      /* OTHERWISE, UPDATE THE BASE PCB
*                                      PTR FOR THE RELATED CHAIN     */
*           DO;                                                    0220
         B     @RC00210                                            0220
@RF00210 DS    0H                                                  0221
*             CPREVREL=CPCURPCB;                                   0221
         STCM  CPCURPCB,7,CPREVREL(PVTPTR)                         0221
*           END;                                                   0222
*       END;                                                       0223
*     ELSE                          /* IF RELATED PCB IS FOR       0224
*                                      DIFFERENT ADDRESS SPACE,    0224
*                                      UPDATE THE BASE PCB PTR FOR 0224
*                                      THE RELATED CHAIN             */
*       DO;                                                        0224
         B     @RC00207                                            0224
@RF00207 DS    0H                                                  0225
*         CPREVREL=CPCURPCB;                                       0225
         STCM  CPCURPCB,7,CPREVREL(PVTPTR)                         0225
*       END;                                                       0226
*   END;                            /* END RELATED PCB DO WHILE LOOP */
@RC00207 DS    0H                                                  0227
@DE00204 L     @12,CPREVREL-1(,PVTPTR)                             0227
         L     @12,PCBRLPA-1(,@12)                                 0227
         LA    @12,0(,@12)                                         0227
         LTR   @12,@12                                             0227
         BNZ   @DL00204                                            0227
*                                                                  0228
*   /*****************************************************************/
*   /*                                                               */
*   /* THE SECOND SEGMENT OF SUBIOERR GETS THE INPUT PCB PROCESSED BY*/
*   /* CALLING SPCBERR. IF ALL PCBS ASSOCIATED WITH THE PAGE-IN      */
*   /* OPERATION HAVE BEEN PROCESSED, THE PFTE FOR THE REAL FRAME    */
*   /* ASSOCIATED WITH THE PAGE-IN IS READY TO BE FREED. IF THE FIX  */
*   /* COUNT IS ZERO AND THE DEFERED RELEASE FLAG IS SET, THE DEFERED*/
*   /* RELEASE ENTRY POINT OF IEAVRELS IS CALLED. THE LOCAL LOCK     */
*   /* REQUIRED BY THIS CALL IS GUARANTEED FOR THIS FUNCTION BECAUSE */
*   /* THE SPCBERR SUBRTN. DOES NOT PROCESS FIX PCB'S UNTIL THE LOCAL*/
*   /* LOCK IS HELD. THE PFTE IS THEN FREED REGARDLESS OF THE ABOVE  */
*   /* CONDITIONS. OTHERWISE, THE INPUT PCB IS LEFT QUEUED OR ANY    */
*   /* RELATED PCB'S ARE ENQUEUED TO THE SAME I/O ACTIVE Q AS THE    */
*   /* INPUT PCB (THE INPUT PCB BEING SENT TO THE FREE Q) AND PIOCP  */
*   /* SCHEDULED TO RUN LATER BY CALLING THE SCHEDULE SUBROUTINE OF  */
*   /* PIOP AS DISCUSSED IN SUBNORML.                                */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0228
*   CPCURPCB=PCBPTR;                /* INIT PARM REG FOR SPCBERR     */
         LR    CPCURPCB,PCBPTR                                     0228
*   CALL SPCBERR;                   /* PROCESS THE INPUT PCB         */
         BAL   @14,SPCBERR                                         0229
*   RESPECIFY                                                      0230
*     CPCURPCB UNRESTRICTED;                                       0230
*   IF CPKEEPCB='0'B THEN           /* IF THE INPUT PCB IS TO BE   0231
*                                      FREED AND THERE ARE NO RELATED
*                                      PCB'S, THE REAL FRAME IS FREED*/
         TM    CPKEEPCB(PVTPTR),B'01000000'                        0231
         BNZ   @RF00231                                            0231
*     DO;                                                          0232
*       IF PCBRLPA=0 THEN           /* DO RELATED PCBS STILL EXIST   */
         L     @12,PCBRLPA-1(,PCBPTR)                              0233
         LA    @12,0(,@12)                                         0233
         LTR   @12,@12                                             0233
         BNZ   @RF00233                                            0233
*         DO;                       /* YES                           */
*           RESPECIFY                                              0235
*             PFTEPTR RESTRICTED;                                  0235
*           PFTEPTR=PVTPFTP+PCBRBN; /* GET PFTE ADDR, USING PCBRBN   */
         LH    PFTEPTR,PCBRBN(,PCBPTR)                             0236
         N     PFTEPTR,@CF02180                                    0236
         AL    PFTEPTR,PVTPFTP(,PVTPTR)                            0236
*           PFTPCBSI='0'B;          /* TURN OFF PCB DEFINED FLAG     */
         NI    PFTPCBSI(PFTEPTR),B'11110111'                       0237
*           IF PFTFXCT=0 THEN       /* IS FIX COUNT IN PFTE ZERO     */
         LH    @12,PFTFXCT(,PFTEPTR)                               0238
         LTR   @12,@12                                             0238
         BNZ   @RF00238                                            0238
*             DO;                   /* FOR PFTES WITH ZERO FIX COUNTS
*                                      CHECK FOR DEFERED RELEASE     */
*               IF PFTDFRLS='1'B THEN/* IS FRAME IN DEFERRED RELEASE
*                                      CONDITION                     */
         TM    PFTDFRLS(PFTEPTR),B'10000000'                       0240
         BNO   @RF00240                                            0240
*                 DO;               /* YES                           */
*                   RESPECIFY                                      0242
*                    (CPARMRG0,                                    0242
*                     CPARMRG1) RESTRICTED;                        0242
*                   CPARMRG1=PFTVBN*256;/* SET UP INPUT VSA FOR    0243
*                                      IEAVRELF                      */
         LH    CPARMRG1,PFTVBN(,PFTEPTR)                           0243
         N     CPARMRG1,@CF02180                                   0243
         SLA   CPARMRG1,8                                          0243
*                   CALL EPDEFREL;  /* CALL DEFERED RELEASE PROCESSOR*/
         L     @15,PVTPRELF(,PVTPTR)                               0244
         BALR  @14,@15                                             0244
*                   RCACSECT=RCACCOMP;/* RESET CSECT ID IN RCA       */
         MVI   RCACSECT(RCAPTR),X'14'                              0245
*                 END;                                             0246
*               ELSE                                               0247
*                 ;                 /* FRAME NOT DEFERRED RELEASED   */
@RF00240 DS    0H                                                  0248
*             END;                                                 0248
*           ELSE                    /* IF PFTE FIX COUNT NOT ZERO, 0249
*                                      RECORD THE ERROR AND FORCE THE
*                                      COUNT TO ZERO                 */
*             DO;                                                  0249
         B     @RC00238                                            0249
@RF00238 DS    0H                                                  0250
*               RCARCRD='0A'X;      /* SET REASON CODE               */
         MVI   RCARCRD(RCAPTR),X'0A'                               0250
*               GEN(ABEND X'C0D',,,SYSTEM) ;                       0251
         ABEND X'C0D',,,SYSTEM
*               PFTFXCT=0;                                         0252
         SLR   @12,@12                                             0252
         STH   @12,PFTFXCT(,PFTEPTR)                               0252
*               IF PFTVBN>=PVTLCSA THEN/* IS FRAME IN COMMON AREA    */
         CLC   PFTVBN(2,PFTEPTR),PVTLCSA(PVTPTR)                   0253
         BL    @RF00253                                            0253
*                 PVTCOMFX=PVTCOMFX-1;/* YES, A COMMON AREA FRAME  0254
*                                      DECREMENT COMMON AREA COUNT 0254
*                                                            @YA01572*/
         LH    @12,PVTCOMFX(,PVTPTR)                               0254
         BCTR  @12,0                                               0254
         STH   @12,PVTCOMFX(,PVTPTR)                               0254
*               ELSE                /* NOT COMMON AREA               */
*                 RSMCNTFX=RSMCNTFX-1;/* DECREMENT COUNT FOR THIS  0255
*                                      ADDRESS SPACE         @YA01572*/
         B     @RC00253                                            0255
@RF00253 LH    @12,RSMCNTFX(,RSMHDPTR)                             0255
         BCTR  @12,0                                               0255
         STH   @12,RSMCNTFX(,RSMHDPTR)                             0255
*               PVTCNTFX=PVTCNTFX-1;/* DECREMENT SYSTEM COUNT      0256
*                                                            @YA01572*/
@RC00253 LH    @12,PVTCNTFX(,PVTPTR)                               0256
         BCTR  @12,0                                               0256
         STH   @12,PVTCNTFX(,PVTPTR)                               0256
*             END;                                                 0257
*           PFTPGID=0;              /* ZERO ASID, VBN IN PFTE TO   0258
*                                      PREVENT RECLAIM               */
@RC00238 SLR   @12,@12                                             0258
         ST    @12,PFTPGID(,PFTEPTR)                               0258
*           RESPECIFY                                              0259
*             PFTEPTR UNRESTRICTED;                                0259
*           CPHI2BYT=CPINITHI;      /* INIT HIGH 2 BYTES OF REG 0  0260
*                                      PARM FOR PFTE ENQ/DEQ TO PLACE
*                                      PFTE ON FRONT OF AFQ          */
         LH    @12,CPINITHI                                        0260
         N     @12,@CF02180                                        0260
         STH   @12,CPHI2BYT(,PVTPTR)                               0260
*           CPRBNPRM=PCBRBN;        /* LOW 2 BYTES IS PFTE INDEX     */
         LH    @12,PCBRBN(,PCBPTR)                                 0261
         N     @12,@CF02180                                        0261
         STH   @12,CPRBNPRM(,PVTPTR)                               0261
*           CPARMRG0=CPENQDEQ;      /* LOAD INITIALIZED PARM FIELD 0262
*                                      INTO REGISTER 0               */
         L     CPARMRG0,CPENQDEQ(,PVTPTR)                          0262
*           CPARMRG1=PSAAOLD;       /* REG 1 IS CURRENT ASCB ADDR    */
         L     CPARMRG1,PSAAOLD                                    0263
*           CALL EPFTENDQ;          /* CALL PFTE ENQ/DEQ TO PLACE  0264
*                                      PFTE ON FRONT OF AFQ          */
         L     @15,PVTPFTE(,PVTPTR)                                0264
         BALR  @14,@15                                             0264
*         END;                      /* END FREE FRAME PROCESSING     */
*       ELSE                        /* IF RELATED PCB'S STILL REMAIN,
*                                      THE FIRST IS ENQUEUED TO THE Q
*                                      OF THE INPUT PCB AND THE INPUT
*                                      PCB FREED BELOW               */
*         DO;                                                      0266
         B     @RC00233                                            0266
@RF00233 DS    0H                                                  0267
*           PCBVBN=0;               /* THE VBN IN THE INPUT PCB IS 0267
*                                      SET TO ZERO TO PREVENT HAVING
*                                      TWO PCB'S WITH THE SAME VBN ON
*                                      A Q AT THE SAME TIME.         */
         SLR   @12,@12                                             0267
         STH   @12,PCBVBN(,PCBPTR)                                 0267
*           CPARMRG0=PCBCQN;        /* SET UP PARM FOR PCB MGR       */
         SLR   CPARMRG0,CPARMRG0                                   0268
         IC    CPARMRG0,PCBCQN(,PCBPTR)                            0268
*           CPARMRG1=PCBRLPA;       /* FIRST RELATED TO BE ENQUEUED  */
         L     CPARMRG1,PCBRLPA-1(,PCBPTR)                         0269
         LA    CPARMRG1,0(,CPARMRG1)                               0269
*           CALL EPPCBMGR;          /* CALL THE PCB MANAGER          */
         L     @15,PVTPCB(,PVTPTR)                                 0270
         BALR  @14,@15                                             0270
*           CALL EPSCHED;           /* CALL SCHEDULE SUBRTN OF PIOP.
*                                      REG 1 STILL HAS PCB ADDR    0271
*                                      BECAUSE PCB MGR DOESN'T     0271
*                                      DESTROY IT.                   */
         L     @15,PVTPOPBR(,PVTPTR)                               0271
         BALR  @14,@15                                             0271
*         END;                                                     0272
*     END;                          /* END FREE INPUT PCB PROCESSING */
*   ELSE                            /* IF THE INPUT PCB CAN NOT BE 0274
*                                      FREED, PIOCP MUST BE SCHEDULED*/
*     DO;                                                          0274
         B     @RC00231                                            0274
@RF00231 DS    0H                                                  0275
*       CPARMRG1=PCBPTR;                                           0275
         LR    CPARMRG1,PCBPTR                                     0275
*       CALL EPSCHED;               /* CALL SCHEDULE SUBRTN OF PIOP  */
         L     @15,PVTPOPBR(,PVTPTR)                               0276
         BALR  @14,@15                                             0276
*       GOTO LBEXIT;                /* SKIP FREEING OF INPUT PCB     */
         B     LBEXIT                                              0277
*     END;                                                         0278
*   CPARMRG0=PCBFREQN;              /* THE INPUT PCB IS NOW FREED BY
*                                      CALLING THE PCB MANAGER     0279
*                                      BECAUSE CPKEEPCB=0.           */
@RC00231 LA    CPARMRG0,16                                         0279
*   CPARMRG1=PCBPTR;                /* SET UP PARM REG WITH PCB ADDR */
         LR    CPARMRG1,PCBPTR                                     0280
*   CALL EPPCBMGR;                  /* CALL THE PCB MANAGER          */
         L     @15,PVTPCB(,PVTPTR)                                 0281
         BALR  @14,@15                                             0281
*   RESPECIFY                                                      0282
*    (CPARMRG0,                                                    0282
*     CPARMRG1) UNRESTRICTED;                                      0282
*LBEXIT:                                                           0283
*   CPRETREG=CPSBRET2;              /* RESTORE RETURN ADDR AND EXIT
*                                      TO THE CALLER                 */
LBEXIT   L     CPRETREG,CPSBRET2(,PVTPTR)                          0283
*   END;                            /* END SUBIOERR INT PROC         */
@EL00004 DS    0H                                                  0284
@EF00004 DS    0H                                                  0284
@ER00004 BR    @14                                                 0284
*                                                                  0285
*   /*****************************************************************/
*   /*                                                               */
*   /* THE SPCBNORM SUBROUTINE IS CALLED FOR EACH RELATED OR QUEUED  */
*   /* PCB FOUND BY THE SUBNORML SUBROUTINE. SPCBNORM'S JOB IS TO    */
*   /* VALIDATE A PAGE AND SET ITS PROTECT KEY IN REAL STORAGE. NOTE */
*   /* THAT THIS PROCESSING IS SKIPPED IF THE PCBVBN IS ZERO OR IF   */
*   /* THE PAGE IS ALREADY VALID. THE PAGE COULD ALREADY BE VALID ON */
*   /* ENTRY TO THIS MODULE OR WAS VALIDATED WHEN PROCESSING ANOTHER */
*   /* PCB RELATED TO THE PRIMARY PCB. THE SNOTIFY SUBROUTINE IS THEN*/
*   /* CALLED. THE ADDRESS OF THE PCB TO BE PROCESSED BY SPCBNORM IS */
*   /* PASSED IN REG 8 (CPCURPCB). NOTE THAT THE ADDR OF THE PRIMARY */
*   /* PCB BEING PROCESSED BY THE MODULE IS IN REG 7 AND MUST NOT BE */
*   /* DISTURBED. OTHER REGISTERS WHICH MUST NOT CHANGE ACROSS THIS  */
*   /* SUBROUTINE ARE 2, 3, AND 10.                                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0285
*SPCBNORM:                                                         0285
*   PROC OPTIONS(NOSAVE NOSAVEAREA);                               0285
SPCBNORM DS    0H                                                  0286
*   CPSBRET3=CPRETREG;              /* SAVE THE RETURN ADDRESS       */
         ST    CPRETREG,CPSBRET3(,PVTPTR)                          0286
*   RESPECIFY                                                      0287
*     CPCURPCB RESTRICTED;                                         0287
*   RESPECIFY                                                      0288
*     PCB BASED(CPCURPCB);          /* THE PCB BASE PTR FOR THIS   0288
*                                      SUBROUTINE IS THE INPUT     0288
*                                      PARAMETER REG, NOT THE      0288
*                                      STANDARD PCB REG FOR THE    0288
*                                      MODULE                        */
*   IF PCBFREAL='0'B THEN           /* IF THE FREE REAL FLAG IS NOT
*                                      SET IN THE PCB, PERFORM THE 0289
*                                      NEXT TWO TESTS BEFORE       0289
*                                      VALIDATING THE PAGE           */
         TM    PCBFREAL(CPCURPCB),B'10000000'                      0289
         BNZ   @RF00289                                            0289
*     DO;                                                          0290
*       CPFREREL='0'B;              /* THE INTERNAL FREE REAL FLAG IS
*                                      RESET TO PREVENT SUBNORML FROM
*                                      FREEING THE REAL FRAME        */
         NI    CPFREREL(PVTPTR),B'11011111'                        0291
*       IF PCBVBN^=0 THEN           /* IF THE PCB VBN IS NOT ZERO, 0292
*                                      THEN DO A LRA ON THE VIRTUAL
*                                      ADDR DEVELOPED FROM THIS VBN
*                                      TO SEE IF THE PAGE HAS ALREADY
*                                      BEEN VALIDATED                */
