*;                                                              01S0100
@DATA    DS    0H
@CH00110 DC    H'0'
@CH00049 DC    H'1'
@CH00124 DC    H'4'
@CH00150 DC    H'5'
@CH00085 DC    H'6'
@CH00105 DC    H'12'
@CH00195 DC    H'63'
@SM00205 MVC   MSGDISP(0),DSPTXT(DSPIDPTR)
@SM00207 MVC   MSGL2TXT(0),VLINETXT-VLINENT(@10)
@SM00209 MVC   0(0,@03),BLANKS
@DATD    DSECT
         DS    0F
@SA00001 DS    18F
@PC00001 DS    5F
@SA00002 DS    0F
         DS    15F
@AL00001 DS    5A
@TF00001 DS    F
IEFAB4B0 CSECT
         DS    0F
@CF00154 DC    F'58'
@CF00156 DC    F'68'
@CF00204 DC    F'-8'
@CF00196 DC    F'-6'
@CF00197 DC    F'-4'
@CF00198 DC    F'-2'
@DATD    DSECT
         DS    0D
FLAGLNTH DS    F
