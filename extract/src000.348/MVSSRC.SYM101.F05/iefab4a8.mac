         TITLE 'IEFAB4A8 - VOLUME RELEASE                              *
                        '
IEFAB4A8 CSECT ,                                                01S0001
@MAINENT DS    0H                                               01S0001
         USING *,@15                                            01S0001
         B     @PROLOG                                          01S0001
         DC    AL1(16)                                          01S0001
         DC    C'IEFAB4A8  73.325'                              01S0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                  01S0001
         BALR  @12,0                                            01S0001
@PSTART  DS    0H                                               01S0001
         USING @PSTART,@12                                      01S0001
         L     @00,@SIZDATD                                     01S0001
         MODESET EXTKEY=SCHED,SAVEKEY=(2)
         GSPACE  R,LV=(0)
         LR    @11,@01                                          01S0001
         USING @DATD,@11                                        01S0001
         ST    @13,@SA00001+4                                   01S0001
         STC   @02,@OLDKEY                                      01S0001
         MODESET KEYADDR=(2)
         LM    @00,@02,20(@13)                                  01S0001
         ST    @11,8(,@13)                                      01S0001
         LR    @13,@11                                          01S0001
         MODESET EXTKEY=ZERO
         MVC   @PC00001(20),0(@01)                              01S0001
         MODESET EXTKEY=SCHED
*RETCODE = ZERO;                       /*INIT RETURN CODE AREA       */
         SLR   @10,@10                                          01S0043
         ST    @10,RETCODE                                      01S0043
*IF VLRGNDEQ = YES THEN                /*GENERIC DEQ REQUESTED       */
         L     @10,@PC00001                                     01S0044
         TM    VLRGNDEQ(@10),B'10000000'                        01S0044
         BNO   @RF00044                                         01S0044
*  DO;                                 /*RELEASE ALL VOLS FOR STEP   */
*    GNDQLIST(1:GNENTLN) =             /*MOVE DEQ LIST SKELETON.     */
*       GNDQFMT(1:GNENTLN);                                     01S0046
         L     @10,GNENTLN                                      01S0046
         BCTR  @10,0                                            01S0046
         EX    @10,@SM01094                                     01S0046
*    RFY (REG0,REG1,REG2) RSTD;                                 01S0047
*    REG1 = ADDR(GNDQLIST);            /*SET ADDR FOR EXECUTE FORM   */
         LA    REG1,GNDQLIST                                    01S0048
*    REG2 = DQTCBP;                    /*TCB PTR FOR DIR DEQ @YM03580*/
         L     @10,@PC00001+8                                   01S0049
         L     REG2,DQTCBP(,@10)                                01S0049
*    GEN(DEQ MF=(E,(1)),TCB=(2),GENERIC=YES,RET=HAVE)           01S0050
*        REFS(REG1,REG2);              /*DO GENERIC DEQ FOR VOLS     */
         DEQ MF=(E,(1)),TCB=(2),GENERIC=YES,RET=HAVE
*    RFY (REG0,REG1,REG2) UNRSTD;                               01S0051
*  END;                                                         01S0052
*ELSE                                                           01S0053
*  DO;                                 /*DETERMINE VOLS TO BE REL'D  */
         B     @RC00044                                         01S0053
@RF00044 DS    0H                                               01S0054
*/********************************************************************/
*/*                                                                  */
*/* SEGMENT NAME - SCNVRLST                                          */
*/*                                                                  */
*/* DESCRIPTIVE NAME - SCAN VOLUME RELEASE LIST                      */
*/*                                                                  */
*/* FUNCTION -                                                       */
*/*   THIS ROUTINE SCANS THE VOLUME RELEASE LIST FOR DUPLICATE OR    */
*/*   INVALID ENTRIES. EACH SUCH ENTRY IS FLAGGED IN THE FLAG BYTE   */
*/*   (VRLFLAGS) OF THAT ENTRY.                                      */
*/*                                                                  */
*/* ENTRY - IN-LINE SEGMENT                                          */
*/*   PURPOSE - TO IDENTIFY DUPLICATE/INVALID ENTRIES IN VRLIST      */
*/*   INPUT -                                                        */
*/*     VRLSTPTR - POINTER TO VOLUME RELEASE LIST                    */
*/*     VRLIST - VOLUME RELEASE LIST (SEE INPUT SECTION OF A4A8DATA  */
*/*              FOR FORMAT OF LIST)                                 */
*/* EXIT -                                                           */
*/*   CONDITION - SCAN COMPLETE                                      */
*/*     OUTPUT -                                                     */
*/*       VRLDUPE - ON IN ANY DUPLICATE/INVALID ENTRY                */
*/*     RETURN CODE - NONE                                           */
*/********************************************************************/
*DO ENTCTR = 1 TO VRLENTNO;            /*CHECK EACH LIST ENTRY       */
         LA    @10,1                                            02S0054
         B     @DE00054                                         02S0054
@DL00054 DS    0H                                               02S0055
*  IF VRLDUPE(ENTCTR) = NO THEN        /*ENTRY NOT ALREADY MARKED    */
         LR    @15,@10                                          02S0055
         SLA   @15,3                                            02S0055
         L     @14,@PC00001+4                                   02S0055
         L     @14,VRLSTPTR(,@14)                               02S0055
         ST    @15,@TF00001                                     02S0055
         ALR   @15,@14                                          02S0055
         TM    VRLDUPE-8(@15),B'01000000'                       02S0055
         BNZ   @RF00055                                         02S0055
*    IF VRLSERNO(ENTCTR,1) ^= ZERO &   /*NON-ZERO ENTRY              */
*        VRLSERNO(ENTCTR) ^= BLANKS &  /*NON-BLANK ENTRY             */
*        VRLSERNO(ENTCTR,1) ^= TSCRSER THEN /*NOT A TAPE SCRATCH SER */
         L     @15,@TF00001                                     02S0056
         SLR   @09,@09                                          02S0056
         IC    @09,VRLSERNO-8(@15,@14)                          02S0056
         CH    @09,@CH00121                                     02S0056
         BE    @RF00056                                         02S0056
         LR    @15,@14                                          02S0056
         AL    @15,@TF00001                                     02S0056
         CLC   VRLSERNO-8(6,@15),@CC00099                       02S0056
         BE    @RF00056                                         02S0056
         AL    @14,@TF00001                                     02S0056
         CLI   VRLSERNO-8(@14),X'FF'                            02S0056
         BE    @RF00056                                         02S0056
*      DO SCNCTR = ENTCTR+1 TO VRLENTNO; /*SEE IF SER IS DUPED       */
         AH    @10,@CH00049                                     02S0057
         B     @DE00057                                         02S0057
@DL00057 DS    0H                                               02S0058
*        IF VRLSERNO(SCNCTR) =                                  02S0058
*            VRLSERNO(ENTCTR) THEN     /*DUPLICATE SER IN LIST       */
         SLA   @10,3                                            02S0058
         L     @15,@PC00001+4                                   02S0058
         L     @15,VRLSTPTR(,@15)                               02S0058
         L     @14,ENTCTR                                       02S0058
         SLA   @14,3                                            02S0058
         ST    @10,@TF00001                                     02S0058
         ALR   @10,@15                                          02S0058
         ALR   @14,@15                                          02S0058
         CLC   VRLSERNO-8(6,@10),VRLSERNO-8(@14)                02S0058
         BNE   @RF00058                                         02S0058
*          DO;                                                  02S0059
*            VRLDUPE(SCNCTR) = ON;     /*FLAG DUPLICATE ENTRY        */
         LR    @10,@15                                          02S0060
         AL    @10,@TF00001                                     02S0060
         OI    VRLDUPE-8(@10),B'01000000'                       02S0060
*            VRLRELNO = VRLRELNO - ONE;/*DECRECEMENT # TO BE RELEASED*/
         L     @10,VRLRELNO(,@15)                               02S0061
         BCTR  @10,0                                            02S0061
         ST    @10,VRLRELNO(,@15)                               02S0061
*          END;                                                 02S0062
*      END;                                                     02S0063
@RF00058 LA    @10,1                                            02S0063
         AL    @10,SCNCTR                                       02S0063
@DE00057 ST    @10,SCNCTR                                       02S0063
         L     @15,@PC00001+4                                   02S0063
         L     @01,VRLSTPTR(,@15)                               02S0063
         C     @10,VRLENTNO(,@01)                               02S0063
         BNH   @DL00057                                         02S0063
*    ELSE                              /*INVALID SERIAL IN ENTRY     */
*      DO;                                                      02S0064
         B     @RC00056                                         02S0064
@RF00056 DS    0H                                               02S0065
*        VRLDUPE(ENTCTR) = ON;         /*FLAG INVALID ENTRY          */
         L     @10,ENTCTR                                       02S0065
         SLA   @10,3                                            02S0065
         L     @15,@PC00001+4                                   02S0065
         L     @15,VRLSTPTR(,@15)                               02S0065
         ALR   @10,@15                                          02S0065
         OI    VRLDUPE-8(@10),B'01000000'                       02S0065
*        VRLRELNO = VRLRELNO - ONE;    /*DECREMENT # TO BE RELEASED  */
         L     @10,VRLRELNO(,@15)                               02S0066
         BCTR  @10,0                                            02S0066
         ST    @10,VRLRELNO(,@15)                               02S0066
*      END;                                                     02S0067
*  ELSE;                               /*ENTRY ALREADY MARKED        */
@RF00055 DS    0H                                               02S0069
*END;                                                           02S0069
@RC00055 LA    @10,1                                            02S0069
         AL    @10,ENTCTR                                       02S0069
@DE00054 ST    @10,ENTCTR                                       02S0069
         L     @15,@PC00001+4                                   02S0069
         L     @01,VRLSTPTR(,@15)                               02S0069
         C     @10,VRLENTNO(,@01)                               02S0069
         BNH   @DL00054                                         02S0069
*    DO LISTCTR = 1 TO VRLENTNO;       /*SEE IF ANY VOLS STILL ALOC'D*/
         LA    @10,1                                            01S0070
         B     @DE00070                                         01S0070
@DL00070 DS    0H                                               01S0071
*      IF VRLDUPE(LISTCTR) = NO THEN   /*ENTRY NOT DUPLICATE         */
         SLA   @10,3                                            01S0071
         L     @15,@PC00001+4                                   01S0071
         L     @15,VRLSTPTR(,@15)                               01S0071
         ST    @10,@TF00001                                     01S0071
         ALR   @10,@15                                          01S0071
         TM    VRLDUPE-8(@10),B'01000000'                       01S0071
         BNZ   @RF00071                                         01S0071
*        IF VRLCHKMA(LISTCTR) = YES THEN /*NEED TO CHECK ALOC'D VOLS */
         AL    @15,@TF00001                                     01S0072
         TM    VRLCHKMA-8(@15),B'00100000'                      01S0072
         BNO   @RF00072                                         01S0072
*          DO;                                                  01S0073
*/********************************************************************/
*/*                                                                  */
*/* SEGMENT NAME - CKMALCDV                                          */
*/*                                                                  */
*/* DESCRIPTIVE NAME - CHECK FOR MULTIPLY ALLOCATED VOLUME           */
*/*                                                                  */
*/* FUNCTION -                                                       */
*/*   THIS ROUTINE DETERMINES IF THE VOLUME IN THE CURRENT ENTRY     */
*/*   OF THE VOLUME RELEASE LIST IS STILL REQUIRED BY THE JOB STEP.  */
*/*   THE ROUTINE WILL COMPARE THE INPUT VOL SER TO ALL VOLUMES      */
*/*   STILL ALLOCATED TO THE JOB STEP. IF THE VOLUME IS NOT          */
*/*   ALLOCATED TO ANOTHER REQUEST AN INDICATOR IS SET IN THE        */
*/*   LIST THAT IT CAN BE RELEASED.                                  */
*/*                                                                  */
*/* ENTRY - IN-LINE SEGMENT                                          */
*/*   PURPOSE - DETERMINE IF VOLUME CAN BE RELEASED                  */
*/*   INPUT -                                                        */
*/*     LISTCTR - CURRENT POSITION IN VOLUME RELEASE LIST            */
*/*     SIOTPTR - PTR TO SIOT OF ALLOCATED REQUESTS                  */
*/*       SIOTETIO - PTR TO DSAB OF REQUEST                          */
*/*         DSABTIOT - PTR TO TIOT DD ENTRY                          */
*/*           TIOEWTCT - NUMBER DEVICE ENTRIES IN TIOT DD ENTRY      */
*/*           TIOUCBP - ARRAY OF PTRS TO UCBS ALLOCATED TO REQUEST   */
*/*             UCBVOLI - VOL SER VOLUME ON UNIT                     */
*/*       SJFCBPTR/SIOTJFX - PTR TO JFCB AND FIRST JFCBX             */
*/*         JFCBNVOL - NUMBER OF VOL SERS IN JFCB/JFCBX              */
*/* EXIT -                                                           */
*/*   CONDITION - INPUT VOLUME NOT ALLOCATED TO INPUT REQUEST        */
*/*     OUTPUT -                                                     */
*/*       MALCDVSW - OFF(0)                                          */
*/*     RETURN CODE - NONE                                           */
*/*   CONDITION - INPUT VOLUME ALLOCATED TO INPUT REQUEST            */
*/*     OUTPUT -                                                     */
*/*       MALCDVSW - ON(1)                                           */
*/*       VRLRELNO - NO. OF VOLUMES TO BE RELEASED DECREMENTED       */
*/*     RETURN CODE - NONE                                           */
*/********************************************************************/
*SIOTPTR = FSIOTPTR;                   /*POINT TO FIRST SIOT ON CHN  */
         L     @10,@PC00001+12                                  02S0074
         MVC   SIOTPTR(4),FSIOTPTR(@10)                         02S0074
*MALCDVSW = OFF;                       /*INIT MULTIPLY ALLOC'D INDIC */
         NI    MALCDVSW,B'01111111'                             02S0075
*DO WHILE(SIOTPTR ^= ZERO &            /*SCAN ALL REQUESTS           */
*   MALCDVSW = OFF);                   /*EXIT IF VOL SER STILL ALOC'D*/
         B     @DE00076                                         02S0076
@DL00076 DS    0H                                               02S0077
*  IF SIOTSTEP = NO THEN               /*REQUEST STILL ALLOCATED     */
         L     @10,SIOTPTR                                      02S0077
         TM    SIOTSTEP(@10),B'00001000'                        02S0077
         BNZ   @RF00077                                         02S0077
*    DO;                               /*CHECK ALL VOLS OF REQUEST   */
*      JFCBPTR = SJFCBPTR;             /*SET PTR TO JFCB OF REQUEST  */
         L     @15,SJFCBPTR(,@10)                               02S0079
         ST    @15,JFCBPTR                                      02S0079
*      JFCBXPTR = SIOTJFX;             /*SET PTR TO FIRST JFCBX      */
         MVC   JFCBXPTR(4),SIOTJFX(@10)                         02S0080
*      DEVCTR = ONE;                   /*INIT INDEX TO TIOT DEVICE
*                                        ENTRY                       */
         LA    @14,1                                            02S0081
         ST    @14,DEVCTR                                       02S0081
*      NUMSERS = ONE;                  /*INIT COUNT OF VOL SERS IN
*                                        REQUEST                     */
         ST    @14,NUMSERS                                      02S0082
*      JVOLSEND = FIVE;                /*SET MAX # SERS IN JFCB      */
         MVC   JVOLSEND(4),@CF00102                             02S0083
*      SERCNTR = ONE;                  /*INIT INDEX INTO VOL SER
*                                        BUFFER                      */
         ST    @14,SERCNTR                                      02S0084
*      VLSERPTR = ADDR(JFCBVOLS);      /*SET VOL SER BUFFER BASE TO
*                                        GET SERS FROM JFCB          */
         LA    @14,JFCBVOLS(,@15)                               02S0085
         ST    @14,VLSERPTR                                     02S0085
*      VOLSEND = JFCBNVOL;             /*MAX # SERS TO PROCESS       */
         SLR   @14,@14                                          02S0086
         IC    @14,JFCBNVOL(,@15)                               02S0086
         ST    @14,VOLSEND                                      02S0086
*      RFY DSAB BASED(SIOTETIO);                                02S0087
*      IF SIOTETIO ^= ZERO &           /*DSAB EXISTS FOR...          */
*          (SIO3DACC = YES ×           /*DIRECT ACCESS DEVICE ...    */
*          SIO3TAPE = YES) THEN        /*... OR TAPE DEVICE          */
         L     @15,SIOTETIO(,@10)                               02S0088
         LTR   @15,@15                                          02S0088
         BZ    @RF00088                                         02S0088
         TM    SIO3DACC(@10),B'10100000'                        02S0088
         BZ    @RF00088                                         02S0088
*        VOLSEND = VOLSEND + TIOEWTCT; /*MUST CHECK VOL SERS IN UCBS,
*                                        SO ADJUST TOTAL COUNT       */
         L     @10,DSABTIOT(,@15)                               02S0089
         SLR   @15,@15                                          02S0089
         IC    @15,TIOEWTCT(,@10)                               02S0089
         ALR   @14,@15                                          02S0089
         ST    @14,VOLSEND                                      02S0089
*      DO WHILE(NUMSERS ^> VOLSEND &   /*PROCESS ALL SERS OF REQUEST */
*          MALCDVSW = OFF);            /*EXIT IF INPUT VOL ALLOCATED */
@RF00088 B     @DE00090                                         02S0090
@DL00090 DS    0H                                               02S0091
*        IF VRLSERNO(LISTCTR) = INVLSER(SERCNTR) THEN /*INPUT SER
*                                        ALLOCATED                   */
         L     @10,LISTCTR                                      02S0091
         SLA   @10,3                                            02S0091
         L     @15,@PC00001+4                                   02S0091
         L     @15,VRLSTPTR(,@15)                               02S0091
         L     @14,SERCNTR                                      02S0091
         MH    @14,@CH00056                                     02S0091
         ALR   @10,@15                                          02S0091
         L     @01,VLSERPTR                                     02S0091
         ALR   @01,@14                                          02S0091
         AL    @01,@CF01098                                     02S0091
         CLC   VRLSERNO-8(6,@10),INVLSER(@01)                   02S0091
         BNE   @RF00091                                         02S0091
*          DO;                                                  02S0092
*            MALCDVSW = ON;            /*INDICATE VOL STILL ALLOCATED*/
         OI    MALCDVSW,B'10000000'                             02S0093
*            VRLRELNO = VRLRELNO - ONE; /*DECREMENT # TO BE RELEASED */
         L     @10,VRLRELNO(,@15)                               02S0094
         BCTR  @10,0                                            02S0094
         ST    @10,VRLRELNO(,@15)                               02S0094
*          END;                                                 02S0095
*/********************************************************************/
*/*                                                                  */
*/* SEGMENT NAME - GETVLSER                                          */
*/*                                                                  */
*/* DECRIPTIVE NAME - GET A VOLUME SERIAL                            */
*/*                                                                  */
*/* FUNCTION -                                                       */
*/*   THIS ROUTINE WILL OBTAIN THE NEXT VOLUME SERIAL                */
*/*   FROM THE JFCB, THEN FROM THE JFCBX(S), AND FROM THE UCB(S)     */
*/*   FOR A GIVEN REQUEST.                                           */
*/*                                                                  */
*/* ENTRY - IN-LINE SEGMENT                                          */
*/*   PURPOSE - TO OBTAIN NEXT VOLSER TO USE IN A SCAN               */
*/*   INPUT -                                                        */
*/*     DEVCTR - CURRENT INDEX INTO TIOT DEVICE ENTRY                */
*/*     JFCBNVOL - NUMBER OF VOL SERS IN JFCB/JFCBX                  */
*/*     JFCBXPTR - PTR TO FIRST JFCBX                                */
*/*     JVOLSEND - CONTROLS SWITCH FROM JFCB TO JFCBX                */
*/*      AND FROM JFCBX TO EACH SUCCEEDING ONE (INTIALLY EQUALS 5)   */
*/*     NUMSERS - NUMBER OF VOL SER BEING PROCESSED                  */
*/*     SERCNTR - CURRENT INDEX INTO VOL SER BUFFER                  */
*/*     VOLSEND - TOTAL NUMBER OF VOL SERS TO PROCESS                */
*/*     VLSERPTR - PTR TO CURRENT SOURCE OF VOL SERS                 */
*/*      (POINTS TO JFCBVOLS ON INITIAL ENTRY)                       */
*/* EXIT -                                                           */
*/*   CONDITION - NEXT VOL SER OBTAINED (IF ANY)                     */
*/*     OUTPUT -                                                     */
*/*       VLSERPTR - PTR TO SOURCE OF NEXT VOLSER                    */
*/*       SERCNTR - INDEX TO NEXT VOL SER                            */
*/*       NUMSERS - ADJUSTED FOR NO. ZERO ENTRIES IN TIOT            */
*/*                 DEVICE ENTRY                                     */
*/*       DEVCTR - INDEX TO NEXT UCB ADDR IN TIOT DEVICE ENTRY       */
*/*     RETURN CODE - NONE                                           */
*/*                                                                  */
*/********************************************************************/
*IF NUMSERS < JFCBNVOL THEN            /*MORE SERS IN JFCB/JFCBX     */
@RF00091 L     @10,NUMSERS                                      03S0096
         L     @01,JFCBPTR                                      03S0096
         SLR   @00,@00                                          03S0096
         IC    @00,JFCBNVOL(,@01)                               03S0096
         CR    @10,@00                                          03S0096
         BNL   @RF00096                                         03S0096
*  DO;                                                          03S0097
*    IF NUMSERS = JVOLSEND THEN        /*NEED TO CHANGE BLOCKS       */
         L     @15,JVOLSEND                                     03S0098
         CR    @10,@15                                          03S0098
         BNE   @RF00098                                         03S0098
*      DO;                                                      03S0099
*        SERCNTR = ONE;                /*RESET VOL SER INDEX         */
         MVC   SERCNTR(4),@CF00049                              03S0100
*        JVOLSEND = JVOLSEND + FIFTEEN; /*SET UPPER LIMIT            */
         AH    @15,@CH00045                                     03S0101
         ST    @15,JVOLSEND                                     03S0101
*        IF NUMSERS ^= FIVE THEN       /*END OF FIRST JFCBX          */
         CH    @10,@CH00102                                     03S0102
         BE    @RF00102                                         03S0102
*          JFCBXPTR = JFCBXNXT;        /*POINT TO NEXT JFCBX         */
         L     @10,JFCBXPTR                                     03S0103
         L     @10,JFCBXNXT(,@10)                               03S0103
         ST    @10,JFCBXPTR                                     03S0103
*        VLSERPTR = ADDR(JFCBXVOL);    /*SET PTR TO INPUT SERS       */
@RF00102 L     @10,JFCBXPTR                                     03S0104
         LA    @10,JFCBXVOL(,@10)                               03S0104
         ST    @10,VLSERPTR                                     03S0104
*      END;                                                     03S0105
*    ELSE                                                       03S0106
*      SERCNTR = SERCNTR + ONE;        /*INCREMENT BUFF INDEX        */
         B     @RC00098                                         03S0106
@RF00098 LA    @10,1                                            03S0106
         AL    @10,SERCNTR                                      03S0106
         ST    @10,SERCNTR                                      03S0106
*  END;                                                         03S0107
*ELSE                                                           03S0108
*  IF NUMSERS ^< VOLSEND THEN          /*MORE SERIALS TO GET         */
         B     @RC00096                                         03S0108
@RF00096 L     @10,NUMSERS                                      03S0108
         C     @10,VOLSEND                                      03S0108
         BL    @RF00108                                         03S0108
*    DO;                               /*GET SERIAL FROM NEXT UCB    */
*      UCBENTSW = OFF;                 /*INIT EXIT SWITCH            */
         NI    UCBENTSW,B'01111111'                             03S0110
*      DO DENTCTR= DEVCTR TO TIOEWTCT  /*CHECK TIOT DEV ENTS         */
*          WHILE(UCBENTSW = OFF);      /*EXIT WHEN FIND NON-0 ENT    */
         L     @10,DEVCTR                                       03S0111
         B     @DE00111                                         03S0111
@DL00111 TM    UCBENTSW,B'10000000'                             03S0111
         BNZ   @DC00111                                         03S0111
*        UCBPTR =TIOUCBP(DENTCTR);     /*SET PTR TO UCB              */
         SLA   @10,2                                            03S0112
         L     @01,SIOTPTR                                      03S0112
         L     @01,SIOTETIO(,@01)                               03S0112
         L     @01,DSABTIOT(,@01)                               03S0112
         L     @10,TIOUCBP-5(@10,@01)                           03S0112
         LA    @10,0(,@10)                                      03S0112
         ST    @10,UCBPTR                                       03S0112
*       IF UCBPTR ^= ZERO THEN         /*DEVICE WAS ALLOCATED        */
         LTR   @10,@10                                          03S0113
         BZ    @RF00113                                         03S0113
*         DO;                                                   03S0114
*           UCBENTSW = ON;             /*SET SWITCH TO EXIT LOOP     */
         OI    UCBENTSW,B'10000000'                             03S0115
*           VLSERPTR = ADDR(UCBVOLI);  /*SET PTR TO INPUT SER        */
         LA    @10,UCBVOLI(,@10)                                03S0116
         ST    @10,VLSERPTR                                     03S0116
*           SERCNTR = ONE;             /*SET INDEX INTO INPUT SER    */
         LA    @10,1                                            03S0117
         ST    @10,SERCNTR                                      03S0117
*           DEVCTR = DEVCTR + ONE;     /*UPDATE DEV. ENTRY INDEX     */
         AL    @10,DEVCTR                                       03S0118
         ST    @10,DEVCTR                                       03S0118
*         END;                                                  03S0119
*       ELSE                           /*ZERO TIOT DEVICE ENTRY      */
*         NUMSERS = NUMSERS + ONE;     /*ADJUST # INPUT SERS         */
         B     @RC00113                                         03S0120
@RF00113 LA    @10,1                                            03S0120
         AL    @10,NUMSERS                                      03S0120
         ST    @10,NUMSERS                                      03S0120
*      END;                                                     03S0121
@RC00113 LA    @10,1                                            03S0121
         AL    @10,DENTCTR                                      03S0121
@DE00111 ST    @10,DENTCTR                                      03S0121
         L     @01,SIOTPTR                                      03S0121
         L     @01,SIOTETIO(,@01)                               03S0121
         L     @01,DSABTIOT(,@01)                               03S0121
         SLR   @00,@00                                          03S0121
         IC    @00,TIOEWTCT(,@01)                               03S0121
         CR    @10,@00                                          03S0121
         BNH   @DL00111                                         03S0121
@DC00111 DS    0H                                               03S0122
*    END;                                                       03S0122
*  ELSE;                               /*NO MORE SERS FOR THIS PASS  */
@RF00108 DS    0H                                               03S0124
