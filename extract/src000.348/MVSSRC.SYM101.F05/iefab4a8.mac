*/* SEGMENT NAME - BLDDEQPL                                          */
*/*                                                                  */
*/* DESCRIPTIVE NAME - BUILD DEQ PARAMETER LIST                      */
*/*                                                                  */
*/* FUNCTION -                                                       */
*/*   THIS ROUTINE OBTAINS CORE FOR AND BUILDS THE DEQ PARAMETER     */
*/*   LIST FOR THOSE VOLUMES IN THE VOLUME RELEASE LIST WHICH CAN    */
*/*   BE RELEASED, A CONDITIONAL DEQ IS THEN ISSUED. THIS DEQ IS     */
*/*   DIRECTED TO THE TCB RECEIVED AS INPUT.                 @YM03580*/
*/*                                                                  */
*/* ENTRY - IN-LINE SEGMENT                                          */
*/*   PURPOSE - TO BUILD VOLUME DEQ PARAMETER LIST                   */
*/*   INPUT -                                                        */
*/*     VRLIST - VOLUME RELEASE LIST (SEE A4A8DATA FOR FORMAT)       */
*/*       VRLRELNO - NUMBER OF VOLUMES TO RELEASE                    */
*/*       VRLDEQVL - IF ON IN ENTRY, VOL CAN BE RELEASED             */
*/*                                                                  */
*/* EXIT -                                                           */
*/*   CONDITION - DEQ DONE                                           */
*/*     OUTPUT -                                                     */
*/*       VOLUMES RELEASED                                           */
*/*     RETURN CODE - NONE                                           */
*/*                                                                  */
*/********************************************************************/
*DEQPLSZE = VRLRELNO*LENGTH(DEQPLENT)  /*CALCULATE PARM LIST SIZE    */
*    + LENGTH(DEQPLHDR);                                        02S0138
         MH    @10,@CH01092                                     02S0138
         AH    @10,@CH00235                                     02S0138
         ST    @10,DEQPLSZE                                     02S0138
* /* ?GETSTORG (SPID,DEQPLSZE,DEQPLPTR);*/                      02S0139
*   /* DEFINITIONS OF GETMAIN PARMS */                          02S0139
*   DCL                                                         02S0139
*     GTSTR0   FIXED(31) REG(0),/*GTMN PARM REG*/               02S0139
*     GTSTR1   FIXED(31) REG(1),/*GTMN PARM REG*/               02S0139
*     GTSTR2   PTR(31)   REG(2),/*GTMN PARM REG*/               02S0139
*     GTSTR15  FIXED(31) REG(15),/*GTMN RET COD*/               02S0139
*     GTSTRTCD FIXED(31); /* GETMAIN RTN CODE  */               02S0139
*   DCL                                                         02S0140
*     GTSTR3   FIXED(31) REG(3),/* CLEARING REG*/               02S0140
*     GTSTR4   FIXED(31) REG(4),/* CLEARING REG*/               02S0140
*     GTSTR5   FIXED(31) REG(5),/* CLEARING REG*/               02S0140
*     GTSTR6   FIXED(31) REG(6),/* CLEARING REG*/               02S0140
*     GTSTR7   FIXED(31) REG(7),/* CLEARING REG*/               02S0140
*     GTSTR8   FIXED(31) REG(8),/* CLEARING REG*/               02S0140
*     GTSTZERO FIXED(31) INIT(0),                               02S0140
*     GTST253  FIXED(31) CONSTANT(253);                         02S0140
*   RFY (GTSTR0,GTSTR1,GTSTR2) RSTD; /*SAVE RGS*/               02S0141
*   GTSTR0 = DEQPLSZE; /* SET AMOUNT*/                          02S0142
         LR    GTSTR0,@10                                       02S0142
*   GTSTR2 = SPID; /* SET SUBPOOL*/                             02S0143
         LA    GTSTR2,230                                       02S0143
*   GENERATE CODE;                                              02S0144
     GETMAIN RC,LV=(0),SP=(2)  DO GETMAIN
*   GTSTRTCD = GTSTR15; /* SAVE GETMAIN RET COD*/               02S0145
         ST    GTSTR15,GTSTRTCD                                 02S0145
*   DEQPLPTR = GTSTR1; /* ADDR OF GOTN STORG*/                  02S0146
         ST    GTSTR1,DEQPLPTR                                  02S0146
*   RFY (GTSTR0,GTSTR1,GTSTR2) UNRSTD; /*RLSE */                02S0147
*   IF GTSTRTCD = 0 THEN /*GETMAIN SUCCESSFUL? */               02S0148
         L     @10,GTSTRTCD                                     02S0148
         LTR   @10,@10                                          02S0148
         BNZ   @RF00148                                         02S0148
*     DO;               /* YES, CLEAR STORAGE.*/                02S0149
*       RFY (GTSTR2,GTSTR4,GTSTR5,GTSTR6,                       02S0150
*                          GTSTR7,GTSTR8) RSTD;                 02S0150
*       GTSTR4 = DEQPLPTR; /*STORG PTR*/                        02S0151
         L     @10,DEQPLPTR                                     02S0151
         LR    GTSTR4,@10                                       02S0151
*       GTSTR5 = DEQPLSZE; /*STORG AMNT*/                       02S0152
         L     GTSTR5,DEQPLSZE                                  02S0152
*       GTSTR7 = '00000000'X; /*PAD CHAR & LNTH*/               02S0153
         SLR   GTSTR7,GTSTR7                                    02S0153
*       IF SPID = GTST253 THEN                                  02S0154
         B     @RF00154                                         02S0154
*      GEN                                                      02S0155
*   (MODESET KEYADDR=GTSTZERO,SAVEKEY=(2),WORKREG=8);           02S0155
         MODESET KEYADDR=GTSTZERO,SAVEKEY=(2),WORKREG=8
*       GEN REFS(GTSTR4,GTSTR6);                                02S0156
@RF00154 DS    0H                                               02S0156
          MVCL  GTSTR4,GTSTR6 CLEAR GOTTEN STORG
*       IF SPID = GTST253 THEN                                  02S0157
         B     @RF00157                                         02S0157
*         GEN (MODESET KEYADDR=(2));                            02S0158
         MODESET KEYADDR=(2)
*       RFY (GTSTR2,GTSTR4,GTSTR5,GTSTR6,                       02S0159
*                          GTSTR7,GTSTR8) UNRSTD;               02S0159
@RF00157 DS    0H                                               02S0160
*     END;       /* STORAGE CLEARING COMPLETED */               02S0160
*   ELSE                                                        02S0161
*   DEQPLPTR= 0;   /* CLEAR RETURN ADDR. */                     02S0161
         B     @RC00148                                         02S0161
@RF00148 SLR   @10,@10                                          02S0161
         ST    @10,DEQPLPTR                                     02S0161
*   GTSTR15 = GTSTRTCD;    /* GTMN RETURN CODE */               02S0162
@RC00148 L     GTSTR15,GTSTRTCD                                 02S0162
*                                      /* RESTORE LIST PRINT STATUS  */
*RETCODE = REG15;                      /*SAVE RETURN CODE            */
         ST    REG15,RETCODE                                    02S0163
*IF RETCODE = ZERO THEN                /*CORE GOTTEN O.K.            */
         L     @10,RETCODE                                      02S0164
         LTR   @10,@10                                          02S0164
         BNZ   @RF00164                                         02S0164
*  DO;                                 /*BUILD PARM LIST             */
*    DEQTCBP = DQTCBP;                 /*SET PTR TO TCB      @YM03580*/
         L     @10,@PC00001+8                                   02S0166
         MVC   DEQTCBP(4),DQTCBP(@10)                           02S0166
*    DEQENTP = DEQPLPTR + LENGTH(DEQPLHDR); /*POINT TO FIRST P-LIST
*                                        ENTRY                       */
         LA    @10,4                                            02S0167
         AL    @10,DEQPLPTR                                     02S0167
         ST    @10,DEQENTP                                      02S0167
*    DO LISTCTR = 1 TO VRLENTNO;       /*SCAN VOL RELEASE LIST       */
         LA    @10,1                                            02S0168
         B     @DE00168                                         02S0168
@DL00168 DS    0H                                               02S0169
*      IF VRLDEQVL(LISTCTR) = YES THEN /*SER IN CURRENT ENTRY CAN BE
*                                        RELEASED                    */
         SLA   @10,3                                            02S0169
         L     @15,@PC00001+4                                   02S0169
         L     @15,VRLSTPTR(,@15)                               02S0169
         ST    @10,@TF00001                                     02S0169
         ALR   @10,@15                                          02S0169
         TM    VRLDEQVL-8(@10),B'10000000'                      02S0169
         BNO   @RF00169                                         02S0169
*        DO;                           /*FILL-IN P-LIST ENTRY        */
*          DEQPLENT(1:DEQENTLN) =      /*MOVE SKELETON VOL DEQ MACRO */
*              DEQFMT(1:DEQENTLN);                              02S0171
         L     @10,DEQENTP                                      02S0171
         L     @14,DEQENTLN                                     02S0171
         LR    @09,@14                                          02S0171
         BCTR  @09,0                                            02S0171
         EX    @09,@SM01100                                     02S0171
*          DEQPLEND = OFF;             /*RESET LAST ENTRY INDICATOR  */
         NI    DEQPLEND(@10),B'01111111'                        02S0172
*          DEQMNRNP = ADDR(VRLSERNO(LISTCTR)); /*SET PTR TO VOL SER  */
         L     @01,@TF00001                                     02S0173
         LA    @15,VRLSERNO-8(@01,@15)                          02S0173
         ST    @15,DEQMNRNP(,@10)                               02S0173
*          DEQENTP = DEQENTP + DEQENTLN; /*SET PTR FOR NEXT P-LIST
*                                         ENTRY                      */
         ALR   @10,@14                                          02S0174
         ST    @10,DEQENTP                                      02S0174
*        END;                                                   02S0175
*      ELSE;                           /*VOL STILL NEEDED IN STEP    */
@RF00169 DS    0H                                               02S0177
*    END;                                                       02S0177
         LA    @10,1                                            02S0177
         AL    @10,LISTCTR                                      02S0177
@DE00168 ST    @10,LISTCTR                                      02S0177
         L     @15,@PC00001+4                                   02S0177
         L     @01,VRLSTPTR(,@15)                               02S0177
         C     @10,VRLENTNO(,@01)                               02S0177
         BNH   @DL00168                                         02S0177
*    DEQENTP = DEQENTP - DEQENTLN;     /*SET PTR BACK TO LAST ENTRY  */
         L     @10,DEQENTP                                      02S0178
         SL    @10,DEQENTLN                                     02S0178
         ST    @10,DEQENTP                                      02S0178
*    DEQPLEND = ON;                    /*SET END-OF-LIST INDICATOR   */
         OI    DEQPLEND(@10),B'10000000'                        02S0179
*    RFY (REG0,REG1) RSTD;                                      02S0180
*    REG1 = DEQPLPTR + LENGTH(DEQPLHDR); /*ADJUST PTR FOR DEQ        */
         LA    REG1,4                                           02S0181
         AL    REG1,DEQPLPTR                                    02S0181
*    GEN(DEQ MF=(E,(1)),TCB=DEQTCBP)   /*DEQ VOLUMES                 */
*        REFS(REG1,DEQTCBP);                                    02S0182
         DEQ MF=(E,(1)),TCB=DEQTCBP
*    REG0 = DEQPLSZE;                  /*AMT OF P-LIST CORE          */
         L     REG0,DEQPLSZE                                    02S0183
*    REG0 = REG0 × SUBPL230;           /*FREE FROM SUBPOOL 230.      */
         O     REG0,@CF00115                                    02S0184
*    REG1 = DEQPLPTR;                  /*START OF P-LIST CORE        */
         L     REG1,DEQPLPTR                                    02S0185
*    GEN(FREEMAIN R,LV=(0),A=(1));     /*FREE DEQ PARAMETER LIST     */
         FREEMAIN R,LV=(0),A=(1)
*    RFY (REG0,REG1) UNRSTD;                                    02S0187
*  END;                                                         02S0188
*ELSE                                  /*P-LIST CORE NOT AVAILABLE   */
*  RSNCODE = VRGMERR;                  /*SET REASON CODE             */
         B     @RC00164                                         02S0189
@RF00164 L     @10,@PC00001+16                                  02S0189
         MVC   RSNCODE(2,@10),@CH00095                          02S0189
*      END;                                                     01S0190
@RC00164 DS    0H                                               01S0191
*    RFY (REG0,REG1) RSTD;                                      01S0191
@RF00136 DS    0H                                               01S0192
*    REG0 = VRLENTNO * LENGTH(VRLSTENT) + LENGTH(VRLSTHDR);     01S0192
         L     @10,@PC00001+4                                   01S0192
         L     @10,VRLSTPTR(,@10)                               01S0192
         L     REG0,VRLENTNO(,@10)                              01S0192
         SLA   REG0,3                                           01S0192
         AH    REG0,@CH00047                                    01S0192
*    REG0 = REG0 × SUBPL230;           /*FREE FROM SUBPOOL 230.      */
         O     REG0,@CF00115                                    01S0193
*    REG1 = VRLSTPTR;                                           01S0194
         LR    REG1,@10                                         01S0194
*    GEN(FREEMAIN R,LV=(0),A=(1));     /*FREE RELEASE LIST           */
         FREEMAIN R,LV=(0),A=(1)
*    RFY (REG0,REG1) UNRSTD;                                    01S0196
*  END;                                                         01S0197
*RETURN CODE(RETCODE);                 /*RETURN TO CALLER            */
@RC00044 L     @10,RETCODE                                      01S0198
         IC    @02,@OLDKEY                                      01S0198
         L     @13,4(,@13)                                      01S0198
         L     @00,@SIZDATD                                     01S0198
         LR    @01,@11                                          01S0198
         FSPACE   R,LV=(0),A=(1)
         MODESET KEYADDR=(2)
         LR    @15,@10                                          01S0198
         L     @14,12(,@13)                                     01S0198
         LM    @00,@12,20(@13)                                  01S0198
         BR    @14                                              01S0198
*END IEFAB4A8                                                   01S0199
