         ALR   @08,INPTR                                        03S0663
         EX    @02,@SC01729                                     03S0663
         BE    @RF00663                                         03S0663
@RT00663 DS    0H                                               03S0664
*                 USSDB=USSDB+((ADDR(USSDVAL)-ADDR(IEDUSSD))+USSDVLEN+
*                     USSDRLEN);    /* STEP POINTER TO NEXT VALUE
*                                      ENTRY IN TABLE                */
         L     @14,USSDB                                        03S0664
         LA    @08,3                                            03S0664
         SLR   @02,@02                                          03S0664
         IC    @02,USSDVLEN(,@14)                               03S0664
         ALR   @08,@02                                          03S0664
         SLR   @02,@02                                          03S0664
         IC    @02,USSDRLEN(,@14)                               03S0664
         ALR   @08,@02                                          03S0664
         ALR   @14,@08                                          03S0664
         ST    @14,USSDB                                        03S0664
*               ELSE                                            03S0665
*                 CALL USSMOVE;     /* ELSE MOVE THE EQUATED VALUE
*                                      FROM THE ENTRY INTO THE  03S0665
*                                      FORMATTED OUTPUT AREA         */
         B     @RC00663                                         03S0665
@RF00663 BAL   @14,USSMOVE                                      03S0665
*             END;                  /* END OF VALUE ENTRY SEARCH     */
@RC00663 DS    0H                                               03S0666
@DE00662 L     @14,USSDB                                        03S0666
         TM    USSDTYPE(@14),B'10000000'                        03S0666
         BNO   @DC00662                                         03S0666
         TM    USSDTYPE(@14),B'01100000'                        03S0666
         BNZ   @DC00662                                         03S0666
         CLI   USSDVLEN(@14),0                                  03S0666
         BE    @DC00662                                         03S0666
         TM    MOVED,B'10000000'                                03S0666
         BNO   @DL00662                                         03S0666
@DC00662 DS    0H                                               03S0667
*                                                               02S0667
*             /*******************************************************/
*             /*                                                     */
*             /* IF ALL VALUE ENTRIES PROCESSED INSURE DATA MOVED    */
*             /*                                                     */
*             /*******************************************************/
*                                                               02S0667
*             IF USSDTYPE^=USSFVAL THEN/* IF ALL VALUE ENTRIES HAVE
*                                      BEEN PROCESSED, ...           */
         L     @14,USSDB                                        02S0667
         TM    USSDTYPE(@14),B'10000000'                        02S0667
         BNO   @RT00667                                         02S0667
         TM    USSDTYPE(@14),B'01100000'                        02S0667
         BZ    @RF00667                                         02S0667
@RT00667 DS    0H                                               02S0668
*               DO;                 /* THEN DO                       */
*                 IF MOVED=NO THEN  /* IF THIS IS A REQUIRED    02S0669
*                                      PARAMETER AND DATA HAS NOT
*                                      BEEN MOVED, ...               */
         TM    MOVED,B'10000000'                                02S0669
         BNZ   @RF00669                                         02S0669
*                   EXITCODE=BADPARM;/* THEN SET INVALID PARAMETER
*                                      ERROR RETURN CODE AND SUSPEND
*                                      PROCESSING                    */
         LA    @14,44                                           02S0670
         ST    @14,EXITCODE                                     02S0670
*                 USSPB=USSDB;      /* RESET CURRENT POSITIONAL 02S0671
*                                      PARAMETER TABLE ENTRY POINTER
*                                      TO THE NEXT POSITIONAL   02S0671
*                                      PARAMETER ENTRY               */
@RF00669 L     USSPB,USSDB                                      02S0671
*               END;                                            02S0672
*                                                               02S0672
*             /*******************************************************/
*             /*                                                     */
*             /* MOVE USER DATA IF PERMITTED, OTHERWISE SET ERROR    */
*             /* RETURN                                              */
*             /*                                                     */
*             /*******************************************************/
*                                                               02S0673
*             ELSE                                              02S0673
*               DO;                 /* ELSE IF A VALUE ENTRY         */
         B     @RC00667                                         02S0673
@RF00667 DS    0H                                               02S0674
*                 IF USSDRLEN=0 THEN/* IF THIS IS A VALUE ENTRY THAT
*                                      SPECIFIES USER INPUT (VALUE
*                                      NAME NOT SPECIFIED AND NO
*                                      REPLACEMENT DATA INDICATED)...*/
         L     @14,USSDB                                        02S0674
         CLI   USSDRLEN(@14),0                                  02S0674
         BNE   @RF00674                                         02S0674
*                   DO;             /* THEN CONVERT AND MOVE USER
*                                      DATA TO OUTPUT FIELD          */
*                                                               03S0676
*                     /***********************************************/
*                     /*                                             */
*                     /* USSPARM.USSCONV: THIS SEGMENT PROCESSES USER*/
*                     /* INPUT DATA, CHARACTER OR BINARY, AND PLACES */
*                     /* IT INTO THE APPROPRIATE COMMAND OUTPUT      */
*                     /* FIELD. CONVERT DATA TO BINARY IF REQUIRED   */
*                     /*                                             */
*                     /***********************************************/
*                                                               03S0676
*                     IF USSPDATA=USSPBIN THEN/* IF DATA IS TO BE
*                                      CONVERTED TO BINARY, ...      */
         TM    USSPDATA(USSPB),B'00000010'                      03S0676
         BNO   @RF00676                                         03S0676
         TM    USSPDATA(USSPB),B'00001101'                      03S0676
         BNZ   @RF00676                                         03S0676
*                       DO;         /* THEN CONVERT TO BINARY   03S0677
*                                      REPLACEMENT DATA              */
*                                                               04S0678
*                         /*******************************************/
*                         /*                                         */
*                         /* USSPARM.USSCONV.USSBBVAL: THIS SEGMENT  */
*                         /* VALIDATES THAT USER INPUT DATA IS ALL   */
*                         /* NUMERIC, THEN TRANSLATES IT TO BINARY   */
*                         /* AND MAKES SURE IT DOESN'T EXCEED MAXIMUM*/
*                         /* VALUE FOR FIELD.                        */
*                         /*                                         */
*                         /*******************************************/
*                                                               04S0678
*                         UNPACKED(1:16)='';/* BLANK WORK AREA       */
*                                                               04S0678
         MVI   UNPACKED,C' '                                    04S0678
         MVC   UNPACKED+1(15),UNPACKED                          04S0678
*                         /*******************************************/
*                         /*                                         */
*                         /* VALIDATE CHARACTERS ARE NUMERIC         */
*                         /*                                         */
*                         /*******************************************/
*                                                               04S0679
*                         UNPACKED(17-CHARLEN:16)=INDATA(FSTCHAR:
*                             FSTCHAR+CHARLEN-1);               04S0679
*                                                               04S0679
         LA    @14,17                                           04S0679
         SLR   @14,CHARLEN                                      04S0679
         LA    @08,UNPACKED-1(@14)                              04S0679
         LCR   @14,@14                                          04S0679
         AL    @14,@CF00166                                     04S0679
         LH    @02,FSTCHAR                                      04S0679
         ALR   @02,INPTR                                        04S0679
         EX    @14,@SM01731                                     04S0679
*                         /*******************************************/
*                         /*                                         */
*                         /* MOVE CHARACTER STRING INTO WORK AREA    */
*                         /*                                         */
*                         /*******************************************/
*                                                               04S0680
*                         DO MOVECOUN=FSTCHAR TO(FSTCHAR+CHARLEN-1)
*                               WHILE(EXITCODE=TRANSOK);        04S0680
*                                                               04S0680
         LH    @14,FSTCHAR                                      04S0680
         LR    MOVECOUN,@14                                     04S0680
         B     @DE00680                                         04S0680
@DL00680 L     @14,EXITCODE                                     04S0680
         LTR   @14,@14                                          04S0680
         BNZ   @DC00680                                         04S0680
*                           /*****************************************/
*                           /*                                       */
*                           /* INSURE THAT CHARACTER STRING IS       */
*                           /* NUMERIC                               */
*                           /*                                       */
*                           /*****************************************/
*                                                               04S0681
*                           IF INDATA(MOVECOUN)<'0'×INDATA(MOVECOUN)>'9
*                             THEN  /* IF CHARACTER NOT NUMERIC, ... */
         LR    @14,INPTR                                        04S0681
         ALR   @14,MOVECOUN                                     04S0681
         CLI   INDATA-1(@14),C'0'                               04S0681
         BL    @RT00681                                         04S0681
         LR    @14,INPTR                                        04S0681
         ALR   @14,MOVECOUN                                     04S0681
         CLI   INDATA-1(@14),C'9'                               04S0681
         BNH   @RF00681                                         04S0681
@RT00681 DS    0H                                               04S0682
*                             EXITCODE=BADPARM;/* THEN SET INVALID
*                                      PARAMETER RETURN CODE         */
         LA    @14,44                                           04S0682
         ST    @14,EXITCODE                                     04S0682
*                         END;      /* END OF CHARACTER VALIDATION
*                                      LOOP                          */
@RF00681 AL    MOVECOUN,@CF00061                                04S0683
@DE00680 LR    @14,CHARLEN                                      04S0683
         AH    @14,FSTCHAR                                      04S0683
         BCTR  @14,0                                            04S0683
         CR    MOVECOUN,@14                                     04S0683
         BNH   @DL00680                                         04S0683
@DC00680 DS    0H                                               04S0684
*                         IF EXITCODE^=TRANSOK THEN             04S0684
         L     @14,EXITCODE                                     04S0684
         LTR   @14,@14                                          04S0684
         BNZ   @RT00684                                         04S0684
*                           ;       /* IF AN ERROR OCCURRED THEN
*                                      SUSPEND PROCESSING            */
*                                                               04S0685
*                         /*******************************************/
*                         /*                                         */
*                         /* IF NO ERRORS CONVERT TO BINARY AND      */
*                         /* INSURE VALUE DOES NOT EXCEED MAXIMUM    */
*                         /* POSSIBLE FOR FIELD                      */
*                         /*                                         */
*                         /*******************************************/
*                                                               04S0686
*                         ELSE                                  04S0686
*                           DO;     /* ELSE CONTINUE ...             */
*                             PACK(PACKED,UNPACKED);/* PACK DATA TO
*                                      BE CONVERTED TO BINARY        */
         PACK  PACKED(8),UNPACKED(16)                           04S0687
*                             CVB(BINDATA,PACKED);/* CONVERT IT TO
*                                      BINARY                        */
         CVB   @14,PACKED                                       04S0688
         ST    @14,BINDATA                                      04S0688
*                             MAXFIELD=-1;/* INITIATE MAX FIELD VALUE*/
         SLR   MAXFIELD,MAXFIELD                                04S0689
         BCTR  MAXFIELD,0                                       04S0689
*                             SHIFT=32-USSPLEN;/* SHIFT VALUE        */
         LA    @02,32                                           04S0690
         SLR   @01,@01                                          04S0690
         ICM   @01,3,USSPLEN(USSPB)                             04S0690
         SLR   @02,@01                                          04S0690
         STC   @02,SHIFT                                        04S0690
*                             SRL(MAXFIELD,SHIFT);/* SET MAXIMUM
*                                      VALUE                         */
         SRL   MAXFIELD,0(@02)                                  04S0691
*                             IF MAXFIELD<BINDATA THEN/* IF CONVERTED
*                                      DATA EXCEEDS MAXIMUM, ...     */
         CR    MAXFIELD,@14                                     04S0692
         BNL   @RF00692                                         04S0692
*                               EXITCODE=TOOMUCH;/* SET RETURN CODE
*                                      FOR INPUT EXCEEDS MAXIMUM
*                                      ALLOWABLE                     */
*                                                               04S0693
         LA    @14,40                                           04S0693
         ST    @14,EXITCODE                                     04S0693
*                             /***************************************/
*                             /*                                     */
*                             /* IF NO ERRORS TRANSFER BINARY NUMBER */
*                             /* TO VALUE ENTRY                      */
*                             /*                                     */
*                             /***************************************/
*                                                               04S0694
*                             ELSE                              04S0694
*                               DO; /* ELSE ...                      */
         B     @RC00692                                         04S0694
@RF00692 DS    0H                                               04S0695
*                                 BLDWIDTH=(USSPLEN+7)/8;/* SET BYTE
*                                      COUNT OF CONVERTED VALUE      */
         LA    @00,7                                            04S0695
         SLR   BLDWIDTH,BLDWIDTH                                04S0695
         ICM   BLDWIDTH,3,USSPLEN(USSPB)                        04S0695
         ALR   @00,BLDWIDTH                                     04S0695
         SRDA  @00,32                                           04S0695
         D     @00,@CF00046                                     04S0695
         LR    BLDWIDTH,@01                                     04S0695
*                                 MYVAL->USSDRLEN=BLDWIDTH;/* SET
*                                      WIDTH OF CONVERTED USER DATA
*                                      INTO VALUE ENTRY              */
         L     @14,MYVAL                                        04S0696
         STC   BLDWIDTH,USSDRLEN(,@14)                          04S0696
*                                 MYVAL->USSDREP(1:BLDWIDTH)=BINDATAC(5
*                                     -BLDWIDTH:4);/* MOVE      04S0697
*                                      REPLACEMENT DATA TO VALUE
*                                      ENTRY                         */
         LR    @02,BLDWIDTH                                     04S0697
         BCTR  @02,0                                            04S0697
         LA    @01,5                                            04S0697
         SLR   @01,BLDWIDTH                                     04S0697
         ST    @02,@TF00001                                     04S0697
         LA    @02,BINDATAC-1(@01)                              04S0697
         L     @01,@TF00001                                     04S0697
         EX    @01,@SM01733                                     04S0697
*                               END;                            04S0698
*                           END;                                04S0699
*                       END;                                    03S0700
*                     ELSE                                      03S0701
*                       DO;         /* ELSE PREPARE CHARACTER   03S0701
*                                      REPLACEMENT DATA              */
         B     @RC00676                                         03S0701
@RF00676 DS    0H                                               03S0702
*                                                               04S0702
*                         /*******************************************/
*                         /*                                         */
*                         /* USSPARM.USSCONV.USSREMOV: THIS SEGMENT  */
*                         /* SCANS THE POSITIONAL PARAMETER/KEYWORD  */
*                         /* VALUE INPUT BY THE USER AND TRANSFERS IT*/
*                         /* TO THE VALUE ENTRY BEING CONSTRUCTED.   */
*                         /* APOSTROPHES ARE REMOVED IN THE PROCESS. */
*                         /*                                         */
*                         /*******************************************/
*                                                               04S0702
*                         TARGINDX=0;/* RESET INDEX INTO TARGET AREA */
         SLR   @14,@14                                          04S0702
         ST    @14,TARGINDX                                     04S0702
*                         DO MOVECOUN=FSTCHAR TO(FSTCHAR+CHARLEN-1);
         LH    MOVECOUN,FSTCHAR                                 04S0703
         B     @DE00703                                         04S0703
@DL00703 DS    0H                                               04S0704
*                                   /* LOOP UNTIL ALL CHARS HAVE BEEN
*                                      MOVED TO VALUE ENTRY          */
*                           TARGINDX=TARGINDX+1;/* INCREMENT INDEX   */
         LA    @14,1                                            04S0704
         L     @02,TARGINDX                                     04S0704
         ALR   @02,@14                                          04S0704
         ST    @02,TARGINDX                                     04S0704
*                           IF INDATA(TARGINDX)='''' THEN       04S0705
         ALR   @02,INPTR                                        04S0705
         CLI   INDATA-1(@02),C''''                              04S0705
         BNE   @RF00705                                         04S0705
*                             MOVECOUN=MOVECOUN+1;/* MUST BUMP INPUT
*                                      INDEX IF QUOTE IS FOUND       */
         ALR   MOVECOUN,@14                                     04S0706
*                           MYVAL->USSDREP(TARGINDX)=INDATA(MOVECOUN);
@RF00705 L     @14,MYVAL                                        04S0707
         AL    @14,TARGINDX                                     04S0707
         LR    @02,INPTR                                        04S0707
         ALR   @02,MOVECOUN                                     04S0707
         MVC   USSDREP-1(1,@14),INDATA-1(@02)                   04S0707
*                                   /* MOVE A BYTE OF DATA           */
*                         END;                                  04S0708
         AL    MOVECOUN,@CF00061                                04S0708
@DE00703 LR    @14,CHARLEN                                      04S0708
         AH    @14,FSTCHAR                                      04S0708
         BCTR  @14,0                                            04S0708
         CR    MOVECOUN,@14                                     04S0708
         BNH   @DL00703                                         04S0708
*                         CHARLEN=TARGINDX;/* UPDATE CHARLEN FIELD   */
         L     CHARLEN,TARGINDX                                 04S0709
*                                   /* REMOVE DUPLICATE QUOTES FROM
*                                      INPUT STREAM AND TRANSFER DATA
*                                      TO CONSTRUCTED VALUE ENTRY    */
*                                                               03S0710
*                         /*******************************************/
*                         /*                                         */
*                         /* BUILD VALUE ENTRY                       */
*                         /*                                         */
*                         /*******************************************/
*                                                               03S0710
*                                                               04S0710
*                         /*******************************************/
*                         /*                                         */
*                         /* USSPARM.USSCONV.USSBCVAL: THIS SEGMENT  */
*                         /* INSURES THAT USER CHARACTER INPUT DOES  */
*                         /* NOT EXCEED THE MAXIMUM AMOUNT ALLOWED   */
*                         /* FOR THE OUTPUT COMMAND FIELD.           */
*                         /*                                         */
*                         /*******************************************/
*                                                               04S0710
*                         IF CHARLEN>((USSPLEN+7)/8) THEN/* IF THE
*                                      NUMBER OF CHARACTERS EXCEEDS
*                                      THE MAXIMUM,...               */
         LA    @00,7                                            04S0710
         SLR   @14,@14                                          04S0710
         ICM   @14,3,USSPLEN(USSPB)                             04S0710
         ALR   @00,@14                                          04S0710
         SRDA  @00,32                                           04S0710
         D     @00,@CF00046                                     04S0710
         CR    CHARLEN,@01                                      04S0710
         BNH   @RF00710                                         04S0710
*                           EXITCODE=BADINPUT;/* THEN SET INVALID
*                                      INPUT RETURN CODE             */
         LA    @14,28                                           04S0711
         ST    @14,EXITCODE                                     04S0711
*                         ELSE                                  04S0712
*                           MYVAL->USSDRLEN=CHARLEN;/* SET LENGTH IN
*                                      VALUE ENTRY                   */
         B     @RC00710                                         04S0712
@RF00710 L     @14,MYVAL                                        04S0712
         STC   CHARLEN,USSDRLEN(,@14)                           04S0712
*                       END;                                    03S0713
@RC00710 DS    0H                                               03S0714
*                     IF EXITCODE^=TRANSOK THEN                 03S0714
@RC00676 L     @14,EXITCODE                                     03S0714
         LTR   @14,@14                                          03S0714
         BNZ   @RT00714                                         03S0714
*                       ;           /* IF AN ERROR HAS OCCURRED, THEN
*                                      NO FURTHER ACTION             */
*                     ELSE                                      03S0716
*                       DO;                                     03S0716
*                         SAVEDB=USSDB;/* SAVE VALUE ENTRY           */
         L     @14,USSDB                                        03S0717
         ST    @14,SAVEDB                                       03S0717
*                         USSDB=MYVAL;/* SET VALUE ENTRY ADDR TO
*                                      DUMMY AREA                    */
         L     @14,MYVAL                                        03S0718
         ST    @14,USSDB                                        03S0718
*                         CALL USSMOVE;/* MOVE USER DATA TO COMMAND
*                                      FIELD IN OUTPUT AREA          */
         BAL   @14,USSMOVE                                      03S0719
*                         USSPB=USSDB;/* SETUP POSTIONAL ENTRY  03S0720
*                                      POINTER                       */
         L     USSPB,USSDB                                      03S0720
*                         SAVEDB=0; /* CLEAR SAVE ADDRESS            */
         SLR   @14,@14                                          03S0721
         ST    @14,SAVEDB                                       03S0721
*                       END;                                    03S0722
*                   END;                                        02S0723
*                 ELSE                                          02S0724
*                   EXITCODE=BADPARM;/* SET INVALID POSITIONAL  02S0724
*                                      PARAMETER RETURN CODE         */
         B     @RC00674                                         02S0724
@RF00674 LA    @14,44                                           02S0724
         ST    @14,EXITCODE                                     02S0724
*               END;                                            02S0725
@RC00674 DS    0H                                               02S0726
*           END;                                                02S0726
@RC00667 DS    0H                                               02S0727
*       END;                                                    02S0727
@RC00659 DS    0H                                               02S0728
*   END;                                                        02S0728
@RT00656 DS    0H                                               02S0728
@DE00582 TM    USSPTYPE(USSPB),B'01000000'                      02S0728
         BNO   @DC00582                                         02S0728
         TM    USSPTYPE(USSPB),B'10100000'                      02S0728
         BNZ   @DC00582                                         02S0728
         L     @14,EXITCODE                                     02S0728
         LTR   @14,@14                                          02S0728
         BZ    @DL00582                                         02S0728
@DC00582 DS    0H                                               02S0729
