         TITLE 'IEDQFM, USS-FSS/FSS-USS CONVERSION PROGRAM             *
                        '
IEDQFM   CSECT ,                                                01S0001
@PROLOG  BALR  @12,0                                            01S0001
@PSTART  DS    0H                                               01S0001
         USING @PSTART,@12                                      01S0001
*                    /*
IEDQFM   IEDHJN AFTERHJN,HJN           MODULE IDENTIFICATION MACRO
*   PARM=RPARM;                     /* SAVE PARAMETER REGISTER       */
         LR    PARM,RPARM                                       03S0317
*   IEDQPPTR=AVTADBUF;              /* BUFFER PREFIX BASE            */
         L     @08,AVTADBUF(,RAVT)                              03S0318
         ST    @08,IEDQPPTR                                     03S0318
*   EXITCODE=TRANSOK;               /* INITIATE RETURN CODE AS  03S0319
*                                      TRANSLATION OK                */
         SLR   @07,@07                                          03S0319
         ST    @07,EXITCODE                                     03S0319
*   SAVEDB=0;                       /* CLEAR VALUE ENTRY POINTER SAVE
*                                      AREA                          */
         SLR   @07,@07                                          03S0320
         ST    @07,SAVEDB                                       03S0320
*   INPTR=PRMINBFR;                 /* INITIATE INPUT AREA BASE      */
         L     INPTR,PRMINBFR(,PARM)                            03S0321
*   OUTPTR=PRMOUTBF;                /* INITIATE OUTPUT AREA BASE     */
         L     OUTPTR,PRMOUTBF(,PARM)                           03S0322
*   LASTIN=INDATLEN;                /* INITIATE INDEX TO LAST CHAR IN
*                                      INPUT AREA - USED TO DETERMINE
*                                      END OF INPUT DATA             */
         LH    LASTIN,INDATLEN(,INPTR)                          03S0323
*   OUTDATA(1:OUTWKLN)=OUTDATA(1:OUTWKLN)&&OUTDATA(1:OUTWKLN);/* ZERO
*                                      THE OUTPUT AREA               */
         LH    @07,OUTWKLN(,OUTPTR)                             03S0324
         BCTR  @07,0                                            03S0324
         EX    @07,@SX01710                                     03S0324
*   LASTOUT=1;                      /* SET INDEX TO FIRST CHARACTER
*                                      OF OUTPUT AREA -THIS INDEX IS
*                                      USED AS HIGH WATER MARK TO
*                                      LAST BYTE OF OUTPUT DATA      */
         MVC   LASTOUT(2),@CH00061                              03S0325
*   KEYWORD=NO;                     /* RESET KEYWORD BEING PROCESSED
*                                      FLAG                          */
         NI    KEYWORD,B'01111111'                              03S0326
*   PAREN=NO;                       /* RESET PARENTHETICAL VALUE FLAG*/
         NI    PAREN,B'01111111'                                03S0327
*   RFY                                                         03S0328
*     RPARM UNRSTD;                                             03S0328
*   IF USSFLAG=USSTYPE THEN         /* IF THE INPUT TABLE TYPE IS
*                                      USS-FSS                       */
         L     @07,PRMTABLE(,PARM)                              02S0329
         CLI   USSFLAG(@07),X'F1'                               02S0329
         BNE   @RF00329                                         02S0329
*     DO;                                                       02S0330
*                                                               02S0330
*       /*************************************************************/
*       /*                                                           */
*       /* THEN PERFORM USS TO FSS CONVERSION                        */
*       /*                                                           */
*       /*************************************************************/
*                                                               02S0331
*                                                               03S0331
*       /*************************************************************/
*       /*                                                           */
*       /* PROCESS.USSTRANS: THIS SEGMENT REPRESENTS THE TOTAL USS TO*/
*       /* FSS CONVERSION PROCESS.                                   */
*       /*                                                           */
*       /*************************************************************/
*                                                               03S0331
*       IF TRHFI^=USS THEN                                      03S0331
         AL    @08,@CF01713                                     03S0331
         TM    TRHFI(@08),B'00001000'                           03S0331
         BNZ   @RT00331                                         03S0331
*         ;                         /* IF THE INPUT RU IS NOT   03S0332
*                                      UNFORMATTED, THEN NO ACTION IS
*                                      TAKEN                         */
*       ELSE                                                    03S0333
*         DO;                                                   03S0333
*           IF LCBRECVN^=YES THEN   /* ELSE IF THIS IS THE OUTPUT
*                                      SIDE OF THE MH,               */
         TM    LCBRECVN(RLCB),B'00000010'                       03S0334
         BO    @RF00334                                         03S0334
*             EXITCODE=BADTABLE;    /* THEN SET INVALID CONVERSION
*                                      TABLE RETURN CODE AND SUSPEND
*                                      TRANSLATION                   */
*                                                               03S0335
         LA    @08,32                                           03S0335
         ST    @08,EXITCODE                                     03S0335
*           /*********************************************************/
*           /*                                                       */
*           /* FETCH THE VERB FROM THE INPUT DATA STREAM             */
*           /*                                                       */
*           /*********************************************************/
*                                                               03S0336
*           ELSE                                                03S0336
*             DO;                                               03S0336
*                                                               03S0336
         B     @RC00334                                         03S0336
@RF00334 DS    0H                                               03S0337
*               /*****************************************************/
*               /*                                                   */
*               /* FETCH THE VERB FROM THE INPUT STREAM              */
*               /*                                                   */
*               /*****************************************************/
*                                                               03S0337
*                                                               04S0337
*               /*****************************************************/
*               /*                                                   */
*               /* PROCESS.USSTRANS.USSCAN: THIS ROUTINE DELINEATES  */
*               /* THE USS COMMAND VERB FROM THE INPUT DATA STREAM.  */
*               /*                                                   */
*               /*****************************************************/
*                                                               04S0337
*               CHARLEN=0;          /* SET LENGTH OF DELIMITED STRING
*                                      TO ZERO                       */
*                                                               04S0337
         SLR   CHARLEN,CHARLEN                                  04S0337
*               /*****************************************************/
*               /*                                                   */
*               /* SCAN TO FIRST CHARACTER IN THE VERB. IF END OF    */
*               /* INPUT REACHED SET INVALID INPUT RETURN CODE.      */
*               /*                                                   */
*               /*****************************************************/
*                                                               04S0338
*               DO SCANPTR=1 TO LASTIN WHILE(INDATA(SCANPTR)=' ');/*
*                                      SCAN TO VERB                  */
         LA    SCANPTR,1                                        04S0338
         B     @DE00338                                         04S0338
@DL00338 LR    @08,INPTR                                        04S0338
         ALR   @08,SCANPTR                                      04S0338
         CLI   INDATA-1(@08),C' '                               04S0338
         BNE   @DC00338                                         04S0338
*               END;                                            04S0339
         AL    SCANPTR,@CF00061                                 04S0339
@DE00338 CR    SCANPTR,LASTIN                                   04S0339
         BNH   @DL00338                                         04S0339
@DC00338 DS    0H                                               04S0340
*               IF SCANPTR>LASTIN THEN                          04S0340
         CR    SCANPTR,LASTIN                                   04S0340
         BNH   @RF00340                                         04S0340
*                 EXITCODE=BADINPUT;/* IF END OF INPUT SET INVALID
*                                      RETURN CODE                   */
         LA    @08,28                                           04S0341
         ST    @08,EXITCODE                                     04S0341
*               ELSE                                            04S0342
*                 DO;                                           04S0342
         B     @RC00340                                         04S0342
@RF00340 DS    0H                                               04S0343
*                   FSTCHAR=SCANPTR;/* SET POINTER TO VERB           */
*                                                               04S0343
         STH   SCANPTR,FSTCHAR                                  04S0343
*                   /*************************************************/
*                   /*                                               */
*                   /* COUNT THE NUMBER OF CHARACTERS IN THE VERB AND*/
*                   /* SAVE                                          */
*                   /*                                               */
*                   /*************************************************/
*                                                               04S0344
*                   DO SCANPTR=SCANPTR BY 1 WHILE(SCANPTR<=LASTIN&
*                         INDATA(SCANPTR)^=' ');/* LOOP UNTIL BLANK
*                                      FOUND OR END OF INPUT OCCURS  */
         B     @DE00344                                         04S0344
@DL00344 DS    0H                                               04S0345
*                   END;            /* END OF VERB COUNT DO WHILE
*                                      LOOP                          */
         AL    SCANPTR,@CF00061                                 04S0345
@DE00344 CR    SCANPTR,LASTIN                                   04S0345
         BH    @DC00344                                         04S0345
         LR    @08,INPTR                                        04S0345
         ALR   @08,SCANPTR                                      04S0345
         CLI   INDATA-1(@08),C' '                               04S0345
         BNE   @DL00344                                         04S0345
@DC00344 DS    0H                                               04S0346
*                   CHARLEN=SCANPTR-FSTCHAR;/* CALCULATE CHARACTER
*                                      LENGTH                        */
         LR    CHARLEN,SCANPTR                                  04S0346
         SH    CHARLEN,FSTCHAR                                  04S0346
*                 END;                                          04S0347
*               IF CHARLEN=0 THEN   /* IF VERB COULD NOT BE FETCHED,
*                                      ...                           */
@RC00340 C     CHARLEN,@CF01591                                 03S0348
         BNE   @RF00348                                         03S0348
*                 EXITCODE=BADINPUT;/* THEN SET INVALID INPUT RETURN
*                                      CODE AND SUSPEND PROCESSING   */
*                                                               03S0349
         LA    @08,28                                           03S0349
         ST    @08,EXITCODE                                     03S0349
*               /*****************************************************/
*               /*                                                   */
*               /* LOCATE VERB'S CONVERSION TABLE ENTRY              */
*               /*                                                   */
*               /*****************************************************/
*                                                               03S0350
*               ELSE                                            03S0350
*                 DO;                                           03S0350
*                                                               03S0350
         B     @RC00348                                         03S0350
@RF00348 DS    0H                                               03S0351
*                   /*************************************************/
*                   /*                                               */
*                   /* LOCATE VERB'S TABLE ENTRY                     */
*                   /*                                               */
*                   /*************************************************/
*                                                               03S0351
*                                                               04S0351
*                   /*************************************************/
*                   /*                                               */
*                   /* PROCESS.USSTRANS.USSVLOOK: THIS ROUTINE       */
*                   /* SEARCHES THE CONVERSION TABLE FOR THE VERB    */
*                   /* ENTRY FOR THIS VERB.                          */
*                   /*                                               */
*                   /*************************************************/
*                                                               04S0351
*                   RFY                                         04S0351
*                     IEDUSSV BASED(TABLEPTR);                  04S0351
*                   TABLEPTR=PRMTABLE+1;/* INITIATE TABLE POINTER TO
*                                      FIRST VERB ENTRY IN CONVERSION
*                                      TABLE                         */
         LA    TABLEPTR,1                                       04S0352
         AL    TABLEPTR,PRMTABLE(,PARM)                         04S0352
*                   USSVB=0;        /* CLEAR CURRENT VERB ENTRY 04S0353
*                                      POINTER                       */
         SLR   @08,@08                                          04S0353
         ST    @08,USSVB                                        04S0353
*                   DO WHILE(TABLEPTR^=0);/* SEARCH FOR VERB ENTRY
*                                      UNTIL END OF TABLE REACHED.
*                                      TABLE ENTRY POINTER IS ZEROED
*                                      WHEN VERB FOUND OR IS ZERO
*                                      WHEN END OF TABLE.            */
         B     @DE00354                                         04S0354
@DL00354 DS    0H                                               04S0355
*                     IF CHARLEN=USSVLEN&INDATA(FSTCHAR:FSTCHAR+CHARLEN
*                         -1)=USSVERB(1:CHARLEN) THEN           04S0355
         SLR   @08,@08                                          04S0355
         IC    @08,USSVLEN(,TABLEPTR)                           04S0355
         CR    CHARLEN,@08                                      04S0355
         BNE   @RF00355                                         04S0355
         LH    @08,FSTCHAR                                      04S0355
         LR    @05,CHARLEN                                      04S0355
         BCTR  @05,0                                            04S0355
         ALR   @08,INPTR                                        04S0355
         EX    @05,@SC01714                                     04S0355
         BNE   @RF00355                                         04S0355
*                       DO;                                     04S0356
*                                                               04S0356
*                         /*******************************************/
*                         /*                                         */
*                         /* IF THIS IS THE VERB ENTRY WE'RE LOOKING */
*                         /* FOR, ... THEN ...                       */
*                         /*                                         */
*                         /*******************************************/
*                                                               04S0357
*                         USSVB=TABLEPTR;/* SET VERB ENTRY BASE      */
         ST    TABLEPTR,USSVB                                   04S0357
*                         TABLEPTR=0;/* CLEAR TABLE SCAN POINTER TO
*                                      BREAK DO WHILE LOOP           */
         SLR   TABLEPTR,TABLEPTR                                04S0358
*                       END;                                    04S0359
*                     ELSE                                      04S0360
*                       TABLEPTR=USSVADDR;/* ELSE, SET POINTER TO
*                                      NEXT VERB ENTRY IN TABLE      */
         B     @RC00355                                         04S0360
@RF00355 ICM   TABLEPTR,15,USSVADDR(TABLEPTR)                   04S0360
*                   END;            /* END OF TABLE SEARCH           */
@RC00355 DS    0H                                               04S0361
@DE00354 SLR   @08,@08                                          04S0361
         CR    TABLEPTR,@08                                     04S0361
         BNE   @DL00354                                         04S0361
*                   RFY                                         04S0362
*                     IEDUSSV BASED(USSVB);                     04S0362
*                   IF USSVB=0 THEN /* IF TABLE ENTRY NOT FOUND, ... */
         C     @08,USSVB                                        03S0363
         BNE   @RF00363                                         03S0363
*                     EXITCODE=BADVERB;/* THEN SET INVALID VERB 03S0364
*                                      RETURN CODE AND SUSPEND  03S0364
*                                      PROCESSING                    */
         LA    @08,20                                           03S0364
         ST    @08,EXITCODE                                     03S0364
*                   ELSE                                        03S0365
*                     DO;           /* ELSE CONTINUE USS CONVERSION
*                                      PROCESS                       */
*                                                               03S0365
         B     @RC00363                                         03S0365
@RF00363 DS    0H                                               03S0366
*                       /*********************************************/
*                       /*                                           */
*                       /* PLACE SNA COMMAND AND IDENTIFIER INTO     */
*                       /* FORMATTED COMMAND                         */
*                       /*                                           */
*                       /*********************************************/
*                                                               03S0366
*                       OUTDATA(1:LENGTH(USSVSSCP))=USSVSSCP;   03S0366
*                                                               03S0366
         L     @08,USSVB                                        03S0366
         MVC   OUTDATA(3,OUTPTR),USSVSSCP(@08)                  03S0366
*                       /*********************************************/
*                       /*                                           */
*                       /* SCAN FOR DATA DELIMITER AND MOVE USER DATA*/
*                       /* TO OUTPUT AREA                            */
*                       /*                                           */
*                       /*********************************************/
*                                                               03S0367
*                                                               04S0367
*                       /*********************************************/
*                       /*                                           */
*                       /* PROCESS.USSTRANS.USSDEL: SCAN FOR USER    */
*                       /* DATA DELIMITER, IF THERE IS ONE, AND IF   */
*                       /* FOUND MOVE USER DATA TO FORMATTED OUTPUT. */
*                       /*                                           */
*                       /*********************************************/
*                                                               04S0367
*                       IF USSVDEL=0 THEN                       04S0367
         CLI   USSVDEL(@08),0                                   04S0367
         BE    @RT00367                                         04S0367
*                         ;         /* IF NO USER DATA DELIMITER
*                                      SPECIFIED, THEN NO FURTHER
*                                      ACTION                        */
*                                                               04S0368
*                       /*********************************************/
*                       /*                                           */
*                       /* SEARCH FOR DELIMITER                      */
*                       /*                                           */
*                       /*********************************************/
*                                                               04S0369
*                       ELSE                                    04S0369
*                         DO;       /* ELSE ...                      */
*                           FOUND=NO;/* RESET FOUND SWITCH           */
         NI    FOUND,B'01111111'                                04S0370
*                           DO DELINDEX=SCANPTR TO LASTIN-1 WHILE(FOUND
*                                 =NO);/* SEARCH INPUT UNTIL END OF
*                                      DATA REACHED OR DELIMITER
*                                      FOUND                         */
         LR    DELINDEX,SCANPTR                                 04S0371
         B     @DE00371                                         04S0371
@DL00371 TM    FOUND,B'10000000'                                04S0371
         BNZ   @DC00371                                         04S0371
*                             IF INDATA(DELINDEX)=USSVDEL THEN/* IF
*                                      THIS IS THE DELIMITER, ...    */
         LR    @08,INPTR                                        04S0372
         ALR   @08,DELINDEX                                     04S0372
         L     @05,USSVB                                        04S0372
         CLC   INDATA-1(1,@08),USSVDEL(@05)                     04S0372
         BNE   @RF00372                                         04S0372
*                               FOUND=YES;/* THEN INDICATE DELIMITER
*                                      FOUND                         */
         OI    FOUND,B'10000000'                                04S0373
*                           END;    /* END OF DELIMITER SEARCH       */
@RF00372 AL    DELINDEX,@CF00061                                04S0374
@DE00371 LR    @08,LASTIN                                       04S0374
         BCTR  @08,0                                            04S0374
         CR    DELINDEX,@08                                     04S0374
         BNH   @DL00371                                         04S0374
@DC00371 DS    0H                                               04S0375
*                           IF FOUND=NO THEN                    04S0375
         TM    FOUND,B'10000000'                                04S0375
         BZ    @RT00375                                         04S0375
*                             ;     /* IF DELIMITER NOT FOUND THEN NO
*                                      FURTHER ACTION                */
*                                                               04S0376
*                           /*****************************************/
*                           /*                                       */
*                           /* IF USER DATA EXCEEDS MAXIMUM, ABORT   */
*                           /*                                       */
*                           /*****************************************/
*                                                               04S0377
*                           ELSE                                04S0377
*                             DO;   /* ELSE CONTINUE PROCESSING      */
*                               CHARLEN=LASTIN-DELINDEX;/* SAVE USER
*                                      DATA LENGTH                   */
         LR    CHARLEN,LASTIN                                   04S0378
         SLR   CHARLEN,DELINDEX                                 04S0378
*                               IF CHARLEN>USSVDLEN THEN/* IF USER
*                                      DATA LENGTH EXCEEDS FORMATTED
*                                      FIELD WIDTH, ...              */
         L     @08,USSVB                                        04S0379
         SLR   @05,@05                                          04S0379
         IC    @05,USSVDLEN(,@08)                               04S0379
         CR    CHARLEN,@05                                      04S0379
         BNH   @RF00379                                         04S0379
*                                 EXITCODE=TOOMUCH;/* THEN SET  04S0380
*                                      INVALID RETURN CODE AND  04S0380
*                                      SUSPEND PROCESSING            */
*                                                               04S0380
         LA    @08,40                                           04S0380
         ST    @08,EXITCODE                                     04S0380
*                               /*************************************/
*                               /*                                   */
*                               /* ELSE PLACE USER DATA INTO         */
*                               /* FORMATTED OUTPUT AREA             */
*                               /*                                   */
*                               /*************************************/
*                                                               04S0381
*                               ELSE                            04S0381
*                                 DO;                           04S0381
         B     @RC00379                                         04S0381
@RF00379 DS    0H                                               04S0382
*                                   DO COUNTER=(USSVDISP+2) TO(USSVDISP
*                                        +1+USSVDLEN);/* LOOP UNTIL
*                                      OUTPUT AREA BLANKED           */
         LA    @10,2                                            04S0382
         L     @08,USSVB                                        04S0382
         SLR   @05,@05                                          04S0382
         IC    @05,USSVDISP(,@08)                               04S0382
         ALR   @10,@05                                          04S0382
         B     @DE00382                                         04S0382
@DL00382 DS    0H                                               04S0383
*                                    OUTDATA(COUNTER)=' ';/* BLANK
*                                      CHARACTER                     */
         ALR   @10,OUTPTR                                       04S0383
         MVI   OUTDATA-1(@10),C' '                              04S0383
*                                   END;/* END BLANKING LOOP         */
*                                                               04S0384
         SLR   @08,@08                                          04S0384
         IC    @08,COUNTER                                      04S0384
         AL    @08,@CF00061                                     04S0384
         LR    @10,@08                                          04S0384
@DE00382 STC   @10,COUNTER                                      04S0384
         LA    @08,1                                            04S0384
         L     @05,USSVB                                        04S0384
         SLR   @15,@15                                          04S0384
         IC    @15,USSVDISP(,@05)                               04S0384
         ALR   @08,@15                                          04S0384
         L     @05,USSVB                                        04S0384
         SLR   @15,@15                                          04S0384
         IC    @15,USSVDLEN(,@05)                               04S0384
         ALR   @08,@15                                          04S0384
         CR    @10,@08                                          04S0384
         BNH   @DL00382                                         04S0384
*                                   /*********************************/
*                                   /*                               */
*                                   /* SET USER DATA LENGTH INTO     */
*                                   /* OUTPUT AREA                   */
*                                   /*                               */
*                                   /*********************************/
*                                                               04S0385
*                                   OUTDATA(USSVDISP+1)=USSVDLEN;
         L     @08,USSVB                                        04S0385
         SLR   @05,@05                                          04S0385
         IC    @05,USSVDISP(,@08)                               04S0385
         SLR   @15,@15                                          04S0385
         IC    @15,USSVDLEN(,@08)                               04S0385
         STC   @15,OUTDATA(@05,OUTPTR)                          04S0385
*                                   OUTDATA(USSVDISP+2:USSVDISP+CHARLEN
*                                       +1)=INDATA(DELINDEX+1:DELINDEX+
*                                       CHARLEN);/* PLACE USER DATA
*                                      INTO FORMATTED OUTPUT AREA    */
         ALR   @05,OUTPTR                                       04S0386
         LR    @08,CHARLEN                                      04S0386
         BCTR  @08,0                                            04S0386
         LR    @01,INPTR                                        04S0386
         ALR   @01,DELINDEX                                     04S0386
         EX    @08,@SM01717                                     04S0386
*                                   LASTOUT=USSVDISP+USSVDLEN+1;/*
*                                      SAVE OUTPUT HIGH WATER MARK   */
         L     @08,USSVB                                        04S0387
         SLR   @05,@05                                          04S0387
         IC    @05,USSVDISP(,@08)                               04S0387
         ALR   @05,@15                                          04S0387
         LA    @05,1(,@05)                                      04S0387
         STH   @05,LASTOUT                                      04S0387
*                                   LASTIN=DELINDEX-2;/* RESET LAST
*                                      INPUT DATA LIMITING ADDRESS   */
         LR    LASTIN,DELINDEX                                  04S0388
         BCTR  LASTIN,0                                         04S0388
         BCTR  LASTIN,0                                         04S0388
*                                 END;                          04S0389
*                             END;                              04S0390
@RC00379 DS    0H                                               04S0391
*                         END;                                  04S0391
@RT00375 DS    0H                                               04S0392
*                       IF EXITCODE=TRANSOK THEN                03S0392
@RT00367 L     @08,EXITCODE                                     03S0392
         LTR   @08,@08                                          03S0392
         BNZ   @RF00392                                         03S0392
*                         DO;       /* IF ERROR WAS NOT FOUND,  03S0393
*                                      CONTINUE PROCESSING           */
*                                                               03S0393
*                           /*****************************************/
*                           /*                                       */
*                           /* PERFORM CONVERSION OF POSITIONAL      */
*                           /* PARAMETERS AND KEYWORDS FOR THIS VERB */
*                           /* AS SPECIFIED IN THE CONVERSION TABLE  */
*                           /*                                       */
*                           /*****************************************/
*                                                               03S0394
*                                                               04S0394
*                           /*****************************************/
*                           /*                                       */
*                           /* PROCESS.USSTRANS.USSVERB: THIS SEGMENT*/
*                           /* PERFORMS USS-FSS CONVERSION AS        */
*                           /* SPECIFIED IN THE TABLE FOR THE VERB   */
*                           /* ENTRY LOCATED EARLIER.                */
*                           /*                                       */
*                           /*****************************************/
*                                                               04S0394
*                           USSPB=USSVB+LENGTH(IEDUSSV)+USSVLEN;/*
*                                      ADDRESS OF FIRST TABLE ENTRY
*                                      FOLLOWING THE VERB ENTRY      */
*                                                               04S0394
         L     @08,USSVB                                        04S0394
         LA    USSPB,17                                         04S0394
         ALR   USSPB,@08                                        04S0394
         SLR   @02,@02                                          04S0394
         IC    @02,USSVLEN(,@08)                                04S0394
         ALR   USSPB,@02                                        04S0394
*                           /*****************************************/
*                           /*                                       */
*                           /* PROCESS THOSE COMMAND FIELDS HAVING NO*/
*                           /* USER INPUT                            */
*                           /*                                       */
*                           /*****************************************/
*                                                               04S0395
*                                                               05S0395
*                           /*****************************************/
*                           /*                                       */
*                           /* PROCESS.USSTRANS.USSVERB.USSNOINP:    */
*                           /* THIS SEGMENT PLACES DATA INTO A SNA   */
*                           /* FORMATTED COMMAND FIELD FOR WHICH NO  */
*                           /* USER INPUT IS REQUIRED. THESE FIELDS  */
*                           /* ARE REPRESENTED BY POSITIONAL         */
*                           /* PARAMETER CONVERSION TABLE ENTRIES    */
*                           /* WHICH ARE FLAGGED AS NO HAVEING USER  */
*                           /* DATA INPUT THE USER'S INPUT SYNTAX IS */
*                           /* NOT AFFECTED BY THESE ENTRIES, THEY   */
*                           /* ARE TRANSPARENT TO THE USER.          */
*                           /*                                       */
*                           /*****************************************/
*                                                               05S0395
*                           DO WHILE(USSPTYPE=USSFPOS&USSPNOIN=YES);
         B     @DE00395                                         05S0395
@DL00395 DS    0H                                               05S0396
*                                   /* LOOP UNTIL ALL NO INPUT  05S0396
*                                      ENTRIES PROCESSED             */
*                             USSDB=USSPB+LENGTH(IEDUSSP);/* ADDRESS
*                                      OF VALUE ENTRY                */
         LA    @08,6                                            05S0396
         ALR   @08,USSPB                                        05S0396
         ST    @08,USSDB                                        05S0396
