*/********************************************************************/
*/*                                                                  */
*/*      STEAL - IRARMSTM INTERNAL SUBROUTINE                        */
*/*                                                                  */
*/*        FUNCTIONS -                                               */
*/*          1) ADD USER TO RFR INTERFACE LIST                       */
*/*          2) IF LIST IS FULL,CALL REAL FRAME REPLACEMENT          */
*/*          3) ACCUMULATE NUMBER OF PAGES STOLEN                    */
*/*                                                                  */
*/*        INPUT -                                                   */
*/*          REG1 = REAL FRAME REPLACEMENT INTERFACE BUFFER ADDRESS  */
*/*          REG4 = ASCB ADDRESS                                     */
*/*          PRX = INDEX INTO INTERFACE LIST                         */
*/*          PRXEND = MAXIMUM INTERFACE LIST INDEX                   */
*/*                                                                  */
*/********************************************************************/
*/*STEAL: CHART (AMODE,SEQ,AREF)                                     */
*/* HEADER                                                         0489
*/* AUG 8,1975                                        STEAL-       0489
*/* RFR INTERFACE LIST BUILDER                                       */
*/*STEAL: E INTERNAL SUBR                                            */
*/* D (YES,%A,NO,) FLUSH INTERFACE LIST ?                            */
*/* P ADD USER TO LIST                                               */
*/* D (NO,%B,YES,) IS LIST FULL ?                                    */
*/*%A: S IRARMI03: CALL REAL FRAME REPLACEMENT                       */
*/* P SAVE COUNT OF STOLEN PAGES BY MEMORY IN OUXB                   */
*/* P UPDATE COUNT OF TOTAL STOLEN PAGES                             */
*/*%B: R RETURN TO CALLER                                            */
*/*STEAL: END                                                        */
*                                                                  0489
*STEAL:                                                            0489
*   PROC OPTIONS(NOSAVE);                                          0489
@EL00001 DS    0H                                                  0489
@EF00001 DS    0H                                                  0489
@ER00001 BR    @14                                                 0489
STEAL    DS    0H                                                  0490
*   RESPECIFY                                                      0490
*    (RFRPTR,                                                      0490
*     PRX) RESTRICTED;              /*                       @Z30WIMG*/
*   IF PRX^=PRXEND THEN             /* TEST NOT FLUSH LIST   @Z30WIMG*/
         C     PRX,PRXEND                                          0491
         BE    @RF00491                                            0491
*     DO;                           /*                       @Z30WIMG*/
*/*                                                                  */
*/*      ADD USER TO RFR INTERFACE LIST                              */
*/*                                                                  */
*                                                                  0493
*       RPRFRSCT=PRSCT;             /* SET MAXIMUM STEAL CNT @ZA20337*/
         LH    @09,PRSCT                                           0493
         STH   @09,RPRFRSCT(,PRX)                                  0493
*       RPRFRINV=PRFLGS;            /* SET OPTION FLAGS      @Z30WIPH*/
         MVC   RPRFRINV(1,PRX),PRFLGS                              0494
*       RPRFRCRI=PRCRI;             /* SET STEAL CRITERIA NUMBER     */
         LH    @09,PRCRI                                           0495
         STC   @09,RPRFRCRI(,PRX)                                  0495
*       RPRFRUSR=ASCBPTR;           /* MOVE IN USER POINTER  @Z30WIMG*/
         ST    ASCBPTR,RPRFRUSR(,PRX)                              0496
*       PRX=PRX+LENGTH(RPRFRINT);   /* POINT TO NEXT ENTRY   @Z30WIMG*/
         AL    PRX,@CF00164                                        0497
*     END;                          /*                       @Z30WIMG*/
*/*                                                          @Z30WIMG*/
*/*      IF LIST IS FULL, CALL REAL FRAME REPLACEMENT        @Z30WIMG*/
*/*                                                          @Z30WIMG*/
*                                                                  0499
*   IF PRX=PRXEND THEN              /* TEST IF LIST IS FULL  @Z30WIPH*/
@RF00491 C     PRX,PRXEND                                          0499
         BNE   @RF00499                                            0499
*     DO;                           /* IF SO,                @Z30WIMG*/
*       PRX=PRX-LENGTH(RPRFRINT);   /* POINT TO LAST ENTRY   @Z30WIPH*/
         SL    PRX,@CF00164                                        0501
*       RPRFREND='1'B;              /* INDICATE LAST ENTRY   @Z30WIMG*/
         OI    RPRFREND(PRX),B'10000000'                           0502
*       PRX=RTNREG;                 /* SAVE RETURN ADDRESS   @Z30WIMG
*                                      CALL REAL FRAME       @Z30WIPH
*                                      REPLACEMENT           @Z30WIPH*/
*                                                                  0503
         LR    PRX,RTNREG                                          0503
*       /*************************************************************/
*       /*                                                           */
*       /* ? IRACTLCL EPN(I03)                                       */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0504
*       CALL RMSBI03->IRARMRTN;     /* INVOKE I03 SUBRTN             */
*                                                                  0504
         L     @15,RMSBI03(,RMCTPTR)                               0504
         BALR  @14,@15                                             0504
*       /*************************************************************/
*       /*                                                           */
*       /*                                                   @Z30WIMG*/
*       /*                                                           */
*       /*************************************************************/
*                                                                  0505
*       RTNREG=PRX;                 /* RESTORE RETURN ADDRESS      0505
*                                                            @Z30WIMG*/
         LR    RTNREG,PRX                                          0505
*       DO PRX=(PRXEND-             /* LOOP THROUGH          @Z30WIPH*/
*             LENGTH(RPRFRINT))     /* EACH ENTRY            @Z30WIPH*/
*             TO RFRPTR BY          /*                       @Z30WIPH*/
*             -LENGTH(RPRFRINT);    /*                       @Z30WIPH*/
         L     PRX,PRXEND                                          0506
         SL    PRX,@CF00164                                        0506
         B     @DE00506                                            0506
@DL00506 DS    0H                                                  0507
*         PRSTCNT=PRSTCNT+RPRFRSCT; /* ADD STOLEN PAGES CNT  @Z30WIPH*/
*                                                                  0507
         LH    @09,RPRFRSCT(,PRX)                                  0507
         ALR   PRSTCNT,@09                                         0507
*         /***********************************************************/
*         /*                                                         */
*         /* ACCUMULATE NUMBER OF PAGES STOLEN FROM EACH USER        */
*         /*                                                 @Z40BPSS*/
*         /*                                                         */
*         /***********************************************************/
*                                                                  0508
*         IF RPRASCBA^=I0 THEN      /* NOT FOR SPA           @Z40BPSS*/
         L     @07,RPRASCBA-1(,PRX)                                0508
         LA    @07,0(,@07)                                         0508
         LTR   @07,@07                                             0508
         BZ    @RF00508                                            0508
*           RPRASCBA->ASCBOUXB->OUXBSTCT=RPRASCBA->ASCBOUXB->OUXBSTCT+
*               RPRFRSCT;           /*                       @Z40BPSS*/
         L     @07,ASCBOUXB(,@07)                                  0509
         AL    @09,OUXBSTCT(,@07)                                  0509
         ST    @09,OUXBSTCT(,@07)                                  0509
*       END;                        /*                       @Z30WIMG*/
@RF00508 AL    PRX,@CF02211                                        0510
@DE00506 CR    PRX,RFRPTR                                          0510
         BNL   @DL00506                                            0510
*       PRX=ADDR(RFRLST1);          /* INITIALIZE LIST INDEX @Z30WIMG*/
         LA    PRX,RFRLST1                                         0511
*     END;                          /*                       @Z30WIMG*/
*   RESPECIFY                                                      0513
*    (RFRPTR,                                                      0513
*     PRX) UNRESTRICTED;            /*                       @Z30WIMG*/
*   END;                                                           0514
@EL00002 DS    0H                                                  0514
@EF00002 DS    0H                                                  0514
@ER00002 BR    @14                                                 0514
*   END IRARMSTM                                                   0515
