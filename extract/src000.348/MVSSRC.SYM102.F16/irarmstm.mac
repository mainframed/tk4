*/********************************************************************/
*/*                                                                  */
*/* ENTRY POINT NAME = IRARMPR5                                      */
*/*                                                                  */
*/* DESCRIPTIVE NAME = PAGE REPLACEMENT REAL PAGE SHORTAGE FORCE     */
*/*                    STEAL ROUTINE                                 */
*/*                                                                  */
*/* FUNCTION = STEAL AS MANY PAGES AS REQUIRED TO RELIEVE PAGE       */
*/*            SHORTAGES. THE STEAL DECISION IS MADE AT ENTRY        */
*/*            MS2. THE OLDEST UNREFERENCED SYSTEM WIDE PAGES        */
*/*            ARE STOLEN FIRST.                                     */
*/*                                                                  */
*/* CALLER = IRARMCTL - SRM CONTROL MODULE                           */
*/*            ALGORITHM REQUEST PROCESSOR                           */
*/*                                                                  */
*/* INPUT = R2 = RMCT ADDRESS                                        */
*/*         R3 = RRPA ADDRESS                                        */
*/*         MCVFRCNT = NUMBER OF PAGES THAT MUST BE STOLEN TO        */
*/*                    REACH MAIN STORAGE TARGET                     */
*/*         MCVSTCRI = THE CRITERIA THAT PR5 WILL START TO           */
*/*                    STEAL AT                                      */
*/*                                                                  */
*/* OUTPUT = NONE                                                    */
*/*                                                                  */
*/********************************************************************/
*/*IRARMPR5: CHART (AMODE,SEQ,AREF)                                  */
*/* HEADER                                                         0122
*/* AUG 8,1975                                         IRARMPR5-   0122
*/* REAL PAGE SHORTAGE FORCE STEAL RTN                               */
*/*IRARMPR5: E INVOKED BY MS2                                        */
*/* P INITIALIZE RFR INTERFACE LIST                                  */
*/* N DO NOT INCR UICS, # FRAMES NEEDED FOUND IN MCVFRCNT            */
*/* P SET STEAL CRITERIA EQUAL TO HIGHEST UIC (MCVSTCRI)             */
*/* P SET MAXIMUM STEAL NUMBER OF PAGES TO STEAL (STLCT)             */
*/*%A: P OBTAIN OUCB FROM INQUE                                      */
*/* D (NO,%D,YES,) USER IN CORE ?                                    */
*/* D (NO,%D,YES,) IS CRITERIA LE OUXBUIC ?                          */
*/* S STEAL: GO INIT RFR INTERFACE LIST                              */
*/* D (YES,%B,NO,) ENOUGH PAGES STOLEN ?                             */
*/*%D: D (NO,%A,YES,) END OF INQUE ?                                 */
*/* D (YES,%B,NO,) ENOUGH PAGES STOLEN ?                             */
*/* D (NO,%B,YES,) IS CRITERIA LE PVTCHUIC ?                         */
*/* S STEAL: STEAL FROM SPA - ASCB=0                                 */
*/*%B: D (NO,%E,YES,) FRAMES STILL NEEDED ?                          */
*/* D (YES,%A,NO,) PAGES AVAIL AT CURRENT STL CRITERIA ?             */
*/* P DECREMENT HIGHEST UIC VALUE - MCVSTCRI                         */
*/* P (,%A) DECREMENT RFR CRITERIA AND STEAL SOME MORE               */
*/*%E: R RETURN TO CALLER                                            */
*/*IRARMPR5: END                                                     */
*/*   ? IRACTLEP EPN(PR5)                                            */
*                                                                  0122
*   DCL                                                            0122
*     IRARMPR5 ENTRY INT;                                          0122
*   GEN CODE REFS(RRPAEPA) DEFS(IRARMPR5) NOEXIT SEQFLOW;          0123
*                                                                  0123
*                                  /*
         ENTRY IRARMPR5
IRARMPR5 DS    0H
         STM   @00,@14,000+000(@13)    SAVE INVOKING RTNE REGS
         ST    @01,RRPAEPA(,RRPAPTR)   INDICATE NEW CURRENT RMEP
         LA    @12,IRARMPR5-@PSTART    DISPLACEMENT TO MODULE BASE
         LCR   @12,@12                 NEGATIVE FOR SUBTRACT
         ALR   @12,@15                 SETUP MODULE ADDRESSABILITY
*/*  INITIALIZE FOR PAGE REPLACEMENT PROCESSING              @Z40BPSS*/
*                                                                  0124
*   RESPECIFY                                                      0124
*    (RFRPTR,                                                      0124
*     PRX) RESTRICTED;                                             0124
*   RFRPTR=ADDR(RFRLST1);           /* SET UP ADDRESSING TO INTERFACE
*                                      LIST                          */
         LA    RFRPTR,RFRLST1                                      0125
*   PRSTCNT=I0;                     /* ZERO STEAL COUNT      @Z30WIPH*/
         SLR   PRSTCNT,PRSTCNT                                     0126
*   PRSCT=STLCNT;                   /* FRAMES TO STL PER CAL @ZA20361*/
         L     @10,STLCNT                                          0127
         STH   @10,PRSCT                                           0127
*   PRX=RFRPTR;                     /* RESET LIST INDEX      @Z30WIMG*/
         LR    PRX,RFRPTR                                          0128
*   PRXEND=PRX+                     /* STEAL FROM ONE USER           */
*       LENGTH(RPRFRINT);           /* USER AT A TIME                */
         LA    @10,8                                               0129
         ALR   @10,PRX                                             0129
         ST    @10,PRXEND                                          0129
*   PRFLGS='';                      /* CLEAR STEAL FLAGS     @Z30WIPH*/
*                                                                  0130
         MVI   PRFLGS,X'00'                                        0130
*   /*****************************************************************/
*   /*                                                               */
*   /* PAGES ARE NEEDED, KEEP STEALING FROM ALL USERS AND THE SYSTEM */
*   /* PAGEABLE AREAS UNTIL STORAGE SHORTAGE IS RELIEVED     @Z40BPSS*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0131
*   DO MSX=MCVSTCRI TO-I1           /* LOOP UNTIL ALL PAGES  @Z40BPSS*/
*         BY-I1 WHILE               /* ARE STOLEN OR SHORTGE @Z40BPSS*/
*         MCVFRCNT>PRSTCNT;         /* IS RELIEVED           @ZA20361*/
         LH    MSX,MCVSTCRI(,RMCTPTR)                              0131
         B     @DE00131                                            0131
@DL00131 CH    PRSTCNT,MCVFRCNT(,RMCTPTR)                          0131
         BNL   @DC00131                                            0131
*     MSXBUMP=I0;                   /*                       @ZA20361*/
         SLR   @09,@09                                             0132
         ST    @09,MSXBUMP                                         0132
*     PRCRI=MAX(I0,MSX);            /* USE MSX AS CRITERIA AND     0133
*                                      PROCESS TWICE AT CRITERIA ZERO
*                                                            @Z40BPSS*/
*                                                                  0133
         CR    @09,MSX                                             0133
         BNL   *+6
         LR    @09,MSX                                             0133
         STH   @09,PRCRI                                           0133
*     /***************************************************************/
*     /*                                                             */
*     /* STEAL FROM PRIVATE MEMORIES                         @Z40BPSS*/
*     /*                                                             */
*     /***************************************************************/
*                                                                  0134
*     OUCBPTR=RMCTINQE;             /* ADDRESSIBILITY TO INQ @Z40BPSS*/
         L     OUCBPTR,RMCTINQE(,RMCTPTR)                          0134
*     OUCBPTR=OUCBFWD;              /* POINT TO FIRST MEMORY @Z40BPSS*/
         L     OUCBPTR,OUCBFWD(,OUCBPTR)                           0135
*     DO WHILE OUCBNAME='OUCB'&     /* LOOP THRU INQ UNTIL   @Z40BPSS*/
*           MCVFRCNT>PRSTCNT;       /* SHORTAGE IS RELIEVED  @ZA20361*/
         B     @DE00136                                            0136
@DL00136 DS    0H                                                  0137
*       ASCBPTR=OUCBASCB;           /* ADDRESSIBILITY TO ASCB      0137
*                                                            @Z40BPSS*/
         L     ASCBPTR,OUCBASCB(,OUCBPTR)                          0137
*       OUXBPTR=ASCBOUXB;           /* ADDRESSIBILITY TO OUXB      0138
*                                                            @Z40BPSS*/
         L     OUXBPTR,ASCBOUXB(,ASCBPTR)                          0138
*       IF OUCBOUT='0'B&            /* STEAL ONLY FROM       @Z40BPSS*/
*           OUCBGOI='0'B&           /*                       @Z40BPSS*/
*           OUCBGOB='0'B&           /*                       @Z40BPSS*/
*           OUCBQSC='0'B&           /* GOO TILL QUIESCE CMPT @ZM43063*/
*           MSX=<OUXBUIC&           /* SOME ARE AVAILABLE    @Z40BPSS*/
*           ASCBRSM^=I0 THEN                                       0139
         TM    OUCBOUT(OUCBPTR),B'01100100'                        0139
         BNZ   @RF00139                                            0139
         TM    OUCBQSC(OUCBPTR),B'00000010'                        0139
         BNZ   @RF00139                                            0139
         CH    MSX,OUXBUIC(,OUXBPTR)                               0139
         BH    @RF00139                                            0139
         L     @09,ASCBRSM(,ASCBPTR)                               0139
         LTR   @09,@09                                             0139
         BZ    @RF00139                                            0139
*         DO;                       /*                       @ZA20361*/
*           CALL STEAL;             /* INIT RFR INTERFACE LST      0141
*                                                            @Z40BPSS*/
         BAL   @14,STEAL                                           0141
*           IF RPRFRSCT^=I0&MSX=<OUXBUIC THEN/* PAGES WERE STOLEN AND
*                                      HI UIC DECREMENTED    @ZA20361*/
         LH    @09,RPRFRSCT(,PRX)                                  0142
         LTR   @09,@09                                             0142
         BZ    @RF00142                                            0142
         CH    MSX,OUXBUIC(,OUXBPTR)                               0142
         BH    @RF00142                                            0142
*             MSXBUMP=I1;           /* NEXT PASS STL AT SAME CRITERIA
*                                                            @ZA20361*/
         MVC   MSXBUMP(4),@CF00090                                 0143
*         END;                      /*                       @ZA20361*/
@RF00142 DS    0H                                                  0145
*       OUCBPTR=OUCBFWD;            /* EXAMINE NEXT ADDR SPACE     0145
*                                                            @Z40BPSS*/
@RF00139 L     OUCBPTR,OUCBFWD(,OUCBPTR)                           0145
*     END;                                                         0146
*                                                                  0146
@DE00136 CLC   OUCBNAME(4,OUCBPTR),@CC02185                        0146
         BNE   @DC00136                                            0146
         CH    PRSTCNT,MCVFRCNT(,RMCTPTR)                          0146
         BL    @DL00136                                            0146
@DC00136 DS    0H                                                  0147
*     /***************************************************************/
*     /*                                                             */
*     /* STEAL FROM SYSTEM PAGEABLE AREA                     @Z40BPSS*/
*     /*                                                             */
*     /***************************************************************/
*                                                                  0147
*     IF MCVFRCNT>PRSTCNT&          /* STILL NEED FRAMES     @ZA20361*/
*         MSX=<PVTCHUIC THEN                                       0147
         CH    PRSTCNT,MCVFRCNT(,RMCTPTR)                          0147
         BNL   @RF00147                                            0147
         L     @09,CVTPTR                                          0147
         L     @09,PVTPTR(,@09)                                    0147
         CH    MSX,PVTCHUIC(,@09)                                  0147
         BH    @RF00147                                            0147
*       DO;                         /* CAN GET SOME          @Z40BPSS*/
*         ASCBPTR=I0;               /* STEAL FROM COMMON     @Z40BPSS*/
         SLR   ASCBPTR,ASCBPTR                                     0149
*         CALL STEAL;               /* INIT RFR INTERFACE LIST     0150
*                                                            @Z40BPSS*/
         BAL   @14,STEAL                                           0150
*         IF RPRFRSCT^=I0&          /* IF PAGES STOLEN AND UIC     0151
*                                                            @ZA20361*/
*             MSX=<PVTCHUIC         /* DECREMENTED USE THE SAME    0151
*                                                            @ZA20361*/
*           THEN                                                   0151
         LH    @09,RPRFRSCT(,PRX)                                  0151
