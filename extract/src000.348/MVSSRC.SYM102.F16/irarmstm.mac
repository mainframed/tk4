@PROLOG  BALR  @12,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@12                                         0001
*                                                                  0052
*                                  /*
         ENTRY IRARMPR1
IRARMPR1 DS    0H
         STM   @00,@14,000+000(@13)    SAVE INVOKING RTNE REGS
         ST    @01,RRPAEPA(,RRPAPTR)   INDICATE NEW CURRENT RMEP
         LA    @12,IRARMPR1-@PSTART    DISPLACEMENT TO MODULE BASE
         LCR   @12,@12                 NEGATIVE FOR SUBTRACT
         ALR   @12,@15                 SETUP MODULE ADDRESSABILITY
*/*                                                                  */
*/*      INITIALIZE PAGE REPLACEMENT PROCESSING                      */
*/*                                                                  */
*                                                                  0053
*   IF UPPVTUIC='0'B&               /* WAS AN SRB SCHEDULED  @ZA33182*/
*       UPCOMUIC='0'B THEN                                         0053
         TM    UPPVTUIC,B'01100000'                                0053
         BNZ   @RF00053                                            0053
*     DO;                           /* FOR PR1 - IF NO THEN SCHEDULE
*                                      ONE AND RETURN        @ZA33182*/
*                                                                  0054
*       /*************************************************************/
*       /*                                                           */
*       /* ? IRACTLCL EPN(PR1)                                       */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0055
*       DO;                                                        0055
*         R1=ADDR(RMEPBPR1);        /* PR1 REQUEST IS PARAM          */
         LA    R1,RMEPBPR1(,RMCTPTR)                               0056
*         CALL RMSBCRL->IRARMRTN;   /* ALGRTHM SCH FUNCT             */
         L     @15,RMSBCRL(,RMCTPTR)                               0057
         BALR  @14,@15                                             0057
*       END;                                                       0058
*       UPPVTUIC='1'B;              /* INDICATE PVT UIC UPDT @ZA33182*/
         OI    UPPVTUIC,B'01000000'                                0059
*     END;                          /*                       @ZA33182*/
*   ELSE                                                           0061
*     DO;                           /* SRB WAS SCHEDULED     @ZA33182*/
         B     @RC00053                                            0061
@RF00053 DS    0H                                                  0062
*       RESPECIFY                                                  0062
*        (RFRPTR,                                                  0062
*         PRX) RESTRICTED;                                         0062
*       RFRPTR=ADDR(RFRLST1);       /* INITIALIZE INTERFACE LIST PTR */
         LA    RFRPTR,RFRLST1                                      0063
*       PRX=RFRPTR;                 /* RESET LIST INDEX      @Z30WIMG*/
         LR    PRX,RFRPTR                                          0064
*       PRXEND=RFRLAST;             /* POINT TO LAST LIST ENTRY      */
         L     @10,RFRLAST                                         0065
         ST    @10,PRXEND                                          0065
*       PRFLGS='';                  /* CLEAR STEAL FLAGS     @Z30WIPH*/
         MVI   PRFLGS,X'00'                                        0066
*       PRUICU='1'B;                /* INCR UIC COUNTS       @Z40BPSS*/
         OI    PRUICU,B'01000000'                                  0067
*       PRCRI=I254;                 /* USE MAX STEAL CRITERIA      0068
*                                                            @Z40BPSS*/
         MVC   PRCRI(2),@CH02148                                   0068
*       PRSCT=I32767;               /* UPDATE ALL UICS       @Z30WIPH*/
         MVC   PRSCT(2),@CH02154                                   0069
*       PRSTCNT=I0;                 /* INITIALIZE STOLEN CNT @Z40BPSS*/
         SLR   PRSTCNT,PRSTCNT                                     0070
*       IF UPPVTUIC='1'B            /* SRB TO UPDATE PVT UICS      0071
*                                                            @ZA33182*/
*         THEN                                                     0071
         TM    UPPVTUIC,B'01000000'                                0071
         BNO   @RF00071                                            0071
*         DO;                       /* IF YES UPDATE THE UICS      0072
*                                                            @ZA33182*/
*/*   CALL REAL FRAME REPLACEMENT FOR EVERY IN CORE USR TO UPDT UICS */
*                                                                  0073
*           OUCBPTR=RMCTINQE;       /* ESTABLISH ADDRESSABILITY TO 0073
*                                      INQ                   @Z40BPSS*/
         L     OUCBPTR,RMCTINQE(,RMCTPTR)                          0073
*           OUCBPTR=OUCBFWD;        /* POINT TO FIRST USER   @Z40BPSS*/
         L     OUCBPTR,OUCBFWD(,OUCBPTR)                           0074
*           DO WHILE OUCBNAME='OUCB';/* EXAMINE EACH VALID USER    0075
*                                                            @Z40BPSS*/
         B     @DE00075                                            0075
@DL00075 DS    0H                                                  0076
*             ASCBPTR=OUCBASCB;     /* POINT TO CORRESPONDING ASCB 0076
*                                                            @Z40BPSS*/
         L     ASCBPTR,OUCBASCB(,OUCBPTR)                          0076
*             IF OUCBOUT='0'B&      /* IGNORE USERS OUT OF MAIN    0077
*                                                            @Z40BPSS*/
*                 OUCBGOO='0'B&     /* STORAGE OR USERS IN   @Z40BPSS*/
*                 OUCBGOI='0'B&     /* TRANSITION ALSO INSURE      0077
*                                                            @Z40BPSS*/
*                 OUCBGOB='0'B&     /* REAL STORAGE MGMT CNTL      0077
*                                                            @Z40BPSS*/
*                 ASCBRSM^=I0 THEN  /* BLOCKS EXISTS         @Z40BPSS*/
         TM    OUCBOUT(OUCBPTR),B'11100100'                        0077
         BNZ   @RF00077                                            0077
         L     @10,ASCBRSM(,ASCBPTR)                               0077
         LTR   @10,@10                                             0077
         BZ    @RF00077                                            0077
*               CALL STEAL;         /* GO INIT RFR INTERFACE LIST  0078
*                                                            @Z40BPSS*/
         BAL   @14,STEAL                                           0078
*             OUCBPTR=OUCBFWD;      /* LOOK AT NEXT USR ON QUEUE   0079
*                                                            @Z40BPSS*/
@RF00077 L     OUCBPTR,OUCBFWD(,OUCBPTR)                           0079
*           END;                    /* END OF DO WHILE       @Z40BPSS*/
@DE00075 CLC   OUCBNAME(4,OUCBPTR),@CC02185                        0080
         BE    @DL00075                                            0080
*           IF PRX^=RFRPTR          /* TEST FOR PARTIAL LIST @ZA33182*/
*             THEN                                                 0081
         CR    PRX,RFRPTR                                          0081
         BE    @RF00081                                            0081
*             DO;                   /* IF ONE EXISTS         @ZA33182*/
*               PRXEND=PRX;         /* DUMMY THE LAST ENTRY  @ZA33182*/
         ST    PRX,PRXEND                                          0083
*               CALL STEAL;         /* GO FLUSH THE LIST     @ZA33182*/
         BAL   @14,STEAL                                           0084
*             END;                  /*                       @ZA33182*/
*           UPPVTUIC='0'B;          /* INDICATE PVT UICS UPDATED   0086
*                                                            @ZA33182*/
@RF00081 DS    0H                                                  0087
*           UPCOMUIC='1'B;          /* REQUEST COMMON UIC UPDATE   0087
*                                                            @ZA33182*/
         OI    UPCOMUIC,B'00100000'                                0087
         NI    UPPVTUIC,B'10111111'                                0087
*/*   ? IRACTLCL EPN(PR1)                                            */
*                                                                  0088
*           DO;                                                    0088
*             R1=ADDR(RMEPBPR1);    /* PR1 REQUEST IS PARAM          */
         LA    R1,RMEPBPR1(,RMCTPTR)                               0089
*             CALL RMSBCRL->IRARMRTN;/* ALGRTHM SCH FUNCT            */
         L     @15,RMSBCRL(,RMCTPTR)                               0090
         BALR  @14,@15                                             0090
*           END;                    /* RESCHEDULE PR1 FOR COMMON   0091
*                                                            @ZA33182*/
*         END;                      /* END OF PVT UIC UPDATE @ZA33182*/
*       ELSE                                                       0093
*         DO;                       /* UPDATE COMMON UICS    @ZA33182*/
*                                                                  0093
         B     @RC00071                                            0093
@RF00071 DS    0H                                                  0094
*           /*********************************************************/
*           /*                                                       */
*           /* CALL REAL FRAME REPLACEMENT FOR SYSTEM PAGEABLE AREA  */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0094
*           PRXEND=PRX+LENGTH(RPRFRINT);/* FLUSH LIST        @Z40BPSS*/
         LA    @10,8                                               0094
         ALR   @10,PRX                                             0094
         ST    @10,PRXEND                                          0094
*           ASCBPTR=I0;             /* INDICATE SPA          @Z40BPSS*/
         SLR   ASCBPTR,ASCBPTR                                     0095
*           CALL STEAL;             /* GO INIT RFR INTERFACE LIST  0096
*                                                            @Z40BPSS*/
         BAL   @14,STEAL                                           0096
*           UPCOMUIC='0'B;          /* INDICATE COMMON UICS UPDATED
*                                                            @ZA33182*/
         NI    UPCOMUIC,B'11011111'                                0097
*         END;                      /* END OF COMMON UIC UPDATE    0098
*                                                            @ZA33182*/
*       RESPECIFY                                                  0099
*        (RFRPTR,                                                  0099
*         PRX) UNRESTRICTED;        /*                       @Z30WIPH*/
@RC00071 DS    0H                                                  0100
*       MCVSTCRI=I0;                /* INITIALIZE HIGHEST SYS UIC  0100
*                                                            @Z40BPSS*/
         SLR   @07,@07                                             0100
         STH   @07,MCVSTCRI(,RMCTPTR)                              0100
*       OUCBPTR=RMCTINQE;           /* ADDRESSIBILITY TO IN QUEUE  0101
*                                                            @Z40BPSS*/
         L     OUCBPTR,RMCTINQE(,RMCTPTR)                          0101
*       OUCBPTR=OUCBFWD;            /* POINT TO FIRST USER   @Z40BPSS*/
         L     OUCBPTR,OUCBFWD(,OUCBPTR)                           0102
*       DO WHILE OUCBNAME='OUCB';   /* EXAMINE EACH USER ON QUEUE  0103
*                                                            @Z40BPSS*/
         B     @DE00103                                            0103
@DL00103 DS    0H                                                  0104
*         ASCBPTR=OUCBASCB;         /* POINT TO CORRESPONDING ASCB 0104
*                                                            @Z40BPSS*/
         L     ASCBPTR,OUCBASCB(,OUCBPTR)                          0104
*         IF OUCBOUT='0'B&          /* IGNORE USERS OUT OF MAIN    0105
*                                                            @Z40BPSS*/
*             OUCBGOO='0'B&         /* STORAGE OR USERS IN   @Z40BPSS*/
*             OUCBGOI='0'B&         /* TRANSITION ALSO INSURE      0105
*                                                            @Z40BPSS*/
*             OUCBGOB='0'B&         /* REAL STORAGE MGMT CNTL      0105
*                                                            @Z40BPSS*/
*             ASCBRSM^=I0 THEN                                     0105
         TM    OUCBOUT(OUCBPTR),B'11100100'                        0105
         BNZ   @RF00105                                            0105
         L     @07,ASCBRSM(,ASCBPTR)                               0105
         LTR   @07,@07                                             0105
         BZ    @RF00105                                            0105
*           DO;                     /* BLOCKS EXISTS         @Z40BPSS*/
*             OUXBPTR=ASCBOUXB;     /* CORRESPONDING OUXB    @Z40BPSS*/
         L     OUXBPTR,ASCBOUXB(,ASCBPTR)                          0107
*             MCVSTCRI=MAX(MCVSTCRI,OUXBUIC);/* HIGHEST SO FAR     0108
*                                                            @Z40BPSS*/
         LH    @07,MCVSTCRI(,RMCTPTR)                              0108
         LH    @15,OUXBUIC(,OUXBPTR)                               0108
         CR    @07,@15                                             0108
         BNL   *+6
         LR    @07,@15                                             0108
         STH   @07,MCVSTCRI(,RMCTPTR)                              0108
*           END;                                                   0109
*         OUCBPTR=OUCBFWD;          /* LOOK AT NEXT OUCB     @Z40BPSS*/
@RF00105 L     OUCBPTR,OUCBFWD(,OUCBPTR)                           0110
*       END;                        /* END OF SEARCH         @Z40BPSS*/
@DE00103 CLC   OUCBNAME(4,OUCBPTR),@CC02185                        0111
         BE    @DL00103                                            0111
*       MCVSTCRI=MAX(MCVSTCRI,PVTCHUIC);/* GET SYSTEM HIGHEST      0112
*                                                            @Z40BPSS*/
         L     @07,CVTPTR                                          0112
         L     @07,PVTPTR(,@07)                                    0112
         LH    @15,MCVSTCRI(,RMCTPTR)                              0112
         LH    @07,PVTCHUIC(,@07)                                  0112
         CR    @15,@07                                             0112
         BNL   *+6
         LR    @15,@07                                             0112
         STH   @15,MCVSTCRI(,RMCTPTR)                              0112
*       IF FIRSTIME='0'B THEN                                      0113
         TM    FIRSTIME,B'10000000'                                0113
         BNZ   @RF00113                                            0113
*         DO;                       /* FIRST TIME SAVE VALUE @ZM43064*/
*           MCCPR1IN=RMEPINT;       /* SAVE INITIAL PR1 RMEPINT    0115
*                                                            @ZM43064*/
         L     @07,RMEPPTR(,RRPAPTR)                               0115
         L     @07,RMEPINT(,@07)                                   0115
         ST    @07,MCCPR1IN(,RMCTPTR)                              0115
*           FIRSTIME='1'B;          /* INDICATE VALUE SAVED  @ZM43064*/
         OI    FIRSTIME,B'10000000'                                0116
*         END;                      /*                       @ZM43064*/
*       IF MCVSTCRI>MCCUICBD THEN   /* NEED PR1 INTERVAL CHANGED   0118
*                                                            @ZM43064*/
@RF00113 LH    @07,MCVSTCRI(,RMCTPTR)                              0118
         C     @07,MCCUICBD(,RMCTPTR)                              0118
         BNH   @RF00118                                            0118
*         RMEPINT=MIN(RMEPINT+MCCUICIN,MCCINTMX);/* INCREMENT PR1  0119
*                                      INTERVAL              @ZM43097*/
         L     @07,RMEPPTR(,RRPAPTR)                               0119
         L     @15,RMEPINT(,@07)                                   0119
         AL    @15,MCCUICIN(,RMCTPTR)                              0119
         L     @14,MCCINTMX(,RMCTPTR)                              0119
         CLR   @15,@14                                             0119
         BNH   *+6
         LR    @15,@14                                             0119
         ST    @15,RMEPINT(,@07)                                   0119
*     END;                          /*                       @ZA33182*/
@RF00118 DS    0H                                                  0121
*/*   ? IRACTLEP ENN(PR1)                                            */
