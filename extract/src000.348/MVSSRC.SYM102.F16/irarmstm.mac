         L     @15,RMSBCRL(,RMCTPTR)                               0249
         BALR  @14,@15                                             0249
*           END;                    /* REINVOKE MS2          @ZA01572*/
*         END;                      /* END OF SHORTAGE STILL EXISTS
*                                                            @ZA01572*/
*       ELSE                                                       0252
*         DO;                       /* SHORTAGE DOESNT EXIST @ZA01572*/
*                                                                  0252
         B     @RC00183                                            0252
@RF00183 DS    0H                                                  0253
*           /*********************************************************/
*           /*                                                       */
*           /* ISSUE SHORTAGE RELIEVED MESSAGE IF NECESSARY  @ZA01572*/
*           /*                                                       */
*           /*********************************************************/
*                                                                  0253
*           IF PVTCNTFX<=PVTFIXOK THEN                             0253
         L     @07,CVTPTR                                          0253
         L     @07,PVTPTR(,@07)                                    0253
         LH    @15,PVTCNTFX(,@07)                                  0253
         CH    @15,PVTFIXOK(,@07)                                  0253
         BH    @RF00253                                            0253
*             DO;                   /* BELOW OK THRESHOLD    @ZA01572*/
*               IF MCTFX1='1'BMCTFX2='1'B THEN                    0255
         TM    MCTFX1(RMCTPTR),B'00000011'                         0255
         BZ    @RF00255                                            0255
*                 DO;               /* NEED TO ISSUE SHORTAGE      0256
*                                      RELIEVED MSG          @ZA01572*/
*                   MSGPTR=MCCMS402;/* PTR TO SHORTAGE REL MSG     0257
*                                                            @ZA01572*/
*                                                                  0257
         L     MSGPTR,MCCMS402(,RMCTPTR)                           0257
*                   /*************************************************/
*                   /*                                               */
*                   /* ? IRACTLCL EPN(I09)                           */
*                   /*                                               */
*                   /*************************************************/
*                                                                  0258
*                   CALL RMSBI09->IRARMRTN;/* INVOKE I09 SUBRTN      */
         L     @15,RMSBI09(,RMCTPTR)                               0258
         BALR  @14,@15                                             0258
*/* ISSUE MSG                 @ZA01572                               */
*                                                                  0259
*                   MCTFX1='0'B;    /* TURN OFF SHORTAGE FLAG      0259
*                                                            @ZA01572*/
*                   MCTFX2='0'B;    /* TURN OFF SHORTAGE FLAG      0260
*                                                            @ZA01572*/
         NI    MCTFX1(RMCTPTR),B'11111100'                         0260
*                 END;              /* END OF MSG NEEDED LOOP      0261
*                                                            @ZA01572*/
*                                                                  0261
*               /*****************************************************/
*               /*                                                   */
*               /* FIND USRS SWAPPED DUE TO SHORTAGE         @ZA01572*/
*               /*                                                   */
*               /*****************************************************/
*                                                                  0262
*               DO FIXINDEX=ADDR(RMCTWTQE) TO ADDR(RMCTINQE) BY LENGTH(
*                     RMCTWTQE);    /* LOOP THRU OUCB QUEUES @ZM43072*/
@RF00255 LA    FIXINDEX,RMCTWTQE(,RMCTPTR)                         0262
         B     @DE00262                                            0262
@DL00262 DS    0H                                                  0263
*                 OUCBPTR=FIXQE;    /* OBTAIN QUEUE HEADER   @ZA01572*/
         L     OUCBPTR,FIXQE(,FIXINDEX)                            0263
*                 OUCBPTR=OUCBFWD;  /* OBTAIN FIRST OUCB ON QUEUE  0264
*                                                            @ZA01572*/
         L     OUCBPTR,OUCBFWD(,OUCBPTR)                           0264
*                 DO WHILE OUCBPTR^=FIXQE;/* KEEP LOOPING    @ZM43072*/
         B     @DE00265                                            0265
@DL00265 DS    0H                                                  0266
*                   DO WHILE OUCBNAME='OUCB'&OUCBFXS='0'B/*  @ZM43072*/
*                         &OUCBJSFS='0'B;                          0266
         B     @DE00266                                            0266
@DL00266 DS    0H                                                  0267
*                     OUCBPTR=OUCBFWD;/* ON TO NEXT OUCB ON QUE    0267
*                                                            @ZA01572*/
         L     OUCBPTR,OUCBFWD(,OUCBPTR)                           0267
*                   END;                                           0268
@DE00266 CLC   OUCBNAME(4,OUCBPTR),@CC02185                        0268
         BNE   @DC00266                                            0268
         TM    OUCBFXS(OUCBPTR),B'00000001'                        0268
         BNZ   @DC00266                                            0268
         TM    OUCBJSFS(OUCBPTR),B'10000000'                       0268
         BZ    @DL00266                                            0268
@DC00266 DS    0H                                                  0269
*                   IF OUCBPTR^=FIXQE THEN                         0269
         C     OUCBPTR,FIXQE(,FIXINDEX)                            0269
         BE    @RF00269                                            0269
*                     DO;           /* DID WE FIND ONE       @ZA01572*/
*                       OUCBFXS='0'B;/* RESET HEAVY USAGE FLAG     0271
*                                                            @ZA01572*/
         NI    OUCBFXS(OUCBPTR),B'11111110'                        0271
*                       OUCBJSFS='0'B;/* RESET JOB SELECT DELAYED  0272
*                                                            @ZM43072*/
         NI    OUCBJSFS(OUCBPTR),B'01111111'                       0272
*                       FIXOUCB=OUCBFWD;/* SAVE FORWARD POINTER    0273
*                                                            @ZM43072*/
         L     FIXOUCB,OUCBFWD(,OUCBPTR)                           0273
*                       ASCBPTR=OUCBASCB;/* ADDRESSIBILITY TO ASCB 0274
*                                                            @ZA01572*/
         L     ASCBPTR,OUCBASCB(,OUCBPTR)                          0274
*                       OUCBOFF='0'B;/* REMOVE FROM WAIT QUEUE     0275
*                                                            @ZA01572*/
*                       OUCBGOB='1'B;/* SET Q TRANSITION FLAG      0276
*                                                            @ZA01572*/
*                                                                  0276
         OI    OUCBGOB(OUCBPTR),B'00100000'                        0276
         NI    OUCBOFF(OUCBPTR),B'11110111'                        0276
*                       /*********************************************/
*                       /*                                           */
*                       /* ? IRACTLCL EPN(RPS)                       */
*                       /*                                           */
*                       /*********************************************/
*                                                                  0277
*                       DO;                                        0277
*                         R1=ADDR(RMEPBRPS);/* RPS REQUEST IS PARAM  */
         LA    R1,RMEPBRPS(,RMCTPTR)                               0278
*                         CALL RMSBCRN->IRARMRTN;/* ACTION INVK FUNCT*/
         L     @15,RMSBCRN(,RMCTPTR)                               0279
         BALR  @14,@15                                             0279
*                       END;        /* INVOKE RPS            @ZA01572*/
*                                                                  0280
*                       /*********************************************/
*                       /*                                           */
*                       /* ? IRACTLCL EPN(WM3)                       */
*                       /*                                           */
*                       /*********************************************/
*                                                                  0281
*                       CALL EPSTWM3->IRARMRTN;/* INVOKE WM3 ACTION  */
         L     @15,EPSTWM3(,RMCTPTR)                               0281
         BALR  @14,@15                                             0281
*/*  RECALCULATE WMR         @ZA01572                                */
*                                                                  0282
*                       OUCBPTR=FIXOUCB;/* LOOK AT NEXT USER @ZM43072*/
         LR    OUCBPTR,FIXOUCB                                     0282
*                     END;          /* END OF SUCCESSFUL FIND LOOP 0283
*                                                            @ZA01572*/
*                 END;              /* END OF THIS QUEUE SEARCH    0284
*                                                            @ZM43072*/
@RF00269 DS    0H                                                  0284
@DE00265 C     OUCBPTR,FIXQE(,FIXINDEX)                            0284
         BNE   @DL00265                                            0284
*               END;                /* END OF LOOPING THRU THE QUEUES
*                                                            @ZA01572*/
         AL    FIXINDEX,@CF00050                                   0285
@DE00262 LA    @15,RMCTINQE(,RMCTPTR)                              0285
         CR    FIXINDEX,@15                                        0285
         BNH   @DL00262                                            0285
*               MCTFAVQ='0'B;       /* TURN OFF MS2 INDICATOR      0286
*                                                            @ZA01572*/
         NI    MCTFAVQ(RMCTPTR),B'11101111'                        0286
*             END;                  /*                       @ZA01572*/
*           ELSE                    /* ? IRACTLCL EPN(MS2)           */
*             DO;                                                  0288
         B     @RC00253                                            0288
@RF00253 DS    0H                                                  0289
*               R1=ADDR(RMEPBMS2);  /* MS2 REQUEST IS PARAM          */
         LA    R1,RMEPBMS2(,RMCTPTR)                               0289
*               CALL RMSBCRL->IRARMRTN;/* ALGRTHM SCH FUNCT          */
         L     @15,RMSBCRL(,RMCTPTR)                               0290
         BALR  @14,@15                                             0290
*             END;                  /* REINVOKE MS2          @ZA01572*/
*         END;                      /*                       @ZA01572*/
@RC00253 DS    0H                                                  0293
*     END;                          /* END OF FIX PAGE SHORTAGE    0293
*                                      PROCESSING            @ZA01572*/
@RC00183 DS    0H                                                  0294
*/*   ? IRACTLEP ENN(MS2)                                            */
