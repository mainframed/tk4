*/*                                                                  */
*/* FUNCTION = INFORM OPERATOR OF SYSTEM QUEUE SPACE SHORTAGES       */
*/*                                                                  */
*/* CALLER = IRARMCTL - SRM CONTROL MODULE                           */
*/*            ALGORITHM REQUEST PROCESSOR                           */
*/*                                                                  */
*/* INPUT = R2 = RMCT ADDRESS                                        */
*/*         R3 = RRPA ADDRESS                                        */
*/*                                                                  */
*/* OUTPUT = NONE                                                    */
*/*                                                                  */
*/********************************************************************/
*/*IRARMSQA: CHART (AMODE,SEQ,AREF)                                  */
*/* HEADER                                                         0464
*/* AUG 8,1975                                     IRARMSQA-       0464
*/* SQA SHORTAGE MESSAGE WRITER                                      */
*/*IRARMSQA: E INVOKED BY CTL                                        */
*/* D (NO,%A,YES,) PREVIOUS SHORTAGE RELIEVED ?                      */
*/* S (,%S) IRARMI09: WRITE SHORTAGE RELIEVED MESSAGE                */
*/*%A: D (NO,%B,YES,) CRITICAL SHORTAGE EXISTS ?                     */
*/* S (,%S) IRARMI09: WRITE CRITICAL SHORTAGE MESSAGE                */
*/*%B: D (NO,%S,YES,) 1ST LEVEL SHORTAGE EXISTS                      */
*/* S IRARMI09: WRITE SHORTAGE MESSAGE                               */
*/*%S:  R RETURN TO CALLER                                           */
*/*IRARMSQA: END                                                     */
*/*   ? IRACTLEP EPN(SQA)                                            */
*                                                                  0464
*   DCL                                                            0464
*     IRARMSQA ENTRY INT;                                          0464
*   GEN CODE REFS(RRPAEPA) DEFS(IRARMSQA) NOEXIT SEQFLOW;          0465
*                                  /*
         ENTRY IRARMSQA
IRARMSQA DS    0H
         STM   @00,@14,000+000(@13)    SAVE INVOKING RTNE REGS
         ST    @01,RRPAEPA(,RRPAPTR)   INDICATE NEW CURRENT RMEP
         LA    @12,IRARMSQA-@PSTART    DISPLACEMENT TO MODULE BASE
         LCR   @12,@12                 NEGATIVE FOR SUBTRACT
         ALR   @12,@15                 SETUP MODULE ADDRESSABILITY
*   RESPECIFY                                                      0466
*     MSGPTR RESTRICTED;                                           0466
*                                                                  0466
*/*                                                                  */
*/*      WRITE SHORTAGE RELIEVED MESSAGE IF SHORTAGE IS GONE         */
*/*                                                                  */
*                                                                  0467
*   IF MCTSQA1='0'B                 /* WRITE MESSAGE ONLY IF NO      */
*       &MCTSMS1='1'B THEN          /* SHORTAGES ARE CURRENT AND IF
*                                      SHORTAGE MESSAGES HAVE      0467
*                                      PREVIOUSLY BEEN WRITTEN       */
         TM    MCTSQA1(RMCTPTR),B'10000000'                        0467
         BNZ   @RF00467                                            0467
         TM    MCTSMS1(RMCTPTR),B'00001000'                        0467
         BNO   @RF00467                                            0467
*     DO;                           /*                               */
*       MSGPTR=MCCMS102;            /* POINT TO APPROPRIATE MESSAGE
*                                      INVOKE RECORD FUNCTION        */
*                                                                  0469
         L     MSGPTR,MCCMS102(,RMCTPTR)                           0469
*       /*************************************************************/
*       /*                                                           */
*       /* ? IRACTLCL EPN(I09)                                       */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0470
*       CALL RMSBI09->IRARMRTN;     /* INVOKE I09 SUBRTN             */
         L     @15,RMSBI09(,RMCTPTR)                               0470
         BALR  @14,@15                                             0470
*       MCTSMS1='0'B;               /* RESET SHORTAGE                */
*       MCTSMS2='0'B;               /* MESSAGE FLAGS                 */
         NI    MCTSMS1(RMCTPTR),B'11110011'                        0472
*     END;                          /*                               */
*/*                                                                  */
*/*      WRITE CRITICAL SHORTAGE MESSAGE FOR SECOND LEVEL SHORTAGE   */
*/*                                                                  */
*                                                                  0474
*   ELSE                                                           0474
*     IF MCTSQA2='1'B               /* WRITE MESSAGE ONLY IF SHORTAGE*/
*         &MCTSMS2='0'B THEN        /* IS CURRENT AND MESSAGE HAS NOT*/
         B     @RC00467                                            0474
@RF00467 TM    MCTSQA2(RMCTPTR),B'01000000'                        0474
         BNO   @RF00474                                            0474
         TM    MCTSMS2(RMCTPTR),B'00000100'                        0474
         BNZ   @RF00474                                            0474
*       DO;                         /* ALREADY BEEN WRITEN           */
*         MSGPTR=MCCMS101;          /* POINT TO MESSAGE INVOKE RECORD
*                                      FUNCTION                      */
*                                                                  0476
         L     MSGPTR,MCCMS101(,RMCTPTR)                           0476
*         /***********************************************************/
*         /*                                                         */
*         /* ? IRACTLCL EPN(I09)                                     */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0477
*         CALL RMSBI09->IRARMRTN;   /* INVOKE I09 SUBRTN             */
         L     @15,RMSBI09(,RMCTPTR)                               0477
         BALR  @14,@15                                             0477
*         MCTSMS2='1'B;             /* SET MESSAGE WRITTEN FLAG      */
*         MCTSMS1='1'B;             /* INDICATE NO NEED FOR MSG 1    */
         OI    MCTSMS2(RMCTPTR),B'00001100'                        0479
*       END;                                                       0480
*                                                                  0480
*/*                                                                  */
*/*      WRITE SHORTAGE MESSAGE FOR FIRST LEVEL SHORTAGE             */
*/*                                                                  */
*                                                                  0481
*     ELSE                                                         0481
*       IF MCTSQA1='1'B             /* WRITE MESSAGE ONLY IF SHORTAGE*/
*           &MCTSMS1='0'B THEN      /* IS CURRENT AND MESSAGE HAS NOT
*                                      ALREADY BEEN WRITTEN          */
         B     @RC00474                                            0481
@RF00474 TM    MCTSQA1(RMCTPTR),B'10000000'                        0481
         BNO   @RF00481                                            0481
         TM    MCTSMS1(RMCTPTR),B'00001000'                        0481
         BNZ   @RF00481                                            0481
*         DO;                       /*                               */
*           MSGPTR=MCCMS100;        /* POINT TO APPROPRIATE MESSAGE
*                                      INVOKE RECORD FUNCTION        */
*                                                                  0483
         L     MSGPTR,MCCMS100(,RMCTPTR)                           0483
*           /*********************************************************/
*           /*                                                       */
*           /* ? IRACTLCL EPN(I09)                                   */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0484
*           CALL RMSBI09->IRARMRTN; /* INVOKE I09 SUBRTN             */
         L     @15,RMSBI09(,RMCTPTR)                               0484
         BALR  @14,@15                                             0484
*           MCTSMS1='1'B;           /* SET MESSAGE WRITTEN FLAG      */
         OI    MCTSMS1(RMCTPTR),B'00001000'                        0485
*         END;                                                     0486
*   RESPECIFY                                                      0487
*     MSGPTR UNRESTRICTED;                                         0487
*                                                                  0487
@RF00481 DS    0H                                                  0487
@RC00474 DS    0H                                                  0487
@RC00467 DS    0H                                                  0488
*/*   ? IRACTLEP ENN(SQA)                                            */
