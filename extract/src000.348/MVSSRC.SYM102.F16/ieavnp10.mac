         TITLE 'IEAVNP10 - SYSTEM RESOURCES MANAGER INITIALIZATION     *
                        '
IEAVNP10 CSECT ,                                                   0001
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @10,0                                               0001
@PSTART  LA    @11,4095(,@10)                                      0001
         USING @PSTART,@10                                         0001
         USING @PSTART+4095,@11                                    0001
         ST    @13,@SA00001+4                                      0001
         LA    @14,@SA00001                                        0001
         ST    @14,8(,@13)                                         0001
         LR    @13,@14                                             0001
*
* REFS ARE GENERATED FOR NVT FIELDS THAT CONTAIN ADDRESSES OF
* NIP SERVICE ROUTINES, SO IEAPMNIP MACRO CALLS TO THESE ROUTINES
* WILL ASSEMBLE PROPERLY.
NP10MSG  DS    0H                                                  0129
*   NP10NVTP=RNVT;                  /* SAVE ADDR OF NVT              */
         ST    RNVT,NP10NVTP                                       0137
*   RMCTPTR=CVTOPCTP;               /* ADDR OF RMCT                  */
         L     RMCTPTR,CVTOPCTP(,CVTPTR)                           0138
*   RMPTPTR=RMCTRMPT;               /* ADDR OF RMPT                  */
         L     RMPTPTR,RMCTRMPT(,RMCTPTR)                          0139
*/*  INITIALIZE RMCT WITH THE ADDRESS OF THE SYSTEM RESOURCES MGR'S  */
*/*  GLOBAL SAVE AREA                                                */
*                                                                  0140
*   RMCTRMSA=WSAGOPTM;              /* ADDR OF SRM'S SAVE AREA       */
         L     @12,CVTSPSA(,CVTPTR)                                0140
         L     @12,WSAGOPTM(,@12)                                  0140
         ST    @12,RMCTRMSA(,RMCTPTR)                              0140
*/*  UPON ENTRY IEAVNP10 OBTAINS THE CPU MODEL NUMBER AND THE        */
*/*  CORRESPONDING ADJUSTMENT FACTOR                                 */
*/*  SEARCH THE ARGUMENTS OF IRARMCMD FOR A CPU MODEL NUMBER         */
*/*  IDENTICAL TO THE ONE OF THE IPLING CPU                          */
*                                                                  0141
*   NP10CPML=CVTMDL;                /* EXTRACT CPU MODEL #   @G51CPEB*/
         LR    @12,CVTPTR                                          0141
         AL    @12,@CF03481                                        0141
         MVC   NP10CPML(2),CVTMDL-250(@12)                         0141
*   NP10CPTR=ADDR(IRARMCMD);        /* ADDR FIRST ENTRY      @G51CPEB*/
         L     NP10CPTR,@CA02898                                   0142
*   DO WHILE NP10CPUM^=ZERO&        /* SEARCH ENTIRE TABLE   @G51CPEB*/
*         NP10CPUM^=NP10CPML;       /* OR UNTIL MATCH        @G51CPEB*/
         B     @DE00143                                            0143
@DL00143 DS    0H                                                  0144
*     NP10CPTR=NP10CPTR+LENGTH(NP10CPUE);/* NEXT             @G51CPEB*/
         AL    NP10CPTR,@CF00060                                   0144
*   END;                            /* COMPLETE SEARCH FOR CPU MODEL
*                                      NO.                   @G51CPEB*/
@DE00143 LH    @12,NP10CPUM(,NP10CPTR)                             0145
         N     @12,@CF03482                                        0145
         LTR   @12,@12                                             0145
         BZ    @DC00143                                            0145
         CLC   NP10CPUM(2,NP10CPTR),NP10CPML                       0145
         BNE   @DL00143                                            0145
@DC00143 DS    0H                                                  0146
*/* IF FAILED TO MATCH CPU MODEL NUMBER USE DEFAULT                  */
*                                                                  0146
*   IF NP10CPUM=ZERO THEN                                          0146
         LH    @12,NP10CPUM(,NP10CPTR)                             0146
         N     @12,@CF03482                                        0146
         LTR   @12,@12                                             0146
         BNZ   @RF00146                                            0146
*     DO;                                                          0147
*       NP10CPTR=ADDR(IRARMCMD);    /* ADDR FIRST ENTRY      @G51CPEB*/
         L     NP10CPTR,@CA02898                                   0148
*       DO WHILE NP10CPUM^=I155;    /* DEFAULT IS 155        @G51CPEB*/
         B     @DE00149                                            0149
@DL00149 DS    0H                                                  0150
*         NP10CPTR=NP10CPTR+LENGTH(NP10CPUE);/* NEXT         @G51CPEB*/
         AL    NP10CPTR,@CF00060                                   0150
*       END;                        /* FOUND 155 ENTRY       @G51CPEB*/
@DE00149 CLC   NP10CPUM(2,NP10CPTR),@CH02693                       0151
         BNE   @DL00149                                            0151
*     END;                          /* END CPUM = 0          @G51CPEB*/
*   NP10CPMP=NP10CPUA;              /* CPU ADJUSTMENT        @G51CPEB*/
@RF00146 L     @12,NP10CPUA(,NP10CPTR)                             0153
         ST    @12,NP10CPMP                                        0153
*/*  ADJUST EACH CPU MODEL DEPENDENT FIELDS BY THE ADJUSTMENT FACTOR */
*/*  IN THE FOLLOWING TABLES :                                       */
*/*  CCT - CPU CONTROL TABLE                                         */
*/*  MCT - MAIN STORAGE CONTROL TABLE                                */
*                                                                  0154
*NP10ADJ:                                                          0154
*   NP10ADJN=ADJSHIFT*NP10INST;     /* 16* NO. OF MICROSECS FOR    0154
*                                      10,000 INSTRUCTIONS           */
NP10ADJ  L     @12,NP10INST                                        0154
         SLA   @12,4                                               0154
         ST    @12,NP10ADJN                                        0154
*   CALL NP10ADJP;                  /* ADJUST NO. FOR IPLING CPU     */
         BAL   @14,NP10ADJP                                        0155
*   RMCTADJC=NP10ADJN;                                             0156
         L     @12,NP10ADJN                                        0156
         ST    @12,RMCTADJC(,RMCTPTR)                              0156
*   NP10ADJN=CCCAPMET;                                             0157
         L     @12,CCCAPMET(,RMCTPTR)                              0157
         ST    @12,NP10ADJN                                        0157
*   CALL NP10ADJP;                  /* ADJUST NO. FOR IPLING CPU     */
         BAL   @14,NP10ADJP                                        0158
*   CCCAPMET=NP10ADJN;                                             0159
         L     @12,NP10ADJN                                        0159
         ST    @12,CCCAPMET(,RMCTPTR)                              0159
*   NP10ADJN=CCCAPMIN;                                             0160
         L     @12,CCCAPMIN(,RMCTPTR)                              0160
         ST    @12,NP10ADJN                                        0160
*   CALL NP10ADJP;                  /* ADJUST NO.FOR IPLING CPU      */
         BAL   @14,NP10ADJP                                        0161
*   CCCAPMIN=NP10ADJN;                                             0162
         L     @12,NP10ADJN                                        0162
         ST    @12,CCCAPMIN(,RMCTPTR)                              0162
*   NP10ADJN=CCCAPMAX;                                             0163
         L     @12,CCCAPMAX(,RMCTPTR)                              0163
         ST    @12,NP10ADJN                                        0163
*   CALL NP10ADJP;                  /* ADJUST NO.FOR IPLING CPU      */
         BAL   @14,NP10ADJP                                        0164
*   CCCAPMAX=NP10ADJN;                                             0165
         L     @12,NP10ADJN                                        0165
         ST    @12,CCCAPMAX(,RMCTPTR)                              0165
*   NP10ADJN=CCCAPDEL;                                             0166
         L     @12,CCCAPDEL(,RMCTPTR)                              0166
         ST    @12,NP10ADJN                                        0166
*   CALL NP10ADJP;                  /* ADJUST NO.FOR IPLING CPU      */
         BAL   @14,NP10ADJP                                        0167
*   CCCAPDEL=NP10ADJN;                                             0168
         L     @12,NP10ADJN                                        0168
         ST    @12,CCCAPDEL(,RMCTPTR)                              0168
*   NP10ADJN=CCCMNUIN;                                             0169
         L     @12,CCCMNUIN(,RMCTPTR)                              0169
         ST    @12,NP10ADJN                                        0169
*   CALL NP10ADJP;                  /* ADJUST NO. FOR IPLING CPU     */
         BAL   @14,NP10ADJP                                        0170
*   CCCMNUIN=NP10ADJN;                                             0171
         L     @12,NP10ADJN                                        0171
         ST    @12,CCCMNUIN(,RMCTPTR)                              0171
*   NP10ADJN=CCCMNSIN;                                             0172
         L     @12,CCCMNSIN(,RMCTPTR)                              0172
         ST    @12,NP10ADJN                                        0172
*   CALL NP10ADJP;                  /* ADJUST NO. FOR IPLING CPU     */
         BAL   @14,NP10ADJP                                        0173
*   CCCMNSIN=NP10ADJN;                                             0174
         L     @12,NP10ADJN                                        0174
         ST    @12,CCCMNSIN(,RMCTPTR)                              0174
*   NP10ADJN=CCCAPDIV;                                             0175
         LH    @12,CCCAPDIV(,RMCTPTR)                              0175
         ST    @12,NP10ADJN                                        0175
*   CALL NP10ADJP;                  /* ADJUST NO.FOR IPLING CPU      */
         BAL   @14,NP10ADJP                                        0176
*   CCCAPDIV=NP10ADJN;                                             0177
         L     @12,NP10ADJN                                        0177
         STH   @12,CCCAPDIV(,RMCTPTR)                              0177
*   NP10ADJN=CCCSIGUR;                                             0178
         LH    @12,CCCSIGUR(,RMCTPTR)                              0178
         ST    @12,NP10ADJN                                        0178
*   CALL NP10ADJP;                  /* ADJUST NO.FOR IPLING CPU      */
         BAL   @14,NP10ADJP                                        0179
*   CCCSIGUR=NP10ADJN;                                             0180
*                                                                  0180
         L     @12,NP10ADJN                                        0180
         STH   @12,CCCSIGUR(,RMCTPTR)                              0180
*/*  FOR EACH PROCESS RATE DEPENDENT ROUTINE, ADJUST THE ROUTINE     */
*/*  INVOCATION INTERVAL BY THE CPU ADJUSTMENT FACTOR OF THE IPLING  */
*/*  CPU                                                             */
*                                                                  0181
*   RESPECIFY                                                      0181
*     WKREGP RSTD;                                                 0181
*   WKREGP=RMCTEPPR;                                               0182
         L     WKREGP,RMCTEPPR(,RMCTPTR)                           0182
*   DO WHILE(EPPRPTR^=ZERO);                                       0183
         B     @DE00183                                            0183
@DL00183 DS    0H                                                  0184
*     RMEPPTR=EPPRPTR;              /* ADDR OF RMEP BLOCK FOR PROCESS
*                                      RATE DEPENDENT RTN            */
         L     RMEPPTR,EPPRPTR(,WKREGP)                            0184
*     NP10ADJN=RMEPINT;             /* ADJUST INVOCATION INTERVAL    */
         L     @12,RMEPINT(,RMEPPTR)                               0185
         ST    @12,NP10ADJN                                        0185
*     CALL NP10ADJP;                /* ADJUST NO. FOR IPLING CPU     */
         BAL   @14,NP10ADJP                                        0186
*     RMEPINT=NP10ADJN;                                            0187
         L     @12,NP10ADJN                                        0187
         ST    @12,RMEPINT(,RMEPPTR)                               0187
*     WKREGP=WKREGP+LENGTH(EPPRPTR);/* POINT TO NEXT ENTRY PROCESSER
*                                      RATE DEPENDENT RTN TABLE      */
         AL    WKREGP,@CF00054                                     0188
*   END;                                                           0189
@DE00183 SLR   @12,@12                                             0189
         C     @12,EPPRPTR(,WKREGP)                                0189
         BNE   @DL00183                                            0189
*   RESPECIFY                                                      0190
*     WKREGP UNRSTD;                                               0190
*                                                                  0190
*/*  OBTAIN THE NUMBER OF ONLINE CPUS AND INITIALIZE PROCESSOR       */
*/*  DEPENDENT FIELDS IN I/O CONTROL TABLE                           */
*/* IF THE NUMBER OF ONLINE CPUS IS INVALID, USE DEFAULT NO.         */
*                                                                  0191
*   IF CSDCPUOL>ZERO&CSDCPUOL<=MAXCPUOL THEN/* NO. OF ONLINE CPUS  0191
*                                      VALID                         */
         L     @08,CVTCSD(,CVTPTR)                                 0191
         LH    @08,CSDCPUOL(,@08)                                  0191
         CR    @08,@12                                             0191
         BNH   @RF00191                                            0191
         C     @08,@CF00074                                        0191
         BH    @RF00191                                            0191
*     CCVCPUCT=CSDCPUOL;            /* NO. OF ONLINE CPUS            */
         STH   @08,CCVCPUCT(,RMCTPTR)                              0192
*   ELSE                            /* NO. OF ONLINE CPUS INVALID    */
*     CCVCPUCT=DEFCPUOL;            /* USE DEFAULT VALUE             */
         B     @RC00191                                            0193
@RF00191 LH    @12,DEFCPUOL                                        0193
         STH   @12,CCVCPUCT(,RMCTPTR)                              0193
*   ICCHIUTH=ICCINHIT(CCVCPUCT);                                   0194
@RC00191 LH    @12,CCVCPUCT(,RMCTPTR)                              0194
         ALR   @12,@12                                             0194
         LH    @08,ICCINHIT-2(@12,RMCTPTR)                         0194
         STH   @08,ICCHIUTH(,RMCTPTR)                              0194
*   ICCLOUTH=ICCINLOT(CCVCPUCT);                                   0195
         LH    @08,ICCINLOT-2(@12,RMCTPTR)                         0195
         STH   @08,ICCLOUTH(,RMCTPTR)                              0195
*   ICCDAUTH=ICCINDAT(CCVCPUCT);                                   0196
*                                                                  0196
         LH    @12,ICCINDAT-2(@12,RMCTPTR)                         0196
         STH   @12,ICCDAUTH(,RMCTPTR)                              0196
*/*  OBTAIN THE NUMBER OF LOGICAL CHANNELS                           */
*/*                                                                  */
*                                                                  0197
*   IOCOMPTR=CVTIXAVL;              /* ADDR OF IOCOM                 */
         L     IOCOMPTR,CVTIXAVL(,CVTPTR)                          0197
*   IOCXPTR=IOCOMEX;                /* ADDR OF IOCX                  */
         L     IOCXPTR,IOCOMEX(,IOCOMPTR)                          0198
*/* IF THE NUMBER OF LOGICAL CHANNELS IS INVALID, USE DEFAULT NO.    */
*                                                                  0199
*   IF IOXLCHCT>ZERO THEN           /* # OF LOGICAL CHANNELS VALID   */
         CLI   IOXLCHCT(IOCXPTR),0                                 0199
         BNH   @RF00199                                            0199
*     ICCLCHN=IOXLCHCT;             /* NO. OF LOGICAL CHANNELS       */
         SLR   @12,@12                                             0200
         IC    @12,IOXLCHCT(,IOCXPTR)                              0200
         STH   @12,ICCLCHN(,RMCTPTR)                               0200
*   ELSE                            /* # OF LOGICAL CHANNELS INVALID */
*     ICCLCHN=DEFLCHN;              /* USE DEFAULT VALUE             */
         B     @RC00199                                            0201
@RF00199 LH    @12,DEFLCHN                                         0201
         STH   @12,ICCLCHN(,RMCTPTR)                               0201
*/*  CONSTRUCT AND INITIALIZE SYSTEM RESOURCES MANAGER LOGICAL       */
*/*  CHANNEL TABLE (RLCT) IN SQA                                     */
*                                                                  0202
*   RESPECIFY                                                      0202
*    (R0,                                                          0202
*     R1,                                                          0202
*     R14,                                                         0202
*     R15) RESTRICTED;                                             0202
@RC00199 DS    0H                                                  0203
*   NP10STSZ=ICCLCHN*LENGTH(RLCT);  /* NEEDED STORAGE FOR RLCT       */
         LH    @05,ICCLCHN(,RMCTPTR)                               0203
         MH    @05,@CH03478                                        0203
         STCM  @05,7,NP10STSZ                                      0203
*   R0=NP10P245;                    /* SUBPOOL 245                   */
         L     R0,NP10P245                                         0204
*   GEN(GETMAIN R,LV=(0));          /* GETMAIN FROM SQA              */
         GETMAIN R,LV=(0)
*   RLCTPTR=R1;                     /* ADDR OF RLCT                  */
         LR    RLCTPTR,R1                                          0206
*   RESPECIFY                                                      0207
*    (R0,                                                          0207
*     R1,                                                          0207
*     R14,                                                         0207
*     R15) UNRESTRICTED;                                           0207
*   RMCTRLCT=RLCTPTR;               /* ADDR OF FIRST RLCT ENTRY      */
         ST    RLCTPTR,RMCTRLCT(,RMCTPTR)                          0208
*   ICCLCLST=RMCTRLCT+(ICCLCHN-ONE)*LENGTH(RLCT);/* ADDR OF LAST RLCT
*                                      ENTRY                         */
         L     @05,RMCTRLCT(,RMCTPTR)                              0209
         LH    @15,ICCLCHN(,RMCTPTR)                               0209
         BCTR  @15,0                                               0209
         MH    @15,@CH03478                                        0209
         ALR   @05,@15                                             0209
         ST    @05,ICCLCLST(,RMCTPTR)                              0209
*/*  INITIALIZE EACH RESOURCE MANAGER LOGICAL CHANNEL BLOCK          */
*/*  TO INDICATE THE LOGICAL CHANNEL NUMBER                          */
*                                                                  0210
*   X=DIM(NP10LCHP);                /* NO. OF BIT PATTERNS           */
         LA    X,32                                                0210
*   DO I=ONE TO ICCLCHN;            /* NO. OF LOGICAL CHANNELS       */
         LA    I,1                                                 0211
         B     @DE00211                                            0211
@DL00211 DS    0H                                                  0212
*     Y=I//X;                       /* GET INDEX TO APPROPRIATE BIT
*                                      PATTERN                       */
         LR    @00,I                                               0212
         SRDA  @00,32                                              0212
         DR    @00,X                                               0212
         LR    Y,@00                                               0212
*     IF Y=ZERO                     /* IF LOGICAL CHANNEL NO. IS A 0213
*                                      MULTIPLE OF THE NO. OF BITS 0213
*                                      USED IN THE PATTERN           */
*       THEN                        /* LOGICAL CHANNEL NO. = NO.OF 0213
*                                      BITS USED IN THE PATTERN      */
         LTR   Y,Y                                                 0213
         BNZ   @RF00213                                            0213
*       Y=X;                                                       0214
         LR    Y,X                                                 0214
*     NP10RLCT=''B;                 /* SET RLCT TO ZEROES            */
@RF00213 XC    NP10RLCT(36,RLCTPTR),NP10RLCT(RLCTPTR)              0215
*     RLCTBIT=NP10LCHP(Y);          /* INITIALIZE RLCTBIT            */
         LR    @06,Y                                               0216
         SLA   @06,2                                               0216
         LA    @01,NP10LCHP-4(@06)                                 0216
         MVC   RLCTBIT(4,RLCTPTR),0(@01)                           0216
*     RLCTPTR=RLCTPTR+LENGTH(RLCT); /* ADVANCE PTR TO RLCT           */
         AL    RLCTPTR,@CF03478                                    0217
*   END;                            /* COMPLETED INITIALIZATION FOR
*                                      RLCT                          */
         AL    I,@CF00062                                          0218
@DE00211 CH    I,ICCLCHN(,RMCTPTR)                                 0218
         BNH   @DL00211                                            0218
*/*  INVOKE SYSTEM PARAMETER PROCESSORS                              */
*/*  THREE SYSTEM PARAMETERS ARE PROCESSED BY IEAVNP10 - APG,OPT,IPS */
*/*  IF A SYSTEM PARAMETER IS SPECIFIED INCORRECTLY, THE OPERATOR    */
*/*  IS PROMPTED TO RESPECIFY IT AND THE SAME SYSTEM PARAMETER       */
*/*  PROCESSOR IS INVOKED TO RE-PROCESS THE PARAMETER                */
*                                                                  0219
*   RESPECIFY                                                      0219
*     RCODE RESTRICTED;                                            0219
*   DO NP10SYCT=                    /* LOOP THRU SYSTEM PARM TABLE   */
*         ONE TO DIM(NP10SYSP);                                    0220
         LA    NP10SYCT,1                                          0220
@DL00220 DS    0H                                                  0221
*     PROCPTR=NP10SYSP(NP10SYCT);   /* ADDR OF PROCESSOR             */
         LR    @14,NP10SYCT                                        0221
         MH    @14,@CH00758                                        0221
         L     PROCPTR,NP10SYSP-12(@14)                            0221
*NP10SPM:                                                          0222
*     CALL NP10PROC;                /* INVOKE SYSTEM PARMAMETER    0222
*                                      PROCESSOR                     */
NP10SPM  LR    @15,PROCPTR                                         0222
         BALR  @14,@15                                             0222
*     IF RCODE^=SUCCESS             /* TEST RETURN CODE              */
*       THEN                        /* NEED TO PROMPT FOR NEW INPUT  */
         LTR   RCODE,RCODE                                         0223
         BZ    @RF00223                                            0223
*       DO;                                                        0224
*         NP10PARM=NP10SYSK(NP10SYCT);/* PARM KEYWORD                */
         LR    @14,NP10SYCT                                        0225
         MH    @14,@CH00758                                        0225
         LA    @12,NP10SYSK-12(@14)                                0225
         MVC   NP10PARM(8),0(@12)                                  0225
*         GEN REFS(NVTPRMPT,NP10PARM)                              0226
*             (IEAPMNIP TYPE=PRMPT,PARAM=NP10PARM,OPT=ERROR);      0226
         IEAPMNIP TYPE=PRMPT,PARAM=NP10PARM,OPT=ERROR
*         GO TO NP10SPM;            /* RE-PROCESS THE SYSTEM PARM    */
         B     NP10SPM                                             0227
*       END;                                                       0228
*   END;                            /* COMPLETED ALL SYSTEM PARM   0229
*                                      PROCESSING                    */
@RF00223 AL    NP10SYCT,@CF00062                                   0229
         C     NP10SYCT,@CF00293                                   0229
         BNH   @DL00220                                            0229
*   RESPECIFY                                                      0230
*     RCODE UNRESTRICTED;                                          0230
*                                                                  0230
*/*  INITIALIZE AUTOMATIC PRIORITY GROUP (APG) DEPENDENT FIELDS      */
*/*  IN CPU CONTROL TABLE (CCT)                                      */
*                                                                  0231
*   CVTAPG=CVTAPG*SIXTEEN;                                         0231
         SLR   @12,@12                                             0231
         IC    @12,CVTAPG(,CVTPTR)                                 0231
         SLA   @12,4                                               0231
         STC   @12,CVTAPG(,CVTPTR)                                 0231
*   CCCAPGDP=CCCAPGDP+CVTAPG;                                      0232
         LH    @05,CCCAPGDP(,RMCTPTR)                              0232
         ALR   @05,@12                                             0232
         STH   @05,CCCAPGDP(,RMCTPTR)                              0232
*   CCCAPBDP=CCCAPBDP+CVTAPG;                                      0233
         LH    @15,CCCAPBDP(,RMCTPTR)                              0233
         ALR   @15,@12                                             0233
         STH   @15,CCCAPBDP(,RMCTPTR)                              0233
*   CCCAPLDP=CCCAPLDP+CVTAPG;                                      0234
         LH    @15,CCCAPLDP(,RMCTPTR)                              0234
         ALR   @15,@12                                             0234
         STH   @15,CCCAPLDP(,RMCTPTR)                              0234
*   CCCAPMDP=CCCAPMDP+CVTAPG;                                      0235
         LH    @15,CCCAPMDP(,RMCTPTR)                              0235
         ALR   @15,@12                                             0235
         STH   @15,CCCAPMDP(,RMCTPTR)                              0235
*   CCCSIGDP=CCCSIGDP+CVTAPG;                                      0236
         AH    @12,CCCSIGDP(,RMCTPTR)                              0236
         STH   @12,CCCSIGDP(,RMCTPTR)                              0236
*   CVTAPG=CCCAPGDP;                /* APG DISPATCHING PRIOTIRY      */
         STC   @05,CVTAPG(,CVTPTR)                                 0237
*/*  INITIALIZATION FOR ADDRESS SPACES CREATED PRIOR TO IEAVNP10     */
*/*  CHECK EACH OF THE POSSIBLE ASVTENTY ENTRIES FOR ADDRESS SPACES  */
*/*  THAT HAVE BEEN ASSIGNED                                         */
*                                                                  0238
*   ASVTPTR=CVTASVT;                /* ADDR OF ASVT                  */
         L     ASVTPTR,CVTASVT(,CVTPTR)                            0238
*   RESPECIFY                                                      0239
*    (R0,                                                          0239
*     R1,                                                          0239
*     R14,                                                         0239
*     R15) RESTRICTED;                                             0239
*   ASVTEPTR=ADDR(ASVTENTY);        /* POINT TO 1ST ASVTENTY ENTRY   */
         LA    ASVTEPTR,ASVTENTY(,ASVTPTR)                         0240
*   DO NP10ASID=ONE TO ASVTMAXU;    /* MAX NO. OF ADDR SPACES        */
         LA    @12,1                                               0241
         B     @DE00241                                            0241
@DL00241 DS    0H                                                  0242
*     IF ASVTAVL='0'B THEN          /* ASID HAS BEEN ASSIGNED        */
         TM    ASVTAVL(ASVTEPTR),B'10000000'                       0242
         BNZ   @RF00242                                            0242
*       DO;                                                        0243
*         ASVTE->ASCBASXB->ASXBOUSB->OUSBNAME=OUSBCHAR;/* INIT OUSB
*                                      WITH ACRONYM                  */
         L     @12,ASVTE(,ASVTEPTR)                                0244
         L     @12,ASCBASXB(,@12)                                  0244
         L     @12,ASXBOUSB(,@12)                                  0244
         MVC   OUSBNAME(4,@12),@CC02717                            0244
*         R1=STARTMEM;              /* INDICATES MEMORY ORIGIN       */
         LA    R1,1                                                0245
*         GEN(SYSEVENT MEMCREAT,ASIDL=NP10ASID)                    0246
*         REFS(NP10ASID);           /* ISSUE MEMCREAT SYSEVENT       */
*                                                                  0246
         SYSEVENT MEMCREAT,ASIDL=NP10ASID
*         /***********************************************************/
*         /*                                                         */
*         /* TURN ON THE TRANSWAP COMPLETE BIT FOR ANY WIRED IN      */
*         /* ADDRESS SPACE. OTHERWISE, THE SYSEVENT TRANSWAP ISSUED  */
*         /* BY THE INITIATOR WILL FAIL. (WIRED IN ADDRESS SPACES ARE*/
*         /* NONSWAPPABLE AT INITATT TIME. ALL OTHER ADDRESS SPACES  */
*         /* ARE SWAPPABLE AT THIS TIME.                     @ZA16887*/
*         /*                                                         */
*         /***********************************************************/
*                                                                  0247
*         ASVTE->ASCBOUCB->OUCBTSWC='1'B;/*                  @ZA16887*/
         L     @12,ASVTE(,ASVTEPTR)                                0247
         L     @12,ASCBOUCB(,@12)                                  0247
         OI    OUCBTSWC(@12),B'00001000'                           0247
*       END;                                                       0248
*     ASVTEPTR=ASVTEPTR+LENGTH(ASVTENTY);/* POINT TO NEXT ASVTENTY 0249
*                                      ENTRY                         */
@RF00242 AL    ASVTEPTR,@CF00054                                   0249
*   END;                                                           0250
         LA    @12,1                                               0250
         AH    @12,NP10ASID                                        0250
@DE00241 STH   @12,NP10ASID                                        0250
         C     @12,ASVTMAXU(,ASVTPTR)                              0250
         BNH   @DL00241                                            0250
*   RESPECIFY                                                      0251
*    (R0,                                                          0251
*     R1,                                                          0251
*     R14,                                                         0251
*     R15) UNRESTRICTED;                                           0251
*                                                                  0251
*/*  COMPLETED SRM INITIALIZATION                                    */
*/*  GET SRM LOCK                                                    */
*                                                                  0252
*   GEN REFS(PSALITA,FLC);                                         0252
NP10GSRM SETLOCK OBTAIN,TYPE=SRM,MODE=UNCOND,REGS=USE,                XX
               RELATED=(SRM,IEAVNP10(NP10RSRM))
*   RMCTINIT='1'B;                  /* INDICATE SRM INIT COMPLETED   */
         OI    RMCTINIT(RMCTPTR),B'00010000'                       0253
*/*  RELEASE SRM LOCK                                                */
*                                                                  0254
*   GEN REFS(PSALITA,FLC);                                         0254
*                                                                  0254
NP10RSRM SETLOCK RELEASE,TYPE=SRM,REGS=USE,                           XX
               RELATED=(SRM,IEAVNP10(NP10GSRM))
*/*  END OF SYSTEM RESOURCES MANAGER INITIALIZATION,                 */
*/*  RETURN TO NIP CONTROL                                           */
*                                                                  0255
*   RETURN;                         /* RETURN TO CALLER              */
@EL00001 L     @13,4(,@13)                                         0255
@EF00001 DS    0H                                                  0255
@ER00001 LM    @14,@12,12(@13)                                     0255
         BR    @14                                                 0255
*                                                                  0256
*/********************************************************************/
*/*                                                                  */
*/*  PROCEDURE NAME: NP10APGP                                        */
*/*                                                                  */
*/*  FUNCTION:                                                       */
*/*  APG SYSTEM PARAMETER PROCESSOR VALIDATES THE APG SYSTEM         */
*/*  PARAMETER AND STORES THE APG VALUE IN THE CVT                   */
*/*                                                                  */
*/********************************************************************/
*                                                                  0256
*NP10APGP:                                                         0256
*   PROC;                                                          0256
NP10APGP STM   @14,@12,12(@13)                                     0256
*   DECLARE                                                        0257
*     NP10APG FIXED(15),            /* APG VALUE                     */
*     NP10DGTC FIXED(15),           /* NO. OF DIGITS                 */
*     NP10DGT CHAR(1),              /* APG PARM INPUT CHAR           */
*     DEFAPG FIXED(8) CONSTANT(7)   /* DEFAULT APG VALUE=7           */
*         ;                                                        0257
*                                                                  0257
*/*  DETERMINE IF DEFAULT SYSTEM PARAMETER VALUE IS REQUESTED        */
*/*  THE APG ENTRY IN PARMTAB IS ZERO IF DEFAULT VALUE IS REQUESTED  */
*/*  BY THE INSTALLATION, OTHERWISE APG POINTS TO A CHARACTER STRING */
*/*  WHICH CONTAINS THE APG VALUE AND A QUOTE AS CLOSING DELIMITER   */
*                                                                  0258
*   IF APG=ZERO                     /* TEST FOR DEFAULT              */
*     THEN                          /* DEFAULT APG IS REQUESTED      */
         SLR   @12,@12                                             0258
         L     @05,NVTPTAB(,RNVT)                                  0258
         C     @12,APG(,@05)                                       0258
         BNE   @RF00258                                            0258
*     DO;                                                          0259
*       CVTAPG=DEFAPG;              /* USE DEFAULT APG VALUE         */
         MVI   CVTAPG(CVTPTR),X'07'                                0260
*       RETURN CODE(SUCCESS);       /* RETURN SUCCESSFUL             */
         LR    @15,@12                                             0261
         L     @14,12(,@13)                                        0261
         LM    @00,@12,20(@13)                                     0261
         BR    @14                                                 0261
*     END;                                                         0262
*                                                                  0262
*/*  VERIFY THAT THE INPUT IS NUMERIC AND DETERMINE THE NUMBER OF    */
*/*  DIGITS IN THE INPUT                                             */
*                                                                  0263
*   ELSE                            /* APG PARM IS SPECIFIED         */
*     DO;                                                          0263
@RF00258 DS    0H                                                  0264
*       NPTEPTR=ADDR(APG);          /* ADDR OF APG PARMTAB ENTRY     */
         L     NPTEPTR,NVTPTAB(,RNVT)                              0264
         LA    NPTEPTR,APG(,NPTEPTR)                               0264
*       SCNPTR=NIPPTADR;            /* ADDR OF INPUT STRING          */
         L     SCNPTR,NIPPTADR(,NPTEPTR)                           0265
*       IF PARMCHAR=LPAREN THEN     /* IGNORE DELIMITER              */
         CLI   PARMCHAR(SCNPTR),C'('                               0266
         BNE   @RF00266                                            0266
*         SCNPTR=SCNPTR+ONE;        /* SKIP LEFT PARENTHESIS         */
         AL    SCNPTR,@CF00062                                     0267
*       NP10DGTC=ZERO;              /* NO. OF DIGITS FOR APG         */
@RF00266 SLR   NP10DGTC,NP10DGTC                                   0268
*       NP10APG=ZERO;               /* APG VALUE=0                   */
         SLR   NP10APG,NP10APG                                     0269
*       DO I=ONE                    /* CHECK UP TO 2 DIGITS          */
*             TO THREE WHILE(PARMCHAR^=QUOTE&PARMCHAR^=RPAREN);/*  0270
*                                      TEST FOR DELIMITERS           */
         LA    I,1                                                 0270
@DL00270 CLI   PARMCHAR(SCNPTR),C''''                              0270
         BE    @DC00270                                            0270
         CLI   PARMCHAR(SCNPTR),C')'                               0270
         BE    @DC00270                                            0270
*         IF PARMCHAR>=CHAR0&PARMCHAR<=CHAR9/* TEST FOR NUMERIC      */
*           THEN                    /* CHAR IS NUMERIC               */
         CLI   PARMCHAR(SCNPTR),C'0'                               0271
         BL    @RF00271                                            0271
         CLI   PARMCHAR(SCNPTR),C'9'                               0271
         BH    @RF00271                                            0271
*           DO;                                                    0272
*             NP10DGTC=NP10DGTC+ONE;/* INCREMENT NO. OF DIGITS FOR 0273
*                                      APG                           */
         LA    @12,1                                               0273
         ALR   NP10DGTC,@12                                        0273
*             NP10DGT=DGTMASK;      /* CONVERT INPUT TO DECIMAL      */
         MVC   NP10DGT(1),DGTMASK                                  0274
*             NP10DGT=NP10DGT&PARMCHAR;                            0275
         NC    NP10DGT(1),PARMCHAR(SCNPTR)                         0275
*             NP10APG=NP10APG*TEN+NP10DGT;/* COMBINE SEVERAL DECIMAL
*                                      NUMBERS                       */
         LR    @09,NP10APG                                         0276
         MH    @09,@CH02141                                        0276
         SLR   @15,@15                                             0276
         IC    @15,NP10DGT                                         0276
         ALR   @09,@15                                             0276
         LR    NP10APG,@09                                         0276
*             SCNPTR=SCNPTR+ONE;    /* INCREMENT PTR TO CHAR         */
         ALR   SCNPTR,@12                                          0277
*           END;                                                   0278
*         ELSE                      /* CHAR IS NON-NUMERIC           */
*           RETURN CODE(FAIL);      /* RETURN UNSUCCESSFUL           */
         B     @RC00271                                            0279
@RF00271 LA    @15,4                                               0279
         L     @14,12(,@13)                                        0279
         LM    @00,@12,20(@13)                                     0279
         BR    @14                                                 0279
*       END;                        /* COMPLETED CHECKING INPUT      */
@RC00271 AL    I,@CF00062                                          0280
         C     I,@CF00293                                          0280
         BNH   @DL00270                                            0280
@DC00270 DS    0H                                                  0281
*       IF NP10DGTC=ZERO            /* TEST FOR NULL STRING          */
*           ×NP10DGTC>TWO           /* TEST FOR MORE THAN THE NO. OF
*                                      DIGITS ALLOWED                */
*           ×NP10APG>APGMAX         /* TEST FOR RANGE                */
*         THEN                      /* INVALID APG PARM VALUE        */
         LTR   NP10DGTC,NP10DGTC                                   0281
         BZ    @RT00281                                            0281
         C     NP10DGTC,@CF00074                                   0281
         BH    @RT00281                                            0281
         C     NP10APG,@CF01393                                    0281
         BNH   @RF00281                                            0281
@RT00281 DS    0H                                                  0282
*         RETURN CODE(FAIL);        /* RETURN UNSUCCESSFUL           */
         LA    @15,4                                               0282
         L     @14,12(,@13)                                        0282
         LM    @00,@12,20(@13)                                     0282
         BR    @14                                                 0282
*       CVTAPG=NP10APG;             /* STORE APG VALUE IN CVT        */
@RF00281 STC   NP10APG,CVTAPG(,CVTPTR)                             0283
*       RETURN CODE(SUCCESS);       /* RETURN SUCCESSFUL             */
         SLR   @15,@15                                             0284
         L     @14,12(,@13)                                        0284
         LM    @00,@12,20(@13)                                     0284
         BR    @14                                                 0284
*     END;                                                         0285
*   END NP10APGP;                   /* END OF APG SYSTEM PARAMETER 0286
*                                      PROCESSING                    */
*                                                                  0287
*/********************************************************************/
*/*                                                                  */
*/*  PROCEDURE NAME: NP10OPTP                                        */
*/*                                                                  */
*/*  FUNCTION:                                                       */
*/*  OPT SYSTEM PARAMETER PROCESSOR VALIDATES THE OPT SYSTEM         */
*/*  PARAMETER AND INVOKES THE OPT LIST PROCESSOR (IRARMOPT)         */
*/*  TO PROCESS THE OPT LIST                                         */
*/*  IT WRITES AN ERROR MESSAGE TO THE OPERATOR FOR EACH CATEGORY    */
*/*  OF OPT PARAMETERS IN THE OPT LIST IN ERROR                      */
*/*                                                                  */
*/********************************************************************/
*                                                                  0287
*NP10OPTP:                                                         0287
*   PROC;                                                          0287
*                                                                  0287
NP10OPTP STM   @14,@12,@SA00003                                    0287
*/*  DETERMINE IF DEFAULT SYSTEM PARAMETER VALUE IS REQUESTED        */
*/*  THE OPT ENTRY IN PARMTAB IS ZERO IF DEFAULT VALUE IS REQUESTED  */
*/*  BY THE INSTALLATION, OTHERWISE OPT POINTS TO A CHARACTER STRING */
*                                                                  0288
*   IF OPT^=ZERO                    /* CHECK IF OPT HAS BEEN SPEC    */
*     THEN                          /* YES                           */
         L     @12,NVTPTAB(,RNVT)                                  0288
         L     @08,OPT(,@12)                                       0288
         LTR   @08,@08                                             0288
         BZ    @RF00288                                            0288
*     DO;                                                          0289
*       NP10LIST=OPTNAME;           /* THE CHARS 'OPT'               */
         MVC   NP10LIST(3),@CC02723                                0290
*       NPTEPTR=ADDR(OPT);          /* ADDR OF PARMTAB OPT ENTRY     */
         LA    NPTEPTR,OPT(,@12)                                   0291
*       ELBLPTR=ADDR(NP10OPTE);     /* LABEL FOR OPT ERROR PROCESSING*/
         LA    ELBLPTR,NP10OPTE                                    0292
*       CALL NP10VLDP;              /* INVOKE SYSTEM PARAMETER     0293
*                                      VALIDATION ROUTINE            */
         BAL   @14,NP10VLDP                                        0293
*/*  INVOKE OPT LIST PROCESSOR TO SCAN AND PROCESS THE DATA IN       */
*/*  THE OPT LIST                                                    */
*                                                                  0294
*       NP10WOPE=ZERO;              /* CLEAR OPT ERROR INDICATOR     */
         SLR   @12,@12                                             0294
         ST    @12,NP10WOPE                                        0294
*       CALL IRARMOPT(NP10WOPE,     /* OPT ERROR INDICATOR           */
*           NP10WST);               /* STATUS INDICATOR              */
         L     @15,@CV02795                                        0295
         LA    @01,@AL00295                                        0295
         BALR  @14,@15                                             0295
*/*  CHECK FOR I/O ERROR                                             */
*                                                                  0296
*       IF NP10IOER='0'B            /* NO I/O ERROR FOUND            */
*         THEN                      /* NO I/O ERROR                  */
         TM    NP10IOER,B'10000000'                                0296
         BNZ   @RF00296                                            0296
*         DO;                                                      0297
*                                                                  0297
*/*  CHECK IF ANY ERROR HAS BEEN FOUND IN THE OPT LIST               */
*/*  A BIT IN NP10WOPE IS SET BY THE OPT LIST PROCESSOR IF THERE IS  */
*/*  ANY ERROR IN A CATEGORY OF OPT PARAMETERS                       */
*/*  FOR EACH CATEGORY IN ERROR, WRITE MESSAGE IEA874I TO OPERATOR   */
*                                                                  0298
*           IF NP10WOPE^=ZERO       /* TEST FOR ERROR                */
*             THEN                  /* ERROR FOUND                   */
         L     @12,NP10WOPE                                        0298
         LTR   @12,@12                                             0298
         BZ    @RF00298                                            0298
*             DO;                                                  0299
*               MSG3I2=NP10WSUF;    /* INSERT SUFFIX FOR MEMBER NAME
*                                      IN MESSAGE                    */
         MVC   MSG3I2(2),NP10WSUF                                  0300
*               DO I=               /* CHECK ERROR INDICATOR         */
*                     ONE TO DIM(NP10CGT);/* NO. OF CATEGORIES       */
         LA    I,1                                                 0301
@DL00301 DS    0H                                                  0302
*                 IF(NP10WOPE&NP10MASK)=NP10MASK THEN/* ERROR IN   0302
*                                      CATEGORY                      */
         L     @12,NP10MASK                                        0302
         L     @08,NP10WOPE                                        0302
         NR    @08,@12                                             0302
         CLR   @08,@12                                             0302
         BNE   @RF00302                                            0302
*                   DO;                                            0303
*                     MSG3I1=NP10CGID(I);/* INSERT CATEGORY ID IN MSG*/
         LR    @12,I                                               0304
         MH    @12,@CH00293                                        0304
         LA    @08,NP10CGID-3(@12)                                 0304
         MVC   MSG3I1(3),0(@08)                                    0304
*                     GEN(IEAPMNIP TYPE=WTO,PARAM=MSG3S)           0305
*                     REFS(NVTWTO); /* WTO IEA874I MESSAGE           */
         IEAPMNIP TYPE=WTO,PARAM=MSG3S
*                   END;                                           0306
*                 NP10WOPE=NP10WOPE/TWO;/* SHIFT ONE BIT TO RIGHT    */
@RF00302 L     @12,NP10WOPE                                        0307
         SRL   @12,1                                               0307
         ST    @12,NP10WOPE                                        0307
*               END;                /* COMPLETED CHECKING FOR OPT  0308
*                                      ERROR                         */
         AL    I,@CF00062                                          0308
         C     I,@CF00293                                          0308
         BNH   @DL00301                                            0308
*             END;                                                 0309
*         END;                                                     0310
*       ELSE                        /* I/O ERROR FOUND OR MEMBER NOT
*                                      FOUND                         */
*         GO TO NP10OPTE;           /* ERROR                         */
*     END;                                                         0312
*                                                                  0312
@RC00296 DS    0H                                                  0313
*/*  SCALE THE RESOURCE COEFFICIENT FACTORS                          */
*                                                                  0313
*   RMPTCPU=RMPTCPU*RCSCALE/TEN;    /* SCALE CPU RESOURCE FACTOR   0313
*                                      COEFFICIENT                   */
@RF00288 LA    @12,10                                              0313
         L     @01,RMPTCPU(,RMPTPTR)                               0313
         M     @00,@CF00685                                        0313
         DR    @00,@12                                             0313
         ST    @01,RMPTCPU(,RMPTPTR)                               0313
*   RMPTIOC=RMPTIOC*RCSCALE/TEN;    /* SCALE IOC RESOURCE FACTOR   0314
*                                      COEFFICIENT                   */
         L     @01,RMPTIOC(,RMPTPTR)                               0314
         M     @00,@CF00685                                        0314
         DR    @00,@12                                             0314
         ST    @01,RMPTIOC(,RMPTPTR)                               0314
*/*  ADJUST CPU DEPENDENT FIELDS IN RMPT BY CPU ADJUSTMENT FACTOR    */
*                                                                  0315
*   NP10ADJN=RMPTTOM;                                              0315
         L     @12,RMPTTOM(,RMPTPTR)                               0315
         ST    @12,NP10ADJN                                        0315
*   CALL NP10ADJP;                  /* ADJUST NO. FOR IPLING CPU     */
         BAL   @14,NP10ADJP                                        0316
*   RMPTTOM=NP10ADJN;                                              0317
         L     @12,NP10ADJN                                        0317
         ST    @12,RMPTTOM(,RMPTPTR)                               0317
*   NP10ADJN=RMPTTOL;                                              0318
         L     @12,RMPTTOL(,RMPTPTR)                               0318
         ST    @12,NP10ADJN                                        0318
*   CALL NP10ADJP;                  /* ADJUST NO. FOR IPLING CPU     */
         BAL   @14,NP10ADJP                                        0319
*   RMPTTOL=NP10ADJN;                                              0320
*                                                                  0320
         L     @12,NP10ADJN                                        0320
         ST    @12,RMPTTOL(,RMPTPTR)                               0320
*/*  CONVERT THE SERVICE UNITS IN THE FOLLOWING RMPT FIELDS TO THE   */
*/*  TIME (IN 1/1024 OF A SECOND) NEEDED TO EXECUTE THESE            */
*/*  SERVICE UNITS.                                                  */
*                                                                  0321
*   NP10ADJN=RMPTERV*NP10INST/K;    /* CONVERT TO MILLISECS          */
         L     @01,RMPTERV(,RMPTPTR)                               0321
         M     @00,NP10INST                                        0321
         D     @00,@CF02671                                        0321
         ST    @01,NP10ADJN                                        0321
*   CALL NP10ADJP;                  /* ADJUST NO. FOR IPLING CPU     */
         BAL   @14,NP10ADJP                                        0322
*   RMPTERV=NP10ADJN;                                              0323
         L     @12,NP10ADJN                                        0323
         ST    @12,RMPTERV(,RMPTPTR)                               0323
*   RETURN CODE(SUCCESS);           /* RETURN WITH SUCCESS           */
         SLR   @15,@15                                             0324
         L     @14,@SA00003                                        0324
         LM    @00,@12,@SA00003+8                                  0324
         BR    @14                                                 0324
*NP10OPTE:                                                         0325
*   RETURN CODE(FAIL);              /* RETURN WITH FAILURE           */
NP10OPTE LA    @15,4                                               0325
         L     @14,@SA00003                                        0325
         LM    @00,@12,@SA00003+8                                  0325
         BR    @14                                                 0325
*   END NP10OPTP;                   /* END OF OPT SYSTEM PARAMETER 0326
*                                      PROCESSING                    */
*                                                                  0327
*/********************************************************************/
*/*                                                                  */
*/*  PROCEDURE NAME: NP10IPSP                                        */
*/*                                                                  */
*/*  FUNCTION:                                                       */
*/*  IPS SYSTEM PARAMETER PROCESSOR VALIDATES THE IPS SYSTEM         */
*/*  PARAMETER AND INVOKES THE IPS LIST PROCESSOR (IRARMIPS)         */
*/*  TO PROCESS THE IPS LIST                                         */
*/*  IF THE IPS LIST IS INVALID OR I/O ERROR HAS OCCURRED,           */
*/*  IT RETURNS TO THE CALLER WITH RETURN CODE=4                     */
*/********************************************************************/
*                                                                  0327
*NP10IPSP:                                                         0327
*   PROC;                                                          0327
NP10IPSP STM   @14,@12,@SA00004                                    0327
*   NP10LIST=IPSNAME;               /* THE CHARS 'IPS'               */
         MVC   NP10LIST(3),@CC02721                                0328
*   NPTEPTR=ADDR(IPS);              /* ADDR OF PARMLIB IPS ENTRY     */
         L     @12,NVTPTAB(,RNVT)                                  0329
         LA    NPTEPTR,IPS(,@12)                                   0329
*/*  DETERMINE IF DEFAULT SYSTEM PARAMETER VALUE IS REQUESTED        */
*/*  THE IPS ENTRY IN PARMTAB IS ZERO IF DEFAULT VALUE IS REQUESTED  */
*/*  BY THE INSTALLATION, OTHERWISE IPS POINTS TO A CHARACTER STRING */
*/*  WHICH CONTAINS THE SUFFIX FOR THE IPS LIST                      */
*                                                                  0330
*   IF IPS=ZERO THEN                /* DEFAULT HAS BEEN REQUESTED    */
         L     @12,IPS(,@12)                                       0330
         LTR   @12,@12                                             0330
         BNZ   @RF00330                                            0330
*     DO;                                                          0331
*                                                                  0331
*/*  THE DEFAULT IPS LIST IN IEAIPS00 MEMBER IS REQUESTED BY THE     */
*/*  INSTALLATION                                                    */
*/*  IF THERE IS ERROR IN THE IEAIPS00 LIST, A WIRED-IN SKELETON     */
*/*  IPS LIST WILL BE USED AND A MESSAGE WILL BE WRITTEN TO THE      */
*/*  OPERATOR                                                        */
*                                                                  0332
*       IF NP10IPDL='0'B            /* TEST IF DEFAULT HAS BEEN USED
*                                      BEFORE                        */
*         THEN                      /* NO                            */
         TM    NP10IPDL,B'00010000'                                0332
         BNZ   @RF00332                                            0332
*         DO;                                                      0333
*           NP10WSUF=DFSUFX;        /* SUFFIX = DEFAULT SUFFIX       */
         MVC   NP10WSUF(2),@CC02533                                0334
*           NIPPANAM(1:3)=IEANAME;  /* STORE MEMBER NAME             */
         L     @12,NVTPAREA(,RNVT)                                 0335
         MVC   NIPPANAM(3,@12),@CC02719                            0335
*           NIPPANAM(4:6)=NP10LIST;                                0336
         MVC   NIPPANAM+3(3,@12),NP10LIST                          0336
*           NIPPANAM(7:8)=NP10WSUF;                                0337
         MVC   NIPPANAM+6(2,@12),NP10WSUF                          0337
*           NP10IOER='0'B;          /* RESET I/O ERROR INDICATOR     */
*           NP10EOF='0'B;           /* RESET END OF FILE INDICATOR   */
         NI    NP10IOER,B'00111111'                                0339
*           IF NIPPTLST='1'B        /* TEST NIP PARM ENTRY LIST FLAG */
*             THEN                  /* LISTING REQUESTED             */
         TM    NIPPTLST(NPTEPTR),B'01000000'                       0340
         BNO   @RF00340                                            0340
*             NP10LST='1'B;         /* SET LIST SWITCH ON            */
         OI    NP10LST,B'00100000'                                 0341
*           ELSE                    /* LISTING NOT REQUESTED         */
*             NP10LST='0'B;         /* SET LIST SWITCH OFF           */
         B     @RC00340                                            0342
@RF00340 NI    NP10LST,B'11011111'                                 0342
*         END;                                                     0343
*       ELSE                        /* DEFAULT HAS BEEN TRIED        */
*         DO;                       /* USE SKELETON IPS              */
         B     @RC00332                                            0344
@RF00332 DS    0H                                                  0345
*           RESPECIFY                                              0345
*            (R0,                                                  0345
*             R1,                                                  0345
*             R14,                                                 0345
*             R15) RESTRICTED;                                     0345
*           NP10STSZ=LENGTH(WMSB);  /* SIZE OF SKELETON IPS          */
         MVC   NP10STSZ(3),@CF03479+1                              0346
*           R0=NP10P245;            /* SUBPOOL NO. 245               */
         L     R0,NP10P245                                         0347
*           GEN(GETMAIN R,LV=(0));  /* GETMAIN FROM SQA              */
         GETMAIN R,LV=(0)
*           WMSTPTR=R1;             /* ADDR OF WMST                  */
         LR    WMSTPTR,R1                                          0349
*           RESPECIFY                                              0350
*            (R0,                                                  0350
*             R1,                                                  0350
*             R14,                                                 0350
*             R15) UNRESTRICTED;                                   0350
*                                                                  0350
*/*  INITIALIZE THE INTERVAL SERVICE VALUE FOR PG 1 & 2 TO THE       */
*/*  MIN INTERVAL SERVICE VALUE IN THE RMPT                          */
*                                                                  0351
*           DPGD0ISV=RMPTIMN;       /* FOR PG 0                      */
         L     @12,RMPTIMN(,RMPTPTR)                               0351
         ST    @12,DPGD0ISV                                        0351
*           DPGD1ISV=RMPTIMN;       /* FOR PG 1                      */
         ST    @12,DPGD1ISV                                        0352
*           DPGD2ISV=RMPTIMN;       /* FOR PG 2                      */
         ST    @12,DPGD2ISV                                        0353
*/*  MOVE DEFAULT IPS TO AREA IN SQA                                 */
*                                                                  0354
*           RMCTWMST=WMSTPTR;       /* ADDR OF WMST IN SQA           */
         ST    WMSTPTR,RMCTWMST(,RMCTPTR)                          0354
*           RESPECIFY                                              0355
*            (R0,                                                  0355
*             R1,                                                  0355
*             R14,                                                 0355
*             R15) RESTRICTED;      /*                       @Z40BPCH*/
*           R0=ADDR(IPSB);          /* ADDR OF AREA WHICH WILL     0356
*                                      CONTAIN IPS           @Z40BPCH*/
         LR    R0,WMSTPTR                                          0356
*           R1=LENGTH(WMSB);        /* SIZE OF SKEL. IPS     @Z40BPCH*/
         LA    @12,272                                             0357
         LR    R1,@12                                              0357
*           R14=ADDR(WMSB);         /* SKELETON IPS          @Z40BPCH*/
         LA    R14,WMSB                                            0358
*           R15=LENGTH(WMSB);       /* SIZE OF SKEL. IPS     @Z40BPCH*/
         LR    R15,@12                                             0359
*           GEN( MVCL R0,R14) ;     /* MOVE IN SKEL. IPS     @Z40BPCH*/
          MVCL R0,R14
*           RESPECIFY                                              0361
*            (R0,                                                  0361
*             R1,                                                  0361
*             R14,                                                 0361
*             R15) UNRESTRICTED;    /*                       @Z40BPCH*/
*/*  ADJUST THE ADDRESSES OF -                                       */
*/*    PERFORMANCE GROUP VECTOR TABLE (WPGVT)                        */
*/*    PERFORMANCE OBJECTIVE VECTOR TABLE (WPOVT)                    */
*/*    PERFORMANCE GROUP DESCRIPTOR TABLE (WPGDT)                    */
*/*    PERFORMANCE OBJECTIVE DESCRIPTOR TABLE (WPODT)                */
*/*    DOMAIN DESCRIPTOR TABLE                               @Z40BPCH*/
*/*    DOMAIN VECTOR TABLE                                   @Z40BPCH*/
*                                                                  0362
