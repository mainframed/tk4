*/*                                                                  */
*/*  PROCEDURE NAME: NP10VLDP                                        */
*/*                                                                  */
*/*  FUNCTION:                                                       */
*/*  CHECK THE INPUT FOR IPS/OPT SYSTEM PARAMETERS.                  */
*/*  DETERMINE IF DEFAULT SYSTEM PARAMETER VALUE IS REQUESTED.       */
*/*  OTHERWISE IT POINTS TO TWO ALPHANUMERIC CHARACTERS WHICH ARE    */
*/*  USED AS SUFFIXES FOR THE MEMBER NAME OF THE IPS/OPT LIST.       */
*/*                                                                  */
*/********************************************************************/
*                                                                  0412
*NP10VLDP:                                                         0412
*   PROC;                                                          0412
         ST    @05,@TF00002                                        0412
NP10VLDP STM   @14,@12,12(@13)                                     0412
*   SCNPTR=NIPPTADR;                /* ADDR OF INPUT STRING          */
         L     SCNPTR,NIPPTADR(,NPTEPTR)                           0413
*   IF PARMCHAR=LPAREN THEN         /* IFNORE DELIMITER              */
         CLI   PARMCHAR(SCNPTR),C'('                               0414
         BNE   @RF00414                                            0414
*     SCNPTR=SCNPTR+ONE;            /* SKIP LEFT PARENTHESIS         */
         AL    SCNPTR,@CF00062                                     0415
*/*  CHECK FOR 2 ALPHANUMERIC CHARACTERS                             */
*                                                                  0416
*   DO I=ONE TO TWO;                                               0416
@RF00414 LA    I,1                                                 0416
@DL00416 DS    0H                                                  0417
*     IF PARMCHAR<CHARA×(PARMCHAR>CHARZ&PARMCHAR<CHAR0)×PARMCHAR>CHAR9
*       THEN                        /* CHAR IS NON-ALPHANUMERIC      */
         CLI   PARMCHAR(SCNPTR),C'A'                               0417
         BL    @RT00417                                            0417
         CLI   PARMCHAR(SCNPTR),C'Z'                               0417
         BNH   @GL00007                                            0417
         CLI   PARMCHAR(SCNPTR),C'0'                               0417
         BL    @RT00417                                            0417
@GL00007 CLI   PARMCHAR(SCNPTR),C'9'                               0417
         BNH   @RF00417                                            0417
@RT00417 DS    0H                                                  0418
*       RETURN TO NP10ELBL;         /* RETURN TO CALLER AT A LABEL 0418
*                                      WHICH PROCESSES ERROR         */
         LR    @14,ELBLPTR                                         0418
         LM    @15,@12,16(@13)                                     0418
         BR    @14                                                 0418
*     ELSE                          /* CHAR IS ALPHANUMERIC          */
*       SCNPTR=SCNPTR+ONE;          /* POINT TO NEXT CHAR            */
@RF00417 AL    SCNPTR,@CF00062                                     0419
*   END;                            /* COMPLETED INPUT VALIDATION    */
         AL    I,@CF00062                                          0420
         C     I,@CF00074                                          0420
         BNH   @DL00416                                            0420
*/*  STORE SUFFIX IN NP10WSUF AND STORE MEMBER NAME IN PARMAREA      */
*/*  TO COMMUNICATE WITH NIP READ SERVICE ROUTINE                    */
*                                                                  0421
*   SCNPTR=SCNPTR-TWO;              /* POINT TO SUFFIX               */
         BCTR  SCNPTR,0                                            0421
         BCTR  SCNPTR,0                                            0421
*   NP10WSUF=SUFFIX;                /* SAVE SUFFIX                   */
         MVC   NP10WSUF(2),SUFFIX(SCNPTR)                          0422
*   NIPPANAM(1:3)=IEANAME;          /* STORE MEMBER NAME             */
         L     @12,NVTPAREA(,RNVT)                                 0423
         MVC   NIPPANAM(3,@12),@CC02719                            0423
*   NIPPANAM(4:6)=NP10LIST;                                        0424
         MVC   NIPPANAM+3(3,@12),NP10LIST                          0424
*   NIPPANAM(7:8)=NP10WSUF;                                        0425
*                                                                  0425
         MVC   NIPPANAM+6(2,@12),NP10WSUF                          0425
*/*  SET SWITCH TO INDICATE WHETHER LISTING IS REQUESTED ACCORDING   */
*/*  TO THE LIST FLAG IN THE IPS/OPT PARMTAB ENTRY                   */
*                                                                  0426
*   IF NIPPTLST='1'B                /* TEST IF LIST IS REQUESTED     */
*     THEN                          /* LISTING IS REQUESTED          */
         TM    NIPPTLST(NPTEPTR),B'01000000'                       0426
         BNO   @RF00426                                            0426
*     NP10LST='1'B;                 /* SET LIST SWITCH ON            */
         OI    NP10LST,B'00100000'                                 0427
*   ELSE                            /* LISTING IS NOT REQUESTED      */
*     NP10LST='0'B;                 /* SET LIST SWITCH OFF           */
         B     @RC00426                                            0428
@RF00426 NI    NP10LST,B'11011111'                                 0428
*   NP10IOER='0'B;                  /* RESET I/O ERROR INDICATOR     */
@RC00426 DS    0H                                                  0430
*   NP10EOF='0'B;                   /* RESET END OF FILE INDICATOR   */
         NI    NP10IOER,B'00111111'                                0430
*   RETURN;                         /* RETURN TO CALLER              */
@EL00005 DS    0H                                                  0431
@EF00005 DS    0H                                                  0431
@ER00005 LM    @14,@12,12(@13)                                     0431
         BR    @14                                                 0431
*   END NP10VLDP;                   /* END VALIDATION ROUTINE        */
