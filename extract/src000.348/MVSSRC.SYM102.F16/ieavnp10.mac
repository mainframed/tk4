*/*                                                                  */
*/*  ENTRY POINT NAME: IRARMRDR                                      */
*/*                                                                  */
*/*  FUNCTION:                                                       */
*/*  INVOKE NIPPMMEM SUBROUTINE TO READ A RECORD FROM A MEMBER OF    */
*/*  SYS1.PARMLIB.  THE MEMBER NAME HAS BEEN PLACED IN THE NIPPANAM  */
*/*  FIELD OF THE NIP PARAMETER AREA (NIPPAREA).                     */
*/*  CHECK RETURN CODE FROM NIPPMMEM.                                */
*/*  IF RETURN CODE INDICATES SUCCESS, GET A BUFFER FROM SUBPOOL 0,  */
*/*  MOVE TEXT RECORD TO BUFFER, IF LISTING HAS BEEN REQUESTED,      */
*/*  WRITE RECORD ON OPERATOR'S CONSOLE, AND RETURN TO CALLER.       */
*/*  IF RETURN CODE FROM NIPPMMEM INDICATES MEMBER NOT FOUND OR I/O  */
*/*  ERROR CONDITION, SET NP10IOER FLAG ON AND RETURN TO CALLER.     */
*/*                                                                  */
*/********************************************************************/
*                                                                  0439
*   GEN CODE DEFS(IRARMRDR) REFS(RMRDRSAV,RMRDRPML) NOEXIT SEQFLOW;
         ENTRY IRARMRDR
RMRDRBSF EQU   0                         DISPLACEMENT FOR BASE REG 1
RMRDRSAP EQU   4                         OFFSET FOR SAVEAREA PTR
RMRDRRTP EQU   8                         OFFSET FOR RETURN PTR
RMRDRREG EQU   12                        OFFSET FOR REG SAVEAREA
RMRDRRG0 EQU   20                        OFFSET FOR REG 0 SAVEAREA
RMRDRBSS EQU   4095                      DISPLACEMENT FOR BASE REG 2
RMRDRPDP EQU   0                         DISPLACEMENT OF PARM LIST
RMRDRPLN EQU   12                        LENGTH OF PARM LIST
IRARMRDR DS    0H
         STM   @14,@12,RMRDRREG(@13)     SAVE REGS
         BALR  @15,RMRDRBSF              SETUP ENTRY ADDRESSABILITY
         LA    @10,*-@PSTART             DISPLACEMENT TO MODULE BASE
         LCR   @10,@10                   NEGATIVE FOR SUBTRACT
         ALR   @10,@15                   SETUP MODULE ADDRESSABILITY
         LA    @11,RMRDRBSS(,@10)        DISPLACEMENT TO BASE+4095
         USING @PSTART,@10
         USING @PSTART+RMRDRBSS,@11
         ST    @13,RMRDRSAV+RMRDRSAP     SAVE SAVEAREA PTR
         LA    @14,RMRDRSAV
         ST    @14,RMRDRRTP(,@13)        SAVE RETURN PTR
         LR    @13,@14
         MVC   RMRDRPML(RMRDRPLN),RMRDRPDP(@01) SET UP PARM LIST
*   RESPECIFY                                                      0440
*    (R0,                                                          0440
*     R1,                                                          0440
*     R14,                                                         0440
*     R15) RSTD;                                                   0440
*   IF NP10EOF='1'B THEN            /* REACH END OF FILE ?           */
         TM    NP10EOF,B'01000000'                                 0441
         BNO   @RF00441                                            0441
*     DO;                                                          0442
*       R15=FAIL;                   /* RETURN UNSUCCESSFUL           */
         LA    R15,4                                               0443
*       GO TO NP10REND;             /* RETURN                        */
         B     NP10REND                                            0444
*     END;                                                         0445
*   RNVT=NP10NVTP;                  /* ADDR OF NVT                   */
@RF00441 L     RNVT,NP10NVTP                                       0446
*   CVTPTR=FLCCVT;                  /* ADDR OF CVT                   */
         L     CVTPTR,FLCCVT                                       0447
*   R0=RECORDSZ;                    /* SUBPOOL NO=0,SIZE=88          */
         LA    R0,88                                               0448
*   GEN( GETMAIN R,LV=(0));         /* GETMAIN FROM SUBPOOL 0        */
          GETMAIN R,LV=(0)
*   NP10RBPT=R1;                    /* ADDR OF BUFFER                */
         L     @12,RMRDRPM1                                        0450
         ST    R1,NP10RBPT(,@12)                                   0450
*   NP10RBLN=RECORDSZ;              /* SIZE OF BUFFER                */
         LA    @12,88                                              0451
         L     @05,RMRDRPM2                                        0451
         ST    @12,NP10RBLN(,@05)                                  0451
*   NP10RBSP=RECORDSZ;              /* SUBPOOL INFORMATION           */
         L     @05,RMRDRPM3                                        0452
         ST    @12,NP10RBSP(,@05)                                  0452
*   CALL NP10SPIO;                  /* READ A RECORD FROM          0453
*                                      SYS1.PARMLIB                  */
         BAL   @14,NP10SPIO                                        0453
*   RESPECIFY                                                      0454
*     RPARM RESTRICTED;                                            0454
*   IF RPARM>=ZERO                  /* TEST RETURN CODE              */
*     THEN                          /* SUCCESSFUL READ               */
         LTR   RPARM,RPARM                                         0455
         BM    @RF00455                                            0455
*     DO;                                                          0456
*       RESPECIFY                                                  0457
*         RPARM UNRESTRICTED;                                      0457
*       NP10RBUF(1:1)=' ';          /* INSERT BLANK IN 1ST BYTE      */
         L     @12,RMRDRPM1                                        0458
         L     @12,NP10RBPT(,@12)                                  0458
         MVI   NP10RBUF(@12),C' '                                  0458
*       NP10RBUF(2:81)=NIPPARCD;    /* MOVE IN DATA                  */
         L     @05,NVTPAREA(,RNVT)                                 0459
         MVC   NP10RBUF+1(80,@12),NIPPARCD(@05)                    0459
*       IF NP10LST='1'B THEN        /* LISTING REQUESTED             */
         TM    NP10LST,B'00100000'                                 0460
         BNO   @RF00460                                            0460
*         DO;                                                      0461
*           MSGLSTP(1:71)=NIPPARCD; /* MOVE TO WTO BUFFER            */
         MVC   MSGLSTP(71),NIPPARCD(@05)                           0462
*           GEN(IEAPMNIP TYPE=WTO,PARAM=MSGLSTS)                   0463
*           REFS(NVTWTO);           /* WTO TEXT RECORD               */
         IEAPMNIP TYPE=WTO,PARAM=MSGLSTS
*         END;                                                     0464
*       R15=SUCCESS;                /* RETURN SUCCESSFUL             */
@RF00460 SLR   R15,R15                                             0465
*       GO TO NP10REND;             /* RETURN                        */
         B     NP10REND                                            0466
*     END;                                                         0467
*   ELSE                            /* UNSUCCESSFUL READ             */
*     DO;                                                          0468
@RF00455 DS    0H                                                  0469
*       NP10EOF='1'B;               /* SET END OF FILE INDICATOR     */
         OI    NP10EOF,B'01000000'                                 0469
*       IF NIPPARC^=EOFCOND         /* TEST CAUSE OF READ FAILURE    */
*         THEN                      /* NOT END-OF-FILE               */
         L     @12,NVTPAREA(,RNVT)                                 0470
         CLC   NIPPARC(4,@12),@CF00792                             0470
         BE    @RF00470                                            0470
*         NP10IOER='1'B;            /* SET I/O ERROR FLAG            */
         OI    NP10IOER,B'10000000'                                0471
