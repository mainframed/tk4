         CL    @09,RMPTSAET(,RMCTPTR)                           02S0476
         BH    @RT00476                                         02S0476
         TM    OUCBRMA(OUCBPTR),B'00100000'                     02S0476
         BNO   @RF00476                                         02S0476
@RT00476 DS    0H                                               02S0477
*           CALL IRARMCVL;          /* CALL CVL TO CMPUT NEW CMPS RV
*                                                            @Z40BPJC*/
         BAL   @14,IRARMCVL                                     02S0477
*         ELSE                                                  02S0478
*           OUCBCMRV=OUCBWMR;       /* OR USE OLD WMR AS COMPOSITE
*                                      SINCE RTB = 0         @Z40BPJC*/
         B     @RC00476                                         02S0478
@RF00476 L     @09,OUCBWMR(,OUCBPTR)                            02S0478
         ST    @09,OUCBCMRV(,OUCBPTR)                           02S0478
*         IF RMCTCPS1='0'B THEN                                 02S0479
@RC00476 TM    RMCTCPS1(RMCTPTR),B'01000000'                    02S0479
         BNZ   @RF00479                                         02S0479
*           DO;                     /* NO PREVIOUS DOMAIN CHANGE
*                                                            @Z40BPJC*/
*             IF DMDTPTR2^=OUCBDMO THEN                         02S0481
         CH    DMDTPTR2,OUCBDMO(,OUCBPTR)                       02S0481
         BE    @RF00481                                         02S0481
*               DO;                 /* IF THIS OUCB'S DOMAIN CHANGED,
*                                                            @Z40BPJC*/
*                 RMCTCPS1='1'B;    /* SIGNAL TO QUIT SRCH FOR HI RV
*                                                            @Z40BPJC*/
         OI    RMCTCPS1(RMCTPTR),B'01000000'                    02S0483
*                 CPICKPTR=0;       /* SET RETURN CODE TO SHOW NO
*                                      OUCB                  @Z40BPJC*/
*                                                               02S0484
         SLR   CPICKPTR,CPICKPTR                                02S0484
*                 /***************************************************/
*                 /*                                                 */
*                 /* ? IRACTLCL EPN(CAP)                             */
*                 /*                                                 */
*                 /***************************************************/
*                                                               02S0485
*                 DO;                                           02S0485
*                   R1=ADDR(RMEPBCAP);/* CAP REQUEST IS PARAM        */
         LA    R1,RMEPBCAP(,RMCTPTR)                            02S0486
*                   CALL RMSBCRL->IRARMRTN;/* ALGRTHM SCH FUNCT      */
         L     @15,RMSBCRL(,RMCTPTR)                            02S0487
         BALR  @14,@15                                          02S0487
*                 END;              /* ENSURE CAP REDRIVE SOON       */
*               END;                /*                       @Z40BPJC*/
*             ELSE                                              02S0490
*               DO;                 /* NO DOMAIN CHG FOUND BY CVL
*                                                            @Z40BPJC*/
         B     @RC00481                                         02S0490
@RF00481 DS    0H                                               02S0491
*                 IF CPICKPTR=0×(OUCBCMRV>CPICKPTR->OUCBCMRV) THEN
         LTR   CPICKPTR,CPICKPTR                                02S0491
         BZ    @RT00491                                         02S0491
         L     @09,OUCBCMRV(,OUCBPTR)                           02S0491
         C     @09,OUCBCMRV(,CPICKPTR)                          02S0491
         BNH   @RF00491                                         02S0491
@RT00491 DS    0H                                               02S0492
*                   CPICKPTR=OUCBPTR;/* USE HIGHR RV OF TWO. IF 02S0492
*                                      EQUAL, USE OLDER OUCB @Z40BPJC*/
         LR    CPICKPTR,OUCBPTR                                 02S0492
*               END;                /*                       @Z40BPJC*/
@RF00491 DS    0H                                               02S0494
*           END;                    /*                       @Z40BPJC*/
@RC00481 DS    0H                                               02S0495
*       END;                        /*                       @Z40BPJC*/
@RF00479 DS    0H                                               02S0496
*     OUCBPTR=OUCBFWD;              /* CONTINUE FORWARD SCAN @Z40BPJC*/
@RF00474 L     OUCBPTR,OUCBFWD(,OUCBPTR)                        02S0496
*   END;                            /*                       @Z40BPJC*/
@DE00473 TM    OUCBSCN(OUCBPTR),B'00000001'                     02S0497
         BZ    @DL00473                                         02S0497
*   OUCBPTR=CPICKPTR;               /* PASS BACK SELECTED OUCB  02S0498
*                                                            @Z40BPJC*/
         LR    OUCBPTR,CPICKPTR                                 02S0498
*   DMDTPTR2=DMDTPTR2+RMCTDMDT;     /* CONVERT BACK TO ADDR. @Z40BPJC*/
         AL    DMDTPTR2,RMCTDMDT(,RMCTPTR)                      02S0499
*   RTNREG=WKRE;                    /* RESTORE RETURN REGISTER  02S0500
*                                                            @Z40BPJC*/
         LR    RTNREG,WKRE                                      02S0500
*   RESPECIFY                                                   02S0501
*     CPICKPTR UNRSTD;              /*                       @Z40BPJC*/
*   END IRARMCPI;                   /*                       @Z40BPJC*/
@EL00005 DS    0H                                               02S0502
@EF00005 DS    0H                                               02S0502
@ER00005 BR    @14                                              02S0502
*                                                               02S0503
*/***********************************************************@Z40BPJC*/
*/*                                                          @Z40BPJC*/
*/*   ROUTINE NAME - IRARMCPO                                @Z40BPJC*/
*/*                                                          @Z40BPJC*/
*/*   DESCRIPTIVE NAME - SYSTEM RESOURCES MANAGER (SRM)      @Z40BPJC*/
*/*                      PICK SWAP-OUT CANDIDATE SUBROUTINE  @Z40BPJC*/
*/*                                                          @Z40BPJC*/
*/*   FUNCTION - THE IRARMCPO ROUTINE SCANS THE IN QUEUE FOR @Z40BPJC*/
*/*              THE USER, BELONGING TO A SPECIFIED DOMAIN,  @Z40BPJC*/
*/*              WITH THE LOWEST RECOMMENDATION VALUE.       @Z40BPJC*/
*/*              SUBROUTINE IRARMCVL IS CALLED TO COMPUTE    @Z40BPJC*/
*/*              A CURRENT SWAP RECOMMENDATION VALUE FOR     @Z40BPJC*/
*/*              EACH USER EVALUATED.                        @Z40BPJC*/
*/*                                                          @Z40BPJC*/
*/*   INPUT  - REGISTER DMDTPTR2 CONTAINS THE ADDRESS OF THE @Z40BPJC*/
*/*            DOMAIN TABLE ENTRY CORRESPONDING TO THE       @Z40BPJC*/
*/*            SPECIFIED DOMAIN.                             @Z40BPJC*/
*/*                                                          @Z40BPJC*/
*/*   OUTPUT - REGISTER 4(OUCBPTR) WILL CONTAIN THE ADDRESS  @Z40BPJC*/
*/*            OF THE OUCB HAVING THE LOWEST RECOMMENDATION  @Z40BPJC*/
*/*            VALUE IN THE SPECIFIED DOMAIN.                @Z40BPJC*/
*/*                                                          @Z40BPJC*/
*/*   EXIT NORMAL - BRANCH ON REGISTER 14.                   @Z40BPJC*/
*/*                 REGISTER 4 IS NON-ZERO.                  @Z40BPJC*/
*/*                                                          @Z40BPJC*/
*/*   EXIT ERROR - REGISTER 4 IS 0.                          @Z40BPJC*/
*/*                A DOMAIN CHANGE OCCURRED.                 @Z40BPJC*/
*/*                                                          @Z40BPJC*/
*/*                                                          @Z40BPJC*/
*/***********************************************************@Z40BPJC*/
*                                                               02S0503
*IRARMCPO:                                                      02S0503
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA,NOENTREG,RETREG,NOPARMREG);/*
*                                                            @Z40BPJC*/
*                                                               02S0503
IRARMCPO DS    0H                                               02S0504
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIAL PTR AND SWITCH SETTING                        @Z40BPJC*/
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S0504
*   WKRE=RTNREG;                    /* SAVE RETURN REGISTER  @Z40BPJC*/
         LR    WKRE,RTNREG                                      02S0504
*   RESPECIFY                                                   02S0505
*     CPICKPTR RSTD;                /*                       @Z40BPJC*/
*   CPICKPTR=0;                     /* INITIALIZE POINTER TO SELECTED
*                                      OUCB                  @Z40BPJC*/
         SLR   CPICKPTR,CPICKPTR                                02S0506
*   DMDTPTR2=DMDTPTR2-RMCTDMDT;     /* CONVERT TO OFFSET     @Z40BPJC*/
         SL    DMDTPTR2,RMCTDMDT(,RMCTPTR)                      02S0507
*   RMCTCPS1='0'B;                  /* SET DOMAIN CHANGE SWITCH OFF
*                                                            @Z40BPJC*/
*                                                               02S0508
         NI    RMCTCPS1(RMCTPTR),B'10111111'                    02S0508
*   /*****************************************************************/
*   /*                                                               */
*   /* FIND OUCBS ON IN QUEUE IN THIS DOMAIN                 @Z40BPJC*/
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S0509
*   OUCBPTR=RMCTINQE->OUCBFWD;      /* SCAN QUEUE FORWARD    @Z40BPJC*/
         L     @09,RMCTINQE(,RMCTPTR)                           02S0509
         L     OUCBPTR,OUCBFWD(,@09)                            02S0509
*   DO WHILE OUCBSCN='0'B;          /* ALL THE WAY TO THE END   02S0510
*                                                            @Z40BPJC*/
         B     @DE00510                                         02S0510
@DL00510 DS    0H                                               02S0511
*     IF DMDTPTR2=OUCBDMO&          /* IF OUCB IN THIS DOMAIN,       */
*         OUCBNSW='0'B&             /* USER SWAPPABLE,               */
*         OUCBBIB='0'B&             /* NOT BEING CANCELLED           */
*         OUCBENQ='0'B&             /* NOT ENQUEUED AND              */
*         OUCBINV='0'B&             /* VALID AND                     */
*         OUCBGOO='0'B&             /* NOT ON WAY OUT                */
*         OUCBGOB='0'B              /* OR TRANSITIONING              */
*       THEN                                                    02S0511
         CH    DMDTPTR2,OUCBDMO(,OUCBPTR)                       02S0511
         BNE   @RF00511                                         02S0511
         TM    OUCBNSW(OUCBPTR),B'10110010'                     02S0511
         BNZ   @RF00511                                         02S0511
         TM    OUCBGOO(OUCBPTR),B'10100000'                     02S0511
         BNZ   @RF00511                                         02S0511
*       DO;                         /*                       @Z40BPJC*/
*         IF(RRPATOD-OUCBTMA)>RMPTSAET×/* AND ENUF TIME              */
*             OUCBRMA='1'B THEN     /* AND RTB = 1,          @Z40BPJC*/
         L     @09,RRPATOD(,RRPAPTR)                            02S0513
         SL    @09,OUCBTMA(,OUCBPTR)                            02S0513
         CL    @09,RMPTSAET(,RMCTPTR)                           02S0513
         BH    @RT00513                                         02S0513
         TM    OUCBRMA(OUCBPTR),B'00100000'                     02S0513
         BNO   @RF00513                                         02S0513
@RT00513 DS    0H                                               02S0514
*           CALL IRARMCVL;          /* CALL CVL TO COMPUTE COMPOSITE
*                                                            @Z40BPJC*/
         BAL   @14,IRARMCVL                                     02S0514
*         ELSE                                                  02S0515
*           OUCBCMRV=OUCBWMR;       /* OR USE OLD WMR AS COMPOSITE
*                                      SINCE RTB = 0         @Z40BPJC*/
         B     @RC00513                                         02S0515
@RF00513 L     @09,OUCBWMR(,OUCBPTR)                            02S0515
         ST    @09,OUCBCMRV(,OUCBPTR)                           02S0515
*         IF RMCTCPS1='0'B THEN                                 02S0516
@RC00513 TM    RMCTCPS1(RMCTPTR),B'01000000'                    02S0516
         BNZ   @RF00516                                         02S0516
*           DO;                     /* NO PREVIOUS DOMAIN CHANGE
*                                                            @Z40BPJC*/
*             IF DMDTPTR2^=OUCBDMO THEN                         02S0518
         CH    DMDTPTR2,OUCBDMO(,OUCBPTR)                       02S0518
         BE    @RF00518                                         02S0518
*               DO;                 /* IF THIS OUCB'S DOMAIN CHANGED,
*                                                            @Z40BPJC*/
*                 RMCTCPS1='1'B;    /* SIGNAL TO QUIT SRCH FOR LOW RV
*                                                            @Z40BPJC*/
         OI    RMCTCPS1(RMCTPTR),B'01000000'                    02S0520
*                 CPICKPTR=0;       /* SET RETURN CODE TO SHOW NO
*                                      OUCB                  @Z40BPJC*/
*                                                               02S0521
         SLR   CPICKPTR,CPICKPTR                                02S0521
*                 /***************************************************/
*                 /*                                                 */
*                 /* ? IRACTLCL EPN(CAP)                             */
*                 /*                                                 */
*                 /***************************************************/
*                                                               02S0522
*                 DO;                                           02S0522
*                   R1=ADDR(RMEPBCAP);/* CAP REQUEST IS PARAM        */
         LA    R1,RMEPBCAP(,RMCTPTR)                            02S0523
*                   CALL RMSBCRL->IRARMRTN;/* ALGRTHM SCH FUNCT      */
         L     @15,RMSBCRL(,RMCTPTR)                            02S0524
         BALR  @14,@15                                          02S0524
*                 END;              /* ENSURE CAP REDRIVE SOON  02S0525
*                                                            @Z40BPJC*/
*               END;                /*                       @Z40BPJC*/
*             ELSE                                              02S0527
*               DO;                 /* NO DOMAIN CHG FOUND BY CVL
*                                                            @Z40BPJC*/
         B     @RC00518                                         02S0527
@RF00518 DS    0H                                               02S0528
*                 IF(CPICKPTR=0×OUCBCMRV<CPICKPTR->OUCBCMRV) THEN
         LTR   CPICKPTR,CPICKPTR                                02S0528
         BZ    @RT00528                                         02S0528
         L     @09,OUCBCMRV(,OUCBPTR)                           02S0528
         C     @09,OUCBCMRV(,CPICKPTR)                          02S0528
         BNL   @RF00528                                         02S0528
@RT00528 DS    0H                                               02S0529
*                   CPICKPTR=OUCBPTR;/* USE LOWER RV OF TWO. IF 02S0529
*                                      EQUAL, USE OLDER OUCB @Z40BPJC*/
         LR    CPICKPTR,OUCBPTR                                 02S0529
*               END;                /*                       @Z40BPJC*/
@RF00528 DS    0H                                               02S0531
*           END;                    /*                       @Z40BPJC*/
@RC00518 DS    0H                                               02S0532
*       END;                        /*                       @Z40BPJC*/
@RF00516 DS    0H                                               02S0533
*     OUCBPTR=OUCBFWD;              /* CONTINUE FORWARD SCAN @Z40BPJC*/
@RF00511 L     OUCBPTR,OUCBFWD(,OUCBPTR)                        02S0533
*   END;                            /*                       @Z40BPJC*/
@DE00510 TM    OUCBSCN(OUCBPTR),B'00000001'                     02S0534
         BZ    @DL00510                                         02S0534
*   OUCBPTR=CPICKPTR;               /* PASS BACK SELECTED OUCB  02S0535
*                                                            @Z40BPJC*/
         LR    OUCBPTR,CPICKPTR                                 02S0535
*   DMDTPTR2=DMDTPTR2+RMCTDMDT;     /* CONVERT BACK TO ADDR. @Z40BPJC*/
         AL    DMDTPTR2,RMCTDMDT(,RMCTPTR)                      02S0536
*   RTNREG=WKRE;                    /* RESTORE RETURN REGISTER  02S0537
*                                                            @Z40BPJC*/
         LR    RTNREG,WKRE                                      02S0537
*   RESPECIFY                                                   02S0538
*     CPICKPTR UNRSTD;              /*                       @Z40BPJC*/
*   END IRARMCPO;                   /*                       @Z40BPJC*/
@EL00006 DS    0H                                               02S0539
@EF00006 DS    0H                                               02S0539
@ER00006 BR    @14                                              02S0539
*                                                               02S0540
*/* *********************************************************@Z40BPJC*/
*/*                                                          @Z40BPJC*/
*/*   ROUTINE NAME - IRARMCVL                                @Z40BPJC*/
*/*                                                          @Z40BPJC*/
*/*   DESCRIPTIVE NAME - SYSTEM RESOURCES MANAGER (SRM)      @Z40BPJC*/
*/*                      USER SWAP EVALUATION ROUTINE        @Z40BPJC*/
*/*                                                          @Z40BPJC*/
*/*   FUNCTION - THE IRARMCVL ROUTINE COMPUTES A NUMERICAL   @Z40BPJC*/
*/*              VALUE REPRESENTING THE RECOMMENDATION OF A  @Z40BPJC*/
*/*              A USER TO BE SWAPPED INTO OR TO REMAIN      @Z40BPJC*/
*/*              IN CORE.  THE RECOMMENDATION VALUE IS THE   @Z40BPJC*/
*/*              COMPOSITE SUM OF THE USER'S NORMALIZED      @Z40BPJC*/
*/*              WORKLOAD LEVEL AND THE RECOMMENDATIONS      @Z40BPJC*/
*/*              OF THE I/O AND CPU RESOURCE MANAGERS.       @Z40BPJC*/
*/*                                                          @Z40BPJC*/
*/*   INPUT - REG 4 CONTAINS THE ADDRESS OF THE OUCB OF THE  @Z40BPJC*/
*/*           USER TO BE EVALUATED.                          @Z40BPJC*/
*/*         - REG 14 CONTAINS THE RETURN ADDRESS.            @Z40BPJC*/
*/*         - RMCTALU CONTAINS FLAGS REPRESENTING IMBALANCED @Z40BPJC*/
*/*           RESOURCES.                                     @Z40BPJC*/
*/*                                                          @Z40BPJC*/
*/*   OUTPUT - THE COMPOSITE RECOMMENDATION VALUE IS STORED  @Z40BPJC*/
*/*            IN -OUCBCMRV-.                                @Z40BPJC*/
*/*                                                          @Z40BPJC*/
*/*   EXIT - BRANCH ON REGISTER 14.                          @Z40BPJC*/
*/*          THERE IS NO ERROR EXIT OR INDICATION.           @Z40BPJC*/
*/*                                                          @Z40BPJC*/
*/*                                                          @Z40BPJC*/
*/* *********************************************************@Z40BPJC*/
*                                                               02S0540
*IRARMCVL:                                                      02S0540
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA,NOENTREG,RETREG,NOPARMREG);/*
*                                                            @Z40BPJC*/
IRARMCVL DS    0H                                               02S0541
*   RESPECIFY                                                   02S0541
*     CVRE RSTD;                    /*                       @Z40BPJC*/
*   PMRO=RTNREG;                    /* SAVE ACROSS CALL TO CL3/IL3
*                                                            @Z40BPJC*/
         LR    PMRO,RTNREG                                      02S0542
*   IF(RRPATOD-OUCBTMA>RMPTSAET) THEN                           02S0543
*                                                               02S0543
         L     @09,RRPATOD(,RRPAPTR)                            02S0543
         SL    @09,OUCBTMA(,OUCBPTR)                            02S0543
         CL    @09,RMPTSAET(,RMCTPTR)                           02S0543
         BNH   @RF00543                                         02S0543
*     /***************************************************************/
*     /*                                                             */
*     /* ? IRACTLCL EPN(WM3)                                         */
*     /*                                                             */
*     /***************************************************************/
*                                                               02S0544
*     CALL EPSTWM3->IRARMRTN;       /* INVOKE WM3 ACTION             */
*                                                               02S0544
         L     @15,EPSTWM3(,RMCTPTR)                            02S0544
         BALR  @14,@15                                          02S0544
*   /*****************************************************************/
*   /*                                                               */
*   /*                                                       @Z40BPJC*/
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S0545
*   CVRE=OUCBWMR;                   /* ACCUM. COMPOSITE EQUALS  02S0545
*                                      NORMALIZED LEVEL      @Z40BPJC*/
@RF00543 L     CVRE,OUCBWMR(,OUCBPTR)                           02S0545
*   IF(OUCBRMA='1'B) THEN                                       02S0546
         TM    OUCBRMA(OUCBPTR),B'00100000'                     02S0546
         BNO   @RF00546                                         02S0546
*     DO;                           /* BE SURE RTB = 1 FOR USER 02S0547
*                                                            @Z40BPJC*/
*       IF(CCTCPUOT='1'B)           /* IS CPU OUT-OF-BAL AND IS THIS */
*           &(OUCBCPL='1'B) THEN                                02S0548
         TM    CCTCPUOT(RMCTPTR),B'01000000'                    02S0548
         BNO   @RF00548                                         02S0548
         TM    OUCBCPL(OUCBPTR),B'00010000'                     02S0548
         BNO   @RF00548                                         02S0548
*         DO;                       /* A SIGNIFICNT USR      @Z40BPJC*/
*           IF(RRPATOD-OUCBTCP)>RMPTCPET THEN/* ENUF TIME PASSED     */
*                                                               02S0550
         L     @09,RRPATOD(,RRPAPTR)                            02S0550
         SL    @09,OUCBTCP(,OUCBPTR)                            02S0550
         CL    @09,RMPTCPET(,RMCTPTR)                           02S0550
         BNH   @RF00550                                         02S0550
*             /*******************************************************/
*             /*                                                     */
*             /* ? IRACTLCL EPN(CL3)                                 */
*             /*                                                     */
*             /*******************************************************/
*                                                               02S0551
*             CALL EPSTCL3->IRARMRTN;/* INVOKE CL3 ACTION            */
*                                                               02S0551
         L     @15,EPSTCL3(,RMCTPTR)                            02S0551
         BALR  @14,@15                                          02S0551
*           /*********************************************************/
*           /*                                                       */
*           /* INVOKE CL3 RTN                                @Z40BPJC*/
*           /*                                                       */
*           /*********************************************************/
*                                                               02S0552
*           CVRE=CVRE+OUCBCRV*RMPTCPU;/* ADD CPU RV          @Z40BPJC*/
@RF00550 L     @09,RMPTCPU(,RMCTPTR)                            02S0552
         MH    @09,OUCBCRV(,OUCBPTR)                            02S0552
         ALR   CVRE,@09                                         02S0552
*         END;                      /*                       @Z40BPJC*/
*       ELSE                        /* CPU IN BAL OR NOT A SIG USER
*                                                            @ZA26908*/
*         OUCBCRV=ZERO;             /* THEN REC VALUE = ZERO @ZA26908*/
         B     @RC00548                                         02S0554
@RF00548 SLR   @09,@09                                          02S0554
         STH   @09,OUCBCRV(,OUCBPTR)                            02S0554
*       IF ICTIOOT='1'B THEN                                    02S0555
@RC00548 TM    ICTIOOT(RMCTPTR),B'00100000'                     02S0555
         BNO   @RF00555                                         02S0555
*         DO;                       /* I/O OUT-OF-BAL        @Z40BPJC*/
*           IF(RRPATOD-OUCBTIO)>RMPTIOET THEN                   02S0557
*                                                               02S0557
         L     @09,RRPATOD(,RRPAPTR)                            02S0557
         SL    @09,OUCBTIO(,OUCBPTR)                            02S0557
         CL    @09,RMPTIOET(,RMCTPTR)                           02S0557
         BNH   @RF00557                                         02S0557
*             /*******************************************************/
*             /*                                                     */
*             /* ? IRACTLCL EPN(IL3)                                 */
*             /*                                                     */
*             /*******************************************************/
*                                                               02S0558
*             CALL EPSTIL3->IRARMRTN;/* INVOKE IL3 ACTION            */
*                                                               02S0558
         L     @15,EPSTIL3(,RMCTPTR)                            02S0558
         BALR  @14,@15                                          02S0558
*           /*********************************************************/
*           /*                                                       */
*           /* IF ENOUGH TIME SINCE LAST CALCULATION, INVOKE IL3 RTN */
*           /*                                               @Z40BPJC*/
*           /*                                                       */
*           /*********************************************************/
*                                                               02S0559
*           CVRE=CVRE+OUCBIRV*RMPTIOC;/* ADD I/O RV          @Z40BPJC*/
@RF00557 L     @09,RMPTIOC(,RMCTPTR)                            02S0559
         MH    @09,OUCBIRV(,OUCBPTR)                            02S0559
         ALR   CVRE,@09                                         02S0559
*         END;                      /*                       @Z40BPJC*/
*       ELSE                        /* I/O IN BAL            @ZA26908*/
*         OUCBIRV=ZERO;             /* THEN REC VALUE = ZERO @ZA26908*/
         B     @RC00555                                         02S0561
@RF00555 SLR   @09,@09                                          02S0561
         STH   @09,OUCBIRV(,OUCBPTR)                            02S0561
*     END;                          /*                       @Z40BPJC*/
@RC00555 DS    0H                                               02S0563
*   RTNREG=PMRO;                    /*                       @Z40BPJC*/
@RF00546 LR    RTNREG,PMRO                                      02S0563
*   OUCBCMRV=CVRE;                  /* SAVE ACCUM COMPOS R.V.   02S0564
*                                                            @Z40BPJC*/
         ST    CVRE,OUCBCMRV(,OUCBPTR)                          02S0564
*   RESPECIFY                                                   02S0565
*     CVRE UNRSTD;                  /*                       @Z40BPJC*/
*   END IRARMCVL;                                               02S0566
@EL00007 DS    0H                                               02S0566
@EF00007 DS    0H                                               02S0566
@ER00007 BR    @14                                              02S0566
*   END IRARMCTL                                                01S0567
