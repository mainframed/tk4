         TITLE 'IRARMCTL -- SYSTEM RESOURCES MANAGER CONTROL ALGORITHM *
                        '
IRARMCTL CSECT ,                                                01S0001
@PROLOG  BALR  @12,0                                            01S0001
@PSTART  DS    0H                                               01S0001
         USING @PSTART,@12                                      01S0001
*                                                               01S0029
*   /*****************************************************************/
*   /*                                                               */
*   /* RMCTPTR (REG 2) MUST BE SET UP BEFORE IRARMCTL IS ENTERED     */
*   /* RRPAPTR (REG 3) MUST BE SET UP BEFORE IRARMCTL IS ENTERED     */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0029
*   SVEREG=RMCTRMSA;                /* SUPPLY SAVE AREA STACK ADDRS  */
         L     SVEREG,RMCTRMSA(,RMCTPTR)                        01S0029
*   RRPAEPA=ADDR(RMEPBCTL);         /* INDICATE CONTROL ROUTINE RMEP */
         LA    @09,RMEPBCTL(,RMCTPTR)                           01S0030
         ST    @09,RRPAEPA(,RRPAPTR)                            01S0030
*                                                               02S0031
*/* **************************************************************** */
*/*                                                                  */
*/*    ROUTINE NAME - IRARMCEN                                       */
*/*                                                                  */
*/*    DESCRIPTIVE NAME - SYSTEM RESOURCES MANAGER CNTRL ALGORITHM   */
*/*                       DEFERRED USER ACTION PROCESSING ROUTINE    */
*/*                                                                  */
*/*    FUNCTION- THE IRARMCEN ROUTINE TESTS THE DEFERRED     @Z40BPCJ*/
*/*          ACTION QUEUE AT -RMCTAQHD-.  IF THE ACTION QUEUE IS     */
*/*          NOT EMPTY, IRARMCEN PICKS THE FIRST OUCB ON THE QUEUE,  */
*/*          AND ROUTES CONTROL TO THOSE ROUTINES WHOSE REQUEST      */
*/*          FLAGS HAVE BEEN SET IN THE -OUCBACN- FIELD.  WHEN       */
*/*          THERE ARE NO MORE ACTIONS TO BE PERFORMED, IRARMCEN     */
*/*          DEQUEUES THE OUCB FROM THE ACTION QUEUE, AND SETS ITS   */
*/*          -OUCBACT- FIELD TO ZERO TO INDICATE UNQUEUED STATUS.    */
*/*                                                                  */
*/*    NOTE  - WHILE IRARMCEN IS EXECUTING, OTHR CONCURRENT PROCSS   */
*/*            MAY INSERT, VIA THE IRARMCRN RTNE, ADDITIONL ACTION   */
*/*            REQUEST BITS INTO THE -OUCBACN- FIELD.                */
*/*                                                                  */
*/*    SETUP PERFORMED FOR EACH INVOKED ACTION PROCESSING ROUTINE    */
*/*          - REG 1 CONTAINS THE ADDRESS OF THE RMEP BLOCK FOR THE  */
*/*            ACTION ROUTINE BEING INVOKED.                         */
*/*          - REG 4 CONTAINS THE ADDRESS OF THE OUCB FOR WHOM THE   */
*/*            ACTION IS TO BE PERFORMED.                            */
*/*          - REG 13 CONTAINS THE ADDRESS OF THE SAVE AREA STACK    */
*/*            ASSOCIATED WITH THE SYSTEM RESOURCES MANAGER LOCK.    */
*/*          - REG 14 CONTAINS THE RETURN ADDRESS.                   */
*/*          - REG 15 CONTAINS THE ACTION RTNE ENTRY ADDRESS.        */
*/*                                                                  */
*/* **************************************************************** */
*                                                               02S0031
*   /*****************************************************************/
*   /*                                                               */
*   /* CAN NOW INVOKE ROUTINES WHICH WERE PREVIOUSLY DEFERRED. ANY   */
*   /* USERS FOR WHOM ACTIONS WERE DEFERRED HAVE BEEN QUEUED ONTO THE*/
*   /* ACTION QUEUE. THE FOLLOWING CODE IS EXECUTED AS LONG AS THE   */
*   /* ACTION QUEUE IS NOT EMPTY. EMPTY IS INDICATED WHEN THE        */
*   /* REMAINING MEMBER IS THE DUMMY PREASSEMBLED OUCB.              */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S0031
*   DO WHILE(RMCTAQHD^=RMCTOUCB);                               02S0031
         B     @DE00031                                         02S0031
@DL00031 DS    0H                                               02S0032
*RMCENL1:                           /* RETRY PICK 1ST OUCB IF QUEUE
*                                      CHANGED BY OTHER PROCESS      */
*     OUCBPTR=RMCTAQHD;             /* PICK TOP OUCB ON ACTION QUE   */
*                                                               02S0032
RMCENL1  L     OUCBPTR,RMCTAQHD(,RMCTPTR)                       02S0032
*     /***************************************************************/
*     /*                                                             */
*     /* DEQUEUE THE OUCB FROM THE ACTION QUEUE, AS ALL REQSTD       */
*     /* ACTIONS WILL HAVE BEEN PERFORMED BY THE TIME THAT THE       */
*     /* IRARMCEN ROUTINE STEPS AWAY FROM THIS OUCB. A ZERO IS       */
*     /* INSERTED INTO THE -OUCBACT- FIELD TO INDICATE THAT THIS USER*/
*     /* IS NO LONGER QUEUED UP FOR ACTION PROCESSING.               */
*     /*                                                             */
*     /***************************************************************/
*                                                               02S0033
*     R0=ZERO;                      /* ZERO - TO MARK OUCB NOT QUED  */
         SLR   R0,R0                                            02S0033
*     SVRE=RMCTAQCT;                /* PICK PREV ACTION QUEUE COUNT  */
         L     SVRE,RMCTAQCT(,RMCTPTR)                          02S0034
*     SVRO=OUCBPTR;                 /* INSURE OUCB STILL QUED AT TOP */
         LR    SVRO,OUCBPTR                                     02S0035
*     WKRE=SVRE;                    /* KEEP PREV ACTION QUEUE COUNT  */
         LR    WKRE,SVRE                                        02S0036
*     WKRO=OUCBACT;                 /* PICK NEW TOP ACTION QUE OUCB  */
         L     WKRO,OUCBACT(,OUCBPTR)                           02S0037
*     CDS(SVRE,WKRE,RMCTACQH);      /* STORE UPDATED QUEUE HEADER    */
         CDS   SVRE,WKRE,RMCTACQH(RMCTPTR)                      02S0038
*     BC(7,RMCENL1);                /* REPEAT IF OUCB WAS NOT AT TOP */
         BC    7,RMCENL1                                        02S0039
*     OUCBACT=R0;                   /* MARK OUCB AS NOT ON QUEUE     */
         ST    R0,OUCBACT(,OUCBPTR)                             02S0040
*     WKRE=OUCBCSW&ACTNDEF;         /* PICK DEFERRED ACTION REQ FLGS */
         L     WKRE,OUCBCSW(,OUCBPTR)                           02S0041
         N     WKRE,ACTNDEF                                     02S0041
*     IF WKRE^=EMPTY THEN           /* ACTIONS HAVE BEEN DEFERRED    */
*                                                               02S0042
         LTR   WKRE,WKRE                                        02S0042
         BZ    @RF00042                                         02S0042
*       /*************************************************************/
*       /*                                                           */
*       /* INVOKE THE IRARMCRT SUBROUTINE, PASSING IT THE ADDRESS OF */
*       /* THE DEFERRABLE ACTION ROUTINE TABLE. IRARMCRT WILL ROUTE  */
*       /* CONTROL TO ALL OF THE ENTRY POINTS WHOSE FLAGS HAVE BEEN  */
*       /* SET IN THE -OUCBACN- FIELD, AND WILL TURN OFF EACH        */
*       /* -OUCBACN- FLAG WHEN THE CORRESPONDING ENTRY POINT ROUTINE */
*       /* HAS COMPLETED ITS PROCESSING.                             */
*       /*                                                           */
*       /*************************************************************/
*                                                               02S0043
*       DO;                                                     02S0043
*         RMCAINV=ACTNDEF;          /* INDICATE THE ACTN REQST FLAGS */
         MVC   RMCAINV(4,RMCTPTR),ACTNDEF                       02S0044
*         PMRE=ADDR(OUCBACN);       /* INDICATE THE RTNE REQST ACCUM */
         LA    PMRE,OUCBACN(,OUCBPTR)                           02S0045
*         RMEPPTR=RMCTEPDT;         /* INDICATE ACTION TABLE TO SCAN */
         L     RMEPPTR,RMCTEPDT(,RMCTPTR)                       02S0046
*         CALL IRARMCRT;            /* SCAN AND PERFORM ACTION RTNES */
         BAL   @14,IRARMCRT                                     02S0047
*       END;                                                    02S0048
*   END;                                                        02S0049
@RF00042 DS    0H                                               02S0049
@DE00031 CLC   RMCTAQHD(4,RMCTPTR),RMCTOUCB(RMCTPTR)            02S0049
         BNE   @DL00031                                         02S0049
*                                                               02S0050
*/* **************************************************************** */
*/*                                                                  */
*/*    ROUTINE NAME - IRARMCEL                                       */
*/*                                                                  */
*/*    DESCRIPTIVE NAME - SYSTEM RESOURCES MANAGER CNTRL ALGORITHM   */
*/*          INDIVIDUALLY REQUESTED ALGORITHM CONTROL ROUTING RTNE   */
*/*                                                                  */
*/*    FUNCTION - THE IRARMCEL ROUTINE GATHERS ALGORITHM EXECUTION   */
*/*          REQUESTS, WHICH HAVE BEEN STORED AS BIT PATTERNS IN     */
*/*          -RMCTALR- BY SYSTEM RESOURCES MANAGER LOCKED RTNES,     */
*/*          AND IN -RMCTALA- BY OUTSIDE-LOCK ROUTINES.      @Z40BPCJ*/
*/*          ALL REQUESTS ARE PUT IN -RMCTALR-.  CONTROL IS  @Z40BPCJ*/
*/*          THEN ROUTED, VIA THE IRARMCRT SBRTNE, TO ALGORITHMS     */
*/*          REQUESTED IN -RMCTALR-.  AS EACH REQUESTED ALGORITHM    */
*/*          COMPLETES, ITS REQUEST BIT IS TURNED OFF IN -RMCTALR-.  */
*/*          WHEN CONTROL ROUTING IS COMPLETE, ANY REMAINING RE-     */
*/*          QUESTS ARE LEFT FOR THE NEXT TIME IRARMCEL IS RUN.      */
*/*                                                                  */
*/*    NOTE  - WHILE IRARMCEL IS EXECUTING, OTHR CONCURRENT PROCSS   */
*/*            MAY INSERT, VIA THE IRARMCRL ROUTINE, ADDITIONL ALG   */
*/*            REQUEST BITS INTO THE -RMCTALA- FIELD.                */
*/*                                                                  */
*/*    SETUP PERFORMED FOR EACH INVOKED ALGORITHM ROUTINE            */
*/*          - REG 1 CONTAINS THE ADDRESS OF THE RMEP BLOCK FOR THE  */
*/*            ALGORITHM BEING INVOKED.                              */
*/*          - REG 13 CONTAINS THE ADDRESS OF THE SAVE AREA STACK    */
*/*            ASSOCIATED WITH THE SYSTEM RESOURCES MANAGER LOCK.    */
*/*          - REG 14 CONTAINS THE RETURN ADDRESS.                   */
*/*          - REG 15 CONTAINS THE ALGORITHM RTNE ENTRY ADDRESS      */
*/*                                                                  */
*/* **************************************************************** */
*                                                               02S0050
*   /*****************************************************************/
*   /*                                                               */
*   /* TEST IF ANY ALGORITHMS HAVE BEEN INDIVIDUALLY REQUESTED       */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S0050
*   R0=EMPTY;                       /* ZERO - TO TEST + CLEAR ACCUMS */
         SLR   R0,R0                                            02S0050
*   WKRE=RMCTALA;                   /* PICK ALG REQUEST ACCUM FIELD  */
         L     WKRE,RMCTALA(,RMCTPTR)                           02S0051
*   WKRO=WKRE×RMCTALR;              /* ADD IMMED ALG REQUEST FIELD   */
         LR    WKRO,WKRE                                        02S0052
         O     WKRO,RMCTALR(,RMCTPTR)                           02S0052
*   IF WKRO^=R0 THEN                /* HAVE ALGORITHM REQUEST        */
*                                                               02S0053
         CLR   WKRO,R0                                          02S0053
         BE    @RF00053                                         02S0053
*     /***************************************************************/
*     /*                                                             */
*     /* THE FOLLOWING INSTRUCTIONS ARE EXECUTED ONLY IF AT LEAST ONE*/
*     /* REQUEST FOR ALGORITHM EXECUTION IS PENDING.                 */
*     /*                                                             */
*     /***************************************************************/
*                                                               02S0054
*     DO;                                                       02S0054
*       IF WKRE^=R0 THEN            /* RMCTALA FIELD NON-ZERO        */
*                                                               02S0055
         CLR   WKRE,R0                                          02S0055
         BE    @RF00055                                         02S0055
*         /***********************************************************/
*         /*                                                         */
*         /* MOVE REQUEST BITS OUT OF OUTSIDE-LOCK REQUEST ACCUMULATR*/
*         /*                                                         */
*         /***********************************************************/
*                                                               02S0056
*         DO;                                                   02S0056
*RMCELL1:                           /* POINT WHERE MOVE RMCTALA BITS
*                                      INTO RMCTALR                  */
*           RMCTALR=WKRE×RMCTALR;   /* GATHER ALL ALG REQUEST BITS   */
RMCELL1  LR    @09,WKRE                                         02S0057
         O     @09,RMCTALR(,RMCTPTR)                            02S0057
         ST    @09,RMCTALR(,RMCTPTR)                            02S0057
*           CS(WKRE,R0,RMCTALAW);   /* CLEAR OUTSIDE-LOCK ACCUMULATR */
         CS    WKRE,@00,RMCTALAW(RMCTPTR)                       02S0058
*           BC(7,RMCELL1);          /* REPEAT IF REQUEST JUST ADDED  */
         BC    7,RMCELL1                                        02S0059
*         END;                                                  02S0060
*                                                               02S0060
*       /*************************************************************/
*       /*                                                           */
*       /* INVOKE THE IRARMCRT SUBROUTINE, PASSING IT THE ADDRESS OF */
*       /* THE INDIVIDUALLY REQUESTABLE ALGORITHMS TABLE. IRARMCRT   */
*       /* WILL ROUTE CONTROL TO THE ALGS REQUESTED IN -RMCTALR-. AS */
*       /* EACH ALGORITHM COMPLETES ITS EXECUTION, IRARMCRT WILL TURN*/
*       /* OFF ITS REQUEST BIT IN -RMCTALR-.                         */
*       /*                                                           */
*       /*************************************************************/
*                                                               02S0061
*       RMCAINV=FLAGDEF;            /* PASS THE ALG REQST FLGS MASK  */
@RF00055 MVC   RMCAINV(4,RMCTPTR),FLAGDEF                       02S0061
*       PMRE=ADDR(RMCTALR);         /* PASS THE ALG REQUEST ACCUML   */
         LA    PMRE,RMCTALR(,RMCTPTR)                           02S0062
*       RMEPPTR=RMCTEPAT;           /* PASS ALGORITHM TABLE TO SCAN  */
         L     RMEPPTR,RMCTEPAT(,RMCTPTR)                       02S0063
*       CALL IRARMCRT;              /* SCAN AND PERFORM ALGORITHMS   */
         BAL   @14,IRARMCRT                                     02S0064
*     END;                                                      02S0065
*                                                               01S0066
*   /*****************************************************************/
*   /*                                                               */
*   /* RETURN TO SYSTEM RESOURCES MANAGER INTERFACE PROGRAM ?        */
*   /* IRACTLCL EPN(I01)                                             */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0066
*   GOTO RMEXI01->IRARMNTR;         /* EXIT TO I01 FUNCT             */
@RF00053 L     @09,RMEXI01(,RMCTPTR)                            01S0066
         BR    @09                                              01S0066
*                                                               02S0067
*/* **************************************************************** */
*/*                                                                  */
*/*    ROUTINE NAME - IRARMCET                                       */
*/*                                                                  */
*/*    DESCRIPTIVE NAME - SYSTEM RESOURCES MANAGER CNTRL ALGORITHM   */
*/*          PERIODIC ENTRY POINT SCHEDULING ROUTINE                 */
*/*                                                                  */
*/*    FUNCTION - THE IRARMCET ROUTINE ACCEPTS TIMER INTERRUPTS,     */
*/*          SETS UP REQUESTS FOR ALL THOSE PERIODICALLY SCHEDU-     */
*/*          LED SYSTEM RESOURCES MANAGER ALGORITHMIC PROCESSING     */
*/*          ROUTINES WHICH ARE THEN DUE, AND RESETS THE SYSTEM      */
*/*          RESOURCES MANAGER TIMER ELEMENT SO AS TO INTERRUPT      */
*/*          AGAIN WHEN IRARMCET WILL BE NEXT REQUIRED.              */
*/*                                                                  */
*/*    INPUT - THE TIME-DRIVEN ALGORITHM QUEUE IS ORDERED BY THE     */
*/*            RMEPTME VALUES OF THE BLOCKS ON THE QUEUE.            */
*/*          - RMCTTOD CONTAINS THE CURRENT TIME.                    */
*/*          - RMPTTOL CONTAINS THE ALLOWED SCHEDULING TOLERANCE.    */
*/*          - RMPTTOM CONTAINS THE REQUIRED MINIMUM SCHD PERIOD.    */
*/*          - THE RMEPINT FIELDS OF THE BLCKS ON THE TIME-DRIVEN    */
*/*            ALG QUE INDICATE THE SCHED INTERVALS FOR THE ALGS.    */
*/*                                                                  */
*/*    OUTPT - RMCTALA CONTAINS THE REQUEST FLAGS FOR ALL THOSE      */
*/*            TIME-DRIVEN ALGORITHMS SCHEDULED TO BE EXECUTED       */
*/*            AT THE PRESENT TIME.                                  */
*/*          - RMPTTOI CONTAINS THE TIME OF EXPIRATION OF THE        */
*/*            NEXT SYSTEM RESOURCES MANAGER SCHEDULING INTERVAL.    */
*/*                                                                  */
*/* **************************************************************** */
*                                                               02S0067
*   DCL                                                         02S0067
*     IRARMCET LABEL EXTERNAL;                                  02S0067
*IRARMCET:                          /* PROCESS SYSTEM RESOURCES 02S0068
*                                      MANAGER TIMER INTERRUPT       */
*   GEN CODE;                                                   02S0068
IRARMCET DS    0H                                               02S0068
*                                  /*
IRARMXTL DS    0H
         ENTRY IRARMXTL
         BALR  @15,0                   SETUP ENTRY ADDRESSABILITY
         LA    @12,*-@PSTART           DISPLACEMENT TO MODULE BASE
         LCR   @12,@12                 NEGATIVE FOR SUBTRACT
         ALR   @12,@15                 SETUP MODULE ADDRESSABILITY
*   SVEREG=RMCTRMSA;                /* SUPPLY SAVE AREA STACK ADDRS  */
         L     SVEREG,RMCTRMSA(,RMCTPTR)                        02S0069
*   RRPAEPA=ADDR(RMEPBCTL);         /* INDICATE CONTROL ROUTINE RMEP */
         LA    @09,RMEPBCTL(,RMCTPTR)                           02S0070
         ST    @09,RRPAEPA(,RRPAPTR)                            02S0070
*   RRPACTL='0'B;                   /* REVERT TO NORMAL FAIL PROCESS */
         NI    RRPACTL(RRPAPTR),B'11110111'                     02S0071
*   RMEPPTR=RMCTTMQE->RMEPFWD;      /* PICK 1ST RMEP ON TMEDRVN QUE  */
         L     @09,RMCTTMQE(,RMCTPTR)                           02S0072
         L     RMEPPTR,RMEPFWD(,@09)                            02S0072
*   PMRE=RMEPINT;                   /* DEFAULT TME OF NXT INTERRUPT  */
         L     PMRE,RMEPINT(,RMEPPTR)                           02S0073
*   IF RRPARFI='0'B THEN            /* NOT MULTIPLE ERROR RECURSION  */
*                                                               02S0074
         TM    RRPARFI(RRPAPTR),B'00000010'                     02S0074
         BNZ   @RF00074                                         02S0074
*     /***************************************************************/
*     /*                                                             */
*     /* REQUEST EACH TIME-DRIVEN QUE RTNE NOT SCHEDULED FOR LATER   */
*     /*                                                             */
*     /***************************************************************/
*                                                               02S0075
*     DO WHILE(RRPATOD+RMPTTOL^<RMEPTME);                       02S0075
         B     @DE00075                                         02S0075
@DL00075 DS    0H                                               02S0076
*       WKRO=RMCTALR;               /* PICK IMMED ALG REQUEST FIELD  */
         L     WKRO,RMCTALR(,RMCTPTR)                           02S0076
*       WKRO=RMEPFLG×WKRO;          /* INSERT FLAG FOR REQUESTD ALG  */
         O     WKRO,RMEPFLG(,RMEPPTR)                           02S0077
*       WKRO=WKRO&FLAGDEF;          /* CLR RTNE CHARACTERISTIC FLGS  */
         N     WKRO,FLAGDEF                                     02S0078
*       RMCTALR=WKRO;               /* STORE UPDATED ALG REQST FLD   */
         ST    WKRO,RMCTALR(,RMCTPTR)                           02S0079
*       IF PMRE>RMEPINT THEN        /* RTNE RESCHED INTERVL SMALLER  */
         L     @09,RMEPINT(,RMEPPTR)                            02S0080
         CLR   PMRE,@09                                         02S0080
         BNH   @RF00080                                         02S0080
*         PMRE=RMEPINT;             /* SET NEW TME OF NXT INTERRUPT  */
         LR    PMRE,@09                                         02S0081
*       RMEPPTR=RMEPFWD;            /* STEP TO NXT TMEDRVN QUE RMEP  */
@RF00080 L     RMEPPTR,RMEPFWD(,RMEPPTR)                        02S0082
*     END;                                                      02S0083
@DE00075 L     @09,RRPATOD(,RRPAPTR)                            02S0083
         AL    @09,RMPTTOL(,RMCTPTR)                            02S0083
         CL    @09,RMEPTME(,RMEPPTR)                            02S0083
         BNL   @DL00075                                         02S0083
*   PMRE=MIN(PMRE,RMEPTME-RRPATOD); /* CONSIDER NEXT RTNE SCHEDL     */
@RF00074 L     @09,RMEPTME(,RMEPPTR)                            02S0084
         SL    @09,RRPATOD(,RRPAPTR)                            02S0084
         CLR   PMRE,@09                                         02S0084
         BNH   *+6
         LR    PMRE,@09                                         02S0084
*   R1=RMCTTOD+MAX(RMPTTOM,PMRE);   /* PASS NXT TIME OF EXPIRATN     */
*                                                               02S0085
         L     R1,RMPTTOM(,RMCTPTR)                             02S0085
         CLR   R1,PMRE                                          02S0085
         BNL   *+6
         LR    R1,PMRE                                          02S0085
         AL    R1,RMCTTOD(,RMCTPTR)                             02S0085
*   /*****************************************************************/
*   /*                                                               */
*   /* ? IRACTLCL EPN(I05)                                           */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S0086
*   CALL RMSBI05->IRARMRTN;         /* INVOKE I05 SUBRTN             */
         L     @15,RMSBI05(,RMCTPTR)                            02S0086
         BALR  @14,@15                                          02S0086
*   IF RTNCOD=ZERO THEN             /* TIMER INTERVAL SET SUCCESSFUL */
         LTR   RTNCOD,RTNCOD                                    02S0087
         BNZ   @RF00087                                         02S0087
*     RMCATOI=R1;                   /* SAVE NEW TIMER EXPIRE SETTING */
*                                                               02S0088
         ST    R1,RMCATOI(,RMCTPTR)                             02S0088
*   /*****************************************************************/
*   /*                                                               */
*   /* ? IRACTLCL EPN(CTL)                                           */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S0089
*   GOTO RMEXCTL->IRARMNTR;         /* EXIT TO CTL FUNCT             */
@RF00087 L     @09,RMEXCTL(,RMCTPTR)                            02S0089
         BR    @09                                              02S0089
*                                                               02S0090
*/* **************************************************************** */
*/*                                                                  */
*/*    ROUTINE NAME - IRARMCED                                       */
*/*                                                                  */
*/*    DESCRIPTIVE NAME - SYSTEM RESOURCES MANAGER CNTRL ALGORITHM   */
*/*          SRB DISPATCHED ORIGINAL ENTRY PROCESSING ROUTINE        */
*/*                                                                  */
*/*    FUNCTION - THE IRARMCED ROUTINE RECEIVES CONTROL UNDER AN     */
*/*          SRB SCHEDULED BY THE DISPATCHER.  IRARMCED SETS UP AN   */
*/*          ENTRY TO THE MAINLINE OF THE SYSTEM RESOURCES MANAGER   */
*/*          BY LINKING TO THE SRM AS SYSEVENT 48.  THIS SYSEVENT    */
*/*          IS INTERPRETED BY THE SRM ROUTINES AS A REQUEST THAT    */
*/*          IRARMCTL BE RE-ENTERED, ONCE THE SRM LOCK AND THE SRM   */
*/*          FRR HAVE BEEN ACQUIRED.  ONE SRB MAKES USE OF THIS      */
*/*                                                          @Z40BPCJ*/
*/*          FACILITY.  IT IS SCHEDULED BY ROUTINES NOT HOLDING      */
*/*                                                          @Z40BPCJ*/
*/*          THE SRM LOCK, IN ORDER THAT DEPENDENT ROUTINES, WHICH   */
*/*          DO REQUIRE THE LOCK, MAY BE RUN (BY THE SRB) WITHOUT    */
*/*          DELAY.                                          @Z40BPCJ*/
*/*                                                                  */
*/*    EXIT  - BRANCH ON REGISTER 14 BACK TO THE DISPATCHER.         */
*/*          - THERE IS NO ERROR EXIT OR INDICATION.                 */
*/*                                                                  */
*/* **************************************************************** */
*                                                               02S0090
*   DCL                                                         02S0090
*     IRARMCED LABEL EXTERNAL;                                  02S0090
*IRARMCED:                          /* SYSTEM RESOURCES MANAGER SRB
*                                      PROCESSING ENTRY POINT        */
*   R1=R0;                          /* SAVE SRB ADDRESS AS PARAMETR  */
IRARMCED LR    R1,R0                                            02S0091
*   RMCTPTR=CVTOPCTP;               /* ADDR SYSTEM RSRCS MNGR TABLE  */
         L     @09,CVTPTR                                       02S0092
         L     RMCTPTR,CVTOPCTP(,@09)                           02S0092
*   SVEREG=RTNREG;                  /* PASS REG 14 CONTENT IN REG 13 */
         LR    SVEREG,RTNREG                                    02S0093
*   R0=E48;                         /* SRB ENTRY SYSEVENT CODE       */
         LA    R0,48                                            02S0094
*   GOTO RMEXI48->IRARMNTR;         /* OBTAIN SRM LOCK, RRPA BLOCK   */
         L     @09,RMEXI48(,RMCTPTR)                            02S0095
         BR    @09                                              02S0095
*                                                               02S0096
*/* **************************************************************** */
*/*                                                                  */
*/*    ROUTINE NAME - IRARMCQT                                       */
*/*                                                                  */
*/*    DESCRIPTIVE NAME - SYSTEM RESOURCES MANAGER CNTRL ALGORITHM   */
*/*          PERIODICALLY INVOKED ENTRY POINT RESCHEDULING SBRTNE    */
*/*                                                                  */
*/*    FUNCTION - THE IRARMCQT SUBROUTINE ACCEPTS A REQUEST TO       */
*/*          RESCHEDULE THE EXECUTION OF A PERIODICALLY INVOKED      */
*/*          SYSTEM RESOURCES MANAGER ENTRY POINT.  IT ADDS THE      */
*/*          INVOCATION INTERVAL TO THE TIME THE ENTRY WAS LAST      */
*/*          SCHEDULED, AND REPOSITIONS THE ROUTINE SCHEDULING       */
*/*          BLOCK ON THE SYSTEM RESOURCES MANAGER ENTRY TIMING      */
*/*          QUEUE ACCORDING TO WHEN IT IS NEXT DUE.                 */
*/*                                                                  */
*/*    INPUT - REG 1 CONTAINS THE ADDRESS OF THE RMEP BLOCK TO       */
*/*            BE PLACED ON THE ENTRY TIMING QUEUE.  THE RMEP        */
*/*            MUST HAVE BEEN DEQUED PRIOR TO USING IRARMCQT.        */
*/*          - REG 14 CONTAINS THE RETURN ADDRESS.                   */
*/*          - RMEPTME CONTAINS THE STARTING TIME WITH RESPECT       */
*/*            TO WHICH THE ENTRY POINT WILL BE RESCHEDULED.         */
*/*          - RMEPINT CONTAINS THE INTERVAL VALUE TO BE ADDED       */
*/*            TO THE STARTING TIME.                                 */
*/*                                                                  */
*/*    OUTPT - THE INTERVAL VALUE IS ADDED TO THE RMEPTME FIELD.     */
*/*          - THE RMEP IS MOVED TO THE PROPER POSITION ON THE       */
*/*            ENTRY TIMING QUEUE.                                   */
*/*                                                                  */
*/*    EXIT  - BRANCH ON REGISTER 14.                                */
*/*          - THERE IS NO ERROR EXIT OR INDICATION.                 */
*/*                                                                  */
*/* **************************************************************** */
*                                                               02S0096
*IRARMCQT:                                                      02S0096
*   PROC OPTIONS(NOSAVE,NOSAVEAREA,NOENTREG,RETREG,NOPARMREG);  02S0096
IRARMCQT DS    0H                                               02S0097
*   RMEPTME=RRPATOD+MAX(RMPTTOM,RMEPINT);/* CALC POSITN VALUE        */
         L     @09,RMPTTOM(,RMCTPTR)                            02S0097
         L     @08,RMEPINT(,RMEPPTR)                            02S0097
         CLR   @09,@08                                          02S0097
         BNL   *+6
         LR    @09,@08                                          02S0097
         AL    @09,RRPATOD(,RRPAPTR)                            02S0097
         ST    @09,RMEPTME(,RMEPPTR)                            02S0097
*   TMRE=RMCTTMQE->RMEPFWD;         /* PICK 1ST TIMED QUEUE RMEP     */
*                                                               02S0098
         L     @09,RMCTTMQE(,RMCTPTR)                           02S0098
         L     TMRE,RMEPFWD(,@09)                               02S0098
*   /*****************************************************************/
*   /*                                                               */
*   /* SEARCH FOR POSITION INTO WHICH TO INSERT THE GIVEN RMEP       */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S0099
*   RESPECIFY                                                   02S0099
*    (RMEP) BASED(TMRE);            /* TME REG USED TO SCAN MEMBERS  */
*                                                               02S0099
*   /*****************************************************************/
*   /*                                                               */
*   /* THE END OF THE Q IS MARKED WITH THE HIGHEST POSSIBLE VALUE    */
*   /* STEP UNTIL FIND QUEUE MEMBER WITH A LARGER TIME VALUE         */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S0100
*   DO WHILE(RMEPPTR->RMEPTME^<RMEPTME);                        02S0100
         B     @DE00100                                         02S0100
@DL00100 DS    0H                                               02S0101
*     TMRE=RMEPFWD;                 /* STEP TO NEXT MEMBER ON QUE    */
         L     TMRE,RMEPFWD(,TMRE)                              02S0101
*   END;                                                        02S0102
*                                                               02S0102
@DE00100 CLC   RMEPTME(4,RMEPPTR),RMEPTME(TMRE)                 02S0102
         BNL   @DL00100                                         02S0102
*   /*****************************************************************/
*   /*                                                               */
*   /* ENQUEUE RMEP AHEAD OF CURRENT EXISTING QUEUE MEMBER           */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S0103
*   RMEPPTR->RMEPFWD=ADDR(RMEP);    /* POINT RMEP TO FWD CHAIN       */
         ST    TMRE,RMEPFWD(,RMEPPTR)                           02S0103
*   RMEPPTR->RMEPBCK=RMEPBCK;       /* POINT RMEP TO BCKWD CHAIN     */
         L     @09,RMEPBCK(,TMRE)                               02S0104
         ST    @09,RMEPBCK(,RMEPPTR)                            02S0104
*   RMEPBCK->RMEPFWD=RMEPPTR;       /* ENQUE RMEP ONTO FORWD CHAIN   */
         ST    RMEPPTR,RMEPFWD(,@09)                            02S0105
*   RMEPBCK=RMEPPTR;                /* ENQUE RMEP ONTO BCKWD CHAIN   */
         ST    RMEPPTR,RMEPBCK(,TMRE)                           02S0106
*   RESPECIFY                                                   02S0107
*    (RMEP) BASED(RMEPPTR);         /* RESTORE NORMAL RMEP BASE      */
*   END IRARMCQT;                                               02S0108
@EL00002 DS    0H                                               02S0108
@EF00002 DS    0H                                               02S0108
@ER00002 BR    @14                                              02S0108
*                                                               02S0109
*/* **************************************************************** */
*/*                                                                  */
*/*    ROUTINE NAME - IRARMCRD                                       */
*/*                                                                  */
*/*    DESCRIPTIVE NAME - SYSTEM RESOURCES MANAGER CNTRL ALGORITHM   */
*/*          SRB SCHEDULING REQUEST RECEIVING SUBROUTINE             */
*/*                                                                  */
*/*    FUNCTION - THE IRARMCRD SUBROUTINE ACCEPTS A REQUEST  @Z40BPCJ*/
*/*          TO SCHEDULE THE SRM SRB TO OBTAIN THE SRM LOCK. @Z40BPCJ*/
*/*          IF THE SRB IS                                   @Z40BPCJ*/
*/*          NOT CURRENTLY IN USE, IRARMCRD ISSUES A 'SCHEDULE' IN   */
*/*          ORDER TO BRING ABOUT THE DISPATCHING OF THAT SRB.       */
*/*                                                                  */
*/*    NOTE  - IRARMCRD DOES NOT REQUIRE THE OPT LOCK TO BE HELD.    */
*/*                                                                  */
*/*    INPUT - REG 1 CONTAINS THE ADDRESS OF THE RMEP BLOCK FOR      */
*/*            THE RTNE ON WHOSE BEHALF THE REQUEST IS ISSUED.       */
*/*          - REG 14 CONTAINS THE RETURN ADDRESS.                   */
*/*          - REG 15 CONTAINS THE IRARMCRD ROUTINE ADDRESS.         */
*/*          - SRBPARM INDICATES WHETHER THE SRB IS AVAILABLE.       */
*/*                                                                  */
*/*    OUTPT - SEE FUNCTION.                                         */
*/*                                                                  */
*/*    EXIT  - BRANCH ON REGISTER 14.                                */
*/*          - THERE IS NO ERROR EXIT OR INDICATION.                 */
*/*                                                                  */
*/* **************************************************************** */
*                                                               02S0109
*IRARMCRD:                                                      02S0109
*   PROC OPTIONS(NOSAVE,NOSAVEAREA,NOENTREG,RETREG,NOPARMREG);  02S0109
IRARMCRD DS    0H                                               02S0110
*   PMRE=RMEPPTR;                   /* SAVE INPUT RTNE BLOCK ADDR    */
         LR    PMRE,RMEPPTR                                     02S0110
*   SRBPTR=RMCTSRBT;                /* PICK LOCK OBTAIN SRB ADDR
*                                                            @Z40BPCJ*/
         L     SRBPTR,RMCTSRBT(,RMCTPTR)                        02S0111
*   WKRE=ZERO;                      /* INDICATOR FOR AVAILABLE SRB   */
         SLR   WKRE,WKRE                                        02S0112
*   DO;                                                         02S0113
*     WKRO=MAX(RRPATOD,EIGHT);      /* PICK NONZERO PARAMETER VALUE  */
         L     WKRO,RRPATOD(,RRPAPTR)                           02S0114
         LA    @09,8                                            02S0114
         CLR   WKRO,@09                                         02S0114
         BNL   *+6
         LR    WKRO,@09                                         02S0114
*     CS(WKRE,WKRO,SRBPARM);        /* STORE VLU AS USAGE INDICATOR  */
         CS    WKRE,@11,SRBPARM(SRBPTR)                         02S0115
*     BC(7,RMCRDX1);                /* BYPASS SCHED IF STORE FAILED  */
         BC    7,RMCRDX1                                        02S0116
*     GEN CODE REFS(CVTGSMQ,SRBFLNK,SRBSECT);                   02S0117
*                                             /*
         SCHEDULE  SRB=(1),SCOPE=GLOBAL
*     IF RRPASVC='1'B&              /* SRM INVOKED VIA TYPE-1 SVC    */
*         RRPACOD^=E48 THEN         /* NOT INVOKED FROM SRB ENTRY    */
         TM    RRPASVC(RRPAPTR),B'00100000'                     02S0118
         BNO   @RF00118                                         02S0118
         CLI   RRPACOD(RRPAPTR),48                              02S0118
         BE    @RF00118                                         02S0118
*       RRPARTN=CVTEXP1;            /* FORCE EXIT THRU DISPATCHER    */
         L     @09,CVTPTR                                       02S0119
         L     @09,CVTEXP1(,@09)                                02S0119
         ST    @09,RRPARTN(,RRPAPTR)                            02S0119
*RMCRDX1:                           /* BRANCH POINT WHEN DESIGNATED
*                                      SRB NOT AVAILABLE FOR USE     */
*   END;                                                        02S0120
@RF00118 DS    0H                                               02S0120
RMCRDX1  DS    0H                                               02S0121
*   RMEPPTR=PMRE;                   /* REST INPUT RTNE BLOCK ADDR    */
         LR    RMEPPTR,PMRE                                     02S0121
*   END IRARMCRD;                                               02S0122
@EL00003 DS    0H                                               02S0122
@EF00003 DS    0H                                               02S0122
@ER00003 BR    @14                                              02S0122
*                                                               02S0123
*/* **************************************************************** */
*/*                                                                  */
*/*    ROUTINE NAME - IRARMCRL                                       */
*/*                                                                  */
*/*    DESCRIPTIVE NAME - SYSTEM RESOURCES MANAGER ALGORITHMIC       */
*/*                       PROCESSING REQUEST RECEIVING SUBROUTINE    */
*/*                                                                  */
*/*    FUNCTION - THE IRARMCRL SUBROUTINE ACCEPTS A REQUEST FOR AN   */
*/*          ALGORITHM, AND TURNS ON A BIT IN THE RMCT TO INDICATE   */
*/*          THAT THAT ALGORITHM HAS BEEN REQUESTED.  SUBSEQUENTLY,  */
*/*          THE IRARMCEL SBRTINE WILL OBSERVE THAT BIT,     @Z40BPCJ*/
*/*          AND WILL INVOKE THE ALGORITHM.                  @Z40BPCJ*/
*/*                                                                  */
*/*    NOTE  - IRARMCRL DOES NOT REQUIRE THE SRM LOCK TO BE HELD.    */
*/*            WHILE IRARMCRL IS EXECUTING, OTHR CONCURRENT PROCSS   */
*/*            MAY BE INSERTING, ALSO VIA THE IRARMCRL ROUTINE, OR   */
*/*            MAY BE REMOVING, VIA THE IRARMCEL ROUTINE, REQUEST    */
*/*            BITS INTO/FROM -RMCTALA-.  HOWEVER, -RMCTALR- USAGE   */
*/*            IS SERIALIZED, IN THAT ALL ROUTINES WHICH ACCESS IT   */
*/*            MUST HOLD THE SYSTEM RESOURCES MANAGER LOCK.          */
*/*                                                                  */
*/*    INPUT - REG 1 CONTAINS THE ADDRESS OF THE RMEP BLOCK FOR THE  */
*/*            ALGORITHM BEING REQUESTED.                            */
*/*          - REG 14 CONTAINS THE RETURN ADDRESS.                   */
*/*          - REG 15 CONTAINS THE IRARMCRL ROUTINE ADDRESS.         */
*/*          - THE RRPA LOCATED BY REG 3 INDICATES IF THIS REQUEST   */
*/*            IS/IS NOT COMING FROM A PROCESS HOLDING THE SYSTEM    */
*/*            RESOURCES MANAGER LOCK.                               */
*/*          - THE RRPAEPA FIELD CONTAINS THE ADDRESS OF THE RMEP    */
*/*            BLOCK OF THE ROUTINE WHICH IS ISSUING THE REQUEST.    */
*/*                                                                  */
*/*    OUTPT - IF IRARMCRL IS ENTERED WITH THE SRM LOCK,     @Z40BPCJ*/
*/*            THE FLAG FROM THE RMEPFLG FIELD OF THE INPUT RMEP IS  */
*/*            OR'ED INTO RMCTALR.  OTHERWISE, THE FLAG      @Z40BPCJ*/
*/*            IS SET IN THE RMCTALA FIELD BY A COMPARE AND SWAP.    */
*/*                                                                  */
*/*    EXIT  - BRANCH ON REGISTER 14.                                */
*/*          - THERE IS NO ERROR EXIT OR INDICATION.                 */
*/*                                                                  */
*/* **************************************************************** */
*                                                               02S0123
*   /*****************************************************************/
*   /*                                                               */
*   /* IRARMCRL ROUTINE - ENTRY SEQUENCE ? IRACTLEP                  */
*   /* EPN(CRL)SAVE(6,14)                                            */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S0123
*   DCL                                                         02S0123
*     IRARMCRL ENTRY INT;                                       02S0123
*   GEN CODE REFS(LCCACPUS,PSALCCAV,WSACOPTM) DEFS(IRARMCRL) NOEXIT
*       SEQFLOW;                                                02S0124
*                                                               02S0124
*                                  /*
         ENTRY IRARMCRL
IRARMCRL DS    0H
         L     @15,PSALCCAV            LCCA ADDRESS
         L     @15,LCCACPUS-LCCA(,@15) CPU SAVE AREA VECTOR TABLE
         L     @15,WSACOPTM-WSAC(,@15) SRM SAVE AREA START ADDRESS
         STM   @06,@14,000+024(@15)    SAVE INVOKING RTNE REGS
         LR    @13,@15                 ROUTINE SAVE AREA ADDRESS
         BALR  @15,0                   SETUP ENTRY ADDRESSABILITY
         LA    @12,*-@PSTART           DISPLACEMENT TO MODULE BASE
         LCR   @12,@12                 NEGATIVE FOR SUBTRACT
         ALR   @12,@15                 SETUP MODULE ADDRESSABILITY
*   /*****************************************************************/
*   /*                                                               */
*   /* TEST THAT LOCK IS HELD, REQST IS NOT RECURSIVE                */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S0125
*   IF RRPAOPT='1'B&RRPAEPA^=R1 THEN                            02S0125
*                                                               02S0125
         TM    RRPAOPT(RRPAPTR),B'00000010'                     02S0125
         BNO   @RF00125                                         02S0125
         C     R1,RRPAEPA(,RRPAPTR)                             02S0125
         BE    @RF00125                                         02S0125
*     /***************************************************************/
*     /*                                                             */
*     /* INSTRUCTION EXECUTION SERIALIZED BY HOLDING THE SRM LOCK    */
*     /*                                                             */
*     /***************************************************************/
*                                                               02S0126
*     DO;                                                       02S0126
*       WKRO=RMCTALR;               /* PICK IMMED ALG REQUEST FIELD  */
         L     WKRO,RMCTALR(,RMCTPTR)                           02S0127
*       WKRO=RMEPFLG×WKRO;          /* INSERT FLAG FOR REQUESTD ALG  */
         O     WKRO,RMEPFLG(,RMEPPTR)                           02S0128
*       WKRO=WKRO&FLAGDEF;          /* CLR RTNE CHARACTERISTIC FLGS  */
         N     WKRO,FLAGDEF                                     02S0129
*       RMCTALR=WKRO;               /* STORE UPDATED ALG REQST FLD   */
         ST    WKRO,RMCTALR(,RMCTPTR)                           02S0130
*     END;                                                      02S0131
*   ELSE                            /* REQUEST FROM OUTSIDE OPT LOCK */
*                                                               02S0132
*     /***************************************************************/
*     /*                                                             */
*     /* CAN EXECUTE IN PARALLEL - MUST USE COMPARE AND SWAP INSTR   */
*     /*                                                             */
*     /***************************************************************/
*                                                               02S0132
*     DO;                                                       02S0132
         B     @RC00125                                         02S0132
@RF00125 DS    0H                                               02S0133
*       WKRE=RMCTALA;               /* PICK ALG REQUEST ACCUM FIELD  */
         L     WKRE,RMCTALA(,RMCTPTR)                           02S0133
*RMCRLL1:                           /* POINT FROM WHICH TRY AGAIN IF
*                                      COMPARE AND SWAP FAILS        */
*       WKRO=WKRE;                  /* PICK ALG REQ ACCUM CONTENTS   */
RMCRLL1  LR    WKRO,WKRE                                        02S0134
*       WKRO=RMEPFLG×WKRO;          /* INSERT FLAG FOR REQUESTD ALG  */
         O     WKRO,RMEPFLG(,RMEPPTR)                           02S0135
*       WKRO=WKRO&FLAGDEF;          /* CLR RTNE CHARACTERISTIC FLGS  */
         N     WKRO,FLAGDEF                                     02S0136
*       CS(WKRE,WKRO,RMCTALAW);     /* STORE UPDATED ALG REQST FLD   */
         CS    WKRE,@11,RMCTALAW(RMCTPTR)                       02S0137
*       BC(7,RMCRLL1);              /* REPEAT IF ACCUM UPDTE FAILED  */
         BC    7,RMCRLL1                                        02S0138
*       IF(RRPAOPT='0'B&RMEPRCR='1'B)/* REQ ACTION W/O DELAY @Z40BPCJ*/
*           ×(R1=ADDR(RMEPBPR1))    /* OR UIC UPDATE REDRIVE @ZA33182*/
*           ×(R1=ADDR(RMEPBCAP)) THEN/* OR IF CAP REDRIVE    @Z40BPCJ*/
*                                                               02S0139
         TM    RRPAOPT(RRPAPTR),B'00000010'                     02S0139
         BNZ   @GL00004                                         02S0139
         TM    RMEPRCR(RMEPPTR),B'00000100'                     02S0139
         BO    @RT00139                                         02S0139
@GL00004 LA    @09,RMEPBPR1(,RMCTPTR)                           02S0139
         CR    R1,@09                                           02S0139
         BE    @RT00139                                         02S0139
         LA    @09,RMEPBCAP(,RMCTPTR)                           02S0139
         CR    R1,@09                                           02S0139
         BNE   @RF00139                                         02S0139
@RT00139 DS    0H                                               02S0140
*         /***********************************************************/
*         /*                                                         */
*         /* REQUEST SCHEDULING OF SRB TO GET LOCK AND RUN ALGORITHM */
*         /*                                                         */
*         /***********************************************************/
*                                                               02S0140
*         DO;                                                   02S0140
*           SVRO=R0;                /* SAVE INPUT R0 ACROSS CALL     */
         LR    SVRO,R0                                          02S0141
*           CALL IRARMCRD;          /* REQUEST LOCK SRB SCHEDULING   */
         BAL   @14,IRARMCRD                                     02S0142
*           R0=SVRO;                /* RESTORE INPUT R0 CONTENTS     */
         LR    R0,SVRO                                          02S0143
*         END;                                                  02S0144
*     END;                                                      02S0145
*                                                               02S0145
@RF00139 DS    0H                                               02S0146
*   /*****************************************************************/
*   /*                                                               */
*   /* IRARMCRL ROUTINE - EXIT SEQUENCE ? IRACTLEP ENN(CRL)          */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S0146
*   GEN CODE EXIT NOSEQFLOW;                                    02S0146
@RC00125 DS    0H                                               02S0146
*                                  /*
         LM    @06,@14,000+024(@13)    RELOAD INVOKING RTNE REGS
         BR    RTNREG                  RETURN TO INVOKER
*                                                               02S0147
*/* **************************************************************** */
*/*                                                                  */
*/*    ROUTINE NAME - IRARMCRN                                       */
*/*                                                                  */
*/*    DESCRIPTIVE NAME - SYSTEM RESOURCES MANAGER DEFERRABLE USER   */
*/*                       ACTION REQUEST RECEIVING SUBROUTINE        */
*/*                                                                  */
*/*    FUNCTION - THE IRARMCRN SUBROUTINE ACCEPTS A REQUEST FOR AN   */
*/*          ACTION WHICH REQUIRES SERIALIZATION WITH OTHER SYSTEM   */
*/*          RESOURCES MANAGER PROCESSING.  IT CHECKS TO SEE IF THE  */
*/*          ACTION MAY BE PERFORMED IMMEDIATELY, IF YES, IT INVOKES */
*/*          THE REQUESTED ROUTINE.  OTHERWISE, IRARMCRN SETS A BIT  */
*/*          IN THE OUCB OF THE AFFECTED USER, AND QUEUES THAT OUCB  */
*/*          FOR SUBSEQUENT PROCESSING BY THE IRARMCTL ROUTINE.      */
*/*                                                                  */
*/*    NOTE  - IRARMCRN DOES NOT REQUIRE THE SRM LOCK.       @Z40BPJC*/
*/*            WHILE IRARMCRN IS EXECUTING, OTHR CONCURRENT PROCSS   */
*/*            MAY BE INSERTING, ALSO VIA THE IRARMCRN ROUTINE, OR   */
*/*            MAY BE REMOVING, VIA THE IRARMCEN ROUTINE, REQUEST    */
*/*            BITS INTO/FROM THE -OUCBACN- FIELDS OF THE SAME OR    */
*/*            DIFFERENT OUCB'S, AND MAY BE ENQUEUEING/DEQUEUEING    */
*/*            THOSE OUCBS TO/FRM THE ACTION QUEUE IN -RMCTAQHD-.    */
*/*                                                                  */
*/*    INPUT - REG 1 CONTAINS THE ADDRESS OF THE RMEP BLOCK FOR THE  */
*/*            ACTION ROUTINE BEING REQUESTED.                       */
*/*          - REG 4 CONTAINS THA ADDRESS OF THE OUCB OF THE USER    */
*/*            FOR WHOM THE ACTION IS TO BE PERFORMED.               */
*/*          - REG 14 CONTAINS THE RETURN ADDRESS.                   */
*/*          - THE RRPA LOCATED BY REG 3 INDICATES IF THIS REQUEST   */
*/*            IS/IS NOT COMING FORM A PROCESS HOLDING THE SYSTEM    */
*/*            RESOURCES MANAGER LOCK.                               */
*/*                                                                  */
*/*    OUTPT - IF THE REQUESTED ACTION IS TO BE DEFERRED,    @Z40BPJC*/
*/*            THE RMEPFLG FIELD OF THE INPUT RMEP IS OR'ED  @Z40BPJC*/
*/*            INTO THE OUCBACN FIELD OF THE OUCB LOCATED BY THE     */
*/*            INPUT ASCB.  THAT OUCB IS THEN ADDED TO THE SYSTEM    */
*/*            RESOURCES MANAGER ACTION QUEUE, WHOSE MEMBERS ARE     */
*/*            LINKED VIA THEIR OUCBACT FIELDS, AND WHOSE HEAD IS    */
*/*            LOCATED VIA THE RMCTAQHD FIELD.                       */
*/*                                                                  */
*/*    SETUP PERFORMED IF THE ACTION ROUTINE IS INVOKED DIRECTLY     */
*/*          - REG 1 CONTAINS THE ADDRESS OF THE RMEP BLOCK FOR THE  */
*/*            ACTION ROUTINE BEING INVOKED.                         */
*/*          - REG 4 CONTAINS THE ADDRESS OF THE OUCB FOR WHOM THE   */
*/*            ACTION IS TO BE PERFORMED.                            */
*/*          - REG 13 CONTAINS THE ADDRESS OF THE SAVE AREA STACK    */
*/*            ASSOCIATED WITH THE SYSTEM RESOURCES MANAGER LOCK.    */
*/*          - REG 14 CONTAINS THE RETURN ADDRESS.                   */
*/*          - REG 15 CONTAINS THE ACTION RTNE ENTRY ADDRESS.        */
*/*                                                                  */
*/*    EXIT  - BRANCH ON REGISTER 14.                                */
*/*          - THERE IS NO ERROR EXIT OR INDICATION.                 */
*/*                                                                  */
*/* **************************************************************** */
*                                                               02S0147
*   /*****************************************************************/
*   /*                                                               */
*   /* IRARMCRN ROUTINE - ENTRY SEQUENCE ? IRACTLEP EPN(CRN)         */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S0147
*   DCL                                                         02S0147
*     IRARMCRN ENTRY INT;                                       02S0147
*   GEN CODE REFS(LCCACPUS,PSALCCAV,WSACOPTM) DEFS(IRARMCRN) NOEXIT
*       SEQFLOW;                                                02S0148
*                                  /*
         ENTRY IRARMCRN
IRARMCRN DS    0H
         L     @15,PSALCCAV            LCCA ADDRESS
         L     @15,LCCACPUS-LCCA(,@15) CPU SAVE AREA VECTOR TABLE
         L     @15,WSACOPTM-WSAC(,@15) SRM SAVE AREA START ADDRESS
         STM   @00,@14,064+000(@15)    SAVE INVOKING RTNE REGS
         LR    @13,@15                 ROUTINE SAVE AREA ADDRESS
         BALR  @15,0                   SETUP ENTRY ADDRESSABILITY
         LA    @12,*-@PSTART           DISPLACEMENT TO MODULE BASE
         LCR   @12,@12                 NEGATIVE FOR SUBTRACT
         ALR   @12,@15                 SETUP MODULE ADDRESSABILITY
*   IF RRPAOPT='1'B&                /* LOCKED,               @Z40BPCJ*/
*       RRPAEPA->RMEPACN='0'B THEN  /* NOT RECURSIV ACN CALL         */
*                                                               02S0149
         TM    RRPAOPT(RRPAPTR),B'00000010'                     02S0149
         BNO   @RF00149                                         02S0149
         L     @09,RRPAEPA(,RRPAPTR)                            02S0149
         TM    RMEPACN(@09),B'00000001'                         02S0149
         BNZ   @RF00149                                         02S0149
*     /***************************************************************/
*     /*                                                             */
*     /* THE REQUEST DOES NOT HAVE TO BE DEFERRED.           @Z40BPCJ*/
*     /* THE REQUESTED RTNE IS INVOKED DIRECTLY.             @Z40BPCJ*/
*     /* HOLDING THE SRM LOCK ASSURES SERIALIZATION.         @Z40BPCJ*/
*     /*                                                             */
*     /***************************************************************/
*                                                               02S0150
*     NTRREG=RMEPEPA;               /* ENTRY TO REQSTD ACTION RTNE   */
         L     NTRREG,RMEPEPA(,RMEPPTR)                         02S0150
*   ELSE                            /* OTHER PROCSS USING RESOURCES  */
*                                                               02S0151
*     /***************************************************************/
*     /*                                                             */
*     /* THE INVOCATION MUST BE DEFERRED. THE REQUEST IS FLAGGED IN  */
*     /* THE USER CONTROL BLOCK, AND THE CTRL BLOCK IS QUEUED FOR    */
*     /* LATER EXAMINATION. COMPARE AND SWAP USAGE PERMITS THE       */
*     /* HANDLING OF SIMULTANEOUS UPDATES.                           */
*     /*                                                             */
*     /***************************************************************/
*                                                               02S0151
*     DO;                                                       02S0151
*                                                               02S0151
         B     @RC00149                                         02S0151
@RF00149 DS    0H                                               02S0152
*       /*************************************************************/
*       /*                                                           */
*       /* INSERT REQUEST FLAG INTO USER DEFERRED ACTION FIELD       */
*       /*                                                           */
*       /*************************************************************/
*                                                               02S0152
*       WKRE=OUCBCSW;               /* PICK DEFERRED ACTION ACCUMUL  */
         L     WKRE,OUCBCSW(,OUCBPTR)                           02S0152
*RMCRNL1:                           /* RETRY INSERT ACTION IF OUCB
*                                      CHANGED BY OTHER PROCESS      */
*       WKRO=RMEPFLG&FLAGDEF;       /* CLR RTNE CHARACTERISTIC FLGS  */
RMCRNL1  L     WKRO,RMEPFLG(,RMEPPTR)                           02S0153
         N     WKRO,FLAGDEF                                     02S0153
*       WKRO=WKRO×WKRE;             /* INSERT FLAG FOR REQUESTD ACN  */
         OR    WKRO,WKRE                                        02S0154
*       CS(WKRE,WKRO,OUCBCSW);      /* STORE UPDATED ACTN REQ FIELD  */
         CS    WKRE,@11,OUCBCSW(OUCBPTR)                        02S0155
*       BC(7,RMCRNL1);              /* REPEAT IF ACCUM UPDATE FAILED */
*                                                               02S0156
         BC    7,RMCRNL1                                        02S0156
*       /*************************************************************/
*       /*                                                           */
*       /* INSURE OUCB IS QUEUED ONTO THE DEFERRED ACTION QUEUE      */
*       /*                                                           */
*       /*************************************************************/
*                                                               02S0157
*       R0=ZERO;                    /* ZERO - TO SEE IF OUCB QUEUED  */
         SLR   R0,R0                                            02S0157
*       WKRO=OUCBPTR;               /* INSERT NEW OUCB FOR Q HEADER  */
         LR    WKRO,OUCBPTR                                     02S0158
*       GEN(LM    SVRE,SVRO,RMCTAQCT(RMCTPTR));/* PICK OLD Q HEADER  */
         LM    SVRE,SVRO,RMCTAQCT(RMCTPTR)
*       CS(R0,SVRO,OUCBACTW);       /* POINT NEW OUCB TO TOP OF QUE  */
         CS    R0,@07,OUCBACTW(OUCBPTR)                         02S0160
*       BC(7,RMCRNX1);              /* EXIT IF OUCB ALREADY ON QUEUE */
*                                                               02S0161
         BC    7,RMCRNX1                                        02S0161
*       /*************************************************************/
*       /*                                                           */
*       /* IT IS THE RESPONSIBILITY OF WHOEVER MAKES -OUCBACT- NON-  */
*       /* ZERO TO INSURE THAT THE OUCB IS PROPERLY QUEUED ONTO THE  */
*       /* ACTION QUEUE. IT IS THE RESPONSIBILITY OF WHOEVER MAKES   */
*       /* -OUCBACT- ZERO TO DO SO ONLY IF ALL ACTIONS REQUESTED UP  */
*       /* TO THAT MOMENT WILL BE COMPLETED BY THE ZERO SET ROUTINE. */
*       /*                                                           */
*       /*************************************************************/
*                                                               02S0162
*RMCRNL2:                           /* RETRY ENQUEUE OUCB IF QUEUE
*                                      CHANGED BY OTHER PROCESS      */
*       WKRE=SVRE+ONE;              /* INCREMENT Q COUNT FOR HEADER  */
RMCRNL2  LA    WKRE,1                                           02S0162
         ALR   WKRE,SVRE                                        02S0162
*       CDS(SVRE,WKRE,RMCTACQH);    /* STORE UPDATED QUEUE HEADER    */
         CDS   SVRE,WKRE,RMCTACQH(RMCTPTR)                      02S0163
*       BC(8,RMCRNX1);              /* EXIT IF OUCB ENQ SUCCESSFUL   */
         BC    8,RMCRNX1                                        02S0164
*       OUCBACT=SVRO;               /* POINT OUCB TO NEW TOP OF QUE  */
         ST    SVRO,OUCBACT(,OUCBPTR)                           02S0165
*       GOTO RMCRNL2;               /* REPEAT IF OUCB ENQUEUE FAILED */
         B     RMCRNL2                                          02S0166
*RMCRNX1:                                                       02S0167
*       ;                           /* BRANCH POINT WHEN ACTION 02S0167
*                                      DEFERRAL HAS BEEN COMPLETED   */
RMCRNX1  DS    0H                                               02S0168
*       IF RRPAOPT='0'B&RMEPRCR='1'B THEN/* REQ ACTION W/O DELAY
*                                                            @Z40BPCJ*/
*                                                               02S0168
         TM    RRPAOPT(RRPAPTR),B'00000010'                     02S0168
         BNZ   @RF00168                                         02S0168
         TM    RMEPRCR(RMEPPTR),B'00000100'                     02S0168
         BNO   @RF00168                                         02S0168
*         /***********************************************************/
*         /*                                                         */
*         /* REQUEST SCHEDULING OF SRB TO GET LOCK AND RUN ALGORITHM */
*         /*                                                         */
*         /***********************************************************/
*                                                               02S0169
*         DO;                                                   02S0169
*           CALL IRARMCRD;          /* REQUEST LOCK SRB SCHEDULING   */
         BAL   @14,IRARMCRD                                     02S0170
*         END;                                                  02S0171
*       NTRREG=CRNRTRN;             /* RETURN TO REQUESTOR OF ACTION */
@RF00168 L     NTRREG,CRNRTRN(,SVEREG)                          02S0172
*     END;                                                      02S0173
*                                                               02S0173
*   /*****************************************************************/
*   /*                                                               */
*   /* IRARMCRN ROUTINE - EXIT SEQUENCE ? IRACTLEP ENN(CRN)          */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S0174
*   GEN CODE EXIT NOSEQFLOW;                                    02S0174
@RC00149 DS    0H                                               02S0174
*                                  /*
         LM    @00,@14,064+000(@13)    RELOAD INVOKING RTNE REGS
         BR    NTRREG                  EXIT TO ACTION/REQUEST
*                                                               02S0175
*/* **************************************************************** */
*/*                                                                  */
*/*    ROUTINE NAME - IRARMCRT                                       */
*/*                                                                  */
*/*    DESCRIPTIVE NAME - SYSTEM RESOURCES MANAGER CNTRL ALGORITHM   */
*/*                       ENTRY POINT TABLE SCANNING SUBROUTINE      */
*/*                                                                  */
*/*    FUNCTION - THE IRARMCRT SUBROUTINE ACCEPTS AN INVOCATION BIT  */
*/*          PATTERN AND AN ENTRY POINT TABLE ADDRESS.  IT SCANS THE */
*/*          TABLE LOOKING FOR ENTRY POINT BLOCKS WHOSE INVOCATION   */
*/*          FLAGS MATCH 'ONE' BITS IN THE INPUT BIT PATTERN.  FOR   */
*/*          EACH SUCCESSFUL MATCH, THE CORRESPONDING ENTRY POINT    */
*/*          ROUTINE IS INVOKED.  AFTER THE ROUTINE COMPLETES, ITS   */
*/*          INVOCATION BIT IS SET 'ZERO' IN THE INPUT BIT PATTERN.  */
*/*                                                                  */
*/*    INPUT - REG 1 CONTAINS THE ADDRESS OF THE TOP RMEP BLOCK IN   */
*/*            A CHAINED TABLE.  THE RMEP BLOCKS IN THIS TABLE CON-  */
*/*            STITUTE THE LIST OF CANDIDATES THAT MAY BE INVOKED.   */
*/*          - REG 8 CONTAINS THE ADDRESS OF A FULL WORD WHOSE BITS  */
*/*            IF 'ONE' REPRESENT ROUTINES TO BE INVOKED.            */
*/*          - REG 13 CONTAINS THE ADDRESS OF THE SAVE AREA STACK    */
*/*            APPLICABLE TO THE ROUTINES TO BE INVOKED.             */
*/*          - REG 14 CONTAINS THE RETURN ADDRESS                    */
*/*                                                                  */
*/*    OUTPT - SEE FUNCTION.                                         */
*/*                                                                  */
*/*    EXIT  - BRANCH ON REGISTER 14.                                */
*/*          - THERE IS NO ERROR EXIT OR INDICATION.                 */
*/*                                                                  */
*/* **************************************************************** */
*                                                               02S0175
*IRARMCRT:                                                      02S0175
*   PROC OPTIONS(NOSAVE,NOSAVEAREA,NOENTREG,RETREG,NOPARMREG);  02S0175
IRARMCRT DS    0H                                               02S0176
*   SVRO=RTNREG;                    /* SAVE RETURN REG CONTENTS      */
         LR    SVRO,RTNREG                                      02S0176
*   WKRE=INMASK&RMCAINV;            /* PICK RTNE INVOCN BIT PATTERN  */
*                                                               02S0177
         L     WKRE,INMASK(,PMRE)                               02S0177
         N     WKRE,RMCAINV(,RMCTPTR)                           02S0177
*   /*****************************************************************/
*   /*                                                               */
*   /* THE FOLLOWING CODE WILL BE EXECUTED AS LONG AS ALL BITS IN THE*/
*   /* SUPPLIED INVOCATION PATTERN HAVE NOT BEEN TURNED OFF.         */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S0178
*   DO WHILE(WKRE^=EMPTY&RMEPPTR^=ZERO);                        02S0178
*                                                               02S0178
         B     @DE00178                                         02S0178
@DL00178 DS    0H                                               02S0179
*     /***************************************************************/
*     /*                                                             */
*     /* THE FOLLOWING CODE STEPS THROUGH THE CHAINED RMEP BLOCKS IN */
*     /* THE SPECIFIED TABLE. TABLE END IS INDICATED BY A ZERO VALUE */
*     /* IN THE CHAIN FORWARD POINTER.                               */
*     /*                                                             */
*     /***************************************************************/
*                                                               02S0179
*     WKRO=WKRE&RMEPFLG;            /* TEST CURRENT RTNE INVOCN BIT  */
         LR    WKRO,WKRE                                        02S0179
         N     WKRO,RMEPFLG(,RMEPPTR)                           02S0179
*     IF WKRO^=EMPTY THEN           /* ROUTINE FLAG MATCHES PATTERN  */
*                                                               02S0180
         LTR   WKRO,WKRO                                        02S0180
         BZ    @RF00180                                         02S0180
*       /*************************************************************/
*       /*                                                           */
*       /* INVOKE THE ROUTINE WHOSE FLAG IS 'ONE' IN THE PATTERN     */
*       /*                                                           */
*       /*************************************************************/
*                                                               02S0181
*       DO;                                                     02S0181
*         WKRE=INMASK;              /* PICK RTNE INVOCN BIT PATTERN  */
         L     WKRE,INMASK(,PMRE)                               02S0182
*RMCRTL1:                           /* POINT FROM WHICH TRY AGAIN IF
*                                      COMPARE AND SWAP FAILS        */
*         RTNREG=WKRE×WKRO;         /* ADD IN FLAG FOR INVOKED RTNE  */
RMCRTL1  LR    RTNREG,WKRE                                      02S0183
         OR    RTNREG,WKRO                                      02S0183
*         RTNREG=RTNREG&&WKRO;      /* DELETE FLAG FOR INVOKED RTNE  */
         XR    RTNREG,WKRO                                      02S0184
*         CS(WKRE,RTNREG,INMASK);   /* STORE UPDATED INVOCN PATTERN  */
         CS    WKRE,@14,INMASK(PMRE)                            02S0185
*         BC(7,RMCRTL1);            /* REPEAT IF PATTRN UPDTE FAILD  */
         BC    7,RMCRTL1                                        02S0186
*         RRPACTL='1'B;             /* CONTINUE IRARMCTL ON 2ND FAIL */
         OI    RRPACTL(RRPAPTR),B'00001000'                     02S0187
*         CALL RMEPEPA->IRARMRTN;   /* INVOKE DESIGNATED ROUTINE     */
         L     @15,RMEPEPA(,RMEPPTR)                            02S0188
         BALR  @14,@15                                          02S0188
*         RRPACTL='0'B;             /* REVERT TO NORMAL FAIL PROCESS */
         NI    RRPACTL(RRPAPTR),B'11110111'                     02S0189
*         RRPAEPA=ADDR(RMEPBCTL);   /* RESTORE CONTROL ROUTINE RMEP  */
         LA    @09,RMEPBCTL(,RMCTPTR)                           02S0190
         ST    @09,RRPAEPA(,RRPAPTR)                            02S0190
*         IF RMEPTMD='1'B&          /* TIME-DRIVEN ALGORITHM INVKD   */
*             RMCTTBS^=ZERO THEN    /* SRM TIMER INTERRUPTS ACTIVE   */
*                                                               02S0191
         TM    RMEPTMD(RMEPPTR),B'00000010'                     02S0191
         BNO   @RF00191                                         02S0191
         L     @09,RMCTTBS(,RMCTPTR)                            02S0191
         LTR   @09,@09                                          02S0191
         BZ    @RF00191                                         02S0191
*           /*********************************************************/
*           /*                                                       */
*           /* CALL SBRTN TO RESCHED THE RMEP ON THE TIMED ENTRY QUE */
*           /*                                                       */
*           /*********************************************************/
*                                                               02S0192
*           DO;                                                 02S0192
*             RMEPBCK->RMEPFWD=RMEPFWD;/* DEQ RMEP FROM FWD CHAIN    */
         L     @09,RMEPBCK(,RMEPPTR)                            02S0193
         L     @08,RMEPFWD(,RMEPPTR)                            02S0193
         ST    @08,RMEPFWD(,@09)                                02S0193
*             RMEPFWD->RMEPBCK=RMEPBCK;/* DEQ RMEP FROM BCK CHAIN    */
         ST    @09,RMEPBCK(,@08)                                02S0194
*             CALL IRARMCQT;        /* RE-ENQUE RMEP ONTO TIMED QUE  */
         BAL   @14,IRARMCQT                                     02S0195
*           END;                                                02S0196
*         WKRE=INMASK&RMCAINV;      /* RELOD RTN INVOCN BIT PATTERN  */
@RF00191 L     WKRE,INMASK(,PMRE)                               02S0197
         N     WKRE,RMCAINV(,RMCTPTR)                           02S0197
*       END;                                                    02S0198
*     RMEPPTR=RMEPPRV;              /* STEP TO NEXT RMEP ON CHAIN    */
@RF00180 L     RMEPPTR,RMEPPRV(,RMEPPTR)                        02S0199
*   END;                                                        02S0200
@DE00178 SLR   @09,@09                                          02S0200
         CLR   WKRE,@09                                         02S0200
         BE    @DC00178                                         02S0200
         CR    RMEPPTR,@09                                      02S0200
         BNE   @DL00178                                         02S0200
@DC00178 DS    0H                                               02S0201
*   RTNREG=SVRO;                    /* RESTORE RETURN REG CONTENTS   */
         LR    RTNREG,SVRO                                      02S0201
*   END IRARMCRT;                                               02S0202
@EL00004 DS    0H                                               02S0202
@EF00004 DS    0H                                               02S0202
@ER00004 BR    @14                                              02S0202
*                                                               02S0203
*/* **************************************************************** */
*/*                                                                  */
*/*    ROUTINE NAME - IRARMCRY                                       */
*/*                                                                  */
*/*    DESCRIPTIVE NAME - SYSTEM RESOURCES MANAGER CNTRL ALGORITHM   */
*/*          USER SWAP REQUEST RECEIVING SUBROUTINE          @Z40BPCJ*/
*/*                                                                  */
*/*    FUNCTION - THE IRARMCRY SUBROUTINE ACCEPTS A REQUEST THAT     */
*/*          A USER BE SWAPPED INTO OR OUT OF CORE.  IF THE USER IS  */
*/*                                                          @Z40BPCJ*/
*/*          IN THE PROCESS OF BEING SWAPPED, A CODE IS RETURNED     */
*/*          TO INDICATE THAT THE REQUEST COULD NOT BE HONORED.      */
*/*          IF THE REQUESTOR HAS OBTAINED THE SYSTEM RESOURCES      */
*/*          MANAGER LOCK, CONTROL IS ROUTED TO THE SUBROUTINE       */
*/*          WHICH WILL PROCESS THE REQUEST.  IF THE SRM LOCK IS     */
*/*          NOT HELD, A CODE IS RETURNED TO INDICATE THAT THE       */
*/*          REQUEST COULD NOT BE HONORED.                           */
*/*                                                                  */
*/*    INPUT - REG 1 CONTAINS THE ADDRESS OF THE RMEP BLOCK FOR THE  */
*/*            SWAP FUNCTION BEING REQUESTED.                @Z40BPCJ*/
*/*          - REG 4 CONTAINS THE ADDRESS OF THE OUCB OF THE USER    */
*/*            TO BE SWAPPED.                                @Z40BPCJ*/
*/*          - REG 14 CONTAINS THE RETURN ADDRESS.                   */
*/*          - REG 15 CONTAINS THE IRARMCRY ROUTINE ADDRESS.         */
*/*          - THE RRPA LOCATED BY REG 3 INDICATES IF THIS REQUEST   */
*/*            IS/IS NOT COMING FROM A PROCESS HOLDING THE SYSTEM    */
*/*            RESOURCES MANAGER LOCK.                               */
*/*          - THE RRPAEPA FIELD CONTAINS THE ADDRESS OF THE RMEP    */
*/*            BLOCK OF THE ROUTINE WHICH IS ISSUING THE REQUEST.    */
*/*          - SEE INPUT DATA DESCRIPTION OF THE INDIVIDUAL SWAP     */
*/*            SUBROUTINES FOR FURTHER PARAMETERS.           @Z40BPCJ*/
*/*                                                                  */
*/*    OUTPT - IF THE CURRENT STATE OF THE SPECIFIED OUCB WAS SUCH   */
*/*            THAT THE REQUEST COULD BE HONORED,            @Z40BPCJ*/
*/*            THE APPROPRIATE SWAPPING PROCEDURE            @Z40BPCJ*/
*/*            WILL HAVE BEEN INITIATED FOR THAT ADDRESS SPACE.      */
*/*                                                                  */
*/*    EXIT NORMAL - BRANCH ON REGISTER 14.                          */
*/*          - 0 IN REG 15 TO INDICATE THAT THE REQUEST WAS HONORED. */
*/*                                                                  */
*/*    EXIT ERROR  - BRANCH ON REGISTER 14.                          */
*/*          - 4 IN REG 15 IF THE INDICATED USER IS          @Z40BPCJ*/
*/*            NON-SWAPPABLE OR INVALID                      @Z40BPCJ*/
*/*          - 8 IN REG 15 IF THE INDICATED USER IS CURRENTLY IN     */
*/*            THE PROCESS OF BEING SWAPPED.                         */
*/*          - SEE ERROR EXIT DESCRIPTION OF THE INDIVIDUAL SWAP     */
*/*            SUBROUTINES FOR FURTHER ERROR CODES.          @Z40BPCJ*/
*/*                                                                  */
*/* **************************************************************** */
*                                                               02S0203
*   /*****************************************************************/
*   /*                                                               */
*   /* IRARMCRY ROUTINE - ENTRY SEQUENCE ? IRACTLEP EPN(CRY)         */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S0203
*   DCL                                                         02S0203
*     IRARMCRY ENTRY INT;                                       02S0203
*   GEN CODE REFS(LCCACPUS,PSALCCAV,WSACOPTM) DEFS(IRARMCRY) NOEXIT
*       SEQFLOW;                                                02S0204
*                                                               02S0204
*                                  /*
         ENTRY IRARMCRY
IRARMCRY DS    0H
         L     @15,PSALCCAV            LCCA ADDRESS
         L     @15,LCCACPUS-LCCA(,@15) CPU SAVE AREA VECTOR TABLE
         L     @15,WSACOPTM-WSAC(,@15) SRM SAVE AREA START ADDRESS
         STM   @00,@14,128+000(@15)    SAVE INVOKING RTNE REGS
         LR    @13,@15                 ROUTINE SAVE AREA ADDRESS
         BALR  @15,0                   SETUP ENTRY ADDRESSABILITY
         LA    @12,*-@PSTART           DISPLACEMENT TO MODULE BASE
         LCR   @12,@12                 NEGATIVE FOR SUBTRACT
         ALR   @12,@15                 SETUP MODULE ADDRESSABILITY
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK IF SWAP INAPPLICABLE TO THIS USER.              @Z40BPCJ*/
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S0205
*   IF(OUCBNSW='0'B×OUCBOUT='1'B)&OUCBNAME='OUCB'&OUCBINV='0'B THEN/*
*                                      USER STATUS SWAPPABLE AND
*                                      VALID                 @Z40BPCJ*/
*                                                               02S0205
         TM    OUCBNSW(OUCBPTR),B'10000000'                     02S0205
         BZ    @GL00011                                         02S0205
         TM    OUCBOUT(OUCBPTR),B'00000100'                     02S0205
         BNO   @RF00205                                         02S0205
@GL00011 CLC   OUCBNAME(4,OUCBPTR),@CC02242                     02S0205
         BNE   @RF00205                                         02S0205
         TM    OUCBINV(OUCBPTR),B'00010000'                     02S0205
         BNZ   @RF00205                                         02S0205
*     /***************************************************************/
*     /*                                                             */
*     /* SWAP NOT PROHIBITED. CHECK IF THE ADDRESS           @Z40BPCJ*/
*     /* SPACE IS IN THE PROCESS OF BEING SWAPPED.                   */
*     /*                                                             */
*     /***************************************************************/
*                                                               02S0206
*     IF OUCBGOO='0'B&OUCBGOI='0'B THEN/* NOT BEING SWAPPED          */
*                                                               02S0206
         TM    OUCBGOO(OUCBPTR),B'11000000'                     02S0206
         BNZ   @RF00206                                         02S0206
*       /*************************************************************/
*       /*                                                           */
*       /* CHECK IF THE FUNCTION CAN BE INVOKED AT THIS TIME.        */
*       /*                                                           */
*       /*************************************************************/
*                                                               02S0207
*       IF RRPAOPT='1'B THEN        /* SRM LOCK HELD BY REQUESTOR    */
*                                                               02S0207
         TM    RRPAOPT(RRPAPTR),B'00000010'                     02S0207
         BNO   @RF00207                                         02S0207
*         /***********************************************************/
*         /*                                                         */
*         /* INVOKE REQUESTED SUBFUNCTION.                   @Z40BPCJ*/
*         /*                                                         */
*         /***********************************************************/
*                                                               02S0208
*         DO;                                                   02S0208
*           WKRO=SVEREG;            /* SAVE IRARMCRY CALLERS REG 13  */
         LR    WKRO,SVEREG                                      02S0209
*           SVEREG=RMCTRMSA;        /* PASS SAVE AREA STACK TO RTNE  */
         L     SVEREG,RMCTRMSA(,RMCTPTR)                        02S0210
*           CALL RMEPEPA->IRARMRTN; /* INVOKE REQSTD SWAP FUNCTN     */
         L     @15,RMEPEPA(,RMEPPTR)                            02S0211
         BALR  @14,@15                                          02S0211
*           SVEREG=WKRO;            /* REST IRARMCRY CALLERS REG 13  */
         LR    SVEREG,WKRO                                      02S0212
*         END;                                                  02S0213
*       ELSE                        /* NONLOCKED REQ FOR SWAP@Z40BPCJ*/
*         RTNCOD=FOUR;              /* RESULT EQUALS NON-SWAPPABLE   */
         B     @RC00207                                         02S0214
@RF00207 LA    RTNCOD,4                                         02S0214
*     ELSE                          /* HAVE ONGOING SWAP     @Z40BPCJ*/
*       RTNCOD=EIGHT;               /* REQUEST GIVEN AT WRONG TIME   */
         B     @RC00206                                         02S0215
@RF00206 LA    RTNCOD,8                                         02S0215
*   ELSE                            /* ADDRESS SPACE NON-SWAPPABLE   */
*     RTNCOD=FOUR;                  /* REQUEST GIVEN FOR WRONG USER  */
*                                                               02S0216
         B     @RC00205                                         02S0216
@RF00205 LA    RTNCOD,4                                         02S0216
*   /*****************************************************************/
*   /*                                                               */
*   /* IRARMCRY ROUTINE - EXIT SEQUENCE ? IRACTLEP ENN(CRY)          */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S0217
*   GEN CODE EXIT NOSEQFLOW;                                    02S0217
@RC00205 DS    0H                                               02S0217
*                                  /*
         LM    @00,@14,128+000(@13)    RELOAD INVOKING RTNE REGS
         BR    RTNREG                  RETURN TO INVOKER
*                                                               02S0218
*/* **************************************************************** */
*/*                                                                  */
*/*    ROUTINE NAME - IRARMCSI                                       */
*/*                                                                  */
*/*    DESCRIPTIVE NAME - SYSTEM RESOURCES MANAGER CNTRL ALGORITHM   */
*/*                       USER SWAP-IN REQUEST RECEIVING SBRTNE      */
*/*                                                                  */
*/*    FUNCTION - THE IRARMCSI SUBROUTINE ACCEPTS A REQUEST THAT AN  */
*/*          ADDRESS SPACE BE SWAPPED IN.  ROUTINES          @Z40BPCJ*/
*/*          ARE INVOKED TO ALLOCATE AN OUXB TO THE ADDRESS SPACE,   */
*/*          AND TO SCHEDULE THE SWAP-IN.  IF THE SWAPIN     @Z40BPCJ*/
*/*          PROCESS WAS SUCCESSFULLY INITIATED, THE OUCB IS MARKED  */
*/*          THAT THE ADDRESS SPACE IS BEING SWAPPED IN.             */
*/*                                                                  */
*/*    NOTE  - THE IRARMCSI ROUTINE DOES NOT REQUIRE SERIALIZATION   */
*/*            WITH OTHER SYSTEM RESOURCES MANAGER PROCESSES, WHICH  */
*/*            COULD BE CONCURRENTLY EXAMINING THE SAME OUCB WHOSE   */
*/*            STATUS IS BEING CHANGED BY IRARMCSI.                  */
*/*                                                                  */
*/*    INPUT - REG 4 CONTAINS THE ADDRESS OF THE OUCB OF THE USER    */
*/*            WHO IS TO BE SWAPPED IN.                              */
*/*          - REG 13 CONTAINS THE ADDRESS OF THE SAVE AREA STACK    */
*/*            ASSOCIATED WITH THE SYSTEM RESOURCES MANAGER LOCK.    */
*/*          - REG 14 CONTAINS THE RETURN ADDRESS.                   */
*/*          - REG 15 CONTAINS THE IRARMCSI ROUTINE ADDRESS.         */
*/*                                                                  */
*/*    OUTPT - AN OUXB IS ASSIGNED TO THE ADDRESS SPACE TO BE        */
*/*            SWAPPED IN.                                           */
*/*          - OUCBGOI IS SET IF THE SWAP-IN IS INITIATED.           */
*/*                                                                  */
*/*    EXIT NORMAL - BRANCH ON REGISTER 14.                          */
*/*          - 0 IN REG 15 TO INDICATE THAT SWAP-IN WAS INITIATED.   */
*/*                                                                  */
*/*    EXIT ERROR  - BRANCH ON REGISTER 14.                          */
*/*          - SEE IRARMI04 ROUTINE TO DETERMINE THE ERROR   @Z40BPCJ*/
*/*            CODES RETURNED WHEN A STORAGE REQUEST FAILS.  @Z40BPCJ*/
*/*          - SEE IRARMI07 ROUTINE TO DETERMINE THE ERROR   @Z40BPCJ*/
*/*            CODES RETURNED WHEN A SWAPIN REQUEST FAILS.   @Z40BPCJ*/
*/*          - 8 IN REG 15 IF THE INDICATED USER IS          @Z40BPCJ*/
*/*            ALREADY SWAPPED IN.                           @Z40BPCJ*/
*/*                                                                  */
*/* **************************************************************** */
*                                                               02S0218
*   /*****************************************************************/
*   /*                                                               */
*   /* IRARMCSI ROUTINE - ENTRY SEQUENCE ? IRACTLEP EPN(CSI)         */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S0218
*   DCL                                                         02S0218
*     IRARMCSI ENTRY INT;                                       02S0218
*   GEN CODE DEFS(IRARMCSI) NOEXIT SEQFLOW;                     02S0219
*                                  /*
         ENTRY IRARMCSI
IRARMCSI DS    0H
         STM   @00,@14,128+000(@13)    SAVE INVOKING RTNE REGS
         LA    @12,IRARMCSI-@PSTART    DISPLACEMENT TO MODULE BASE
         LCR   @12,@12                 NEGATIVE FOR SUBTRACT
         ALR   @12,@15                 SETUP MODULE ADDRESSABILITY
*   IF OUCBOUT='1'B THEN                                        02S0220
         TM    OUCBOUT(OUCBPTR),B'00000100'                     02S0220
         BNO   @RF00220                                         02S0220
*     DO;                           /* USER CURRENTLY OUT OF CORE
*                                                            @Z40BPCJ*/
*       ASCBPTR=OUCBASCB;           /* PICK USER TO BE SWPD  @ZA00250*/
         L     ASCBPTR,OUCBASCB(,OUCBPTR)                       02S0222
*       R1=ASCBOUXB;                /* PICK EXTENSION ADDRS  @ZA00250*/
         L     R1,ASCBOUXB(,ASCBPTR)                            02S0223
*       IF R1=RMCTOUXB THEN         /* DON'T HAVE EXTENSION  @Z40BPCJ*/
*                                                               02S0224
         C     R1,RMCTOUXB(,RMCTPTR)                            02S0224
         BNE   @RF00224                                         02S0224
*         /***********************************************************/
*         /*                                                         */
*         /* PREPARE FOR SWAP-IN - OBTAIN USER CNTRL BLOCK EXTENSION */
*         /*                                                         */
*         /***********************************************************/
*                                                               02S0225
*         DO;                                                   02S0225
*                                                               02S0225
*           /*********************************************************/
*           /*                                                       */
*           /* INVOKE SUBROUTINE TO OBTAIN STORAGE FOR ADDR SPACE    */
*           /* OUXB                                                  */
*           /*                                                       */
*           /*********************************************************/
*                                                               02S0226
*           R0=LENGTH(OUXB);        /* INDICATE SIZE OF AREA WANTED  */
         LA    R0,144                                           02S0226
*           R1=ZERO;                /* INDICATE STORAGE GET REQUEST  */
*                                                               02S0227
         SLR   R1,R1                                            02S0227
*           /*********************************************************/
*           /*                                                       */
*           /* ? IRACTLCL EPN(I04)                                   */
*           /*                                                       */
*           /*********************************************************/
*                                                               02S0228
*           CALL RMSBI04->IRARMRTN; /* INVOKE I04 SUBRTN             */
         L     @15,RMSBI04(,RMCTPTR)                            02S0228
         BALR  @14,@15                                          02S0228
*           IF RTNCOD^=ZERO THEN                                02S0229
         LTR   RTNCOD,RTNCOD                                    02S0229
         BNZ   @RT00229                                         02S0229
*             GO TO RMCSIL5;        /* DONT HAVE STORAGE FOR OUXB
*                                                            @Z40BPCJ*/
*         END;                                                  02S0231
*                                                               02S0231
*       /*************************************************************/
*       /*                                                           */
*       /* INVOKE SUBROUTINE TO SCHEDULE SWAP-IN FOR ADDRESS SPACE ? */
*       /* IRACTLCL EPN(I07)                                         */
*       /*                                                           */
*       /*************************************************************/
*                                                               02S0232
*       CALL RMSBI07->IRARMRTN;     /* INVOKE I07 SUBRTN             */
@RF00224 L     @15,RMSBI07(,RMCTPTR)                            02S0232
         BALR  @14,@15                                          02S0232
*       IF RTNCOD=ZERO THEN         /* PHYSICAL SWAP-IN INITIATED    */
*                                                               02S0233
         LTR   RTNCOD,RTNCOD                                    02S0233
         BNZ   @RF00233                                         02S0233
*         /***********************************************************/
*         /*                                                         */
*         /* INITIALIZE BLOCKS FOR ADDRESS SPACE SWAP-IN             */
*         /*                                                         */
*         /***********************************************************/
*                                                               02S0234
*         DO;                                                   02S0234
*           R1->OUXB=R1->OUXB&&R1->OUXB;/* CLEAR OUXB                */
         XC    OUXB(144,R1),OUXB(R1)                            02S0235
*           R1->OUXBNAME='OUXB';    /* SETUP BLOCK IDENTIFIER        */
         MVC   OUXBNAME(4,R1),@CC02246                          02S0236
*           ASCBOUXB=R1;            /* STORE NEW OUXB ADDRESS        */
         ST    R1,ASCBOUXB(,ASCBPTR)                            02S0237
*           OUCBGOI='1'B;           /* INDICATE USER STATUS CHANGE   */
         OI    OUCBGOI(OUCBPTR),B'01000000'                     02S0238
*           RMCACIUS=RMCACIUS+ONE;  /* ADD TO COMING IN USRS @Z30WIGS*/
         LA    @09,1                                            02S0239
         AH    @09,RMCACIUS(,RMCTPTR)                           02S0239
         STH   @09,RMCACIUS(,RMCTPTR)                           02S0239
*         END;                                                  02S0240
*       ELSE                        /* ERROR IN TRYING TO SWAP-IN    */
*                                                               02S0241
*         /***********************************************************/
*         /*                                                         */
*         /* FREE STORAGE OBTAINED FOR USE AS OUXB                   */
*         /*                                                         */
*         /***********************************************************/
*                                                               02S0241
*         DO;                                                   02S0241
         B     @RC00233                                         02S0241
@RF00233 DS    0H                                               02S0242
*           RESPECIFY                                           02S0242
*             CVRE RSTD;            /*                       @Z40BPCJ*/
*           CVRE=RTNCOD;            /*                       @Z40BPCJ*/
         LR    CVRE,RTNCOD                                      02S0243
*           ASCBOUXB=RMCTOUXB;      /* STORE PTR TO PREDEFINED OUXB
*                                                            @Z40BPCJ*/
         L     @09,RMCTOUXB(,RMCTPTR)                           02S0244
         ST    @09,ASCBOUXB(,ASCBPTR)                           02S0244
*           R0=LENGTH(OUXB);        /* INDICATE SIZE OF AREA FREED   */
*                                                               02S0245
         LA    R0,144                                           02S0245
*           /*********************************************************/
*           /*                                                       */
*           /* ? IRACTLCL EPN(I04)                                   */
*           /*                                                       */
*           /*********************************************************/
*                                                               02S0246
*           CALL RMSBI04->IRARMRTN; /* INVOKE I04 SUBRTN             */
         L     @15,RMSBI04(,RMCTPTR)                            02S0246
         BALR  @14,@15                                          02S0246
*           RTNCOD=CVRE;            /*                       @Z40BPCJ*/
         LR    RTNCOD,CVRE                                      02S0247
*           RESPECIFY                                           02S0248
*             CVRE UNRSTD;          /*                       @Z40BPCJ*/
*         END;                                                  02S0249
*RMCSIL5:                                                       02S0250
*     END;                          /*                       @Z40BPCJ*/
*   ELSE                                                        02S0251
*     RTNCOD=EIGHT;                 /*                       @Z40BPCJ*/
*                                                               02S0251
         B     @RC00220                                         02S0251
@RF00220 LA    RTNCOD,8                                         02S0251
*   /*****************************************************************/
*   /*                                                               */
*   /* ? IRACTLEP ENN(CSI)                                           */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S0252
*   GEN CODE EXIT NOSEQFLOW;                                    02S0252
@RC00220 DS    0H                                               02S0252
*                                  /*
         LM    @00,@14,128+000(@13)    RELOAD INVOKING RTNE REGS
         BR    RTNREG                  RETURN TO INVOKER
*                                                               02S0253
*/* **************************************************************** */
*/*                                                                  */
*/*    ROUTINE NAME - IRARMCSO                                       */
*/*                                                                  */
*/*    DESCRIPTIVE NAME - SYSTEM RESOURCES MANAGER CNTRL ALGORITHM   */
*/*                       USER SWAP-OUT REQUEST RECEIVING SBRTNE     */
*/*                                                                  */
*/*    FUNCTION - THE IRARMCSO SUBROUTINE ACCEPTS A REQUEST THAT AN  */
*/*          ADDRESS SPACE BE SWAPPED OUT.  IF THE ADDRESS SPACE IS  */
*/*          ALREADY SWAPPED OUT, A CODE IS RETURNED TO INDICATE     */
*/*          THAT THE REQUEST COULD NOT BE HONORED.  OTHERWISE, THE  */
*/*          IRARMI06 ROUTINE IS INVOKED TO POST QUIESCE FOR THE     */
*/*          ADDRESS SPACE, AND THE SYSTEM RESOURCES MANAGER USER    */
*/*          CONTROL BLOCK IS MARKED TO SHOW THAT THE USER IS IN     */
*/*          THE PROCESS OF BEING SWAPPED OUT.                       */
*/*                                                                  */
*/*    NOTE  - THE IRARMCSO ROUTINE DOES NOT REQUIRE SERIALIZATION   */
*/*            WITH OTHER SYSTEM RESOURCES MANAGER PROCESSES, WHICH  */
*/*            COULD BE EXAMINING THE SAME OUCB WHOSE STATUS IS      */
*/*            BEING CHANGED BY IRARMCSO.                            */
*/*                                                                  */
*/*    INPUT - REG 4 CONTAINS THE ADDRESS OF THE OUCB OF THE USER    */
*/*            WHO IS TO BE SWAPPED OUT.                             */
*/*          - REG 14 CONTAINS THE RETURN ADDRESS.                   */
*/*          - REG 15 CONTAINS THE IRARMCSO ROUTINE ADDRESS.         */
*/*                                                                  */
*/*    OUTPT - OUCBGOO IS SET IF THE SWAP-OUT IS INITIATED.          */
*/*                                                                  */
*/*    EXIT NORMAL - BRANCH ON REGISTER 14.                          */
*/*          - 0 IN REG 15 TO INDICATE THAT SWAP-OUT WAS INITIATED.  */
*/*                                                                  */
*/*    EXIT ERROR  - BRANCH ON REGISTER 14.                          */
*/*          - 4 IN REG 15 IF AN ERROR WAS EXPERIENCED WHILE         */
*/*            TRYING TO POST QUIESCE FOR THE ADDRESS SPACE.         */
*/*          - 8 IN REG 15 IF THE INDICATED USER IS ALREADY          */
*/*            SWAPPED OUT.                                          */
*/*          - SEE IRARMCRY EXIT DESCRIPTION FOR ERROR CODES         */
*/*            GIVEN IF THE USER IS NOT IN SWAPPABLE STATUS.         */
*/*                                                                  */
*/* **************************************************************** */
*                                                               02S0253
*   /*****************************************************************/
*   /*                                                               */
*   /* IRARMCSO ROUTINE - ENTRY SEQUENCE ? IRACTLEP EPN(CSO)         */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S0253
*   DCL                                                         02S0253
*     IRARMCSO ENTRY INT;                                       02S0253
*   GEN CODE DEFS(IRARMCSO) NOEXIT SEQFLOW;                     02S0254
*                                                               02S0254
*                                  /*
         ENTRY IRARMCSO
IRARMCSO DS    0H
         STM   @00,@14,128+000(@13)    SAVE INVOKING RTNE REGS
         LA    @12,IRARMCSO-@PSTART    DISPLACEMENT TO MODULE BASE
         LCR   @12,@12                 NEGATIVE FOR SUBTRACT
         ALR   @12,@15                 SETUP MODULE ADDRESSABILITY
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK IF SWAP-OUT REQUEST INAPPLICABLE TO THIS USER.          */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S0255
*   IF OUCBOUT='0'B THEN            /* USER CURRENTLY IN CORE        */
*                                                               02S0255
         TM    OUCBOUT(OUCBPTR),B'00000100'                     02S0255
         BNZ   @RF00255                                         02S0255
*     /***************************************************************/
*     /*                                                             */
*     /* INITIATE ACTUAL SWAP-OUT PROCESS.                           */
*     /*                                                             */
*     /***************************************************************/
*                                                               02S0256
*     DO;                                                       02S0256
*                                                               02S0256
*       /*************************************************************/
*       /*                                                           */
*       /* INVOKE SUBROUTINE TO POST QUIESCE.                        */
*       /*                                                           */
*       /*************************************************************/
*                                                               02S0257
*       ASCBPTR=OUCBASCB;           /* PASS USER TO BE QUIESCED      */
         L     ASCBPTR,OUCBASCB(,OUCBPTR)                       02S0257
*       OUCBPSTR='1'B;              /* IF POST ERROR, RECOVER (BIT IS
*                                      RESET BY QSCEST SYSEVENT)
*                                                            @ZA16887*/
         OI    OUCBPSTR(OUCBPTR),B'00001000'                    02S0258
*       R0=ZERO;                    /* PASS POST CODE 0      @Z116887*/
         SLR   R0,R0                                            02S0259
*       R1=ADDR(ASCBECB);           /* PASS RCT ECB ADDRESS          */
         LA    R1,ASCBECB(,ASCBPTR)                             02S0260
*       ASCBFQUB='1'B;              /* TELL RCT TO QUIESCE           */
*                                                               02S0261
         L     @09,ASCBFQUB-2(,ASCBPTR)                         02S0261
@GL00013 LR    @08,@09                                          02S0261
         O     @08,@CF02292                                     02S0261
         CS    @09,@08,ASCBFQUB-2(ASCBPTR)                      02S0261
         BNE   @GL00013                                         02S0261
*       /*************************************************************/
*       /*                                                           */
*       /* ? IRACTLCL EPN(I06)                                       */
*       /*                                                           */
*       /*************************************************************/
*                                                               02S0262
*       CALL RMSBI06->IRARMRTN;     /* INVOKE I06 SUBRTN             */
         L     @15,RMSBI06(,RMCTPTR)                            02S0262
         BALR  @14,@15                                          02S0262
*       IF RTNCOD=ZERO THEN         /* PHYSICAL SWAP-OUT INITIATED   */
*                                                               02S0263
         LTR   RTNCOD,RTNCOD                                    02S0263
         BNZ   @RF00263                                         02S0263
*         /***********************************************************/
*         /*                                                         */
*         /* PUT USER AT TOP OF DISPATCHING QUEUE DURING QUIESCE     */
*         /*                                                         */
*         /***********************************************************/
*                                                               02S0264
*         DO;                       /*                       @YM05632*/
*           OUCBGOO='1'B;           /* INDICATE USER STATUS CHANGE   */
         OI    OUCBGOO(OUCBPTR),B'10000000'                     02S0265
*           IF OUCBAPG='0'B         /* PREVENT ZERO DP IF A  @ZA11365*/
*             THEN                                              02S0266
         TM    OUCBAPG(OUCBPTR),B'01000000'                     02S0266
         BNZ   @RF00266                                         02S0266
*             OUCBNDP=ASCBDP;       /* SWAP BEFORE EVENT 10. @ZA11365*/
         IC    @09,ASCBDP(,ASCBPTR)                             02S0267
         STC   @09,OUCBNDP(,OUCBPTR)                            02S0267
*           RMCACHU=ASCBPTR;        /* PASS USER TO BE CHAPPED  02S0268
*                                                            @YM05632*/
@RF00266 ST    ASCBPTR,RMCACHU(,RMCTPTR)                        02S0268
*           R1=ADDR(RMCACHP);       /* ADDR OF CHAP PARAM LIST  02S0269
*                                                            @YM05632*/
*                                                               02S0269
         LA    R1,RMCACHP(,RMCTPTR)                             02S0269
*           /*********************************************************/
*           /*                                                       */
*           /* ? IRACTLCL EPN(I02)                                   */
*           /*                                                       */
*           /*********************************************************/
*                                                               02S0270
*           CALL RMSBI02->IRARMRTN; /* INVOKE I02 SUBRTN             */
         L     @15,RMSBI02(,RMCTPTR)                            02S0270
         BALR  @14,@15                                          02S0270
*           RTNCOD=ZERO;            /* SWAP-OUT REQUEST PROCESSED
*                                                            @YM05632*/
         SLR   RTNCOD,RTNCOD                                    02S0271
*         END;                      /*                       @YM05632*/
*       ELSE                        /* ERROR IN TRYING TO SWAP-OUT   */
*         RTNCOD=FOUR;              /* PASS INVOKER ERROR RESPONSE   */
         B     @RC00263                                         02S0273
@RF00263 LA    RTNCOD,4                                         02S0273
*     END;                                                      02S0274
*   ELSE                            /* USER ALREADY SWAPPED OUT      */
*     RTNCOD=EIGHT;                 /* REQUEST GIVEN AT WRONG TIME   */
*                                                               02S0275
         B     @RC00255                                         02S0275
@RF00255 LA    RTNCOD,8                                         02S0275
*   /*****************************************************************/
*   /*                                                               */
*   /* IRARMCSO ROUTINE - EXIT SEQUENCE ? IRACTLEP ENN(CSO)          */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S0276
*   GEN CODE EXIT NOSEQFLOW;                                    02S0276
@RC00255 DS    0H                                               02S0276
*                                  /*
         LM    @00,@14,128+000(@13)    RELOAD INVOKING RTNE REGS
         BR    RTNREG                  RETURN TO INVOKER
*                                                               02S0277
*/* *********************************************************@Z40BPCH*/
*/*                                                          @Z40BPCH*/
*/* ROUTINE NAME - IRARMRPS                                  @Z40BPCH*/
*/*                                                          @Z40BPCH*/
*/* DESCRIPTIVE NAME - SYSTEM RESOURCES MANAGER USER CONTROL @Z40BPCH*/
*/*                    BLOCK REPOSITIONING ROUTINE           @Z40BPCH*/
*/*                                                          @Z40BPCH*/
*/* FUNCTION - THE IRARMRPS SUBROUTINE DEQUEUES THE INDICATED@Z40BPCH*/
*/*            OUCB FROM THE PRESENT POSITION ON ITS PRESENT @Z40BPCH*/
*/*            QUEUE, AND ENQUEUES THE OUCB ONTO THE QUEUE   @Z40BPCH*/
*/*            SPECIFIED BY THE FLAGS IN ITS OUCBQFL FIELD   @Z40BPCH*/
*/*            AT THE END POSITION ON THE QUEUE.             @Z40BPCH*/
*/*                                                          @Z40BPCH*/
*/* NOTE - IRARMRPS IS AN 'ACTION ROUTINE' I.E. IT PERFORMS  @Z40BPCH*/
*/*        OUCB ENQUEUEING FOR ONE USER. IRARMRPS RUNS UNDER @Z40BPCH*/
*/*        THE SRM LOCK.                                     @Z40BPCH*/
*/*                                                          @Z40BPCH*/
*/* INPUT - OUCBGOB MUST BE SET '1'B WHEN CALLED             @Z40BPCH*/
*/*         REG 1 CONTAINS THE ADDRESS OF THE RMEP FOR RPS   @Z40BPCH*/
*/*         REG 4 CONTAINS THE ADDR OF THE OUCB TO BE QUEUED @Z40BPCH*/
*/*         REG 13 CONTAINS THE ADDR OF THE SAVE AREA STACK  @Z40BPCH*/
*/*         ASSOCIATED WITH THE SRM LOCK                     @Z40BPCH*/
*/*         REG 14 CONTAINS THE RETURN ADDRESS               @Z40BPCH*/
*/*         REG 15 CONTAINS THE RPS ROUTINE ADDRESS          @Z40BPCH*/
*/*         IF THE OUCBOUT FLAG IS OFF, THE OUCB WILL BE     @Z40BPCH*/
*/*         PLACED ON THE IN-CORE QUEUE.                     @Z40BPCH*/
*/*         ELSE, IF THE OUCBOFF FLAG IS ON AND THE USER IS  @Z40BPCH*/
*/*         NOT READY OR IS BEING FORCED OUT BY AN AUXILIARY @Z40BPCH*/
*/*         STORAGE SHORTAGE, THE OUCB WILL BE PLACED ON THE @Z40BPCH*/
*/*         WAIT QUEUE. OTHERWISE THE OUCB WILL BE PLACED ON @Z40BPCH*/
*/*         THE OUT QUEUE.                                   @Z40BPCH*/
*/*                                                          @Z40BPCH*/
*/* OUTPUT - THE OUXB IS MOVED TO THE END POSITION ON THE    @Z40BPCH*/
*/*          INDICATED QUEUE.                                @Z40BPCH*/
*/*                                                          @Z40BPCH*/
*/* EXIT - BRANCH ON REGISTER 14                             @Z40BPCH*/
*/*        THERE IS NO ERROR EXIT OR INDICATOR.              @Z40BPCH*/
*/*                                                          @Z40BPCH*/
*/* *********************************************************@Z40BPCH*/
*/* IRARMRPS ROUTINE - ENTRY SEQUENCE                        @Z40BPCH*/
*                                                               02S0277
*   /*****************************************************************/
*   /*                                                               */
*   /* ? IRACTLEP EPN(RPS)SAVE(6,14)CODEREG(*)                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S0277
*   DCL                                                         02S0277
*     IRARMRPS ENTRY INT;                                       02S0277
*   GEN CODE REFS(RMCTRMSA,RRPAEPA) DEFS(IRARMRPS) NOEXIT SEQFLOW;
*                                                               02S0278
*                                  /*
         ENTRY IRARMRPS
IRARMRPS DS    0H
         L     @15,RMCTRMSA(,RMCTPTR)  SRM SAVE AREA START ADDRESS
         STM   @00,@14,064+000(@15)    SAVE INVOKING RTNE REGS
         MVC   064+060(4,@15),RRPAEPA(RRPAPTR) SAVE PREV RMEP
         ST    @01,RRPAEPA(,RRPAPTR)   INDICATE NEW CURRENT RMEP
         LR    @13,@15                 ROUTINE SAVE AREA ADDRESS
         BALR  @15,0                   RESET ENTRY ADDRESSABILITY
         USING *,@15
*/*                                                          @Z40BPCH*/
*/* DEQUEUE OUCB FROM ITS PRESENT POSITION ON SOME QUEUE. THE@Z40BPCH*/
*/* SUBROUTINE IS ONLY EXECUTED WHILE HOLDING THE SRM LOCK.  @Z40BPCH*/
*/* COMPARE AND SWAP INSTRUCTIONS DO NOT HAVE TO BE USED.    @Z40BPCH*/
*                                                               02S0279
*   OUCBFWD->OUCBBCK=OUCBBCK;       /* DEQ OUCB FROM PREV BACK CHAIN
*                                                            @Z40BPCH*/
         L     @09,OUCBFWD(,OUCBPTR)                            02S0279
         L     @08,OUCBBCK(,OUCBPTR)                            02S0279
         ST    @08,OUCBBCK(,@09)                                02S0279
*   OUCBBCK->OUCBFWD=OUCBFWD;       /* DEQ OUCB FROM PREV FWD CHAIN
*                                                            @Z40BPCH*/
         ST    @09,OUCBFWD(,@08)                                02S0280
*/* DETERMINE QUEUE ONTO WHICH OUCB IS TO BE PLACED          @Z40BPCH*/
*                                                               02S0281
*   IF OUCBOUT='0'B THEN            /* TEST IF USER IS IN CORE  02S0281
*                                                            @Z40BPCH*/
         TM    OUCBOUT(OUCBPTR),B'00000100'                     02S0281
         BNZ   @RF00281                                         02S0281
*     PMRO=RMCTINQE;                /* SAVE INQ QUEUE HEADER ADDRESS
*                                                            @Z40BPCH*/
         L     PMRO,RMCTINQE(,RMCTPTR)                          02S0282
*   ELSE                            /* USER CONSIDERED OUT OF CORE
*                                                            @Z40BPCH*/
*/* THERE ARE TWO QUEUES FOR OUT OF CORE USERS. THE LONG WAIT QUEUE
*                                                            @Z40BPCH*/
*/* CONTAINS USERS WHICH MUST BE AWAKENED BEFORE THEY CAN EXECUTE.
*                                                            @Z40PBCH*/
*/* THE OUT OF CORE QUEUE CONTAINS USERS SUBJECT TO PERFORMANCE 02S0283
*                                                            @Z40PBCH*/
*/* ANALYSIS BEFORE BEING BROUGHT BACK IN.                   @Z40BPCH*/
*                                                               02S0283
*     DO;                           /*                       @Z40BPCH*/
         B     @RC00281                                         02S0283
@RF00281 DS    0H                                               02S0284
*/* TEST IF USER PUT ON WAIT QUEUE BY LONG WAIT OR BY AUXILIARY 02S0284
*                                                            @Z40BPCH*/
*/* STORAGE SHORTAGE.                                        @Z40BPCH*/
*                                                               02S0284
*       IF OUCBOFF='1'B&(OUCBRDY='0'B×OUCBAXS='1'B×OUCBFXS='1'B×
*           OUCBJSAS='1'B×OUCBJSFS='1'B) THEN/*              @ZM43072*/
         TM    OUCBOFF(OUCBPTR),B'00001000'                     02S0284
         BNO   @RF00284                                         02S0284
         TM    OUCBRDY(OUCBPTR),B'10000000'                     02S0284
         BZ    @RT00284                                         02S0284
         TM    OUCBAXS(OUCBPTR),B'00000101'                     02S0284
         BNZ   @RT00284                                         02S0284
         TM    OUCBJSAS(OUCBPTR),B'11000000'                    02S0284
         BZ    @RF00284                                         02S0284
@RT00284 DS    0H                                               02S0285
*         PMRO=RMCTWTQE;            /* PLACE USER ON WAIT QUEUE 02S0285
*                                                            @Z40BPCH*/
         L     PMRO,RMCTWTQE(,RMCTPTR)                          02S0285
*       ELSE                        /*                       @Z40BPCH*/
*         DO;                       /* PLACE USER ON OUT QUEUE  02S0286
*                                                            @Z40BPCH*/
         B     @RC00284                                         02S0286
@RF00284 DS    0H                                               02S0287
*           OUCBOFF='0'B;           /* REMOVE USER IF ON WAIT QUEUE
*                                                            @Z40BPCH*/
         NI    OUCBOFF(OUCBPTR),B'11110111'                     02S0287
*           PMRO=RMCTOTQE;          /* PLACE USER ON OUT QUEUE  02S0288
*                                                            @Z40BPCH*/
         L     PMRO,RMCTOTQE(,RMCTPTR)                          02S0288
*         END;                      /*                       @Z40BPCH*/
*     END;                          /*                       @Z40BPCH*/
@RC00284 DS    0H                                               02S0291
*/* INSERT THE OUCB ON THE END OF THE QUEUE                  @Z40BPCH*/
*                                                               02S0291
*   RESPECIFY                       /*                       @Z40BPCH*/
*    (OUCB) BASED(PMRO);            /*                       @Z40BPCH*/
@RC00281 DS    0H                                               02S0292
*   PMRO=OUCBBCK;                   /* ADDRESS OF LAST OUCB ON QUEUE
*                                                            @Z40BPCH*/
         L     PMRO,OUCBBCK(,PMRO)                              02S0292
*   OUCBPTR->OUCBBCK=ADDR(OUCB);    /* POINT OUCB TO BACK CHAIN 02S0293
*                                                            @Z40BPCH*/
         ST    PMRO,OUCBBCK(,OUCBPTR)                           02S0293
*   OUCBPTR->OUCBFWD=OUCBFWD;       /* POINT TO QUEUE HEADER @Z40BPCH*/
         L     @09,OUCBFWD(,PMRO)                               02S0294
         ST    @09,OUCBFWD(,OUCBPTR)                            02S0294
*   OUCBFWD->OUCBBCK=OUCBPTR;       /* ENQ OUCB ON BACKWD CHAIN 02S0295
*                                                            @Z40BPCH*/
         ST    OUCBPTR,OUCBBCK(,@09)                            02S0295
*   OUCBFWD=OUCBPTR;                /* ENQ OUCB ON FORWORD CHAIN
*                                                            @Z40BPCH*/
         ST    OUCBPTR,OUCBFWD(,PMRO)                           02S0296
*   RESPECIFY                       /*                       @Z40BPCH*/
*    (OUCB) BASED(OUCBPTR);         /* RESTORE NORMAL OUCB BASE 02S0297
*                                                            @Z40BPCH*/
*   OUCBGOB='0'B;                   /* CLEAR OUCB REPOSITION FLAG
*                                                            @Z40BPCH*/
         NI    OUCBGOB(OUCBPTR),B'11011111'                     02S0298
*/* IRARMRPS ROUTINE - EXIT SEQUENCE                         @Z40BPCH*/
*                                                               02S0299
*   /*****************************************************************/
*   /*                                                               */
*   /* ? IRACTLEP ENN(RPS)                                           */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S0299
*   GEN CODE EXIT NOSEQFLOW;                                    02S0299
*                                                               02S0299
*                                  /*
         MVC   RRPAEPA(4,RRPAPTR),064+060(@13) RESTORE PREV RMEP
         LM    @06,@14,064+024(@13)    RELOAD INVOKING RTNE REGS
         BR    RTNREG                  RETURN TO INVOKER
         DROP  @15
*/*                                                          @Z40BPCH*/
*                                                               02S0300
*                                                               02S0300
*/* **************************************************************** */
*/*                                                                  */
*/*    ROUTINE NAME - IRARMWMY                                       */
*/*                                                                  */
*/*    DESCRIPTIVE NAME - SYSTEM RESOURCES MANAGER CNTRL ALGORITHM   */
*/*          PERIODIC ENTRY POINT INVOKE INITIALIZATION ROUTINE      */
*/*                                                                  */
*/*    FUNCTION - THE IRARMWMY ROUTINE TAKES THE EXISTING BCKWRDS    */
*/*          CHAINED ENTRY TIMING QUEUE LOCATED BY -RMCTTMQE- AND    */
*/*          REQUEUES ALL OF ITS MEMBERS, ADJUSTING THE TIME DUE     */
*/*          FIELD OF EACH BY THE DIFFERENCE BETWEEN -RMCTTOD-       */
*/*          AND -RMPTTOI-.  THIS EFFECTIVELY RESETS THE ENTIRE      */
*/*          QUEUE TO START ANEW BASED UPON THE CURRENT TIME.        */
*/*                                                                  */
*/* **************************************************************** */
*                                                               02S0300
*   /*****************************************************************/
*   /*                                                               */
*   /* IRARMWMY ROUTINE - ENTRY SEQUENCE ? IRACTLEP EPN(WMY)         */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S0300
*   DCL                                                         02S0300
*     IRARMWMY ENTRY INT;                                       02S0300
*   GEN CODE DEFS(IRARMWMY) NOEXIT SEQFLOW;                     02S0301
*                                                               02S0301
*                                  /*
         ENTRY IRARMWMY
IRARMWMY DS    0H
         STM   @00,@14,128+000(@13)    SAVE INVOKING RTNE REGS
         LA    @12,IRARMWMY-@PSTART    DISPLACEMENT TO MODULE BASE
         LCR   @12,@12                 NEGATIVE FOR SUBTRACT
         ALR   @12,@15                 SETUP MODULE ADDRESSABILITY
*   /*****************************************************************/
*   /*                                                               */
*   /* CALCULATE AMT BY WHICH TO ADJUST SCHEDULED INVOCATION TIMES   */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S0302
*   WKRO=RRPATOD-RMCATOI;           /* DELTA BETWEEN EXPECT AND GET  */
         L     @09,RRPATOD(,RRPAPTR)                            02S0302
         LR    WKRO,@09                                         02S0302
         SL    WKRO,RMCATOI(,RMCTPTR)                           02S0302
*   IF RRPATOD=WKRO THEN            /* RRPATOD NOT ALLOW TO BE ZERO  */
         CLR   @09,WKRO                                         02S0303
         BNE   @RF00303                                         02S0303
*     WKRO=WKRO-ONE;                /* INSURE RMCTTOD NOT MADE ZERO  */
         BCTR  WKRO,0                                           02S0304
*   IF RMCTTBS=WKRO THEN            /* RMCTTBS ZERO IF HAVEN'T INIT  */
@RF00303 CL    WKRO,RMCTTBS(,RMCTPTR)                           02S0305
         BNE   @RF00305                                         02S0305
*     WKRO=WKRO-ONE;                /* INSURE RMCTTBS NOT MADE ZERO  */
         BCTR  WKRO,0                                           02S0306
*   RMCTTBS=RMCTTBS-WKRO;           /* ADJUST RSRCS MNGR TIME BASE   */
@RF00305 L     @09,RMCTTBS(,RMCTPTR)                            02S0307
         SLR   @09,WKRO                                         02S0307
         ST    @09,RMCTTBS(,RMCTPTR)                            02S0307
*   RRPATOD=RRPATOD-WKRO;           /* ADJUST RSRCS MNGR CURR TIME   */
         L     @09,RRPATOD(,RRPAPTR)                            02S0308
         SLR   @09,WKRO                                         02S0308
         ST    @09,RRPATOD(,RRPAPTR)                            02S0308
*   RMCTTOD=RMCTTOD-WKRO;           /* ADJUST RSRCS MNGR CURR TIME   */
*                                                               02S0309
         L     @09,RMCTTOD(,RMCTPTR)                            02S0309
         SLR   @09,WKRO                                         02S0309
         ST    @09,RMCTTOD(,RMCTPTR)                            02S0309
*   /*****************************************************************/
*   /*                                                               */
*   /* START NEW TIMED ENTRY QUEUE BY CHAINING HEADER TO ITSELF      */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S0310
*   RMEPPTR=RMCTTMQE;               /* PICK TIMED QUEUE HEADER ADDR  */
         L     RMEPPTR,RMCTTMQE(,RMCTPTR)                       02S0310
*   PMRE=RMEPBCK;                   /* PRESERVE PREVIOUS MEMBER ADR  */
         L     PMRE,RMEPBCK(,RMEPPTR)                           02S0311
*   RMEPFWD=RMEPPTR;                /* ENQUE FORWRD CHAIN TO HEADER  */
         ST    RMEPPTR,RMEPFWD(,RMEPPTR)                        02S0312
*   RMEPBCK=RMEPPTR;                /* ENQUE BCKWRD CHAIN TO HEADER  */
         ST    RMEPPTR,RMEPBCK(,RMEPPTR)                        02S0313
*   RMEPPTR=PMRE;                   /* PICK 1ST FORMER QUEUE MEMBER  */
*                                                               02S0314
         LR    RMEPPTR,PMRE                                     02S0314
*   /*****************************************************************/
*   /*                                                               */
*   /* STEP THROUGH MEMBERS OF PREVIOUS ENTRY TIMING QUE. ADJUST EACH*/
*   /* TO THE NEW TIME BASE, AND REQUE ONTO THE ENTRY TIMING QUEUE.  */
*   /* DONE STEPPING WHEN ARRIVE BACK AT QUEUE HEADER.               */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S0315
*   DO WHILE(RMEPFLG^=EMPTY);                                   02S0315
         B     @DE00315                                         02S0315
@DL00315 DS    0H                                               02S0316
*     PMRE=RMEPBCK;                 /* PRESERVE PREVIOUS MEMBER ADR  */
         L     PMRE,RMEPBCK(,RMEPPTR)                           02S0316
*     CALL IRARMCQT;                /* REENQUE MEMBR ONTO TIMED QUE  */
         BAL   @14,IRARMCQT                                     02S0317
*     RMEPPTR=PMRE;                 /* PICK NXT FORMER QUEUE MEMBER  */
         LR    RMEPPTR,PMRE                                     02S0318
*   END;                                                        02S0319
*                                                               02S0319
@DE00315 L     @09,RMEPFLG(,RMEPPTR)                            02S0319
         LTR   @09,@09                                          02S0319
         BNZ   @DL00315                                         02S0319
*/*  THE SUBSEQUENT IRARMCET ROUTINE WILL RESET RMCTTOI AND RESET    */
*/*  THE TIMER EXPIRE SETTING FOR THE SYSTEM RESOURCES MANAGER.      */
*                                                               02S0320
*   /*****************************************************************/
*   /*                                                               */
*   /* IRARMWMY ROUTINE - EXIT SEQUENCE ? IRACTLEP ENN(WMY)          */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S0320
*   GEN CODE EXIT NOSEQFLOW;                                    02S0320
*                                  /*
         LM    @00,@14,128+000(@13)    RELOAD INVOKING RTNE REGS
         BR    RTNREG                  RETURN TO INVOKER
*                                                               02S0321
*/* *********************************************************@Z40BPJC*/
*/*                                                          @Z40BPJC*/
*/*   ROUTINE NAME - IRARMCAP                                @Z40BPJC*/
*/*                                                          @Z40BPJC*/
*/*   DESCRIPTIVE NAME - SYSTEM RESOURCES MANAGER (SRM)      @Z40BPJC*/
*/*                      SWAP ANALYSIS ALGORITHM             @Z40BPJC*/
*/*                                                          @Z40BPJC*/
*/*   FUNCTION - THE IRARMCAP ROUTINE ATTEMPTS TO KEEP THE   @Z40BPJC*/
*/*              MULTIPROGRAMMING LEVEL (MPL) PER DOMAIN AT  @Z40BPJC*/
*/*              ITS TARGET LEVEL.  THE TARGET MPL PER DOMAIN@Z40BPJC*/
*/*              IS THE DESIRED NUMBER OF MEMORIES BELONGING @Z40BPJC*/
*/*              TO THAT DOMAIN TO BE KEPT IN CORE.  IRARMCAP@Z40BPJC*/
*/*              ATTEMPTS TO MAINTAIN THE MPL PER DOMAIN AT  @Z40BPJC*/
*/*              ITS TARGET BY PERFORMING MEMORY SWAPS.      @Z40BPJC*/
*/*                                                          @Z40BPJC*/
*/*   NOTE - IRARMCAP IS AN 'ALGORITHM' ROUTINE - I.E. IT HAS@Z40BPJC*/
*/*          A SYSTEM-WIDE SCOPE OF APPLICABILITY.  IRARMCAP @Z40BPJC*/
*/*          RUNS UNDER THE SRM LOCK.  IT IS A TIMED         @Z40BPJC*/
*/*          ALGORITHM AND IS ALSO INVOKED BY THE USERRDY AND@Z40BPJC*/
*/*          SWOUTCMP SYSEVENTS.                             @Z40BPJC*/
*/*                                                          @Z40BPJC*/
*/*   INPUT - REG 1 CONTAINS THE ADDRESS OF RMEP FOR IRARMCAP@Z40BPJC*/
*/*         - REG 13 CONTAINS THE ADDRESS OF THE SAVE AREA   @Z40BPJC*/
*/*         STACK ASSOCIATED WITH THE SYSTEM RESOURCES       @Z40BPJC*/
*/*         MANAGER LOCK.                                    @Z40BPJC*/
*/*         - REG 14 CONTAINS THE RETURN ADDRESS.            @Z40BPJC*/
*/*         - REG 15 CONTAINS THE IRARMCAP ROUTINE ADDRESS.  @Z40BPJC*/
*/*         - THE OUCB OUT QUEUE CONTAINS USERS WHICH ARE    @Z40BPJC*/
*/*         CANDIDATES FOR SWAP-IN.                          @Z40BPJC*/
*/*         - THE OUCB IN QUEUE CONTAINS USERS WHICH ARE     @Z40BPJC*/
*/*         CANDIDATES FOR SWAP-OUT.                         @Z40BPJC*/
*/*         - THE DOMAIN TABLE CONTAINS THE TARGET MPL PER   @Z40BPJC*/
*/*         DOMAIN                                           @Z40BPJC*/
*/*                                                          @Z40BPJC*/
*/*   OUTPUT - THE FOLLOWING DATA IS CALCULATED PER DOMAIN   @Z40BPJC*/
*/*            AND STORED IN THE DOMAIN TABLE:  THE CURRENT  @Z40BPJC*/
*/*            MPL, THE NUMBER OF SWAPPABLE INCORE USERS, THE@Z40BPJC*/
*/*            NUMBER OF OUT USERS, AND THE NUMBER OF USERS  @Z40BPJC*/
*/*            GOING OUT.  THESE COUNTS ARE UPDATED TO       @Z40BPJC*/
*/*            REFLECT CHANGES RESULTING FROM SWAPS.  THE    @Z40BPJC*/
*/*            STEPS OF THE SWAP ANALYSIS ARE DEFINED        @Z40BPJC*/
*/*            BELOW IN THE ORDER IN WHICH THEY ARE          @Z40BPJC*/
*/*            INVOKED.  ALL DOMAINS ARE CONSIDERED FOR EACH @Z40BPJC*/
*/*            STEP BEFORE PROCEEDING TO THE NEXT STEP.      @Z40BPJC*/
*/*                                                          @Z40BPJC*/
*/*            1) FOR EACH DOMAIN, SWAP OUT THE REQUIRED     @Z40BPJC*/
*/*               NUMBER OF USERS TO DRIVE THE MPL DOWN TO   @Z40BPJC*/
*/*               ITS TARGET.  SELECT THE USERS WITH THE     @Z40BPJC*/
*/*               SMALLEST RECOMMENDATION VALUES.  IF ANY    @Z40BPJC*/
*/*               SWAPS ARE INITIATED BY STEP 1, THE OTHER   @ZA29015*/
*/*               STEPS ARE SKIPPED, AND THE ANALYSIS ENDS.  @ZA29015*/
*/*                                                          @Z40BPJC*/
*/*            2) IF THERE IS A USER ON THE OUT QUEUE        @Z40BPJC*/
*/*               ENQUEUED UPON A RESOURCE REQUESTED BY      @Z40BPJC*/
*/*               ANOTHER USER, OR BEING CANCELLED, SWAP IN  @Z40BPJC*/
*/*               THE USER IF THE MPL FOR ITS DOMAIN         @Z40BPJC*/
*/*               IS LESS THAN ITS TARGET.   OTHERWISE       @Z40BPJC*/
*/*               SWAP OUT THE INCORE USER IN THAT DOMAIN    @Z40PBJC*/
*/*               WITH THE SMALLEST RECOMMENDATION           @Z40BPJC*/
*/*               VALUE. IF NO INCORE USER IN                @Z40BPJC*/
*/*               THAT DOMAIN IS SWAPPABLE, SWAP IN          @Z40BPJC*/
*/*               THE OUT-OF-CORE USER ANYWAY.  IF A SWAP    @Z40BPJC*/
*/*               IS INITIATED BY STEP 2, THE REMAINING      @ZA29015*/
*/*               STEPS ARE SKIPPED, AND THE ANALYSIS ENDS.  @ZA29015*/
*/*                                                          @Z40BPJC*/
*/*            3) FOR EACH DOMAIN, SWAP IN THE REQUIRED      @Z40BPJC*/
*/*               NUMBER OF USERS TO RAISE THE MPL TO ITS    @Z40BPJC*/
*/*               TARGET VALUE.  SELECT THE USERS WITH THE   @Z49BPJC*/
*/*               LARGEST RECOMMENDATION VALUES.  IF ANY     @Z40BPJC*/
*/*               SWAPS ARE INITIATED BY STEP 3, STEP 4 IS   @ZA29015*/
*/*               SKIPPED, AND THE ANALYSIS ENDS.            @ZA29015*/
*/*                                                          @Z40BPJC*/
*/*            4) IF THE MPL FOR A DOMAIN IS EQUAL TO ITS    @Z40BPJC*/
*/*               TARGET, SELECT AN INCORE AND OUT OF CORE   @Z40BPJC*/
*/*               USER OF THE GIVEN DOMAIN TO EXCHANGE.  FIND@Z40BPJC*/
*/*               THE OUT OF CORE USER WITH THE LARGEST      @Z40BPJC*/
*/*               RECOMMENDATION VALUE AND THE INCORE USER   @Z40BPJC*/
*/*               WITH THE SMALLEST RECOMMENDATION VALUE.    @Z40BPJC*/
*/*               THE RECOMMENDATION VALUE OF THE OUT OF CORE@Z40BPJC*/
*/*               USER MUST EXCEED THE RECOMMENDATION VALUE  @Z40BPJC*/
*/*               OF THE INCORE USER BY A THRESHOLD.  IF AN  @Z40BPJC*/
*/*               EXCHANGE IS JUSTIFIED, A SWAPOUT OF THE    @Z40BPJC*/
*/*               INCORE USER IS INITIATED.  THE SWAPIN OF   @Z40BPJC*/
*/*               THE OUT OF CORE MEMORY MAY BE INITIATED    @Z40BPJC*/
*/*               UPON THE NEXT INVOCATION OF THE SWAP       @Z40BPJC*/
*/*               ANALYSIS ALGORITHM.                        @Z40BPJC*/
*/*                                                          @Z40BPJC*/
*/*   EXIT - BRANCH ON REGISTER 14.                          @Z40BPJC*/
*/*          THERE IS NO ERROR EXIT OR INDICATION.           @Z40BPJC*/
*/*                                                          @Z40BPJC*/
*/*                                                          @Z40BPJC*/
*/* *********************************************************@Z40BPJC*/
*                                                               02S0321
*   /*****************************************************************/
*   /*                                                               */
*   /* IRARMCAP ROUTINE - ENTRY SEQUENCE                     @Z40BPJC*/
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S0321
*/*   ? IRACTLEP EPN(CAP)                                            */
*                                                               02S0321
*   DCL                                                         02S0321
*     IRARMCAP ENTRY INT;                                       02S0321
*   GEN CODE REFS(RRPAEPA) DEFS(IRARMCAP) NOEXIT SEQFLOW;       02S0322
*                                                               02S0322
*                                  /*
         ENTRY IRARMCAP
IRARMCAP DS    0H
         STM   @00,@14,000+000(@13)    SAVE INVOKING RTNE REGS
         ST    @01,RRPAEPA(,RRPAPTR)   INDICATE NEW CURRENT RMEP
         LA    @12,IRARMCAP-@PSTART    DISPLACEMENT TO MODULE BASE
         LCR   @12,@12                 NEGATIVE FOR SUBTRACT
         ALR   @12,@15                 SETUP MODULE ADDRESSABILITY
*   /*****************************************************************/
*   /*                                                               */
*   /*                                                       @Z40BPJC*/
*   /* REINITIALIZE DOMAIN TABLE                                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S0323
*   RESPECIFY                                                   02S0323
*     DMDT BASED(DMDTPTR2);         /*                       @Z40BPJC*/
*   DO DMDTPTR2=RMCTDMDT BY LENGTH(DMDT) TO RMCTDMDE;/* DO EVERY
*                                      DOMAIN                @Z40BPJC*/
         L     DMDTPTR2,RMCTDMDT(,RMCTPTR)                      02S0324
         B     @DE00324                                         02S0324
@DL00324 DS    0H                                               02S0325
*     DMDTBLK1=0;                   /* ZERO NUMBER OF USERS GOING
*                                      OUT, CURRENT MPL      @Z40BPJC*/
         SLR   @09,@09                                          02S0325
         ST    @09,DMDTBLK1(,DMDTPTR2)                          02S0325
*     DMDTBLK2=0;                   /* NUMBER OF SWAPPABLE IN USERS
*                                      AND NUMBER OF OUT USERS  02S0326
*                                                            @Z40BPJC*/
         ST    @09,DMDTBLK2(,DMDTPTR2)                          02S0326
*   END;                            /*                       @Z40BPJC*/
*                                                               02S0327
         AL    DMDTPTR2,@CF02274                                02S0327
@DE00324 C     DMDTPTR2,RMCTDMDE(,RMCTPTR)                      02S0327
         BNH   @DL00324                                         02S0327
*   /*****************************************************************/
*   /*                                                               */
*   /* COUNT THREE TYPES OF USERS ON IN QUEUE:               @Z40BPJC*/
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S0328
*   OUCBPTR=RMCTINQE->OUCBFWD;      /* SCAN QUEUE FRWRD      @Z40BPJC*/
         L     @09,RMCTINQE(,RMCTPTR)                           02S0328
         L     OUCBPTR,OUCBFWD(,@09)                            02S0328
*   DO WHILE OUCBSCN='0'B;          /* TO ITS END            @Z40BPJC*/
         B     @DE00329                                         02S0329
@DL00329 DS    0H                                               02S0330
*     DMDTPTR2=RMCTDMDT+OUCBDMO;    /* POINT TO CURRENT OUCB'S 'S
*                                      DOMAIN ENTRY          @Z40BPJC*/
         LH    DMDTPTR2,OUCBDMO(,OUCBPTR)                       02S0330
         AL    DMDTPTR2,RMCTDMDT(,RMCTPTR)                      02S0330
*     IF OUCBNSW='0'B&              /* COUNT ONLY VALID/SWAPPABLE    */
*         OUCBINV='0'B THEN         /* USERS                 @Z40BPCJ*/
         TM    OUCBNSW(OUCBPTR),B'10010000'                     02S0331
         BNZ   @RF00331                                         02S0331
*       DO;                         /*                       @Z40BPJC*/
*         IF(OUCBGOO='1'B×OUCBGOB='1'B) THEN                    02S0333
         TM    OUCBGOO(OUCBPTR),B'10100000'                     02S0333
         BZ    @RF00333                                         02S0333
*           DMDTGOOU=DMDTGOOU+1;    /* COUNT USERS GOING OUT @Z40BPJC*/
         LA    @09,1                                            02S0334
         AH    @09,DMDTGOOU(,DMDTPTR2)                          02S0334
         STH   @09,DMDTGOOU(,DMDTPTR2)                          02S0334
*         ELSE                                                  02S0335
*           DO;                     /*                       @Z40BPJC*/
         B     @RC00333                                         02S0335
@RF00333 DS    0H                                               02S0336
*             DMDTCMPL=DMDTCMPL+1;  /* INCLUDE IN CURRENT MPL   02S0336
*                                                            @Z40BPJC*/
         LA    @09,1                                            02S0336
         LH    @08,DMDTCMPL(,DMDTPTR2)                          02S0336
         ALR   @08,@09                                          02S0336
         STH   @08,DMDTCMPL(,DMDTPTR2)                          02S0336
*             IF OUCBENQ='0'B&OUCBBIB='0'B THEN                 02S0337
         TM    OUCBENQ(OUCBPTR),B'00100010'                     02S0337
         BNZ   @RF00337                                         02S0337
*               DMDTINCU=DMDTINCU+1;/* COUNT INCORE USERS    @Z40BPJC*/
         AH    @09,DMDTINCU(,DMDTPTR2)                          02S0338
         STH   @09,DMDTINCU(,DMDTPTR2)                          02S0338
*           END;                    /*                       @Z40BPJC*/
@RF00337 DS    0H                                               02S0340
*       END;                        /*                       @Z40BPJC*/
@RC00333 DS    0H                                               02S0341
*     OUCBPTR=OUCBFWD;              /* CHAIN THRU ALL OUCBS  @Z40BPJC*/
@RF00331 L     OUCBPTR,OUCBFWD(,OUCBPTR)                        02S0341
*   END;                            /*                       @Z40BPJC*/
*                                                               02S0342
@DE00329 TM    OUCBSCN(OUCBPTR),B'00000001'                     02S0342
         BZ    @DL00329                                         02S0342
*   /*****************************************************************/
*   /*                                                               */
*   /* EXAMINE OUT QUEUE FOR USERS IN EACH DOMAIN WHICH ARE OUT,     */
*   /* GOING IN, OUT WITH ENQ BIT OR CANCEL BIT ON.          @Z40BPJC*/
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S0343
*   RMCACPW1=0;                     /* INITIALIZE POINTER TO OLDEST
*                                      ENQUEUED USER         @Z40BPJC*/
         SLR   @09,@09                                          02S0343
         ST    @09,RMCACPW1(,RMCTPTR)                           02S0343
*   OUCBPTR=RMCTOTQE->OUCBBCK;      /* START WITH YOUNGEST USER 02S0344
*                                                            @Z40BPJC*/
         L     @09,RMCTOTQE(,RMCTPTR)                           02S0344
         L     OUCBPTR,OUCBBCK(,@09)                            02S0344
*   DO WHILE OUCBSCN='0'B;          /* LOOP THROUGH OUCBS BACK TO HDR
*                                                            @Z40BPJC*/
         B     @DE00345                                         02S0345
@DL00345 DS    0H                                               02S0346
*     IF OUCBNSWI='0'B THEN         /* EXCLUDE TERMINATING USERS
*                                                            @ZA29015*/
         TM    OUCBNSWI(OUCBPTR),B'00001000'                    02S0346
         BNZ   @RF00346                                         02S0346
*       DO;                         /* FROM DOMAIN COUNTS    @ZA29015*/
*         DMDTPTR2=RMCTDMDT+OUCBDMO;/* POINT TO THIS OUCB'S DOMAIN
*                                                            @Z40BPJC*/
         LH    DMDTPTR2,OUCBDMO(,OUCBPTR)                       02S0348
         AL    DMDTPTR2,RMCTDMDT(,RMCTPTR)                      02S0348
*         IF OUCBGOI='1'B×OUCBGOB='1'B THEN                     02S0349
         TM    OUCBGOI(OUCBPTR),B'01100000'                     02S0349
         BZ    @RF00349                                         02S0349
*           DMDTCMPL=DMDTCMPL+1;    /* ADD 1 TO CURRENT MPL COUNT
*                                                            @Z40BPJC*/
         LA    @09,1                                            02S0350
         AH    @09,DMDTCMPL(,DMDTPTR2)                          02S0350
         STH   @09,DMDTCMPL(,DMDTPTR2)                          02S0350
*         ELSE                                                  02S0351
*           DO;                     /*                       @Z4OBPJC*/
         B     @RC00349                                         02S0351
@RF00349 DS    0H                                               02S0352
*             IF OUCBGOO='0'B THEN                              02S0352
         TM    OUCBGOO(OUCBPTR),B'10000000'                     02S0352
         BNZ   @RF00352                                         02S0352
*               DO;                 /* EXCLUDE GOING OUT USERS  02S0353
*                                                            @Z40BPJC*/
*                 DMDTOUTU=DMDTOUTU+1;/* ADD 1 TO OUT USER COUNT
*                                                            @Z40BPJC*/
         LA    @09,1                                            02S0354
         AH    @09,DMDTOUTU(,DMDTPTR2)                          02S0354
         STH   @09,DMDTOUTU(,DMDTPTR2)                          02S0354
*                 IF OUCBENQ='1'B×OUCBBIB='1'B THEN             02S0355
         TM    OUCBENQ(OUCBPTR),B'00100010'                     02S0355
         BZ    @RF00355                                         02S0355
*                   RMCACPW1=OUCBPTR;/* OLDEST OUCB FOUND SO FAR
*                                                            @Z40BPJC*/
         ST    OUCBPTR,RMCACPW1(,RMCTPTR)                       02S0356
*               END;                /*                       @Z40BPJC*/
*             ELSE                                              02S0358
*               DMDTGOOU=DMDTGOOU+1;/* COUNT USERS GOING OUT @Z40BPJC*/
         B     @RC00352                                         02S0358
@RF00352 LA    @09,1                                            02S0358
         AH    @09,DMDTGOOU(,DMDTPTR2)                          02S0358
         STH   @09,DMDTGOOU(,DMDTPTR2)                          02S0358
*           END;                    /*                       @Z40BPJC*/
@RC00352 DS    0H                                               02S0360
*       END;                        /* END DOMAIN COUNTS     @ZA29015*/
@RC00349 DS    0H                                               02S0361
*     OUCBPTR=OUCBBCK;              /* GET NEXT OLDER OUCB   @Z40BPJC*/
@RF00346 L     OUCBPTR,OUCBBCK(,OUCBPTR)                        02S0361
*   END;                            /*                       @Z40BPJC*/
@DE00345 TM    OUCBSCN(OUCBPTR),B'00000001'                     02S0362
         BZ    @DL00345                                         02S0362
*/* COUNTERS NOW UPDATED. RMCACPW1 HOLDS ADDRESS OF OLDEST OUCB 02S0363
*              ENQUEUED ON A RESOURCE                        @Z40BPJC*/
*                                                               02S0363
*                                                               02S0363
*   /*****************************************************************/
*   /*                                                               */
*   /* UNILATERAL SWAPOUT                                    @Z40BPJC*/
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S0363
*   OUCBPTR=0;                      /* PTR = 0 SHOWS NO SWAPOUTS YET
*                                                            @Z40BPJC*/
         SLR   OUCBPTR,OUCBPTR                                  02S0363
*   DO DMDTPTR2=RMCTDMDT BY LENGTH(DMDT) TO RMCTDMDE;/* DO EVERY
*                                      DOMAIN                @Z40BPJC*/
         L     DMDTPTR2,RMCTDMDT(,RMCTPTR)                      02S0364
         B     @DE00364                                         02S0364
@DL00364 DS    0H                                               02S0365
*     DO WHILE DMDTCMPL>DMDTMPLT&   /* IN EACH DOMAIN REDUCE MPL TO
*                                      TARGET,               @Z40BPJC*/
*           DMDTINCU^=0;            /* IF THIS DOMAIN HAS USERS IN
*                                      CORE AND SWAPPABLE.   @Z40BPJC*/
         B     @DE00365                                         02S0365
@DL00365 DS    0H                                               02S0366
*       CALL IRARMCPO;              /* PICK BEST CANDIDATE FOR  02S0366
*                                      SWAPOUT. DMDTPTR2 HAS ADDRESS
*                                      OF DMDT ENTRY         @Z40BPJC*/
         BAL   @14,IRARMCPO                                     02S0366
*       IF OUCBPTR=0 THEN                                       02S0367
         LTR   OUCBPTR,OUCBPTR                                  02S0367
         BZ    @RT00367                                         02S0367
*         GO TO RMCAPEND;           /* CHK RETURN AND QUIT IF CPO
*                                      DETECTED A DOMAIN CHANGE 02S0368
*                                                            @Z40BPJC*/
*                                                               02S0368
*       /*************************************************************/
*       /*                                                           */
*       /* ? IRACTLCL EPN(CSO)                                       */
*       /*                                                           */
*       /*************************************************************/
*                                                               02S0369
*       DO;                                                     02S0369
*         R1=ADDR(RMEPBCSO);        /* CSO REQUEST IS PARAM          */
         LA    R1,RMEPBCSO(,RMCTPTR)                            02S0370
*         CALL RMSBCRY->IRARMRTN;   /* ACTION INVK FUNCT             */
         L     @15,RMSBCRY(,RMCTPTR)                            02S0371
         BALR  @14,@15                                          02S0371
*       END;                        /* SWAP OUT THE USER, OUCBPTR
*                                      NON-ZERO SHOWS SWPOUT         */
*       IF RTNCOD^=0 THEN                                       02S0373
         LTR   RTNCOD,RTNCOD                                    02S0373
         BNZ   @RT00373                                         02S0373
*         GO TO RMCAPEND;           /* QUIT ALGORITHM TO AVOID  02S0374
*                                      LOOPING               @Z40BPJC*/
*       OUCBSRC=SRCDUSSC;           /* SET SWAP REASON CODE  @Z40BPJC*/
         MVI   OUCBSRC(OUCBPTR),X'0A'                           02S0375
*       DMDTCMPL=DMDTCMPL-1;        /* MPL=MPL-1             @Z40BPJC*/
