         TITLE 'IRARMIOM - SYSTEM RESOURCES MANAGEMENT I/O MANAGEMENT  *
                        '
IRARMIOM CSECT ,                                                   0001
@PROLOG  BALR  @12,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@12                                         0001
*                                  /*
         ENTRY IRARMIL0
IRARMIL0 DS    0H
         STM   @00,@14,128+000(@13)    SAVE INVOKING RTNE REGS
         LA    @12,IRARMIL0-@PSTART    DISPLACEMENT TO MODULE BASE
         LCR   @12,@12                 NEGATIVE FOR SUBTRACT
         ALR   @12,@15                 SETUP MODULE ADDRESSABILITY
*   IF ICTIOL='0'B THEN             /* DONT BOTHER IF I/O LOAD       */
         TM    ICTIOL(RMCTPTR),B'01000000'                         0060
         BZ    @RT00060                                            0060
*     GO TO IL0END;                 /* BALANCING IS NOT ACTIVE       */
*   OUXBPTR=ASCBOUXB;               /* ESTABLISH OUXB ADDRESSIBILITY */
         L     OUXBPTR,ASCBOUXB(,ASCBPTR)                          0062
*   ILTIME=RMCTTOD                  /* COMPUTE ELAPSED TIME SINCE  0063
*                                      LAST                          */
*       -OUXBITD;                   /* USER I/O RATE COMPUTATION     */
         L     @07,RMCTTOD(,RMCTPTR)                               0063
         SL    @07,OUXBITD(,OUXBPTR)                               0063
         ST    @07,ILTIME                                          0063
*/*                                                                  */
*/*      FIRST CHECK IF USER I/O MEASUREMENT CONTROL BLOCK           */
*/*      ALREADY EXISTS                                              */
*/*                                                                  */
*                                                                  0064
*   IF OUCBIMCB^=I0 THEN            /* IF TCTIOT COUNTS HAVE       0064
*                                      PREVIOUSLY BEEN MONITORED     */
         L     @11,OUCBIMCB(,OUCBPTR)                              0064
         LTR   @11,@11                                             0064
         BZ    @RF00064                                            0064
*     IF ILTIME<=ICCMNUIN THEN      /* AND USER HAS NOT EXECUTED VERY
*                                      MUCH SINCE THEN               */
         C     @07,ICCMNUIN(,RMCTPTR)                              0065
         BNH   @RT00065                                            0065
*       GOTO IL0END;                /* RETURN IMMEDIATELY            */
*     ELSE                                                         0067
*       ;                           /* IF IMCB DOES EXIST AND ENOUGH
*                                      TIME HAS PASSED GO DIRECTLY TO
*                                      ACCUMULATE TCTIOT COUNTS      */
*/*                                                                  */
*/*      IF IMCB DOES NOT EXIST FOR USER, DETERMINE WHETHER ONE      */
*/*      SHOULD BE INITIALIZED BASED ON USERS MEASURED I/O ACTIVITY  */
*/*      AND LOGICAL CHANNEL ALLOCATION PATTERN                      */
*/*                                                                  */
*                                                                  0068
*   ELSE                            /* OTHERWISE                     */
*     DO;                           /*                               */
         B     @RC00064                                            0068
@RF00064 DS    0H                                                  0069
*       IF ILTIME>ICCMNUIN THEN     /* IF ELAPSED TIME IS LONG ENOUGH*/
         L     @07,ILTIME                                          0069
         C     @07,ICCMNUIN(,RMCTPTR)                              0069
         BNH   @RF00069                                            0069
*         DO;                       /*                               */
*           OUCBIOR                 /* COMPUTE USER I/O REQUEST RATE */
*               =(OUCBIOSM+DAIOSM   /* IN TERMS OF REQUESTS PER    0071
*                                                            @ZA10808*/
*               -OUXBILS)           /* SECOND OF ELAPSED TIME        */
*               *I1000              /*                               */
*               /ILTIME;            /*                               */
         LH    @11,DAIOSM(,ASCBPTR)                                0071
         N     @11,@CF03075                                        0071
         AL    @11,OUCBIOSM(,OUCBPTR)                              0071
         LR    @08,@11                                             0071
         SL    @08,OUXBILS(,OUXBPTR)                               0071
         ST    @11,@TF00001                                        0071
         LR    @11,@08                                             0071
         M     @10,@CF02921                                        0071
         DR    @10,@07                                             0071
         STH   @11,OUCBIOR(,OUCBPTR)                               0071
*           OUXBILS=OUCBIOSM+DAIOSM;/* RESET BASE REQUEST COUNT    0072
*                                                            @ZA10808*/
         L     @07,@TF00001                                        0072
         ST    @07,OUXBILS(,OUXBPTR)                               0072
*           OUXBITD=RMCTTOD;        /* RESET BASE TIME OF DAY        */
         L     @07,RMCTTOD(,RMCTPTR)                               0073
         ST    @07,OUXBITD(,OUXBPTR)                               0073
*         END;                                                     0074
*       IF OUCBIOR>ICCMNIOR THEN    /* IF USER I/O RATE IS HIGH      */
@RF00069 LH    @07,OUCBIOR(,OUCBPTR)                               0075
         CH    @07,ICCMNIOR(,RMCTPTR)                              0075
         BNH   @RF00075                                            0075
*         DO;                       /* GET AND INITIALIZE IMCB       */
*           ILGT0=LENGTH(OUXB);     /* SET UP GETMAIN PARM REGS TO   */
         LA    ILGT0,144                                           0077
*           ILGT1=I0;               /* OBTAIN ONE. INVOKE GETMAIN VIA
*                                      INTERFACE PR                  */
*                                                                  0078
         SLR   ILGT1,ILGT1                                         0078
*           /*********************************************************/
*           /*                                                       */
*           /* ? IRACTLCL EPN(I04)                                   */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0079
*           CALL RMSBI04->IRARMRTN; /* INVOKE I04 SUBRTN             */
         L     @15,RMSBI04(,RMCTPTR)                               0079
         BALR  @14,@15                                             0079
*           IF ILGT15^=I0 THEN      /* IF GETMAIN FAILS,             */
         SLR   @07,@07                                             0080
         CR    ILGT15,@07                                          0080
         BNE   @RT00080                                            0080
*             GOTO IL0END;          /* GIVE UP                       */
*           ELSE                    /* OTHERWISE INITIALIZE IMCB.    */
*             DO;                                                  0082
*               RESPECIFY                                          0083
*                 IMCB BASED(ILGT1);                               0083
*               OUCBIMCB=ILGT1;     /* SAVE IMCB ADDRESS IN OUCB     */
         ST    ILGT1,OUCBIMCB(,OUCBPTR)                            0084
*               IMCBIMCB='IMCB';    /* PUT IN IMCB MNEUMONIC         */
         MVC   IMCBIMCB(4,ILGT1),@CC02927                          0085
*               IMCBLBGN=           /* INDICATE IMCB LCH ENTRIES   0086
*                                      EMPTY                         */
*                   ADDR(IMCB)+LENGTH(OUXB);                       0086
         LA    @08,144                                             0086
         ALR   @08,ILGT1                                           0086
         ST    @08,IMCBLBGN(,ILGT1)                                0086
*               IMCBLEND=IMCBLBGN   /* INIDICATE IMCBLCH ENTRIES   0087
*                                      EMPTY                         */
*                   -LENGTH(IMCBLCH);                              0087
         SL    @08,@CF00164                                        0087
         ST    @08,IMCBLEND(,ILGT1)                                0087
*               IMCBRV=I0;          /* ZERO RECOMMENDATION SAVE AREA */
         STH   @07,IMCBRV(,ILGT1)                                  0088
*               RESPECIFY                                          0089
*                 IMCB BASED(IMCBPTR);                             0089
*             END;                                                 0090
*         END;                      /*                               */
*       ELSE                        /* IF IMCB WAS NOT INITIALIZED   */
*         GOTO IL0END;              /* RETURN TO CALLER              */
*     END;                          /*                               */
*   RESPECIFY                                                      0094
*    (ILGT0,                                                       0094
*     ILGT1,                                                       0094
*     ILGT15) UNRESTRICTED;                                        0094
@RC00064 DS    0H                                                  0095
*                                                                  0095
*/*                                                                  */
*/*      CLEAR LCH EXCP COUNT ACCUMULATORS                           */
*/*                                                                  */
*                                                                  0095
*   DO RLCTPTR=RMCTRLCT             /* SET UP ADDRESSIBILITY TO      */
*         TO ICCLCLST BY RLCHLEN;   /* EACH LCH TABLE ENTRY          */
         L     RLCTPTR,RMCTRLCT(,RMCTPTR)                          0095
         B     @DE00095                                            0095
@DL00095 DS    0H                                                  0096
*     RLCTUMWA=I0;                  /* CLEAR EXCP COUNT ACCUMULATOR  */
         SLR   @07,@07                                             0096
         ST    @07,RLCTUMWA(,RLCTPTR)                              0096
*   END;                                                           0097
*                                                                  0097
         AH    RLCTPTR,RLCHLEN                                     0097
@DE00095 C     RLCTPTR,ICCLCLST(,RMCTPTR)                          0097
         BNH   @DL00095                                            0097
*/*                                                                  */
*/*      ACCUMULATE USER EXCP COUNTS BY LOGICAL CHANNEL              */
*/*                                                                  */
*                                                                  0098
*   RESPECIFY                                                      0098
*    (ILLUPTR,                                                     0098
*     ILCTPTR,                                                     0098
*     ILSCTR) RESTRICTED;                                          0098
*   RESPECIFY                                                      0099
*     RLCT BASED                    /* BASE RLCT ON UCB LCH INDEX    */
*         (EVAL(RMCTRLCT+UCBLCI*RLCHLEN));                         0099
*   RESPECIFY                                                      0100
*     UCBOB BASED(TCTUCBP);         /* BASE UCB ON TCT DD ENTRY UCB
*                                      POINTER                       */
*   TCBPTR=ASXBLTCB;                /* POINT TO LOWEST USER TCB      */
         L     @11,ASXBPTR(,ASCBPTR)                               0101
         L     TCBPTR,ASXBLTCB(,@11)                               0101
*   TCBPTR=TCBJSTCB;                /* POINT TO JOB STEP TCB         */
         L     TCBPTR,TCBJSTCB(,TCBPTR)                            0102
*   IF TCBTCT^=I0&                  /* IF TCT AND                    */
*       TCTIOTBL^=I0 THEN           /* TCTIOT EXIST                  */
         SLR   @08,@08                                             0103
         L     @11,TCBTCT(,TCBPTR)                                 0103
         CR    @11,@08                                             0103
         BE    @RF00103                                            0103
         L     @11,TCTIOTBL(,@11)                                  0103
         CR    @11,@08                                             0103
         BE    @RF00103                                            0103
*     DO;                                                          0104
*       ILLUPTR=ADDR(TCTTIOT)+I8;   /* POINT TO FIRST TCT DD LOOKUP
*                                      TABLE ENTRY                   */
         AL    @11,@CF00164                                        0105
         LR    ILLUPTR,@11                                         0105
*       DO WHILE TCTIOTSD^=I0;      /* LOOK AT EACH DD ENTRY         */
         B     @DE00106                                            0106
@DL00106 DS    0H                                                  0107
*         ILCTPTR=ADDR(TCTTIOT)     /* POINT TO CURRENT DO ENTRY     */
*             +TCTIOTSD;                                           0107
         L     @08,TCBTCT(,TCBPTR)                                 0107
         LH    ILCTPTR,TCTIOTSD(,ILLUPTR)                          0107
         AL    ILCTPTR,TCTIOTBL(,@08)                              0107
*         IF TCTSCTR^='FF'X THEN    /* LOOK AT EACH NON-SYSIN DD   0108
*                                      ENTRY                         */
         CLI   TCTSCTR(ILCTPTR),255                                0108
         BE    @RF00108                                            0108
*           DO ILSCTR=TCTSCTR       /* LOOK AT EACH DEVICE ASSOCIATED*/
*                 TO I1 BY-I1;      /* WITH DATA SET                 */
         SLR   ILSCTR,ILSCTR                                       0109
         IC    ILSCTR,TCTSCTR(,ILCTPTR)                            0109
         B     @DE00109                                            0109
@DL00109 DS    0H                                                  0110
*             IF TCTUCBP^=I0 THEN   /* IF DATA SET ASSOCIATED WITH A */
         LH    @08,TCTUCBP(,ILCTPTR)                               0110
         N     @08,@CF03075                                        0110
         LTR   @08,@08                                             0110
         BZ    @RF00110                                            0110
*               RLCTUMWA=RLCTUMWA   /* SPECIFIC DEVICE INCLUDE EXCP  */
*                   +TCTDCTR;       /* COUNT IN THE LCH ACCUMULATION */
         L     @11,RMCTRLCT(,RMCTPTR)                              0111
         SLR   @10,@10                                             0111
         IC    @10,UCBLCI(,@08)                                    0111
         MH    @10,RLCHLEN                                         0111
         LR    @08,@11                                             0111
         ALR   @08,@10                                             0111
         ALR   @11,@10                                             0111
         L     @11,RLCTUMWA(,@11)                                  0111
         AL    @11,TCTDCTR(,ILCTPTR)                               0111
         ST    @11,RLCTUMWA(,@08)                                  0111
*             ILCTPTR=ILCTPTR+I8;   /* POINT TO NEXT DEVICE ENTRY    */
@RF00110 AL    ILCTPTR,@CF00164                                    0112
*           END;                                                   0113
         BCTR  ILSCTR,0                                            0113
@DE00109 LTR   ILSCTR,ILSCTR                                       0113
         BP    @DL00109                                            0113
*         ILLUPTR=ILLUPTR+I4;       /* POINT TO NEXT DD ENTRY IN     */
@RF00108 AL    ILLUPTR,@CF00050                                    0114
*       END;                        /* LOOKUP TABLE                  */
@DE00106 LH    @08,TCTIOTSD(,ILLUPTR)                              0115
         LTR   @08,@08                                             0115
         BNZ   @DL00106                                            0115
*     END;                                                         0116
*   RESPECIFY                                                      0117
*    (ILLUPTR,                                                     0117
*     ILCTPTR,                                                     0117
*     ILSCTR) UNRESTRICTED;                                        0117
@RF00103 DS    0H                                                  0118
*                                                                  0118
*/*                                                                  */
*/*      INITIALIZE USER LOGICAL CHANNEL TABLE ENTRIES               */
*/*                                                                  */
*                                                                  0118
*   RESPECIFY                                                      0118
*     IMCBLPTR RESTRICTED;                                         0118
*   RESPECIFY                                                      0119
*     RLCT BASED(RLCTPTR);                                         0119
*   IMCBPTR=OUCBIMCB;               /* INITIALIZE IMCB BASE REGISTER */
         L     IMCBPTR,OUCBIMCB(,OUCBPTR)                          0120
*   IMCBLPTR=IMCBLEND;              /* POINT TO LAST IMCB LCH ENTRY  */
         L     IMCBLPTR,IMCBLEND(,IMCBPTR)                         0121
*   ILCBEND=ADDR(IMCBLCHT)          /* SET LIMIT TO IMCB LCH TABLE   */
*       +LENGTH(IMCBLCH);           /* SIZE                          */
         LA    @11,IMCBLCHT(,IMCBPTR)                              0122
         AL    @11,@CF00164                                        0122
         ST    @11,ILCBEND                                         0122
*   IMCBSLCB=I0;                    /* CLEAR SIGNIFICANT LCH USAGE 0123
*                                      BIT PATTERN                   */
         SLR   @11,@11                                             0123
         ST    @11,IMCBSLCB(,IMCBPTR)                              0123
*   DO RLCTPTR=ICCLCLST             /* LOOK AT EACH LOGICAL CHANNEL  */
*         TO RMCTRLCT BY-RLCHLEN;   /* STARTING FROM END OF TABLE    */
         L     RLCTPTR,ICCLCLST(,RMCTPTR)                          0124
         B     @DE00124                                            0124
@DL00124 DS    0H                                                  0125
*     RLCTX=RLCTPTR-RMCTRLCT;       /* COMPUTE OFFSET OF RLCT ENTRY  */
         LR    RLCTX,RLCTPTR                                       0125
         SL    RLCTX,RMCTRLCT(,RMCTPTR)                            0125
*/*                                                                  */
*/*        IF LOGICAL CHANNEL ALREADY HAS AN ENTRY IN THE USER       */
*/*        IMCB,  COMPUTE USER EXCP RATES ON THE LCH AND --          */
*/*                                                                  */
*                                                                  0126
*     IF RLCTX=IMCBLCTX THEN        /* IF LCH IS REPRESENTED IN THE  */
         CH    RLCTX,IMCBLCTX(,IMCBLPTR)                           0126
         BNE   @RF00126                                            0126
*       DO;                         /* IMCB THEN                     */
*         ILUR=                     /* LCH EXCP RATE EQUALS          */
*             (RLCTUMWA             /* RECENT EXCP                   */
*             -IMCBBLCC)            /* COUNT                         */
*             *I1000                /* TIMES 1000 FOR EXCPS / SEC    */
*             /ILTIME;              /* DIVIDED BY ELAPSED TIME       */
         L     @11,RLCTUMWA(,RLCTPTR)                              0128
         LR    @08,@11                                             0128
         SL    @08,IMCBBLCC(,IMCBLPTR)                             0128
         LR    @11,@08                                             0128
         M     @10,@CF02921                                        0128
         D     @10,ILTIME                                          0128
         LR    ILUR,@11                                            0128
*         IMCBBLCC=RLCTUMWA;        /* SAVE NEW BASE EXCP COUNT      */
         L     @08,RLCTUMWA(,RLCTPTR)                              0129
         ST    @08,IMCBBLCC(,IMCBLPTR)                             0129
*         IMCBLCUR=ILUR;            /* SAVE NEWLY COMPUTED RATE      */
         STH   ILUR,IMCBLCUR(,IMCBLPTR)                            0130
*/*                                                                  */
*/*      -- IF USER IS A HEAVY USER OF THE LCH SET LCH BIT IN THE    */
*/*      IMCB SIGNIFICANT LCH USE BIT PATTERN                        */
*/*                                                                  */
*                                                                  0131
*         IF ILUR*I100              /* IF THIS USERS SHARE OF      0131
*                                      REQUESTS                      */
*             /MAX(RLCTRQRT,I1)     /* TO THIS LCH IS GREATER THAT   */
*             >ICCSIGUP THEN        /* A SIGNIFICANT PERCENTAGE,     */
         LR    @08,ILUR                                            0131
         MH    @08,@CH02918                                        0131
         LH    @11,RLCTRQRT(,RLCTPTR)                              0131
         LA    @10,1                                               0131
         CR    @11,@10                                             0131
         BNL   *+6
         LR    @11,@10                                             0131
         LR    @10,@08                                             0131
         ST    @11,@TF00001                                        0131
         SRDA  @10,32                                              0131
         D     @10,@TF00001                                        0131
         CH    @11,ICCSIGUP(,RMCTPTR)                              0131
         BNH   @RF00131                                            0131
*           IMCBSLCB=IMCBSLCB       /* SET LCH BIT ON IN IMCB        */
*               ×RLCTBIT;           /* SIGNIFICANT LCH USE PATTERN   */
         OC    IMCBSLCB(4,IMCBPTR),RLCTBIT(RLCTPTR)                0132
*         IMCBLPTR=IMCBLPTR         /* POINT TO NEXT IMCB LCH ENTRY  */
*             -LENGTH(IMCBLCH);     /*                               */
@RF00131 SL    IMCBLPTR,@CF00164                                   0133
*       END;                                                       0134
*                                                                  0134
*/*                                                                  */
*/*      OTHERWISE                                                   */
*/*        IF THE LCH DOES NOT HAVE AN ENTRY IN THE IMCB AND THE     */
*/*        USER IS ACTIVE ON THE LCH, CREATE A NEW IMCB LCH ENTRY    */
*/*                                                                  */
*                                                                  0135
*     ELSE                                                         0135
*       IF RLCTUMWA^=I0             /* IF USER IS ACTIVE ON THIS LCH */
*           &IMCBLBGN>=ILCBEND      /* AND THERE IS ROOM IN THE      */
*         THEN                      /* IMCB,                         */
         B     @RC00126                                            0135
@RF00126 L     @08,RLCTUMWA(,RLCTPTR)                              0135
         LTR   @08,@08                                             0135
         BZ    @RF00135                                            0135
         L     @08,IMCBLBGN(,IMCBPTR)                              0135
         C     @08,ILCBEND                                         0135
         BL    @RF00135                                            0135
*         DO;                       /*                               */
*           IMCBLBGN=IMCBLBGN       /* COMPUTE NEW ADDRESS OF FIRST  */
*               -LENGTH(IMCBLCH);   /* LCH ENTRY                     */
         SL    @08,@CF00164                                        0137
         ST    @08,IMCBLBGN(,IMCBPTR)                              0137
*           IF IMCBLPTR>            /* IF NEW ENTRY GOES INTO MIDDLE */
*               IMCBLBGN THEN       /* OF IMCB LCH TABLE             */
         CR    IMCBLPTR,@08                                        0138
         BNH   @RF00138                                            0138
*             DO;                   /* MOVE PRIOR ENTRIES TO MAKE  0139
*                                      ROOM                          */
*               RESPECIFY                                          0140
*                 ILMVCL RESTRICTED;                               0140
*               ILMVCL=IMCBLPTR     /* COMPUTE LENGTH OF MOVE        */
*                   -ADDR(IMCBLCHT)+I7;                            0141
         LA    ILMVCL,IMCBLCHT(,IMCBPTR)                           0141
         LCR   ILMVCL,ILMVCL                                       0141
         ALR   ILMVCL,IMCBLPTR                                     0141
         AL    ILMVCL,@CF00450                                     0141
*               GENERATE(EX    ILMVCL,ILMOVE);/* EXECUTE             */
         EX    ILMVCL,ILMOVE
*               GENERATE DATA REFS(IMCBLCHT);                      0143
*               RESPECIFY                                          0144
*                 ILMVCL UNRESTRICTED;                             0144
*             END;                                                 0145
*           IMCBLCTX=RLCTX;         /* SAVE RLCT OFFSET AS ID        */
@RF00138 STH   RLCTX,IMCBLCTX(,IMCBLPTR)                           0146
*           IMCBBLCC=RLCTUMWA;      /* SAVE BASE EXCP COUNTS         */
         L     @11,RLCTUMWA(,RLCTPTR)                              0147
         ST    @11,IMCBBLCC(,IMCBLPTR)                             0147
*           IMCBLCUR=I0;            /* ZERO LCH USAGE RATE           */
         SLR   @11,@11                                             0148
         STH   @11,IMCBLCUR(,IMCBLPTR)                             0148
*           IMCBLPTR=IMCBLPTR       /* POINT TO NEXT IMCB LCH ENTRY  */
*               -LENGTH(IMCBLCH);                                  0149
         SL    IMCBLPTR,@CF00164                                   0149
*         END;                                                     0150
*   END;                                                           0151
@RF00135 DS    0H                                                  0151
@RC00126 LH    @11,RLCHLEN                                         0151
         LCR   @11,@11                                             0151
         ALR   RLCTPTR,@11                                         0151
@DE00124 C     RLCTPTR,RMCTRLCT(,RMCTPTR)                          0151
         BNL   @DL00124                                            0151
*   ASCBPTR=OUCBASCB;               /* ESTABLISH ASCB BASE ADDRESS   */
         L     ASCBPTR,OUCBASCB(,OUCBPTR)                          0152
*   OUXBPTR=ASCBOUXB;               /* ESTABLISH OUXB BASE ADDRESS   */
         L     OUXBPTR,ASCBOUXB(,ASCBPTR)                          0153
*   OUXBITD=RMCTTOD;                /* SAVE NEW RATE BASE TIME       */
         L     @11,RMCTTOD(,RMCTPTR)                               0154
         ST    @11,OUXBITD(,OUXBPTR)                               0154
*   OUXBILS=OUCBIOSM+DAIOSM;        /* RESET I.O SERVICE BASE COUNT
*                                                            @ZA10808*/
         LH    @11,DAIOSM(,ASCBPTR)                                0155
         N     @11,@CF03075                                        0155
         AL    @11,OUCBIOSM(,OUCBPTR)                              0155
         ST    @11,OUXBILS(,OUXBPTR)                               0155
*   RESPECIFY                                                      0156
*     IMCBLPTR UNRESTRICTED;                                       0156
*IL0END:                            /* IL0 MONITORING BYPASS POINT   */
*/*   ? IRACTLEP ENN(IL0)                                            */
*                                                                  0157
*   GEN CODE EXIT NOSEQFLOW;                                       0157
IL0END   DS    0H                                                  0157
*                                  /*
         LM    @00,@14,128+000(@13)    RELOAD INVOKING RTNE REGS
         BR    RTNREG                  RETURN TO INVOKER
*                                                                  0158
*/********************************************************************/
*/*                                                                  */
*/* ENTRY POINT NAME = IRARMIL1                                      */
*/*                                                                  */
*/* DESCRIPTIVE NAME = I/O LOAD BALANCING LOGICAL CHANNEL            */
*/*                    UTILIZATION MONITORING                        */
*/*                                                                  */
*/* FUNCTION = COMPUTE LOGICAL CHANNEL UTILIZATION VALUES FOR I/O    */
*/*            LOAD BALANCING AND PAGE REPLACEMENT ALGORITHMS AND    */
*/*            DEVICE ALLOCATION SYSEVENT.  INVOKE USER I/O          */
*/*            MONITORING FOR HEAVY USERS OF OUT OF BALANCE LOGICAL  */
*/*            CHANNELS IS THE USER I/O PROFILE IS OUT OF DATE       */
*/*                                                                  */
*/* CALLER = IRARMCTL - SRM CONTROL MODULE                           */
*/*            TIME QUEUE PROCESSOR                                  */
*/*                                                                  */
*/* INPUT = R2 = RMCT ADDRESS                                        */
*/*         R3 = RRPA ADDRESS                                        */
*/*                                                                  */
*/* OUTPUT = NONE                                                    */
*/*                                                                  */
*/********************************************************************/
*/*   ? IRACTLEP EPN(IL1)                                            */
*                                                                  0158
*   DCL                                                            0158
*     IRARMIL1 ENTRY INT;                                          0158
*   GEN CODE REFS(RRPAEPA) DEFS(IRARMIL1) NOEXIT SEQFLOW;          0159
*                                  /*
         ENTRY IRARMIL1
IRARMIL1 DS    0H
         STM   @00,@14,000+000(@13)    SAVE INVOKING RTNE REGS
         ST    @01,RRPAEPA(,RRPAPTR)   INDICATE NEW CURRENT RMEP
         LA    @12,IRARMIL1-@PSTART    DISPLACEMENT TO MODULE BASE
         LCR   @12,@12                 NEGATIVE FOR SUBTRACT
         ALR   @12,@15                 SETUP MODULE ADDRESSABILITY
*   IF RMPTIOC=I0×                  /* IF I/O LOAD BALANCING IS OFF  */
*       (SMCAOPT2='0'B              /* OR USER DATA SET ACTIVITY     */
*       &SMCADSA='0'B) THEN         /* STATISTICS ARE NOT BEING KEPT */
         L     @07,RMPTIOC(,RMCTPTR)                               0160
         LTR   @07,@07                                             0160
         BZ    @RT00160                                            0160
         L     @07,CVTPTR                                          0160
         L     @07,CVTSMCA(,@07)                                   0160
         TM    SMCAOPT2(@07),B'01010000'                           0160
         BNZ   @RF00160                                            0160
@RT00160 DS    0H                                                  0161
*     ICTIOL='0'B;                  /* SET LOAD BALANCING FLAG OFF   */
         NI    ICTIOL(RMCTPTR),B'10111111'                         0161
*   ELSE                            /* OTHERWISE                     */
