*/********************************************************************/
*/*                                                                  */
*/*      DEVRULE4 - DEVALLOC INTERNAL SUBROUTINE             @Z30WIAW*/
*/*                                                                  */
*/*      FUNCTION -                                                  */
*/*        FROM LIST OF EQUAL CANDIDATES CHOOSE ONE AT RANDOM        */
*/*                                                                  */
*/********************************************************************/
*                                                                  0496
*DEVRULE4:                                                         0496
*   PROC OPTIONS(NOSAVE);           /*                       @Z30WIAW*/
DEVRULE4 DS    0H                                                  0497
*   IDX=IDXST                       /* CHOOSE CANDIDATE FROM PRIMARY */
*       +ABS((DEVRV/CON4096*TWO)    /* LIST AT POSITION DETERMINED   */
*       //(IDXEND-IDXST+TWO));      /* FROM CURRENT TOD CLOCK        */
         L     IDX,DEVRV(,RMCTPTR)                                 0497
         SRL   IDX,12                                              0497
         ALR   IDX,IDX                                             0497
         LR    @15,IDXEND                                          0497
         SLR   @15,IDXST                                           0497
         AL    @15,@CF00188                                        0497
         LR    @11,IDX                                             0497
         SLR   @10,@10                                             0497
         DR    @10,@15                                             0497
         ALR   @10,IDXST                                           0497
         LR    IDX,@10                                             0497
*   JDXPTR=IDXPTR;                  /* MOVE CANDIDATE TO SECONDARY   */
         LH    @15,IDXPTR(,IDX)                                    0498
         STH   @15,JDXPTR(,JDX)                                    0498
*   JDX=JDX+IDXLEN;                 /* LIST                          */
         ALR   JDX,IDXLEN                                          0499
*   END DEVRULE4;                   /*                       @Z30WIAW*/
@EL00007 DS    0H                                                  0500
@EF00007 DS    0H                                                  0500
@ER00007 BR    @14                                                 0500
*                                                                  0501
*/********************************************************************/
*/*                                                                  */
*/*      DEVRULE5 - DEVALLOC INTERNAL SUBROUTINE             @Z30WIAW*/
*/*                                                                  */
*/*      FUNCTION - INSURE THAT IF POSSIBLE SELECTED CANDIDATE       */
*/*        HAS NOT BEEN PREVIOUSLY ALLOCATED TO SAME USER.           */
*/*        IF SO, FIND A CANDIDATE THAT HAS NOT BEEN         @ZM43099*/
*/*        ALLOCATED TO SAME USER.  IF NONE EXIST, RECREATE  @ZM43099*/
*/*        THE ORIGINAL LIST AND PICK ONE CANDIDATE AT       @ZM43099*/
*/*        RANDOM TO AVOID CLUMPING TO ONE CHANNEL & DEVICE. @ZM43099*/
*/*                                                                  */
*/********************************************************************/
*                                                                  0501
*DEVRULE5:                                                         0501
*   PROC OPTIONS(NOSAVE);           /*                       @Z30WIAW*/
DEVRULE5 DS    0H                                                  0502
*   IDX=IDXST;                      /* POINT TO SELECTED CANDIDATE   */
         LR    IDX,IDXST                                           0502
*   IF UCB3DACC='1'B                /* IF CANDIDATE IS DA DEVICE     */
*       &DEVLCB^=ZERO               /* AND USER HAS OTHER DEVICES  0503
*                                      ALLOCATED             @Z40BPEB*/
*       &DEVDUP='0'B THEN           /* AND SUCH DEVICES HAVE NOT BEEN*/
         LH    @15,IDXPTR(,IDX)                                    0503
         AL    @15,DEVST1                                          0503
         L     @15,UCBPTR2(,@15)                                   0503
         TM    UCB3DACC(@15),B'00100000'                           0503
         BNO   @RF00503                                            0503
         L     @08,DEVLCB                                          0503
         LTR   @08,@08                                             0503
         BZ    @RF00503                                            0503
         TM    DEVDUP,B'10000000'                                  0503
         BNZ   @RF00503                                            0503
*     DO;                           /* REMOVED FROM THE INPUT LIST,  */
*       DEVSAVE=ADDR(UCBOB);        /* GET ADDRESS OF CANDIDATE      */
         LR    DEVSAVE,@15                                         0505
*       DO DEVX2=DEVST2 TO          /* COMPARE CANDIDATES ADDRESS TO */
*             DEVEND2 BY DEVXLEN    /* EACH ADDRESS IN ALLOCATED     */
*             WHILE DEVSAVE^=       /* UCB LIST                      */
*             DEVUCB2;              /*                               */
         L     DEVX2,DEVST2                                        0506
         B     @DE00506                                            0506
@DL00506 C     DEVSAVE,DEVUCB2(,DEVX2)                             0506
         BE    @DC00506                                            0506
*       END;                                                       0507
         AL    DEVX2,DEVXLEN                                       0507
@DE00506 C     DEVX2,DEVEND2                                       0507
         BNH   @DL00506                                            0507
@DC00506 DS    0H                                                  0508
*       IF DEVX2<=DEVEND2 THEN      /* IF THERE IS A MATCH,          */
         C     DEVX2,DEVEND2                                       0508
         BH    @RF00508                                            0508
*         DO;                       /* REBUILD PRIMARY LIST TO     0509
*                                      CONTAIN                       */
*           DO DEVX1=DEVST1 TO      /* NO PREVIOUSLY ALLOCATED UCBS. */
*                 DEVEND1 BY DEVXLEN;/* LOOK AT EACH CANDIDATE       */
         L     @15,DEVST1                                          0510
         B     @DE00510                                            0510
@DL00510 DS    0H                                                  0511
*             DEVSAVE=DEVUCB1;      /* SAVE ADDRESS OF CANDIDATE     */
         L     DEVSAVE,DEVUCB1(,@15)                               0511
*             DO DEVX2=DEVST2 TO    /* COMPARE CANDIDATES ADDRESS    */
*                   DEVEND2 BY DEVXLEN/* TO EACH ADDRESS IN THE      */
*                   WHILE DEVSAVE^= /* ALLOCATED UCBLIST             */
*                   DEVUCB2;        /*                               */
         L     DEVX2,DEVST2                                        0512
         B     @DE00512                                            0512
@DL00512 C     DEVSAVE,DEVUCB2(,DEVX2)                             0512
         BE    @DC00512                                            0512
*             END;                  /*                               */
         AL    DEVX2,DEVXLEN                                       0513
@DE00512 C     DEVX2,DEVEND2                                       0513
         BNH   @DL00512                                            0513
@DC00512 DS    0H                                                  0514
*             IF DEVX2>DEVEND2 THEN /* IF THERE IS NOT A MATCH       */
         C     DEVX2,DEVEND2                                       0514
         BNH   @RF00514                                            0514
*               DO;                 /*                               */
*                 JDXPTR=           /* COMPUTE INDEX INTO DEV LIST   */
*                     DEVX1-DEVST1;                                0516
         L     @15,DEVX1                                           0516
         SL    @15,DEVST1                                          0516
         STH   @15,JDXPTR(,JDX)                                    0516
*                 JDX=JDX+IDXLEN;   /* POINT TO NEXT ENTRY IN SEC LST*/
         ALR   JDX,IDXLEN                                          0517
*               END;                                               0518
*           END;                    /*                               */
@RF00514 L     @15,DEVX1                                           0519
         AL    @15,DEVXLEN                                         0519
@DE00510 ST    @15,DEVX1                                           0519
         C     @15,DEVEND1                                         0519
         BNH   @DL00510                                            0519
*           DEVDUP='1'B;            /* ALLOCATED DEVICES HAVE BEEN 0520
*                                      REMOVED FROM LIST.... @ZM43099*/
         OI    DEVDUP,B'10000000'                                  0520
*           IF JDX^=JDXST THEN      /* SOME UNALLOCATED CANDIDATES...*/
         CR    JDX,JDXST                                           0521
         BE    @RF00521                                            0521
*             DEVEPX=DEVEPRST;      /* APPLY ALL RULES TO SHORT LIST */
         L     @15,DEVEPRST                                        0522
         ST    @15,DEVEPX                                          0522
*           ELSE                    /* ALL CANDIDATES HAVE   @ZM43099*/
*             DO;                   /* ALREADY BEEN ALLOC.   @ZM43099*/
         B     @RC00521                                            0523
@RF00521 DS    0H                                                  0524
*               DEVEPX=ADDR(DEVEPTAB(3));/* PREPARE RULE 4   @ZM43099*/
         LA    @15,DEVEPTAB+8                                      0524
         ST    @15,DEVEPX                                          0524
*               DO DEVX1=DEVST1 TO DEVEND1/* USING ENTIRE    @ZM43099*/
*                     BY DEVXLEN;   /* ORIGINAL CANDIDATE    @ZM43099*/
         L     @15,DEVST1                                          0525
         B     @DE00525                                            0525
@DL00525 DS    0H                                                  0526
*                 JDXPTR=DEVX1-DEVST1;/* LIST TO FORM THE    @ZM43099*/
         LR    @08,@15                                             0526
         SL    @08,DEVST1                                          0526
         STH   @08,JDXPTR(,JDX)                                    0526
*                 JDX=JDX+IDXLEN;   /* NEW PRIMARY LIST.     @ZM43099*/
         ALR   JDX,IDXLEN                                          0527
*               END;                /* LIST IS DONE.         @ZM43099*/
         AL    @15,DEVXLEN                                         0528
@DE00525 ST    @15,DEVX1                                           0528
         C     @15,DEVEND1                                         0528
         BNH   @DL00525                                            0528
*             END;                  /* READY FOR RULE 4      @ZM43099*/
*         END;                                                     0530
*     END;                                                         0531
*   END DEVRULE5;                   /*                       @Z30WIAW*/
@EL00008 DS    0H                                                  0532
@EF00008 DS    0H                                                  0532
@ER00008 BR    @14                                                 0532
