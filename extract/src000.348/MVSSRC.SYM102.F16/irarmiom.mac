         ST    @01,RRPAEPA(,RRPAPTR)   INDICATE NEW CURRENT RMEP
         LR    @13,@15                 ROUTINE SAVE AREA ADDRESS
         BALR  @15,0                   SETUP ENTRY ADDRESSABILITY
         LA    @12,*-@PSTART           DISPLACEMENT TO MODULE BASE
         LCR   @12,@12                 NEGATIVE FOR SUBTRACT
         ALR   @12,@15                 SETUP MODULE ADDRESSABILITY
*/*                                                                  */
*/*      FREE IMCB                                                   */
*/*                                                                  */
*                                                                  0246
*   RESPECIFY                       /* RESTRICT FREEMAIN PARAMETER   */
*    (ILGT0,                        /* REGISTERS                     */
*     ILGT1,                                                       0246
*     ILGT15) RESTRICTED;                                          0246
*   ILGT0=LENGTH(OUXB);             /* IMCB IS SAME SIZE AS OUXB     */
         LA    ILGT0,144                                           0247
*   ILGT1=OUCBIMCB;                 /* GET ADDRESS OF BLOCK TO BE  0248
*                                      FREED                         */
         L     ILGT1,OUCBIMCB(,OUCBPTR)                            0248
*   IF ILGT1^=I0 THEN               /* IF IMCB EXISTS                */
         SLR   @07,@07                                             0249
         CR    ILGT1,@07                                           0249
         BE    @RF00249                                            0249
*     DO;                           /*                               */
*       OUCBIMCB=I0;                /* FORGET OLD IMCB AND FREE IT   */
*                                                                  0251
         ST    @07,OUCBIMCB(,OUCBPTR)                              0251
*       /*************************************************************/
*       /*                                                           */
*       /* ? IRACTLCL EPN(I04)                                       */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0252
*       CALL RMSBI04->IRARMRTN;     /* INVOKE I04 SUBRTN             */
         L     @15,RMSBI04(,RMCTPTR)                               0252
         BALR  @14,@15                                             0252
*     END;                                                         0253
*   RESPECIFY                       /* PROGRAM                       */
*    (ILGT0,                                                       0254
*     ILGT1,                                                       0254
*     ILGT15) UNRESTRICTED;                                        0254
*                                                                  0254
@RF00249 DS    0H                                                  0255
*/*   ? IRACTLEP ENN(IL4)                                            */
*                                                                  0255
*   GEN CODE EXIT NOSEQFLOW;                                       0255
*                                  /*
         MVC   RRPAEPA(4,RRPAPTR),064+060(@13) RESTORE PREV RMEP
         LM    @00,@14,064+000(@13)    RELOAD INVOKING RTNE REGS
         BR    RTNREG                  RETURN TO INVOKER
*                                                                  0256
*/********************************************************************/
*/*                                                                  */
*/*      LCHUSE - IRARMIOM INTERNAL SUBROUTINE                       */
*/*                                                                  */
*/*      FUNCTION - FOR EACH LOGICAL CHANNEL FLAGGED IN INPUT        */
*/*        BIT PATTERN COMPUTE                                       */
*/*            1) LOGICAL CHANNEL UTILIZATION                        */
*/*            2) LOGICAL CHANNEL REQUEST RATE                       */
*/*            3) I/O LOAD BALANCING RECOMMENDATION VALUE            */
*/*               COMPUTATION LOGICAL CHANNEL UTILIZATION FACTOR     */
*/*                                                                  */
*/*      INPUT -                                                     */
*/*        1) ILLCHPT - BIT PATTERN OF LCHS TO CHECK                 */
*/*                                                                  */
*/********************************************************************/
*/*                                                                  */
*                                                                  0256
*LCHUSE:                                                           0256
*   PROC OPTIONS(NOSAVE);                                          0256
@EL00001 DS    0H                                                  0256
@EF00001 DS    0H                                                  0256
@ER00001 BR    @14                                                 0256
LCHUSE   DS    0H                                                  0257
*   RESPECIFY                                                      0257
*    (LCHPTR,                                                      0257
*     RTNREG) RESTRICTED;                                          0257
*   ILTIM=RMCTTOD-ICVLUTBT;         /* TIME DELTA SINCE LAST LCHUSE
*                                      COMPUTATION.          @Z40BPEB*/
         L     @01,RMCTTOD(,RMCTPTR)                               0258
         SL    @01,ICVLUTBT(,RMCTPTR)                              0258
         ST    @01,ILTIM                                           0258
*   LCHPTR=CVTILCH;                 /* POINT TO FIRST IOS LCH ENTRY  */
         L     @01,CVTPTR                                          0259
         L     LCHPTR,CVTILCH(,@01)                                0259
*   ILLCBPT=I0;                     /* CLEAR LCH IMBALANCE BIT     0260
*                                      PATTERN                       */
         SLR   @01,@01                                             0260
         ST    @01,ILLCBPT                                         0260
*   ICVOLCBT=I0;                    /* CLEAR OVERUTILIZED BIT PATTERN*/
         ST    @01,ICVOLCBT(,RMCTPTR)                              0261
*   ICVULCBT=I0;                    /* CLEAR UNDERUTILIZED BIT     0262
*                                      PATTERN                       */
         ST    @01,ICVULCBT(,RMCTPTR)                              0262
*   DO RLCTPTR=RMCTRLCT             /* LOOK AT EACH LOGICAL CHANNEL  */
*         TO ICCLCLST               /* ENTRY                         */
*         BY RLCHLEN;               /*                               */
         L     RLCTPTR,RMCTRLCT(,RMCTPTR)                          0263
         B     @DE00263                                            0263
@DL00263 DS    0H                                                  0264
*     IF(ILLCHPT&RLCTBIT)           /* COMPUTE UTILIZATION IF LCH    */
*         ^=I0 THEN                 /* FLAGGED IN INPUT PATTERN      */
         SLR   @01,@01                                             0264
         L     @11,ILLCHPT                                         0264
         N     @11,RLCTBIT(,RLCTPTR)                               0264
         CLR   @11,@01                                             0264
         BE    @RF00264                                            0264
*       DO;                                                        0265
*                                                                  0265
*/*                                                                  */
*/*      COMPUTE LCH UTILIZATION                                     */
*/*                                                                  */
*                                                                  0266
*         ILREQ=LCHTOTCT            /* COMPUTE RECENT REQUESTS TO    */
*             -RLCTRQCT;            /* LOGICAL CHANNEL               */
         LH    ILREQ,LCHTOTCT(,LCHPTR)                             0266
         N     ILREQ,@CF03075                                      0266
         LH    @08,RLCTRQCT(,RLCTPTR)                              0266
         N     @08,@CF03075                                        0266
         SLR   ILREQ,@08                                           0266
*         IF ILREQ<=I0 THEN         /* IF NO REQS WERE MADE  @ZM43081*/
         CLR   ILREQ,@01                                           0267
         BH    @RF00267                                            0267
*           DO;                     /* OR WRAP OF LCHTOTCT           */
*             RLCTUTIL=I0;          /* UTILIZATION = ZERO            */
         STH   @01,RLCTUTIL(,RLCTPTR)                              0269
*             RLCTRQRT=I0;          /* REQUEST RATE = ZERO           */
         STH   @01,RLCTRQRT(,RLCTPTR)                              0270
*           END;                    /*                               */
*         ELSE                      /* OTHERWISE IF THERE            */
*           DO;                     /* WAS LCH ACTIVITY              */
         B     @RC00267                                            0272
@RF00267 DS    0H                                                  0273
*             RLCTUTIL=             /* LCH UTILIZATION EQUALS        */
*                 (LCHLGBCT         /*                               */
*                 +LCHPYBCT         /* RECENT DELAY COUNTS           */
*                 -RLCTDDCT         /*                               */
*                 -RLCTPDCT)        /*                               */
*                 *I100             /* TIMES 100 FOR PERCENT         */
*                 /ILREQ;           /* DEVIDED BY RECENT REQUEST   0273
*                                      TOTAL                         */
         LH    @10,LCHLGBCT(,LCHPTR)                               0273
         N     @10,@CF03075                                        0273
         LH    @08,LCHPYBCT(,LCHPTR)                               0273
         N     @08,@CF03075                                        0273
         ALR   @10,@08                                             0273
         LH    @08,RLCTDDCT(,RLCTPTR)                              0273
         N     @08,@CF03075                                        0273
         SLR   @10,@08                                             0273
         LH    @08,RLCTPDCT(,RLCTPTR)                              0273
         N     @08,@CF03075                                        0273
         SLR   @10,@08                                             0273
         LR    @11,@10                                             0273
         M     @10,@CF02918                                        0273
         DR    @10,ILREQ                                           0273
         STH   @11,RLCTUTIL(,RLCTPTR)                              0273
*             IF RLCTUTIL<I0 THEN   /* COUNTS HAVE WRAPPED   @ZM43081*/
         SLR   @08,@08                                             0274
         CR    @11,@08                                             0274
         BNL   @RF00274                                            0274
*               RLCTUTIL=I0;                                       0275
*                                                                  0275
         STH   @08,RLCTUTIL(,RLCTPTR)                              0275
*/*                                                                  */
*/*      COMPUTE RECENT LCH REQUEST RATE                             */
*/*                                                                  */
*                                                                  0276
*             RLCTRQRT=             /* RECENT REQUEST RATE EQUALS    */
*                 ILREQ             /* RECENT REQUESTS               */
*                 *I1000            /* TIMES 1000                    */
*                 /ILTIM;           /* DIVIDED BY ELAPSED TIME IN MS */
@RF00274 LR    @11,ILREQ                                           0276
         M     @10,@CF02921                                        0276
         D     @10,ILTIM                                           0276
         STH   @11,RLCTRQRT(,RLCTPTR)                              0276
*           END;                    /* TO GET REQUESTS / SEC.        */
*                                                                  0278
*/*                                                                  */
*/*      FLAG OUT OF BALANCE LCHS AND COMPUTE IOL RV COMPUTATION     */
*/*      LCH UTILIZATION FACTOR                                      */
*/*                                                                  */
*                                                                  0278
*         RLCTOVER='0'B;            /* CLEAR IMBALANCE FLAGS         */
@RC00267 DS    0H                                                  0279
*         RLCTUNDR='0'B;            /*                               */
         NI    RLCTOVER(RLCTPTR),B'00111111'                       0279
*         IF ICTIOL='1'B THEN       /* IF I/O LOAD BALANCING         */
         TM    ICTIOL(RMCTPTR),B'01000000'                         0280
         BNO   @RF00280                                            0280
*           DO;                     /* IS ACTIVE                     */
*             IF RLCTUTIL>          /* IF LCH UTILIZATION > HIGH     */
*                 ICCHIUTH THEN     /* IMBALANCE THRESHOLD           */
         LH    @01,ICCHIUTH(,RMCTPTR)                              0282
         CH    @01,RLCTUTIL(,RLCTPTR)                              0282
         BNL   @RF00282                                            0282
*               DO;                                                0283
*                 ILDIF=ICCHIUTH;   /* SAVE HIGH THRESHOLD VALUE     */
         LR    ILDIF,@01                                           0284
*                 ILLCBPT=ILLCBPT   /* REMEMBER LCH IS OUT OF BALANCE*/
*                     ×RLCTBIT;     /*                               */
         OC    ILLCBPT(4),RLCTBIT(RLCTPTR)                         0285
*                 ICVOLCBT=         /* REMEMBER LCH IS OVERUTILIZED  */
*                     ICVOLCBT      /*                               */
*                     ×RLCTBIT;     /*                               */
         OC    ICVOLCBT(4,RMCTPTR),RLCTBIT(RLCTPTR)                0286
*                 RLCTOVER='1'B;    /* INDICATE LCH IS OVER UTILIZED */
         OI    RLCTOVER(RLCTPTR),B'10000000'                       0287
*               END;                                               0288
*             ELSE                  /* OTHERWIZE IF LCH UTILIZATION  */
*               IF RLCTUTIL<        /* LESS THAN LOW IMBALANCE       */
*                   ICCLOUTH THEN   /* THRESHOLD,                    */
         B     @RC00282                                            0289
@RF00282 LH    @01,ICCLOUTH(,RMCTPTR)                              0289
         CH    @01,RLCTUTIL(,RLCTPTR)                              0289
         BNH   @RF00289                                            0289
*                 DO;                                              0290
*                   ILDIF=          /* SAVE LOW THRESHOLD            */
*                       ICCLOUTH;                                  0291
         LR    ILDIF,@01                                           0291
*                   ILLCBPT=ILLCBPT×RLCTBIT;/* REMEMBER LCH IS OUT OF
*                                      BALANCE                       */
         OC    ILLCBPT(4),RLCTBIT(RLCTPTR)                         0292
*                   ICVULCBT=       /* REMEMBER LCH IS UNDERUTILIZED */
*                       ICVULCBT    /*                               */
*                       ×RLCTBIT;   /*                               */
         OC    ICVULCBT(4,RMCTPTR),RLCTBIT(RLCTPTR)                0293
*                   RLCTUNDR='1'B;  /* INDICATE LCH IS UNDER UTILIZED*/
         OI    RLCTUNDR(RLCTPTR),B'01000000'                       0294
*                 END;                                             0295
*               ELSE                /* OTHERWIZE                     */
*                 ILDIF=RLCTUTIL;   /* INSURE ZERO LCH UTILIZATION 0296
*                                      FACTOR                        */
         B     @RC00289                                            0296
@RF00289 LH    ILDIF,RLCTUTIL(,RLCTPTR)                            0296
*             ILDIF=ILDIF           /* COMPUTE DIFFERENCE BETWEEN    */
*                 -RLCTUTIL;        /* UTILIZATION AND THRESHOLD     */
@RC00289 DS    0H                                                  0297
@RC00282 SH    ILDIF,RLCTUTIL(,RLCTPTR)                            0297
*             RLCTRVUF=             /* UTILIZATION FACTOR IS SQUARE
*                                      OF                            */
*                 ABS(ILDIF)        /* THE DIFFERENCE WITH SAME SIGN */
*                 *ILDIF;           /* AS DIFFERENCE                 */
         LPR   @11,ILDIF                                           0298
         MR    @10,ILDIF                                           0298
         STH   @11,RLCTRVUF(,RLCTPTR)                              0298
*           END;                    /*                               */
*       END;                                                       0300
@RF00280 DS    0H                                                  0301
*     LCHPTR=LCHPTR                 /* POINT TO NEXT IOS LCH TABLE   */
*         +LCHLEN;                  /* ENTRY                         */
@RF00264 AH    LCHPTR,LCHLEN                                       0301
*   END;                                                           0302
         AH    RLCTPTR,RLCHLEN                                     0302
@DE00263 C     RLCTPTR,ICCLCLST(,RMCTPTR)                          0302
         BNH   @DL00263                                            0302
*   ICVLCBPT=ILLCBPT;               /* REMEMBER OUT OF BALANCE LCHS  */
         MVC   ICVLCBPT(4,RMCTPTR),ILLCBPT                         0303
*   RESPECIFY                                                      0304
*    (LCHPTR,                                                      0304
*     RTNREG) UNRESTRICTED;                                        0304
*   END LCHUSE;                                                    0305
@EL00002 DS    0H                                                  0305
@EF00002 DS    0H                                                  0305
@ER00002 BR    @14                                                 0305
*                                                                  0306
*/* **************************************************************** */
*/*                                                                  */
*/*  ENTRY POINT NAME = IRARME28 (MOVED FROM IRARMEVT)       @ZA20975*/
*/*                                                                  */
*/*  DESCRIPTIVE NAME = DEVICE ALLOCATION SYSEVENT                   */
*/*                                                                  */
*/*  CALLER = IRARMEVT VIA THE 'SYSEVENT DEVALLOC' MACRO             */
*/*                                                                  */
*/*  INPUT = R2 - RMCT ADDRESS                                       */
*/*          R3 - RRPA ADDRESS (RRPA HAS POINTER TO PARAMETER LIST)  */
*/*          R5 - ASCB ADDRESS                                       */
*/*                                                                  */
*/* **************************************************************** */
*                                                                  0306
*IRARME28:                          /* SYSEVENT 28 - DEVALLOC        */
*DEVALLOC:                                                         0306
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0306
IRARME28 DS    0H                                                  0306
DEVALLOC DS    0H                                                  0307
*   GENERATE CODE;                                                 0307
        ENTRY IRARME28                ENTRY TO SYSEVENT 28 - DEVALLOC
        BALR  @12,0                   GETS NEXT ADDRESS
        LA    @15,*-@PSTART           DISPLACEMENT FROM BEGINNING
        SLR   @12,@15                 MODULE ADRESSABILITY
*   RESPECIFY                                                      0308
*    (RTNREG,                                                      0308
*     NTRREG,                                                      0308
*     RLCTPTR,                                                     0308
*     IMCBPTR)                      /* FREE REGS FOR USE BY  @ZA20975*/
*         UNRESTRICTED;             /* DEVALLOC              @ZA20975*/
*   GEN DATA;                                                      0309
*                                                                  0310
*/*                                                                  */
*/*                                                                  */
*/********************************************************************/
*/*                                                                  */
*/*      DEVICE ALLOCATION INPUT AND WORKAREA DEFINITIONS            */
*/*                                                                  */
*/*      DEVICE ALLOCATION DECLARE STATEMENTS DEFINE SIX LISTS:      */
*/*                                                                  */
*/*        1) DEVUCBL1 - INPUT CANDIDATE LIST CONSISTING OF          */
*/*            FULL WORD UCB POINTERS PLUS NUMBER OF ENTRIES         */
*/*        2) DEVUCBL2 - INPUT ALLOCATED UCB LIST CONSISTING OF      */
*/*            FULL WORD POINTERS TO UCBS ALREADY ALLOCATED TO THE   */
*/*            USER PLUS NUMBER OF ENTRIES IN LIST                   */
*/*        3) RLCT LIST - LIST OF FULLWORD POINTERS TO SRM LOGICAL   */
*/*            CHANNEL TABLE ENTRIES.  EACH FULL WORD PTR POINTS TO  */
*/*            THE RLCT ENTRY FOR THE LOGICAL CHANNEL OF THE UCB AT  */
*/*            THE CORRESPONDING OFFSET IN THE INPUT CANDIDATE LIST  */
*/*        4) IDX LIST - PRIMARY INDEX LIST OF CANDIDATES STILL      */
*/*            IN THE RUNNING FOR ALLOCATION.  EACH HALFWORD ENTRY IN*/
*/*            THE LIST DEFINES AN INDEX INTO THE CANDIDATE AND RLCT */
*/*            LISTS                                                 */
*/*        5) JDX LIST - SECONDARY INDEX LIST OF CANDIDATES BEING    */
*/*            CHOSEN FROM THE CURRENT IDX LIST BY APPLICATION OF    */
*/*            A CANDIDATE ELIMINATION RULE.                         */
*/*        6) DEVEP LIST - LIST OF DEVICE ALLOCATION INTERNAL        */
*/*            PROCEDURES REPRESENTING RULES APPLIED IN              */
*/*            CHOOSING ONE UCB FROM THE INPUT CANDIDATE LIST        */
*/*                                                                  */
*/********************************************************************/
*/*                                                                  */
*/*      INPUT LIST DEFINITIONS                                      */
*/*                                                                  */
*                                                                  0310
*   DECLARE                                                        0310
*     1 DEVINPUT BASED(RRPAINP),    /* INPUT PARAMETER LIST          */
*      2 DEVIN1 PTR(31),            /* CANDIDATE LIST POINTER        */
*      2 DEVIN2 PTR(31),            /* ALLOCATED UCB LIST POINTER    */
*      2 DEVIN3 PTR(31),            /* RETURN AREA POINTER           */
*     1 DEVUCBL1 BASED(DEVIN1),     /* CANDIDATE LIST                */
*      2 DEVCT1 FIXED(31),          /* NUMBER OF ENTRIES             */
*      2 DEVPTR1(*) PTR(31),        /* CANDIDATE UCB POINTERS        */
*     1 DEVUCBL2 BASED(DEVIN2),     /* ALLOCATED UCB LIST            */
*      2 DEVCT2 FIXED(31),          /* NUMBER OF ENTRIES             */
*      2 DEVPTR2(*) PTR(31),        /* ALLOCATED UCB POINTERS        */
*     1 DEVRETRN BASED(DEVIN3),     /* RETURN AREA                   */
*      2 DEVUCB PTR(31),            /* POINTER TO SELECTED UCB       */
*      2 DEVRTC FIXED(31),          /* RETURN CODE                   */
*     DEVX1 PTR(31),                /* CANDIDATE LIST INDEX          */
*     DEVST1 PTR(31),               /* FIRST ENTRY IN CANDIDATE LIST */
*     DEVEND1 PTR(31),              /* LAST ENTRY IN CANDIDATE LIST  */
*     DEVUCB1 PTR(31) BASED(DEVX1), /* CANDIDATE LIST ENTRY          */
*     DEVX2 PTR(31) REG(1)          /* ALLOCATED UCB LIST INDEX      */
*         RESTRICTED,                                              0310
*     DEVST2 PTR(31),               /* FIRST ENTRY IN ALLOCATED LIST */
*     DEVEND2 PTR(31),              /* LAST ENTRY IN ALLOCATED LIST  */
*     DEVUCB2 PTR(31) BASED(DEVX2), /* ALLOCATED UCB LIST ENTRY      */
*     DEVPTR REG(6) PTR(31)         /* POINTER TO ADDRESS OF         */
*         RESTRICTED,               /* SELECTED UCB                  */
*     DEVXLEN FIXED(31) INIT(4),    /* LIST ENTRY LENGTH             */
*     UCBPTR2 PTR(31)               /* DEFINE UCBPTR AS DEVLST1 ENTRY*/
*         BASED(EVAL(DEVST1+IDXPTR)),/*                      @ZA20975*/
*     R10 REG(10) PTR(31),          /* REG FOR SUBROUTINES   @ZA20975*/
*     R11 REG(11) PTR(31),          /* REG FOR SUBROUTINES   @ZA20975*/
*     R14 REG(14) PTR(31),          /* REG FOR SUBROUTINES   @ZA20975*/
*     ZERO FIXED(8) CONSTANT(0),    /* CONSTANT 0            @ZA20975*/
*     ONE FIXED(8) CONSTANT(1),     /* CONSTANT 1            @ZA20975*/
*     TWO FIXED(8) CONSTANT(2),     /* CONSTANT 2            @ZA20975*/
*     CON8 FIXED(8) CONSTANT(8),    /* CONSTANT 8            @ZA20975*/
*     CON256 FIXED(15) CONSTANT(256),/* CONSTANT 256         @ZA20975*/
*     CON4096 FIXED(31) CONSTANT(4096),/* CONSTANT 4096      @ZA20975*/
*     MAXCUA FIXED(31) CONSTANT(131072),/* MAXIMUM POSSIBLE TAPE   0310
*                                      CHANNEL UNIT ADDRESS  @ZA20975*/
*/*                                                                  */
*/*                                                                  */
*/*      RLCT LIST DEFINITIONS                                       */
*/*                                                                  */
*                                                                  0310
*     RLCTST PTR(31),               /* RLCT LIST START ADDRESS       */
*     RLCTPTR2 PTR(31)              /* RLCT ENTRY BASE ADDRESS       */
*         BASED(EVAL(RLCTST+IDXPTR)),/*                      @ZA20975*/
*     RLCTSPTR PTR(31) REG(11),     /* ALTERNATE RLCT BASE   @Z30WIAW*/
*/*      IDX LIST DEFINITIONS                                        */
*/*                                                                  */
*                                                                  0310
*     IDX REG(4) PTR(31) RESTRICTED,/* PRIMARY INDEX LIST INDEX      */
*     IDXST REG(7) PTR(31)          /* PRIMARY INDEX LIST START      */
*         RESTRICTED,                                              0310
*     IDXEND PTR(31) REG(1)         /* PRIMARY INDEX LIST END        */
*         RESTRICTED,                                              0310
*     IDXPTR PTR(15) BASED(IDX),    /* PRIMARY INDEX LIST ENTRY      */
*     IDXLEN FIXED(31) REG(0)       /* LENGTH OF INDEX LIST ENTRY    */
*         RESTRICTED,                                              0310
*                                                                  0310
*/*                                                                  */
*/*      JDX LIST DEFINITIONS                                        */
*/*                                                                  */
*                                                                  0310
*     JDX REG(6) PTR(31) RESTRICTED,/* SECONDARY INDEX LIST INDEX    */
*     JDXST REG(9) PTR(31)          /* SECONDARY INDEX LIST START    */
*         RESTRICTED,               /* (CHGD TO REGISTER 9)  @ZA20975*/
*     JDXPTR PTR(15) BASED(JDX),    /* SECONDARY INDEX LIST ENTRY    */
*/*                                                                  */
*/*      DEVEP LIST DEFINITIONS                                      */
*/*                                                                  */
*                                                                  0310
*     DEVEP ENTRY BASED(DEVEPA)     /* RULE ENTRY POINT      @Z30WIMG*/
*         VALUERANGE(DEVRULE1,DEVRULE2,DEVRULE3,/*           @Z30WIAW*/
*         DEVRULE4,DEVRULE5),       /*                       @Z30WIAW*/
*     DEVEPTAB(5) PTR(31) INIT(     /* DEVICE ALLOCATION     @ZA00252*/
*         ADDR(DEVRULE1),           /* ENTRY POINT TABLE     @ZA00252*/
*         ADDR(DEVRULE2),           /*                               */
*         ADDR(DEVRULE3),           /*                               */
*         ADDR(DEVRULE4),           /*                               */
*         ADDR(DEVRULE5)),          /*                       @Z30WIAW*/
*     DEVEPX PTR(31),               /* ENTRY TABLE INDEX             */
*     DEVEPST PTR(31)               /* ENTRY TABLE START             */
*         INIT(ADDR(DEVEPTAB)),     /*                               */
*     DEVEPEND PTR(31)              /* ENTRY TABLE END               */
*         INIT(ADDR(DEVEPTAB)       /*                               */
*         +4*LENGTH(DEVEPTAB)),     /*                       @Z30WIAW*/
*     DEVEPRST PTR(31)              /* ENTRY TABLE LOOP              */
*         INIT(ADDR(DEVEPTAB)-      /* RESTART ADDRESS               */
*         LENGTH(DEVEPTAB)),        /*                               */
*     DEVEPA PTR(31) BASED(DEVEPX), /* ENTRY POINT TABLE ENTRY       */
*/*                                                                  */
*/*      RLCT, IDX, AND JDX LIST BUFFER DEFINITIONS                  */
*/*                                                                  */
*                                                                  0310
*     1 DEVWA BDY(WORD),            /* PERMANENTLY ALLOCATED         */
*      2 DEVWABUF(32) FIXED,        /* DEVICE ALLOCATION WORKAREA    */
*     DEVWALEN FIXED(15)            /* LENGTH OF PERMANENTLY         */
*         INIT(LENGTH(DEVWA)),      /* ALLOCATED WORKAREA            */
*     DEVLEN FIXED(15) REG(5)       /* LENGTH OF REQUIRED WORKAREA   */
*         RESTRICTED,                                              0310
*     DEVGTPTR PTR(31),             /* ADDRESS OF GETMAINED WORKAREA */
*     DEVGTLEN PTR(31),             /* LENGTH OF GETMAINED WORKAREA  */
*     DEVPR0 REG(0) FIXED(31),      /* GETMAIN                       */
*     DEVPR1 REG(1) PTR(31),        /* PARAMETER                     */
*     DEVPR15 REG(15) FIXED(31),    /* REGISTERS                     */
*/*                                                                  */
*/*      OTHER DEVICE ALLOCATION WORKING VARIABLES                   */
*/*                                                                  */
*                                                                  0310
*     DEVLCB BIT(32) BDY(WORD),     /* USER LCH BIT PATTERN          */
*     DEVDUP BIT(1),                /* FLAG - UCBS ALREADY ALLOCATED
*                                      TO USER HAVE BEEN REMOVED FROM
*                                      CANDIDATE LIST                */
*     DEVSAVE FIXED(31) REG(5)      /* TEMPORARY VALUE HOLDING AREA  */
*         RESTRICTED,                                              0310
*     DEVOUXB PTR(31),              /* OUXB POINTER SAVE AREA        */
*     DEVRV BIT(32)                 /* RANDOM VARIABLE               */
*         DEF(RMCTTOCI) POS(33);    /*                               */
*                                                                  0311
*/*                                                                  */
*/*      INITIALIZE LCH BIT PATTERN REPRESENTING LOGICAL CHANNELS    */
*/*      ON WHICH THE USER HAS DATA SETS ALLOCATED                   */
*/*                                                                  */
*                                                                  0311
*   RESPECIFY                                                      0311
*     UCBOB BASED(DEVUCB2);         /* BASE UCB ON ALLOCATED LIST    */
*   DEVOUXB=ASCBOUXB;               /* SAVE PTR TO OUXB              */
         L     @08,ASCBOUXB(,ASCBPTR)                              0312
         ST    @08,DEVOUXB                                         0312
*   DEVLCB=ZERO;                    /* CLEAR LCH BIT PATTERN         */
