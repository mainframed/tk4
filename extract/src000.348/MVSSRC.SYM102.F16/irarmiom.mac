*       END;                        /*                       @ZA00252*/
*   END;                            /*                       @ZA00252*/
@RF00425 ALR   IDX,IDXLEN                                          0430
@DE00424 CR    IDX,IDXEND                                          0430
         BNH   @DL00424                                            0430
*   IDX=IDXST;                      /* THIS IS FOR UCB BASING FOR THE
*                                      TEST OF THE TAPE BIT  @Z40BPEB*/
         LR    IDX,IDXST                                           0431
*   IF JDX=JDXST&                   /* IF THE SECONDARY LIST IS EMPTY
*                                      AND,                  @Z40BPJE*/
*       UCB3TAPE='1'B THEN          /* THIS IS A TAPE REQUEST      0432
*                                                            @Z40BPJE*/
         CR    JDX,JDXST                                           0432
         BNE   @RF00432                                            0432
         LH    @15,IDXPTR(,IDX)                                    0432
         AL    @15,DEVST1                                          0432
         L     @15,UCBPTR2(,@15)                                   0432
         TM    UCB3TAPE(@15),B'10000000'                           0432
         BNO   @RF00432                                            0432
*     DO IDX=IDXST                  /* GO THRU PRIMARY CANDIDATE     */
*           TO IDXEND BY IDXLEN;    /* LIST                  @Z40BPJE*/
         LR    IDX,IDXST                                           0433
         B     @DE00433                                            0433
@DL00433 DS    0H                                                  0434
*       IF UCBBPRV='0'B&            /* LOOKING FOR NON-PRIVATE AND   */
*           UCBMONT='0'B THEN       /* NON PASSED CANDIDATE  @Z40BPJE*/
         LH    @15,IDXPTR(,IDX)                                    0434
         L     @08,DEVST1                                          0434
         ALR   @08,@15                                             0434
         L     @08,UCBPTR2(,@08)                                   0434
         TM    UCBBPRV(@08),B'00010000'                            0434
         BNZ   @RF00434                                            0434
         TM    UCBMONT(@08),B'00000001'                            0434
         BNZ   @RF00434                                            0434
*         DO;                       /* IF FOUND MOVE         @Z40BPJE*/
*           JDXPTR=IDXPTR;          /* CANDIDATE TO THE      @Z40BPJE*/
         STH   @15,JDXPTR(,JDX)                                    0436
*           JDX=JDX+IDXLEN;         /* SECONDARY LIST.       @Z40BPJE*/
         ALR   JDX,IDXLEN                                          0437
*         END;                      /*                       @Z40BPJE*/
*     END;                          /*                       @Z40BPJE*/
@RF00434 ALR   IDX,IDXLEN                                          0439
@DE00433 CR    IDX,IDXEND                                          0439
         BNH   @DL00433                                            0439
*   END DEVRULE1;                   /*                       @Z30WIAW*/
@EL00004 DS    0H                                                  0440
@EF00004 DS    0H                                                  0440
@ER00004 BR    @14                                                 0440
