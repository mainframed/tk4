*   IF IMCBPTR^=I0&^                /* RV REMAINS ZERO IF NO USER I/O*/
*       ((IMCBSLCB&                 /* MONITORING HAS BEEN DONE OR IF*/
*       ICVOLCBT)^=I0               /* USER IS A SIGNIFICANT USER OF */
*       &(IMCBSLCB&                 /* BOTH OVER AND UNDERUTILIZED   */
*       ICVULCBT)^=I0) THEN         /* LCHS                          */
         SLR   @08,@08                                             0216
