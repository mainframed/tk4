*/********************************************************************/
*/*                                                                  */
*/*      DEVRULE2 - DEVALLOC INTERNAL SUBROUTINE             @Z30WIAW*/
*/*                                                                  */
*/*      FUNCTION -                                                  */
*/*            CHOOSE CANDIDATES ON LOGICAL CHANNEL WITH             */
*/*            LOWEST UTILIZATION, ADDING ASSUMED DELTA IF  @Z40BPEB */
*/*            ANY DD WAS PREVIOUSLY ALLOCATED TO THE USER  @Z40BPEB */
*/*            ON THE SAME LCH.                              @Z30WIAW*/
*/*                                                                  */
*/********************************************************************/
*                                                                  0441
*DEVRULE2:                                                         0441
*   PROC OPTIONS(NOSAVE);           /*                       @Z30WIAW*/
DEVRULE2 DS    0H                                                  0442
*   DEVSAVE='7FFFFFFF'X;            /* GET INIT UTILIZATION > MAX    */
         L     DEVSAVE,@CF03053                                    0442
*   DO IDX=IDXST                    /* LOOK AT EACH CANDIDATE ON     */
*         TO IDXEND BY IDXLEN;      /* PRIMARY LIST                  */
         LR    IDX,IDXST                                           0443
         B     @DE00443                                            0443
@DL00443 DS    0H                                                  0444
*     IF RLCTUMWA<=                 /* IF LCH UTIL FOR THIS  @Z30WIAW*/
*         DEVSAVE THEN              /* CANDIDATE IS <= LOWEST SO FAR */
         LH    @15,IDXPTR(,IDX)                                    0444
         AL    @15,RLCTST                                          0444
         L     @15,RLCTPTR2(,@15)                                  0444
         L     @15,RLCTUMWA(,@15)                                  0444
         CR    @15,DEVSAVE                                         0444
         BH    @RF00444                                            0444
*       DO;                         /* THEN                          */
*         IF RLCTUMWA<              /* LCH UTIL < OLD LOW    @Z30WIAW*/
*             DEVSAVE THEN          /* THEN                          */
         CR    @15,DEVSAVE                                         0446
         BNL   @RF00446                                            0446
*           DO;                                                    0447
*             JDX=JDXST;            /* START NEW SECONDARY LIST      */
         LR    JDX,JDXST                                           0448
*             DEVSAVE=RLCTUMWA;     /* SAVE NEW LOW UTIL.    @Z30WIAW*/
         LR    DEVSAVE,@15                                         0449
*           END;                                                   0450
*         JDXPTR=IDXPTR;            /* MOVE CANDIDATE TO             */
@RF00446 LH    @15,IDXPTR(,IDX)                                    0451
         STH   @15,JDXPTR(,JDX)                                    0451
*         JDX=JDX+IDXLEN;           /* SECONDARY LIST                */
         ALR   JDX,IDXLEN                                          0452
*       END;                                                       0453
*   END;                                                           0454
@RF00444 ALR   IDX,IDXLEN                                          0454
@DE00443 CR    IDX,IDXEND                                          0454
         BNH   @DL00443                                            0454
*   END DEVRULE2;                   /*                       @Z30WIAW*/
@EL00005 DS    0H                                                  0455
@EF00005 DS    0H                                                  0455
@ER00005 BR    @14                                                 0455
*                                                                  0456
*/********************************************************************/
*/*                                                                  */
*/*      DEVRULE3 - DEVALLOC INTERNAL SUBROUTINE             @Z30WIAW*/
*/*                                                                  */
*/*      FUNCTION -                                                  */
*/*        CHOOSE DIRECT ACCESS UCBS WITH LOWEST ALLOCATED           */
*/*        USER COUNT                                                */
*/*        FOR TAPE CANDIDATES, PICK THE DEVICE WITH UCB     @ZM43099*/
*/*        ADDRESS THAT IS HIGHER THAN, AND NEAREST TO,      @ZM43099*/
*/*        THE LAST PREVIOUSLY ASSIGNED TAPE.                @ZM43099*/
*/*        IF NO HIGHER UCB ADDRESS EXISTS, PICK THE UCB     @ZM43099*/
*/*        WITH THE LOWEST ADDRESS.                          @ZM43099*/
*/********************************************************************/
*                                                                  0456
*DEVRULE3:                                                         0456
*   PROC OPTIONS(NOSAVE);           /*                       @Z30WIAW*/
DEVRULE3 DS    0H                                                  0457
*   DEVSAVE=CON256;                 /* INIT USER COUNT > MAX POSSIBLE*/
         LA    DEVSAVE,256                                         0457
*   IDX=IDXST;                      /* SET UP ADDRESSING TO FIRST UCB*/
         LR    IDX,IDXST                                           0458
*   IF UCB3DACC='1'B THEN           /* IF THE CANDIDATES ARE DA,     */
         LH    @15,IDXPTR(,IDX)                                    0459
         AL    @15,DEVST1                                          0459
         L     @15,UCBPTR2(,@15)                                   0459
         TM    UCB3DACC(@15),B'00100000'                           0459
         BNO   @RF00459                                            0459
*     DO IDX=IDX                    /* LOOK AT EACH CANDIDATE ON     */
*           TO IDXEND BY IDXLEN;    /* PRIMARY LIST                  */
         B     @DE00460                                            0460
@DL00460 DS    0H                                                  0461
*       IF UCBUSER<=DEVSAVE THEN    /* IF UCB USER COUNT IS <=       */
         LH    @15,IDXPTR(,IDX)                                    0461
         AL    @15,DEVST1                                          0461
         L     @15,UCBPTR2(,@15)                                   0461
         SLR   @08,@08                                             0461
         IC    @08,UCBUSER(,@15)                                   0461
         CR    @08,DEVSAVE                                         0461
         BH    @RF00461                                            0461
*         DO;                       /* LOWEST SO FAR THEN            */
*           IF UCBUSER<DEVSAVE THEN /* IF UCB USER COUNT LESS THAN   */
         CR    @08,DEVSAVE                                         0463
         BNL   @RF00463                                            0463
*             DO;                   /* OLD LOW THEN                  */
*               JDX=JDXST;          /* START NEW SECONDARY LIST      */
         LR    JDX,JDXST                                           0465
*               DEVSAVE=UCBUSER;    /* SAVE NEW LOW USER COUNT       */
         LR    DEVSAVE,@08                                         0466
*             END;                                                 0467
*           JDXPTR=IDXPTR;          /* MOVE CANDIDATE TO             */
@RF00463 LH    @15,IDXPTR(,IDX)                                    0468
         STH   @15,JDXPTR(,JDX)                                    0468
*           JDX=JDX+IDXLEN;         /* TO SECONDARY LIST             */
         ALR   JDX,IDXLEN                                          0469
*         END;                                                     0470
*     END;                                                         0471
@RF00461 ALR   IDX,IDXLEN                                          0471
@DE00460 CR    IDX,IDXEND                                          0471
         BNH   @DL00460                                            0471
*   ELSE                            /* CANDIDATES ARE NOT DA,      0472
*                                                            @ZM43099*/
*     IF UCB3TAPE='1'B THEN         /* BUT ARE TAPE          @ZM43099*/
         B     @RC00459                                            0472
@RF00459 LH    @15,IDXPTR(,IDX)                                    0472
         AL    @15,DEVST1                                          0472
         L     @15,UCBPTR2(,@15)                                   0472
         TM    UCB3TAPE(@15),B'10000000'                           0472
         BNO   @RF00472                                            0472
*       DO;                         /* PICK ONE :            @ZM43099*/
*         DEVSAVE=MAXCUA;           /* INIT TO > MAX PTR.    @ZM43099*/
         L     DEVSAVE,@CF02998                                    0474
*         DO IDX=IDX TO IDXEND      /* LOOK THROUGH PRIMARY  @ZM43099*/
*               BY IDXLEN;          /* LIST                  @ZM43099*/
         B     @DE00475                                            0475
@DL00475 DS    0H                                                  0476
*           IF UCBPTR2>RCVTAPAD&    /* HIGHER PTR THAN LAST, @ZA20975*/
*               UCBPTR2<DEVSAVE     /* BUT LOWER THAN PREVIOUS     0476
*                                                            @ZA20975*/
*             THEN                                                 0476
         LH    @15,IDXPTR(,IDX)                                    0476
         L     @08,DEVST1                                          0476
         ALR   @08,@15                                             0476
         L     @08,UCBPTR2(,@08)                                   0476
         C     @08,RCVTAPAD(,RMCTPTR)                              0476
         BNH   @RF00476                                            0476
         CR    @08,DEVSAVE                                         0476
         BNL   @RF00476                                            0476
*             DO;                   /* IN THE LIST,          @ZM43099*/
*               DEVSAVE=UCBPTR2;    /* SAVE FOR LOOP.        @ZA20975*/
         LR    DEVSAVE,@08                                         0478
*               JDX=JDXST;          /* RESTART SECONDARY LIST      0479
*                                                            @ZM43099*/
         LR    JDX,JDXST                                           0479
*               JDXPTR=IDXPTR;      /* MOVE CANDIDATE        @ZM43099*/
         STH   @15,JDXPTR(,JDX)                                    0480
*               JDX=JDX+IDXLEN;     /* TO SECONDARY LIST.    @ZM43099*/
         ALR   JDX,IDXLEN                                          0481
*             END;                  /* END SEARCH FOR NEXT   @ZM43099*/
*         END;                      /* HIGHEST PTR.          @ZM43099*/
@RF00476 ALR   IDX,IDXLEN                                          0483
@DE00475 CR    IDX,IDXEND                                          0483
         BNH   @DL00475                                            0483
*         IF DEVSAVE=MAXCUA THEN    /* IF NONE WAS FOUND,    @ZM43099*/
         C     DEVSAVE,@CF02998                                    0484
         BNE   @RF00484                                            0484
*           DO IDX=IDXST TO IDXEND  /* SEARCH PRIMARY LIST   @ZM43099*/
*                 BY IDXLEN;        /* FOR THE LOWEST UCBPTR @ZM43099*/
         LR    IDX,IDXST                                           0485
         B     @DE00485                                            0485
@DL00485 DS    0H                                                  0486
*             IF UCBPTR2<DEVSAVE    /* IF THIS IS LOWER THAN @ZA20975*/
*               THEN                                               0486
         LH    @15,IDXPTR(,IDX)                                    0486
         L     @08,DEVST1                                          0486
         ALR   @08,@15                                             0486
         L     @08,UCBPTR2(,@08)                                   0486
         CR    @08,DEVSAVE                                         0486
         BNL   @RF00486                                            0486
*               DO;                 /* OTHERS                @ZM43099*/
*                 DEVSAVE=UCBPTR2;  /* SAVE FOR LOOP         @ZA20975*/
         LR    DEVSAVE,@08                                         0488
*                 JDX=JDXST;        /* RESTART SECONDARY LIST      0489
*                                                            @ZM43099*/
         LR    JDX,JDXST                                           0489
*                 JDXPTR=IDXPTR;    /* WITH THIS CANDIDATE.  @ZM43099*/
         STH   @15,JDXPTR(,JDX)                                    0490
*                 JDX=JDX+IDXLEN;   /* UPDATE                @ZM43099*/
         ALR   JDX,IDXLEN                                          0491
*               END;                /* AND CONTINUE SEARCH.  @ZM43099*/
*           END;                    /* LOWEST UCBPTR IS FOUND      0493
*                                                            @ZM43099*/
@RF00486 ALR   IDX,IDXLEN                                          0493
@DE00485 CR    IDX,IDXEND                                          0493
         BNH   @DL00485                                            0493
*       END;                                                       0494
*   END DEVRULE3;                   /*                       @Z30WIAW*/
@EL00006 DS    0H                                                  0495
@EF00006 DS    0H                                                  0495
@ER00006 BR    @14                                                 0495
