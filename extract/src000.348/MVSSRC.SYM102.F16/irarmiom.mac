         LR    ILRV,@08                                            0237
*       IMCBRV=ILRV;                /* SAVE NEW RECOMMENDATION       */
@RF00236 DS    0H                                                  0238
@RC00232 DS    0H                                                  0238
@RF00231 STH   ILRV,IMCBRV(,IMCBPTR)                               0238
*     END;                                                         0239
