         CL    @08,ICCMNSWP(,RMCTPTR)                              0231
         BNL   @RF00231                                            0231
*         IF OUCBOUT='0'B THEN      /* IF USER IS IN                 */
         TM    OUCBOUT(OUCBPTR),B'00000100'                        0232
