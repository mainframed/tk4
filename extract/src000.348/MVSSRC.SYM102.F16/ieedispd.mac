         TITLE 'DISPLAY DOMAIN PROCESSOR - IEEDISPD                    *
                        '
IEEDISPD CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(24)                                             0001
         DC    C'IEEDISPD 78024  UZ16178 '                         0001
         DROP  @15
@PROLOG  BALR  @12,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@12                                         0001
         LR    @03,@01                                             0001
         L     @00,@SIZDATD                                        0001
         GETMAIN  R,LV=(0)
         LR    @11,@01                                             0001
         USING @DATD,@11                                           0001
         LR    @01,@03                                             0001
         ST    @13,@SA00001+4                                      0001
         LR    @13,@11                                             0001
         XC    @ZTEMPS(@ZLEN),@ZTEMPS
         MVC   @PC00001(4),0(@01)                                  0001
*                                                                  0031
*   /*****************************************************************/
*   /*                                                               */
*   /* WHEN THIS MODULE IS ATTACHED,REGISTER 1 POINTS TO THE DISPLAY */
*   /* PROCESSOR'S CSCB. THE DMN KEYWORD IS LOCATED IN CHBUF IN THIS */
*   /* CSCB. IEEBUFSC WILL BE USED TO SCAN FOR THIS KEYWORD AND ITS  */
*   /* ASSOCIATED PARAMETER. THIS TOP SEGMENT DETERMINES THE FLOW OF */
*   /* CONTROL THRU THE MODULE                                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0031
*   REG14SAV=R14;                   /* SAVE RETURN ADDRESS           */
         ST    R14,REG14SAV                                        0031
*   CALL SETSTAE;                   /* CREATE STAE ENVIRONMENT       */
         BAL   @14,SETSTAE                                         0032
*   IF R15=ZERO THEN                /* CHECK IF STAE CREATED         */
         LTR   R15,R15                                             0033
         BNZ   @RF00033                                            0033
*     DO;                           /* PROCESSING TO DISPLAY DMN   0034
*                                      TABLE                         */
*       FLAGS=ZERO;                 /* ZERO FLAGS                    */
         MVI   FLAGS,X'00'                                         0035
*       ORIGID=CHCNID;              /* SAVE ORIGIN. CONSOLE ID       */
         SLR   @03,@03                                             0036
         IC    @03,CHCNID(,CSCBPTR)                                0036
         STH   @03,ORIGID                                          0036
*       R0SAVE=ORIGID;              /* SET ROSAVE FOR 1ST WTO        */
         ST    @03,R0SAVE                                          0037
*       ORIGAREA=CHARID;            /* SAVE SCREEN AREA ID           */
*                                                                  0038
         MVC   ORIGAREA(1),CHARID(CSCBPTR)                         0038
*       /*************************************************************/
*       /*                                                           */
*       /* GET STORAGE FOR WORKAREAS AND PARAMETERS                  */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0039
*       CALL GETSTOR;                                              0039
*                                                                  0039
         BAL   @14,GETSTOR                                         0039
*       /*************************************************************/
*       /*                                                           */
*       /* CALL ROUTINES TO BUILD 1ST TWO LINES OF THE MULTI-LINE WTO*/
*       /* IN THE PARAMETER LIST                                     */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0040
*       CALL TITLEBLD;              /* BUILD CONTROL LINE            */
         BAL   @14,TITLEBLD                                        0040
*       CALL LABELBLD;              /* BUILD LABEL LINE              */
*                                                                  0041
         BAL   @14,LABELBLD                                        0041
*       /*************************************************************/
*       /*                                                           */
*       /* INITIALIZE FIRST DATA LINE                        @ZA25176*/
*       /*                                                           */
*       /*************************************************************/
*                                                                  0042
*       WPLML0=ZERO;                /*                       @ZA25176*/
         MVI   WPLML0(WPLPTR),X'00'                                0042
*       WPLMLLEN=LENGTH(DLINE);     /* SET LENGTH IN WPL     @ZA25176*/
         MVI   WPLMLLEN(WPLPTR),X'34'                              0043
*       WPLMLLTF=ZERO;              /* ZERO LINETYPE FLAGS   @ZA25176*/
         SLR   @08,@08                                             0044
         STH   @08,WPLMLLTF(,WPLPTR)                               0044
*       WPLMLLTC='1'B;              /* SIGNAL DATALINE       @ZA25176*/
         OI    WPLMLLTC(WPLPTR),B'00100000'                        0045
*       TEXTPTR=WPLPTR;             /* SET TEXTPTR FOR NEXT LINE     */
         LR    TEXTPTR,WPLPTR                                      0046
*       MSGTXT=BLANK;               /* BLANK TEXT FIELD              */
*                                                                  0047
         MVI   MSGTXT+5(TEXTPTR),C' '                              0047
         MVC   MSGTXT+6(46,TEXTPTR),MSGTXT+5(TEXTPTR)              0047
         MVC   MSGTXT+4(1,TEXTPTR),BLANK                           0047
*       /*************************************************************/
*       /*                                                           */
*       /* DECIDE WHETHER TO DISPLAY WHOLE DOMAIN TABLE,OR ONE OF ITS*/
*       /* ENTRIES.                                                  */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0048
*       CALL DMNSCAN;               /* IS 'DMN ' IN CHBUF?           */
         BAL   @14,DMNSCAN                                         0048
*       IF R15=ZERO THEN            /* YES,THEN SET COUNT FOR NO. OF */
         LTR   R15,R15                                             0049
         BNZ   @RF00049                                            0049
*         NODMNWRT=NODMNS;          /* DOMAINS TO BE DISPLAYED.      */
         L     @08,AREAPTR                                         0050
         LH    @08,NODMNS(,@08)                                    0050
         ST    @08,NODMNWRT                                        0050
*       ELSE                                                       0051
*         DO;                       /* NO,CHECK FOR OTHER OPTION,    */
         B     @RC00049                                            0051
@RF00049 DS    0H                                                  0052
*           CALL DMNVSCAN;          /* DMN=VALUE                     */
         BAL   @14,DMNVSCAN                                        0052
*           IF R15=ZERO THEN        /* OPTION IS FOUND               */
         LTR   R15,R15                                             0053
         BNZ   @RF00053                                            0053
*             DO;                                                  0054
*               CALL VALDMN;        /* VERIFY VALUE IN CORRECT RANGE */
         BAL   @14,VALDMN                                          0055
*               IF R15=ZERO THEN    /* VALUE IS ACCEPTABLE           */
         LTR   R15,R15                                             0056
         BNZ   @RF00056                                            0056
*                 DO;               /* SET COUNT FOR # DMNS TO     0057
*                                      DISPLAY                       */
*                   IF BUFCHAR^=BLANK THEN/* MORE OPERANDS EXIST     */
         L     @08,ENDFLD                                          0058
         CLC   BUFCHAR(1,@08),BLANK                                0058
         BE    @RF00058                                            0058
*                     CALL ERRMSG(PI,DISPLAY);/* ISSUE ERROR MSG     */
         LA    @01,@AL00059                                        0059
         BAL   @14,ERRMSG                                          0059
*                   ELSE                                           0060
*                     NODMNWRT=ONE; /* DISPLAY ONLY ONE DOMAIN       */
         B     @RC00058                                            0060
@RF00058 MVC   NODMNWRT(4),@CF00072                                0060
*                 END;                                             0061
*               ELSE                                               0062
*                 CALL ERRMSG(KVI,DOMAIN);/* VALUE IS INVLAID,ISSUE
*                                      MSG                           */
         B     @RC00056                                            0062
@RF00056 LA    @01,@AL00062                                        0062
         BAL   @14,ERRMSG                                          0062
*             END;                                                 0063
*           ELSE                    /* VALUE IS INVALID              */
*             CALL ERRMSG(KVI,DOMAIN);/* ISSUE ERROR MSG             */
         B     @RC00053                                            0064
@RF00053 LA    @01,@AL00064                                        0064
         BAL   @14,ERRMSG                                          0064
*         END;                                                     0065
*                                                                  0065
@RC00053 DS    0H                                                  0066
*       /*************************************************************/
*       /*                                                           */
*       /* DISPLAY THE DOMAIN TABLE                                  */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0066
*       DO COUNT=FIRST TO NODMNWRT; /* PROCESS FOR EACH DOMAIN       */
@RC00049 LA    @08,1                                               0066
         B     @DE00066                                            0066
@DL00066 DS    0H                                                  0067
*         CALL DATABLD;             /* BUILD A DATA LINE FOR WTO     */
         BAL   @14,DATABLD                                         0067
*         IF COUNT=NODMNWRT THEN                                   0068
         CLC   COUNT(4),NODMNWRT                                   0068
         BNE   @RF00068                                            0068
*           DONE='1'B;              /* SIGNAL LAST LINE TO MSGSET  0069
*                                                            @ZA25176*/
         OI    DONE,B'10000000'                                    0069
*         CALL MSGSET;              /* WRITE OUT TEN LINES           */
@RF00068 BAL   @14,MSGSET                                          0070
*       END;                                                       0071
*                                                                  0071
         LA    @08,1                                               0071
         AL    @08,COUNT                                           0071
@DE00066 ST    @08,COUNT                                           0071
         C     @08,NODMNWRT                                        0071
         BNH   @DL00066                                            0071
*       /*************************************************************/
*       /*                                                           */
*       /* AFTER COMPLETING LOOP,IF ALL LINES HAVE BEEN WRITTEN, FREE*/
*       /* UP STORAGE AND RETURN.                            @ZA25176*/
*       /*                                                           */
*       /*************************************************************/
*                                                                  0072
*       CALL FREESTOR;              /* FREE STORAGE                  */
         BAL   @14,FREESTOR                                        0072
*     END;                                                         0073
*   CALL FREECSCB;                  /* FREE CSCB                     */
@RF00033 BAL   @14,FREECSCB                                        0074
*   R14=REG14SAV;                   /* RESTORE REG14                 */
         L     R14,REG14SAV                                        0075
*   RETURN;                                                        0076
@EL00001 L     @13,4(,@13)                                         0076
@EF00001 L     @00,@SIZDATD                                        0076
         LR    @01,@11                                             0076
         FREEMAIN R,LV=(0),A=(1)
@ER00001 BR    @14                                                 0076
*SETSTAE:                                                          0077
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0077
*                                                                  0077
SETSTAE  DS    0H                                                  0078
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS ROUTINE CREATES A STAE ENVIRONMENT THROUGH THE MODULE    */
*   /* IEECB860. A PARAMETER LIST IS BUILT AND PASSED TO MODULE      */
*   /* IEECB860 VIA REGISTER 1.                                      */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0078
*   R6=R1;                          /* SAVE SETS CSCB PTR            */
         LR    R6,R1                                               0078
*   W1=R14;                         /* SAVE RETURN ADDR              */
         LR    W1,R14                                              0079
*   R1=ADDR(WORKAREA);              /* GET CORE FOR PARM LIST        */
         LA    R1,WORKAREA                                         0080
*   R2=R1;                          /* SAVE CORE ADDR                */
         LR    R2,R1                                               0081
*   CSCBADDR=R6;                    /* STORE CSCB PTR IN LIST        */
         ST    R6,CSCBADDR(,R1)                                    0082
*   W2=ADDR(STAEBUF);               /* GET PTR TO CMD BUF            */
         LA    W2,STAEBUF(,R1)                                     0083
*   BUFPTR=W2;                      /* STORE CMD BUF PTR IN PARAMETER
*                                      LIST                          */
         ST    W2,BUFPTR(,R1)                                      0084
*   CMDSIZE=CMDLEN;                 /* STORE LENGTH OF COMMAND       */
         MVI   CMDSIZE(R1),X'07'                                   0085
*   COMMAND=DISPLAY;                /* STORE VERB                    */
         MVI   COMMAND+8(R1),C' '                                  0086
         MVC   COMMAND+9(2,R1),COMMAND+8(R1)                       0086
         MVC   COMMAND(8,R1),DISPLAY                               0086
*   RFY                                                            0087
*    (R11,                                                         0087
*     R12,                                                         0087
*     R13) RSTD;                                                   0087
*   RETADDR=W1;                     /* SAVE RETURN ADDRESS           */
         ST    W1,RETADDR(,R1)                                     0088
*   REG6SAV=R6;                     /* SAVE CSCB ADDRESS             */
         ST    R6,REG6SAV(,R1)                                     0089
*   REG11SAV=R11;                   /* SAVE DATA REG PTR             */
         ST    R11,REG11SAV(,R1)                                   0090
*   REG12SAV=R12;                   /* SAVE BASE REG PTR             */
         ST    R12,REG12SAV(,R1)                                   0091
*   REG13SAV=R13;                   /* SAVE SAVE AREA PTR            */
         ST    R13,REG13SAV(,R1)                                   0092
*   GEN(LOAD EP=IEECB860);          /* LOAD STAE CREATING MOD        */
         LOAD EP=IEECB860
*   R15=R0;                         /* PUT ENTRY ADDR IN REG 15      */
         LR    R15,R0                                              0094
*   R1=R2;                          /* RESTORE PARM PTR              */
         LR    R1,R2                                               0095
*   GEN(BALR R14,R15);              /* BRANCH TO STAE ROUTINE        */
         BALR R14,R15
*   W1=RETADDR;                     /* RESTORE RETURN ADDRESS        */
         L     W1,RETADDR(,R1)                                     0097
*   R6=REG6SAV;                     /* RESTORE CSCB ADDR             */
         L     R6,REG6SAV(,R1)                                     0098
*   R11=REG11SAV;                   /* RESTORE DATA REGISTER         */
         L     R11,REG11SAV(,R1)                                   0099
*   R12=REG12SAV;                   /* RESTORE BASE REGISTER         */
         L     R12,REG12SAV(,R1)                                   0100
*   R13=REG13SAV;                   /* RESTORE SAVE AREA PTR         */
         L     R13,REG13SAV(,R1)                                   0101
*   RFY                                                            0102
*    (R11,                                                         0102
*     R12,                                                         0102
*     R13) UNRSTD;                                                 0102
*   W2=R15;                         /* SAVE RETURN CODE              */
         LR    W2,R15                                              0103
*   GEN(DELETE EP=IEECB860);        /* DELETE STAE CREATE MOD        */
         DELETE EP=IEECB860
*   R15=W2;                         /* PUT RETURN CODE IN REGISTER 15*/
         LR    R15,W2                                              0105
*   R14=W1;                         /* RESTORE RETURN ADDR           */
         LR    R14,W1                                              0106
*   END SETSTAE;                                                   0107
@EL00002 DS    0H                                                  0107
@EF00002 DS    0H                                                  0107
@ER00002 BR    @14                                                 0107
*GETSTOR:                                                          0108
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0108
*                                                                  0108
GETSTOR  DS    0H                                                  0109
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS ROUTINE OBTAINS STORAGE FOR A COPY OF THE DOMAIN TABLE   */
*   /* FROM SUBPOOL 253. THE COPY IS OBTAINED VIA SYSEVENT #40       */
*   /* COPYDMDT. STORAGE IS ALSO OBTAINED FOR WTO WORKAREA.          */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0109
*                                                                  0109
*   /*****************************************************************/
*   /*                                                               */
*   /* GET STORAGE FOR WTO PARAMETER LIST AND STORAGE FOR SYSEVENT   */
*   /* PROCESSOR PARAMETERS                                          */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0109
*   REGISTER=R14;                   /* SAVE RETURN ADDR              */
         LR    REGISTER,R14                                        0109
*   SIZE=MAXSIZE+WTOSIZE+FOUR;      /* TABLE SIZE AND COUNT FIELD    */
         LA    @09,904                                             0110
         AL    @09,MAXSIZE                                         0110
         ST    @09,SIZE                                            0110
*   R0=SIZE×SUBPL253;               /* SUBPOOL NUMBER AND STORAGE  0111
*                                      SIZE                          */
         O     @09,SUBPL253                                        0111
         LR    R0,@09                                              0111
*   GEN(GETMAIN R,LV=(0));                                         0112
*                                                                  0112
         GETMAIN R,LV=(0)
*   /*****************************************************************/
*   /*                                                               */
*   /* CLEAR OBTAINED AREA                                           */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0113
*   AREAPTR=R1;                     /* SAVE ADDRESS OF PAR AREA      */
         ST    R1,AREAPTR                                          0113
*   WPLBASE=R1+MAXSIZE+FOUR;        /* SAVE ADDR OF WTO WORKAREA     */
         LR    @09,R1                                              0114
         AL    @09,MAXSIZE                                         0114
         AL    @09,@CF00049                                        0114
         ST    @09,WPLBASE                                         0114
*   WPLPTR=WPLBASE;                 /* ADDR OF FIRST WTO LINE        */
         LR    WPLPTR,@09                                          0115
*   DMDTPTR=ADDR(DMNTAB);           /* SAVE ADDRESS OF DOMAIN COPY   */
         L     @09,AREAPTR                                         0116
         LA    DMDTPTR,DMNTAB(,@09)                                0116
*   R0=R1;                          /* ADDRESS OF AREA TO BE CLEARED */
         LR    R0,R1                                               0117
*   R1=SIZE;                        /* LENGTH OF AREA        @ZM43101*/
         L     R1,SIZE                                             0118
*   R14=ADDR(CLEAR);                /* ADDR OF LOCATION WITH 0       */
         LA    R14,CLEAR                                           0119
*   R15=ZERO;                       /* PADDING CHAR IS 0             */
         SLR   R15,R15                                             0120
*   GEN REFS(R0,R14)(MVCL R0,R14);                                 0121
*                                                                  0121
         MVCL R0,R14
*   /*****************************************************************/
*   /*                                                               */
*   /* GET COPY OF DOMAIN TABLE AND THE COUNT OF THE NO. OF ENTRIES  */
*   /* IN THE TABLE.                                                 */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0122
*   R1=AREAPTR;                     /* ADDR OF PARM AREA             */
         L     R1,AREAPTR                                          0122
*   GEN(SYSEVENT COPYDMDT,ENTRY=BRANCH);                           0123
         SYSEVENT COPYDMDT,ENTRY=BRANCH
*   R14=REGISTER;                   /* RESTORE R14                   */
         LR    R14,REGISTER                                        0124
*   END GETSTOR;                                                   0125
@EL00003 DS    0H                                                  0125
@EF00003 DS    0H                                                  0125
@ER00003 BR    @14                                                 0125
*TITLEBLD:                                                         0126
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0126
*                                                                  0126
TITLEBLD DS    0H                                                  0127
*   /*****************************************************************/
*   /*                                                               */
*   /* BUILDS CONTROL LINE FOR DOMAIN DISPLAY TEXTPTR PTS TO         */
*   /* BEGINNING OF WTO PARAMETER LIST.                              */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0127
*   WPLMCSFA='1'B;                  /* ROUT/DESC CODES EXIST         */
*   WPLMCSFB='1'B;                  /* REG0 - Q TO CONSOLE IF ACTIVE */
*   WPLMCSFC='1'B;                  /* COMMAND RESPONSE IND          */
         OI    WPLMCSFA(WPLPTR),B'11100000'                        0129
*   WPLMCSFJ='1'B;                  /* TURN ON MLWTO INDICATOR       */
         OI    WPLMCSFJ(WPLPTR),B'01000000'                        0130
*   MCSFSAVE=WPLMCSF;               /* SAVE MCS FLAGS                */
         LH    @03,WPLMCSF(,WPLPTR)                                0131
         N     @03,@CF01644                                        0131
         STH   @03,MCSFSAVE                                        0131
*   WPLTXT(1:LENGTH(CNTLINE))=CNTLINE;/* FILL IN HEADER              */
         MVC   WPLTXT(31,WPLPTR),CNTLINE                           0132
*TIME:                                                             0133
*   GEN(TIME  DEC,ERRET=NOTIME);    /* GET TIME                      */
TIME     TIME  DEC,ERRET=NOTIME
*   CONVERT1=R0;                    /* SAVE TIME FROM REG 0          */
         LA    @03,CONVERT                                         0134
         ST    R0,CONVERT1(,@03)                                   0134
*   GEN(UNPK  UNPAK(7),CONVERT(4)); /* CONV TO EBCDIC                */
         UNPK  UNPAK(7),CONVERT(4)
*   WPLTXT(9:10)=UNPAK(1:2);        /* HOURS                         */
         MVC   WPLTXT+8(2,WPLPTR),UNPAK                            0136
*   WPLTXT(12:13)=UNPAK(3:4);       /* MINUTES                       */
         MVC   WPLTXT+11(2,WPLPTR),UNPAK+2                         0137
*   WPLTXT(15:16)=UNPAK(5:6);       /* SECONDS                       */
         MVC   WPLTXT+14(2,WPLPTR),UNPAK+4                         0138
*NOTIME:                                                           0139
*   WPLLGH=LENGTH(CNTLINE)+FOUR;    /* SET CNTL LINE LENGTH          */
NOTIME   LA    @03,35                                              0139
         STH   @03,WPLLGH(,WPLPTR)                                 0139
*   WPLPTR=WPLPTR+WPLLGH;           /* UPDATE PAST TEXT              */
         ALR   WPLPTR,@03                                          0140
*   WPLDESCE='1'B;                  /* DESC CODE 5                   */
*   WPLDESCH='1'B;                  /* DESC CODE 8                   */
         OI    WPLDESCE(WPLPTR),B'00001001'                        0142
*   WPLDESCI='1'B;                  /* DESC CODE 9                   */
         OI    WPLDESCI(WPLPTR),B'10000000'                        0143
*   DESCSAVE=WPLDESC;               /* SAVE DESCRIPTOR FLAGS         */
         MVC   DESCSAVE(2),WPLDESC(WPLPTR)                         0144
*   WPLROUT=ZERO;                   /* ZERO ROUTE CODES              */
         SLR   @03,@03                                             0145
         ST    @03,@TF00001                                        0145
         MVC   WPLROUT(2,WPLPTR),@TF00001+2                        0145
*   WPLPTR=WPLPTR+FOUR;             /* UPDATE TO NEXT WPL SECTION    */
         LA    @03,4                                               0146
         ALR   WPLPTR,@03                                          0146
*   WPLLTFA='1'B;                   /* LINE TYPE - CONTROL LINE      */
         OI    WPLLTFA(WPLPTR),B'10000000'                         0147
*   WPLAREA=ORIGAREA;               /* SET AREA ID - TARGET CONSOLE  */
         MVC   WPLAREA(1,WPLPTR),ORIGAREA                          0148
*   LINEBASE=ADDR(WPLLINES);        /* SAVE PTR TO # LINES FLD       */
         LA    @09,WPLLINES(,WPLPTR)                               0149
         ST    @09,LINEBASE                                        0149
*   WPLPTR=WPLPTR+FOUR;             /* UPDATE TO NEXT WPL SECTION    */
         ALR   WPLPTR,@03                                          0150
*   END TITLEBLD;                                                  0151
@EL00004 DS    0H                                                  0151
@EF00004 DS    0H                                                  0151
@ER00004 BR    @14                                                 0151
*LABELBLD:                                                         0152
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0152
*                                                                  0152
LABELBLD DS    0H                                                  0153
*   /*****************************************************************/
*   /*                                                               */
*   /* BUILD LABEL LINE FOR DOMAIN DISPLAY TEXTPTR PTS TO NEXT SLOT  */
*   /* IN WTO PARAMETER LIST                                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0153
*   WPLMLTXT(1:LENGTH(LABELINE))=LABELINE;/* MOVE TEXT               */
         MVC   WPLMLTXT(48,WPLPTR),LABELINE                        0153
*   WPLMLLEN=LENGTH(LABELINE)+FOUR; /* LENGTH IN WPL                 */
         LA    @03,52                                              0154
         STC   @03,WPLMLLEN(,WPLPTR)                               0154
*   WPLMLLTB='1'B;                  /* SET LINE TYPE= LABEL LINE     */
         OI    WPLMLLTB(WPLPTR),B'01000000'                        0155
*   WPLPTR=WPLPTR+WPLMLLEN;         /* UPDATE TO NEXT WPL SECTION    */
         ALR   WPLPTR,@03                                          0156
*   LINECT=2;                       /* SET CURRENT COUNT LINES BUILT */
         MVC   LINECT(2),@CH00113                                  0157
*   END LABELBLD;                                                  0158
@EL00005 DS    0H                                                  0158
@EF00005 DS    0H                                                  0158
@ER00005 BR    @14                                                 0158
*DMNSCAN:                                                          0159
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0159
*                                                                  0159
DMNSCAN  DS    0H                                                  0160
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS ROUTINE SEARCHES FOR 'DMN ' IN CHBUF AND SETS THE RETURN */
*   /* CODE TO 0 IF FOUND, 8 IF NOT FOUND.                           */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0160
*   R15=NOTFOUND;                                                  0160
         LA    R15,8                                               0160
*   SCANPTR=ADDR(CHBUF);            /* SET BEGINNING OF SCAN         */
         LA    @08,CHBUF(,CSCBPTR)                                 0161
         ST    @08,SCANPTR                                         0161
*   EOB=SCANPTR+BUFLEN;             /* SET PTR TO END OF CHBUF       */
         LA    @03,123                                             0162
         ALR   @03,@08                                             0162
         ST    @03,EOB                                             0162
*   DO SEARCH=SCANPTR TO EOB WHILE(R15=NOTFOUND);                  0163
         L     @03,SCANPTR                                         0163
         LR    SEARCH,@03                                          0163
         B     @DE00163                                            0163
@DL00163 LA    @03,8                                               0163
         CR    R15,@03                                             0163
         BNE   @DC00163                                            0163
*     IF SEARCH=EOB THEN                                           0164
         C     SEARCH,EOB                                          0164
         BNE   @RF00164                                            0164
*       R15=NOTFOUND;               /* THIS OPTION NOT FOUND         */
         LR    R15,@03                                             0165
*     ELSE                                                         0166
*       DO;                                                        0166
         B     @RC00164                                            0166
@RF00164 DS    0H                                                  0167
*         IF KEYWMAP=KEYW1 THEN                                    0167
