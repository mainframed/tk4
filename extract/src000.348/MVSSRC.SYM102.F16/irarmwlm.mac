         LH    WPGDPTR,OUCBWMG(,OUCBPTR)                        02S0448
         AL    WPGDPTR,WMSTPGDT(,WMSTPTR)                       02S0448
*    IF OUCBPVL = '0'B THEN         /* USER SUBJECT TO WLM ANALYSIS  */
         TM    OUCBPVL(OUCBPTR),B'00000100'                     02S0449
         BNZ   @RF00449                                         02S0449
*      OUCBTMW = MAX(ONE,OUCBTMS);  /* INSERT NONZERO STARTING TIME  */
         L     @09,ONE                                          02S0450
         L     @08,OUCBTMS(,OUCBPTR)                            02S0450
         CLR   @09,@08                                          02S0450
         BNL   *+6
         LR    @09,@08                                          02S0450
         ST    @09,OUCBTMW(,OUCBPTR)                            02S0450
*    OUCBTMS = RRPATOD;             /* SAVE ACTUAL LAST ACTION TIME  */
@RF00449 L     @09,RRPATOD(,RRPAPTR)                            02S0451
         ST    @09,OUCBTMS(,OUCBPTR)                            02S0451
