*/* ALGORITHM APPLICABILITY DETERMINATION SUBROUTINE                 */
*/* **************************************************************** */
*/*                                                                  */
*/*    ROUTINE NAME - IRARMWMJ                                       */
*/*                                                                  */
*/*    DESCRIPTIVE NAME - ROUTINE TO DETERMINE THE SCOPE OF          */
*/*          APPLICABILITY OF ANALYSIS PROCESSING TO A USER          */
*/*                                                                  */
*/*    FUNCTION - THE IRARMWMJ ROUTINE EXAMINES THE CURRENT          */
*/*          SWAPPABILITY STATUS AND THE PERFORMANCE SPECI-          */
*/*          FICATION FOR AN USER.  IT INDICATES IF THE RESOURCE     */
*/*          MANAGER ALGORITHMS ARE APPLICABLE TO THIS USER.         */
*/*                                                                  */
*/*    CALLED BY - IRARMWMK - WLM DONTSWAP/OKSWAP SERVICE UPDT RTNE  */
*/*                IRARMWMN - USER TRANSACTION STARTUP ROUTINE       */
*/*                IRARMWMO - USER TRANSACTION SUSPEND ROUTINE       */
*/*                IRARMWMR - USER TRANSACTION RESUMPT ROUTINE       */
*/*                IRARMWM5 - PERFORMANCE PERIOD CHECK SBRTINE       */
*/*                                                                  */
*/*    INPUT - REG 4 CONTAINS THE ADDRESS OF THE OUCB FOR THE        */
*/*            USER WHOSE ANALYSIS APPLIC IS BEING DETERMINED.       */
*/*          - REG 14 CONTAINS THE RETURN ADDRESS.                   */
*/*          - REG 15 CONTAINS THE IRARMWMK RTNE ADDRESS.            */
*/*          - OUCBNSW INDICATES IF THE USER IS NON-SWAPPABLE.       */
*/*          - OUCBNPG INDICATES THE PERF GROUP FOR THE USER.        */
*/*          - OUCBPGP INDICATES THE PERF PERIOD FOR THE USER.       */
*/*          - WPGDRTB INDICATES THE RESPONSE/THROUGHPUT BIAS.       */
*/*                                                                  */
*/*    OUTPT - OUCBRMA IS SET ON IF RESOURCE ALGORITHM RECOM-        */
*/*            MENDATIONS ARE TO BE SOLICITED FOR THIS USER.         */
*/*                                                                  */
*/*    EXIT  - BRANCH ON REGISTER 14.                                */
*/*          - THERE IS NO ERROR EXIT OR INDICATION.                 */
*/*                                                                  */
*/* **************************************************************** */
*IRARMWMJ: PROC OPTIONS(NOSAVE,NOSAVEAREA,NOENTREG,RETREG,NOPARMREG);
IRARMWMJ DS    0H                                               02S0458
*    RFY (WKRE) UNRSTD;                                         02S0458
*    STPR = RTNREG;                 /* SAVE RETURN REG CONTENTS      */
         LR    STPR,RTNREG                                      02S0459
*    /* NON-SWAPPABLE USERS ARE EXCLUDED FROM SWAP ANALYSIS          */
*    OUCBRMA = '0'B;                /* RESET RESOURCE ANALYSIS FLG   */
         NI    OUCBRMA(OUCBPTR),B'11011111'                     02S0460
*    IF OUCBNSW = '0'B THEN /* USER  WKLD CONTROLLED */         02S0461
         TM    OUCBNSW(OUCBPTR),B'10000000'                     02S0461
         BNZ   @RF00461                                         02S0461
*      DO;                                                      02S0462
*      WMSTPTR = RMCTWMST;          /* PICK WLM SPECIFICATION TABLE  */
         L     WMSTPTR,RMCTWMST(,RMCTPTR)                       02S0463
*      WPGDPTR = WMSTPGDT + WPGVTENT; /* PICK PERF GROUP DESCRIPTOR  */
         SLR   @10,@10                                          02S0464
         IC    @10,OUCBNPG(,OUCBPTR)                            02S0464
         ALR   @10,@10                                          02S0464
         AL    @10,WMSTPGVT(,WMSTPTR)                           02S0464
         LH    WPGDPTR,WPGVTENT(,@10)                           02S0464
         AL    WPGDPTR,WMSTPGDT(,WMSTPTR)                       02S0464
*      WPGPPTR = WPGDPTR + OUCBPGP; /* PICK CURR PERF GRP PERIOD   */
         LR    WPGPPTR,WPGDPTR                                  02S0465
         SLR   @10,@10                                          02S0465
         IC    @10,OUCBPGP(,OUCBPTR)                            02S0465
         ALR   WPGPPTR,@10                                      02S0465
*      IF WPGPRTB = '1'B THEN     /* THROUGHPUT BIAS INDICATED     */
         TM    WPGPRTB(WPGPPTR),B'00000001'                     02S0466
         BNO   @RF00466                                         02S0466
*        OUCBRMA = '1'B;          /* RESOURCE ALGORITHMS APPLIC    */
         OI    OUCBRMA(OUCBPTR),B'00100000'                     02S0467
*      ELSE                       /* IF RTB=0 BE SURE THAT  @ZA26908*/
*        DO;                      /* LOAD BALANCER RECOMMEN @ZA26908*/
         B     @RC00466                                         02S0468
@RF00466 DS    0H                                               02S0469
*        OUCBCRV=ZERO;            /* DATION VALUES ARE      @ZA26908*/
         SLR   @10,@10                                          02S0469
         STH   @10,OUCBCRV(,OUCBPTR)                            02S0469
*        OUCBIRV=ZERO;            /* ZERO                   @ZA26908*/
         STH   @10,OUCBIRV(,OUCBPTR)                            02S0470
*        END;                     /*                        @ZA26908*/
*      END;                                                     02S0472
*    ELSE                         /* IF NONSWAPPABLE MEMORY @ZA26908*/
*      DO;                        /* LOAD BALANCER RECOMEN  @ZA26908*/
         B     @RC00461                                         02S0473
@RF00461 DS    0H                                               02S0474
*      OUCBCRV=ZERO;              /* DATION VALUES SHOULD   @ZA26908*/
         SLR   @10,@10                                          02S0474
         STH   @10,OUCBCRV(,OUCBPTR)                            02S0474
*      OUCBIRV=ZERO;              /* BE ZERO                @ZA26908*/
         STH   @10,OUCBIRV(,OUCBPTR)                            02S0475
*      END;                       /*                        @ZA26908*/
*    RTNREG = STPR;                 /* RESTORE RETURN REG CONTENTS   */
@RC00461 LR    RTNREG,STPR                                      02S0477
*    RFY (WKRE) RSTD;                                           02S0478
*    END IRARMWMJ;                                              02S0479
@EL00004 DS    0H                                               02S0479
@EF00004 DS    0H                                               02S0479
@ER00004 BR    @14                                              02S0479
