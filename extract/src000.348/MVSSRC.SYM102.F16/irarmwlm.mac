         L     @15,RMSBWM1(,RMCTPTR)                            02S0435
         BALR  @14,@15                                          02S0435
*    OUXBTRS = OUXBTRS + OUCBWMS;   /* ADD SERVICE TO TRANSACTION    */
         L     @09,OUXBTRS(,OUXBPTR)                            02S0436
         AL    @09,OUCBWMS(,OUCBPTR)                            02S0436
         ST    @09,OUXBTRS(,OUXBPTR)                            02S0436
*    OUXBTRT = OUXBTRT + (RRPATOD - OUCBTMS); /* ADD TIME TO XACN    */
         L     @09,RRPATOD(,RRPAPTR)                            02S0437
         SL    @09,OUCBTMS(,OUCBPTR)                            02S0437
         L     @08,OUXBTRT(,OUXBPTR)                            02S0437
         ALR   @08,@09                                          02S0437
         ST    @08,OUXBTRT(,OUXBPTR)                            02S0437
*    OUXBTRR = OUXBTRR + (RRPATOD - OUCBTMS); /* ADD RESIDNT TIME    */
         AL    @09,OUXBTRR(,OUXBPTR)                            02S0438
         ST    @09,OUXBTRR(,OUXBPTR)                            02S0438
*    IF RMCTMFA = '1'B THEN         /* WORKLOAD ACTIVITY MEASURED    */
         TM    RMCTMFA(RMCTPTR),B'10000000'                     02S0439
         BNO   @RF00439                                         02S0439
*      /* UPDATE WORKLOAD ACTIVITY MEASUREMENT REPORT STATISTICS     */
*      DO;                                                      02S0440
*      RRPAWAR = '1'B;              /* INSERT WKLD ACTIVTY RTNE FLAG */
         OI    RRPAWAR(RRPAPTR),B'01000000'                     02S0441
*      /* ? IRACTLCL EPN(WR4) */                                02S0442
*      CALL RMSBWR4 -> IRARMRTN ;               /* INVOKE WR4 SUBRTN */
         L     @15,RMSBWR4(,RMCTPTR)                            02S0442
         BALR  @14,@15                                          02S0442
*      RRPAWAR = '0'B;              /* DELETE WKLD ACTIVTY RTNE FLAG */
         NI    RRPAWAR(RRPAPTR),B'10111111'                     02S0443
*      END;                                                     02S0444
*    RTNREG = PMRO;                 /* RESTORE RETURN REG CONTENTS   */
@RF00439 LR    RTNREG,PMRO                                      02S0445
*/*    ENTRY WHEN DONT NEED TO ACCUM DATA, JUST START INTERVAL ANEW  */
*IRARMWMH: /* SUBROUTINE TO RESET WORKLOAD MANAGER INTERVAL          */
