*/* IGC042R1: CHART FOR ATTACH REENTRY ROUTINE                       */
*/* FOOTING                                                        0399
*/* REENTRY ROUTINE                                                  */
*/********************************************************************/
*/*                                                                  */
*/*    THIS ROUTINE IS THE FIRST CODE EXECUTED UNDER THE NEW TASK.   */
*/*    IF SVAREA=YES WAS SPECIFIED, A PROBLEM PROGRAM SAVE AREA IS   */
*/*    OBTAINED, FROM SUBPOOL 250, AND CHAINED OFF TCBFSA. THE SVRB  */
*/*    AND REGISTERS ARE SET UP FOR LINK, AND THE LINK SERVICE       */
*/*    ROUTINE IS ENTERED AT BRANCH ENTRY POINT IEAQCS01. CONTROL IS */
*/*    NOT RETURNED TO ATTACH.                                       */
*/*                                                                  */
*/*    REGISTER USAGE - REGISTERS UPON ENTRY ARE AS IN THE SVRB'S    */
*/*                     RBGRSAVE FIELD, AS SET IN ATTACH MAINLINE:   */
*/*                                                                  */
*/*    R0WORK   - OUTPUT, ADDRESS OF ENTRY POINT NAME, OR            */
*/*               COMPLEMENTED ADDRESS OF DE.                        */
*/*    R1AREA   - OUTPUT, ADDRESS OF DCB AS SPECIFIED ON ATTACH      */
*/*               MACRO, OR ZERO                                     */
*/*    R3WORKAD - INPUT, ADDRESS OF NEW (CURRENT) SVRB               */
*/*             - OUTPUT, ADDRESS OF CVT (INPUT CONTENTS SWITCHES)   */
*/*    R4CURTCB - ADDRESS OF THE NEW TCB, NOW THE CURRENT TCB        */
*/*    R5CURRB  - ADDRESS OF THE NEW SVRB, NOW THE CURRENT RB        */
*/*    R6BASE   - ADDRESS OF IGC0004B, ENTRY POINT TO SVC ATTACH     */
*/*             - OUTPUT, ADDRESS OF IEAQCS01, LINK BRANCH ENTRY     */
*/*    R9BASE   - INPUT, R6BASE + 4096                               */
*/*    R10WORK  - INPUT, SAME AS R0WORK OUTPUT                       */
*/*    R11WORK  - INPUT, SAME AS R1AREA OUTPUT                       */
*/*    R12SVRB  - INPUT, ADDRESS OF RBEXSAVE                         */
*/*                                                                  */
*/********************************************************************/
*/* IGC042R1: E REENTRY FROM DISPATCHER                              */
*                                                                  0399
*   RFY                                                            0399
*     R10WORK RSTD;                                                0399
*   RFY                                                            0400
*     R11WORK RSTD;                                                0400
*                                                                  0400
*/* D (NO,%FL01500,YES,) WAS SVAREA=YES SPECIFIED ?                  */
*                                                                  0401
*   IF WKSVAREA='0'B THEN                                          0401
         TM    WKSVAREA(R12SVRB),B'10000000'                       0401
         BNZ   @RF00401                                            0401
*     DO;                           /* DO000040                      */
*       RFY                                                        0403
*         R1AREA RSTD;                                             0403
*       RFY                                                        0404
*         R0WORK RSTD;                                             0404
*       GENERATE SETS(R0WORK,R1AREA) NOFLOWS;                      0405
         SPACE
*/* L (,%FL01510) GETMAIN: OBTAIN PROBLEM PGM SAVE AREA, SP 250      */
         SPACE
         GETMAIN R,LV=SALENGTH,SP=SP250EQU OBTAIN SAVE AREA
         USING TCB,R4CURTCB ADDRESSABILITY FOR TCB KEY         @ZA30792
         MODESET  EXTKEY=TCB,WORKREG=2 GET IN SAME KEY AS TCB  @ZA30792
         XC   C0(SALENGTH,R1AREA),C0(R1AREA)    CLEAR AREA     @ZA25845
         MODESET  EXTKEY=ZERO,WORKREG=2 RETURN TO KEY ZERO     @ZA30792
         DROP R4CURTCB          DROP ADDRESSIBILITY            @ZA30792
*     END;                          /* END00040                      */
*/* %FL01500: P SET REG 1 = 0, NO PP SAVE AREA                       */
*                                                                  0407
*   ELSE                                                           0407
*     R1AREA=ZERO;                  /* CLEAR POINTER, NO SAVE AREA   */
         B     @RC00401                                            0407
@RF00401 SLR   R1AREA,R1AREA                                       0407
*/* %FL01510: P SET TCBFSA = REG 1, RBGRS13 = REG 1                  */
*                                                                  0408
*   R4CURTCB->TCBFSAB=R1AREA;       /* SAVE AREA ADDR OR ZERO        */
@RC00401 STCM  R1AREA,7,TCBFSAB(R4CURTCB)                          0408
*   R5CURRB->RBGRS13=R1AREA;        /* SAVE AREA ADDR, OR ZERO       */
         ST    R1AREA,RBGRS13(,R5CURRB)                            0409
*/* P CLEAR RESERVED PORTION OF SVRB EXTENDED SAVE AREA              */
*                                                                  0410
*   R1AREA=ADDR(RBEXSAVE)+LENGTH(WKNEWXSA);/* ADDR RESERVED          */
         LA    R1AREA,RBEXSAVE(,R3WORKAD)                          0410
         AH    R1AREA,@CH02795                                     0410
*   RFY                                                            0411
*     R7WORK RSTD;                                                 0411
*   R7WORK=LENGTH(RBEXSAVE)-LENGTH(WKNEWXSA);/* LENGTH               */
         LA    R7WORK,39                                           0412
*   AREA(1:R7WORK)=AREA(1:R7WORK)&&AREA(1:R7WORK);/* CLEAR           */
         LR    @08,R7WORK                                          0413
         BCTR  @08,0                                               0413
         EX    @08,@SX03144                                        0413
*   RFY                                                            0414
*     R7WORK UNRSTD;                                               0414
*                                                                  0414
*/* P ESTABLISH REGISTERS FOR LINK BRANCH ENTRY                      */
*                                                                  0415
*   R0WORK=R10WORK;                 /* ESTABLISH REG 0, ADDRESS OF EP
*                                      NAME OR COMPLEMENTED ADDRESS
*                                      OF DE                         */
         LR    R0WORK,R10WORK                                      0415
*   R1AREA=R11WORK;                 /* ESTABLISH REG 1, 0 OR DCB   0416
*                                      ADDRESS, FROM DCB PARAMETER   */
         LR    R1AREA,R11WORK                                      0416
*   R3WORK=CVTPTR;                  /* ADDRESS OF CVT                */
         L     R3WORK,CVTPTR                                       0417
*   RFY                                                            0418
*     R7ASCB RSTD;                                                 0418
*   R7ASCB=PSAAOLD;                 /* ASCB ADDRESS                  */
*                                                                  0419
         L     R7ASCB,PSAAOLD                                      0419
*   /*****************************************************************/
*   /*                                                               */
*   /* REGS 4, AND 5 ALREADY PROPERLY SET                            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0420
*   R6BASE=R3WORK->CVTQCS01;        /* LINK BRANCH ENTRY ADDRESS   0420
*                                      IEAQCS01                      */
         L     R6BASE,CVTQCS01(,R3WORK)                            0420
*/* R BRANCH TO LINK ENTRY POINT IEAQCS01                            */
*                                                                  0421
*   GOTO R6BASE;                    /* PASS CONTROL TO LINK          */
         BR    R6BASE                                              0421
*/* IGC042R1: END FLOWCHART FOR REENTRY ROUTINE                      */
*                                                                  0422
*   RFY                                                            0422
*     R0WORK UNRSTD;                                               0422
*   RFY                                                            0423
*     R1AREA UNRSTD;                                               0423
*   RFY                                                            0424
*     R7ASCB UNRSTD;                                               0424
*   RFY                                                            0425
*     R10WORK UNRSTD;                                              0425
*   RFY                                                            0426
*     R11WORK UNRSTD;                                              0426
*   END IGC042R1;                   /* END REENTRY ROUTINE           */
