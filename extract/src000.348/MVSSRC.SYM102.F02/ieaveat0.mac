GETCBS   DS    0H                                                  0431
*/* GETCBS: CHART (AREF) FLOWCHART SUBROUTINE GETCBS                 */
*/* FOOTING                                                        0431
*/* SUBROUTINE OBTAINS TCB IQE IRB                                   */
*/********************************************************************/
*/*                                                                  */
*/*    THIS SUBROUTINE OBTAINS STORAGE FOR THE NEW TCB,AND ALSO FOR  */
*/*    AN IQE, AND AN IRB, IF NECESSARY. THE STORAGE IS CLEARED TO   */
*/*    ZERO BEFORE USE. IF ACQUIRED, THE IRB AND IQE ARE INITIALIZED */
*/*    AND CHAINED OFF THE NEW TCB.                                  */
*/*                                                                  */
*/* SUBROUTINES CALLED: UNLOCK                                       */
*/*                                                                  */
*/* SYSTEM ROUTINES CALLED / MACROS ISSUED:                          */
*/*                                 GETMAIN SVC                      */
*/*                                 CIRB SVC                         */
*/*                                                                  */
*/* RETURN CODES SET: X00NOERR, AND THE ERROR CODE FROM UNLOCK       */
*/*                                                                  */
*/* REGISTER USAGE: R5NEWTCB - OUTPUT, ADDRESS OF THE NEW TCB        */
*/*                                                                  */
*/********************************************************************/
*/* E ENTER GETCBS                                                   */
*                                                                  0431
*   RFY                                                            0431
*     R14RET RSTD;                                                 0431
*   WKRTSAVE=R14RET;                /* SAVE AROUND MACROS, UNLOCK    */
         ST    R14RET,WKRTSAVE(,R3WORKAD)                          0432
*   RFY                                                            0433
*     R14RET UNRSTD;                                               0433
*                                                                  0433
*/* D (NO,%FL01060,YES,) WAS ETXR SPECIFIED ?                        */
*                                                                  0434
*   IF ATETXRB^=ZERO THEN           /* IF ETXR ADDR PARM SPECIFIED,  */
         ICM   @08,7,ATETXRB(R3WORKAD)                             0434
         BZ    @RF00434                                            0434
*     DO;                           /* DO000008: AN IQE AND POSSIBLY
*                                      IRB NEEDED                    */
*                                                                  0435
*       /*************************************************************/
*       /*                                                           */
*       /* CHECK FOR A SUBTASK HAVING AN IQE WHICH POINTS TO AN IRB  */
*       /* WHICH CAN ALSO BE USED FOR THIS REQUEST.                  */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0436
*       RFY                                                        0436
*         R2WORK RSTD;                                             0436
*       RFY                                                        0437
*         R7IQE RSTD;                                              0437
*       RFY                                                        0438
*         R8IRB RSTD;                                              0438
*       R2WORK=R4CURTCB->TCBLTC;    /* ADDR OF FIRST SUBTASK         */
         L     R2WORK,TCBLTC(,R4CURTCB)                            0439
*       DO WHILE(R2WORK^=0);        /* DO000010: CHECK ALL SUBTASKS  */
         B     @DE00440                                            0440
@DL00440 DS    0H                                                  0441
*/* %FL01080: D (NO,%FL01070,YES,) IS THERE A SUBTASK ?              */
*                                                                  0441
*         R7IQE=R2WORK->TCBIQE;     /* ETXR IQE ADDR                 */
         L     R7IQE,TCBIQE(,R2WORK)                               0441
*/* D (NO,AT001090,YES,) IS THERE AN ETXR IQE ?                      */
*                                                                  0442
*         IF R7IQE=ZERO THEN        /* IS THERE AN IQE ?             */
         LTR   R7IQE,R7IQE                                         0442
         BZ    @RT00442                                            0442
*           GOTO AT001090;          /* NO, CHECK NEXT SUBTASK        */
*         R8IRB=R7IQE->IQEIRB;      /* ETXR IRB ADDR                 */
         L     R8IRB,IQEIRB(,R7IQE)                                0444
*/* D (NO,AT001090,YES,) IS IRB'S ENTRY ADDR = ETXR ADDR ?           */
*                                                                  0445
*         IF ATETXRB=R8IRB->RBEP THEN/* IRB FOR SAME EXIT RTN ?      */
         SLR   @15,@15                                             0445
         ICM   @15,7,ATETXRB(R3WORKAD)                             0445
         C     @15,RBEP(,R8IRB)                                    0445
         BNE   @RF00445                                            0445
*           DO;                     /* DO000009: YES, REUSE THIS IRB */
*/* P INCREMENT IRB USE COUNT BY 1                                   */
*                                                                  0447
*             R8IRB->RBUSE=R8IRB->RBUSE+ONE;/* USE COUNT             */
         LA    @15,1                                               0447
         SLR   @14,@14                                             0447
         IC    @14,RBUSE(,R8IRB)                                   0447
         ALR   @15,@14                                             0447
         STC   @15,RBUSE(,R8IRB)                                   0447
*/* S UNLOCK: RELEASE LOCAL LOCK                                     */
*                                                                  0448
*             RFY                                                  0448
*               R15RETCD RSTD;                                     0448
*             CALL UNLOCK;          /* RELEASE LOCAL LOCK (ALTERS  0449
*                                      REGS 0,1,11,13,15)            */
         BAL   @14,UNLOCK                                          0449
*/* D (YES,AT001390,NO,) ANY ERRORS ?                                */
*                                                                  0450
*             IF R15RETCD^=X00NOERR THEN/* IF ERROR OCCURRED         */
         LTR   R15RETCD,R15RETCD                                   0450
         BNZ   @RT00450                                            0450
*               GOTO AT001390;      /* GO TO EXIT                    */
*             RFY                                                  0452
*               R15RETCD UNRSTD;                                   0452
*ATFP50:                                                           0453
*             WKFCODE=FCODE50;      /* LOCK RELEASED                 */
*                                                                  0453
ATFP50   MVI   WKFCODE(R12SVRB),X'32'                              0453
*             /*******************************************************/
*             /*                                                     */
*             /* OBTAIN STORAGE FOR TCB AND IQE                      */
*             /*                                                     */
*             /*******************************************************/
*                                                                  0454
*/* L (,AT001100) GETMAIN FOR TCB, IQE, FROM SUBPOOL 253             */
*                                                                  0454
*             RFY                                                  0454
*               R0WORK RSTD;                                       0454
*             RFY                                                  0455
*               R1AREA RSTD;                                       0455
*             R0WORK=LENGTH(TCBFIX)+LENGTH(TCB)+LENGTH(TCBXTNT2)+LENGTH
*                 (IQESECT);        /* GETMAIN LENGTH                */
         LA    R0WORK,384                                          0456
*             GENERATE SETS(R1AREA) REFS(R0WORK,SPID253) NOFLOWS;  0457
         SPACE
         ICM   R0WORK,8,SPID253        SET SP ID INTO HIGH ORDER BYTE
         GETMAIN R,LV=(0)              GET STORAGE FOR TCB, IQE
         SPACE
*             RFY                                                  0458
*               R0WORK UNRSTD;                                     0458
*             R7IQE=R1AREA;         /* IQE ADDR                      */
         LR    R7IQE,R1AREA                                        0459
*             RFY                                                  0460
*               R1AREA UNRSTD;                                     0460
*             GOTO AT001100;        /* GO PROCESS TCB, IQE           */
         B     AT001100                                            0461
*           END;                    /* END00009                      */
*/* AT001090: P (,%FL01080) GET NEXT SUBTASK PTR                     */
*                                                                  0463
*AT001090:                                                         0463
*         R2WORK=R2WORK->TCBNTC;    /* ADDR NEXT SUBTASK             */
@RF00445 DS    0H                                                  0463
AT001090 L     R2WORK,TCBNTC(,R2WORK)                              0463
*       END;                        /* END00010                      */
@DE00440 LTR   R2WORK,R2WORK                                       0464
         BNZ   @DL00440                                            0464
*       RFY                                                        0465
*         R2WORK UNRSTD;                                           0465
*       RFY                                                        0466
*         R7IQE UNRSTD;                                            0466
*       RFY                                                        0467
*         R8IRB UNRSTD;                                            0467
*                                                                  0467
*       /*************************************************************/
*       /*                                                           */
*       /* NO REUSABLE IRB EXISTS, NEED STORAGE FOR TCB, IRB, IQE    */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0468
*/* %FL01070: S UNLOCK: RELEASE LOCAL LOCK                           */
*                                                                  0468
*       RFY                                                        0468
*         R15RETCD RSTD;                                           0468
*       CALL UNLOCK;                /* RELEASE LOCAL LOCK            */
         BAL   @14,UNLOCK                                          0469
*/* D (YES,AT001390,NO,) ANY ERRORS ?                                */
*                                                                  0470
*       IF R15RETCD^=X00NOERR THEN  /* CHECK FOR ERROR IN UNLOCK     */
         LTR   R15RETCD,R15RETCD                                   0470
         BNZ   @RT00470                                            0470
*         GOTO AT001390;            /* ON ERROR, GO EXIT             */
*       RFY                                                        0472
*         R15RETCD UNRSTD;                                         0472
*ATFP60:                                                           0473
*       WKFCODE=FCODE60;            /* LOCK RELEASED                 */
ATFP60   MVI   WKFCODE(R12SVRB),X'3C'                              0473
*/* L CIRB: BUILD IRB AND GET STORAGE FOR TCB,IQE FROM SUBPOOL 253   */
*/*       CIRB SVC BUILDS AN IRB,WITH PROBLEM PROGRAM SAVE AREA IN   */
*/*       SUBPOOL 250. STORAGE FOR A TCB AND AN IQE IS OBTAINED.     */
*/*       THE IRB AND THE STORAGE FOR THE TCB AND IQE ARE IN SUB-    */
*/*       POOL 253.                                                  */
*                                                                  0474
*       RFY                                                        0474
*         R0WORK RSTD;                                             0474
*       RFY                                                        0475
*         R1AREA RSTD;                                             0475
*       R0WORK=ATETXRB;             /* ENTRY POINT ADDR              */
         SLR   R0WORK,R0WORK                                       0476
         ICM   R0WORK,7,ATETXRB(R3WORKAD)                          0476
*       R1WORK=(LENGTH(CIRBAREA)/EIGHT);/* NEW TCB AND IQE IN      0477
*                                      WORKAREA                      */
         LA    R1WORK,48                                           0477
*       GENERATE REFS(R0WORK,R1AREA) SETS(R1AREA) NOFLOWS;         0478
         SPACE
         CIRB  EP=(0),STAB=(DYN),SVAREA=YES,RETIQE=NO,                 *
               WKAREA=(1)
         SPACE
*       RFY                                                        0479
*         R0WORK UNRSTD;                                           0479
*       RFY                                                        0480
*         R8IRB RSTD;                                              0480
*       RFY                                                        0481
*         R7IQE RSTD;                                              0481
*       R8IRB=R1AREA;               /* ADDRESS OF THE NEW IRB        */
         LR    R8IRB,R1AREA                                        0482
*       R7IQE=R1AREA+ADDR(IRBLEN)-ADDR(RBPRFLNA);/* IQE ADDR         */
         LA    R7IQE,IRBLEN                                        0483
         ALR   R7IQE,R1AREA                                        0483
         LA    @02,RBPRFLNA                                        0483
         SLR   R7IQE,@02                                           0483
*       RFY                                                        0484
*         R1AREA UNRSTD;                                           0484
*                                                                  0484
*/* P SET RBETXR=1 RBIQETP=RBIQENR RBUSE=1 AND RBSIZE                */
*                                                                  0485
*       R8IRB->RBETXR='1'B;         /* INDICATE ETXR IRB             */
         OI    RBETXR(R8IRB),B'00010000'                           0485
*       R8IRB->RBIQETP='10'B;       /* FREE IQE UPON EXIT            */
         NI    RBIQETP(R8IRB),B'11111011'                          0486
         OI    RBIQETP(R8IRB),B'00001000'                          0486
*       R8IRB->RBUSE=ONE;           /* SET INITIAL USE COUNT = 1     */
         MVI   RBUSE(R8IRB),X'01'                                  0487
*       RFY                                                        0488
*         R0WORK RSTD;                                             0488
*       GENERATE REFS(R0WORK,R8IRB,RBSIZE) NOFLOWS;                0489
         SPACE
         LA    R0WORK,IRBDWDS          SIZE OF IRB IN DOUBLEWORDS
         STH   R0WORK,RBSIZE(,R8IRB)     INTO IRB
         SPACE
*       RFY                                                        0490
*         R0WORK UNRSTD;                                           0490
*                                                                  0490
*/* AT001100: P CLEAR TCB AND IQE STORAGE TO ZERO                    */
*                                                                  0491
*AT001100:                          /* PROCESS TCB, IQE              */
*       RFY                                                        0491
*         R1AREA RSTD;                                             0491
AT001100 DS    0H                                                  0492
*       R1AREA=R7IQE;               /* ADDRESSING REGISTER           */
         LR    R1AREA,R7IQE                                        0492
*       AREA=AREA&&AREA;            /* CLEAR 256 BYTES CLEAR       0493
*                                      REMAINING PORTION             */
         XC    AREA(256,R1AREA),AREA(R1AREA)                       0493
*       EVAL(R1AREA+LENGTH(AREA))->AREA(1:LENGTH(TCBFIX)+LENGTH(TCB)+
*           LENGTH(TCBXTNT2)+LENGTH(IQESECT)-LENGTH(AREA))=EVAL(R1AREA+
*           LENGTH(AREA))->AREA(1:LENGTH(TCBFIX)+LENGTH(TCB)+LENGTH(
*           TCBXTNT2)+LENGTH(IQESECT)-LENGTH(AREA))&&EVAL(R1AREA+LENGTH
*           (AREA))->AREA(1:LENGTH(TCBFIX)+LENGTH(TCB)+LENGTH(TCBXTNT2)
*           +LENGTH(IQESECT)-LENGTH(AREA));                        0494
         LA    @02,256                                             0494
         ALR   @02,R1AREA                                          0494
         XC    AREA(128,@02),AREA(@02)                             0494
*       RFY                                                        0495
*         R1AREA UNRSTD;                                           0495
*       R5NEWTCB=R7IQE+LENGTH(IQESECT)+LENGTH(TCBFIX);/* ADDR NEW TCB*/
         LA    R5NEWTCB,56                                         0496
         ALR   R5NEWTCB,R7IQE                                      0496
*/* P SET TCBIQE = IQE ADDR, TCBETXR = 1                             */
*                                                                  0497
*       R5NEWTCB->TCBIQE=R7IQE;     /* IQE ADDR INTO NEW TCB         */
         ST    R7IQE,TCBIQE(,R5NEWTCB)                             0497
*       R5NEWTCB->TCBFETXR='1'B;    /* ETXR FLAG ON                  */
         OI    TCBFETXR(R5NEWTCB),B'00000010'                      0498
*/* P (,%FL01390) INITIALIZE NEW IQE                                 */
*                                                                  0499
*       R7IQE->IQEIRB=R8IRB;        /* IRB ADDR INTO NEW IQE         */
         ST    R8IRB,IQEIRB(,R7IQE)                                0499
*       RFY                                                        0500
*         R8IRB UNRSTD;                                            0500
