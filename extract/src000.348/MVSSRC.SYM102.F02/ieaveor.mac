         TITLE 'IEAVEOR - VS2 ETM EXIT/EOT - SVC 3                     *
                        '
IGC003   CSECT ,                                                01S0001
@PROLOG  DS    0H                                               01S0002
CVTPTR   EQU 16
RBPRFXLN EQU 64
RBPRFLNA EQU 32
PRBLEN   EQU 136
SIRBLEN  EQU 200
TIRBLEN  EQU 136
IRBLEN   EQU 128
SVRBLEN  EQU 224
         USING IGC003,R6CSECT
         MODID BR=YES
*/* P  SAVE R15-R1 FROM ENTRY IN TCB RSA AND DO INITIALIZATION       */
*                                                               01S0069
*   TCBGRS15=R15;                   /* SAVE R15                      */
         ST    R15,TCBGRS15(,R4TCB)                             01S0069
*   TCBGRS0=R0;                     /* SAVE R0                       */
         ST    R0,TCBGRS0(,R4TCB)                               01S0070
*   TCBGRS1=R1;                     /* SAVE R1                       */
         ST    R1,TCBGRS1(,R4TCB)                               01S0071
*   TCBNONPR='0'B;                  /* SET TASK IN A PREEMPTIVE STATE
*                                      AGAIN                 @Z40FPYK*/
         NI    TCBNONPR(R4TCB),B'11101111'                      01S0072
*   R8DSECT=WSALEXIT;               /* ADDRESS EXIT WORK/SAVE AREA   */
         L     @12,ASCBASXB(,R7ASCB)                            01S0073
         L     @12,ASXBSPSA(,@12)                               01S0073
         L     R8DSECT,WSALEXIT(,@12)                           01S0073
*   GEN(USING @DATD,R8DSECT)                                    01S0074
*   REFS(R8DSECT);                  /* DATAREG(8)                    */
         USING @DATD,R8DSECT
*   R14SAVE=CVTEXP1;                /* SAVE RETURN ADDRESS           */
         L     @12,CVTEXP1(,R3CVT)                              01S0075
         ST    @12,R14SAVE                                      01S0075
*   R13AREA=ADDR(REGSAVE);          /* ADDRESS SUBROUTINE SAVEAREA   */
         LA    R13AREA,REGSAVE                                  01S0076
*   RFY                                                         01S0077
*    (R14,                                                      01S0077
*     R15) UNRSTD;                  /* RELEASE ENTRY REGISTERS FOR
*                                      SUBROUTINE CALLS              */
*   FLAGS=0;                        /* INITIALIZE WSA BITS           */
         SLR   @12,@12                                          01S0078
         ST    @12,FLAGS                                        01S0078
*TESTREGS:                          /* TESTING LABEL                 */
*                                                               01S0079
*   /*****************************************************************/
*   /*                                                               */
*   /* EXIT TERMINATES NOT ONLY RB'S BUT SPIE EXITS RUNNING UNDER    */
*   /* RB'S AND MUST DISTINGUISH THE TWO CASES WITHOUT RECEIVING ANY */
*   /* DATA FROM THE EXITING PROGRAM. TOWARD THIS END PFLIH INVOKES  */
*   /* SPIE EXITS WITH R14 ADDRESSING AN SVC3 INSTRUCTION RESERVED   */
*   /* FOR SPIE EXIT TERMINATION. IF A TASK WHICH HAS NOT ISSUED SPIE*/
*   /* EXECUTES THAT SVC3 THEN EXIT TERMINATES THE CURRENT RB        */
*   /* NORMALLY.                                                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0079
*/* D (YES,DOSPIE,NO,)  SVC3 FROM PCIH AND SPIE ISSUED               */
*                                                               01S0079
*   IF PSWECIA                      /* IF INSTRUCTION ADDRESS FROM
*                                      SVC 3                         */
*       =ADDR(IEAPSVC3)             /* EQUALS ADDRESS OF BYTE   01S0079
*                                      FOLLOWING PCIH SVC 3          */
*       &TCBPIEA^=0                 /* AND TASK HAS ISSUED SPIE      */
*       &SCAPIE^=0                  /* AND SPIE VALIDITY CHECK HAS
*                                      NOT FAILED                    */
*       &PIEPICAA^=0 THEN           /* AND TASK HAS NOT CANCELLED
*                                      SPIE THEN                     */
TESTREGS L     @12,@CA02028                                     01S0079
         C     @12,PSWECIA(,R5RB)                               01S0079
         BNE   @RF00079                                         01S0079
         L     @12,TCBPIEA-1(,R4TCB)                            01S0079
         LA    @12,0(,@12)                                      01S0079
         LTR   @12,@12                                          01S0079
         BZ    @RF00079                                         01S0079
         L     @12,TCBPIE(,R4TCB)                               01S0079
         L     @12,SCAPIE(,@12)                                 01S0079
         LTR   @12,@12                                          01S0079
         BZ    @RF00079                                         01S0079
         L     @12,PIEPICAA-1(,@12)                             01S0079
         LA    @12,0(,@12)                                      01S0079
         LTR   @12,@12                                          01S0079
         BNZ   @RT00079                                         01S0079
*     GO TO DOSPIE;                 /* GO TERMINATE SPIE EXIT        */
*                                                               01S0081
*   /*****************************************************************/
*   /*                                                               */
*   /* AN RB MAY REQUEST BARRING FROM ITS TASK OF STATUS STOPS AND   */
*   /* ATTENTION EXITS FOR THE DURATION OF THE EXISTENCE OF THAT RB. */
*   /* EXIT UNBARS THE TASK UPON TERMINATION OF SUCH AN RB IF THE    */
*   /* TASK HAS NO OTHER SUCH RB.                                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0081
*/* P  PERMIT STOP'S AND ATTN'S IF NO RB EXCEPT EXITOR BARS THEM     */
*                                                               01S0081
*   IF RBATTN='1'B                  /* IF EXITING RB REQUESTS   01S0081
*                                      ATTN/STOP BARRING             */
*       &RBTCBNXT='0'B THEN         /* AND HAS MOTHER RB THEN        */
@RF00079 TM    RBATTN(R5RB),B'00100000'                         01S0081
         BNO   @RF00081                                         01S0081
         TM    RBTCBNXT(R5RB),B'10000000'                       01S0081
         BNZ   @RF00081                                         01S0081
*     DO;                           /* TRY TO UNBAR STOP'S AND ATTN'S
*                                      FROM TASK                     */
*       R10WORK=RBLINKB;            /* ADDRESS RB FOR LOOP ON RB
*                                      CHAIN                         */
         L     R10WORK,RBLINKB-1(,R5RB)                         01S0083
         LA    R10WORK,0(,R10WORK)                              01S0083
*       DO WHILE                    /* SCAN RB QUEUE FOR ANOTHER RB
*                                      REQUESTING BARRING WHILE      */
*             (R10WORK->RBTCBNXT='0'B/* END OF RB QUEUE NOT REACHED  */
*             &R10WORK->RBATTN='0'B);/* AND ANOTHER BARRING ANCESTOR
*                                      NOT FOUND                     */
         B     @DE00084                                         01S0084
@DL00084 DS    0H                                               01S0085
*         R10WORK=R10WORK->RBLINKB; /* ADDRESS NEXT MOTHER RB        */
         L     @12,RBLINKB-1(,R10WORK)                          01S0085
         LA    @12,0(,@12)                                      01S0085
         LR    R10WORK,@12                                      01S0085
*       END;                        /* LOOP TO TEST EXIT CONDITIONS  */
@DE00084 TM    RBTCBNXT(R10WORK),B'10000000'                    01S0086
         BNZ   @DC00084                                         01S0086
         TM    RBATTN(R10WORK),B'00100000'                      01S0086
         BZ    @DL00084                                         01S0086
@DC00084 DS    0H                                               01S0087
*       IF R10WORK->RBATTN='0'B THEN/* IF NO BARRING RB FOUND THEN   */
         TM    RBATTN(R10WORK),B'00100000'                      01S0087
         BNZ   @RF00087                                         01S0087
*         TCBATT='0'B;              /* UNBAR STOP/ATTN'S FROM TASK   */
         NI    TCBATT(R4TCB),B'11011111'                        01S0088
*TESTBAR:                                                       01S0089
*       GEN(DS 0H);                 /* TESTING LABEL                 */
@RF00087 DS    0H                                               01S0089
TESTBAR  DS 0H
*     END;                          /* UNBARRING ATTEMPT COMPLETED   */
*                                                               01S0091
*   /*****************************************************************/
*   /*                                                               */
*   /* MAKING STOP'S PERMISSABLE MAKES ANY DEFERRED STOP COMPLETABLE.*/
*   /* EXIT NOTIFIES STATUS STOP SYNCH OF DEFERRED STOP COMPLETION.  */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0091
*/* P  COMPLETE STOP IF TASK HAS STOP PENDING AND DOESN'T BAR STOP'S */
*                                                               01S0091
*   IF TCBSTPPR='1'B THEN                                       01S0091
@RF00081 TM    TCBSTPPR(R4TCB),B'01000000'                      01S0091
         BNO   @RF00091                                         01S0091
*     DO;                           /* IF TASK HAS STOP PENDING      */
*       IF TCBATT='0'B THEN         /* AND TASK DOESN'T BAR STOPS
*                                      THEN                          */
         TM    TCBATT(R4TCB),B'00100000'                        01S0093
         BNZ   @RF00093                                         01S0093
*         DO;                       /* COMPLETE STOP                 */
*           TCBPNDSP='1'B;          /* SET TASK NONDISPATCHABLE      */
         OI    TCBPNDSP(R4TCB),B'00000001'                      01S0095
*           TCBSTPP='1'B;           /* INDICATE NONDISPATCHABILITY
*                                      DUE TO STOP                   */
         OI    TCBSTPP(R4TCB),B'01000000'                       01S0096
*           TCBSTPPR='0'B;          /* INDICATE TASK HAS NO STOP
*                                      PENDING                       */
         NI    TCBSTPPR(R4TCB),B'10111111'                      01S0097
*STATUS1:                           /* TESTING LABEL                 */
*           CALL IEAVESSS;          /* DECREMENT STATUS STOP    01S0098
*                                      SYNCH'ER'S WAIT COUNT         */
STATUS1  L     @15,@CV02049                                     01S0098
         BALR  @14,@15                                          01S0098
*         END;                      /* STOP COMPLETED                */
*       ELSE                                                    01S0100
*         DO;                       /* CHECK END OF TASK             */
         B     @RC00093                                         01S0100
@RF00093 DS    0H                                               01S0101
*           IF TCBEOT='1'B          /* IF END OF TASK                */
*               &RBTCBNXT='1'B THEN                             01S0101
         TM    TCBEOT(R4TCB),B'00000100'                        01S0101
         BNO   @RF00101                                         01S0101
         TM    RBTCBNXT(R5RB),B'10000000'                       01S0101
         BNO   @RF00101                                         01S0101
*             DO;                   /* AND LAST RB ON CHAIN          */
*               CALL IEAVESSS;      /* GO TO STATUS FOR CLEAN UP     */
         L     @15,@CV02049                                     01S0103
         BALR  @14,@15                                          01S0103
*             END;                  /* CLEAN UP COMPLETE             */
*         END;                      /* END END OF TASK CHECK         */
@RF00101 DS    0H                                               01S0106
*     END;                          /* STOP PENDING COMPLETE         */
*                                                               01S0106
@RC00093 DS    0H                                               01S0107
*   /*****************************************************************/
*   /*                                                               */
*   /* A TASK BECOMES NONDISPATCHABLE WHEN THE MOTHER RB HAS         */
*   /* OUTSTANDING WAIT'S OR EXIT HAS COMPLETED A STATUS STOP.       */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0107
*/* S CSPROC:  DECREMENT READY TCB COUNT ON WAIT OR STOP             */
*                                                               01S0107
*   IF RBTCBNXT='0'B&               /* IF TASK NOT ENDED BUT         */
*       (RBLINKB->RBWCF>0           /* EITHER MOTHER RB WAITING      */
*       ×TCBSTPP='1'B) THEN         /* OR TASK STOPPED BY EXIT THEN  */
@RF00091 TM    RBTCBNXT(R5RB),B'10000000'                       01S0107
         BNZ   @RF00107                                         01S0107
         L     @12,RBLINKB-1(,R5RB)                             01S0107
         CLI   RBWCF(@12),0                                     01S0107
         BH    @RT00107                                         01S0107
         TM    TCBSTPP(R4TCB),B'01000000'                       01S0107
         BNO   @RF00107                                         01S0107
@RT00107 DS    0H                                               01S0108
*     CALL CSPROC;                  /* DECREMENT READY TASK COUNT    */
         BAL   @14,CSPROC                                       01S0108
*                                                               01S0109
*   /*****************************************************************/
*   /*                                                               */
*   /* SVC 13 PERFORMS NORMAL TASK TERMINATION WHEN EXIT RECOGNIZES  */
*   /* THE LAST RB AS EXITING FOR THE FIRST TIME. SVC 13 RUNS UNDER  */
*   /* THE LAST RB AND SETS IT UP FOR SVC 3 WHICH TERMINATES IT AFTER*/
*   /* SVC 13 HAS EXITED.                                            */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0109
*/* D (YES,DOSVC13,NO,)  LAST RB EXITING AND SVC 13 NOT EXECUTED     */
*                                                               01S0109
*   IF RBTCBNXT='1'B                /* IF TCB HAS ONLY TERMINATING RB*/
*       &TCBEOT='0'B THEN           /* AND TASK TERMINATION HAS NOT
*                                      EXECUTED THEN                 */
@RF00107 TM    RBTCBNXT(R5RB),B'10000000'                       01S0109
         BNO   @RF00109                                         01S0109
         TM    TCBEOT(R4TCB),B'00000100'                        01S0109
         BZ    @RT00109                                         01S0109
*     GO TO DOSVC13;                /* BRANCH TO EXECUTE IT          */
*                                                               01S0110
*   /*****************************************************************/
*   /*                                                               */
*   /* OTHERWISE EXIT TERMINATES THE EXITING RB.                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0111
*/* SVC3RB: L  TERMINATE RB -- CHART SVC3RB                          */
*                                                               01S0111
*                                                               02S0111
*/* SVC3RB: CHART (AREF)                                             */
*/* HEADER                                                      02S0111
*/* 3.1.7.9.17 - EXIT/EOT - SVC 3 - IEAVEOR                     02S0111
*/* SECOND OF TWO CHARTS - TERMINATION OF CONTROL LEVEL              */
*/* E  RB TERMINATION -- FROM CHART SVC3                             */
*                                                               02S0111
*   ;                                                           02S0111
*                                                               02S0111
@RF00109 DS    0H                                               02S0112
*   /*****************************************************************/
*   /*                                                               */
*   /* THE INNER HALF OF EXIT PERFORMS SIMPLE RB TERMINATION         */
*   /* DEPENDING ON SUPERVISOR/PROGRAM/INTERRUPT/SYSTEM-INTERRUPT    */
*   /* REQUEST BLOCK TYPE.                                           */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S0112
*                                                               02S0112
*   /*****************************************************************/
*   /*                                                               */
*   /* EXIT DEQUEUES THE TERMINATING RB (UNLESS THIS WOULD LEAVE THE */
*   /* TCB QUEUED BUT WITHOUT AN RB) MARKS THE RB INACTIVE AND THEN  */
*   /* CALLS STAE CONTROL BLOCK PURGE IF THE RB HAS ISSUED STAE OR ON*/
*   /* END OF TASK (IN WHICH CASE THE MOTHER TASK MAY HAVE ISSUED    */
*   /* STAI).                                                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S0112
*/* P  DEQUEUE RB UNLESS LAST RB UNDER TCB                           */
*                                                               02S0112
*   IF RBTCBNXT='0'B THEN           /* IF RB HAS MOTHER RB THEN      */
         TM    RBTCBNXT(R5RB),B'10000000'                       02S0112
         BNZ   @RF00112                                         02S0112
*     TCBRBP=RBLINKB;               /* POINT TCB AT MOTHER RB        */
         L     @12,RBLINKB-1(,R5RB)                             02S0113
         LA    @12,0(,@12)                                      02S0113
         ST    @12,TCBRBP(,R4TCB)                               02S0113
*   ELSE                            /* IF RB IS LAST RB THEN @ZA09499*/
*     NEXT='1'B;                    /* SET SWITCH TO INDICATE SO
*                                                            @ZA09499*/
         B     @RC00112                                         02S0114
@RF00112 OI    NEXT,B'10000000'                                 02S0114
*TESTRB:                            /* TESTING LABEL                 */
*/* S SCBPURGE:  CALL SCB PURGE IF STAE ISSUED OR END OF TASK        */
*                                                               02S0115
*   IF RBSCB='1'B                   /* IF RB HAS ISSUED STAE         */
*       ×RBTCBNXT='1'B THEN         /* OR HAS NO MOTHER THEN         */
@RC00112 DS    0H                                               02S0115
TESTRB   LR    @12,R5RB                                         02S0115
         AL    @12,@CF02210                                     02S0115
         TM    RBSCB-56(@12),B'00000010'                        02S0115
         BO    @RT00115                                         02S0115
         TM    RBTCBNXT(R5RB),B'10000000'                       02S0115
         BNO   @RF00115                                         02S0115
@RT00115 DS    0H                                               02S0116
*     DO;                           /* CALL SCB PURGE                */
*       R0=ADDR(RBSECT);            /* PASS RB ADDRESS               */
         LR    R0,R5RB                                          02S0117
*       R1=0;                       /* INDICATE EXIT NOT XCTL        */
         SLR   R1,R1                                            02S0118
*SCB1:                              /* TESTING LABEL                 */
*       CALL SCBPURGE;              /* INVOKE STAE RTRM              */
SCB1     L     @15,CVTSCBP(,R3CVT)                              02S0119
         BALR  @14,@15                                          02S0119
*SCB2:                              /* TESTING LABEL                 */
*     END;                          /* SCB'S PURGED                  */
SCB2     DS    0H                                               02S0121
*                                                               02S0121
*   /*****************************************************************/
*   /*                                                               */
*   /* THE SVRB CONTAINS REGISTERS 2-14 FROM TIME OF SVC INSTRUCTION */
*   /* EXECUTION AND REGISTERS 15-1 FROM TIME OF SVC ROUTINE RETURN. */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S0121
*/* D (YES,,NO,%DOSVRB)  SVRB EXITING                                */
*                                                               02S0121
*   IF RBFTP='110'B THEN            /* IF SVRB EXITING THEN          */
@RF00115 TM    RBFTP(R5RB),B'11000000'                          02S0121
         BNO   @RF00121                                         02S0121
         TM    RBFTP(R5RB),B'00100000'                          02S0121
         BNZ   @RF00121                                         02S0121
*DOSVRB:                                                        02S0122
*     DO;                           /* TERMINATE SVRB                */
DOSVRB   DS    0H                                               02S0123
*       R14SAVE=CVTEXPRO;           /* SET NORMAL EXIT PROLOGUE ENTRY*/
         L     @12,CVTEXPRO(,R3CVT)                             02S0123
         ST    @12,R14SAVE                                      02S0123
*/* N  USUAL PATH -- SVRB TERMINATION                                */
*/* P  MOVE R2-R14 FROM SVRB TO TCB (R15-R1 DONE AT EXIT ENTRY)      */
*                                                               02S0124
*       ADDR(TCBGRS2)->WORDS13=ADDR(RBGRS2)->WORDS13;/* MOVE 13 02S0124
*                                      REGISTERS                     */
         MVC   WORDS13+56(52,R4TCB),WORDS13+40(R5RB)            02S0124
*TESTREG1:                          /* TESTING LABEL                 */
*                                                               02S0125
*       /*************************************************************/
*       /*                                                           */
*       /* THE SVRB STORAGE SHOULD HAVE BEEN ALLOCATED       @Z40FPYK*/
*       /* BY THE SVC FIRST LEVEL INTERRUPT HANDLER.         @Z40FPYK*/
*       /*                                                           */
*       /*************************************************************/
*                                                               02S0125
*/* S MSS:  FREE SP255 OR CELL SVRB IF DYNAMIC                       */
*/* D (YES,,NO,DOPRB) CAN SVRB BE FREED ?                            */
*                                                               02S0125
*       IF RBFDYN='1'B              /* IF SVRB FREEABLE              */
*           &RBTCBNXT='0'B THEN     /* AND SVRB HAS MOTHER RB THEN   */
TESTREG1 TM    RBFDYN(R5RB),B'00000010'                         02S0125
         BNO   @RF00125                                         02S0125
         TM    RBTCBNXT(R5RB),B'10000000'                       02S0125
         BNZ   @RF00125                                         02S0125
*         DO;                       /* RETURN SVRB TO THE COMMON
*                                      MEMORY SVRB POOL      @Z40FPYK*/
*/* D (YES,,NO,PGMGR) VALID SVRB ?                                   */
*                                                               02S0127
*           RFY                                                 02S0127
*             MBCB BASED(MBCBPTR);                              02S0127
*           MBCBPTR=R5RB-LENGTH(MBCBMAIN)-ADDR(RBPRFLNA);/* START OF
*                                      THE SVRB BUFFER POOL  @Z40FPYK*/
         LR    @12,R5RB                                         02S0128
         SL    @12,@CF00061                                     02S0128
         LA    @11,RBPRFLNA                                     02S0128
         SLR   @12,@11                                          02S0128
         ST    @12,MBCBPTR                                      02S0128
*/* P SET UP TO RETURN SVRB TO POOL                                  */
*                                                               02S0129
*           IF RBNOCELL='1'B THEN                               02S0129
         TM    RBNOCELL(R5RB),B'10000000'                       02S0129
         BO    @RT00129                                         02S0129
*             GO TO PGMGR;          /*                        @M41810*/
*           IF MBCBID=CHARSVRB THEN                             02S0131
         CLC   MBCBID(4,@12),CHARSVRB                           02S0131
         BNE   @RF00131                                         02S0131
*             DO;                   /* DETERMINE IF THIS IS A VALID
*                                      SVRB THAT IS BEING RETURNED TO
*                                      THE SVRB POOL. THIS IS   02S0132
*                                      DETERMINED BY A VERIFICATION
*                                      OF WHETHER THE MBCB IDENTIFIER
*                                      IS 'SVRB'. IF YES THEN THIS IS
*                                      INDEED AN SVRB WHICH BELONGS
*                                      TO THE COMMON SVRB MEMORY
*                                      POOL.                 @Z40FPYK*/
*               R0=ASCBSVRB;        /* SET UP TO RETURN THE SVRB TO
*                                      THE COMMON MEMORY POOL   02S0133
*                                                            @Z40FPYK*/
         L     R0,ASCBSVRB(,R7ASCB)                             02S0133
*               R1=ASCBSYNC;        /* SYNCHRONIZATION COUNTER TO
*                                      CONTROL MULTIPLE ACCESSES TO
*                                      THE ASCB SVRB POOL    @Z40FPYK*/
         L     R1,ASCBSYNC(,R7ASCB)                             02S0134
*/*FREESVRB: P SET UP SVRB LINK FIELD FOR POOL                       */
*                                                               02S0135
*FREESVRB:                                                      02S0135
*               MBCBLINK=ASCBSVRB;  /* UPDATE CURRENT LINK FIELD IN
*                                      THE SVRB BEING FREED. @Z40FPYK*/
FREESVRB L     @12,ASCBSVRB(,R7ASCB)                            02S0135
         L     @11,MBCBPTR                                      02S0135
         ST    @12,MBCBLINK(,@11)                               02S0135
*/* P INCREMENT SYNCHRONOUS COUNTER                                  */
*                                                               02S0136
*               RFY                                             02S0136
*                (R14,                                          02S0136
*                 R15) RSTD;        /* RESTRICT REGISTERS 14 AND 15
*                                                            @Z40FPYK*/
*               R15=R1+1;           /* INCREMENT SYNCHRONOUS COUNT IN
*                                      ASCB                  @Z40FPYK*/
         LA    R15,1                                            02S0137
         ALR   R15,R1                                           02S0137
*/* P SVRB BEING RETURNED TO POOL                                    */
*                                                               02S0138
*               R14=MBCBPTR;        /* SVRB WHICH IS BEING RETURNED
*                                      TO POOL               @Z40FPYK*/
         L     R14,MBCBPTR                                      02S0138
*/* P RETURN SVRB TO POOL                                            */
*                                                               02S0139
*               CDS(R0,R14,ASCBSUPC);/* RETURN SVRB TO COMMON MEMORY
*                                      POOL                  @Z40FPYK*/
         CDS   R0,R14,ASCBSUPC(R7ASCB)                          02S0139
*/* D (YES,FREESVRB,NO,) SVRB RETURNED ?                             */
*                                                               02S0140
*               BC(7,FREESVRB);     /* WAS SVRB RETURNED TO POOL ?
*                                      NO--                  @Z40FPYK*/
         BC    7,FREESVRB                                       02S0140
*/* P (,DOSVRB) SVRB RETURNED SUCCESSFULLY                           */
*                                                               02S0141
*             END;                  /* YES, WE HAVE FINISHED @Z40FPYK*/
*           ELSE                                                02S0142
*             DO;                   /* DETERMINE IF INVALID SVRB OR
*                                      PROGRAM MGR SVRB      @Z40FPYK*/
         B     @RC00131                                         02S0142
@RF00131 DS    0H                                               02S0143
*/*PGMGR: D (YES,,NO,%ABEND) PROGRAM MGR SVRB ?                      */
*                                                               02S0143
*PGMGR:                                                         02S0143
*               IF RBNOCELL='1'B THEN                           02S0143
PGMGR    TM    RBNOCELL(R5RB),B'10000000'                       02S0143
         BNO   @RF00143                                         02S0143
*                 DO;               /* PROGRAM MANAGER SVRB OR  02S0144
*                                      REQUEST TO FREE SVRB  @ZM41810*/
*/* P SET UP TO FREEMAIN RB                                          */
*                                                               02S0145
*                   R1=ADDR(RBPRFXST);/* POINT TO PREFIX WHICH MUST
*                                      BE FREED              @Z40FPYK*/
         LR    R1,R5RB                                          02S0145
         AL    R1,@CF02211                                      02S0145
*                   R0=RBSIZE*8;    /* SIZE OF RB TO BE FREED   02S0146
*                                                            @Z40FPYK*/
         LH    R0,RBSIZE(,R5RB)                                 02S0146
         SLA   R0,3                                             02S0146
*                   R10WORK=R3CVT;  /* SAVE REGISTER CLOBBERED BY
*                                      FREEMAIN              @Z40FPYK*/
         LR    R10WORK,R3CVT                                    02S0147
*/* S (,DOPRB) FREEMAIN:GO FREE THE SVRB                             */
*                                                               02S0148
