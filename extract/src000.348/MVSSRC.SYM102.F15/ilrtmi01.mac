         TITLE 'ILRTMI01 - ESTAE FOR ILRTMRLG AND ILRTMI00             *
                        '
ILRTMI01 CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'ILRTMI01  76.105'                                 0001
         DROP  @15
@PROLOG  DS    0H                                                  0002
*                                                                  0068
         USING  ILRTMI01,REG15
         C     REG0,TWELVE         SDWA PASSED
         BNE   YESSDWA             YES
         LR    BASEREG,REG15       NO, LOAD BASE VALUE
         B     NOSDWA              CODE IS AT END OF MODULE
TWELVE   DC    F'12'               VALUE FOR NO SDWA PASSED
YESSDWA  STM   REG14,REG12,12(REG13) SAVE REGISTERS
         DROP  REG15
         LR    BASEREG,REG15      LOAD BASE VALUE
         USING ILRTMI01,BASEREG
*   /*****************************************************************/
*   /*                                                               */
*   /* RTM PASSED A SDWA (ADDR IN REG 1) AND THE EPATH ADDR IS       */
*   /* POINTED TO BY SDWAPARM. REG 13 POINTS TO A 72 BYTE REG SAVE   */
*   /* AREA.                                                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0069
*   ASMVTPTR=CVTASMVT;              /* GET THE PTR TO THE ASMVT      */
         L     @12,CVTPTR                                          0069
         L     ASMVTPTR,CVTASMVT(,@12)                             0069
*   RFY                                                            0070
*     SDWA BASED(REG1);                                            0070
*   EPAPTR=SDWAPARM;                /* GET PTR TO EPATH              */
*                                                                  0071
         L     EPAPTR,SDWAPARM(,REG1)                              0071
*   /*****************************************************************/
*   /*                                                               */
*   /* SET DEFAULT RECORD INFORMATION AND REQUEST PERCOLATION        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0072
*   DO;                             /* SETRP RC(0)RECORD(YES)RECPARM(
*                                      RECNAMES)                     */
*     RESPECIFY                                                    0073
*      (GPR00P,                                                    0073
*       GPR01P,                                                    0073
*       GPR14P,                                                    0073
*       GPR15P) RSTD;                                              0073
*     GPR01P->SDWARCDE=0;           /* SAVE RC VALUE                 */
         MVI   SDWARCDE(GPR01P),X'00'                              0074
*     GPR01P->SDWARCRD='1'B;        /* TURN ON RECORD INDICATOR      */
         OI    SDWARCRD(GPR01P),B'10000000'                        0075
*     GPR15P=ADDR(RECNAMES);        /* ACCESS RECORD PARAMETER LIST  */
         LA    GPR15P,RECNAMES                                     0076
*     GPR01P->SDWARECP=GPR15P->I256C(1:24);/* COPY RECORD PARAMETERS */
         MVC   SDWARECP(24,GPR01P),I256C(GPR15P)                   0077
*     RESPECIFY                                                    0078
*      (GPR00P,                                                    0078
*       GPR01P,                                                    0078
*       GPR14P,                                                    0078
*       GPR15P) UNRSTD;                                            0078
*   END;                                                           0079
*   ADDR(SDWAVRA)->EPATH=EPATH;     /* COPY EPATH TO SDWA VARIABLE 0080
*                                      RECORDING AREA                */
         MVC   EPATH+404(44,REG1),EPATH(EPAPTR)                    0080
*   SDWAURAL=LENGTH(EPATH);         /* UPDATE SPACE USED COUNT       */
         MVI   SDWAURAL(REG1),X'2C'                                0081
*                                                                  0082
*   /*****************************************************************/
*   /*                                                               */
*   /* SEE IF WE SHOULD PERCOLATE BEFORE WE GET STORAGE              */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0082
*   IF ASMNOTMR=BITON               /* NO TMRLG TASK INDICATOR ON    */
*     THEN                                                         0082
         TM    ASMNOTMR(ASMVTPTR),B'00100000'                      0082
         BO    @RT00082                                            0082
*     GOTO PERCOLAT;                /* YES, QUIT                     */
*   IF EPARECUR=BITON THEN          /* RECURSIVE ERROR CONDITION IN
*                                      TMRLG                         */
         TM    EPARECUR(EPAPTR),B'00000010'                        0084
         BO    @RT00084                                            0084
*     GOTO SETNOTMR;                /* SET NO TMRLG TASK INDICATOR ON
*                                      AND PERC                      */
*   RFY                                                            0086
*    (REG0,                                                        0086
*     REG5,                                                        0086
*     REG15) RSTD;                                                 0086
*                                                                  0086
*   /*****************************************************************/
*   /*                                                               */
*   /* GET STORAGE FOR A WORK AREA. IF THE GETMAIN FAILS, RETURN TO  */
*   /* RTM TO CONTINUE WITH TERMINATION                              */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0087
*   REG5=REG1;                      /* SAVE REG1 AROUND GETMAIN      */
         LR    REG5,REG1                                           0087
*   GEN;                                                           0088
         L     REG0,@SIZDATD     GET LENGTH OF AREA
         GETMAIN RC,LV=(0),SP=255
*   IF REG15^=0 THEN                                               0089
         LTR   REG15,REG15                                         0089
         BZ    @RF00089                                            0089
*     DO;                           /* DID NOT GET A WORK AREA       */
*SETNOTMR:                                                         0091
*       ASMNOTMR=BITON;             /* SET NO TMRLG INDICATOR ON,  0091
*                                      SINCE THE TASK WILL TERMINATE */
SETNOTMR OI    ASMNOTMR(ASMVTPTR),B'00100000'                      0091
*PERCOLAT:                          /* RESTORE REGS AND RETURN TO RTM*/
*       GEN;                                                       0092
PERCOLAT DS    0H                                                  0092
         LM    REG14,REG12,12(REG13)  RESTORE REGS
         BR    REG14
*     END;                          /* FOR DID NOT GET THE STORAGE   */
*                                                                  0094
*   /*****************************************************************/
*   /*                                                               */
*   /* WE DID OBTAIN STORAGE FOR A WORKAREA. NOW SET UP              */
*   /* ADDRESSABILITY AND THE SAVE AREAS                             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0094
*   PTRDATD=REG1;                   /* PUT WORK AREA ADDR IN ITS BASE
*                                      REG                           */
@RF00089 LR    PTRDATD,REG1                                        0094
*   GEN;                                                           0095
         USING @DATD,PTRDATD
         ST    REG13,TMRSAVE+4  SAVE CALLER S SAVE AREA ADDR
         LA    REG15,TMRSAVE
         ST    REG15,8(REG13)   CHAIN THE  SAVE AREAS
         LR    REG13,REG15
*   REG1=REG5;                      /* RESTORE SDWA BASE             */
         LR    REG1,REG5                                           0096
*   SDWAPTR=REG1;                   /* SAVE ADDR OF SDWA             */
         ST    REG1,SDWAPTR                                        0097
*   RFY                                                            0098
*    (REG0,                                                        0098
*     REG15,                                                       0098
*     REG5,                                                        0098
*     REG14) UNRSTD;                                               0098
*   IF EPATMI=BITON THEN                                           0099
         TM    EPATMI(EPAPTR),B'00000100'                          0099
         BNO   @RF00099                                            0099
*     SDWACSCT=TMI00;               /* ILRTMI00 WAS IN CONTROL AT THE
*                                      ERROR                         */
         MVC   SDWACSCT(8,REG1),@CC02153                           0100
*   ELSE                                                           0101
*     IF EPAVSAMI=BITON THEN                                       0101
         B     @RC00099                                            0101
@RF00099 TM    EPAVSAMI(EPAPTR),B'10000000'                        0101
         BNO   @RF00101                                            0101
*       SDWACSCT=VSAMI;             /* ILRVSAMI WAS IN CONTROL       */
         MVC   SDWACSCT(8,REG1),@CC02155                           0102
*   RFY                                                            0103
*     REG5 RSTD;                                                   0103
@RF00101 DS    0H                                                  0103
@RC00099 DS    0H                                                  0104
*   REG5=(SDWAABCC&CMPCMASK);       /* GET COMPLETION CODE FROM SDWA */
         L     REG5,SDWAABCC(,REG1)                                0104
         N     REG5,@CF02137                                       0104
*   IF REG5=RCONLY1×REG5=RCONLY2 THEN/* A RECORD ONLY ABEND          */
         C     REG5,@CF02139                                       0105
         BE    @RT00105                                            0105
         C     REG5,@CF02141                                       0105
         BNE   @RF00105                                            0105
@RT00105 DS    0H                                                  0106
*     DO;                           /* RECORD AND RETRY AT NSI       */
*       RFY                                                        0107
*         REG5 UNRSTD;                                             0107
*       RFY                                                        0108
*         RTYAREG RSTD;                                            0108
*       RTYAREG=SDWANXT1;           /* MOVE RETRY ADDR TO REG FOR  0109
*                                      SETRP                         */
         L     RTYAREG,SDWANXT1(,REG1)                             0109
*       SDWASRSV(1:LENGTH(RCOREGS))=RCOREGS;/* MOVE VALUES FOR REGS
*                                      0-14 TO SDWA FOR UPDATE       */
         L     @12,EPARTYRG(,EPAPTR)                               0110
         MVC   SDWASRSV(60,REG1),RCOREGS(@12)                      0110
*       GOTO TMSETRP;               /* GO ISSUE SETRP AND RETURN TO
*                                      RTM                           */
         B     TMSETRP                                             0111
*       RFY                                                        0112
*         RTYAREG UNRSTD;                                          0112
*     END;                          /* FOR RECORD AND RETRY          */
*   IF EPAMAST=BITON THEN                                          0114
@RF00105 TM    EPAMAST(EPAPTR),B'00001000'                         0114
         BNO   @RF00114                                            0114
*     DO;                                                          0115
*                                                                  0115
*       /*************************************************************/
*       /*                                                           */
*       /* M.S.I. POSTED, TMRLG ACE PROCESSOR WAS IN CONTROL         */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0116
*       IF EPAVWKA^=0 THEN                                         0116
         L     @12,EPAVWKA(,EPAPTR)                                0116
         LTR   @12,@12                                             0116
         BZ    @RF00116                                            0116
*         DO;                       /* VSAMI HAD A WORK AREA         */
*           RFY                                                    0118
*            (REG0,                                                0118
*             REG2,                                                0118
*             REG5,                                                0118
*             REG15) RSTD;                                         0118
*           REG0=SWKLEN;            /* LENGTH OF WORK AREA           */
         LA    REG0,512                                            0119
*           REG2=EPAVWKA;           /* GET ADDR OF AREA              */
         LR    REG2,@12                                            0120
*           REG5=ASMVTPTR;          /* REG 3 GETS DESTROYED          */
         LR    REG5,ASMVTPTR                                       0121
*           CALL IEAVEADV;          /* ADDR VERIFY THE WORK AREA     */
         L     @12,CVTPTR                                          0122
         L     @15,CVTADV(,@12)                                    0122
         BALR  @14,@15                                             0122
*           ASMVTPTR=REG5;          /* RESTORE REG 3                 */
         LR    ASMVTPTR,REG5                                       0123
*           IF REG15=0 THEN                                        0124
         LTR   REG15,REG15                                         0124
         BNZ   @RF00124                                            0124
*             DO;                   /* WORK AREA ADDRESSABLE         */
*               IF SDWAPERC=BITON THEN                             0126
         TM    SDWAPERC(REG1),B'00010000'                          0126
         BNO   @RF00126                                            0126
*                 DO;               /* SOMEONE PERCOLATED TO US      */
*                   IF EPARPLB=BITON THEN                          0128
         TM    EPARPLB(EPAPTR),B'00000001'                         0128
         BNO   @RF00128                                            0128
*                     DO;           /* ILRVSAMI HAS BUILT ITS RPL    */
*                       EPARPLB=BITOFF;/* RESET FOR RECOVERY NEXT  0130
*                                      TIME THROUGH(?)               */
*                                                                  0130
         NI    EPARPLB(EPAPTR),B'11111110'                         0130
*                       /*********************************************/
*                       /*                                           */
*                       /* TERMINATE ANY OUTSTANDING VSAM REQUESTS   */
*                       /* GENERATED BY ILRVASMI                     */
*                       /*                                           */
*                       /*********************************************/
*                                                                  0131
*                       RFY                                        0131
*                        (REG0,                                    0131
*                         REG14,                                   0131
*                         REG15) RSTD;                             0131
*                       REG1=EPAVWKA;/* GET ADDR OF RPL WHICH IS 1ST
*                                      AREA IN WORK AREA             */
         L     REG1,EPAVWKA(,EPAPTR)                               0132
*                       GEN(ENDREQ RPL=(1));                       0133
         ENDREQ RPL=(1)
*                     END;          /* FOR VSAMI HAS BUILT ITS RPL   */
*                 END;              /* FOR SOMEONE PERCOLATED        */
*                                                                  0135
@RF00128 DS    0H                                                  0136
*               /*****************************************************/
*               /*                                                   */
*               /* WE DID NOT GET PERCOLATED TO FROM ANYONE          */
*               /*                                                   */
*               /*****************************************************/
*                                                                  0136
*               REG1=EPAVWKA;       /* GMA REQUIRES AREA IN REG 1    */
@RF00126 L     REG1,EPAVWKA(,EPAPTR)                               0136
*               EPAVWKA=0;          /* MAKE SURE EPATH IS REUSABLE   */
*                                                                  0137
         SLR   @12,@12                                             0137
         ST    @12,EPAVWKA(,EPAPTR)                                0137
*               /*****************************************************/
*               /*                                                   */
*               /* BEGIN ILRGMA MACRO EXPANSION                      */
*               /*                                                   */
*               /*****************************************************/
*                                                                  0138
*               DO;                 /* ILRGMA FREE                 0138
*                                      CELL(SWK)WORKREGS(REG11,REG12)*/
*                 DECLARE           /* ASMVT VECTOR TO ILRPEX        */
*                   ILRPEX ENTRY BASED(ASMPEX) VALRG(0);           0139
*                 DECLARE           /* MAP FOR CELL CONTROL WORDS    */
*                   1 GMACELL BASED BDY(WORD),                     0140
*                    2 GMANEXT PTR(31);/* PTR NEXT FREE CELL         */
*                 RESPECIFY                                        0141
*                  (R0,                                            0141
*                   R1,                                            0141
*                   REG11,                                         0141
*                   REG12,                                         0141
*                   R14,                                           0141
*                   R15) RESTRICTED;                               0141
*                 RESPECIFY                                        0142
*                   ASMPOOLS BASED(REG11);                         0142
*                 RESPECIFY                                        0143
*                   GMACELL BASED(R1);                             0143
*                 REG11=ADDR(ASMSWKPC);                            0144
         LA    REG11,ASMSWKPC(,ASMVTPTR)                           0144
*                 R14=ASMCPAVL;                                    0145
         L     R14,ASMCPAVL(,REG11)                                0145
*GMA00112:                                                         0146
*                 GMANEXT=R14;      /* PUT ADDR CURRENT FIRST IN   0146
*                                      INPUT CELL                    */
GMA00112 ST    R14,GMANEXT(,R1)                                    0146
*                 CS(R14,R1,ASMCPAVL);/* ADD INPUT CELL TO POOL      */
         CS    R14,@01,ASMCPAVL(REG11)                             0147
*                 BC(7,GMA00112);                                  0148
         BC    7,GMA00112                                          0148
*                 RESPECIFY                                        0149
*                  (R0,                                            0149
*                   R1,                                            0149
*                   REG11,                                         0149
*                   REG12,                                         0149
*                   R14,                                           0149
*                   R15) UNRESTRICTED;                             0149
*               END;                /* ILRGMA FREE                 0150
*                                      CELL(SWK)WORKREGS(REG11,REG12)*/
*                                                                  0150
*               /*****************************************************/
*               /*                                                   */
*               /* END ILRGMA MACRO EXPANSION                        */
*               /*                                                   */
*               /*****************************************************/
*                                                                  0151
*               RFY                                                0151
*                (REG0,                                            0151
*                 REG2,                                            0151
*                 REG5,                                            0151
*                 REG15) UNRSTD;                                   0151
*             END;                  /* FOR WORK AREA ADDRESSABLE     */
*                                                                  0153
*           /*********************************************************/
*           /*                                                       */
*           /* SEE IF THERE IS AN IO BUFFER TO FREE                  */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0153
*           IF EPABASP^=0 THEN                                     0153
@RF00124 SLR   @15,@15                                             0153
         L     @12,EPABASP(,EPAPTR)                                0153
         CR    @12,@15                                             0153
         BE    @RF00153                                            0153
*             DO;                   /* VSAMI HAD AN IO BUFFER        */
*               RFY                                                0155
*                 REG0 RSTD;                                       0155
*               REG1=EPABASP;       /* ADDR OF BUFFER                */
         LR    REG1,@12                                            0156
*               REG0=BUFLEN;        /* GET LENGTH                    */
         LA    REG0,2048                                           0157
*               EPABASP=0;          /* KEEP EPATH REUSABLE           */
         ST    @15,EPABASP(,EPAPTR)                                0158
*               GEN;                                               0159
         FREEMAIN RC,A=(1),SP=230,LV=(0)
*               RFY                                                0160
*                 REG0 UNRSTD;                                     0160
*             END;                  /* FOR VSAMI HAD IO BUFFER       */
*         END;                      /* FOR VSAMI HAD A WORK AREA     */
@RF00153 DS    0H                                                  0163
*       RFY                                                        0163
*        (REG0,                                                    0163
*         REG15) RSTD;                                             0163
*                                                                  0163
@RF00116 DS    0H                                                  0164
*       /*************************************************************/
*       /*                                                           */
*       /* SET UP AND VALIDITY CHECK THE ACE                         */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0164
*       REG0=EPAACE;                /* GET ADDR OF ACE               */
         L     REG0,EPAACE(,EPAPTR)                                0164
*       REG1=SDWAPTR;                                              0165
         L     REG1,SDWAPTR                                        0165
*       STM(REG2,REG7,SAVEDES);     /* REGS 2-7 DESTROYED            */
         STM   REG2,REG7,SAVEDES                                   0166
*       CALL ILRVACE;                                              0167
         L     @15,ASMPVACE(,ASMVTPTR)                             0167
         BALR  @14,@15                                             0167
*       LM(REG2,REG7,SAVEDES);      /* RESTORE REGS                  */
         LM    REG2,REG7,SAVEDES                                   0168
*       EPAACE=0;                                                  0169
         SLR   @12,@12                                             0169
         ST    @12,EPAACE(,EPAPTR)                                 0169
*       IF REG15=0 THEN                                            0170
         CR    REG15,@12                                           0170
         BNE   @RF00170                                            0170
*         DO;                       /* ACE IS VALID RETURN IT TO ITS
*                                      POOL                          */
*           RFY                                                    0172
*             REG15 UNRSTD;                                        0172
*           REG1=REG0;              /* GMA REQUIRES ACE ADDR IN REG1 */
*                                                                  0173
         LR    REG1,REG0                                           0173
*           /*********************************************************/
*           /*                                                       */
*           /* BEGIN ILRGMA MACRO EXPANSION                          */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0174
*           DO;                     /* ILRGMA FREE                 0174
*                                      CELL(ACE)WORKREGS(REG11,REG12)*/
*             RESPECIFY                                            0175
*              (R0,                                                0175
*               R1,                                                0175
*               REG11,                                             0175
*               REG12,                                             0175
*               R14,                                               0175
*               R15) RESTRICTED;                                   0175
*             RESPECIFY                                            0176
*               ASMPOOLS BASED(REG11);                             0176
*             RESPECIFY                                            0177
*               GMACELL BASED(R1);                                 0177
*             REG11=ADDR(ASMACEPC);                                0178
         LA    REG11,ASMACEPC(,ASMVTPTR)                           0178
*             LM(R14,R15,ASMCPRSQ); /* GET CONTENTS OF Q             */
         LM    R14,R15,ASMCPRSQ(REG11)                             0179
*GMA00121:                                                         0180
*             IF R14^=0 THEN        /* TEST RESERVE Q                */
GMA00121 LTR   R14,R14                                             0180
         BZ    @RF00180                                            0180
*               DO;                 /* REPLENISH ACE RESERVE Q       */
*                 R0=R14-1;         /* SET UP FOR COMPARE AND SWAP   */
         LR    R0,R14                                              0182
         BCTR  R0,0                                                0182
*                 GMANEXT=R15;      /* CELL TO FREE IN R1            */
         ST    R15,GMANEXT(,R1)                                    0183
*                 CDS(R14,R0,ASMCPRSQ);/* ADD CELL TO RESERVE Q      */
         CDS   R14,R0,ASMCPRSQ(REG11)                              0184
*                 BC(7,GMA00121);                                  0185
         BC    7,GMA00121                                          0185
*               END;                                               0186
*             ELSE                                                 0187
*               DO;                 /* RETURN ACE TO NORMAL Q        */
         B     @RC00180                                            0187
@RF00180 DS    0H                                                  0188
*                 R14=ASMCPAVL;                                    0188
         L     R14,ASMCPAVL(,REG11)                                0188
*GMA00122:                                                         0189
*                 GMANEXT=R14;      /* PUT ADDR CURRENT FIRST IN   0189
*                                      INPUT CELL                    */
GMA00122 ST    R14,GMANEXT(,R1)                                    0189
*                 CS(R14,R1,ASMCPAVL);/* ADD INPUT CELL TO POOL      */
         CS    R14,@01,ASMCPAVL(REG11)                             0190
*                 BC(7,GMA00122);                                  0191
         BC    7,GMA00122                                          0191
*               END;                /* ACE RETURNED TO NORMAL Q      */
*             RESPECIFY                                            0193
*              (R0,                                                0193
*               R1,                                                0193
*               REG11,                                             0193
*               REG12,                                             0193
*               R14,                                               0193
*               R15) UNRESTRICTED;                                 0193
@RC00180 DS    0H                                                  0194
*           END;                    /* ILRGMA FREE                 0194
*                                      CELL(ACE)WORKREGS(REG11,REG12)*/
*                                                                  0194
*           /*********************************************************/
*           /*                                                       */
*           /* END ILRGMA MACRO EXPANSION                            */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0195
*         END;                      /* FOR ACE IS VALID              */
*       IF EPARASP^=0 THEN                                         0196
@RF00170 SLR   @15,@15                                             0196
         L     @12,EPARASP(,EPAPTR)                                0196
         CR    @12,@15                                             0196
         BE    @RF00196                                            0196
*         DO;                       /* THERE IS A CHAIN OF BUFFERS 0197
*                                      FOR RETRIEVED ASPCT S         */
*           REG1=EPARASP;           /* ILRAFS00 REQUIRES ADDR OF 1ST
*                                      BUFFER IN REG 1               */
         LR    REG1,@12                                            0198
*           EPARASP=0;                                             0199
         ST    @15,EPARASP(,EPAPTR)                                0199
*           REG0=BUFLEN;            /* LENGTH OF BUFFER REQUIRED IN
*                                      REG 0                         */
         LA    REG0,2048                                           0200
*           CALL ILRAFS00;          /* GO FREE THE CHAIN OF BUFFERS  */
         BAL   @14,ILRAFS00                                        0201
*           RFY                                                    0202
*             REG0 UNRSTD;                                         0202
*         END;                      /* FOR THERE IS A CHAIN          */
*                                                                  0204
*       /*************************************************************/
*       /*                                                           */
*       /* THE CHAIN OF ACE S ON THE WORK QUEUE USED BY TMRLG WILL BE*/
*       /* VALIDITY CHECKED. THE QUEUE WILL BE TRUNCATED IF BAD      */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0204
*       REG1=SDWAPTR;               /* REG1 WAS DESTROYED            */
@RF00196 L     REG1,SDWAPTR                                        0204
*       RFY                                                        0205
*        (REG5,                                                    0205
*         REG6,                                                    0205
*         REG7,                                                    0205
*         REG8) RSTD;                                              0205
*       RFY                                                        0206
*         REG0 RSTD;                                               0206
*       REG0=ADDR(ASMRLGWQ);        /* ADDR OF QUEUE HEADER          */
         LA    REG0,ASMRLGWQ(,ASMVTPTR)                            0207
*       REG8=ADDR(QVWORKA);         /* GET ADDR OF WORK AREA FOR   0208
*                                      QUEUE VERIFIER                */
         LA    REG8,QVWORKA                                        0208
*       CALL ILRVACQ2;              /* VERIFY THE ACE WORK QUEUE     */
         L     @15,ASMPVACQ(,ASMVTPTR)                             0209
         BALR  @14,@15                                             0209
*       RFY                                                        0210
*         REG0 UNRSTD;                                             0210
*       RFY                                                        0211
*        (REG5,                                                    0211
*         REG6,                                                    0211
*         REG7,                                                    0211
*         REG8) UNRSTD;                                            0211
*       EPAVSAMI=BITOFF;            /* RESET IF ON                   */
*       EPARPLB=BITOFF;             /* RESET IF ON                   */
         NI    EPAVSAMI(EPAPTR),B'01111110'                        0213
*       EPARECUR=BITON;             /* TURN ON TMRLG RECURSION     0214
*                                      INDICATOR                     */
         OI    EPARECUR(EPAPTR),B'00000010'                        0214
*       GOTO MAINRTRY;                                             0215
         B     MAINRTRY                                            0215
*     END;                          /* FOR M.S.I. POSTED TMRLG IN  0216
*                                      CONTROL                       */
*                                                                  0217
*   /*****************************************************************/
*   /*                                                               */
*   /* AT THIS POINT M.S.I. IS NOT POSTED. DETERMINE IF TMI WAS IN   */
*   /* CONTROL                                                       */
*   /*                                                               */
*   /*****************************************************************/
