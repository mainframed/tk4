         TITLE 'SET TOD CLOCK--MODULE IEE6603D                         *
                       '
IEE6603D CSECT ,                                                   0001
IGC6603D DS    0H                                                  0001
         ENTRY IGC6603D
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL2(@EP00001-@MAINENT)
         DC    AL1(16)                                             0001
         DC    C'IEE6603D  73.228'                                 0001
         DROP  @15
@PROLOG  BALR  @10,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@10                                         0001
         AH    @15,4(,@15)                                         0001
         BR    @15                                                 0001
@EP00001 DS    0H                                                  0002
*                                                                  0050
*/********************************************************************/
*/*                                                                  */
*/* SET CSECT CODE IN XSA FOR IEE0603D ESTAE ERROR EXIT.  ESTABLISH  */
*/* ADDRESSABILITY FOR THE TOD CLOCK WORKAREA, AND SAVE THE          */
*/* CALLER'S RETURN ADDRESS.                                         */
*/*                                                                  */
*/********************************************************************/
*                                                                  0050
*   CSECTCDE='C0'X;                                                0050
         MVI   CSECTCDE(XSAPTR),X'C0'                              0050
*   TCWAPTR=TPCTCWA;                                               0051
         L     @15,CVTPTR                                          0051
         L     @15,TPCPTR(,@15)                                    0051
         L     TCWAPTR,TPCTCWA(,@15)                               0051
*   RESPECIFY                                                      0052
*     TCWA BASED(TCWAPTR);                                         0052
*   RETSAVE=REG14;                                                 0053
*                                                                  0053
         LR    RETSAVE,REG14                                       0053
*/********************************************************************/
*/*                                                                  */
*/* FIX THE ROUTINE TO BE INVOKED BY THE IPC.                        */
*/*                                                                  */
*/********************************************************************/
*                                                                  0054
*   RESPECIFY                                                      0054
*    (REG0,                                                        0054
*     REG1,                                                        0054
*     REG15)RESTRICTED;                                            0054
*   REG1=ADDR(DELTA7);              /* GET PTR TO START OF SIGP'D TO
*                                      ROUTINE                       */
         LA    REG1,DELTA7                                         0055
*   REG15=ADDR(DEL7END);            /* GET PTR TO END OF SIGP'D TO 0056
*                                      ROUTINE                       */
         LA    REG15,DEL7END                                       0056
*   REG0=ADDR(XSAECB);              /* GET ECB ADDRESS               */
         LA    REG0,XSAECB(,XSAPTR)                                0057
*   XSAECB=0;                       /* CLEAR ECB                     */
         SR    @12,@12                                             0058
         ST    @12,XSAECB(,XSAPTR)                                 0058
*   GENERATE CODE(PGFIX R,A=(1),EA=(15),ECB=(0),LONG=N);           0059
         PGFIX R,A=(1),EA=(15),ECB=(0),LONG=N
*   RESPECIFY                                                      0060
*    (REG0,                                                        0060
*     REG1,                                                        0060
*     REG15)UNRESTRICTED;                                          0060
*                                                                  0060
*/********************************************************************/
*/*                                                                  */
*/* TEST FOR SPECIFICATION OF THE CLOCK PARAMETER.                   */
*/*                                                                  */
*/********************************************************************/
*                                                                  0061
*   IF CLOCK='1'B THEN                                             0061
*                                                                  0061
         TM    CLOCK(XSAPTR),B'00000001'                           0061
         BNO   @RF00061                                            0061
*/********************************************************************/
*/*                                                                  */
*/* IF IT WAS SPECIFIED, UNPACK THE SPECIFIED VALUE.                 */
*/*                                                                  */
*/********************************************************************/
*                                                                  0062
*     UNPK(TCWAWRK1(1:6),PKTIME(1:4));                             0062
         UNPK  TCWAWRK1(6,TCWAPTR),PKTIME(4,XSAPTR)                0062
*   ELSE                                                           0063
*                                                                  0063
*/********************************************************************/
*/*                                                                  */
*/* IF IT WAS NOT SPECIFIED, TEST FOR MESSAGE IEA888A.               */
*/*                                                                  */
*/********************************************************************/
*                                                                  0063
*     IF TCGXXX^='1'B THEN                                         0063
*                                                                  0063
         B     @RC00061                                            0063
@RF00061 TM    TCGXXX(TCWAPTR),B'10000000'                         0063
         BO    @RF00063                                            0063
*/********************************************************************/
*/*                                                                  */
*/* IF MESSAGE IEA888A IS NOT OUTSTANDING, DEFAULT THE TIME TO       */
*/* 00.00.00.                                                        */
*/*                                                                  */
*/********************************************************************/
*                                                                  0064
*       TCWAWRK1(1:6)=DAYSTART;                                    0064
         MVC   TCWAWRK1(6,TCWAPTR),DAYSTART                        0064
*     ELSE                                                         0065
*                                                                  0065
*/********************************************************************/
*/*                                                                  */
*/* IF IT IS OUTSTANDING, UNPACK THE TIME DISPLAYED IN IT.           */
*/*                                                                  */
*/********************************************************************/
*                                                                  0065
*       UNPK(TCWAWRK1(1:6),TCWAPGT(1:4));                          0065
*                                                                  0065
         B     @RC00063                                            0065
@RF00063 UNPK  TCWAWRK1(6,TCWAPTR),TCWAPGT(4,TCWAPTR)              0065
*/********************************************************************/
*/*                                                                  */
*/* CONVERT SECONDS TO PACKED DECIMAL AND THEN TO BINARY.            */
*/*                                                                  */
*/********************************************************************/
*                                                                  0066
*   PACK(TCWAWRK2,TCWAWRK1(5:6));                                  0066
@RC00063 DS    0H                                                  0066
@RC00061 PACK  TCWAWRK2(8,TCWAPTR),TCWAWRK1+4(2,TCWAPTR)           0066
*   CVB(SECONDS,TCWAWRK2);                                         0067
*                                                                  0067
         CVB   SECONDS,TCWAWRK2(,TCWAPTR)                          0067
*/********************************************************************/
*/*                                                                  */
*/* MULTIPLY SECONDS ELAPSED SINCE THE LAST MINUTE BY THE NUMBER OF  */
*/* MICROSECONDS PER SECOND GIVING MICROSECONDS ELAPSED SINCE THE    */
*/* LAST MINUTE.                                                     */
*/*                                                                  */
*/********************************************************************/
*                                                                  0068
*   MICSECS=SECONDS*MICSSEC;                                       0068
*                                                                  0068
         LR    @01,SECONDS                                         0068
         M     @00,MICSSEC                                         0068
         LR    MICSECS,@01                                         0068
*/********************************************************************/
*/*                                                                  */
*/* CONVERT MINUTES TO PACKED DECIMAL AND THEN TO BINARY.            */
*/*                                                                  */
*/********************************************************************/
*                                                                  0069
*   PACK(TCWAWRK2,TCWAWRK1(3:4));                                  0069
         PACK  TCWAWRK2(8,TCWAPTR),TCWAWRK1+2(2,TCWAPTR)           0069
*   CVB(MINUTES,TCWAWRK2);                                         0070
*                                                                  0070
         CVB   MINUTES,TCWAWRK2(,TCWAPTR)                          0070
*/********************************************************************/
*/*                                                                  */
*/* CONVERT HOURS TO PACKED DECIMAL AND THEN TO BINARY.              */
*/*                                                                  */
*/********************************************************************/
*                                                                  0071
*   PACK(TCWAWRK2,TCWAWRK1(1:2));                                  0071
         PACK  TCWAWRK2(8,TCWAPTR),TCWAWRK1(2,TCWAPTR)             0071
*   CVB(HOURS,TCWAWRK2);                                           0072
*                                                                  0072
         CVB   HOURS,TCWAWRK2(,TCWAPTR)                            0072
*/********************************************************************/
*/*                                                                  */
*/* MULTIPLY HOURS ELAPSED SINCE LAST MIDNIGHT BY THE NUMBER OF MIN- */
*/* UTES PER HOUR AND ADD TO MINUTES ELAPSED SINCE LAST HOUR GIVING  */
*/* MINUTES ELAPSED SINCE LAST MIDNIGHT.                             */
*/*                                                                  */
*/********************************************************************/
*                                                                  0073
*   MINUTES=(HOURS*MINSHOUR)+MINUTES;                              0073
*                                                                  0073
         LR    @15,HOURS                                           0073
         MH    @15,MINSHOUR                                        0073
         AR    MINUTES,@15                                         0073
*/********************************************************************/
*/*                                                                  */
*/* TEST FOR SPECIFICATION OF THE DATE PARAMETER.                    */
*/*                                                                  */
*/********************************************************************/
*                                                                  0074
*   IF DATE='1'B THEN                                              0074
*                                                                  0074
         TM    DATE(XSAPTR),B'00000010'                            0074
         BNO   @RF00074                                            0074
*/********************************************************************/
*/*                                                                  */
*/* IF IT WAS SPECIFIED, UNPACK THE SPECIFIED VALUE.                 */
*/*                                                                  */
*/********************************************************************/
*                                                                  0075
*     UNPK(TCWAWRK1(1:5),PKDATE(2:4));                             0075
         UNPK  TCWAWRK1(5,TCWAPTR),PKDATE+1(3,XSAPTR)              0075
*   ELSE                                                           0076
*                                                                  0076
*/********************************************************************/
*/*                                                                  */
*/* IF IT WAS NOT SPECIFIED, UNPACK THE PREVIOUSLY USED VALUE.       */
*/*                                                                  */
*/********************************************************************/
*                                                                  0076
*     UNPK(TCWAWRK1(1:5),TCWAPGD(2:4));                            0076
*                                                                  0076
         B     @RC00074                                            0076
@RF00074 UNPK  TCWAWRK1(5,TCWAPTR),TCWAPGD+1(3,TCWAPTR)            0076
*/********************************************************************/
*/*                                                                  */
*/* CONVERT DAYS TO PACKED DECIMAL AND THEN TO BINARY.               */
*/*                                                                  */
*/********************************************************************/
*                                                                  0077
*   PACK(TCWAWRK2,TCWAWRK1(3:5));                                  0077
@RC00074 PACK  TCWAWRK2(8,TCWAPTR),TCWAWRK1+2(3,TCWAPTR)           0077
*   CVB(DAYS,TCWAWRK2);                                            0078
*                                                                  0078
         CVB   DAYS,TCWAWRK2(,TCWAPTR)                             0078
*/********************************************************************/
*/*                                                                  */
*/* MAKE DAYS ZERO ORIGIN.                                           */
*/*                                                                  */
*/********************************************************************/
*                                                                  0079
*   DAYS=DAYS-1;                                                   0079
*                                                                  0079
         BCTR  DAYS,0                                              0079
*/********************************************************************/
*/*                                                                  */
*/* CONVERT YEARS TO PACKED DECIMAL AND THEN TO BINARY.              */
*/*                                                                  */
*/********************************************************************/
*                                                                  0080
*   PACK(TCWAWRK2,TCWAWRK1(1:2));                                  0080
         PACK  TCWAWRK2(8,TCWAPTR),TCWAWRK1(2,TCWAPTR)             0080
*   CVB(YEARS,TCWAWRK2);                                           0081
*                                                                  0081
         CVB   YEARS,TCWAWRK2(,TCWAPTR)                            0081
*/********************************************************************/
*/*                                                                  */
*/* THE FOLLOWING FORMULA IS EQUIVALENT TO (YEARS*365.25)-.25, DIS-  */
*/* CARDING ANY FRACTIONAL PART OF THE RESULT.  THIS FORMULA GIVES   */
*/* THE CORRECT NUMBER OF DAYS ELAPSED FOR FULL YEARS PASSED SINCE   */
*/* THE EPOCH BECAUSE IN IT, THE SURPLUS .25 DAYS PER YEAR MINUS THE */
*/* .25 DAYS WILL NOT ACCUMULATE TO A FULL DAY FOR THE FIRST TIME    */
*/* UNTIL 5 YEARS HAS PASSED SINCE THE BEGINNING OF THE EPOCH--THAT  */
*/* IS UNTIL 1904 ( THE FIRST LEAP YEAR AFTER THE EPOCH) HAS PASSED. */
*/*                                                                  */
*/* WHEN 1904 HAS PASSED (AND IT IS 1905), THE ACCUMULATION IS 1 DAY */
*/* (1.25-.25=1).  FROM THAT POINT ON, 1 DAY ACCUMULATES FOR EVERY   */
*/* FOUR YEARS (EG. 2.25-.25 AFTER 1908, 3.25-.25 AFTER 1912, ETC.). */
*/* FOR ANY YEARS IN BETWEEN, THE ACCUMULATION IS SOME INTEGER PLUS  */
*/* .00 TO .75 MINUS THE CONSTANT .25 DAYS, AND IS DISCARDED.        */
*/*                                                                  */
*/********************************************************************/
*                                                                  0082
*   DAYS=(((YEARS*DAYS4YRS)-1)/4)+DAYS;                            0082
*                                                                  0082
         LR    @00,YEARS                                           0082
         MH    @00,DAYS4YRS                                        0082
         BCTR  @00,0                                               0082
         SRDA  @00,32                                              0082
         D     @00,@CF00097                                        0082
         AR    DAYS,@01                                            0082
*/********************************************************************/
*/*                                                                  */
*/* MULTIPLY DAYS ELAPSED SINCE THE EPOCH BY THE NO. OF MINUTES PER  */
*/* DAY AND ADD TO MINUTES ELAPSED SINCE LAST MIDNIGHT GIVING MINUTES*/
*/* ELAPSED SINCE EPOCH.                                             */
*/*                                                                  */
*/********************************************************************/
*                                                                  0083
*   MINUTES=(DAYS*MINSDAY)+MINUTES;                                0083
*                                                                  0083
         LR    @15,DAYS                                            0083
         MH    @15,MINSDAY                                         0083
         AR    MINUTES,@15                                         0083
*/********************************************************************/
*/*                                                                  */
*/* MULTIPLY MINUTES ELAPSED SINCE EPOCH BY MICROSECONDS PER MINUTE  */
*/* AND ADD IN MICROSECONDS GIVING MICROSECONDS ELAPSED SINCE EPOCH. */
*/*                                                                  */
*/********************************************************************/
*                                                                  0084
*   RESPECIFY                                                      0084
*    (TMICSECL,                                                    0084
*     TMICSECR)RESTRICTED;                                         0084
*   TMICSECR=MINUTES;                                              0085
         LR    TMICSECR,MINUTES                                    0085
*   GENERATE CODE REFS(TMICSECL,MICSMIN)(M     TMICSECL,MICSMIN);  0086
         M     TMICSECL,MICSMIN
*   TMICSECR=TMICSECR+MICSECS;                                     0087
*                                                                  0087
         ALR   TMICSECR,MICSECS                                    0087
*/********************************************************************/
*/*                                                                  */
*/* TEST THE ADDITION FOR A CARRY OUT OF THE LOW ORDER WORD.         */
*/*                                                                  */
*/********************************************************************/
*                                                                  0088
*   BC(12,MICSTOCU);                                               0088
*                                                                  0088
         BC    12,MICSTOCU                                         0088
*/********************************************************************/
*/*                                                                  */
*/* IF THERE IS A CARRY, ADD 1 TO THE HIGH ORDER WORD.               */
*/*                                                                  */
*/********************************************************************/
*                                                                  0089
*   TMICSECL=TMICSECL+1;                                           0089
*                                                                  0089
         AL    TMICSECL,@CF00105                                   0089
*/********************************************************************/
*/*                                                                  */
*/* SHIFT BIT 63, WHICH REPRESENTS 1 MICROSECOND, TO BIT 51, WHICH   */
*/* REPRESENTS 1 MICROSECOND IN THE TOD CLOCK.                       */
*/*                                                                  */
*/********************************************************************/
*                                                                  0090
*MICSTOCU:                                                         0090
*   SLDL(TMICSECL,12);                                             0090
*                                                                  0090
MICSTOCU SLDL  TMICSECL,12                                         0090
*/********************************************************************/
*/*                                                                  */
*/* TEST FOR SPECIFICATION OF THE GMT PARAMETER.                     */
*/*                                                                  */
*/********************************************************************/
*                                                                  0091
*   IF GMT^='1'B THEN                                              0091
*                                                                  0091
         TM    GMT(XSAPTR),B'00000100'                             0091
         BO    @RF00091                                            0091
*/********************************************************************/
*/*                                                                  */
*/* IF IT IS NOT SPECIFIED, A LOCAL VALUE WAS ENTERED, SO SUBTRACT   */
*/* THE TIME ZONE CONSTANT FROM THE CALCULATED TOD CLOCK VALUE TO    */
*/* OBTAIN A GMT VALUE WITH WHICH TO SET THE CLOCK.                  */
*/*                                                                  */
*/********************************************************************/
*                                                                  0092
*     TMICSECL=TMICSECL-CVTTZ;                                     0092
*                                                                  0092
         L     @15,CVTPTR                                          0092
         SL    TMICSECL,CVTTZ(,@15)                                0092
*/********************************************************************/
*/*                                                                  */
*/* STORE THE CALCULATED TOD CLOCK VALUE.                            */
*/*                                                                  */
*/********************************************************************/
*                                                                  0093
*   STM(TMICSECL,TMICSECR,TCWACVAL);                               0093
@RF00091 STM   TMICSECL,TMICSECR,TCWACVAL(TCWAPTR)                 0093
*   RESPECIFY                                                      0094
*    (REG0,                                                        0094
*     REG1,                                                        0094
*     REG11,                                                       0094
*     REG12,                                                       0094
*     REG15)RESTRICTED;                                            0094
*                                                                  0094
*/********************************************************************/
*/*                                                                  */
*/* WAIT ON PAGE FIX.                                                */
*/*                                                                  */
*/********************************************************************/
*                                                                  0095
*   REG1=ADDR(XSAECB);                                             0095
         LA    REG1,XSAECB(,XSAPTR)                                0095
*   GENERATE CODE REFS(REG1)(WAIT  1,ECB=(1));                     0096
*                                                                  0096
         WAIT  1,ECB=(1)
*/********************************************************************/
*/*                                                                  */
*/* TELL THE OPERATOR TO DEPRESS THE TOD CLOCK SECURITY SWITCH AT    */
*/* THE MOMENT THAT THE TIME THAT WAS ENTERED OCCURS.  THEN WAIT FOR */
*/* HIM TO INDICATE THAT HE HAS RECEIVED THE MESSAGE.                */
*/*                                                                  */
*/********************************************************************/
*                                                                  0097
*   REG11=ADDR(TCWATXT);            /* GET REPLY ADDRESS             */
         LA    REG11,TCWATXT(,TCWAPTR)                             0097
*   REG12=ADDR(XSAECB);             /* GET ECB ADDRESS               */
         LA    REG12,XSAECB(,XSAPTR)                               0098
*   XSAECB=0;                       /* CLEAR ECB                     */
         SR    @03,@03                                             0099
         ST    @03,XSAECB(,XSAPTR)                                 0099
*   GENERATE CODE;                                                 0100
         WTOR 'IEA903A DEPRESS TOD CLOCK SECURITY SWITCH WHEN ENTERED TT
               IME OCCURS',(11),40,(12),ROUTCDE=(1),                  **
               MCSFLAG=(NOTIME),DESC=(2)
*   REG1=ADDR(XSAECB);              /* GET ECB ADDRESS               */
         LA    REG1,XSAECB(,XSAPTR)                                0101
*   GENERATE CODE REFS(REG1)(WAIT  1,ECB=(1));                     0102
*                                                                  0102
         WAIT  1,ECB=(1)
*/********************************************************************/
*/*                                                                  */
*/* TEST FOR UNI-PROCESSOR.  IF STRICTLY UNI-PROCESSOR, GET THE      */
*/* DISPATCHER LOCK, AND BRANCH AND LINK TO THE ROUTINE THAT WILL    */
*/* SET TOD CLOCK.  IF NOT, INVOKE THE ROUTINE VIA INTER-PROCESSOR   */
*/* COMMUNICATIONS.                                                  */
*/*                                                                  */
*/********************************************************************/
*                                                                  0103
*   IF CSDMP='0'B THEN                                             0103
         L     @03,CVTPTR                                          0103
         L     @03,CVTCSD(,@03)                                    0103
         TM    CSDMP(@03),B'10000000'                              0103
         BNZ   @RF00103                                            0103
*     DO;                                                          0104
*       RESPECIFY                                                  0105
*        (REG13)RESTRICTED;                                        0105
*       REG15=ADDR(DELTA7);         /* GET ENTRY POINT ADDRESS       */
         LA    REG15,DELTA7                                        0106
*       REG1=TCWAPTR;               /* GET TOD CLOCK WORKAREA ADDRESS*/
         LR    REG1,TCWAPTR                                        0107
*       GENERATE CODE REFS(REG15,PSALITA)SETS(REG14);              0108
*                                                    /*
         SETLOCK OBTAIN,TYPE=DISP,MODE=UNCOND,                        **
               RELATED=('DISABLE FOR CPUT LOOP',(IEE6603D,(NONE)))
         BALR  REG14,REG15         BRANCH TO SET TOD CLOCK
         SETLOCK RELEASE,TYPE=DISP,                                   **
               RELATED=('DISABLE FOR CPUT LOOP',(IEE6603D,(NONE)))  */
*       RESPECIFY                                                  0109
*        (REG13)UNRESTRICTED;                                      0109
*     END;                                                         0110
*   ELSE                                                           0111
*                                                                  0111
*/********************************************************************/
*/*                                                                  */
*/* SET UP INTERFACE FOR IPC.                                        */
*/*                                                                  */
*/********************************************************************/
*                                                                  0111
*     DO;                                                          0111
         B     @RC00103                                            0111
@RF00103 DS    0H                                                  0112
*       REG11=TCWAPTR;              /* ADDR TCWA TO REG 11           */
         LR    REG11,TCWAPTR                                       0112
*       REG1=EVAL(TCWAPTR+LENGTH(TCWA))->TCWAPCCA;/* ADDRESS OF PCCA
*                                      TO REG 1                      */
         LA    @03,112                                             0113
         AR    @03,TCWAPTR                                         0113
         L     REG1,TCWAPCCA(,@03)                                 0113
*       REG12=ADDR(DELTA7);         /* ENTRY POINT ADDR TO REG 12    */
*                                                                  0114
         LA    REG12,DELTA7                                        0114
*/********************************************************************/
*/*                                                                  */
*/* ISSUE A SIGP TO GET A TOD CLOCK SET WITH THE CORRECT GMT.        */
*/*                                                                  */
*/********************************************************************/
*                                                                  0115
*       GENERATE CODE REFS(CVTIPCRI,CVT)                           0115
*           (RISGNL SERIAL,CPU=(1),EP=(12),PARM=(11));             0115
         RISGNL SERIAL,CPU=(1),EP=(12),PARM=(11)
*     END;                                                         0116
*                                                                  0116
*/********************************************************************/
*/*                                                                  */
*/* FREE THE FIXED PAGE.                                             */
*/*                                                                  */
*/********************************************************************/
*                                                                  0117
*   REG1=ADDR(DELTA7);              /* GET PTR TO START OF SIGP'D TO
*                                      ROUTINE                       */
@RC00103 LA    REG1,DELTA7                                         0117
*   REG15=ADDR(DEL7END);            /* GET PTR TO END OF SIGP'D TO 0118
*                                      ROUTINE                       */
         LA    REG15,DEL7END                                       0118
*   GENERATE CODE(PGFREE  R,A=(1),EA=(15));                        0119
*                                                                  0119
         PGFREE  R,A=(1),EA=(15)
*/********************************************************************/
*/*                                                                  */
*/* RESTORE THE CALLER'S RETURN ADDRESS TO THE RETURN REGISTER.      */
*/*                                                                  */
*/********************************************************************/
*                                                                  0120
*   REG14=RETSAVE;                                                 0120
         LR    REG14,RETSAVE                                       0120
*   RESPECIFY                                                      0121
*    (REG11,                                                       0121
*     REG12,                                                       0121
*     TMICSECL,                                                    0121
*     TMICSECR)UNRESTRICTED;                                       0121
*                                                                  0121
*/********************************************************************/
*/*                                                                  */
*/* SET CSECT ID FIELD OF XSA TO 0 TO INDICATE THAT IEE6603D IS NO   */
*/* LONGER IN CONTROL.  RETURN TO IEE0603D.                          */
*/*                                                                  */
*/********************************************************************/
*                                                                  0122
*   CSECTCDE='00'X;                                                0122
         MVI   CSECTCDE(XSAPTR),X'00'                              0122
*   RETURN;                                                        0123
@EL00001 DS    0H                                                  0123
@EF00001 DS    0H                                                  0123
@ER00001 BR    @14                                                 0123
*                                                                  0124
*/********************************************************************/
*/*                                                                  */
*/* THIS ROUTINE IS INVOKED VIA THE IPC.  IT ATTEMPTS TO SET THE TOD */
*/* CLOCK WITH THE VALUE PASSED TO IT.                               */
*/*                                                                  */
*/********************************************************************/
*                                                                  0124
*   RESPECIFY                                                      0124
*     TCWA BASED(REG1);             /* REG1 NOW POINTING TO TCWA     */
*                                                                  0124
*/********************************************************************/
*/*                                                                  */
*/* TURN OF THE TOD CLOCK SYNCHRONIZATION BIT IN CONTROL REG 0 SO    */
*/* THAT THE CLOCK WILL NOT STOP WHEN IT IS SET.  THEN INITIALIZE    */
*/* THE 1 MINUTE TIME-OUT LOOP.                                      */
*/*                                                                  */
*/********************************************************************/
*                                                                  0125
*   GENERATE CODE REFS(REG15)(USING *,REG15  ADDRESSABILITY FOR CODE);
         USING *,REG15  ADDRESSABILITY FOR CODE
*DELTA7:                                                           0126
*   GENERATE CODE REFS(REG1)SETS(TCWACTL)                          0126
*       (STCTL 0,0,TCWACTL(REG1)  SAVE CONTROL REG 0);             0126
DELTA7   STCTL 0,0,TCWACTL(REG1)  SAVE CONTROL REG 0
*   TCSTOP='0'B;                    /* TURN OFF SYNCHRONIZATION BIT  */
         NI    TCSTOP(REG1),B'11011111'                            0127
*   GENERATE CODE REFS(TCWACTL,REG1)SETS(TCWAA1,TCWAA2);           0128
*                                                                  0128
        LCTL  0,0,TCWACTL(REG1)  RESTORE CONTROL REG 0
        STPT  TCWAA1(REG1)       STORE STARTING TIME VALUE
        STPT  TCWAA2(REG1)       STORE ENDING TIME VALUE
*/********************************************************************/
*/*                                                                  */
*/* TURN ON THE TOD CLOCK NOT SET INDICATOR.                         */
*/*                                                                  */
*/********************************************************************/
*                                                                  0129
*   TCGNSET='1'B;                                                  0129
*                                                                  0129
         OI    TCGNSET(REG1),B'00001000'                           0129
*/********************************************************************/
*/*                                                                  */
*/* LOOP ON A SET CLOCK INSTRUCTION UNTIL THE OPERATOR DEPRESSES THE */
*/* SECURITY SWITCH ALLOWING THE CLOCK TO BE SET, OR THE ALLOTTED    */
*/* TIME INTERVAL EXPIRES.                                           */
*/*                                                                  */
*/********************************************************************/
*                                                                  0130
*   DO WHILE TCWAA1L-TCWAA2L<INTERVAL&TCGNSET='1'B;                0130
         B     @DE00130                                            0130
@DL00130 DS    0H                                                  0131
*     GENERATE CODE REFS(TCWACVAL,REG1)(SCK   TCWACVAL(REG1));     0131
*                                                                  0131
         SCK   TCWACVAL(REG1)
*/********************************************************************/
*/*                                                                  */
*/* TEST FOR A SUCCESSFUL EXECUTION OF THE SET CLOCK INSTRUCTION.    */
*/*                                                                  */
*/********************************************************************/
*                                                                  0132
*     BC(7,STCPUT);                                                0132
*                                                                  0132
         BC    7,STCPUT                                            0132
*/********************************************************************/
*/*                                                                  */
*/* IF THE TOD CLOCK IS SET, TURN OFF THE TOD CLOCK NOT SET          */
*/* INDICATOR.  WHETHER SET OR NOT, STORE A NEW ENDING VALUE.        */
*/*                                                                  */
*/********************************************************************/
*                                                                  0133
