*/*  *****************************************************************/
*/*                                                                  */
*/*                SYNCHRONOUS RECOVERY ROUTINE                      */
*/*                                                                  */
*/*  *****************************************************************/
*                                                                  0256
*IEAVRSRC:                                                         0256
*   RESPECIFY                                                      0256
*     CSD BASED(CSDPTR);                                           0256
IEAVRSRC DS    0H                                                  0257
*   RESPECIFY                                                      0257
*    (R10,                                                         0257
*     BADCPU,                                                      0257
*     LCCAPTR,                                                     0257
*     PCCAPTR) RSTD;                                               0257
*   RESPECIFY                                                      0258
*    (R5) RSTD;                                                    0258
*   R5=R13;                         /* SAVE SAVE AREA ADDRESS        */
         LR    R5,R13                                              0259
*   R10=0;                          /* INITIALIZE-'NEED IEAVRCLS'    */
         SLR   R10,R10                                             0260
*   LCCAPTR=PSALCCAV;               /* SET UP LCCA ADDRESSABILITY    */
         L     LCCAPTR,PSALCCAV                                    0261
*   IF LCCAACR='1'B                 /* IF ENTRY IS DUE TO ACR        */
*     THEN                          /* ADJUST FOR DOWN CPU           */
         TM    LCCAACR(LCCAPTR),B'10000000'                        0262
         BNO   @RF00262                                            0262
*     DO;                                                          0263
*       PCCAPTR=PCCAT00P(BADCPU+1); /* GET PTR TO BAD CPU'S PCCA     */
         LR    @12,BADCPU                                          0264
         SLL   @12,2                                               0264
         L     @11,CVTPTR                                          0264
         L     @08,CVTPCCAT(,@11)                                  0264
         L     PCCAPTR,PCCAT00P(@12,@08)                           0264
*       RESPECIFY                                                  0265
*        (BADCPU) UNRSTD;                                          0265
*       CSDPTR=CVTCSD;              /* SET UP CSD ADDRESSABILITY     */
         L     CSDPTR,CVTCSD(,@11)                                 0266
*       RESPECIFY                                                  0267
*        (R6,                                                      0267
*         R7) RSTD;                                                0267
*       IF PCCANFCC='0'B            /* IF CLOCK COMPARATOR IS NOT  0268
*                                      PERMANENTLY DAMAGED,          */
*         THEN                      /* TREAT IT AS SUCH.             */
         TM    PCCANFCC(PCCAPTR),B'01000000'                       0268
         BNZ   @RF00268                                            0268
*         DO;                                                      0269
*           PCCACCE='FF'X;          /* MARK CLR COMP PERM DAMAGED    */
         MVI   PCCACCE(PCCAPTR),X'FF'                              0270
*           R6=CSDGDCC;                                            0271
         L     R6,CSDGDCC(,CSDPTR)                                 0271
*REDOCC:                                                           0272
*           R7=R6-1;                /* REDUCE BY ONE THE COUNT OF    */
REDOCC   LR    R7,R6                                               0272
         BCTR  R7,0                                                0272
*           GENERATE CODE;                                         0273
*                                     /* USABLE CLK COMPS IN SYSTEM */
                CS   R6,R7,CSDGDCC(CSDPTR)
                BNZ  REDOCC
*         END;                                                     0274
*       IF PCCANFTD='0'B            /* IF TOPC NOT PERM DAMAGED,     */
*         THEN                      /* TREAT IT AS SUCH.             */
@RF00268 TM    PCCANFTD(PCCAPTR),B'01000000'                       0275
         BNZ   @RF00275                                            0275
*         DO;                                                      0276
*           PCCATODE='FF'X;         /* MARK TOD CLK PERM DAMAGED     */
         MVI   PCCATODE(PCCAPTR),X'FF'                             0277
*           R6=CSDGDTOD;                                           0278
         L     R6,CSDGDTOD(,CSDPTR)                                0278
*REDOTOD:                                                          0279
*           R7=R6-1;                /* REDUCE BY 1 THE NUMBER OF     */
REDOTOD  LR    R7,R6                                               0279
         BCTR  R7,0                                                0279
*           GENERATE;                                              0280
*                                     /* CPUS WITH GOOD TOD CLOCKS  */
                CS   R6,R7,CSDGDTOD(CSDPTR)
                BNZ  REDOTOD
*         END;                                                     0281
*       IF PCCANFIN='0'B            /* IF CPU TMR NOT PER DAMAGED    */
*         THEN                      /* TREAT IT AS SUCH              */
@RF00275 TM    PCCANFIN(PCCAPTR),B'01000000'                       0282
         BNZ   @RF00282                                            0282
*         DO;                                                      0283
*           PCCAINTE='FF'X;         /* MARK CPU TMR PERM DAMAGED     */
         MVI   PCCAINTE(PCCAPTR),X'FF'                             0284
*           R6=CSDGDINT;                                           0285
         L     R6,CSDGDINT(,CSDPTR)                                0285
*REDONT:                                                           0286
*           R7=R6-1;                /* REDUCE BY ONE THE COUNT OF    */
REDONT   LR    R7,R6                                               0286
         BCTR  R7,0                                                0286
*           GENERATE CODE;                                         0287
*                                     /* USABLE CPU TMRS IN SYSTEM  */
                CS   R6,R7,CSDGDINT(CSDPTR)
                BNZ  REDONT
*         END;                                                     0288
*       IF PCCATQEP^=0              /* IF BAD CPU WAS TIMNG A TQE    */
*         THEN                      /* MARK TQE AS NO LONGER         */
@RF00282 L     @12,PCCATQEP(,PCCAPTR)                              0289
         LTR   @12,@12                                             0289
         BZ    @RF00289                                            0289
*         PCCATQEP->TQECOMP='0'B;   /* BEING TIMED                   */
         NI    TQECOMP(@12),B'01111111'                            0290
*       RESPECIFY                                                  0291
*        (R6,                                                      0291
*         R7,                                                      0291
*         CSDPTR) UNRSTD;                                          0291
@RF00289 DS    0H                                                  0292
*     END;                                                         0292
*   ELSE                            /* HERE IF A MACHINE CHECK       */
*     DO;                                                          0293
         B     @RC00262                                            0293
@RF00262 DS    0H                                                  0294
*       PCCAPTR=PSAPCCAV;           /* SET UP PCCA ADDRESSABILITY    */
         L     PCCAPTR,PSAPCCAV                                    0294
*       IF PCCAINIT='0'B            /* IF NOT YET INITIALIZED        */
*         THEN                      /* INDICATE IEAVRCLS NOT         */
