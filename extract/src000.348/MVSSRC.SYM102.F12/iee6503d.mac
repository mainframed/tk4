         TITLE 'SET LOCAL TIME--MODULE IEE6503D                        *
                        '
IEE6503D CSECT ,                                                   0001
IGC6503D DS    0H                                                  0001
         ENTRY IGC6503D
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL2(@EP00001-@MAINENT)
         DC    AL1(16)                                             0001
         DC    C'IEE6503D  76.229'                                 0001
         DROP  @15
@PROLOG  BALR  @10,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@10                                         0001
         LR    @03,@15                                             0001
         L     @00,@SIZDATD                                        0001
         GETMAIN  R,LV=(0)
         LR    @09,@01                                             0001
         USING @DATD,@09                                           0001
         LR    @15,@03                                             0001
         AH    @15,4(,@15)                                         0001
         BR    @15                                                 0001
@EP00001 DS    0H                                                  0002
*                                                                  0086
*/********************************************************************/
*/*                                                                  */
*/* SET CSECT CODE IN XSA FOR IEE0603D ESTAE ERROR EXIT ROUTINE.     */
*/* SAVE THE CALLER'S RETURN ADDRESS AND THE ADDRESS OF THE XSA.     */
*/* ESTABLISH ADDRESSABILITY FOR THE CVT. INITIALIZE THE ERROR CODE. */
*/*                                                                  */
*/********************************************************************/
*                                                                  0086
*   CSECTCDE='40'X;                 /* INDICATE IEE6503D IN CONTROL  */
         MVI   CSECTCDE(XSAPTR),X'40'                              0086
*   SAVERET=REG14;                  /* SAVE RETURN ADDRESS           */
         ST    REG14,SAVERET                                       0087
*   RESPECIFY                                                      0088
*    (REG0,                                                        0088
*     REG14) UNRESTRICTED;                                         0088
*   SAVEXSA=XSAPTR;                 /* SAVE POINTER TO XSA           */
         ST    XSAPTR,SAVEXSA                                      0089
*   LCVTPTR=CVTPTR;                                                0090
         L     LCVTPTR,CVTPTR                                      0090
*   RESPECIFY                                                      0091
*     CVTMAP BASED(LCVTPTR);                                       0091
*   RESPECIFY                                                      0092
*    (ERRCODE) RESTRICTED;                                         0092
*   ERRCODE=0;                                                     0093
         SLR   ERRCODE,ERRCODE                                     0093
*                                                                  0094
*/********************************************************************/
*/*                                                                  */
*/* TEST FOR SPECIFICATION OF THE RESET OR CLOCK PARAMETERS.         */
*/*                                                                  */
*/********************************************************************/
*                                                                  0094
*   IF RESET='1'B×CLOCK='1'B THEN                                  0094
*                                                                  0094
         TM    RESET(XSAPTR),B'00010001'                           0094
         BZ    @RF00094                                            0094
*/********************************************************************/
*/*                                                                  */
*/* IF EITHER WAS SPECIFIED, SEE IF IT IS INITIALIZATION/VARY AND    */
*/* THE CONFIGURATION IS NOT A UNI-PROCESSOR.   IF THAT IS THE CASE, */
*/* HAVE THE CPU REPRESENTED BY THE FIRST TCWA ENTRY STORE ITS       */
*/* CLOCK, AND MOVE THE STORED VALUE TO THE XSA.                     */
*/*                                                                  */
*/********************************************************************/
*                                                                  0095
*     DO;                                                          0095
*       RESPECIFY                                                  0096
*        (REG0,                                                    0096
*         REG1) RESTRICTED;                                        0096
*       IF BAIN='0'B&CSDMP='1'B THEN                               0097
         L     @12,CVTMSER(,LCVTPTR)                               0097
         TM    BAIN(@12),B'10000000'                               0097
         BNZ   @RF00097                                            0097
         L     @12,CVTCSD(,LCVTPTR)                                0097
         TM    CSDMP(@12),B'10000000'                              0097
         BNO   @RF00097                                            0097
*         DO;                                                      0098
*           RESPECIFY                                              0099
*            (REG11,                                               0099
*             REG12) RESTRICTED;                                   0099
*           REG11=TPCTCWA;                                         0100
         L     @08,TPCPTR(,LCVTPTR)                                0100
         L     REG11,TPCTCWA(,@08)                                 0100
*           REG12=ADDR(DELTA3);                                    0101
         LA    REG12,DELTA3                                        0101
*           REG1=EVAL(REG11+LENGTH(TCWA))->TCWAPCCA;               0102
         LA    @08,112                                             0102
         ALR   @08,REG11                                           0102
         L     REG1,TCWAPCCA(,@08)                                 0102
*           GENERATE CODE REFS(CVTIPCRI,CVT)                       0103
*               (RISGNL SERIAL,CPU=(1),EP=(12),PARM=(11));         0103
         RISGNL SERIAL,CPU=(1),EP=(12),PARM=(11)
*           XSAGMT=REG11->TCWACVAL;                                0104
         MVC   XSAGMT(8,XSAPTR),TCWACVAL(REG11)                    0104
*           RESPECIFY                                              0105
*            (REG11,                                               0105
*             REG12) UNRESTRICTED;                                 0105
*         END;                                                     0106
*       ELSE                                                       0107
*                                                                  0107
*/********************************************************************/
*/*                                                                  */
*/* IF IT IS NOT IPL/VARY AND AN MP CONFIGURATION,                   */
*/* STORE THE TOD CLOCK TO GET THE CURRENT VALUE.  IF THE RESULTING  */
*/* CONDITION CODE IS NOT ZERO (IE. THE CLOCK ON THIS CPU IS IN THE  */
*/* ERROR STATE OR THE STOPPED STATE), ISSUE A TIME MACRO TO GET THE */
*/* VALUE FROM ANOTHER TOD CLOCK.  IF THIS ACTION ALSO FAILS, THE    */
*/* NEW LOCAL TIME AND DATE CANNOT BE CALCULATED.                    */
*/*                                                                  */
*/********************************************************************/
*                                                                  0107
*         DO;                                                      0107
         B     @RC00097                                            0107
@RF00097 DS    0H                                                  0108
*           GENERATE CODE SETS(XSAGMT)                             0108
*               (STCK  XSAGMT(XSAPTR) GET THE CURRENT TOD CLOCK VALUE);
         STCK  XSAGMT(XSAPTR) GET THE CURRENT TOD CLOCK VALUE
*           BC(8,TESTRC);           /* BRANCH IF THE CLOCK IS SET    */
         BC    8,TESTRC                                            0109
*/********************************************************************/
*/*                                                                  */
*/* IF THE CLOCK IS NOT SET, ISSUE A TIME SVC TO GET THE CURRENT     */
*/* VALUE FROM A CLOCK THAT IS SET.                                  */
*/*                                                                  */
*/********************************************************************/
*                                                                  0110
*           REG0=ADDR(XSAGMT);                                     0110
         LR    REG0,XSAPTR                                         0110
*           GENERATE CODE SETS(XSAGMT) FLOWS(TESTRC)               0111
*               (TIME  STCK,(0),ERRET=TESTRC);                     0111
         TIME  STCK,(0),ERRET=TESTRC
*         END;                                                     0112
*       RESPECIFY                                                  0113
*        (REG0,                                                    0113
*         REG1) UNRESTRICTED;                                      0113
@RC00097 DS    0H                                                  0114
*     END;                                                         0114
*                                                                  0115
*/********************************************************************/
*/*                                                                  */
*/* TEST THE VALUE OF THE ERROR CODE.  IT WILL BE ZERO ONLY IF A     */
*/* GOOD TOD CLOCK EXISTS IN THE CONFIGURATION (IE. THE STCK OR THE  */
*/* TIME SVC WAS SUCCESSFUL).                                        */
*/*                                                                  */
*/********************************************************************/
*                                                                  0115
*TESTRC:                                                           0115
*   IF ERRCODE=0 THEN                                              0115
*                                                                  0115
@RF00094 DS    0H                                                  0115
TESTRC   LTR   ERRCODE,ERRCODE                                     0115
         BNZ   @RF00115                                            0115
*/********************************************************************/
*/*                                                                  */
*/* IF SUCCESSFUL, SEE IF THE RESET PARAMETER WAS SPECIFIED.         */
*/*                                                                  */
*/********************************************************************/
*                                                                  0116
*     DO;                                                          0116
*       RESPECIFY                                                  0117
*        (ERRCODE) UNRESTRICTED;                                   0117
*       IF RESET='1'B THEN                                         0118
*                                                                  0118
         TM    RESET(XSAPTR),B'00010000'                           0118
         BNO   @RF00118                                            0118
*/********************************************************************/
*/*                                                                  */
*/* IF RESET WAS SPECIFIED, USE THE TIME ZONE CONSTANT IN EFFECT     */
*/* AT IPL TIME AS THE NEW TIME ZONE CONSTANT.  THEN GET THE LOCAL   */
*/* TIME AND DAY, IN TERMS OF SECONDS, FOR THE NEW TIME ZONE         */
*/* CONSTANT.                                                        */
*/*                                                                  */
*/********************************************************************/
*                                                                  0119
*         DO;                                                      0119
*           RESPECIFY                                              0120
*            (REG12,                                               0120
*             REG13,                                               0120
*             REG14) RESTRICTED;                                   0120
*           NEWTZ=TPCTZORG;                                        0121
         L     @15,TPCPTR(,LCVTPTR)                                0121
         L     NEWTZ,TPCTZORG(,@15)                                0121
*           REG13=XSAGMTL+NEWTZ;    /* GET NEW LOCAL CLOCK VALUE     */
         LR    REG13,NEWTZ                                         0122
         AL    REG13,XSAGMTL(,XSAPTR)                              0122
*           CALL CONVERT;           /* CONVERT TO SECONDS            */
         BAL   @14,CONVERT                                         0123
*           RESPECIFY                                              0124
*            (REG14) UNRESTRICTED;                                 0124
*           NLCLTOD=REG12;          /* SAVE SECONDS ELAPSED SINCE  0125
*                                      MIDNIGHT                      */
         ST    REG12,NLCLTOD                                       0125
*/********************************************************************/
*/*                                                                  */
*/* CALCULATE NEW LOCAL DATE.  ADD 1 TO DAYS ELAPSED SINCE THE EPOCH,*/
*/* AND SAVE THE VALUE FOR LATER USE IN CALCULATING THE NEW VALUE FOR*/
*/* THE MIDNIGHT TQE.  THEN MULTIPLY THE SUM BY 4, AND CLEAR REG 12  */
*/* FOR DIVISION.                                                    */
*/*                                                                  */
*/********************************************************************/
*                                                                  0126
*           REG13=REG13+1;                                         0126
         AH    REG13,@CH00118                                      0126
*           ELDAYS=REG13;                                          0127
         ST    REG13,ELDAYS                                        0127
*           REG13=4*REG13;                                         0128
         SLL   REG13,2                                             0128
*           REG12=0;                                               0129
*                                                                  0129
         SLR   REG12,REG12                                         0129
*/********************************************************************/
*/*                                                                  */
*/* DIVIDE BY THE NUMBER OF DAYS IN 4 YEARS GIVING YEAR OF THE       */
*/* CENTURY IN REGISTER 13.  THEN DIVIDE THE REMAINDER BY 4 GIVING   */
*/* DAYS ELAPSED SINCE THE BEGINNING OF THE YEAR (EXCEPT FOR 1900)   */
*/* IN REGISTER 12.                                                  */
*/*                                                                  */
*/********************************************************************/
*                                                                  0130
*           GENERATE CODE REFS(DAYS4YRS) SETS(REG12,REG13)         0130
*               (D     REG12,DAYS4YRS);                            0130
         D     REG12,DAYS4YRS
*           SRL(REG12,2);                                          0131
*                                                                  0131
         SRL   REG12,2                                             0131
*/********************************************************************/
*/*                                                                  */
*/* TEST REGISTER 13 FOR NON-ZERO INDICATING NOT THE YEAR 1900.      */
*/*                                                                  */
*/********************************************************************/
*                                                                  0132
*           IF REG13^=0 THEN                                       0132
*                                                                  0132
         LTR   REG13,REG13                                         0132
         BZ    @RF00132                                            0132
*/********************************************************************/
*/*                                                                  */
*/* IF IT IS NOT 1900, ADD 1 TO THE NUMBER OF DAYS ELAPSED SINCE THE */
*/* EPOCH SINCE THEY ARE NOT ONE-ORIGINED.  THEN SHIFT THE YEAR VALUE*/
*/* TO ALIGN PROPERLY WITH DAYS FOR CONVERSION TO PACKED DECIMAL.    */
*/*                                                                  */
*/********************************************************************/
*                                                                  0133
*             DO;                                                  0133
*               REG12=REG12+1;                                     0134
         AH    REG12,@CH00118                                      0134
*               REG13=REG13*1000;                                  0135
         LR    @15,REG13                                           0135
         MH    @15,@CH01722                                        0135
         LR    REG13,@15                                           0135
*             END;                                                 0136
*                                                                  0136
*/********************************************************************/
*/*                                                                  */
*/* CONCATENATE DAYS TO YEAR, CONVERT TO PACKED DECIMAL AND STORE    */
*/* THE VALUE IN THE PKDATE.                                         */
*/*                                                                  */
*/********************************************************************/
*                                                                  0137
*           REG13=REG13+REG12;                                     0137
@RF00132 ALR   REG13,REG12                                         0137
*           CVD(REG13,XAV6503D);                                   0138
         CVD   REG13,XAV6503D(,XSAPTR)                             0138
*           XAV6503D(8)=XAV6503D(8)×'0F'X;/* MAKE SIGN HEX F         */
         OI    XAV6503D+7(XSAPTR),X'0F'                            0139
*           RESPECIFY                                              0140
*            (REG12,                                               0140
*             REG13) UNRESTRICTED;                                 0140
*         END;                                                     0141
*                                                                  0142
*/********************************************************************/
*/*                                                                  */
*/* IF RESET WAS NOT SPECIFIED, SEE IF THE CLOCK PARAMETER WAS       */
*/* SPECIFIED.                                                       */
*/*                                                                  */
*/********************************************************************/
*                                                                  0142
*       ELSE                                                       0142
*         DO;                                                      0142
         B     @RC00118                                            0142
@RF00118 DS    0H                                                  0143
*           IF CLOCK='1'B THEN                                     0143
*                                                                  0143
         TM    CLOCK(XSAPTR),B'00000001'                           0143
         BNO   @RF00143                                            0143
*/********************************************************************/
*/*                                                                  */
*/* IF SO, CALCULATE THE NEW TIME ZONE CONSTANT.                     */
*/*                                                                  */
*/********************************************************************/
*                                                                  0144
*             DO;                                                  0144
*               UNPK(WORK1(1:6),PKTIME);/* UNPACK LOCAL TIME         */
         UNPK  WORK1(6),PKTIME(4,XSAPTR)                           0145
*               PACK(WORK2,WORK1(1:2));/* PACK HH                    */
         PACK  WORK2(8),WORK1(2)                                   0146
*               CVB(HOURS,WORK2);   /* CONVERT HH TO BINARY          */
         CVB   HOURS,WORK2                                         0147
*               PACK(WORK2,WORK1(3:4));/* PACK MM                    */
         PACK  WORK2(8),WORK1+2(2)                                 0148
*               CVB(MINUTES,WORK2); /* CONVERT MM TO BINARY          */
         CVB   MINUTES,WORK2                                       0149
*               PACK(WORK2,WORK1(5:6));/* PACK SS                    */
         PACK  WORK2(8),WORK1+4(2)                                 0150
*               CVB(SECONDS,WORK2); /* CONVERT SS TO BINARY          */
         CVB   SECONDS,WORK2                                       0151
*/********************************************************************/
*/*                                                                  */
*/* CONVERT HOURS, MINUTES, AND SECONDS TO TOTAL SECONDS.            */
*/*                                                                  */
*/********************************************************************/
*                                                                  0152
*               NLCLTOD=SECONDS+(60*(MINUTES+(60*HOURS)));         0152
*                                                                  0152
         LA    @15,60                                              0152
         LR    @01,HOURS                                           0152
         MR    @00,@15                                             0152
         ALR   @01,MINUTES                                         0152
         MR    @00,@15                                             0152
         ALR   @01,SECONDS                                         0152
         ST    @01,NLCLTOD                                         0152
*/********************************************************************/
*/*                                                                  */
*/* CONVERT HIGH ORDER 32 BITS OF GMT CLOCK VALUE TO SECONDS.        */
*/*                                                                  */
*/********************************************************************/
*                                                                  0153
*               RESPECIFY                                          0153
*                (REG12,                                           0153
*                 REG13,                                           0153
*                 REG14) RESTRICTED;                               0153
*               REG13=XSAGMTL;      /* GET LEFT HALF OF GREENWICH  0154
*                                      TIME                          */
         L     REG13,XSAGMTL(,XSAPTR)                              0154
*               CALL CONVERT;       /* CONVERT TO SECONDS            */
         BAL   @14,CONVERT                                         0155
*               RESPECIFY                                          0156
*                (REG14) UNRESTRICTED;                             0156
*                                                                  0156
*/********************************************************************/
*/*                                                                  */
*/* CALCULATE THE DIFFERENCE BETWEEN THE NEW LOCAL TIME & GREENWICH  */
*/* TIME.                                                            */
*/*                                                                  */
*/********************************************************************/
*                                                                  0157
*               REG12=NLCLTOD-REG12;                               0157
*                                                                  0157
         L     @15,NLCLTOD                                         0157
         SLR   @15,REG12                                           0157
         LR    REG12,@15                                           0157
*/********************************************************************/
*/*                                                                  */
*/* SEE IF THE ABSOLUTE DIFFERENCE IS GREATER THAN 12 HOURS.         */
*/*                                                                  */
*/********************************************************************/
*                                                                  0158
*               IF ABS(REG12)>K12HRSEC THEN                        0158
*                                                                  0158
         LPR   @15,REG12                                           0158
         C     @15,K12HRSEC                                        0158
         BNH   @RF00158                                            0158
*/********************************************************************/
*/*                                                                  */
*/* IF IT IS GREATER THAN 12 HOURS AND THE DIFFERENCE IS NEGATIVE,   */
*/* ADD 24 HOURS TO THE DIFFERENCE TO GET THE CORRECT TIME ZONE;     */
*/* BUT IF THE DIFFERENCE IS POSITIVE, SUBTRACT 24 HOURS.            */
*/*                                                                  */
*/********************************************************************/
*                                                                  0159
*                 DO;                                              0159
*                   REG13=K24HRSEC;                                0160
         L     REG13,K24HRSEC                                      0160
*                   IF REG12<0 THEN                                0161
         LTR   REG12,REG12                                         0161
         BNM   @RF00161                                            0161
*                     REG12=REG12+REG13;                           0162
         ALR   REG12,REG13                                         0162
*                   ELSE                                           0163
*                     REG12=REG12-REG13;                           0163
         B     @RC00161                                            0163
@RF00161 SLR   REG12,REG13                                         0163
*                 END;                                             0164
*                                                                  0164
@RC00161 DS    0H                                                  0165
*/********************************************************************/
*/*                                                                  */
*/* RECONVERT THE NEW TIME ZONE CONSTANT TO 2**20 MICROSECOND UNITS  */
*/* AND SAVE IT.                                                     */
*/*                                                                  */
*/********************************************************************/
*                                                                  0165
*               REG12=REG12*K15625H;                               0165
@RF00158 LR    @15,REG12                                           0165
         MH    @15,K15625H                                         0165
         LR    REG12,@15                                           0165
*               SRA(REG12,14);                                     0166
         SRA   REG12,14                                            0166
*               NEWTZ=REG12;                                       0167
         LR    NEWTZ,REG12                                         0167
*               RESPECIFY                                          0168
*                (REG12,                                           0168
*                 REG13) UNRESTRICTED;                             0168
*             END;                                                 0169
*         END;                                                     0170
@RF00143 DS    0H                                                  0171
*                                                                  0171
*/********************************************************************/
*/*                                                                  */
*/* CLEAR THE PAGE-FIXING ECB, AND SET UP THE PAGE-FIXING INTERFACE. */
*/*                                                                  */
*/********************************************************************/
*                                                                  0171
*       XSAECB=0;                                                  0171
@RC00118 SLR   @15,@15                                             0171
         ST    @15,XSAECB(,XSAPTR)                                 0171
*       RESPECIFY                                                  0172
*        (REG0,                                                    0172
*         REG1,                                                    0172
*         REG11,                                                   0172
*         REG12,                                                   0172
*         REG13,                                                   0172
*         REG14,                                                   0172
*         REG15) RESTRICTED;                                       0172
*       REG1=ADDR(BEGLOCK);         /* GET PTR TO START OF LOCKED  0173
*                                      CODE                          */
         LA    REG1,BEGLOCK                                        0173
*       REG15=ADDR(ENDLOCK);        /* GET PTR TO END OF LOCKED CODE */
         LA    REG15,ENDLOCK                                       0174
*       REG0=ADDR(XSAECB);          /* GET ADDRESS OF ECB            */
         LA    REG0,XSAECB(,XSAPTR)                                0175
*/********************************************************************/
*/*                                                                  */
*/* INVOKE THE PAGE-FIXING ROUTINE TO GET THE PAGE CONTAINING THE    */
*/* WORKAREA FIXED, AND WAIT FOR THE COMPLETION OF THE OPERATION.    */
*/* THEN, GET THE DISPATCHER LOCK TO SERIALIZE USE OF THE REAL TIME  */
*/* TQE QUEUE.                                                       */
*/*                                                                  */
*/********************************************************************/
*                                                                  0176
*       GENERATE CODE(PGFIX R,A=(1),EA=(15),ECB=(0),LONG=N);       0176
         PGFIX R,A=(1),EA=(15),ECB=(0),LONG=N
*       REG1=ADDR(XSAECB);          /* GET ECB ADDRESS               */
         LA    REG1,XSAECB(,XSAPTR)                                0177
*       GENERATE CODE REFS(PSALITA) SETS(XSAECB);                  0178
*                                                                  0178
*                                                 /*
         WAIT  1,ECB=(1)        WAIT FOR THE PAGE FIX
         SETLOCK  OBTAIN,TYPE=DISP,MODE=UNCOND,                       **
               RELATED=('REAL TQE QUEUE',(IEE6503D,(NOLABEL)))  */
*/********************************************************************/
*/*                                                                  */
*/* SET UP FRR INTERFACE AND ISSUE SETFRR TO ESTABLISH A RECOVERY    */
*/* EXIT FOR THE LOCKED CODE.                                        */
*/*                                                                  */
*/********************************************************************/
*                                                                  0179
*BEGLOCK:                                                          0179
*       REG11=TPCFRRP;              /* GET EXIT ROUTINE ADDRESS      */
BEGLOCK  L     @03,TPCPTR(,LCVTPTR)                                0179
         L     REG11,TPCFRRP(,@03)                                 0179
*       GENERATE CODE REFS(REG11,PSACSTK,PSA,PSALSFCC,FRRS,FRRSLAST,
*           FRRSENTR,FRRSELEN) SETS(FRRSCURR,FRRSFRRA,FRRSPARM,FRRSPTR,
*           FRREPTR,REG12);                                        0180
*                                                                   /*
         SETFRR A,FRRAD=(11),PARMAD=(12),WRKREGS=(FRRSPTR,FRREPTR),   **
               RELATED=('DISP LOCK',(IEE6503D,(NONE)))          */
*       FRRPADDR=REG12;             /* SAVE ADDR OF PARAMETER AREA   */
         LR    FRRPADDR,REG12                                      0181
*       RESPECIFY                                                  0182
*         TFRRPARM BASED(FRRPADDR);                                0182
*       TFRRID=ID6503D;             /* INDICATE CSECT AND FUNCTION   */
         MVC   TFRRID(4,FRRPADDR),ID6503D                          0183
*       TFRRDISP='1'B;              /* INDICATE DISPATCHER LOCK      */
         OI    TFRRDISP(FRRPADDR),B'10000000'                      0184
*       RESPECIFY                                                  0185
*        (REG0,                                                    0185
*         REG1,                                                    0185
*         REG11,                                                   0185
*         REG12,                                                   0185
*         REG13,                                                   0185
*         REG14,                                                   0185
*         REG15) UNRESTRICTED;                                     0185
*                                                                  0186
*/********************************************************************/
*/*                                                                  */
*/* TEST FOR SPECIFICATION OF THE DATE PARAMETER BY ITSELF.          */
*/*                                                                  */
*/********************************************************************/
*                                                                  0186
*       IF CLOCK^='1'B&RESET^='1'B THEN                            0186
*                                                                  0186
         TM    CLOCK(XSAPTR),B'00010001'                           0186
         BNZ   @RF00186                                            0186
*/********************************************************************/
*/*                                                                  */
*/* IF DATE WAS SPECIFIED ALONE, SET THE CORRECTION FACTOR TO ZERO.  */
*/*                                                                  */
*/********************************************************************/
*                                                                  0187
*         CORRFACT=0;                                              0187
*                                                                  0187
         SLR   CORRFACT,CORRFACT                                   0187
*/********************************************************************/
*/*                                                                  */
*/* IF CLOCK OR RESET WAS SPECIFIED, CALCULATE THE CORRECTION FACTOR */
*/* AS THE DIFFERENCE BETWEEN THE OLD AND NEW LOCAL TIMES.           */
*/* THEN PUT THE NEW TIME ZONE CONSTANT INTO THE CVT.                */
*/*                                                                  */
*/********************************************************************/
*                                                                  0188
*       ELSE                                                       0188
*         DO;                                                      0188
         B     @RC00186                                            0188
@RF00186 DS    0H                                                  0189
*           RESPECIFY                                              0189
*            (REG12,                                               0189
*             REG13,                                               0189
*             REG14) RESTRICTED;                                   0189
*           REG13=XSAGMTL+CVTTZ;    /* GET OLD LOCAL CLOCK VALUE     */
         L     REG13,XSAGMTL(,XSAPTR)                              0190
         AL    REG13,CVTTZ(,LCVTPTR)                               0190
*           CALL CONVERT;           /* CONVERT TO SECONDS            */
         BAL   @14,CONVERT                                         0191
*           RESPECIFY                                              0192
*            (REG13,                                               0192
*             REG14) UNRESTRICTED;                                 0192
*                                                                  0192
*/********************************************************************/
*/*                                                                  */
*/* CONVERT CORRECTION FACTOR TO 1.048576 SECOND UNITS.              */
*/*                                                                  */
*/********************************************************************/
*                                                                  0193
*           REG12=(REG12-NLCLTOD)*K15625H;                         0193
         LR    @01,REG12                                           0193
         SL    @01,NLCLTOD                                         0193
         MH    @01,K15625H                                         0193
         LR    REG12,@01                                           0193
*           SRA(REG12,14);                                         0194
         SRA   REG12,14                                            0194
*           CORRFACT=REG12;         /* SAVE CORRECTION FACTOR        */
         LR    CORRFACT,REG12                                      0195
*           CVTTZ=NEWTZ;            /* STORE NEW TIME ZONE CONSTANT  */
         ST    NEWTZ,CVTTZ(,LCVTPTR)                               0196
*           RESPECIFY                                              0197
*            (REG12) UNRESTRICTED;                                 0197
*                                                                  0198
*/********************************************************************/
*/*                                                                  */
*/* SEARCH THE QUEUE OF REAL TIME TQE'S FOR THOSE SPECIFIED WITH THE */
*/* TOD OPTION.  WHEN ONE IS FOUND UPDATE THE TQEVAL FIELD TO        */
*/* REFLECT THE NEW LOCAL TIME.  END THE SEARCH WHEN THE DUMMY TQE   */
*/* IS ENCOUNTERED.                                                  */
*/*                                                                  */
*/* GET THE ADDRESS OF THE TOP REAL TIME TQE, AND INITIALIZE THE     */
*/* TEMPORARY CHAIN OF TQE'S.                                        */
*/*                                                                  */
*/********************************************************************/
*                                                                  0198
*           RESPECIFY                                              0198
*            (TQEPTR,                                              0198
*             CHAINREG) RESTRICTED;                                0198
*           TQEPTR=TPCHDCCQ;                                       0199
         L     @15,TPCPTR(,LCVTPTR)                                0199
         L     TQEPTR,TPCHDCCQ(,@15)                               0199
*           TFRRPHS1='1'B;          /* INDICATE TQE'S BEING DEQUEUED */
         OI    TFRRPHS1(FRRPADDR),B'10000000'                      0200
*           CHAINREG=0;                                            0201
         SLR   CHAINREG,CHAINREG                                   0201
*           DO WHILE TQEDUM='0'B;                                  0202
*                                                                  0202
         B     @DE00202                                            0202
@DL00202 DS    0H                                                  0203
*/********************************************************************/
*/*                                                                  */
*/* TEST FOR SPECIFICATION OF THE TOD OPTION.                        */
*/*                                                                  */
*/********************************************************************/
*                                                                  0203
*             IF TQETOD='1'B THEN                                  0203
*                                                                  0203
         TM    TQETOD(TQEPTR),B'01000000'                          0203
         BNO   @RF00203                                            0203
*/********************************************************************/
*/*                                                                  */
*/* IF THE TOD OPTION WAS SPECIFIED FOR THIS TQE, CALCULATE THE      */
*/* OLD TIME REMAINING (IE. THE TIME REMAINING BEFORE THE TQE        */
*/* EXPIRES, RELATIVE TO THE OLD LOCAL TIME). THE OLD TIME           */
*/* REMAINING IS THE DIFFERENCE BETWEEN THE CURRENT TOD CLOCK        */
*/* VALUE, AND TQEVAL.                                               */
*/*                                                                  */
*/********************************************************************/
*                                                                  0204
*               DO;                                                0204
*                 OLDTR=TQEVALLH-XSAGMTL;                          0205
*                                                                  0205
         L     OLDTR,TQEVALLH(,TQEPTR)                             0205
         SL    OLDTR,XSAGMTL(,XSAPTR)                              0205
*/********************************************************************/
*/*                                                                  */
*/* TEST FOR ANY OLD TIME REMAINING.                                 */
*/*                                                                  */
*/********************************************************************/
*                                                                  0206
*                 IF OLDTR>0 THEN                                  0206
*                                                                  0206
         LTR   OLDTR,OLDTR                                         0206
         BNP   @RF00206                                            0206
*/********************************************************************/
*/*                                                                  */
*/* IF THERE IS SOME, CALL THE ROUTINE TO DEQUEUE THE TQE.           */
*/*                                                                  */
*/********************************************************************/
*                                                                  0207
*                   DO;                                            0207
*                     RESPECIFY                                    0208
*                      (REG15) RESTRICTED;                         0208
*                     REG15=ADDR(DEQTQE);                          0209
         L     REG15,CVTQTD00(,LCVTPTR)                            0209
*                     GENERATE CODE SETS(REG2) REFS(REG15)         0210
*                         (BALR  REG2,REG15);                      0210
         BALR  REG2,REG15
*                     RESPECIFY                                    0211
*                      (REG15) UNRESTRICTED;                       0211
*                     XSAPTR=SAVEXSA;/* RESTORE POINTER TO XSA       */
         L     XSAPTR,SAVEXSA                                      0212
*/********************************************************************/
*/*                                                                  */
*/* CALCULATE THE NEW TIME REMAINING (IE. THE TIME REMAINING BEFORE  */
*/* THE TQE EXPIRES, RELATIVE TO THE NEW LOCAL TIME).                */
*/* THE NEW TIME REMAINING IS THE SUM OF THE OLD TIME REMAINING AND  */
*/* THE PREVIOUSLY CALCULATED CORRECTION FACTOR.                     */
*/*                                                                  */
*/********************************************************************/
*                                                                  0213
*                     NEWTR=OLDTR+CORRFACT;                        0213
*                                                                  0213
         LR    NEWTR,OLDTR                                         0213
         ALR   NEWTR,CORRFACT                                      0213
*/********************************************************************/
*/*                                                                  */
*/* TEST FOR A NEW TIME REMAINING THAT IS GREATER THAN 24 HOURS.     */
*/*                                                                  */
*/********************************************************************/
*                                                                  0214
*                     IF NEWTR>K24HRTOD THEN                       0214
*                                                                  0214
         L     @15,K24HRTOD                                        0214
         CR    NEWTR,@15                                           0214
         BNH   @RF00214                                            0214
*/********************************************************************/
*/*                                                                  */
*/* IF IT IS GREATER, SUBTRACT 24 HOURS FROM THE NEW TIME REMAINING. */
*/* NOTE: WHEN A TQE IS ON A QUEUE, THE VALUE OF TQEVAL CAN NEVER BE */
*/* SUCH THAT MORE THAN 24 HOURS WILL PASS BEFORE IT EXPIRES.  THIS  */
*/* CONSTRAINT IS DICTATED BY THE TIME SVC.  IN THE ALGORITHM USED   */
*/* HERE, THE NEW TIME REMAINING CAN NEVER BE GREATER THAN 48 HOURS  */
*/* SINCE NEITHER THE OLD TIME REMAINING, NOR THE CORRECTION FACTOR  */
*/* CAN BE GREATER THAN 24 HOURS EACH.  THUS SUBTRACTING 24 HOURS    */
*/* FROM A NEW TIME REMAINING THAT IS GREATER THAN 24 HOURS GIVES THE*/
*/* CORRECT AMOUNT OF TIME REMAINING RELATIVE TO THE NEW LOCAL TIME. */
*/*                                                                  */
*/********************************************************************/
*                                                                  0215
*                       NEWTR=NEWTR-K24HRTOD;                      0215
*                                                                  0215
         SLR   NEWTR,@15                                           0215
*/********************************************************************/
*/*                                                                  */
*/* SET THE TQEVAL EQUAL TO THE SUM OF THE CURRENT TOD CLOCK VALUE   */
*/* AND THE NEW TIME REMAINING.                                      */
*/*                                                                  */
*/********************************************************************/
*                                                                  0216
*                     TQEVALLH=NEWTR+XSAGMTL;                      0216
*                                                                  0216
@RF00214 LR    @15,NEWTR                                           0216
         AL    @15,XSAGMTL(,XSAPTR)                                0216
         ST    @15,TQEVALLH(,TQEPTR)                               0216
*/********************************************************************/
*/*                                                                  */
*/* PLACE THE UPDATED TQE ON THE TEMPORARY CHAIN.                    */
*/* NOTE: THE PURPOSE OF THIS IS TO PREVENT UPDATED TQE'S FROM       */
*/* BEING MISTAKENLY UPDATED AGAIN. THIS COULD HAPPEN IF THE NEW     */
*/* TQEVAL CAUSED THE TQE TO BE ENQUEUED FARTHER DOWN IN THE REAL    */
*/* TIME TQE QUEUE THAN IT PREVIOUSLY HAD BEEN.                      */
*/*                                                                  */
*/********************************************************************/
*                                                                  0217
*                     TQEBLNK=CHAINREG;                            0217
         ST    CHAINREG,TQEBLNK(,TQEPTR)                           0217
*                     CHAINREG=TQEPTR;                             0218
         LR    CHAINREG,TQEPTR                                     0218
*                   END;                                           0219
*               END;                                               0220
*                                                                  0220
@RF00206 DS    0H                                                  0221
*/********************************************************************/
*/*                                                                  */
*/* POINT TO THE NEXT TQE ON THE REAL TIME QUEUE.                    */
*/*                                                                  */
*/********************************************************************/
*                                                                  0221
*             TQEPTR=TQEFLNK;                                      0221
@RF00203 L     TQEPTR,TQEFLNK(,TQEPTR)                             0221
*             TFRRTQEP=TQEPTR;      /* SAVE PTR TO NEXT TQE TO     0222
*                                      PROCESS IN FRR PARM AREA      */
         ST    TQEPTR,TFRRTQEP(,FRRPADDR)                          0222
*           END;                                                   0223
@DE00202 TM    TQEDUM(TQEPTR),B'00010000'                          0223
         BZ    @DL00202                                            0223
*                                                                  0224
*/********************************************************************/
*/*                                                                  */
*/* ONE BY ONE, TAKE THE UPDATED TQE'S FROM THE TEMPORARY CHAIN AND  */
*/* RE-ENQUEUE THEM ON THE REAL TIME QUEUE.                          */
*/*                                                                  */
*/********************************************************************/
*                                                                  0224
*           DO WHILE CHAINREG^=0;                                  0224
         B     @DE00224                                            0224
@DL00224 DS    0H                                                  0225
*             TFRRPHS2='1'B;        /* INDICATE TQE'S BEING        0225
*                                      RE-ENQUEUED                   */
         OI    TFRRPHS2(FRRPADDR),B'01000000'                      0225
*/********************************************************************/
*/*                                                                  */
*/* GET THE ADDRESS OF THE FIRST (NEXT) TQE ON THE CHAIN.            */
*/*                                                                  */
*/********************************************************************/
*                                                                  0226
*             TQEPTR=CHAINREG;                                     0226
         LR    TQEPTR,CHAINREG                                     0226
*             TFRRTQEP=TQEPTR;      /* SAVE PTR TO TQE BEING ENQUEUED
*                                      IN FRR PARM AREA              */
         ST    TQEPTR,TFRRTQEP(,FRRPADDR)                          0227
*             CHAINREG=TQEBLNK;                                    0228
*                                                                  0228
         L     CHAINREG,TQEBLNK(,TQEPTR)                           0228
*/********************************************************************/
*/* BEFORE RE-ENQUEING THE TQE, TURN OFF THE TQEUSER FLAG SO THAT    */
*/* THE TQE ENQUEUE ROUTINE WILL NOT TRY TO STORE THE TQE ADDRESS IN */
*/* TCB.  SINCE WE ARE EXECUTING IN THE MASTER'S ADDRESS SPACE, A    */
*/* REFERENCE TO A TCB NOT IN THIS ADDRESS SPACE COULD CAUSE AN 0C4  */
*/* OR A WILD STORE.  THE TQEUSER FLAG WILL BE TURNED BACK ON WHEN   */
*/* CONTROL IS RECEIVED BACK FROM THE TQE ENQUEUE ROUTINE.   @ZA12715*/
*/********************************************************************/
*                                                                  0229
*             TQEUSER='0'B;         /*                       @ZA12715*/
         NI    TQEUSER(TQEPTR),B'10111111'                         0229
*/********************************************************************/
*/*                                                                  */
*/* CALL THE ROUTINE TO RE-ENQUEUE THE TQE.                          */
*/*                                                                  */
*/********************************************************************/
*                                                                  0230
*             RESPECIFY                                            0230
*              (REG15) RESTRICTED;                                 0230
*             REG15=ADDR(ENQTQE);                                  0231
         L     REG15,CVTQTE00(,LCVTPTR)                            0231
*             GENERATE CODE SETS(REG2) REFS(REG15)(BALR  REG2,REG15);
         BALR  REG2,REG15
*             RESPECIFY                                            0233
*              (REG15) UNRESTRICTED;                               0233
*             TQEUSER='1'B;         /*                       @ZA12715*/
         OI    TQEUSER(TQEPTR),B'01000000'                         0234
*           END;                                                   0235
*                                                                  0235
@DE00224 LTR   CHAINREG,CHAINREG                                   0235
         BNZ   @DL00224                                            0235
*/********************************************************************/
*/*                                                                  */
*/* WHEN ALL TQE'S HAVE BEEN ENQUEUED, RESTORE THE POINTER TO THE XSA*/
*/*                                                                  */
*/********************************************************************/
*                                                                  0236
*           XSAPTR=SAVEXSA;                                        0236
         L     XSAPTR,SAVEXSA                                      0236
*           TFRRPHS3='1'B;          /* INDICATE MIDNIGHT TQE BEING 0237
*                                      PROCESSED                     */
         OI    TFRRPHS3(FRRPADDR),B'00100000'                      0237
*         END;                                                     0238
*                                                                  0239
*/********************************************************************/
*/*                                                                  */
*/* TEST FOR IPL OR VARY CPU ONLINE NOT IN PROGRESS.                 */
*/*                                                                  */
*/********************************************************************/
*                                                                  0239
*       IF BAIN='1'B THEN                                          0239
*                                                                  0239
@RC00186 L     @15,CVTMSER(,LCVTPTR)                               0239
         TM    BAIN(@15),B'10000000'                               0239
         BNO   @RF00239                                            0239
*/********************************************************************/
*/*                                                                  */
*/* IF THAT IS THE CASE, GET THE ADDRESS OF THE MIDNIGHT TQE, AND    */
*/* CALL THE DEQUEUE TQE ROUTINE TO DEQUEUE THE MIDNIGHT ELEMENT.    */
*/*                                                                  */
*/********************************************************************/
*                                                                  0240
*         DO;                                                      0240
*           TQEPTR=ADDR(TPCMNTQE);                                 0241
         L     TQEPTR,TPCPTR(,LCVTPTR)                             0241
         LA    TQEPTR,TPCMNTQE(,TQEPTR)                            0241
*           RESPECIFY                                              0242
*            (REG15) RESTRICTED;                                   0242
*           REG15=ADDR(DEQTQE);                                    0243
         L     REG15,CVTQTD00(,LCVTPTR)                            0243
*           GENERATE CODE SETS(REG2) REFS(REG15)(BALR  REG2,REG15);
         BALR  REG2,REG15
*           RESPECIFY                                              0245
*            (REG15) UNRESTRICTED;                                 0245
*           XSAPTR=SAVEXSA;         /* RESTORE POINTER TO XSA        */
         L     XSAPTR,SAVEXSA                                      0246
*/********************************************************************/
*/*                                                                  */
*/* TEST FOR SPECIFICATION OF THE RESET PARAMETER, AND IF SPECIFIED, */
*/* CALCULATE THE NEW TQEVAL OF THE MIDNIGHT ELEMENT.                */
*/*                                                                  */
*/********************************************************************/
*                                                                  0247
*           IF RESET='1'B THEN                                     0247
         TM    RESET(XSAPTR),B'00010000'                           0247
         BNO   @RF00247                                            0247
*             DO;                                                  0248
*               RESPECIFY                                          0249
*                (TMICSECL,                                        0249
*                 TMICSECR) RESTRICTED;                            0249
*               CVTDATE=PKDATE;     /* MOVE CALCULATED DATE TO CVT   */
         MVC   CVTDATE(4,LCVTPTR),PKDATE(XSAPTR)                   0250
*               TMICSECR=ELDAYS;    /* FETCH DAYS ELAPSED SINCE EPOCH
*                                      PLUS 1                        */
         L     TMICSECR,ELDAYS                                     0251
*/********************************************************************/
*/*                                                                  */
*/* CONVERT DAYS TO MICROSECONDS.                                    */
*/*                                                                  */
*/********************************************************************/
*                                                                  0252
*               GENERATE CODE REFS(MINSDAY,MICSMIN) SETS(TMICSECL, 0252
*                   TMICSECR);                                     0252
*                                                                  0252
          MH    TMICSECR,MINSDAY      CONVERT DAYS TO MINUTES
          M     TMICSECL,MICSMIN      CONVERT MINS TO MIC SECS
          SLDL  TMICSECL,12           1 MIC SEC TO BIT 51
*/********************************************************************/
*/*                                                                  */
*/* CORRECT FOR THE TIME ZONE AND STORE INTO THE MIDNIGHT TQE.       */
*/*                                                                  */
*/********************************************************************/
*                                                                  0253
*               TMICSECL=TMICSECL-CVTTZ;                           0253
         SL    TMICSECL,CVTTZ(,LCVTPTR)                            0253
*               STM(TMICSECL,TMICSECR,TQEVAL);                     0254
         STM   TMICSECL,TMICSECR,TQEVAL(TQEPTR)                    0254
*               RESPECIFY                                          0255
*                (TMICSECL,                                        0255
*                 TMICSECR) UNRESTRICTED;                          0255
*             END;                                                 0256
*                                                                  0256
*/********************************************************************/
*/*                                                                  */
*/* IF RESET WAS NOT SPECIFIED, USE THE CORRECTION FACTOR TO         */
*/* CALCULATE AND STORE THE NEW TQEVAL FOR THE MIDNIGHT TQE.  THEN,  */
*/* IF THE DATE PARAMETER WAS SPECIFIED, STORE THE DATE INTO THE CVT.*/
*/*                                                                  */
*/********************************************************************/
*                                                                  0257
*           ELSE                                                   0257
*             DO;                                                  0257
         B     @RC00247                                            0257
@RF00247 DS    0H                                                  0258
*               TQEVALLH=TQEVALLH+CORRFACT;                        0258
         LR    @15,CORRFACT                                        0258
         AL    @15,TQEVALLH(,TQEPTR)                               0258
         ST    @15,TQEVALLH(,TQEPTR)                               0258
*               IF DATE='1'B THEN                                  0259
         TM    DATE(XSAPTR),B'00000010'                            0259
         BNO   @RF00259                                            0259
*                 CVTDATE=PKDATE;                                  0260
         MVC   CVTDATE(4,LCVTPTR),PKDATE(XSAPTR)                   0260
*             END;                                                 0261
*                                                                  0261
@RF00259 DS    0H                                                  0262
*/********************************************************************/
*/*                                                                  */
*/* CALL THE ENQUEUE TQE ROUTINE TO RE-ENQUEUE THE MIDNIGHT TQE.     */
*/* THEN RESTORE THE POINTER TO THE EXTENDED SAVE AREA.              */
*/*                                                                  */
*/********************************************************************/
*                                                                  0262
*           RESPECIFY                                              0262
*            (REG15) RESTRICTED;                                   0262
@RC00247 DS    0H                                                  0263
*           REG15=ADDR(ENQTQE);                                    0263
         L     REG15,CVTQTE00(,LCVTPTR)                            0263
*           GENERATE CODE SETS(REG2) REFS(REG15)(BALR  REG2,REG15);
         BALR  REG2,REG15
*           RESPECIFY                                              0265
*            (REG15) UNRESTRICTED;                                 0265
*           XSAPTR=SAVEXSA;                                        0266
         L     XSAPTR,SAVEXSA                                      0266
*         END;                                                     0267
*                                                                  0267
*/********************************************************************/
*/*                                                                  */
*/* IF IPL OR VARY CPU ONLINE IS IN PROGRESS, SEE IF THE DATE PARA-  */
*/* METER WAS SPECIFIED, AND IF SO, STORE THE NEW LOCAL DATE INTO    */
*/* THE CVT.                                                         */
*/*                                                                  */
*/********************************************************************/
*                                                                  0268
*       ELSE                                                       0268
*         IF DATE='1'B THEN                                        0268
         B     @RC00239                                            0268
@RF00239 TM    DATE(XSAPTR),B'00000010'                            0268
         BNO   @RF00268                                            0268
*           CVTDATE=PKDATE;                                        0269
         MVC   CVTDATE(4,LCVTPTR),PKDATE(XSAPTR)                   0269
*                                                                  0270
*/********************************************************************/
*/*                                                                  */
*/* DELETE THE RECOVERY EXIT FOR THE LOCKED CODE.  THEN, RELEASE     */
*/* THE DISPATCHER LOCK, AND FREE THE WORKAREA PAGE.                 */
*/*                                                                  */
*/********************************************************************/
*                                                                  0270
*       RESPECIFY                                                  0270
*        (REG0,                                                    0270
*         REG1,                                                    0270
*         REG11,                                                   0270
*         REG12,                                                   0270
*         REG13,                                                   0270
*         REG14,                                                   0270
*         REG15) RESTRICTED;                                       0270
@RF00268 DS    0H                                                  0270
@RC00239 DS    0H                                                  0271
*       GENERATE CODE REFS(PSALITA,PSACSTK,PSA,FRRS,FRRSEMP,FRRSELEN)
*           SETS(FRRSCURR,FRRSPTR,FRREPTR);                        0271
*                                             /*
         SETFRR D,WRKREGS=(FRRSPTR,FRREPTR),                          **
               RELATED=('DISP LOCK',(IEE6503D,(NONE)))
         SETLOCK  RELEASE,TYPE=DISP,                                  **
               RELATED=('REAL TQE QUEUE',(IEE6503D,(NOLABEL)))   */
*       REG1=ADDR(BEGLOCK);         /* GET PTR TO START OF LOCKED  0272
*                                      CODE                          */
         LA    REG1,BEGLOCK                                        0272
*       REG15=ADDR(ENDLOCK);        /* GET PTR TO END OF LOCKED CODE */
         LA    REG15,ENDLOCK                                       0273
*       GENERATE CODE(PGFREE   R,A=(1),EA=(15));                   0274
         PGFREE   R,A=(1),EA=(15)
*     END;                                                         0275
*                                                                  0276
*/********************************************************************/
*/*                                                                  */
*/* IF NO GOOD TOD CLOCK WAS FOUND, GET THE ID OF THE CONSOLE FROM   */
*/* WHICH THE SET COMMAND WAS ISSUED, AND TELL THE OPERATOR OF THE   */
*/* FAILURE.                                                         */
*/*                                                                  */
*/********************************************************************/
*                                                                  0276
*   ELSE                                                           0276
*     DO;                                                          0276
         B     @RC00115                                            0276
@RF00115 DS    0H                                                  0277
*       REG0=CONSID;                                               0277
         SLR   REG0,REG0                                           0277
         IC    REG0,CONSID(,XSAPTR)                                0277
*       GENERATE CODE;                                             0278
*                      /*
         WTO 'IEA899I LOCAL TIME CHANGE REJECTED-NO GOOD TOD CLOCKS', **
               MCSFLAG=(REG0,RESP,NOTIME),DESC=(5)             */
*     END;                                                         0279
*                                                                  0279
*/********************************************************************/
*/*                                                                  */
*/* RESTORE THE CALLER'S RETURN ADDRESS TO REGISTER 14.              */
*/*                                                                  */
*/********************************************************************/
*                                                                  0280
*   REG14=SAVERET;                                                 0280
*                                                                  0280
@RC00115 L     REG14,SAVERET                                       0280
*/********************************************************************/
*/*                                                                  */
*/* SET CSECT ID FIELD OF XSA TO 0 TO INDICATE THAT IEE6503D IS NO   */
*/* LONGER IN CONTROL, AND RETURN TO IEE0603D.                       */
*/*                                                                  */
*/********************************************************************/
*                                                                  0281
*   CSECTCDE='00'X;                                                0281
         MVI   CSECTCDE(XSAPTR),X'00'                              0281
*                                                                  0282
*/********************************************************************/
*/*                                                                  */
*/* THIS ROUTINE RECEIVES A HIGH ORDER 32 BIT TOD CLOCK VALUE IN     */
*/* REGISTER 13, AND CONVERTS IT TO DAYS ELAPSED SINCE THE EPOCH IN  */
*/* REGISTER 13 AND SECONDS ELAPSED SINCE THE LAST MIDNIGHT IN       */
*/* REGISTER 12.                                                     */
*/*                                                                  */
*/********************************************************************/
*                                                                  0282
*CONVERT:                                                          0282
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA,NOENTREG,NOPARMREG,RETREG);
@EL00001 DS    0H                                                  0282
@EF00001 L     @00,@SIZDATD                                        0282
         LR    @01,@09                                             0282
         FREEMAIN R,LV=(0),A=(1)
@ER00001 BR    @14                                                 0282
CONVERT  DS    0H                                                  0283
*   RESPECIFY                                                      0283
*    (REG12,                                                       0283
*     REG13) RESTRICTED;                                           0283
*                                                                  0283
*/* D465000                                                          */
*/********************************************************************/
*/*                                                                  */
*/* CONVERT THE 32 BIT TOD CLOCK VALUE TO SECONDS.                   */
*/*                                                                  */
*/********************************************************************/
*                                                                  0284
*   SRL(REG13,1);                   /* ENSURE 0 SIGN BIT     @YM06850*/
         SRL   REG13,1                                             0284
*/********************************************************************/
*/*                                                                  */
*/* MULTIPLY H. O. TOD CLOCK VALUE SCALED -1 BY 2**20 MICROSECONDS   */
*/* SCALED +30, GIVING SECONDS SCALED +29.                           */
*/*                                                                  */
*/********************************************************************/
*                                                                  0285
*   GENERATE CODE SETS(REG12,REG13) REFS(TCUTOSEC)                 0285
*       (M     REG12,TCUTOSEC);     /*                       @YM06850*/
         M     REG12,TCUTOSEC
*/********************************************************************/
*/*                                                                  */
*/* SHIFT THE RESULT 29 BITS RIGHT TO SCALE IT 0, CLEAR REGISTER 12, */
*/* AND PLACE THE RESULT IN REGISTER 13 FOR DIVISION.                */
*/*                                                                  */
*/********************************************************************/
*                                                                  0286
*   SRDL(REG12,29);                 /*                       @YM06850*/
         SRDL  REG12,29                                            0286
*/********************************************************************/
*/*                                                                  */
*/* DIVIDE SECONDS BY THE NUMBER OF SECONDS IN 1 DAY, GIVING DAYS    */
*/* ELAPSED SINCE THE EPOCH IN REG 13, AND SECONDS ELAPSED SINCE THE */
*/* LAST MIDNIGHT IN REG 12.                                         */
*/*                                                                  */
*/********************************************************************/
*/* D475600                                                          */
*                                                                  0287
*   GENERATE CODE REFS(K24HRSEC) SETS(REG12,REG13)                 0287
*       (D     REG12,K24HRSEC);                                    0287
         D     REG12,K24HRSEC
*   RESPECIFY                                                      0288
*    (REG12,                                                       0288
*     REG13) UNRESTRICTED;                                         0288
*                                                                  0288
*/********************************************************************/
*/*                                                                  */
*/* RETURN.                                                          */
*/*                                                                  */
*/********************************************************************/
*                                                                  0289
*   END CONVERT;                                                   0289
@EL00002 DS    0H                                                  0289
@EF00002 DS    0H                                                  0289
@ER00002 BR    @14                                                 0289
*DELTA3:                                                           0290
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA,ENTREG,RETREG,NOPARMREG);  0290
DELTA3   DS    0H                                                  0291
*   GENERATE CODE REFS(REG1)(STCK  0(REG1));                       0291
         STCK  0(REG1)
*   END DELTA3;                                                    0292
@EL00003 DS    0H                                                  0292
@EF00003 DS    0H                                                  0292
@ER00003 BR    @14                                                 0292
*   END IEE6503D                                                   0293
