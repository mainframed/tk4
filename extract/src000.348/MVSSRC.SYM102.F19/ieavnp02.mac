         TITLE 'IEAVNP02 - UCB INITIALIZATION ROUTINE                  *
                        '
IEAVNP02 CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL2(@EP00001-@MAINENT)
         DC    AL1(22)                                             0001
         DC    C'IEAVNP02 UZ09243 77117'                           0001
IEAVNPC2 DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL2(@EP00216-IEAVNPC2)
         ENTRY IEAVNPC2
IEAVNPB2 DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL2(@EP00221-IEAVNPB2)
         ENTRY IEAVNPB2
IEAVNPA2 DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL2(@EP00224-IEAVNPA2)
         ENTRY IEAVNPA2
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @12,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@12                                         0001
         ST    @13,@SA00001+4                                      0001
         LA    @14,@SA00001                                        0001
         ST    @14,8(,@13)                                         0001
         LR    @13,@14                                             0001
         AH    @15,4(,@15)                                         0001
         BR    @15                                                 0001
@EP00001 DS    0H                                                  0002
*
* REFS ARE GENERATED FOR NVT FIELDS THAT CONTAIN ADDRESSES OF
* NIP SERVICE ROUTINES, SO IEAPMNIP MACRO CALLS TO THESE ROUTINES
* WILL ASSEMBLE PROPERLY.
*                                                                  0215
*/********************************************************************/
*/*                                                                  */
*/*  THIS THIRD ENTRY POINT IS THE SAME AS THE SECOND ENTRY POINT    */
*/*  EXCEPT THAT ONLY VIRTUAL UCBS ARE VERIFIED                      */
*/*                                                                  */
*/********************************************************************/
*                                                                  0216
*IEAVNPC2:                          /* THIRD ENTRY POINT     @ZM31050*/
*   ENTRY;                                                         0216
@EP00216 DS    0H                                                  0217
*   NPENTRY=THREE;                  /* INDICATE 3RD ENTRY    @ZM31050*/
         MVI   NPENTRY,X'03'                                       0217
*   IF ICBI=NOTON THEN              /* IF MSS NOT INITIALIZED      0218
*                                                            @G24LPLN*/
         L     @11,CVTEXT2(,RCVT)                                  0218
         L     @11,CVTICB(,@11)                                    0218
         TM    ICBI(@11),B'10000000'                               0218
         BNZ   @RF00218                                            0218
*     NP2NOMSS=ON;                  /* INDICATE FOR LATER USE      0219
*                                                            @G24LPLN*/
         OI    NP2NOMSS,B'00010000'                                0219
*   GO TO NP2COMM;                  /* DO COMMON PROCESSING  @ZM31050*/
         B     NP2COMM                                             0220
*/********************************************************************/
*/*                                                                  */
*/*  OBTAIN THE ADDRESS OF THE UCB ADDRESS TABLE, IECILK2,           */
*/*  FROM THE CVT (CVTILK2) AND SET A TABLE INDEX, I, TO             */
*/*  REFLECT THE FIRST TABLE ENTRY.  INCREMENT THE TABLE INDEX       */
*/*  AND EXAMINE EACH UCB WHICH IS REPRESENTED BY A HALFWORD UCB     */
*/*  ADDRESS ENTRY IN IECILK2.                                       */
*/*                                                                  */
*/*  BYPASS TESTING FOR DUMMY ENTRIES IN IECILK2 AND ANY UCB         */
*/*  WHICH HAS BEEN PREVIOUSLY TESTED.  DEVICE TESTING IS            */
*/*  COMPLETE WHEN THE INDEX SEARCH REACHES THE END OF IECILK2.      */
*/*                                                                  */
*/********************************************************************/
*/*  SECOND ENTRY ALIAS                                              */
*                                                                  0221
*IEAVNPB2:                          /* SECOND ENTRY ALIAS            */
*                                                                  0221
*   /*****************************************************************/
*   /*                                                               */
*   /* SECOND ENTRY POINT                                            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0221
*   ENTRY;                          /* SECOND ENTRY                  */
@EP00221 DS    0H                                                  0222
*   NPENTRY=TWO;                    /* INDICATE 2ND ENTRY    @ZM31050*/
         MVI   NPENTRY,X'02'                                       0222
*   GO TO NP2COMM;                  /* DO COMMON PROCESSING  @ZM31050*/
         B     NP2COMM                                             0223
*/*  FIRST ENTRY ALIAS                                               */
*                                                                  0224
*IEAVNPA2:                          /* FIRST ENTRY ALIAS             */
*                                                                  0224
*   /*****************************************************************/
*   /*                                                               */
*   /* FIRST ENTRY POINT                                             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0224
*   ENTRY;                          /* FIRST ENTRY                   */
@EP00224 DS    0H                                                  0225
*   NPENTRY=ONE;                    /* INDICATE 1ST ENTRY    @ZM31050*/
         MVI   NPENTRY,X'01'                                       0225
*NP2COMM:                           /* COMMON FOR ALL ENTRIES      0226
*                                                            @ZM31050*/
*                                                                  0226
*   /*****************************************************************/
*   /*                                                               */
*   /* PROGRAM, SRB AND IOSB INITIALIZATION                          */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0226
*   I=ONE;                          /* SET UCB LOOK UP TBL INCR      */
NP2COMM  MVC   I(2),@CH00068                                       0226
*   IOSB=IOSB&&IOSB;                /* CLEAR IOSB                    */
         LA    @11,NP2IOSB                                         0227
         XC    IOSB(108,@11),IOSB(@11)                             0227
*   IOSASID=ASCBASID;               /* SET ASCB ID                   */
         L     @10,PSAAOLD                                         0228
         MVC   IOSASID(2,@11),ASCBASID(@10)                        0228
*   IOSDVRID=IOSMISID;              /* SET DRIVER ID                 */
         MVI   IOSDVRID(@11),X'01'                                 0229
*   IOSPGAD=ADDR(NP2TERM);          /* SET TERMINATION RTN ADDR      */
         LA    @00,NP2TERM                                         0230
         ST    @00,IOSPGAD(,@11)                                   0230
*   IOSPSLL=ON;                     /* SET LOCAL LOCK NOT WANTED     */
         OI    IOSPSLL(@11),B'00010000'                            0231
*   IOSNRM=ADDR(NP2BR14);           /* SET NORMAL END APPENDAGE      */
         LA    @00,NP2BR14                                         0232
         ST    @00,IOSNRM(,@11)                                    0232
*   IOSABN=ADDR(NP2ABNAP);          /* SET ABNORMAL END APPENDAGE    */
         LA    @00,NP2ABNAP                                        0233
         ST    @00,IOSABN(,@11)                                    0233
*   SRB=SRB&&SRB;                   /* CLEAR SRB                     */
         LA    @10,NP2SRB                                          0234
         XC    SRB(44,@10),SRB(@10)                                0234
*   SRBPARM=ADDR(NP2IOSB);          /* IOSB ADDR TO SRB              */
         ST    @11,SRBPARM(,@10)                                   0235
*/*************************************************************      */
*/* SEARCH FOR VALID UCB AND PASS CONTROL TO NP2DEVCK.               */
*/* RETURN CONTROL TO IEAVNIPM WHEN TABLE IS EXHAUSTED.              */
*/*************************************************************      */
*                                                                  0236
*NP2SRC01:                          /* BEGINNING OF UCB SEARCH       */
*   RSCAN=IECILK2(I);               /* UCB LOOK UP TBL ENTRY         */
*                                                                  0236
NP2SRC01 LH    @11,I                                               0236
         ALR   @11,@11                                             0236
         L     @10,CVTILK2(,RCVT)                                  0236
         BCTR  @10,0                                               0236
         BCTR  @10,0                                               0236
         LH    RSCAN,IECILK2(@11,@10)                              0236
         N     RSCAN,@CF02875                                      0236
*   /*****************************************************************/
*   /*                                                               */
*   /* UCB LOOK UP TABLE IS EXHAUSTED - REPLACE SVC TABLE ENTRY IF   */
*   /* NECESSARY AND RETURN TO IEAVNIPM                              */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0237
*   IF RSCAN=TABLEND THEN                                          0237
         CL    RSCAN,@CF00064                                      0237
         BNE   @RF00237                                            0237
*     DO;                                                          0238
*       IF NP2PURGE=ON THEN         /* WAS PURGE ISSUSED     @YM03866*/
         TM    NP2PURGE,B'00100000'                                0239
         BNO   @RF00239                                            0239
*         SVCENTRY=PGOLDSVC;        /* UPDATE IGC0001F SVC TABLE   0240
*                                      ENTRY                         */
         LH    @11,SVC16                                           0240
         AL    @11,SVCTBPTR(,RNVT)                                 0240
         MVC   SVCENTRY(8,@11),PGOLDSVC                            0240
*       ELSE                                                       0241
*         ;                                                        0241
@RF00239 DS    0H                                                  0242
*       RETURN;                     /* RETURN TO IEAVNIPM            */
@EL00001 L     @13,4(,@13)                                         0242
@EF00001 DS    0H                                                  0242
@ER00001 LM    @14,@12,12(@13)                                     0242
         BR    @14                                                 0242
*     END;                                                         0243
*                                                                  0243
*   /*****************************************************************/
*   /*                                                               */
*   /* VALIDATE UCB LOOK UP TABLE ENTRY                              */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0244
*   ELSE                                                           0244
*     DO;                                                          0244
@RF00237 DS    0H                                                  0245
*       IF RSCAN=ZERO THEN          /* DUMMY UCB                     */
         LTR   RSCAN,RSCAN                                         0245
         BZ    @RT00245                                            0245
*         GO TO NP2SRC03;           /* GO BUMP PTR                   */
*       ELSE                        /* GOOD UCB                      */
*         DO;                                                      0247
*           UCBPTR=RSCAN;           /* RESET UCB PTR                 */
*                                                                  0248
         LR    UCBPTR,RSCAN                                        0248
*           /*********************************************************/
*           /*                                                       */
*           /* DETERMINE IF UCB SHOULD BE TESTED                     */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0249
*           IF UCBTESTD=NOTON THEN  /* UCB NOT TESTED                */
         TM    UCBTESTD(UCBPTR),B'10000000'                        0249
         BNZ   @RF00249                                            0249
*             DO;                                                  0250
*               IF UCBONLI=ON THEN  /* DEVICE GENED ONLINE           */
         TM    UCBONLI(UCBPTR),B'10000000'                         0251
         BNO   @RF00251                                            0251
*                 DO;                                              0252
*                                                                  0252
*                   /*************************************************/
*                   /*                                               */
*                   /* NON DIRECT ACCESS DEVICES                     */
*                   /*                                               */
*                   /*************************************************/
*                                                                  0253
*                   IF UCB3DACC=NOTON THEN                         0253
*                                                                  0253
         TM    UCB3DACC(UCBPTR),B'00100000'                        0253
         BNZ   @RF00253                                            0253
*                     /***********************************************/
*                     /*                                             */
*                     /* IF SECOND OR THIRD ENTRY, GO CHECK NEXT UCB */
*                     /*                                             */
*                     /***********************************************/
*                                                                  0254
*                     IF NPENTRY^=ONE THEN/* ENTRY 1 IS FOR NON-DA 0254
*                                                            @ZM31050*/
*                                                                  0254
         CLI   NPENTRY,1                                           0254
         BNE   @RT00254                                            0254
*                       /*********************************************/
*                       /*                                           */
*                       /* GET NEXT UCB                              */
*                       /*                                           */
*                       /*********************************************/
*                                                                  0255
*                       GO TO NP2SRC03;/* GO BUMP POINTER            */
*                     ELSE                                         0256
*                       ;           /* NO-1ST ENTRY TO PROG          */
*                                                                  0256
*                   /*************************************************/
*                   /*                                               */
*                   /* DIRECT ACCESS DEVICES                         */
*                   /*                                               */
*                   /*************************************************/
*                                                                  0257
*                   ELSE                                           0257
*                                                                  0257
*                     /***********************************************/
*                     /*                                             */
*                     /* IF FIRST ENTRY, GO CHECK NEXT UCB           */
*                     /*                                             */
*                     /***********************************************/
*                                                                  0257
*                     IF UCBRVDEV=NOTON THEN/* IS IT A REAL DEVICE 0257
*                                                            @ZM31050*/
         B     @RC00253                                            0257
@RF00253 TM    UCBRVDEV(UCBPTR),B'00001000'                        0257
         BNZ   @RF00257                                            0257
*                       IF NPENTRY^=TWO THEN/* YES--IF ENTRY 1 OR 3
*                                                            @ZM31050*/
         CLI   NPENTRY,2                                           0258
         BNE   @RT00258                                            0258
*                         GO TO NP2SRC02;/* SKIP AND GET NEXT UCB  0259
*                                                            @ZM31050*/
*                       ELSE                                       0260
*                         ;         /* ENTRY1--PROCESS UCB MZA12345  */
*                     ELSE          /* IT IS A VIRTUAL UCB   @ZM31050*/
*                       IF NPENTRY^=THREE THEN/* IF ENTRY 1 OR 2   0261
*                                                            @ZM31050*/
         B     @RC00257                                            0261
@RF00257 CLI   NPENTRY,3                                           0261
         BNE   @RT00261                                            0261
*                         GO TO NP2SRC02;/* SKIP AND GET NEXT UCB AND
*                                      CHECK SHAREABLE DEV         0262
*                                      @ZM31050,@ZA18117             */
*                                                                  0262
*                   /*************************************************/
*                   /*                                               */
*                   /* GO MAKE AVAILABLE PATH TESTS                  */
*                   /*                                               */
*                   /*************************************************/
*                                                                  0263
*                   CALL NP2DEVCK;                                 0263
@RC00257 DS    0H                                                  0263
@RC00253 BAL   @14,NP2DEVCK                                        0263
*                 END;                                             0264
*               ELSE                                               0265
*                 DO;               /*                       @OZ00078*/
         B     @RC00251                                            0265
@RF00251 DS    0H                                                  0266
*                   UCBVALPH=ON;    /* TURN ON NO PATHS VALIDATED  0266
*                                                            @OZ00078*/
         OI    UCBVALPH(UCBPTR),B'00000010'                        0266
*                   IF UCB3DACC=NOTON THEN/*                 @YM03452*/
         TM    UCB3DACC(UCBPTR),B'00100000'                        0267
         BNZ   @RF00267                                            0267
*                     UCBVORSN=ON;  /* SET VARY OPER REASON  @YM03452*/
         OI    UCBVORSN(UCBPTR),B'00000100'                        0268
*                   ELSE                                           0269
*                     ;                                            0269
@RF00267 DS    0H                                                  0270
*                 END;                                             0270
*             END;                                                 0271
*                                                                  0271
*           /*********************************************************/
*           /*                                                       */
*           /* INDICATE UCB HAS BEEN TESTED                          */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0272
*           ELSE                                                   0272
*             UCBTESTD=NOTON;       /* RESET ALREADY TESTED BIT      */
         B     @RC00249                                            0272
@RF00249 NI    UCBTESTD(UCBPTR),B'01111111'                        0272
*         END;                                                     0273
@RC00249 DS    0H                                                  0274
*     END;                                                         0274
*NP2SRC02:                                                         0275
*                                                                  0275
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK FOR SETTING UCBRR BIT FOR SHAREABLE DEVICE      @OZ00805*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0275
*   RFY                                                            0275
*    (REG5,                                                        0275
*     REG6) RSTD;                   /*                       @OZ00805
*                                                            @OZ00805*/
NP2SRC02 DS    0H                                                  0276
*   GEN REFS(UCBOB,UCBTBYT2,UCBTBYT3,UCBSTAT,UCBSQC,UCBFLP1);      0276
        IOSGEN DETECTOR,UCB=(RSCAN),REG=(REG5,REG6),VAR=2   @OZ00805
*   RFY                                                            0277
*    (REG5,                                                        0277
*     REG6) UNRSTD;                 /*                       @OZ00805*/
*NP2SRC0A:                          /*                       @OZ00805*/
*                                                                  0278
*   /*****************************************************************/
*   /*                                                               */
*   /* 2305 HAS MULTIPLE EXPOSURES                                   */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0278
*   IF UCB3DACC=ON THEN                                            0278
NP2SRC0A TM    UCB3DACC(UCBPTR),B'00100000'                        0278
         BNO   @RF00278                                            0278
*     IF UCBMTPXP=ON THEN                                          0279
         TM    UCBMTPXP(UCBPTR),B'00001000'                        0279
         BNO   @RF00279                                            0279
*       I=I+SEVEN;                                                 0280
*                                                                  0280
         LH    @11,I                                               0280
         LA    @11,7(,@11)                                         0280
         STH   @11,I                                               0280
*   /*****************************************************************/
*   /*                                                               */
*   /* NOT MULTIPLE EXPOSURE DEVICE                                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0281
*     ELSE                                                         0281
*       ;                                                          0281
*                                                                  0281
@RF00279 DS    0H                                                  0282
*   /*****************************************************************/
*   /*                                                               */
*   /* NO A DIRECT ACCESS DEVICE                                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0282
*   ELSE                                                           0282
*     ;                                                            0282
@RF00278 DS    0H                                                  0283
*NP2SRC03:                          /* POINT TO NEXT UCB             */
*                                                                  0283
*   /*****************************************************************/
*   /*                                                               */
*   /* BUMP POINTER TO NEXT TABLE ENTRY                              */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0283
*   I=I+ONE;                        /* GET NEXT UCB ADDR             */
NP2SRC03 LH    @11,I                                               0283
         LA    @11,1(,@11)                                         0283
         STH   @11,I                                               0283
*   GO TO NP2SRC01;                 /* GO INSPECT NEXT UCB           */
         B     NP2SRC01                                            0284
*                                                                  0285
*/********************************************************************/
*/*                                                                  */
*/*  THIS ROUTINE USES THE IOSGEN MACRO TO COMPLETE THE              */
*/*  PATHMAP IN THE UCB.                                             */
*/*                                                                  */
*/*  PREPARE TO ISSUE I/O DOWN ALL PATHS INDICATED AS AVAILABLE      */
*/*  IN THE IOSGEN MAP TABLE.  I/O ISSUED SEQUENTIALLY AND USES      */
*/*  SHOULDER TAP TO TEST PATHS IN ANOTHER CPU.                      */
*/*                                                                  */
*/*  SET DEVICE NOT READY FOR ALL DEVICES WHICH ARE NOT READY.       */
*/*  SET ALL SUB-UCBS FOR MULTIPLE EXPOSURE DEVICES.                 */
*/*                                                                  */
*/********************************************************************/
*                                                                  0285
*NP2DEVCK:                          /* DEVICE CHECK SUBROUTINE       */
*   PROC OPTIONS(NOSAVE);           /* PROCEDURE                     */
NP2DEVCK DS    0H                                                  0286
*   RFY                                                            0286
*    (REG10) RSTD;                                                 0286
*   IOSUCB=RSCAN;                   /* SET UCB ADDR                  */
         LA    @11,NP2IOSB                                         0287
         ST    RSCAN,IOSUCB(,@11)                                  0287
*   REG10=RSAVE;                    /* SAVE SAVEAREA PTR             */
         LR    REG10,RSAVE                                         0288
*   RSAVE=ADDR(IOSGSAVE);           /* GET IOSGEN SAVEAREA PTR       */
*                                                                  0289
         LA    RSAVE,IOSGSAVE                                      0289
*   /*****************************************************************/
*   /*                                                               */
*   /* ISSUE IOSGEN TO ACQUIRE DEVICE PATH MAP                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0290
*   GEN REFS(UCBBGN,UCBFL5,UCBUA,UCBLCI,UCBALTPH,CVTILCH,CVTPCCAT, 0290
*       CVTCSD,PCCACPUA,PCCACAFM,PCCACAT,CSDCPUAL,CATFLG,LCHTCH,UCBBASE
*       ,UCBOB)(IOSGEN MAP,TABLE=PATHMAP,UCB=RSCAN,VAR=1);         0290
         IOSGEN MAP,TABLE=PATHMAP,UCB=RSCAN,VAR=1
*   RSAVE=REG10;                    /* RESTORE SAVEAREA REG          */
         LR    RSAVE,REG10                                         0291
*   RFY                                                            0292
*    (REG10) UNRSTD;                                               0292
*                                                                  0292
*   /*****************************************************************/
*   /*                                                               */
*   /* DETERMINE IF ANY PATHS ARE AVAILABLE                          */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0293
*   IF PRIME^=BOTHOFF×SECOND^=BOTHOFF THEN/* YES- PATHS ARE AVAILABLE*/
         CLC   PRIME(2),@CB00086                                   0293
         BNE   @RT00293                                            0293
         CLC   SECOND(2),@CB00086                                  0293
         BE    @RF00293                                            0293
@RT00293 DS    0H                                                  0294
*     DO;                                                          0294
*       IOSPATH=PRADDR;             /* SET PRIMARY PATH              */
*                                                                  0295
         LA    @11,NP2IOSB                                         0295
         LH    @00,PRADDR                                          0295
         STH   @00,IOSPATH(,@11)                                   0295
*       /*************************************************************/
*       /*                                                           */
*       /* TEST CPU 0 PRIMARY PATH                                   */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0296
*       IF PRCPUA=PATHON THEN       /* CPU 0 PRIMARY IS AVAILABLE    */
         CLI   PRCPUA,X'01'                                        0296
         BNE   @RF00296                                            0296
*         DO;                                                      0297
*           IOSAFF=CPUID00;         /* I/O THRU CPU 0                */
*                                                                  0298
         MVI   IOSAFF(@11),X'00'                                   0298
*           /*********************************************************/
*           /*                                                       */
*           /* GO ISSUE I/O                                          */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0299
*           CALL NP2IORTN;          /* CALL I/O ROUTINE              */
         BAL   @14,NP2IORTN                                        0299
*         END;                                                     0300
*       ELSE                        /* CPU 0 PRIMARY IS NOT AVAIL    */
*         UCBPPA=ON;                /* SET PRIMARY, CPU 0 OFFLINE    */
*                                                                  0301
         B     @RC00296                                            0301
@RF00296 OI    UCBPPA(UCBPTR),B'10000000'                          0301
*       /*************************************************************/
*       /*                                                           */
*       /* TEST CPU 1 PRIMARY PATH                                   */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0302
*       IF PRCPUB=PATHON THEN       /* CPU 1 PRIMARY IS AVAILABLE    */
@RC00296 CLI   PRCPUB,X'01'                                        0302
         BNE   @RF00302                                            0302
*         DO;                                                      0303
*           IOSAFF=CPUID01;         /* I/0 THRU CPU 1                */
*                                                                  0304
         LA    @11,NP2IOSB                                         0304
         MVI   IOSAFF(@11),X'01'                                   0304
*           /*********************************************************/
*           /*                                                       */
*           /* GO ISSUE I/O                                          */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0305
*           CALL NP2IORTN;          /* CALL I/O ROUTINE              */
         BAL   @14,NP2IORTN                                        0305
*         END;                                                     0306
*       ELSE                        /* CPU 1 PRIMARY IS NOT AVAIL    */
*         UCBPPB=ON;                /* SET PRIMARY, CPU 1 OFFLINE    */
*                                                                  0307
         B     @RC00302                                            0307
@RF00302 OI    UCBPPB(UCBPTR),B'00100000'                          0307
*       /*************************************************************/
*       /*                                                           */
*       /* TEST FOR ALTERNATE PATH                                   */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0308
*       IF UCBALTPH=ON THEN         /* THERE IS AN ALTERNATE PATH    */
@RC00302 TM    UCBALTPH(UCBPTR),B'00000001'                        0308
         BNO   @RF00308                                            0308
*         DO;                                                      0309
*           IOSPATH=SECADDR;        /* SET SECONDARY PATH            */
*                                                                  0310
         LA    @11,NP2IOSB                                         0310
         LH    @00,SECADDR                                         0310
         STH   @00,IOSPATH(,@11)                                   0310
*           /*********************************************************/
*           /*                                                       */
*           /* TEST CPU 0 SECONDARY PATH                             */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0311
*           IF SECPUA=PATHON THEN   /* CPU 0 SECONDARY IS AVAIL      */
         CLI   SECPUA,X'01'                                        0311
         BNE   @RF00311                                            0311
*             DO;                                                  0312
*               IOSAFF=CPUID00;     /* I/O THRU CPU 0                */
*                                                                  0313
         MVI   IOSAFF(@11),X'00'                                   0313
*               /*****************************************************/
*               /*                                                   */
*               /* GO ISSUE I/O                                      */
*               /*                                                   */
*               /*****************************************************/
*                                                                  0314
*               CALL NP2IORTN;      /* CALL I/O ROUTINE              */
         BAL   @14,NP2IORTN                                        0314
*             END;                                                 0315
*           ELSE                    /* CPU 0 SECONDARY IS NOT AVAIL  */
*             UCBSPA=ON;            /* SET SECONDARY, CPU 0 OFFLINE  */
*                                                                  0316
         B     @RC00311                                            0316
@RF00311 OI    UCBSPA(UCBPTR),B'01000000'                          0316
*           /*********************************************************/
*           /*                                                       */
*           /* TEST CPU 1 SECONDARY PATH                             */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0317
*           IF SECPUB=PATHON THEN   /* CPU 1 SECONDARY IS AVAIL      */
@RC00311 CLI   SECPUB,X'01'                                        0317
         BNE   @RF00317                                            0317
*             DO;                                                  0318
*               IOSAFF=CPUID01;     /* I/O THRU CPU 1                */
