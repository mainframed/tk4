         TITLE 'IECVCINT - CHANNEL RECONFIGURATION HARDWARE MODULE     *
                        '
*                                                                  0141
IECVCINT   CSECT
     ENTRY IECVCRHA                    DEFINE
     ENTRY IECVCRHD                        ENTRY
     ENTRY IECVCRHT                            POINTS
     ENTRY IECVCRHV                                FOR
     ENTRY IECCRHAF                                  ROUTINES
     ENTRY IECCRHDF
     ENTRY IECCINTF
     ENTRY IECVCRHS
     ENTRY IECCRHTF
     LR    BASE,CINTENT                LOAD BASE REGISTER
     USING IECVCINT,BASE               INDICATE ADRESSABILITY
*/********************************************************************/
*/* IF CRH NOT ACTIVE PASS INTERRUPT ON TO IOS SLIH WHICH PROCESSES  */
*/* THE INTERRUPT AND THEN RETURNS DIRECTLY TO I/O FLIH.             */
*/********************************************************************/
*    IF CVTCRCA=ZERO                   /* IS CRH ACTIVE ?            */
*       THEN DO;                       /*NO, PASS INTERRUPT TO SLIH  */
         L     @10,CVTPTR                                          0142
         ICM   @03,15,CVTCRCA(@10)                                 0142
         BNZ   @RF00142                                            0142
*            CINTENT=ADDR(IECINT);     /* GET ADDRESS OF IOS SLIH    */
         L     @10,IOCOMPTR(,@10)                                  0144
         L     CINTENT,IOCINT(,@10)                                0144
*            GEN( BR CINTENT);         /* CALL IOS SLIH - RETURN   0145
*                                         REGISTER 4 IS PASSED TO IOS
*                                         SLIH THEREFOR IECINT RETURNS
*                                         DIRECTLY TO THE I/O FLIH   */
          BR CINTENT
*            END;                                                  0146
*/********************************************************************/
*/*  PROVIDE FRR COVERAGE                                            */
*/********************************************************************/
*    RFY (WRK1,WRK3) RSTD;                                         0147
@RF00142 DS    0H                                                  0148
*    IF SETFRRC=OFF                    /* ALREADY COVERED BY FRR ?   */
*       THEN DO;                                                   0148
         TM    SETFRRC,B'00100000'                                 0148
         BNZ   @RF00148                                            0148
*            GEN CODE(SETFRR A,FRRAD=CINTF,WRKREGS=(WRK1,WRK3))    0150
*                     SETS(WRK1,WRK3) REFS(CINTF); /* SET FRR        */
         SETFRR A,FRRAD=CINTF,WRKREGS=(WRK1,WRK3)
*            SETFRRC=ON;               /* INDICATE FRR COVERAGE      */
         OI    SETFRRC,B'00100000'                                 0151
*            END;                                                  0152
*    RFY (WRK1,WRK3) UNRSTD;                                       0153
@RF00148 DS    0H                                                  0154
*/********************************************************************/
*/*       DO SOME INITITALIZATION                                    */
*/********************************************************************/
*    SAVERET=CINTRET;                  /* SAVE RETURN REGISTER 4     */
         ST    CINTRET,SAVERET                                     0154
*    CRCAPTR=CVTCRCA;                  /* SET UP CRCA BASE REGISTER  */
         L     @10,CVTPTR                                          0155
         L     CRCAPTR,CVTCRCA(,@10)                               0155
*    CRCADINT=OFF;                     /* INITIALIZE FLAG TO OFF     */
         NI    CRCADINT(CRCAPTR),B'10111111'                       0156
*    GEN CODE REFS(CRCAPTR) SETS(CRCAITIM);                        0157
     STCK  CRCAITIM(CRCAPTR)           SAVE TIME OF THIS INTERRUPT
*/********************************************************************/
*/* CHECK WHETHER INTERRUPT WAS SOLICITED ACROSS CRH CONNECTION      */
*/********************************************************************/
*    IF CRCADIAG=ON                    /* CRH CONNECTION OUTSTANDING?*/
*       THEN DO;                       /* YES-INTERRUPT ACROSS CRH   */
         TM    CRCADIAG(CRCAPTR),B'10000000'                       0158
         BNO   @RF00158                                            0158
*            BUILDIOA=FLCIOAA;         /* UPDATE I/O INTERRUPT       */
         MVC   BUILDIOA(3),FLCIOAA                                 0160
*            IOAA(1:1)=CRCACHAN(1:1);  /* ADDRESS IN FIXED LOW CORE  */
         MVC   IOAA(1),CRCACHAN(CRCAPTR)                           0161
*            FLCIOAA=BUILDIOA;         /* TO REFLECT ACTUAL CHANNEL  */
         MVC   FLCIOAA(3),BUILDIOA                                 0162
*            CATINDEX=IOAA(1:1);       /* GET CHANNEL ADDRESS        */
         SLR   CATINDEX,CATINDEX                                   0163
         IC    CATINDEX,IOAA                                       0163
*            SLL(CATINDEX,CATELP2);    /*SHIFT TO MULTIPLY BY LENGTH */
         LA    @10,3                                               0164
         SLL   CATINDEX,0(@10)                                     0164
*            CATPTR=CRCACAT+CATINDEX;  /* GET POINTER TO CAT ENTRY   */
         LR    CATPTR,CATINDEX                                     0165
         AL    CATPTR,CRCACAT(,CRCAPTR)                            0165
*            CATBSY=OFF;               /* MAKE SURE CHANNEL NOT BUSY */
         NI    CATBSY(CATPTR),B'01111111'                          0166
*            END;                                                  0167
*/********************************************************************/
*/*                                                                  */
*/*         FIND UCB FOR DEVICE WHICH CAUSED INTERRUPT               */
*/*                                                                  */
*/********************************************************************/
*                                                                  0168
*/********************************************************************/
*/*  VALIDITY CHECK THE INTERRUPT ADDRESS                            */
*/********************************************************************/
*    RFY (WRK0,WRK1,WRK3,WRK5) RSTD;                               0168
@RF00158 DS    0H                                                  0169
*    WRK0=FLCIOA;                      /* GET INTERRUPT ADDRESS      */
         L     WRK0,FLCIOA                                         0169
*    GEN CODE(  SRDL WRK0,3 );         /* SHIFT 3 LOW ORDER BITS INTO
*                                         NEXT REGISTER              */
           SRDL WRK0,3
*    WRK3=(IOCUCBBM+WRK0)->BITMASK;    /* PICK UP UCB BIT MASK USING
*                                         CONTENTS OF WRK0 AS INDEX  */
         L     @10,CVTPTR                                          0171
         L     @10,IOCOMPTR(,@10)                                  0171
         LR    @09,WRK0                                            0171
         AL    @09,IOCUCBBM(,@10)                                  0171
         SLR   WRK3,WRK3                                           0171
         IC    WRK3,BITMASK(,@09)                                  0171
*    GEN CODE( SRL WRK1,29);           /* SHIFT 3 BITS TO LOW END    */
          SRL WRK1,29
*    WRK5=X80;                         /* GET BIT READY POSITION     */
         L     WRK5,X80                                            0173
*    GEN CODE( SRL WRK5,0(WRK1));      /* SHIFT BIT INTO AND POSITION*/
          SRL WRK5,0(WRK1)
*    WRK5=WRK5&WRK3;                   /* CHECK IF BIT ON IN MASK    */
         NR    WRK5,WRK3                                           0175
*    BC( SEVEN,VCINT100);              /* BRANCH IF VALID DEVICE     */
         BC    7,VCINT100                                          0176
*/********************************************************************/
*/*  INVALID DEVICE - ZERO UCB POINTER                               */
*/********************************************************************/
*    UCBPTR=ZERO;                      /* ZERO UCB POINTER           */
         SLR   UCBPTR,UCBPTR                                       0177
*    GOTO VCINT200;                    /* GO CHECK IF CALLED BY HIO  */
         B     VCINT200                                            0178
*/********************************************************************/
*/*  VALID DEVICE - DO UCB LOOK UP                                   */
*/********************************************************************/
*VCINT100: ;                                                       0179
VCINT100 DS    0H                                                  0180
*    GEN CODE( SRDL WRK0,5);           /* SHIFT CU TO NEXT REGISTER  */
          SRDL WRK0,5
*    GEN CODE( SRL  WRK1,28);          /* SHIFT TO LOW ORDER END AND
*                                         DROP LEFT OVER DEVICE BIT  */
          SRL  WRK1,28
*    WRK3=(CVTILK1+WRK0)->CHANCON;     /* GET CHANNEL CONSTANT       */
         L     @09,CVTPTR                                          0182
         L     @08,CVTILK1(,@09)                                   0182
         LR    @07,WRK0                                            0182
         ALR   @07,@08                                             0182
         SLR   WRK3,WRK3                                           0182
         IC    WRK3,CHANCON(,@07)                                  0182
*    WRK1=WRK3+WRK1;                   /* GET TWO TIMES CONTROL UNIT */
         ALR   WRK1,WRK3                                           0183
*    WRK1=WRK1+WRK1;                   /* PLUS CHANNEL CONST @ZA08721*/
         ALR   WRK1,WRK1                                           0184
*    WRK3=FLCIOA;                      /* GET INTERRUPT ADDRESS      */
         L     WRK3,FLCIOA                                         0185
*    WRK5=WRK3&ONEFOX;                 /* ISOLATE DEVICE ADDRESS     */
         LR    WRK5,WRK3                                           0186
         N     WRK5,ONEFOX                                         0186
*    WRK3=(CVTILK1+WRK1)->CUCON;       /* GET CU CONSTANT            */
         ALR   @08,WRK1                                            0187
         SLR   WRK3,WRK3                                           0187
         ICM   WRK3,3,CUCON(@08)                                   0187
*    WRK3=WRK3+WRK5;                   /* ADD TO DEVICE NUMBER       */
         ALR   WRK3,WRK5                                           0188
*    WRK3=WRK3+WRK3;                   /* DOUBLE SUM                 */
         ALR   WRK3,WRK3                                           0189
*    UCBPTR=(CVTILK2+WRK3)->UCBADDR;   /* GET ADDRESS OF UCB         */
         LR    @08,WRK3                                            0190
         AL    @08,CVTILK2(,@09)                                   0190
         SLR   UCBPTR,UCBPTR                                       0190
         ICM   UCBPTR,3,UCBADDR(@08)                               0190
*/********************************************************************/
*/*         SET UP ACTIVE IOQ FOR FRR                                */
*/********************************************************************/
*    IRTIOQ=UCBIOQ;                    /* PUT ACTIVE IOQ IN IRT      */
         LA    @09,736                                             0191
         AL    @09,LCCAPTR                                         0191
         LR    @08,UCBPTR                                          0191
         AL    @08,@CF03026                                        0191
         MVC   IRTIOQ(4,@09),UCBIOQ-508(@08)                       0191
*    IRTIOQA=ON;                       /* INDICATE ACTIVE IOQ        */
         OI    IRTIOQA(@09),B'00000100'                            0192
*/********************************************************************/
*/********************************************************************/
*/*                                                                  */
*/*      IF CALLED BY HIO PASS INTERRUPT TO HIO ENTRY OF IOS SLIH    */
*/*                                                                  */
*/********************************************************************/
*VCINT200: ;                                                       0193
VCINT200 DS    0H                                                  0194
*    IF CRCAHIO=ON                     /* HIO ENTRY ?                */
*       THEN DO;                       /* YES - PROCESS HIO INTERRUPT*/
         TM    CRCAHIO(CRCAPTR),B'00010000'                        0194
         BNO   @RF00194                                            0194
*            CRCAMCWM=OFF;             /* SET UP MCW TO BREAK CRH  0196
*                                         CONNECTION                 */
         NI    CRCAMCWM(CRCAPTR),B'11101111'                       0196
*            GEN CODE REFS(CRCAMCW,CRCAPTR,CRCA);                  0197
             USING CRCA,CRCAPTR        ESTABLISH ADDRESSABILITY
             DC X'8300',S(CRCAMCW)     ISSUE DIAGNOSE
             DROP CRCAPTR
*            CRCADIAG=OFF;             /* INDICATE NO CRH CONNECTION */
         NI    CRCADIAG(CRCAPTR),B'01111111'                       0198
*            IF UCBPTR=0               /* CHECK IF VALID DEVICE      */
*               THEN CRCADINT=ON;      /* NO- INVALID CRH INTERRUPT  */
         LTR   UCBPTR,UCBPTR                                       0199
         BNZ   @RF00199                                            0199
         OI    CRCADINT(CRCAPTR),B'01000000'                       0200
*               ELSE UCBIORST=ON;      /* YES-INDICATE CRH INTERRUPT */
         B     @RC00199                                            0201
@RF00199 OI    UCBIORST(UCBPTR),B'10000000'                        0201
*            CRCAHIO=OFF;              /* TURN OFF HIO ENTRY FLAG    */
@RC00199 NI    CRCAHIO(CRCAPTR),B'11101111'                        0202
*            GEN (SETFRR D,WRKREGS=(WRK1,WRK3));  /* DELETE FRR      */
         SETFRR D,WRKREGS=(WRK1,WRK3)
*            SETFRRC=OFF;              /* INDICATE NO FRR            */
         NI    SETFRRC,B'11011111'                                 0204
*            CINTENT=ADDR(IECINT);     /* GET ADDRESS OF IOS SLIH  0205
*                                         HIO ENTRY          @ZA27164*/
         L     @09,CVTPTR                                          0205
         L     @09,IOCOMPTR(,@09)                                  0205
         L     CINTENT,IOCINT(,@09)                                0205
*/********************************************************************/
*/*  NOTE: REGISTER 11 CONTAINS AN ENVIRONMENT CODE WHICH MUST BE    */
*/*        MUST BE PASSED TO HIO ENTRY OF SLIH.                      */
*/********************************************************************/
*            GEN( B 4(CINTENT));       /* CALL IOS SLIH - RETURN   0206
*                                         REGISTER 4 IS PASSED TO IOS
*                                         SLIH THEREFOR IECINT RETURNS
*                                         DIRECTLY TO HIO    @ZA27164*/
          B 4(CINTENT)
*            END;                                                  0207
*    RFY (WRK1,WRK3) UNRSTD;                                       0208
@RF00194 DS    0H                                                  0209
*/********************************************************************/
*/*                                                                  */
*/*        NORMAL ENTRY - PROCESS INTERRUPT AND PASS TO IOS SLIH     */
*/*                                                                  */
*/********************************************************************/
*                                                                  0209
*    IF CRCADIAG=ON                    /* CRH CONNECTION OUTSTANDING?*/
*/********************************************************************/
*/*          INTERRUPT WAS SOLICITED ACROSS CRH CONNECTION           */
*/********************************************************************/
*       THEN DO;                       /* YES-INTERRUPT ACROSS CRH   */
         TM    CRCADIAG(CRCAPTR),B'10000000'                       0209
         BNO   @RF00209                                            0209
*            CRCAMCWM=OFF;             /* SET UP MCW TO BREAK CRH  0211
*                                         CONNECTION                 */
         NI    CRCAMCWM(CRCAPTR),B'11101111'                       0211
*            GEN CODE REFS(CRCAMCW,CRCAPTR,CRCA);                  0212
             USING CRCA,CRCAPTR        ESTABLISH ADDRESSABILITY
             DC X'8300',S(CRCAMCW)     ISSUE DIAGNOSE
             DROP CRCAPTR
*            CRCADIAG=OFF;             /* INDICATE NO CRH CONNECTION */
         NI    CRCADIAG(CRCAPTR),B'01111111'                       0213
*            GEN CODE( LCTL CTL2,CTL2,SAVECTL2 );  /* RESTORE SYSTEM
*                                                     CONTROL REG2   */
          LCTL CTL2,CTL2,SAVECTL2
*            IF UCBPTR^=0              /* CHECK IF VALID DEVICE      */
*               THEN DO;               /* VALID DEVICE               */
         LTR   UCBPTR,UCBPTR                                       0215
         BZ    @RF00215                                            0215
*                    IF UCK=ON×        /* WILL SENSE BE REQUIRED ?   */
*                       CDC=ON×                                    0217
*                       CCC=ON×                                    0217
*                       ICC=ON                                     0217
*                       THEN UCBCRHSN=ON; /* YES, INDICATE IN UCB    */
         TM    UCK,B'00000010'                                     0217
         BO    @RT00217                                            0217
         TM    CDC,B'00001110'                                     0217
         BZ    @RF00217                                            0217
@RT00217 DS    0H                                                  0218
         OI    UCBCRHSN(UCBPTR),B'00000100'                        0218
*                       ELSE;                                      0219
@RF00217 DS    0H                                                  0220
*                    IF (UCBBSY=ON) &  /* EXPECTED I/O INTERRUPT ?   */
*                       (UCBIORST=OFF) /*BUT NOT STARTED ACROSS CRH ?*/
*                       THEN IF CDC=ON×  /* YES, CHANNEL DATA CHECK? */
         TM    UCBBSY(UCBPTR),B'10000000'                          0220
         BNO   @RF00220                                            0220
         TM    UCBIORST(UCBPTR),B'10000000'                        0220
         BNZ   @RF00220                                            0220
*                               CCC=ON×  /* CHANNEL CONTROL CHECK ?  */
*                               ICC=ON   /* INTERFACE CHANNEL CHECK ?*/
*                               THEN CRCACCH=ON; /*INDICATE ERRORS ON
*                                                  INTERRUPT TO IOS  */
         TM    CDC,B'00001110'                                     0221
         BZ    @RF00221                                            0221
         OI    CRCACCH(CRCAPTR),B'00100000'                        0222
*                               ELSE;  /*                    @ZA16176*/
@RF00221 DS    0H                                                  0224
*                       ELSE;                                      0224
@RF00220 DS    0H                                                  0225
*                    END;              /* END VALID DEVICE           */
*               ELSE CRCADINT=ON;      /* INDICATE INVALID CRH INT.  */
         B     @RC00215                                            0226
@RF00215 OI    CRCADINT(CRCAPTR),B'01000000'                       0226
*            ENVCODE=CRHINT;           /* SET UP REG 11 FOR CRH    0227
*                                         INTERRUPT          @ZA16177*/
@RC00215 L     ENVCODE,CRHINT                                      0227
*            END;                      /* END INTERRUPT ACROSS CRH   */
*/********************************************************************/
*/*          INTERRUPT WAS NOT SOLICITED ACROSS CRH CONNECTION       */
*/********************************************************************/
*       ELSE DO;                       /* INTERRUPT NOT ACROSS CRH   */
         B     @RC00209                                            0229
@RF00209 DS    0H                                                  0230
*            GEN CODE(STCTL CTL2,CTL2,SAVECTL2 ); /* SAVE SYSTEM   0230
*                                                    CONTROL REG2    */
         STCTL CTL2,CTL2,SAVECTL2
*            IF UCBPTR^=0              /* CHECK IF VALID DEVICE      */
*               THEN DO;               /* VALID DEVICE               */
         LTR   UCBPTR,UCBPTR                                       0231
         BZ    @RF00231                                            0231
*                    IF UCK=ON×        /* WILL SENSE BE REQUIRED ?   */
*                       CDC=ON×                                    0233
*                       CCC=ON×                                    0233
*                       ICC=ON                                     0233
*                       THEN UCBCRHSN=OFF; /* ENSURE FLAG OFF        */
         TM    UCK,B'00000010'                                     0233
         BO    @RT00233                                            0233
         TM    CDC,B'00001110'                                     0233
         BZ    @RF00233                                            0233
@RT00233 DS    0H                                                  0234
         NI    UCBCRHSN(UCBPTR),B'11111011'                        0234
*                       ELSE;                                      0235
@RF00233 DS    0H                                                  0236
*                                      /* 8 LINES DELETED    @ZA16176*/
*                    END;              /* END VALID DEVICE           */
*               ELSE;                  /*INVALID DEVICE-PASS TO SLIH */
@RF00231 DS    0H                                                  0238
*            ENVCODE=NORMINT;          /* SET UP REG 11 FOR NORMAL 0238
*                                         INTERRUPT          @ZA16177*/
         L     ENVCODE,NORMINT                                     0238
*            END;                      /* END NOT CRH INTERRUPT      */
*                                                                  0239
*/********************************************************************/
*/*  PASS I/O INTERRUPT TO IOS SLIH IECINT VIA BALR 4,5              */
*/********************************************************************/
*    CINTENT=ADDR(IECINT);             /* GET ADDRESS OF IOS SLIH (REG
*                                         11 ALREADY SET UP) @ZA27164*/
@RC00209 L     @10,CVTPTR                                          0240
         L     @01,IOCOMPTR(,@10)                                  0240
         L     CINTENT,IOCINT(,@01)                                0240
*    GEN CODE ( BAL CINTRET,4(CINTENT) )                           0241
*              REFS( CINTRET,CINTENT); /* CALL I/O SLIH      @ZA27164*/
          BAL CINTRET,4(CINTENT)
*    CRCAPTR=CVTCRCA;                  /* RESTORE POINTER TO CRCA    */
         L     @10,CVTPTR                                          0242
         L     CRCAPTR,CVTCRCA(,@10)                               0242
*    BASE=CRCACINT;                    /* RESTORE BASE REGISTER      */
         L     BASE,CRCACINT(,CRCAPTR)                             0243
*/********************************************************************/
*/*                                                                  */
*/*     POLL ALL CHANNELS ON DEAD CPU THRU CRH BY SELECTIVELY        */
*/*     ENABLING FOR INTERRUPTS FROM EACH CHANNEL.                   */
*/*                                                                  */
*/********************************************************************/
*                                                                  0244
*    POLLCHAN=ZERO;                    /* BEGIN POLLING WITH CHAN. 0 */
         MVI   POLLCHAN,X'00'                                      0244
*    GEN CODE ( LCTL  CTL2,CTL2,ENABLE6 )                          0245
*    REFS(ENABLE6) SETS(CTL2);        /* ENABLE INTERRUPTS ONLY    0245
*                                         ACROSS CHANNEL 6           */
          LCTL  CTL2,CTL2,ENABLE6
*/********************************************************************/
*/*  INDEX THRU ALL VALID CHANNELS ON THIS CPU                       */
*/********************************************************************/
*    DO WHILE POLLCHAN<=IOXHICH;       /* HIGHEST CHANNEL SYSGENED ? */
         B     @DE00246                                            0246
@DL00246 DS    0H                                                  0247
*       CATPTR=CRCACAT+(POLLCHAN*EIGHT); /* INDEX TO CAT           0247
*                                                    ENTRY FOR CHAN. */
         SLR   CATPTR,CATPTR                                       0247
         IC    CATPTR,POLLCHAN                                     0247
         SLA   CATPTR,3                                            0247
         AL    CATPTR,CRCACAT(,CRCAPTR)                            0247
*       IF CATNGEN=ON×                 /* CHANNEL NOT SYSGENED ?     */
*          CATNCPU=ON                  /* CHANNEL NOT ON DEAD CPU ?  */
*          THEN;                       /* YES - BUMP TO NEXT CHANNEL */
         TM    CATNGEN(CATPTR),B'00110000'                         0248
         BNZ   @RT00248                                            0248
*          ELSE DO;                    /* CHANNEL VALID - POLL CHAN. */
*               /*****************************************************/
*               /*  MAKE CRH CONNECTION TO CHANNEL ON DEAD CPU       */
*               /*****************************************************/
*               CRCAMCWF=POLLCHAN;     /* SET UP MCW TO USE THIS CHAN*/
         MVC   CRCAMCWF(1,CRCAPTR),POLLCHAN                        0251
*               CRCACHAN=POLLADDR;     /* SAVE REMOTE CHANNEL ADDRESS*/
         MVC   CRCACHAN(2,CRCAPTR),POLLADDR                        0252
*               CRCAMCWI=ON;           /* INDICATE MCW FOR CRH       */
*               CRCAMCWM=ON;           /* INDICATE MAKE CONNECTION   */
         OI    CRCAMCWI(CRCAPTR),B'00110000'                       0254
*               CRCADIAG=ON;           /* INDICATE CRH CONNECTION  0255
*                                         OUTSTANDING                */
         OI    CRCADIAG(CRCAPTR),B'10000000'                       0255
*               GEN CODE REFS(CRCAMCW,CRCAPTR,CRCA);               0256
                USING CRCA,CRCAPTR        ESTABLISH ADDRESSABILITY
                DC X'8300',S(CRCAMCW)     ISSUE DIAGNOSE
                DROP CRCAPTR
*               /*****************************************************/
*               /*  SOLICIT AN I/O INTERRUPT ACROSS CRH CONNECTION   */
*               /*****************************************************/
*               GEN CODE REFS(SAVEMASK,ENABLEIO,DISABLE);          0257
                STOSM SAVEMASK,ENABLEIO       ENABLE
                STNSM SAVEMASK,DISABLE        DISABLE
*               END;                                               0258
*       POLLCHAN=POLLCHAN+ONE;         /* BUMP TO NEXT CHANNEL NUMBER*/
@RT00248 IC    @01,POLLCHAN                                        0259
         LA    @01,1(,@01)                                         0259
         STC   @01,POLLCHAN                                        0259
*    END;                                                          0260
@DE00246 L     @10,CVTPTR                                          0260
         L     @01,IOCOMPTR(,@10)                                  0260
         L     @01,IOCXPTR(,@01)                                   0260
         CLC   POLLCHAN(1),IOXHICH(@01)                            0260
         BNH   @DL00246                                            0260
*/********************************************************************/
*/*                                                                  */
*/*                   EXIT ROUTINE                                   */
*/*                                                                  */
*/********************************************************************/
*    IF CRCADIAG=ON                    /* CRH CONNECTION OUTSTANDING?*/
*       THEN DO;                       /* YES                        */
         TM    CRCADIAG(CRCAPTR),B'10000000'                       0261
         BNO   @RF00261                                            0261
*            CRCAMCWM=OFF;             /* INDICATE BREAK CONNECTION  */
         NI    CRCAMCWM(CRCAPTR),B'11101111'                       0263
*            GEN CODE REFS(CRCAMCW,CRCAPTR,CRCA,SAVECTL2) SETS(CTL2);
             LCTL CTL2,CTL2,SAVECTL2      RESTORE SYSTEM CONTROL REG2
             USING CRCA,CRCAPTR           ESTABLISH ADDRESSABILITY
             DC X'8300',S(CRCAMCW)        ISSUE DIAGNOSE
             DROP CRCAPTR
*            CRCADIAG=OFF;             /* INDICATE NO CRH CONNECTION */
         NI    CRCADIAG(CRCAPTR),B'01111111'                       0265
*            END;                                                  0266
*    RFY (WRK1,WRK3) RSTD;                                         0267
@RF00261 DS    0H                                                  0268
*    GEN CODE (SETFRR D,WRKREGS=(WRK1,WRK3))                       0268
*             SETS(WRK1,WRK3);         /* DELETE FRR                 */
         SETFRR D,WRKREGS=(WRK1,WRK3)
*    SETFRRC=OFF;                      /* INDICATE NO FRR            */
         NI    SETFRRC,B'11011111'                                 0269
*    CINTRET=SAVERET;                  /* RESTORE RETURN REGISTER 4  */
         L     CINTRET,SAVERET                                     0270
*    GEN CODE REFS(CINTRET);                                       0271
     BR  CINTRET                       RETURN TO CALLER
*    RFY (CINTRET,CINTENT,WRK1,WRK3) UNRSTD;                       0272
*/*********************************************************************
**                                                                 0273
** ENTRY NAME - IECVCRHA                                           0273
**                                                                 0273
** DESCRIPTIVE NAME - CRH ACTIVATION ROUTINE.                      0273
**                                                                 0273
** FUNCTION - THIS ENTRY WILL PERFORM THE INITIALIZATION REQUIRED TO
**            ALLOW THE OS/VS2 SYSTEM TO USE THE CHANNEL           0273
**            RECONFIGURATION HARDWARE IN ORDER TO ACCESS A DEVICE 0273
**            ON A DEAD CPU FROM THE CPU WHICH IS STILL RUNNING IN 0273
**            A 168MP ENVIRONMENT.                                 0273
**                                                                 0273
**  LINKAGE - STANDARD LINKAGE IS USED ON ENTRY AND EXIT           0273
**            NON-STANDARD LINKAGE IS USED FOR CALL TO IECVSMGR    0273
**                                                                 0273
**  INPUT                                                          0273
**      REGISTERS - SAVEPTR  (REGISTER 13) POINTS TO 18 WORD SAVE AREA
**                  RETRNREG (REGISTER 14) ADDRESS OF RETURN POINT 0273
**                  ENTRYREG (REGISTER 15) ADDRESS OF IECVCRHA     0273
**                                                                 0273
**  OUTPUT                                                         0273
**      REGISTERS - RETCODE  (REGISTER 15) SEE EXIT                0273
**                                                                 0273
**   EXIT - NORMAL - RETURN TO CALLER WITH RETURN CODE OF 0        0273
**                                                                 0273
**   EXIT - ERROR - RETURN TO CALLER WITH RETURN CODE              0273
**                  RC=4  CRH ALREADY ACTIVE                       0273
**                  RC=8  UNABLE TO ACTIVATE                       0273
**                  RC=12 NOT ON A 168MP SYSTEM                    0273
**                                                                 0273
**********************************************************************/
*/********************************************************************/
*/*                                                                  */
*/*           ESTABLISH ADRESSABILITY AND ENVIRONMENT                */
*/*                                                                  */
*/********************************************************************/
*/* IECVCRHA: ENTRY                       ENTRY POINT FOR ACTIVATION */
*/********************************************************************/
*/*      STANDARD ENTRY LINKAGE                                      */
*/********************************************************************/
*    GEN   CODE DEFS(IECVCRHA)                                     0273
*          REFS(IECVCRHA,RETRNREG,BASE,SAVEPTR,ENTRYREG,MYSAVE,CALLER);
IECVCRHA   DS 0F                           ENTRY POINT
     STM   RETRNREG,BASE,SAVREGS(SAVEPTR)  SAVE CALLERS REGISTERS
     LR    BASE,ENTRYREG                   LOAD BASE REGISTER
     USING IECVCRHA,BASE                   INDICATE ADDRESSABILITY
     LA    RETRNREG,MYSAVE                 GET ADDRESS OF MY SAVE AREA
     ST    RETRNREG,CALLED(SAVEPTR)        STORE IN CALLERS SAVE AREA
     ST    SAVEPTR,CALLER(RETRNREG)        SAVE PTR TO CALLERS AREA
     LR    SAVEPTR,RETRNREG                POINT REG13 TO MY SAVE AREA
*/********************************************************************/
*/*  DISABLE FOR INTERRUPTS AND PROVIDE FRR COVERAGE                 */
*/********************************************************************/
*    FOOTPRNT=ZERO;                    /* ZERO PROGRESS FLAGS        */
         SLR   @10,@10                                             0274
         ST    @10,FOOTPRNT                                        0274
*    RFY (WRK4,WRK5) RSTD;                                         0275
*    GEN CODE REFS(DISABLE,SAVEMASK,WRK4,WRK5);                    0276
     STNSM SAVEMASK,DISABLE            DISABLE FOR I/O AND EXT. INTER.
     SETFRR  A,FRRAD=CRHAF,WRKREGS=(WRK4,WRK5)
*/********************************************************************/
*/*  WHEN CALLED BY VARY IECVCRHA NEEDS THE IOS SUPER BIT ON SO THAT */
*/*  MY FRR WILL GET CONTROL RATHER THAN THE VARY ESTAE EXIT.        */
*/********************************************************************/
*    IF PSAIOSUP=OFF                   /* CHECK IF IOS SUPER BIT ON  */
*       THEN DO;                       /* NEED SUPER BIT FOR FRR     */
         TM    PSAIOSUP,B'10000000'                                0277
         BNZ   @RF00277                                            0277
*            PSAIOSUP=ON;              /* TURN IT ON                 */
         OI    PSAIOSUP,B'10000000'                                0279
*            IOSUP=ON;                 /* INDICATE I TURNED IT ON    */
         OI    IOSUP,B'10000000'                                   0280
*            END;                                                  0281
*    SETFRRA=ON;                       /* INDICATE FRR COVERAGE      */
@RF00277 OI    SETFRRA,B'10000000'                                 0282
*    RFY (WRK4,WRK5) UNRSTD;                                       0283
*/********************************************************************/
*/*                                                                  */
*/*               CHECK FOR ERROR CONDITIONS                         */
*/*                                                                  */
*/********************************************************************/
*                                                                  0284
*/********************************************************************/
*/*  MAKE SURE WE ARE RUNNING ON A 168MP SYSTEM                      */
*/********************************************************************/
*    IF PSACPUSA=ZERO                  /* ARE WE RUNNING ON CPU 0?   */
*       THEN INDEX=ZERO;               /* YES - CPU ID IS ZERO       */
         ICM   @10,3,PSACPUSA                                      0284
         BNZ   @RF00284                                            0284
         SLR   INDEX,INDEX                                         0285
*       ELSE INDEX=FOUR;               /* NO - CPU ID IS FOUR        */
         B     @RC00284                                            0286
@RF00284 LA    INDEX,4                                             0286
*    IF (PCCACPID(9:12)^=CPU168)×(CSDMP=OFF)                       0287
*       THEN DO;                                                   0287
@RC00284 L     @10,CVTPTR                                          0287
         LR    @01,INDEX                                           0287
         AL    @01,CVTPCCAT(,@10)                                  0287
         L     @01,PCCAPTR(,@01)                                   0287
         CLC   PCCACPID+8(4,@01),CPU168                            0287
         BNE   @RT00287                                            0287
         L     @01,CVTCSD(,@10)                                    0287
         TM    CSDMP(@01),B'10000000'                              0287
         BNZ   @RF00287                                            0287
@RT00287 DS    0H                                                  0288
*            IOXCRHA=ZERO;             /* ZERO POINTER TO IECVCRHA   */
         L     @10,CVTPTR                                          0289
         L     @01,IOCOMPTR(,@10)                                  0289
         L     @01,IOCXPTR(,@01)                                   0289
         SLR   @10,@10                                             0289
         ST    @10,IOXCRHA(,@01)                                   0289
*            RETCODE = TWELVE;         /* SETUP RETURN CODE          */
         LA    RETCODE,12                                          0290
*            GOTO CRHAEXIT;            /* CALL EXIT SUBROUTINE       */
         B     CRHAEXIT                                            0291
*            END;                                                  0292
*/********************************************************************/
*/*  CHECK WHETHER DSS IS ACTIVE                                     */
*/********************************************************************/
*    IF CVTDSSAC=FF                    /* TEST WHETHER DSS ACTIVE    */
*       THEN DO;                       /* YES, DO NOT ACTIVATE CRH   */
@RF00287 L     @10,CVTPTR                                          0293
         CLC   CVTDSSAC(1,@10),FF                                  0293
         BNE   @RF00293                                            0293
*            GEN CODE REFS(IEA989I);                               0295
             RECORD  TYPE=WTO,DATAADR=IEA989I,BUFFER=NO,RCVRY=SETFRR,LEX
                NGTH=LIEA989I
*            RETCODE=EIGHT;            /* INDICATE UNABLE TO ACTIVATE*/
         LA    RETCODE,8                                           0296
*            GOTO CRHAEXIT;            /* GO RETURN TO CALLER        */
         B     CRHAEXIT                                            0297
*            END;                                                  0298
*/********************************************************************/
*/*  CHECK WHETHER CRH HAS ALREADY BEEN ACTIVATED                    */
*/********************************************************************/
*    IF CVTCRCA^=ZERO                  /* IS CRCA POINTER ZERO       */
*       THEN DO;                                                   0299
@RF00293 L     @10,CVTPTR                                          0299
         ICM   @03,15,CVTCRCA(@10)                                 0299
         BZ    @RF00299                                            0299
*            RETCODE=FOUR;             /* SET UP RETURN CODE         */
         LA    RETCODE,4                                           0301
*            GOTO CRHAEXIT;            /* CALL EXIT SUBROUTINE       */
         B     CRHAEXIT                                            0302
*            END;                                                  0303
*    CVTCRCA = ADDR(IECVCRCA);         /* A NON-ZERO POINTER TO CRCA
*                                         INDICATES CRH IS ACTIVE    */
@RF00299 L     @10,@CA02779                                        0304
         L     @03,CVTPTR                                          0304
         ST    @10,CVTCRCA(,@03)                                   0304
*    CRCAPTR=CVTCRCA;                  /* SET UP ADDRESSABILITY      */
         LR    CRCAPTR,@10                                         0305
*    CRCAFTIM=ON;                        /* TURN ON 1ST TIME @ZM30496
*                                           SWITCH FOR SWITCH        */
         OI    CRCAFTIM(CRCAPTR),B'00001000'                       0306
*    CRHSADDR=CRHS;                      /* ADDR OF SRB SCHED@ZM30496
*                                           RTN FOR TIMER SLIH       */
         L     @10,@CA02758                                        0307
         MVC   CRHSADDR-CRHSADDR(4,@10),CRHS                       0307
*/********************************************************************/
*/*  INITIALIZE CRH COMMUNICATION AREA (CRCA)                        */
*/********************************************************************/
*    IF PSACPUSA=ZERO                  /* ARE WE RUNNING ON CPU 0 ?  */
*       THEN INDEX=FOUR;               /* YES-DEAD CPU ID IS ONE     */
         ICM   @10,3,PSACPUSA                                      0308
         BNZ   @RF00308                                            0308
         LA    INDEX,4                                             0309
*       ELSE INDEX=ZERO;               /* NO - DEAD CPU ID IS ZERO   */
         B     @RC00308                                            0310
@RF00308 SLR   INDEX,INDEX                                         0310
*    CRCAPCCA=(CVTPCCAT+INDEX)->PCCAPTR; /* GET VIRTUAL ADDRESS OF 0311
*                                           PCCA FOR DEAD CPU        */
@RC00308 L     @10,CVTPTR                                          0311
         LR    @01,INDEX                                           0311
         AL    @01,CVTPCCAT(,@10)                                  0311
         L     @01,PCCAPTR(,@01)                                   0311
         ST    @01,CRCAPCCA(,CRCAPTR)                              0311
*    CRCACAT=ADDR(PCCACAT);            /* GET CAT FOR DEAD CPU       */
         LA    @01,PCCACAT(,@01)                                   0312
         ST    @01,CRCACAT(,CRCAPTR)                               0312
*/********************************************************************/
*/*                                                                  */
*/*                 HOOK INTO OS/VS2 SYSTEM                          */
*/*                                                                  */
*/********************************************************************/
*                                                                  0313
*/********************************************************************/
*/*  CHANGE ADDRESS OF SLIH IN THE I/O FLIH TO POINT TO IECVCINT     */
*/********************************************************************/
*    SLIHPTR=ADDR(IECVCINT);           /* POINT TO CRH SLIH-IECVCINT */
         L     @03,@CA02782                                        0313
         L     @01,@CA00028                                        0313
         ST    @01,SLIHPTR-IEAVEIOI(,@03)                          0313
*    SLIHCHNG=ON;                      /* INDICATE POINTER TO SLIH 0314
*                                         WAS CHANGED                */
         OI    SLIHCHNG,B'00010000'                                0314
*/********************************************************************/
*/*  PUT HOOKS IN IOS MAINLINE - IECIOSCN                            */
*/********************************************************************/
*    SAVEHK1=IECVHK1A;                 /* SAVE OVERLAID INSTRUCTION  */
         L     @03,@CA02759                                        0315
         MVC   SAVEHK1(4),IECVHK1A-IECVHK1A(@03)                   0315
*    IECVHK1A=IECVHK1B;                /* OVERLAY INSTRUCTION        */
         L     @01,@CA02760                                        0316
         MVC   IECVHK1A-IECVHK1A(4,@03),IECVHK1B-IECVHK1B(@01)     0316
*    TCHHOOK=ON;                       /* INDICATE TCH HOOK IN       */
         OI    TCHHOOK,B'00001000'                                 0317
*    SAVEHK2=IECVHK2A;                 /* SAVE OVERLAID INSTRUCTION  */
         L     @03,@CA02761                                        0318
         MVC   SAVEHK2(4),IECVHK2A-IECVHK2A(@03)                   0318
*    IECVHK2A=IECVHK2B;                /* OVERLAY INSTRUCTION        */
         L     @01,@CA02762                                        0319
         MVC   IECVHK2A-IECVHK2A(4,@03),IECVHK2B-IECVHK2B(@01)     0319
*    SIOHOOK=ON;                       /* INDICATE SIO HOOK IN       */
         OI    SIOHOOK,B'00000100'                                 0320
*    SAVEHK3=IECVHK3A;                 /* SAVE OVERLAID INSTRUCTION  */
         L     @03,@CA02763                                        0321
         MVC   SAVEHK3(4),IECVHK3A-IECVHK3A(@03)                   0321
*    IECVHK3A=IECVHK3B;                /* OVERLAY INSTRUCTION        */
         L     @01,@CA02764                                        0322
         MVC   IECVHK3A-IECVHK3A(4,@03),IECVHK3B-IECVHK3B(@01)     0322
*    SNSHOOK=ON;                       /* INDICATE SENSE HOOK IN     */
         OI    SNSHOOK,B'00000010'                                 0323
*/********************************************************************/
*/*                                                                  */
*/*        SCAN THRU ALL UCBS FOR I/O ON DEAD CPU                    */
*/*                                                                  */
*/********************************************************************/
*    RFY UCBOB BASED(NXTUCB);          /* BASE UCBS ON UCB LOOKUP  0324
*                                         TABLE ENTRIES              */
*    RFY UCB BASED(NXTUCB-512);        /* BASE UCB PREFIX ON NXTUCB  */
*    RFY IOQ BASED(UCBIOQ);            /* BASE IOQ ON POINTER IN UCB */
*    LOOKUP=CVTILK2;                   /* GET POINTER TO UCB LOOK UP
*                                         TABLE                      */
         MVC   LOOKUP(4),CVTILK2(@10)                              0327
*    IF PSACPUSA=ZERO                  /* ARE WE RUNNING ON CPU 0 ?  */
*       THEN DEADCPU=ONE;              /* YES-DEAD CPU ID IS ONE     */
         ICM   @10,3,PSACPUSA                                      0328
         BNZ   @RF00328                                            0328
         MVI   DEADCPU,X'01'                                       0329
*       ELSE DEADCPU=ZERO;             /* NO - DEAD CPU ID IS ZERO   */
         B     @RC00328                                            0330
@RF00328 MVI   DEADCPU,X'00'                                       0330
*    SCANUCBS=ON;                      /* INDICATE UCBS BEING SCANNED
*                                         FOR I/O ON DEAD CPU        */
@RC00328 OI    SCANUCBS,B'00000001'                                0331
*    DO WHILE NXTUCB^=TABLEND;         /* CHECK FOR END OF TABLE     */
         B     @DE00332                                            0332
@DL00332 DS    0H                                                  0333
*       IF NXTUCB^=ZERO                /* ENSURE NONZERO UCB POINTER */
*          THEN IF UCBCPU=DEADCPU      /* LAST I/O STARTED TO DEAD ? */
         L     @10,LOOKUP                                          0333
         ICM   @03,3,NXTUCB(@10)                                   0333
         BZ    @RF00333                                            0333
*                  THEN DO;            /* YES                        */
         L     @10,LOOKUP                                          0334
         SLR   @01,@01                                             0334
         ICM   @01,3,NXTUCB(@10)                                   0334
         CLC   UCBCPU(1,@01),DEADCPU                               0334
         BNE   @RF00334                                            0334
*                       UCBCPU=UCBCPU&&ONE;/* SWITCH CPU ID          */
         L     @10,LOOKUP                                          0336
         SLR   @01,@01                                             0336
         ICM   @01,3,NXTUCB(@10)                                   0336
         L     @10,LOOKUP                                          0336
         SLR   @03,@03                                             0336
         ICM   @03,3,NXTUCB(@10)                                   0336
         SLR   @10,@10                                             0336
         IC    @10,UCBCPU(,@03)                                    0336
         X     @10,@CF00036                                        0336
         STC   @10,UCBCPU(,@01)                                    0336
*                       UCBIORST=ON;   /* INDICATE LAST PATH STARTED
*                                         WAS THRU DEAD CPU          */
         L     @10,LOOKUP                                          0337
         SLR   @01,@01                                             0337
         ICM   @01,3,NXTUCB(@10)                                   0337
         OI    UCBIORST(@01),B'10000000'                           0337
*                       IF UCBRESVH=ON /*CHECK IF RESERVE OUTSTANDING*/
*                          THEN UCBCRHRV=ON;/*YES,RESERVE ON DEAD CPU*/
         L     @10,LOOKUP                                          0338
         SLR   @01,@01                                             0338
         ICM   @01,3,NXTUCB(@10)                                   0338
         TM    UCBRESVH(@01),B'00010000'                           0338
         BNO   @RF00338                                            0338
         L     @10,LOOKUP                                          0339
         SLR   @01,@01                                             0339
         ICM   @01,3,NXTUCB(@10)                                   0339
         OI    UCBCRHRV(@01),B'00001000'                           0339
*                          ELSE;                                   0340
@RF00338 DS    0H                                                  0341
*                       IF UCBSPST=ON  /* ASSOCIATED SENSE IOQ ?     */
*                          THEN IF IOQENQ=ON&  /* IOQ ON A QUEUE ?   */
         L     @10,LOOKUP                                          0341
         SLR   @01,@01                                             0341
         ICM   @01,3,NXTUCB(@10)                                   0341
         TM    UCBSPST(@01),B'00100000'                            0341
         BNO   @RF00341                                            0341
*                                  IOQSLCH=ON  /*QUEUED ON SENSE LCH?*/
*                                  THEN UCBCRHSN=ON;/*NEED CRH SENSE */
         L     @10,LOOKUP                                          0342
         SLR   @01,@01                                             0342
         ICM   @01,3,NXTUCB(@10)                                   0342
         AL    @01,@CF03026                                        0342
         L     @01,UCBIOQ-508(,@01)                                0342
         TM    IOQENQ(@01),B'11000000'                             0342
         BNO   @RF00342                                            0342
         L     @10,LOOKUP                                          0343
         SLR   @01,@01                                             0343
         ICM   @01,3,NXTUCB(@10)                                   0343
         OI    UCBCRHSN(@01),B'00000100'                           0343
*                                  ELSE;                           0344
@RF00342 DS    0H                                                  0345
*                          ELSE;                                   0345
@RF00341 DS    0H                                                  0346
*                       END;                                       0346
*                  ELSE;               /*NOT LAST I/O STARTED TO DEAD*/
@RF00334 DS    0H                                                  0348
*          ELSE;                       /* ZERO UCB PTR,BUMP TO NEXT  */
@RF00333 DS    0H                                                  0349
*       LOOKUP=LOOKUP+TWO;             /* BUMP TO NEXT ENTRY IN UCB
*                                         LOOKUP TABLE - POINT TO  0349
*                                         NEXT UCB                   */
         LA    @10,2                                               0349
         AL    @10,LOOKUP                                          0349
         ST    @10,LOOKUP                                          0349
*    END;                                                          0350
@DE00332 L     @10,LOOKUP                                          0350
         CLC   NXTUCB(2,@10),TABLEND                               0350
         BNE   @DL00332                                            0350
*    IRTCHMSK=IOXCHMSK;                /* INSURE THAT ALL LOGICAL  0351
*                                         CHANNEL QUEUES ARE       0351
*                                         SEARCHED           @ZA16177*/
         L     @10,CVTPTR                                          0351
         L     @01,IOCOMPTR(,@10)                                  0351
         L     @01,IOCXPTR(,@01)                                   0351
         LH    @10,IOXCHMSK(,@01)                                  0351
         L     @03,LCCAPTR                                         0351
         ST    @10,IRTCHMSK+736(,@03)                              0351
*/********************************************************************/
*/*                                                                  */
*/*                 SCHEDULE SRB TO GET IECVCRHT DISPATCHED          */
*/*                                                                  */
*/********************************************************************/
*                                                                  0352
*    SRBPTR=ADDR(SRBAREA);             /*                    @ZM30496*/
         LA    SRBPTR,SRBAREA                                      0352
*    SRBASCB=ADDR(IEAMASCB);           /* PUT IN ASCB ADDRESS        */
         L     @10,@CA02780                                        0353
         ST    @10,SRBASCB(,SRBPTR)                                0353
*    SRBEP=ADDR(IECVCRHT);             /* POINT SRB TO TIMER @ZM30496
*                                         POP ROUTINE                */
*                                                                  0354
         LA    @10,IECVCRHT                                        0354
         ST    @10,SRBEP(,SRBPTR)                                  0354
*/********************************************************************/
*/*  SCHEDULE GLOBAL SRB TO IECVCRHT                                 */
*/********************************************************************/
*    GEN CODE REFS(SRBPTR);                                        0355
     SCHEDULE SRB=(SRBPTR),SCOPE=GLOBAL  SCHEDULE GLOBAL SRB
*    SCHEDSRB=ON;                      /* INDICATE SRB SCHEDULED     */
         OI    SCHEDSRB,B'00100000'                                0356
*/********************************************************************/
*/*                                                                  */
*/*                       EXIT ROUTINE                               */
*/*                                                                  */
*/********************************************************************/
*                                                                  0357
*/********************************************************************/
*/*  ISSUE CRH ACTIVATION MESSAGE VIA RECORD FACILITY                */
*/********************************************************************/
*    GEN CODE REFS(IEA970I);                                       0357
     RECORD   TYPE=WTO,DATAADR=IEA970I,BUFFER=NO,RCVRY=SETFRR,LENGTH=LIX
               EA970I
*    RETCODE=ZERO;                     /* RETURN CODE = 0 ACTIVATION
*                                         COMPLETED SUCCESSFULLY     */
         SLR   RETCODE,RETCODE                                     0358
*/********************************************************************/
*/*  CLEANUP, SET UP RETURN CODE AND RETURN                          */
*/********************************************************************/
*CRHAEXIT: ;                           /* EXIT LINKAGE               */
CRHAEXIT DS    0H                                                  0360
*    IF IOSUP=ON                       /* DID I TURN ON IOS SUPER    */
*       THEN DO;                       /* YES I DID                  */
         TM    IOSUP,B'10000000'                                   0360
         BNO   @RF00360                                            0360
*            PSAIOSUP=OFF;             /* TURN IT OFF                */
         NI    PSAIOSUP,B'01111111'                                0362
*            IOSUP=OFF;                /* INDICATE IOSUP OFF         */
         NI    IOSUP,B'01111111'                                   0363
*            END;                                                  0364
*    RFY (WRK4,WRK5) RSTD;                                         0365
@RF00360 DS    0H                                                  0366
*    GEN CODE REFS(WRK4,WRK5);                                     0366
     SETFRR D,WRKREGS=(WRK4,WRK5)      DELETE FRR
     IC    WRK4,SAVEMASK               GET SAVED SYSTEM MASK
     EX    WRK4,STOSMA                 RESTORE SYSTEM MASK
     L     SAVEPTR,CALLER(SAVEPTR)     GET PTR TO CALLERS SAVE AREA
     L     RETRNREG,SAVREG14(SAVEPTR)  RESTORE RETURN REGISTER
     LM    REG0,BASE,SAVREG0(SAVEPTR)  RESTORE REGISTERS
     BR    RETRNREG                    RETURN TO CALLER WITH RC
STOSMA   STOSM  SAVEMASK,AZERO         RESTORE SYSTEM MASK
*    RFY (WRK4,WRK5) UNRSTD;                                       0367
*/********************************************************************/
*/*    ENTRY POINT FROM RTM ON FRR RETRY                             */
*/********************************************************************/
*CRHARTRY: ;                                                       0368
CRHARTRY DS    0H                                                  0369
*    RETCODE=EIGHT;                    /* SET UP RETURN CODE         */
         LA    RETCODE,8                                           0369
*    GO TO CRHAEXIT;                   /* GO TO EXIT ROUTINE         */
         B     CRHAEXIT                                            0370
*/*********************************************************************
**                                                                 0371
** ENTRY NAME - IECVCRHD                                           0371
**                                                                 0371
** DESCRIPTIVE NAME - CRH DEACTIVATION ROUTINE                     0371
**                                                                 0371
** FUNCTION - THIS ENTRY DEACTIVATES THE CHANNEL RECONFIGURATION   0371
**            HARDWARE FUNCTION IN THE OS/VS2 SYSTEM. IT REMOVES   0371
**            HOOKS IN IOS MAINLINE MODULE IECIOSCN, POINTS THE    0371
**            I/O FLIH BACK TO THE IOS SLIH AND PROCESSES THE UCBS 0371
**            FOR USE WITHOUT CRH. THE CRH FUNCTION IS LOGICALLY   0371
**            DEACTIVATED WHEN THE POINTER IN THE CVT (CVTCRCA)    0371
**            TO THE CRH COMMUNICATION AREA HAS BEEN ZEROED.       0371
**                                                                 0371
** LINKAGE - STANDARD LINKAGE                                      0371
**                                                                 0371
** INPUT                                                           0371
**                                                                 0371
**      REGISTERS - SAVEPTR  (REGISTER 13) POINTS TO 18 WORD SAVE AREA
**                  RETURN   (REGISTER 14) ADDRESS OF RETURN POINT 0371
**                  ENTRYREG (REGISTER 15) ADDRESS OF IECVCRHA     0371
**                                                                 0371
** OUTPUT - NONE                                                   0371
**                                                                 0371
** EXIT - NORMAL - RETURN TO CALLER WITH RETURN CODE OF 0          0371
**                                                                 0371
** EXIT - ERROR - RETURN TO CALLER WITH RETURN CODE                0371
**                  RC=4  CRH ALREADY ACITVE                       0371
**********************************************************************/
*/********************************************************************/
*/*                                                                  */
*/*           ESTABLISH ADRESSABILITY AND ENVIRONMENT                */
*/*                                                                  */
*/********************************************************************/
*/* IECVCRHD: ENTRY                       CRH DEACTIVATION ROUTINE   */
*/********************************************************************/
*/*      STANDARD ENTRY LINKAGE                                      */
*/********************************************************************/
*    GEN CODE DEFS(IECVCRHD)                                       0371
*        REFS(RETRNREG,BASE,SAVEPTR,ENTRYREG,MYSAVE,CALLER);       0371
IECVCRHD   DS  0F                          ENTRY POINT
     STM   RETRNREG,BASE,SAVREGS(SAVEPTR)  SAVE CALLERS REGISTERS
     LR    BASE,ENTRYREG                   LOAD BASE REGISTER
     USING IECVCRHD,BASE                   INDICATE ADDRESSABILITY
     LA    RETRNREG,MYSAVE                 GET ADDRESS OF MY SAVE AREA
     ST    RETRNREG,CALLED(SAVEPTR)        STORE IN CALLERS SAVE AREA
     ST    SAVEPTR,CALLER(RETRNREG)        SAVE PTR TO CALLERS AREA
     LR    SAVEPTR,RETRNREG                POINT REG13 TO MY SAVE AREA
*/********************************************************************/
*/*  DISABLE FOR INTERRUPTS AND PROVIDE FRR COVERAGE                 */
*/********************************************************************/
*    RFY (WRK4,WRK5) RSTD;                                         0372
*    GEN CODE REFS(DISABLE,SAVEMASK,WRK4,WRK5);                    0373
     STNSM SAVEMASK,DISABLE            DISABLE FOR I/O AND EXT. INTER.
     SETFRR  A,FRRAD=CRHDF,WRKREGS=(WRK4,WRK5)
*    RFY (WRK4,WRK5) UNRSTD;                                       0374
*    PSAIOSUP=ON;                      /* TURN ON IOS SUPER BIT SO 0375
*                                       THAT MY FRR WILL GET CONTROL
*                                       INSTEAD OF THE VARY ESTAE    */
         OI    PSAIOSUP,B'10000000'                                0375
*    SETFRRD=ON;                       /* INDICATE FRR COVERAGE      */
         OI    SETFRRD,B'01000000'                                 0376
*/********************************************************************/
*/*                                                                  */
*/*       MAKE SURE CRH FUNCTION IS ACTIVE AND THEN DEACTIVATE       */
*/*                                                                  */
*/********************************************************************/
*                                                                  0377
*/********************************************************************/
*/*  CVTCRCA=0 INDICATES CRH NOT ACTIVE                              */
*/********************************************************************/
*    IF CVTCRCA=ZERO                   /* CHECK IF CRH ACTIVE        */
*       THEN DO;                       /* CRH NOT ACTIVE             */
         L     @10,CVTPTR                                          0377
         ICM   @03,15,CVTCRCA(@10)                                 0377
         BNZ   @RF00377                                            0377
*              RETCODE=FOUR;           /* INDICATE CRH NOT ACTIVE    */
         LA    RETCODE,4                                           0379
*              GOTO CRHDEXIT;          /* CALL EXIT ROUTINE          */
         B     CRHDEXIT                                            0380
*            END;                                                  0381
*    CVTCRCA=ZERO;                     /* INDICATE CRH NO LONGER   0382
*                                         ACTIVE                     */
