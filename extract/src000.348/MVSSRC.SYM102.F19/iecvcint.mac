*    RFY UCBOB BASED(NXTUCB);         /* BASE UCBS ON UCB LOOKUP   0595
*                                         TABLE ENTRIES              */
@RF00590 DS    0H                                                  0596
*    IF SCANUCBS=OFF                  /* NEED TO SWITCH UCBS BACK ?  */
*       THEN RETURN;                  /* NO, RETURN TO CALLER        */
         TM    SCANUCBS,B'00000001'                                0596
         BZ    @RT00596                                            0596
*       ELSE;                         /* YES, SCANUCBS               */
*    IF DOINSCAN=ON                   /* RECURSIVE CALL BY FRR?      */
*       THEN LOOKUP=LOOKUP+TWO;       /* YES,BUMP TO NEXT UCB        */
         TM    DOINSCAN,B'10000000'                                0599
         BNO   @RF00599                                            0599
         LA    @10,2                                               0600
         AL    @10,LOOKUP                                          0600
         ST    @10,LOOKUP                                          0600
*       ELSE DOINSCAN=ON;             /* INDICATE FIRST ENTRY        */
         B     @RC00599                                            0601
@RF00599 OI    DOINSCAN,B'10000000'                                0601
