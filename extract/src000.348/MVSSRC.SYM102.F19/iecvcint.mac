@RC00599 B     @DE00602                                            0602
@DL00602 DS    0H                                                  0603
*       IF NXTUCB^=ZERO                /* ENSURE NONZERO UCB POINTER */
*         THEN DO;                     /* PROCESS UCB                */
         L     @10,LOOKUP                                          0603
         ICM   @03,3,NXTUCB(@10)                                   0603
         BZ    @RF00603                                            0603
*              IF UCBIORST=ON × UCBCRHRV=ON × UCBCRHSN=ON          0605
*                                      /* CHECK IF LAST I/O STARTED
*                                    WAS THRU CRH CONNECTION OR IF CRH
*                                    PATH RESERVED @ZA15805  @ZA16177*/
*                THEN DO;              /* YES                        */
         L     @10,LOOKUP                                          0605
         SLR   @01,@01                                             0605
         ICM   @01,3,NXTUCB(@10)                                   0605
         TM    UCBIORST(@01),B'10001100'                           0605
