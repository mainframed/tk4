*/********************************************************************/
*/* CHECK TO MAKE SURE CRH FUNCTION IS STILL ACTIVE                  */
*/********************************************************************/
*    IF CVTCRCA=ZERO                   /* CRH FUNCTION ACTIVE ?      */
*       THEN GOTO CRHTEXIT;            /* NO, EXIT                   */
         L     @10,CVTPTR                                          0531
         L     @10,CVTCRCA(,@10)                                   0531
         LTR   @10,@10                                             0531
         BZ    @RT00531                                            0531
*/********************************************************************/
*/* CRH ACTIVE,INITIALIZE INTERRUPT TIME AND TQE             @ZM30496*/
*/********************************************************************/
*    CRCAPTR=CVTCRCA;                  /* GET ADDRESS OF CRCA        */
         LR    CRCAPTR,@10                                         0533
*    GEN CODE REFS(CRCAPTR) SETS(CRCACTIM);                        0534
     STCK  CRCACTIM(CRCAPTR)           GET CURRENT TIME
*    IF CRCAFTIM=ON                    /* 1ST TIME ENTERED?  @ZM30496*/
*       THEN DO;                       /* YES                @ZM30496*/
         TM    CRCAFTIM(CRCAPTR),B'00001000'                       0535
         BNO   @RF00535                                            0535
*               CRCAFTIM=OFF;          /* TURN OF 1ST TIME SW@ZM30496*/
         NI    CRCAFTIM(CRCAPTR),B'11110111'                       0537
*               CRCAITIM=CRCAITIM&&CRCAITIM;         /*      @ZM30496*/
         XC    CRCAITIM(8,CRCAPTR),CRCAITIM(CRCAPTR)               0538
*                                       /*ZERO INTERRUPT TIME@ZM30496*/
*               TQEOFF=ON;             /* TURN ON TQE FLAGS  @ZM30496*/
         L     @10,TQEPTR                                          0539
         OI    TQEOFF(@10),B'10000000'                             0539
*               TQETYPE=REALTQE;       /*                    @ZM30496*/
         OI    TQETYPE(@10),B'00000011'                            0540
*               TQECRH=ON;             /*                    @ZM30496*/
         OI    TQECRH(@10),B'00100000'                             0541
*            END;                      /*                    @ZM30496*/
*/********************************************************************/
*/*                                                          @ZM30496*/
*/*     THIS SEGMENT COMPARES THE TIME OF THE LAST INTERRUPT TO THE  */
*/*     CURRENT TIME.  IF THE DIFFERENCE IS LESS THAN TWO SECONDS,   */
*/*     THE TIME INTERVAL WE NEXT WANT TO BE DISPATCHED IN IS SET.   */
*/*     OTHERWISE, AN INTERRUPT IS SIMULATED AND THEN THE TIME       */
*/*     INTERVAL IS SET.                                             */
*/********************************************************************/
*                                                                  0543
*    IF CWORD1-IWORD1<TWO              /* DIFF<2 SEC?        @ZM30496*/
*       THEN;                          /*   DO NOT DO SIMINT @ZM30496*/
@RF00535 L     @10,CWORD1(,CRCAPTR)                                0543
         SL    @10,IWORD1(,CRCAPTR)                                0543
         CL    @10,@CF00052                                        0543
         BL    @RT00543                                            0543
*       ELSE DO;                       /*                    @ZM30496*/
*                                                                  0545
*    RFY   (WRK4,WRK5) RSTD;                                       0546
*CRHT100:                                                          0547
*            WRK4=SIMADDR;             /* LOAD DEVICE ADDRESS FOR  0547
*                                         SIMULATED INTERRUPT        */
CRHT100  L     WRK4,SIMADDR                                        0547
*            GEN CODE(IOSINTRP UCB=(WRK4),VAR=2,BRANCH=YES);       0548
         IOSINTRP UCB=(WRK4),VAR=2,BRANCH=YES
*            END;                                                  0549
*                                                                  0549
*    TIMEWRK1=IWORD1+TWO;              /* FOR NEXT TIME TO   @ZM30496*/
@RT00543 LA    @10,2                                               0550
         AL    @10,IWORD1(,CRCAPTR)                                0550
         ST    @10,TIMEWRK1                                        0550
*                                      /* GET DISPATCHED             */
*    RFY   (WRK4,WRK5) UNRSTD;                                     0551
*/********************************************************************/
*/* ENQ TQE TO GET US DISPATCHED AGAIN IN 2.5 SECONDS.       @ZM30496*/
*/********************************************************************/
*    RFY (WRK1,WRK4,WRK5) RSTD;             /*               @ZM30496*/
*    TQEVAL=TIMEWORK;                       /* TIMER VALUE   @ZM30496*/
         L     @10,TQEPTR                                          0553
         MVC   TQEVAL(8,@10),TIMEWORK                              0553
*/********************************************************************/
*/*    GET LOCK AND ENQ TQE   NOTE: NON-STANDARD LINKAGE     @ZM30496*/
