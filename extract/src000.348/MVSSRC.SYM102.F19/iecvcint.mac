         STH   PARMSREG,CRCACHAN(,CRCAPTR)                         0560
*    CRCAMCWF=CRCACHAN(ONE:ONE);        /* SET UP MCW TO MAKE CRH  0561
*                                          CONNECTION TO CHANNEL     */
         MVC   CRCAMCWF(1,CRCAPTR),CRCACHAN(CRCAPTR)               0561
*    CRCAMCWI=ON;                       /* INDICATE CRH MCW          */
*    CRCAMCWM=ON;                       /* INDICATE MAKE CONNECTION  */
         OI    CRCAMCWI(CRCAPTR),B'00110000'                       0563
*    CRCADIAG=ON;                       /* INDICATE CRH CONNECTION 0564
*                                          OUTSTANDING               */
         OI    CRCADIAG(CRCAPTR),B'10000000'                       0564
*    PARMSREG=SIMADDR;                  /* USE CHANNEL 6 FOR STIDC   */
         L     PARMSREG,SIMADDR                                    0565
*    GEN CODE REFS(CRCA,CRCAPTR,CRCAMCW) SETS(PARMSREG);           0566
     USING CRCA,CRCAPTR
     DC  X'8300',S(CRCAMCW)             ISSUE DIAGNOSE
     DROP  CRCAPTR
     STIDC  AZERO(PARMSREG)             ISSUE STORE CHANNEL ID
     BALR   PARMSREG,AZERO              SAVE CONDITION CODE
*/********************************************************************/
*/*    BREAK CRH CONNECTION                                          */
*/********************************************************************/
*    CRCAMCWM=OFF;                      /* INDICATE BREAK CONNECTION */
         NI    CRCAMCWM(CRCAPTR),B'11101111'                       0567
*    GEN CODE REFS(CRCA,CRCAPTR,CRCAMCW);                          0568
     USING CRCA,CRCAPTR
     DC  X'8300',S(CRCAMCW)             ISSUE DIAGNOSE
     DROP  CRCAPTR
*    CRCADIAG=OFF;                      /* INDICATE NO CRH CONNECTION
*                                          OUTSTANDING               */
         NI    CRCADIAG(CRCAPTR),B'01111111'                       0569
*/********************************************************************/
*/*                  EXIT LINKAGE                                    */
*/********************************************************************/
*CRHVEXIT:                                                         0570
*    RFY WRK4 RSTD;                                                0570
