         TITLE 'IGE0025C -WTO ROUTINE FOR MVM ERPS                     *
                        '
IGE0025C CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(22)                                             0001
         DC    C'IGE0025C UZ16360 78017'                           0001
         DROP  @15
@PROLOG  DS    0H                                                  0002
*               /*
         BALR  BASEREG,0
         USING *,BASEREG              SET UP ADDRESSABILITY
*   SAVIREG='00000000'X;            /* ZERO SAVE IN CASE NOTHING ON
*                                      QUEUE ALREADY         @YM06716*/
         SLR   SAVIREG,SAVIREG                                     0187
*   IF IOSDOM='1'B THEN                                            0188
         TM    IOSDOM(IOSBPTR),B'00000010'                         0188
         BO    @RT00188                                            0188
*     GOTO DOMRTN;                  /* DOM REQUIRED          @YM06828*/
*   IF CVTNIP='1'B                  /* IF NIP IS IN CONTROL,DO       */
*     THEN                                                         0190
         L     @12,CVTPTR                                          0190
         TM    CVTNIP(@12),B'00010000'                             0190
         BO    @RT00190                                            0190
*     GOTO RET3;                    /* NOT ISSUE MSG, BUT DO LOG   0191
*                                      ERROR. THEN EXIT              */
*                                                                  0191
*   /*****************************************************************/
*   /*                                                               */
*   /* GET LOCAL LOCK TO PROTECT FRR STACK FOR CORE MGR REGISTERS    */
*   /* 11,12,13,14 ARE DESTROYED BY SETLOCK REGISTER 13 IS ONLY ONE  */
*   /* WE NEED TO PROTECT                                            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0192
*   RFY                                                            0192
*    (EWAPTR,                                                      0192
*     R13,                                                         0192
*     WKREG,                                                       0192
*     CALLRG) RSTD;                                                0192
*   EWAPTR=R13;                     /* SAVE SAVE AREA POINTER        */
         LR    EWAPTR,R13                                          0193
*   GEN REFS(PSALITA);                                             0194
*                  /*
GETLOCK SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,                         X
               RELATED=(IECVSMGR,IGE0025C(FREELOCK))
*   R13=EWAPTR;                     /* RESTORE POINTER               */
*                                                                  0195
         LR    R13,EWAPTR                                          0195
*   /*****************************************************************/
*   /*                                                               */
*   /* SETUP FOR AND GO TO CORE MANAGER TO GET 160 BYTE BLOCK IN     */
*   /* WHICH TO BUILD MESSAGE                                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0196
*/*                                                                  */
*                                                                  0196
*   WKREG='10000020'X;              /* R11 =# BLKS,ASID              */
         L     WKREG,@CF02609                                      0196
*   CALLRG=IOCORMGT;                /* ADDR OF CORE MANAGER          */
         L     @12,CVTPTR                                          0197
         L     @12,IOCOMPTR(,@12)                                  0197
         L     CALLRG,IOCORMGT(,@12)                               0197
*   GEN REFS(IOSASID);                                             0198
         ICM   WKREG,6,IOSASID(IOSBPTR)   GET ASID IN REG11
         BALR  @14,CALLRG          GO GET CORE
*   MSGPTR=WKREG;                   /* PICK UP ADDR OF CORE          */
*                                                                  0199
         LR    MSGPTR,WKREG                                        0199
*   /*****************************************************************/
*   /*                                                               */
*   /* FREE LOCAL LOCK JUST GOTTEN FOR CORE MANAGER SINCE NO SVC'S   */
*   /* CAN BE ISSUED WHILE IT IS HELD. REGISTER 13 AGAIN MUST BE     */
*   /* SAVED.                                                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0200
*/*                                                                  */
*                                                                  0200
*   EWAPTR=R13;                     /* SAVE SAVE AREA PTR            */
         LR    EWAPTR,R13                                          0200
*   GEN REFS(PSALITA);                                             0201
*                            /*
FREELOCK SETLOCK RELEASE,TYPE=LOCAL,                                   X
               RELATED=(IECVSMGR,IGE0025C(GETLOCK))
*   R13=EWAPTR;                     /* RESTORE SAV AREA ADDR         */
         LR    R13,EWAPTR                                          0202
*   RFY                                                            0203
*    (EWAPTR,                                                      0203
*     R13,                                                         0203
*     WKREG,                                                       0203
*     CALLRG) UNRSTD;                                              0203
*   RFY                                                            0204
*     UCBPTR RSTD;                                                 0204
*                                                                  0204
*   /*****************************************************************/
*   /*                                                               */
*   /* BEGIN MESSAGE DETERMINATION AND BUILD                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0205
*STAR:                                                             0205
*   UCBPTR=IOSUCB;                  /* ADDR OF UCB                   */
STAR     L     UCBPTR,IOSUCB(,IOSBPTR)                             0205
*/*         DETERMINE WHAT MESSAGE SHOULD BE ISSUED                  */
*                                                                  0206
*   IF IOSMSG='1'B                  /* PERM I/O ERROR MSG            */
*     THEN                                                         0206
         TM    IOSMSG(IOSBPTR),B'00000100'                         0206
         BO    @RT00206                                            0206
*     GOTO MSG2;                    /* INDICATOR                     */
*   IF IOSCC3A='1'B&IOSCC4='1'B     /* PATH INOPERATIVE              */
*     THEN                                                         0208
         TM    IOSCC3A(IOSBPTR),B'00110000'                        0208
         BO    @RT00208                                            0208
*     GOTO MSG3;                    /* CONDITION CODE                */
*/*                                                                  */
*                                                                  0210
*   IF UCB3DACC^='1'B THEN          /* NO NEED TO CHECK UCBIVRR    0210
*                                                             @YM7472*/
         TM    UCB3DACC(UCBPTR),B'00100000'                        0210
         BNO   @RT00210                                            0210
*     GO TO INTR;                   /* INTERVENTION IS REQUIRED    0211
*                                                             @YM7472*/
*                                                                  0211
*   /*****************************************************************/
*   /*                                                               */
*   /* NONE OF THE ABOVE, MUST BE INTERVENTION REQUIRED TEST TO SEE  */
*   /* IF MESSAGE IS STILL NECESSARY IF UCBIVRR IS NOT ON,           */
*   /* INTERVENTION IS NO NEEDED. IF UCBIVRS IS ON, WTO HAS ALREADY  */
*   /* ISSUED ONE MESSAGE FOR THIS CONDITION,DO NOT ISSUE ANOTHER.   */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0212
*/*         USE IOSGEN TO TEST BITS- SET UP AND CALL                 */
*                                                                  0212
*   GEN REFS(UCBFLC,UCBBGN,RET0);                                  0212
*                                       /*
         IOSGEN UCBFLG,UCB=(@04),VAR=TEST,TABLE=UCBIVRR
         BZ    RET0              NO INTERVENTION NEEDED,GO
*                                  CHECK FOR MORE REQUESTS
*   IF UCBMTPXP='1'B THEN           /* MULIPLE EXPOSURE      @VS30419*/
         TM    UCBMTPXP(UCBPTR),B'00001000'                        0213
         BNO   @RF00213                                            0213
*     DO;                           /* DEVICE                @VS30419*/
*       GEN REFS(UCBFLC,UCBBGN,UCBBASE,PREFIX);                    0215
*                                                 /*@VS30419,@ZA19320*/
           IOSGEN UCBFLG,UCB=(@04),VAR=OFF,TABLE=UCBIVRR     @VS30419
*                                      * TURN OFF INT REQ IN @VS30419
           L   @04,UCBBASE-UCBOB(@04) IN SPECIFIC EXP.       @VS30419
           A   @04,PREFIX             PT TO UCBOB FOR IOSGEN @ZA19320
*                                      * TURN ON BASE EXP    @VS30419
           IOSGEN UCBFLG,UCB=(@04),VAR=ON,TABLE=UCBIVRR      @VS30419
*     END;                          /*                       @VS30419*/
*INTR:                                                             0217
*   GEN REFS(UCBFLC,UCBBGN,RET0);                                  0217
*                                                                  0217
@RF00213 DS    0H                                                  0217
INTR     DS    0H                                                  0217
         IOSGEN UCBFLG,UCB=(@04),VAR=TEST,TABLE=UCBIVRS
         BNZ    RET0              MSG ALREADY SENT,GO CHECK
*                                  FOR MORE REQUESTS
*   /*****************************************************************/
*   /*                                                               */
*   /* STILL NEED MESSAGE,BUILD AND ISSUE                            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0218
*   RFY                                                            0218
*    (EWAPTR,                                                      0218
*     WKREG) RSTD;                                                 0218
*   EWAPTR=IOSERP;                  /* ADDR. OF ERP.         @Y30LPLC*/
         L     EWAPTR,IOSERP(,IOSBPTR)                             0219
*   IF UCB3UREC='1'B                /*                       @Y30LPLC*/
*     THEN                                                         0220
         TM    UCB3UREC(UCBPTR),B'00001000'                        0220
         BNO   @RF00220                                            0220
*     DO;                                                          0221
*       IF UCBTBYT4='42'X           /*                       @Y30LPLC*/
*           &MOINTREQ='1'B THEN     /*                       @Y30LPLC*/
         CLI   UCBTBYT4(UCBPTR),X'42'                              0222
         BNE   @RF00222                                            0222
         TM    MOINTREQ(EWAPTR),B'01000000'                        0222
         BNO   @RF00222                                            0222
*         MSID=MSG2ID;              /* INFORMATIONAL INTER-  @Y30LPLC*/
         MVC   MSID(8,MSGPTR),MSG2ID                               0223
*       ELSE                        /* VENTION REQUIRED.     @Y30LPLC*/
*         MSID=MSG1ID;              /* GET MESSAGE ID                */
         B     @RC00222                                            0224
@RF00222 MVC   MSID(8,MSGPTR),MSG1ID                               0224
*     END;                          /*                       @Y30LPLC*/
*   ELSE                                                           0226
*     MSID=MSG1ID;                  /* ACTION MESSAGE        @Y30LPLC*/
         B     @RC00220                                            0226
@RF00220 MVC   MSID(8,MSGPTR),MSG1ID                               0226
*   UADR=UCBNAME;                   /* CHANNEL AND UNIT ADDR         */
@RC00220 MVC   UADR(3,MSGPTR),UCBNAME(UCBPTR)                      0227
*   C1='6B'X;                       /* COMMA DELINEATER              */
         MVI   C1(MSGPTR),X'6B'                                    0228
*   TXT=IRQ;                        /* INTER REQ TEXT                */
         MVC   TXT(8,MSGPTR),IRQ                                   0229
*   CM1='EF'X;                                                     0230
         MVI   CM1(MSGPTR),X'EF'                                   0230
*   CM2='EF'X;                                                     0231
         MVI   CM2(MSGPTR),X'EF'                                   0231
*   WKREG=IOSCSWCA-8;               /* ADDR OF LAST COMMAND          */
         L     WKREG,IOSCSWCA-1(,IOSBPTR)                          0232
         LA    WKREG,0(,WKREG)                                     0232
         SL    WKREG,@CF00075                                      0232
*   GEN REFS(CM);                                                  0233
         LTR     WKREG,WKREG       TEST FOR MINUS,THEN NO OP
         BM    FIN                 SO LEAVE ASTERISKS
         UNPK CM(3,@10),0(2,WKREG)
FIN      EQU   *
*   C2='EE'X;                       /* COMMA                         */
*                                                                  0234
         MVI   C2(MSGPTR),X'EE'                                    0234
*   /*****************************************************************/
*   /*                                                               */
*   /* UNPACK STATUS                                                 */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0235
*   GEN REFS(STAT1)(UNPK STAT1(5,@10),IOSTATUS(3,@01));            0235
         UNPK STAT1(5,@10),IOSTATUS(3,@01)
*   C3='EE'X;                                                      0236
*                                                                  0236
         MVI   C3(MSGPTR),X'EE'                                    0236
*   /*****************************************************************/
*   /*                                                               */
*   /* SETUP EWAPTR                                                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0237
*   RFY                                                            0237
*     WKREG RSTD;                                                  0237
*   EWAPTR=IOSERP;                  /* ADDR OF EWA                   */
*                                                                  0238
         L     EWAPTR,IOSERP(,IOSBPTR)                             0238
*   /*****************************************************************/
*   /*                                                               */
*   /* TEST FOR 3340 WHICH REQUIRES ALL 24 BYTES OF SENSE ALSO 3800  */
*   /* (ARGONAUT) NEEDS 24 BYTES OF SENSE                    @Y40MPLG*/
*   /* THEREFORE NEEDING A TWO LINE MESSAGE                          */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0239
*   IF UCBTBYT3^='20'X              /* ***********************     0239
*                                                            @YM05692*/
*       ×UCBTBYT4^='0A'X            /* IF NOT A 3340, GO BUILD       */
*     THEN                                                         0239
         CLI   UCBTBYT3(UCBPTR),X'20'                              0239
         BNE   @RT00239                                            0239
         CLI   UCBTBYT4(UCBPTR),X'0A'                              0239
         BE    @RF00239                                            0239
@RT00239 DS    0H                                                  0240
*     IF UCBTBYT3^='08'X            /*                       @Y40MPLG*/
*         ×UCBTBYT4^='0E'X          /* NOT 3340, IS IT 3800? @Y40MPLG*/
*       THEN                        /*                       @ZA06055*/
         CLI   UCBTBYT3(UCBPTR),X'08'                              0240
         BNE   @RT00240                                            0240
         CLI   UCBTBYT4(UCBPTR),X'0E'                              0240
         BE    @RF00240                                            0240
@RT00240 DS    0H                                                  0241
*       IF^(UCBTBYT3='80'X          /* IS IT 3400?           @ZA06055*/
*           &UCBTBYT4='03'X)        /*                       @ZA06055*/
*         THEN                                                     0241
         CLI   UCBTBYT3(UCBPTR),X'80'                              0241
         BNE   @RT00241                                            0241
         CLI   UCBTBYT4(UCBPTR),X'03'                              0241
         BNE   @RT00241                                            0241
*         GOTO NOTWIN;              /* ONE LINE MESSAGE NEEDS 24   0242
*                                      BYTES OF SENSE, WILL BE ON  0242
*                                      SECOND LINE           @ZA06055*/
*   GEN REFS(CM)(TR CM(8,@10),TRTABLE-238);                        0243
@RF00240 DS    0H                                                  0243
@RF00239 DS    0H                                                  0243
         TR CM(8,@10),TRTABLE-238
*   WSNS='6B'X;                     /* COMMA TO SENSE FIELD          */
*                                                                  0244
         MVI   WSNS(MSGPTR),X'6B'                                  0244
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE POINTER TO VOLUME SERIAL                           */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0245
*   CODEPTR=ADDR(WSNS)+'01'X;                                      0245
*                                                                  0245
         LA    CODEPTR,WSNS(,MSGPTR)                               0245
         AL    CODEPTR,@CF00445                                    0245
*   /*****************************************************************/
*   /*                                                               */
*   /* IF VOL SER FIELD IS ZERO IN THE EWA,PUT IN A COMMA            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0246
*   IF UCB3DACC='1'B THEN           /* IF DASD CHECK VOL SER @Y40MPLG*/
         TM    UCB3DACC(UCBPTR),B'00100000'                        0246
         BNO   @RF00246                                            0246
*     IF EWDCCW9^='000000000000'X THEN                             0247
         CLC   EWDCCW9(6,EWAPTR),@CB02623                          0247
         BE    @RF00247                                            0247
*       DO;                                                        0248
*         SER1=EWDCCW9;             /* VOL SER TO BUFFER             */
         MVC   SER1(6,CODEPTR),EWDCCW9(EWAPTR)                     0249
*         CODEPTR=CODEPTR+'06'X;    /* LENG TO PTR                   */
         AL    CODEPTR,@CF00454                                    0250
*       END;                                                       0251
*   IF UCB3TAPE='1'B THEN           /* IF TAPE VOLSER IN UCB @ZA06055*/
@RF00247 DS    0H                                                  0252
@RF00246 TM    UCB3TAPE(UCBPTR),B'10000000'                        0252
         BNO   @RF00252                                            0252
*     IF UCBVOLI^='000000000000'X   /*                       @ZA06055*/
*       THEN                        /* NOT ZEROS, VALID VOLSER     0253
*                                                            @ZA06055*/
         CLC   UCBVOLI(6,UCBPTR),@CB02623                          0253
         BE    @RF00253                                            0253
*       IF UCBVOLI(1:1)^='FF'X THEN /* CHECK FOR FF-SVA-ID   @ZA18804*/
         CLI   UCBVOLI(UCBPTR),X'FF'                               0254
         BE    @RF00254                                            0254
*         DO;                                                      0255
*           SER1=UCBVOLI;           /* VOLSER INTO MSG       @ZA06055*/
         MVC   SER1(6,CODEPTR),UCBVOLI(UCBPTR)                     0256
*           CODEPTR=CODEPTR+6;      /* INCRMNT BY LENGTH     @ZA06055*/
         AL    CODEPTR,@CF00073                                    0257
*         END;                      /*                       @ZA06055*/
*   C4='6B'X;                       /* COMMA AFTER OR IN PLACE OF  0259
*                                      VOLUME SERIAL                 */
*                                                                  0259
@RF00254 DS    0H                                                  0259
@RF00253 DS    0H                                                  0259
@RF00252 MVI   C4(CODEPTR),X'6B'                                   0259
*   /*****************************************************************/
*   /*                                                               */
*   /* JOBNAME PROCESSING IF A TCB AND A TIOT EXIST,WE SHOULD HAVE A */
*   /* VALID JOBNAME                                                 */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0260
*   IF SRBPTCB^='00000000'X         /* IF THERE IS A TCB             */
*     THEN                                                         0260
         SLR   @12,@12                                             0260
         L     @05,IOSSRB(,IOSBPTR)                                0260
         L     @05,SRBPTCB(,@05)                                   0260
         CR    @05,@12                                             0260
         BE    @RF00260                                            0260
*     IF TCBTIO^='00000000'X        /* AND A TIOT,PICK UP            */
*       THEN                                                       0261
         L     @05,TCBTIO(,@05)                                    0261
         CR    @05,@12                                             0261
         BE    @RF00261                                            0261
*       DO;                         /* JOBNAME                       */
*         JNM=TIOCNJOB;                                            0263
         MVC   JNM(8,CODEPTR),TIOCNJOB(@05)                        0263
*         CODEPTR=CODEPTR+8;        /* INCREMENT PTR                 */
         AL    CODEPTR,@CF00075                                    0264
*       END;                                                       0265
*   CODEPTR=CODEPTR+'01'X;          /* INCREMENT PTR                 */
*                                                                  0266
@RF00261 DS    0H                                                  0266
@RF00260 AL    CODEPTR,@CF00445                                    0266
*   /*****************************************************************/
*   /*                                                               */
*   /* SET UP POINTER TO ROUTE AND DESCRIPTOR CODES                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0267
*   RCODE2='00'X;                   /* ROUTE CODE                    */
         MVI   RCODE2(CODEPTR),X'00'                               0267
*   DCODE='4000'X;                  /* DESC CODE FOR IMMEDIATE       */
         MVC   DCODE(2,CODEPTR),@CB02626                           0268
*   MSMLWTO='1'B;                   /* INDICATE MLWTO        @ZA10122*/
         OI    MSMLWTO(MSGPTR),B'01000000'                         0269
*   MLCODE=DATALINE;                /* INDICATE DATA ONLY ACTION   0270
*                                      REQUIRED              @ZA10122*/
         MVC   MLCODE(4,CODEPTR),DATALINE                          0270
*   GEN(BAL CALLRG2,RDCODE);        /* GO ISSUE WTO AND RETURN       */
*                                                                  0271
         BAL CALLRG2,RDCODE
*   /*****************************************************************/
*   /*                                                               */
*   /* AFTER ISSUING FIRST LINE SET UP AND ISSUE SECOND LINE, SENSE  */
*   /* ONLY                                                          */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0272
*   MSID=MSG2ID;                    /* 2ND LINE INFO ONLF MSG      0272
*                                                            @YM06828*/
         MVC   MSID(8,MSGPTR),MSG2ID                               0272
*/*      D1=COMA;                     DELETED FOR APAR       @ZA25849*/
*                                                                  0273
*   D2='6B'X;                       /* COMMA FOR EMPTY FIELDS        */
         MVI   D2(MSGPTR),X'6B'                                    0273
*   D3='6B'X;                       /* COMMA FOR EMPTY FIELDS        */
         MVI   D3(MSGPTR),X'6B'                                    0274
*   D4='6B'X;                       /* COMMA FOR EMPTY FIELDS        */
         MVI   D4(MSGPTR),X'6B'                                    0275
*   GEN REFS(WSENS,WSENS1,WSENS2,WSENS3);                          0276
         UNPK WSENS(15,@10),EWASNS(8,@02)
         UNPK WSENS1(15,@10),EWASNS+7(8,@02)
         UNPK WSENS2(15,@10),EWASNS+14(8,@02)
         UNPK WSENS3(7,@10),EWASNS+21(4,@02)
         TR    WSENS(48,MSGPTR),TRTABLE-238
*   D5='6B'X;                       /* COMMA FOR EMPTY FIELDS        */
         MVI   D5(MSGPTR),X'6B'                                    0277
*   D6='6B'X;                       /* COMMA FOR EMPTY FIELDS        */
*                                                                  0278
         MVI   D6(MSGPTR),X'6B'                                    0278
*   /*****************************************************************/
*   /*                                                               */
*   /* SET UP REGISTERS FOR CODES                                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0279
*   RFY                                                            0279
*     CALLRG2 RSTD;                                                0279
*   CODEPTR=ADDR(D6)+'01'X;                                        0280
         LA    CODEPTR,D6(,MSGPTR)                                 0280
         AL    CODEPTR,@CF00445                                    0280
*   RCODE2='00'X;                   /* ROUTE CODE                    */
         MVI   RCODE2(CODEPTR),X'00'                               0281
*   DCODE='4000'X;                  /* DESCRIPTOR=ACTION             */
         MVC   DCODE(2,CODEPTR),@CB02626                           0282
*   CALLRG2=ADDR(SETUCB);           /* RETURN ADDRESS                */
         LA    CALLRG2,SETUCB                                      0283
*   MSMLWTO='1'B;                   /* INDICATE MLWTO        @ZA10122*/
         OI    MSMLWTO(MSGPTR),B'01000000'                         0284
*   MLCODE=ENDLINE;                 /* INDICATE LAST LINE OF DATA  0285
*                                                            @ZA10122*/
         MVC   MLCODE(4,CODEPTR),ENDLINE                           0285
*   GOTO RDCODE;                    /* GO WRITE MSG AND RETURN       */
         B     RDCODE                                              0286
*   RFY                                                            0287
*     CALLRG2 UNRSTD;                                              0287
*                                                                  0288
*   /*****************************************************************/
*   /*                                                               */
*   /* END OF SPECIAL WINCHESTER MESSAGE SENSE FOR NORMAL            */
*   /* INTERVENTION MSG GOES HERE UCBSNSCT HAS COUNT IF NOT A        */
*   /* WINCHESTER, CHECK FOR DA OR TAPE, IF ONE OR THE OTHER, GET VOL*/
*   /* SER FROM EWA                                                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0288
*NOTWIN:                                                           0288
*   IF UCBSNSCT<='02'X              /* TWO SENSE BYTES TO CONVERT    */
*     THEN                                                         0288
NOTWIN   L     @12,UCBEXTPT(,UCBPTR)                               0288
         CLI   UCBSNSCT(@12),2                                     0288
         BH    @RF00288                                            0288
*     DO;                                                          0289
*       GEN REFS(SNS,EWASNS,CM);                                   0290
               UNPK SNS(5,@10),EWASNS(3,@02)
               TR  CM(12,@10),TRTABLE-238
*       SC1='6B'X;                  /* COMMA AFTER SENSE             */
         MVI   SC1(MSGPTR),X'6B'                                   0291
*       CODEPTR=ADDR(SNS)+5;                                       0292
         LA    CODEPTR,SNS(,MSGPTR)                                0292
         AL    CODEPTR,@CF00123                                    0292
*       GOTO VOLSER;                /* PTR SET FOR VOL SERIAL        */
         B     VOLSER                                              0293
*     END;                                                         0294
*                                                                  0294
*   /*****************************************************************/
*   /*                                                               */
*   /* FIVE SENSE BYTES FOR MESSAGE                                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0295
*   GEN REFS(SNS,EWASNS,CM);                                       0295
@RF00288 DS    0H                                                  0295
         UNPK  SNS(11,MSGPTR),EWASNS(6,EWAPTR)
         TR    CM(18,MSGPTR),TRTABLE-238
*   SC2='6B'X;                                                     0296
         MVI   SC2(MSGPTR),X'6B'                                   0296
*   CODEPTR=ADDR(SNS)+11;           /* PTR TO VOL SER FIELD          */
*                                                                  0297
         LA    CODEPTR,SNS(,MSGPTR)                                0297
         AL    CODEPTR,@CF00118                                    0297
*   /*****************************************************************/
*   /*                                                               */
*   /* FOR DA OR TAPES, GET VOLUME SERIAL                            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0298
*VOLSER:                                                           0298
*   IF UCB3DACC='1'B                /* IF DA, GET VOL SER FROM       */
*     THEN                          /* EWA AND CHECK IT              */
VOLSER   TM    UCB3DACC(UCBPTR),B'00100000'                        0298
         BNO   @RF00298                                            0298
*     IF EWDCCW9^='000000000000'X THEN                             0299
         CLC   EWDCCW9(6,EWAPTR),@CB02623                          0299
         BE    @RF00299                                            0299
