*                                                                  0344
*/********************************************************************/
*/*              PARSE CONTINUE SUBCMD                               */
*/********************************************************************/
*                                                                  0344
*   PPLCBUF=CSPLCBUF;               /* ADDR OF CMD BUFFER            */
CONTIN   L     @14,PPLPTR                                          0344
         L     @10,CSPLPTR                                         0344
         MVC   PPLCBUF(4,@14),CSPLCBUF(@10)                        0344
*   PPLUWA=ZERO;                    /* ZERO VALIDITY CHECK PARM ADDR */
         SLR   @10,@10                                             0345
         ST    @10,PPLUWA(,@14)                                    0345
*   PPLPCL=VPARSCON;                /* ADDR OF PCE                   */
         MVC   PPLPCL(4,@14),VPARSCON                              0346
*   KEY1=NO;                        /* INDICATE KEY 0 FOR '67        */
         NI    KEY1(OCMTBPTR),B'01111111'                          0347
*   GEN(MODESET EXTKEY=SUPR);       /* KEY 0 FOR PARSE               */
         MODESET EXTKEY=SUPR
*   REG1=ADDR(PPL);                 /* PASS PARSE PARAM LIST         */
         L     REG1,PPLPTR                                         0349
*   GEN(LINK EP=IKJPARS);           /* LINK TO PARSE                 */
         LINK EP=IKJPARS
*   OUTRTCD=REG15;                  /* SAVE RET CODE AROUND MODESET  */
         STH   REG15,OUTRTCD(,OCMTBPTR)                            0351
*   GEN(MODESET EXTKEY=SCHED);      /* RETURN TO KEY 1               */
         MODESET EXTKEY=SCHED
*   KEY1=YES;                       /* INDICATE BACK TO KEY1 FOR '67 */
         OI    KEY1(OCMTBPTR),B'10000000'                          0353
*   REG5=OUTSBPDL;                  /* LOAD ADDR OF SUBCMD PDL       */
         L     REG5,OUTSBPDL(,OCMTBPTR)                            0354
*   GEN(USING CONTD,REG5);          /* PDL ADDRESSABILTY             */
         USING CONTD,REG5
*/********************************************************************/
*/*              CHECK PARSE RETURN CODE                             */
*/********************************************************************/
*                                                                  0356
*   IF OUTRTCD^=ZERO THEN           /* IF PARSE RC IS NON ZERO       */
         LH    @14,OUTRTCD(,OCMTBPTR)                              0356
         LTR   @14,@14                                             0356
         BZ    @RF00356                                            0356
*     DO;                                                          0357
*       IF OUTRTCD=8 THEN           /* ATTN DETECTED DURING PARS     */
         CH    @14,@CH00045                                        0358
         BNE   @RF00358                                            0358
*         DO;                                                      0359
*           CALL TCLRQSTK;          /* CALL RTN TO RESET STACK     0360
*                                                            @ZA15715*/
         BAL   @14,TCLRQSTK                                        0360
*           GOTO SCANSUB;           /* RETURN FOR NEW SUBCMD         */
         B     SCANSUB                                             0361
*         END;                                                     0362
*       ELSE                        /* ANY OTHER ERROR CONDITION     */
*         IF OUTRTCD=4 THEN         /* 4 - USER IN NOPROMPT MODE & 0363
*                                      COULD NOT BE PROMPTED         */
@RF00358 CLC   OUTRTCD(2,OCMTBPTR),@CH00051                        0363
         BNE   @RF00363                                            0363
*           IF NONTERM=YES THEN     /* INPUT NOT FROM TERMINAL       */
         TM    NONTERM(OCMTBPTR),B'00100000'                       0364
         BNO   @RF00364                                            0364
*             DO;                                                  0365
*               ERROR=YES;          /* QUIT OUTPUT                   */
         OI    ERROR(OCMTBPTR),B'00001000'                         0366
*               GOTO ENDRETRN;      /* RETURN TO '62                 */
         B     ENDRETRN                                            0367
*             END;                                                 0368
*           ELSE                    /* INPUT FROM TERMINAL           */
*             DO;                   /* GIVE USER ANOTHER CHANCE      */
@RF00364 DS    0H                                                  0370
*               GEN(TCLEARQ INPUT); /* FLUSH INPUT STACK             */
         TCLEARQ INPUT
*               OUTMSGID=0;         /* ISSUE MODE MSG                */
         MVI   OUTMSGID(OCMTBPTR),X'00'                            0371
*               CALL IKJCT467(OUTCOMTB);/* CALL MSG WRITER           */
         ST    OCMTBPTR,@AL00001                                   0372
         L     @15,@CV01609                                        0372
         LA    @01,@AL00001                                        0372
         BALR  @14,@15                                             0372
*               GOTO SCANSUB;       /* SCAN NEW SUBCMD               */
         B     SCANSUB                                             0373
*             END;                                                 0374
*         ELSE                      /* ELSE OTHER THAN 4 OR 8        */
*           DO;                                                    0375
@RF00363 DS    0H                                                  0376
*             OUTMSGID=12;          /* INDICATE PARSE ERROR          */
         MVI   OUTMSGID(OCMTBPTR),X'0C'                            0376
*             CALL IKJCT467(OUTCOMTB);/* CALL MSG WRITER             */
         ST    OCMTBPTR,@AL00001                                   0377
         L     @15,@CV01609                                        0377
         LA    @01,@AL00001                                        0377
         BALR  @14,@15                                             0377
*             IF ERROR=YES×NONTERM=YES THEN/* ERROR IN '67 OR INPUT
*                                      NOT FROM THE TERMINAL         */
         TM    ERROR(OCMTBPTR),B'00001000'                         0378
         BO    @RT00378                                            0378
         TM    NONTERM(OCMTBPTR),B'00100000'                       0378
         BNO   @RF00378                                            0378
@RT00378 DS    0H                                                  0379
*               DO;                                                0379
*                 ERROR=YES;        /* SET ERROR SWITCH ON           */
         OI    ERROR(OCMTBPTR),B'00001000'                         0380
*                 GOTO ENDRETRN;    /* GOTO END AND RETURN           */
         B     ENDRETRN                                            0381
*               END;                                               0382
*             ELSE                  /* MSG PUT OUT & TERM INPUT      */
*               DO;                 /* GIVE USER ANOTHER CHANCE AFTER
*                                      PARSE ERROR                   */
@RF00378 DS    0H                                                  0384
*                 GEN(TCLEARQ INPUT);/* FLUSH INPUT STACK            */
         TCLEARQ INPUT
*                 OUTMSGID=0;       /* ISSUE MODE MSG                */
         MVI   OUTMSGID(OCMTBPTR),X'00'                            0385
*                 CALL IKJCT467(OUTCOMTB);/* CALL MSG WRITER FOR   0386
*                                      PUTGET                        */
         ST    OCMTBPTR,@AL00001                                   0386
         L     @15,@CV01609                                        0386
         LA    @01,@AL00001                                        0386
         BALR  @14,@15                                             0386
*                 GOTO SCANSUB;     /* GO GET NEW CMD                */
         B     SCANSUB                                             0387
*               END;                                               0388
*           END;                                                   0389
*     END;                                                         0390
*                                                                  0391
*/********************************************************************/
*/*                                                                  */
*/*        DETERMINE THE OPERANDS ON THE CONTINUE SUBCOMMAND         */
*/*                                                                  */
*/********************************************************************/
*                                                                  0391
*   IF CONTPAUS>ZERO THEN           /* PAUSE OR NOPAUSE ENTERED      */
@RF00356 LH    @14,CONTPAUS                                        0391
         LTR   @14,@14                                             0391
         BNP   @RF00391                                            0391
*     DO;                                                          0392
*       IF CONTPAUS=1 THEN          /* PAUSE ENTERED                 */
         CH    @14,@CH00033                                        0393
         BNE   @RF00393                                            0393
*         PAUSE=YES;                /* MAKE SURE PAUSE BIT IS ON     */
         OI    PAUSE(OCMTBPTR),B'10000000'                         0394
*       ELSE                        /* ELSE NOPAUSE ENTERED          */
*         PAUSE=NO;                 /* MAKE SURE PAUSE BIT IS OFF    */
         B     @RC00393                                            0395
@RF00393 NI    PAUSE(OCMTBPTR),B'01111111'                         0395
*     END;                                                         0396
@RC00393 DS    0H                                                  0397
*   SUBCONT=YES;                    /* SET SUBCOMMAND SW ON          */
@RF00391 DS    0H                                                  0398
*   SUBHERE=NO;                     /* CLEAR HERE                    */
*   SUBBEGN=NO;                     /* BEGIN                         */
*   SUBNEXT=NO;                     /* AND NEXT SWITCHES             */
         OI    SUBCONT(OCMTBPTR),B'00100000'                       0400
         NI    SUBHERE(OCMTBPTR),B'11100011'                       0400
*   IF CONTHERE<2 THEN              /* NEXT SPECIFIED (=1) OR        */
         LH    @14,CONTHERE                                        0401
         CH    @14,@CH00066                                        0401
         BNL   @RF00401                                            0401
*     DO;                           /* DEFAULTED (=0)                */
*       EODSW=NO;                   /* MAKE SURE EODSW IS OFF        */
         NI    EODSW(OCMTBPTR),B'11101111'                         0403
*       SUBNEXT=YES;                /* SET BIT IN OCMTB              */
         OI    SUBNEXT(OCMTBPTR),B'00000100'                       0404
*       GOTO ENDRETRN;              /* RETURN TO CALLER              */
         B     ENDRETRN                                            0405
*     END;                                                         0406
*   IF CONTHERE=2 THEN              /* HERE ENTERED                  */
@RF00401 CLC   CONTHERE(2),@CH00066                                0407
         BNE   @RF00407                                            0407
*     DO;                                                          0408
*       SUBHERE=YES;                /* INDICATE HERE IN OUTCOMTB     */
         OI    SUBHERE(OCMTBPTR),B'00010000'                       0409
*       GOTO ENDRETRN;              /* LEAVE SUBCMD MODE             */
         B     ENDRETRN                                            0410
*     END;                                                         0411
*   ELSE                            /* MUST BE BEGIN                 */
*     SUBBEGN=YES;                  /* INDICATE BEGIN                */
@RF00407 OI    SUBBEGN(OCMTBPTR),B'00001000'                       0412
*   GOTO ENDRETRN;                  /* RETURN TO CALLER              */
         B     ENDRETRN                                            0413
*                                                                  0414
*/********************************************************************/
*/*                                                                  */
*/*     HELP SUB COMMAND                                             */
*/*          FOR INTEGRITY REASONS 'HELP' WILL BE ATTACHED           */
*/*          IN PROBLEM PROGRAM STATE KEY 8. TO AVOID FETCH          */
*/*          PROTECT PROBLEMS, THE HELP PARAMETER LIST MUST BE       */
*/*          IN KEY 8 CORE (SUBPOOL 78).                             */
*/*                                                                  */
*/********************************************************************/
*                                                                  0414
*HELP:                                                             0414
*   KEY1=NO;                        /* INDICATE NOT IN KEY 1         */
HELP     NI    KEY1(OCMTBPTR),B'01111111'                          0414
*   GEN(MODESET EXTKEY=SUPR);       /* GO TO KEY0 TO WRITE IN KEY8 0415
*                                      CORE                          */
         MODESET EXTKEY=SUPR
*   REG1=LENGTH(CPPL)+CMDBUFLN+4;   /* LENGTH FOR GETMAIN - CPPL,  0416
*                                      CBUF, ECB                     */
         L     @14,OUTSBBUF(,OCMTBPTR)                             0416
         LA    REG1,20                                             0416
         MVC   @TF00001(2),CMDBUFLN(@14)                           0416
         AH    REG1,@TF00001                                       0416
*   SP78LEN=REG1;                   /* SAVE LENGTH OF SP78 CORE      */
         STH   REG1,SP78LEN                                        0417
*   GEN(GETMAIN R,LV=(1),SP=78);    /* GET CORE IN SP 78 (KEY8) FOR
*                                      HELP PARMLIST                 */
         GETMAIN R,LV=(1),SP=78
*   SP78PTR=REG1;                   /* BASE MAP FOR SP78 CORE        */
         LR    SP78PTR,REG1                                        0419
*   SP78CPPL=CPPL;                  /* COPY TMP'S CPPL INTO KEY8 CORE*/
         L     @14,CPPLPTR                                         0420
         MVC   SP78CPPL(16,SP78PTR),CPPL(@14)                      0420
*   SP78CBUF=ADDR(SP78BUFF);        /* SET CPPL TO POINT TO SUBCMD 0421
*                                      BUFFER FOR THE HELP CP        */
         LA    @14,SP78BUFF(,SP78PTR)                              0421
         ST    @14,@TF00001                                        0421
         MVC   SP78CBUF(4,SP78PTR),@TF00001                        0421
*   SP78BUFF(1:SP78LEN-(LENGTH(CPPL)+4))=CMDBUFF(1:CMDBUFLN);/* COPY
*                                      CMDBUFFER INTO KEY8 CORE      */
         LH    @14,SP78LEN                                         0422
         SH    @14,@CH01732                                        0422
         L     @10,OUTSBBUF(,OCMTBPTR)                             0422
         EX    @14,@SM01756                                        0422
*   SP78ECB=ZERO;                   /* ZERO ECB                      */
         SLR   @14,@14                                             0423
         ST    @14,@TF00001                                        0423
         MVC   SP78ECB(4,SP78PTR),@TF00001                         0423
*   OHELPECB=ADDR(SP78ECB);         /* SAVE ADDR OF ECB FOR '72      */
         LA    @14,SP78ECB(,SP78PTR)                               0424
         ST    @14,OHELPECB(,OCMTBPTR)                             0424
*   ATCHLSTD(1:ATCHLNTH)=ATCHLIST(1:ATCHLNTH);/* MAP INTO DYN CORE   */
         L     @10,ATCHLNTH                                        0425
         BCTR  @10,0                                               0425
         EX    @10,@SM01758                                        0425
*   REG15=ADDR(ATCHLSTD);           /* LIST FORM OF ATTACH           */
         LA    REG15,ATCHLSTD                                      0426
*   REG2=ADDR(SP78ECB);             /* PTR TO ECB FOR ATTACH         */
         LR    REG2,@14                                            0427
*   REG1=SP78PTR;                   /* PARAMETERS FOR HELP           */
         LR    REG1,SP78PTR                                        0428
*   REG5=CVTLINK;                   /* GET DCB ADDR FOR LINKLIB      */
         L     @14,CVTPTR                                          0429
         L     REG5,CVTLINK(,@14)                                  0429
*/********************************************************************/
*/*             ATTACH HELP CP AND CHECK RETURN CODES                */
*/*                   'HELP' IS ATTACHED NONDISPATCHABLE SO THAT     */
*/*                   THE TCB KEY CAN BE MODIFIED TO INDICATE KEY 8  */
*/*                   THE TCBECBNV FLAG IS SET TO INDICATE NO KEY    */
*/*                   VERIFICATION ON THE DETACH (BECAUSE THE ECB    */
*/*                   IS IN KEY 1, VERIFICATION WILL CAUSE AN ABEND) */
*/********************************************************************/
*                                                                  0430
*   GEN;                                                           0430
         ATTACH EP=HELP,ECB=(2),MF=(E,(1)),SF=(E,(15)),                *
               STAI=(HELPSTAI),SHSPV=78,DISP=NO,DCB=(5),SZERO=NO
*   OUTRTCD=REG15;                  /* SAVE RETURN CODE              */
         STH   REG15,OUTRTCD(,OCMTBPTR)                            0431
*   IF OUTRTCD^=ZERO THEN           /* NONZERO RETURN CODE FROM    0432
*                                      ATTACH                        */
         LH    @14,OUTRTCD(,OCMTBPTR)                              0432
         LTR   @14,@14                                             0432
         BZ    @RF00432                                            0432
*     DO;                                                          0433
*       REG1=SP78LEN;               /* LENGTH OF SP78 CORE           */
         LH    REG1,SP78LEN                                        0434
*       REG2=SP78PTR;               /* ADDR OF SP78 CORE             */
         LR    REG2,SP78PTR                                        0435
*       GEN(FREEMAIN R,LV=(1),A=(2),SP=78);/* FREE SP78 CORE         */
         FREEMAIN R,LV=(1),A=(2),SP=78
*       GEN(MODESET EXTKEY=SCHED);  /* GO BACK TO KEY1               */
         MODESET EXTKEY=SCHED
*       KEY1=YES;                   /* INDICATE BACK IN KEY1         */
         OI    KEY1(OCMTBPTR),B'10000000'                          0438
*       OUTMSGID=4;                 /* MSG FOR ERROR DURING ATTACH   */
         MVI   OUTMSGID(OCMTBPTR),X'04'                            0439
*       CALL IKJCT467(OUTCOMTB);    /* CALL MSG WRITER               */
         ST    OCMTBPTR,@AL00001                                   0440
         L     @15,@CV01609                                        0440
         LA    @01,@AL00001                                        0440
         BALR  @14,@15                                             0440
*       IF ERROR=YES THEN           /* ERROR IN MSG HANDLER          */
         TM    ERROR(OCMTBPTR),B'00001000'                         0441
         BO    @RT00441                                            0441
*         GOTO ENDRETRN;            /* GOTO END AND RETURN TO CALLER */
*       ELSE                        /* MSG HANDLER SUCCESSFULLY    0443
*                                      ISSUED ATTACH ERROR MSG       */
*         DO;                       /* ISSUE MODE MSG WITHOUT CHECK
*                                      FOR NON TERMINAL INPUT SINCE
*                                      IT IS UNLIKELY ANY SUBSEQUENT
*                                      CMD IN A CLIST DEPENDS ON HELP*/
*           OUTMSGID=0;             /* ISSUE MODE MSG                */
         MVI   OUTMSGID(OCMTBPTR),X'00'                            0444
*           CALL IKJCT467(OUTCOMTB);/* CALL MSG WRITER               */
         ST    OCMTBPTR,@AL00001                                   0445
         L     @15,@CV01609                                        0445
         LA    @01,@AL00001                                        0445
         BALR  @14,@15                                             0445
*           GOTO SCANSUB;           /* GO RETRIEVE NEXT SUBCMD       */
         B     SCANSUB                                             0446
*         END;                                                     0447
*     END;                                                         0448
*   TCBPTR=REG1;                    /* BASE HELP TCB                 */
@RF00432 ST    REG1,TCBPTR                                         0449
*   OUTTCBH=REG1;                   /* PUT TCB ADDR IN OCMTB         */
         ST    REG1,OUTTCBH(,OCMTBPTR)                             0450
*   TCBFLAG='1000'B;                /* SET KEY8 PROB PGM IN TCB      */
         L     @14,TCBPTR                                          0451
         NI    TCBFLAG(@14),B'10001111'                            0451
         OI    TCBFLAG(@14),B'10000000'                            0451
*   TCBECBNV='1'B;                  /* DO NOT VERIFY ECB KEY FOR   0452
*                                      DETACH                        */
         OI    TCBECBNV(@14),B'00000100'                           0452
*/********************************************************************/
*/*         ISSUE STATUS RESET TO ALLOW 'HELP' TO RUN                */
*/********************************************************************/
*                                                                  0453
*   GEN(STATUS RESET,ND,(1),(12),E);/* START HELP CP                 */
         STATUS RESET,ND,(1),(12),E
*   OUTRTCD=REG15;                  /* SAVE RETURN CODE              */
         STH   REG15,OUTRTCD(,OCMTBPTR)                            0454
*   IF OUTRTCD^=0 THEN              /* IF NONZERO RETURN CODE        */
         LH    @14,OUTRTCD(,OCMTBPTR)                              0455
         LTR   @14,@14                                             0455
         BZ    @RF00455                                            0455
*     DO;                                                          0456
*       REG2=SP78PTR;               /* ADDR OF SP 78 CORE            */
         LR    REG2,SP78PTR                                        0457
*       REG1=SP78LEN;               /* LEN OF SP78 CORE              */
         LH    REG1,SP78LEN                                        0458
*       GEN(FREEMAIN R,LV=(1),A=(2),SP=78);/* FREE SP78 CORE         */
         FREEMAIN R,LV=(1),A=(2),SP=78
*       GEN(MODESET EXTKEY=SCHED);  /* GO BACK TO KEY1               */
         MODESET EXTKEY=SCHED
*       KEY1=YES;                   /* BACK IN KEY1                  */
         OI    KEY1(OCMTBPTR),B'10000000'                          0461
*       OUTMSGID=48;                /* STATUS ERROR MSG              */
         MVI   OUTMSGID(OCMTBPTR),X'30'                            0462
*       CALL IKJCT467(OUTCOMTB);    /* CALL MSG WRITER               */
         ST    OCMTBPTR,@AL00001                                   0463
         L     @15,@CV01609                                        0463
         LA    @01,@AL00001                                        0463
         BALR  @14,@15                                             0463
*       GOTO DETHELP;               /* DETACH HELP CP & ISSUE MODE 0464
*                                      MSG                           */
         B     DETHELP                                             0464
*     END;                                                         0465
*                                                                  0465
*/********************************************************************/
*/*             ISSUE WAIT  AND CHECK ECB RETURN CODES               */
*/********************************************************************/
*                                                                  0466
*   REG2=ADDR(SP78ECB);             /* PTR TO ECB FOR WAIT           */
@RF00455 LA    REG2,SP78ECB(,SP78PTR)                              0466
*   GEN(WAIT ECB=(2),LONG=YES);     /* WAIT FOR COMPLETION OF HELP   */
         WAIT ECB=(2),LONG=YES
*   MVO(ATCHCODE,ATCHCODE);         /* LEFT JUSTIFY ABEND CODE (IF 0468
*                                      PRESENT) SO THAT IT CAN BE  0468
*                                      INSERTED INTO A MSG.          */
         MVO   ATCHCODE(3,SP78PTR),ATCHCODE(3,SP78PTR)             0468
*   OUTRTCD=ABENDCDE;               /* SAVE HELP COMP CODE FOR '67 0469
*                                                            @OZ00849*/
         MVC   OUTRTCD(2,OCMTBPTR),ABENDCDE(SP78PTR)               0469
*   REG2=SP78PTR;                   /* ADDR OF SP 78 CORE            */
         LR    REG2,SP78PTR                                        0470
*   REG1=SP78LEN;                   /* LENGTH OF SP78 CORE           */
         LH    REG1,SP78LEN                                        0471
*   GEN(FREEMAIN R,LV=(1),A=(2),SP=78);/* FREE SP 78 CORE            */
         FREEMAIN R,LV=(1),A=(2),SP=78
*   GEN(MODESET EXTKEY=SCHED);      /* RETURN TO KEY1                */
         MODESET EXTKEY=SCHED
*   KEY1=YES;                       /* INDICATE KEY1 FOR '67         */
         OI    KEY1(OCMTBPTR),B'10000000'                          0474
*   IF OUTRTCD^=ZERO THEN           /* IF HELP ABENDED       @OZ00849*/
         LH    @14,OUTRTCD(,OCMTBPTR)                              0475
         LTR   @14,@14                                             0475
         BZ    @RF00475                                            0475
*     DO;                                                          0476
*       OUTMSGID=36;                /* HELP ABEND MSG                */
         MVI   OUTMSGID(OCMTBPTR),X'24'                            0477
*       CALL IKJCT467(OUTCOMTB);    /* CALL MSG HANDLER              */
         ST    OCMTBPTR,@AL00001                                   0478
         L     @15,@CV01609                                        0478
         LA    @01,@AL00001                                        0478
         BALR  @14,@15                                             0478
*       IF ERROR=YES THEN           /* DID '67 HAVE ANY ERRORS       */
         TM    ERROR(OCMTBPTR),B'00001000'                         0479
         BO    @RT00479                                            0479
*         GOTO ENDRETRN;            /* GO TO END AND RETURN          */
*       ELSE                        /* IF ERROR MSG PUT TO USER      */
*         DO;                                                      0481
*           OUTMSGID=0;             /* MODE MSG ID                   */
         MVI   OUTMSGID(OCMTBPTR),X'00'                            0482
*           CALL IKJCT467(OUTCOMTB);/* CALL MSG WRITER TO PUT MODE 0483
*                                      MSG                           */
         ST    OCMTBPTR,@AL00001                                   0483
         L     @15,@CV01609                                        0483
         LA    @01,@AL00001                                        0483
         BALR  @14,@15                                             0483
*           GOTO SCANSUB;           /* RETURN TO TOP OF CODE TO GET
*                                      NEXT COMMAND                  */
         B     SCANSUB                                             0484
*         END;                                                     0485
*     END;                                                         0486
*                                                                  0487
*/********************************************************************/
*/*             CALL DAIR TO PURGE HELP'S DSE                        */
*/********************************************************************/
*                                                                  0487
*   DAPL(1:12)=PPL(1:12);           /* INITIALIZE DAPL               */
@RF00475 L     @14,DAPLPTR                                         0487
         L     @10,PPLPTR                                          0487
         MVC   DAPL(12,@14),PPL(@10)                               0487
*   DAPLPSCB=CPPLPSCB;              /* ....                          */
         L     @10,CPPLPTR                                         0488
         MVC   DAPLPSCB(4,@14),CPPLPSCB(@10)                       0488
*   DAPLDAPB=ADDR(DAPB2C);          /* ....                          */
         L     @10,DAPB2PTR                                        0489
         ST    @10,DAPLDAPB(,@14)                                  0489
*   DA2CCD='002C'X;                 /* FILL IN PURGE DSE CODE        */
         MVC   DA2CCD(2,@10),@CB01722                              0490
*   DA2CTCB=OUTTCBH;                /* ADDR OF SUBTASK TCB           */
         MVC   DA2CTCB(4,@10),OUTTCBH(OCMTBPTR)                    0491
