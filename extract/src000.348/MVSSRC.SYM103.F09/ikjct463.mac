@RF00318 DS    0H                                                  0333
OPENEXIT DS    0H                                                  0334
*   IF DCBRECFM=ZERO THEN           /* IF RECFM OF DCB = '00'B       */
         L     @10,DCBPTR                                          0334
         LA    @04,28                                              0334
         ALR   @04,@10                                             0334
         CLI   DCBRECFM(@04),0                                     0334
         BNE   @RF00334                                            0334
*     DO;                                                          0335
*       DCBLRECL=132;               /* STANDARD SYSOUT PRINTER LINE  */
         MVC   DCBLRECL+72(2,@10),@CH01706                         0336
*       DCBRECFM='98'X;             /* RECFM = FBS                   */
         MVI   DCBRECFM(@04),X'98'                                 0337
*       DCBBLKSI=3036;              /* 23 REC PER BLOCK              */
         MVC   DCBBLKSI+52(2,@10),@CH01714                         0338
*     END;                                                         0339
*   GEN(BR  REG14);                 /* RETURN TO OPEN MACRO          */
@RF00334 DS    0H                                                  0340
         BR  REG14
