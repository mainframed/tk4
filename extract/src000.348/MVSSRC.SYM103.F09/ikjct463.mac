         TITLE 'IKJCT463 - OUTPUT SUBCOMMAND HANDLER                   *
                        '
IKJCT463 CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'IKJCT463  77.006'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @12,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@12                                         0001
         L     @00,@SIZDATD                                        0001
         GETMAIN  R,LV=(0)
         LR    @11,@01                                             0001
         USING @DATD,@11                                           0001
         ST    @13,@SA00001+4                                      0001
         LM    @00,@01,20(@13)                                     0001
         ST    @11,8(,@13)                                         0001
         LR    @13,@11                                             0001
         XC    @ZTEMPS(@ZLEN),@ZTEMPS
         MVC   @PC00001(4),0(@01)                                  0001
*                                                                  0055
*/********************************************************************/
*/*                                                                  */
*/*                                                                  */
*/*      B E G I N   C O D E                                         */
*/*                                                                  */
*/*                                                                  */
*/********************************************************************/
*                                                                  0055
*   OCMTBPTR=OUTP;                  /* SAVE PARAMETER FROM CALLER    */
         L     @14,@PC00001                                        0055
         L     OCMTBPTR,OUTP(,@14)                                 0055
*   SUBCMODE=YES;                   /* INDICATE IN SUBCMD MODE       */
         OI    SUBCMODE(OCMTBPTR),B'01000000'                      0056
*/********************************************************************/
*/*       BUILD COMMAND SCAN AND PARSE PARAMETER LISTS               */
*/********************************************************************/
*                                                                  0057
*   CPPLPTR=OUTCPPL;                /* ADDR FOR CPPL DSECT           */
         L     @14,OUTCPPL(,OCMTBPTR)                              0057
         ST    @14,CPPLPTR                                         0057
*   CSPLPTR=ADDR(CSPLAREA);         /* BASE CSPL ON CORE GOTTEN IN 63*/
         LA    @10,CSPLAREA                                        0058
         ST    @10,CSPLPTR                                         0058
*   CSPLUPT=CPPLUPT;                /* FILL IN ADDR OF UPT           */
         L     @09,CPPLUPT(,@14)                                   0059
         ST    @09,CSPLUPT(,@10)                                   0059
*   CSPLECT=CPPLECT;                /* FILL IN ADDR OF ECT           */
         L     @14,CPPLECT(,@14)                                   0060
         ST    @14,CSPLECT(,@10)                                   0060
*   CSPLECB=ADDR(OUTATTN);          /* .....                         */
         LA    @08,OUTATTN(,OCMTBPTR)                              0061
         ST    @08,CSPLECB(,@10)                                   0061
*   CSOAPTR=ADDR(CSOAAREA);         /* BASE CSOA ON DYNAMIC AREA     */
         LA    CSOAPTR,CSOAAREA                                    0062
*   CSPLOA=ADDR(CSOA);              /* ADDR OF DATA AREA FOR PARMLIST*/
         ST    CSOAPTR,CSPLOA(,@10)                                0063
*   CSOA=CSOA&&CSOA;                /* ZERO CMD SCAN OUTPUT AREA     */
         XC    CSOA(8,CSOAPTR),CSOA(CSOAPTR)                       0064
*   PPLPTR=ADDR(PPLAREA);           /* BASE PARSE PARM LIST          */
         LA    @10,PPLAREA                                         0065
         ST    @10,PPLPTR                                          0065
*   PPL=PPL&&PPL;                   /* ZERO PARSE PARM LIST          */
         XC    PPL(28,@10),PPL(@10)                                0066
*   PPLUPT=CPPLUPT;                 /* .....                         */
         ST    @09,PPLUPT(,@10)                                    0067
*   PPLECT=CPPLECT;                 /* .....                         */
         ST    @14,PPLECT(,@10)                                    0068
*   PPLECB=ADDR(OUTATTN);           /* OUTATTN FIELD IN OCMTB IS ECB */
         ST    @08,PPLECB(,@10)                                    0069
*   PPLANS=ADDR(OUTSBPDL);          /* SAVE PDE ADDR IN OUTCOMTB     */
         LA    @14,OUTSBPDL(,OCMTBPTR)                             0070
         ST    @14,PPLANS(,@10)                                    0070
*/********************************************************************/
*/*         SET BASES FOR CONTROL BLOCKS                             */
*/********************************************************************/
*                                                                  0071
*   DCBPTR=ADDR(DCBOUTD);           /* BASE IHADCB ON DYNAMIC CORE   */
         LA    @14,DCBOUTD                                         0071
         ST    @14,DCBPTR                                          0071
*   DAPLPTR=ADDR(DAPLAREA);         /* BASE DAPL MAP ON DYNAMIC AREA */
         LA    @14,DAPLAREA                                        0072
         ST    @14,DAPLPTR                                         0072
*   DAPB2PTR=ADDR(DAIRAREA);        /* BASE DAIR BLOCK ON DYNAMIC  0073
*                                      AREA                          */
         LA    @14,DAIRAREA                                        0073
         ST    @14,DAPB2PTR                                        0073
*                                                                  0074
*/********************************************************************/
*/*                                                                  */
*/*                   SCAN THE SUBCOMMAND                            */
*/*                                                                  */
*/********************************************************************/
*                                                                  0074
*SCANSUB:                           /* RETURN HERE TO SCAN ANY NEW 0074
*                                      SUBCOMMAND                    */
*   CSPLCBUF=OUTSBBUF;              /* ADDR NEW SUBCMD FOR CMD SCAN  */
SCANSUB  L     @14,CSPLPTR                                         0074
         MVC   CSPLCBUF(4,@14),OUTSBBUF(OCMTBPTR)                  0074
*   CSPLFLG=ADDR(OUTWORK);          /* ADDR OF REQUEST FLAG          */
         LA    @10,OUTWORK(,OCMTBPTR)                              0075
         ST    @10,CSPLFLG(,@14)                                   0075
*   OUTWORK=ZERO;                   /* SET INDICATOR FLAG TO SYNTAX
*                                      CHECK REQUEST                 */
         SLR   @14,@14                                             0076
         ST    @14,OUTWORK(,OCMTBPTR)                              0076
*   POSTED=NO;                      /* BE SURE BIT OFF. THEN IF IT 0077
*                                      SHOULD BE ON AFTER CMD SCAN,
*                                      IT'S DUE TO A NEW SUBCMD    0077
*                                      ENTERED AND NOT DUE TO THE  0077
*                                      LAST SUBCMD JUST SCANNED. A 0077
*                                      RESCAN OF A SUBCMD CAUSES   0077
*                                      PROBLEMS DUE TO BUFFER OFFSETS*/
         NI    POSTED(OCMTBPTR),B'10111111'                        0077
*   KEY1=NO;                        /* INCICATE KEY0 FOR '67         */
         NI    KEY1(OCMTBPTR),B'01111111'                          0078
*   GEN(MODESET EXTKEY=SUPR);       /* OBTAIN KEY0 FOR CMD SCAN      */
         MODESET EXTKEY=SUPR
*   REG1=ADDR(CSPL);                /* PARM FOR CMD SCAN             */
         L     REG1,CSPLPTR                                        0080
*   GEN(LINK EP=IKJSCAN);           /* LINK TO CMD SCAN              */
         LINK EP=IKJSCAN
*   OUTRTCD=REG15;                  /* SAVE RETURN CODE              */
         STH   REG15,OUTRTCD(,OCMTBPTR)                            0082
*   GEN(MODESET EXTKEY=SCHED);      /* RETURN TO KEY 1               */
         MODESET EXTKEY=SCHED
*   KEY1=YES;                       /* INDICATE KEY 1 FOR '67        */
         OI    KEY1(OCMTBPTR),B'10000000'                          0084
*                                                                  0085
*/********************************************************************/
*/*          CHECK COMMAND SCAN RESULTS                              */
*/********************************************************************/
*                                                                  0085
*   IF POSTED=YES THEN              /* IF ATTN WAS STRUCK            */
         TM    POSTED(OCMTBPTR),B'01000000'                        0085
         BNO   @RF00085                                            0085
*     DO;                                                          0086
*       CALL TCLRQSTK;              /* CALL RTN, RESET STACK @ZA15715*/
         BAL   @14,TCLRQSTK                                        0087
*       GOTO SCANSUB;               /* GO LOOK FOR NEXT CMD          */
         B     SCANSUB                                             0088
*     END;                                                         0089
*   IF OUTRTCD^=ZERO×CSOABAD='1'B THEN/* SOME KIND OF ERROR FROM   0090
*                                      CMDSCAN                       */
@RF00085 LH    @14,OUTRTCD(,OCMTBPTR)                              0090
         LTR   @14,@14                                             0090
         BNZ   @RT00090                                            0090
         TM    CSOABAD(CSOAPTR),B'00001000'                        0090
         BNO   @RF00090                                            0090
@RT00090 DS    0H                                                  0091
*     DO;                                                          0091
*       IF CSOABAD='1'B THEN        /* IF SYNTACTICALLY INVALID CMD  */
         TM    CSOABAD(CSOAPTR),B'00001000'                        0092
         BNO   @RF00092                                            0092
*         DO;                                                      0093
*           OUTMSGID=128;           /* INVALID SUBCMD MSG 323I       */
         MVI   OUTMSGID(OCMTBPTR),X'80'                            0094
*           CALL IKJCT467(OUTCOMTB);/* CALL MSG WRITER               */
         ST    OCMTBPTR,@AL00001                                   0095
         L     @15,@CV01609                                        0095
         LA    @01,@AL00001                                        0095
         BALR  @14,@15                                             0095
*           IF NONTERM=YES×ERROR=YES THEN/* INPUT NOT FROM TERMINAL
*                                      OR MSG HANDLER COULDN'T PUT 0096
*                                      MSG                           */
         TM    NONTERM(OCMTBPTR),B'00100000'                       0096
         BO    @RT00096                                            0096
         TM    ERROR(OCMTBPTR),B'00001000'                         0096
         BNO   @RF00096                                            0096
@RT00096 DS    0H                                                  0097
*             DO;                                                  0097
*               ERROR=YES;          /* INDICATE ERROR CONDITION      */
         OI    ERROR(OCMTBPTR),B'00001000'                         0098
*               GOTO ENDRETRN;      /* QUIT PROCESSING               */
         B     ENDRETRN                                            0099
*             END;                                                 0100
*           ELSE                    /* WROTE MSG OK & INPUT FROM TERM*/
*             DO;                   /* GIVE USER ANOTHER CHANCE      */
@RF00096 DS    0H                                                  0102
*               GEN(TCLEARQ INPUT); /* FLUSH INPUT STACK             */
         TCLEARQ INPUT
*               OUTMSGID=0;         /* MODE MSG 300A                 */
         MVI   OUTMSGID(OCMTBPTR),X'00'                            0103
*               CALL IKJCT467(OUTCOMTB);/* CALL MSG WRITER           */
         ST    OCMTBPTR,@AL00001                                   0104
         L     @15,@CV01609                                        0104
         LA    @01,@AL00001                                        0104
         BALR  @14,@15                                             0104
*               GOTO SCANSUB;       /* GO SCAN NEW SUBCMD            */
         B     SCANSUB                                             0105
*             END;                                                 0106
*         END;                                                     0107
*       ELSE                        /* IF RETURN CODE NON ZERO       */
*         DO;                                                      0108
@RF00092 DS    0H                                                  0109
*           ERROR=YES;              /* SERIOUS ERROR                 */
         OI    ERROR(OCMTBPTR),B'00001000'                         0109
*           OUTMSGID=20;            /* CMD SYS ERROR MSG             */
         MVI   OUTMSGID(OCMTBPTR),X'14'                            0110
*           CALL IKJCT467(OUTCOMTB);/* CALL MSG WRITER               */
         ST    OCMTBPTR,@AL00001                                   0111
         L     @15,@CV01609                                        0111
         LA    @01,@AL00001                                        0111
         BALR  @14,@15                                             0111
*           GOTO ENDRETRN;          /* RETURN                        */
         B     ENDRETRN                                            0112
*         END;                                                     0113
*     END;                                                         0114
*   IF CSOANOC='1'B THEN            /* NULL LINE ENTERED             */
@RF00090 TM    CSOANOC(CSOAPTR),B'00010000'                        0115
         BNO   @RF00115                                            0115
*     DO;                                                          0116
*       SUBCONT=YES;                /* INDICATE 'CONTINUE' WAS     0117
*                                      ENTERED                       */
*       SUBHERE=YES;                /* DEFAULT IS 'HERE'             */
*       SUBNEXT=NO;                 /* MAKE SURE BITS ARE OFF        */
*       SUBBEGN=NO;                 /* .....                         */
         OI    SUBCONT(OCMTBPTR),B'00110000'                       0120
         NI    SUBNEXT(OCMTBPTR),B'11110011'                       0120
*       GOTO ENDRETRN;              /* RETURN TO '62 & PROCESS 'CONT'*/
         B     ENDRETRN                                            0121
*     END;                                                         0122
*   IF ERROR=YES THEN               /* IF ERRORS DETECTED ANYWHERE   */
@RF00115 TM    ERROR(OCMTBPTR),B'00001000'                         0123
         BO    @RT00123                                            0123
*     GOTO ENDRETRN;                /* RETURN TO CALLER              */
*                                                                  0125
*/********************************************************************/
*/*                                                                  */
*/*   DETERMINE WHICH SUBCMD IS IN THE BUFFER                        */
*/*                                                                  */
*/********************************************************************/
*                                                                  0125
*   /*****************************************************************/
*   /*                                                               */
*   /* IF 'C' OR 'CONTINUE' ENTERED                                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0125
*   IF(CSOALNM=1&SUBCMND='C')×(CSOALNM=8&SUBCMND='CONTINUE') THEN  0125
         CLC   CSOALNM(2,CSOAPTR),@CH00033                         0125
         BNE   @GL00003                                            0125
         L     @14,CSOACNM(,CSOAPTR)                               0125
         CLI   SUBCMND(@14),C'C'                                   0125
         BE    @RT00125                                            0125
@GL00003 CLC   CSOALNM(2,CSOAPTR),@CH00045                         0125
         BNE   @RF00125                                            0125
         L     @14,CSOACNM(,CSOAPTR)                               0125
         CLC   SUBCMND(8,@14),@CC01689                             0125
         BE    @RT00125                                            0125
*     GOTO CONTIN;                  /* PROCESS CONTINUE CMD          */
*                                                                  0126
*   /*****************************************************************/
*   /*                                                               */
*   /* IF 'S' OR 'SAVE' ENTERED                                      */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0127
*   IF(CSOALNM=1&SUBCMND='S')×(CSOALNM=4&SUBCMND='SAVE') THEN      0127
@RF00125 CLC   CSOALNM(2,CSOAPTR),@CH00033                         0127
         BNE   @GL00006                                            0127
         L     @14,CSOACNM(,CSOAPTR)                               0127
         CLI   SUBCMND(@14),C'S'                                   0127
         BE    @RT00127                                            0127
@GL00006 CLC   CSOALNM(2,CSOAPTR),@CH00051                         0127
         BNE   @RF00127                                            0127
         L     @14,CSOACNM(,CSOAPTR)                               0127
         CLC   SUBCMND(4,@14),@CC01692                             0127
         BE    @RT00127                                            0127
*     GOTO SAVE;                    /* GOTO SAVE CMD HANDLER         */
*                                                                  0128
*   /*****************************************************************/
*   /*                                                               */
*   /* IF 'END' ENTERED                                              */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0129
*   IF CSOALNM=3&SUBCMND='END' THEN                                0129
@RF00127 CLC   CSOALNM(2,CSOAPTR),@CH00268                         0129
         BNE   @RF00129                                            0129
         L     @14,CSOACNM(,CSOAPTR)                               0129
         CLC   SUBCMND(3,@14),@CC01694                             0129
         BE    @RT00129                                            0129
*     GOTO ENDCMD;                  /* GOTO END CMD HANDLER          */
*                                                                  0130
*   /*****************************************************************/
*   /*                                                               */
*   /* IF 'H' OR 'HELP' ENTERED                                      */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0131
*   IF(CSOALNM=1&SUBCMND='H')×(CSOALNM=4&SUBCMND='HELP') THEN      0131
@RF00129 CLC   CSOALNM(2,CSOAPTR),@CH00033                         0131
         BNE   @GL00010                                            0131
         L     @14,CSOACNM(,CSOAPTR)                               0131
         CLI   SUBCMND(@14),C'H'                                   0131
         BE    @RT00131                                            0131
@GL00010 CLC   CSOALNM(2,CSOAPTR),@CH00051                         0131
         BNE   @RF00131                                            0131
         L     @14,CSOACNM(,CSOAPTR)                               0131
         CLC   SUBCMND(4,@14),@CC01697                             0131
         BE    @RT00131                                            0131
*     GOTO HELP;                    /* GOTO THE HELP PROCESSOR       */
*                                                                  0132
*   /*****************************************************************/
*   /*                                                               */
*   /* INVALID SUBCOMMAND ENTERED                                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0133
*   OUTMSGID=128;                   /* MUST BE BAD OR MISSPELLED CMD */
@RF00131 MVI   OUTMSGID(OCMTBPTR),X'80'                            0133
*   CALL IKJCT467(OUTCOMTB);        /* CALL MSG WRITER               */
         ST    OCMTBPTR,@AL00001                                   0134
         L     @15,@CV01609                                        0134
         LA    @01,@AL00001                                        0134
         BALR  @14,@15                                             0134
*   IF NONTERM=YES THEN             /* YES - MUST BE CLIST OR      0135
*                                      NOPROMPT                      */
         TM    NONTERM(OCMTBPTR),B'00100000'                       0135
         BNO   @RF00135                                            0135
*     DO;                                                          0136
*       ERROR=YES;                  /* SET ERROR INDICATERS          */
         OI    ERROR(OCMTBPTR),B'00001000'                         0137
*       GOTO ENDRETRN;              /* RETURN TO CALLER              */
         B     ENDRETRN                                            0138
*     END;                                                         0139
*   ELSE                            /* IF MSG WAS PUT OUT            */
*     DO;                           /* GIVE USER ANOTHER CHANCE      */
@RF00135 DS    0H                                                  0141
*       GEN(TCLEARQ INPUT);         /* FLUSH INPUT STACK             */
         TCLEARQ INPUT
*       OUTMSGID=0;                 /* ISSUE MODE MSG                */
         MVI   OUTMSGID(OCMTBPTR),X'00'                            0142
*       CALL IKJCT467(OUTCOMTB);    /* CALL MSG WRITER               */
         ST    OCMTBPTR,@AL00001                                   0143
         L     @15,@CV01609                                        0143
         LA    @01,@AL00001                                        0143
         BALR  @14,@15                                             0143
*       GOTO SCANSUB;               /* MUST HAVE ENTERED NEW CMD     */
         B     SCANSUB                                             0144
*     END;                                                         0145
*                                                                  0146
*/********************************************************************/
*/*                                                                  */
*/*                                                                  */
*/*                BEGIN  PROCESSING SUB COMMANDS                    */
*/*                                                                  */
*/*                                                                  */
*/********************************************************************/
*                                                                  0146
*                                                                  0146
*/********************************************************************/
*/*                                                                  */
*/*                    SAVE     SUBCMD                               */
*/*                                                                  */
*/********************************************************************/
*                                                                  0146
*SAVE:                                                             0146
*                                                                  0146
*/********************************************************************/
*/*                      PARSE SAVE SUBCOMMAND                       */
*/********************************************************************/
*                                                                  0146
*   PPLCBUF=CSPLCBUF;               /* PASS CMD BUF W/ UPDATED OFFSET*/
SAVE     L     @14,PPLPTR                                          0146
         L     @10,CSPLPTR                                         0146
         MVC   PPLCBUF(4,@14),CSPLCBUF(@10)                        0146
*   PPLPCL=VSAVEPCL;                /* PASS ADR OF SAVE PCL          */
         MVC   PPLPCL(4,@14),VSAVEPCL                              0147
*   KEY1=NO;                        /* INDICATE KEY 0 FOR '67        */
         NI    KEY1(OCMTBPTR),B'01111111'                          0148
*   GEN(MODESET EXTKEY=SUPR);       /* GOTO KEY 0 FOR PARSE          */
         MODESET EXTKEY=SUPR
*   REG1=ADDR(PPL);                 /* PARAM FOR PARSE               */
         L     REG1,PPLPTR                                         0150
*   GEN(LINK EP=IKJPARS);           /* LINK TO IKJPARS               */
         LINK EP=IKJPARS
*   OUTRTCD=REG15;                  /* SAVE RC AROUND MODESET        */
         STH   REG15,OUTRTCD(,OCMTBPTR)                            0152
*   GEN(MODESET EXTKEY=SCHED);      /* RETURN TO KEY 1               */
         MODESET EXTKEY=SCHED
*   KEY1=YES;                       /* INDICATE BACK TO KEY1         */
         OI    KEY1(OCMTBPTR),B'10000000'                          0154
*/********************************************************************/
*/*              CHECK PARSE RETURN CODE                             */
*/********************************************************************/
*                                                                  0155
*   IF OUTRTCD^=ZERO THEN           /* IF PARSE RC IS NON ZERO       */
         LH    @14,OUTRTCD(,OCMTBPTR)                              0155
         LTR   @14,@14                                             0155
         BZ    @RF00155                                            0155
*     DO;                                                          0156
*       IF OUTRTCD=8 THEN           /* ATTN DETECTED DURING PARSE    */
         CH    @14,@CH00045                                        0157
         BNE   @RF00157                                            0157
*         DO;                                                      0158
*           CALL TCLRQSTK;          /* CALL RTN TO RESET STACK     0159
*                                                            @ZA15715*/
         BAL   @14,TCLRQSTK                                        0159
*           GOTO SCANSUB;           /* GO SCAN NEW SUBCMD            */
         B     SCANSUB                                             0160
*         END;                                                     0161
*       ELSE                        /* ANY OTHER ERROR CONDITION     */
*         IF OUTRTCD=4 THEN         /* 4 - USER IN NOPROMPT AND COULD
*                                      NOT BE PROMPTED               */
@RF00157 CLC   OUTRTCD(2,OCMTBPTR),@CH00051                        0162
         BNE   @RF00162                                            0162
*           IF NONTERM=YES THEN     /* INPUT NOT FROM TERMINAL       */
         TM    NONTERM(OCMTBPTR),B'00100000'                       0163
         BNO   @RF00163                                            0163
*             DO;                                                  0164
*               ERROR=YES;          /* QUIT OUTPUT                   */
         OI    ERROR(OCMTBPTR),B'00001000'                         0165
*               GOTO ENDRETRN;      /* RETURN TO '62                 */
         B     ENDRETRN                                            0166
*             END;                                                 0167
*           ELSE                    /* CMDS ENTERED FROM TERMINAL    */
*             DO;                                                  0168
@RF00163 DS    0H                                                  0169
*               GEN(TCLEARQ INPUT); /* FLUSH INPUT STACK             */
         TCLEARQ INPUT
*               OUTMSGID=0;         /* ISSUE MODE MSG                */
         MVI   OUTMSGID(OCMTBPTR),X'00'                            0170
*               CALL IKJCT467(OUTCOMTB);/* CALL MSG WRITER           */
         ST    OCMTBPTR,@AL00001                                   0171
         L     @15,@CV01609                                        0171
         LA    @01,@AL00001                                        0171
         BALR  @14,@15                                             0171
*               GOTO SCANSUB;       /* SCAN NEW SUBCMD               */
         B     SCANSUB                                             0172
*             END;                                                 0173
*         ELSE                      /* ELSE OTHER THAN 4 OR 8        */
*           DO;                                                    0174
@RF00162 DS    0H                                                  0175
*             OUTMSGID=12;          /* INDICATE PARSE ERROR          */
         MVI   OUTMSGID(OCMTBPTR),X'0C'                            0175
*             CALL IKJCT467(OUTCOMTB);/* CALL MSG WRITER             */
         ST    OCMTBPTR,@AL00001                                   0176
         L     @15,@CV01609                                        0176
         LA    @01,@AL00001                                        0176
         BALR  @14,@15                                             0176
*             IF ERROR=YES×NONTERM=YES THEN/* IF ERROR WRITING MSG OR
*                                      OR INPUT NOT FROM TERMINAL    */
         TM    ERROR(OCMTBPTR),B'00001000'                         0177
         BO    @RT00177                                            0177
         TM    NONTERM(OCMTBPTR),B'00100000'                       0177
         BNO   @RF00177                                            0177
@RT00177 DS    0H                                                  0178
*               DO;                                                0178
*                 ERROR=YES;        /* SET ERROR SWITCH ON           */
         OI    ERROR(OCMTBPTR),B'00001000'                         0179
*                 GOTO ENDRETRN;    /* GOTO END AND RETURN           */
         B     ENDRETRN                                            0180
*               END;                                               0181
*             ELSE                  /* MSG PUT OUT & TERM INPUT      */
*               DO;                 /* GIVE USER ANOTHER CHANCE      */
@RF00177 DS    0H                                                  0183
*                 GEN(TCLEARQ INPUT);/* FLUSH INPUT STACK            */
         TCLEARQ INPUT
*                 OUTMSGID=0;       /* ISSUE MODE MSG                */
         MVI   OUTMSGID(OCMTBPTR),X'00'                            0184
*                 CALL IKJCT467(OUTCOMTB);/* CALL MSG WRITER         */
         ST    OCMTBPTR,@AL00001                                   0185
         L     @15,@CV01609                                        0185
         LA    @01,@AL00001                                        0185
         BALR  @14,@15                                             0185
*                 GOTO SCANSUB;     /* GO SCAN NEW SUBCMD            */
         B     SCANSUB                                             0186
*               END;                                               0187
*           END;                                                   0188
*     END;                                                         0189
*                                                                  0190
*/********************************************************************/
*/*               GET ADDRESSABILITY FOR PDL AND ALLOC SAVE DS       */
*/********************************************************************/
*                                                                  0190
*   REG5=OUTSBPDL;                  /* GET ADR OF PDL                */
@RF00155 L     REG5,OUTSBPDL(,OCMTBPTR)                            0190
*   GEN(USING  SAVEPDL,REG5);       /* GET PDL ADDRESSABILTY         */
         USING  SAVEPDL,REG5
*   OSVDSPDE=ADDR($SAVDSN);         /* PUT PDE ADDR IN OUTCOMTB      */
         LA    @14,$SAVDSN                                         0192
         ST    @14,OSVDSPDE(,OCMTBPTR)                             0192
*   STRCTNUM=STRCTNUM+1;            /* SET STRUCTURE NUM FOR '73 TO
*                                      INDICATE A SAVE DS            */
         LA    @14,1                                               0193
         AL    @14,STRCTNUM(,OCMTBPTR)                             0193
         ST    @14,STRCTNUM(,OCMTBPTR)                             0193
*   CALL IKJCT473(ADDR(OUTCOMTB));  /* CALL ALLOC MOD                */
         ST    OCMTBPTR,@TF00001                                   0194
         LA    @14,@TF00001                                        0194
