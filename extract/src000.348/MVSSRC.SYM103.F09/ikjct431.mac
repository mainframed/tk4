         L     @06,SVTABPTR                                        0207
         LA    @15,3                                               0207
         AL    @15,SVTABFRE(,@06)                                  0207
         ST    @15,SVTABFRE(,@06)                                  0207
*       CALL SVTELUPT;                                             0208
*                                                                  0208
         BAL   @14,SVTELUPT                                        0208
