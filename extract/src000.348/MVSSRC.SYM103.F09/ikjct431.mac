         TITLE 'IKJCT431 EXEC COMMAND SYMBOLIC PARAMETER DEFINITION    *
                        '
IKJCT431 CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'IKJCT431  78.048'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @11,0                                               0001
@PSTART  LA    @12,4095(,@11)                                      0001
         USING @PSTART,@11                                         0001
         USING @PSTART+4095,@12                                    0001
         L     @00,@SIZDATD                                        0001
         GETMAIN  R,LV=(0)
         LR    @10,@01                                             0001
         USING @DATD,@10                                           0001
         ST    @13,@SA00001+4                                      0001
         LM    @00,@01,20(@13)                                     0001
         ST    @10,8(,@13)                                         0001
         LR    @13,@10                                             0001
         MVC   @PC00001(4),0(@01)                                  0001
*   ECDAPTR=R1;                     /* SAVE ADDRESS OF COMMON DATA   */
         LR    ECDAPTR,R1                                          0093
*   RFY                                                            0094
*     R1 UNRSTD;                                                   0094
*   LINELNG=RECLNG-CON4;            /* LINE LENGTH FOR FIRST RECORD  */
         L     @06,ECDAIREC(,ECDAPTR)                              0095
         LH    @06,RECLNG(,@06)                                    0095
         SL    @06,@CF00062                                        0095
         ST    @06,LINELNG                                         0095
*   ECB=CON0;                       /* INIT AN ECB FOR SERVICE RTNS  */
         SLR   @06,@06                                             0096
         ST    @06,ECB                                             0096
*   FLGCT431='';                    /* INITIALIZE FLAG AREA          */
         MVI   FLGCT431,X'00'                                      0097
*   ERRPTR=ADDR(ERRPARMS);          /* SERVICE RTN PARMLIST AREA     */
*                                                                  0098
         LA    @06,ERRPARMS                                        0098
         ST    @06,ERRPTR                                          0098
*   /*****************************************************************/
*   /*                                                               */
*   /* ISSUE GETMAIN FOR: LSD, EXECDATA, SNTAB, SVTAB AND THE FIRST  */
*   /* COMMAND PROCEDURE BLOCK                                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0099
*   RFY                                                            0099
*     R15 RSTD;                     /* ISSUE GETMAIN FOR STANDARD    */
*   DO;                             /* GETMAIN (EC) LV(GETSIZE)    0100
*                                      A(ECDAGADD) SP(78) RTCD(R15)  */
*     RESPECIFY                                                    0101
*      (GPR01P,                                                    0101
*       GPR15F) RESTRICTED;                                        0101
*     I004=I004&&I004;              /* INIT LIST                     */
         XC    I004(10),I004                                       0102
*     I00404='20'X;                 /* TYPE                          */
         MVI   I00404,X'20'                                        0103
*     I00401=GETSIZE;               /* LENGTH                        */
         MVC   I00401(4),@CF01243                                  0104
*     I00403=ADDR(ECDAGADD);        /* ADDR OF ADDR LIST             */
         LA    @14,ECDAGADD(,ECDAPTR)                              0105
         STCM  @14,7,I00403                                        0105
*     I00405=78;                    /* SUBPOOL VALUE                 */
         MVI   I00405,X'4E'                                        0106
*     GPR01P=ADDR(I004);            /* REG1 POINTS TO LIST           */
         LA    GPR01P,I004                                         0107
*     SVC(4);                       /* STANDARD-FORM OF GETMAIN      */
         SVC   4                                                   0108
*     R15=GPR15F;                   /* SET RETURN CODE               */
*     RESPECIFY                                                    0110
*      (GPR01P,                                                    0110
*       GPR15F) UNRESTRICTED;                                      0110
*   END;                            /* GETMAIN (EC) LV(GETSIZE)    0111
*                                      A(ECDAGADD) SP(78) RTCD(R15)
*                                      EXEC                          */
*                                                                  0112
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK GETMAIN RETURN CODE                                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0112
*   IF R15^=CON0 THEN               /* CONTROL BLOCKS                */
         LTR   R15,R15                                             0112
         BZ    @RF00112                                            0112
*     DO;                           /* IF STORAGE COULD NOT BE       */
*       RFY                                                        0114
*         R15 UNRSTD;               /* OBTAINED THEN NOTIFY USER AND */
*       EXMSGID=M511;               /* RETURN RC=16                  */
         MVC   EXMSGID(4),@CC01271                                 0115
*       CALL MSGRTN;                /* ISSUE MESSAGE                 */
         BAL   @14,MSGRTN                                          0116
*       NOTEXEC=YES;                /* COMMAND PROCEDURE NOT       0117
*                                      EXECUTABLE                    */
         OI    NOTEXEC(ECDAPTR),B'01000000'                        0117
*       CT431RET=CON16;                                            0118
         MVC   CT431RET(4),@CF01028                                0118
*     END;                          /* CONTROL RETURNS TO EXIT POINT */
*                                                                  0119
*   /*****************************************************************/
*   /*                                                               */
*   /* IF ERRORS HAVE OCCURRED THEN CONTROL RETURNS TO IKJCT431 EXIT */
*   /* POINT                                                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0120
*                                                                  0120
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE THE EXEC CONTROL BLOCKS - FIRST THE LSD            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0120
*   ELSE                            /* GETMAIN SUCCESSFUL - CONTINUE */
*     DO;                                                          0120
         B     @RC00112                                            0120
@RF00112 DS    0H                                                  0121
*       SP78BLK=YES;                /* INDICATE CORE HAS BEEN GOTTEN */
*       SP78CORE=YES;                                              0122
         OI    SP78BLK(ECDAPTR),B'00100100'                        0122
*       ECDALSD=ECDAGADD;           /* LSD IS FIRST BLOCK IN INITIAL */
         L     @06,ECDAGADD(,ECDAPTR)                              0123
         ST    @06,ECDALSD(,ECDAPTR)                               0123
*       LSDRCLEN=CON0;              /* GETMAIN AREA - RECLEN IS      */
         SLR   @15,@15                                             0124
         L     @14,LSDPTR(,ECDAPTR)                                0124
         STH   @15,LSDRCLEN(,@14)                                  0124
*       LSDADATA=ECDAGADD+HALF;     /* VARIABLE                      */
         LA    @09,2048                                            0125
         LR    @08,@06                                             0125
         ALR   @08,@09                                             0125
         ST    @08,LSDADATA(,@14)                                  0125
*       LSDANEXT=LSDADATA+CON12;    /* UPDATE PAST BLOCK HEADER      */
         LA    @05,12                                              0126
         ALR   @08,@05                                             0126
         ST    @08,LSDANEXT(,@14)                                  0126
*       LSDBLKID=CON1;              /* INIT TO ID OF FIRST BLOCK     */
         MVI   LSDBLKID(@14),X'01'                                 0127
*       LSDADAID=CON1;                                             0128
         MVI   LSDADAID(@14),X'01'                                 0128
*       LSDEXEC=ECDALSD+((LENGTH(LSD)+CON7)/CON8*CON8);/* PLACE    0129
*                                      EXECDATA ON DOUBLE WORD     0129
*                                      BOUNDRY                       */
*                                                                  0129
         AL    @06,@CF01028                                        0129
         ST    @06,LSDEXEC(,@14)                                   0129
*       /*************************************************************/
*       /*                                                           */
*       /* INITIALIZE THE COMMAND PROCEDURE BLOCK HEADER INFORMATION */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0130
*       ECDACBLK=LSDADATA;          /* INITIALIZE COMMAND PROCEDURE  */
         L     @08,LSDADATA(,@14)                                  0130
         ST    @08,ECDACBLK(,ECDAPTR)                              0130
*       ECDACNXT=LSDANEXT;                                         0131
         L     @14,LSDANEXT(,@14)                                  0131
         ST    @14,ECDACNXT(,ECDAPTR)                              0131
*       ECDACPRE=ECDACNXT;          /* SAME AS NEXT PTR INITIALLY    */
         ST    @14,ECDACPRE(,ECDAPTR)                              0132
*       COMPRNXT=CON0;              /* INDICATE NO MORE BLOCKS       */
         STCM  @15,7,COMPRNXT(@08)                                 0133
*       COMPRID=CON1;               /* THIS IS FIRST BLOCK           */
         MVI   COMPRID(@08),X'01'                                  0134
*       COMPRLNG=HALF;              /* ORIGIONAL SIZE IS HALF OF PAGE*/
         ST    @09,COMPRLNG(,@08)                                  0135
*       COMPRUSE=CON12;                                            0136
         ST    @05,COMPRUSE(,@08)                                  0136
*                                                                  0137
*       /*************************************************************/
*       /*                                                           */
*       /* INITIALIZE THE EXEC DATA CONTROL BLOCK                    */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0137
*       EXECDATA=EXECDATA&&EXECDATA;/* CLEAR CORE                    */
         XC    EXECDATA(64,@06),EXECDATA(@06)                      0137
*       IF NESTED=YES THEN          /* IF THIS IS A NESTED EXEC THEN */
         TM    NESTED(ECDAPTR),B'00000010'                         0138
         BNO   @RF00138                                            0138
*         GEXECDAT=ECDAGDAT->GEXECDAT;/* PICK UP ADDRESS OF NEXT   0139
*                                      LOWER EXECDATA OTHERWISE      */
         L     @01,ECDAGDAT(,ECDAPTR)                              0139
         L     @15,GEXECDAT(,@01)                                  0139
         ST    @15,GEXECDAT(,@06)                                  0139
*       ELSE                        /* SAVE THE ADDRESS OF FIRST     */
*         GEXECDAT=ADDR(EXECDATA);  /* LEVEL EXECDATA                */
         B     @RC00138                                            0140
@RF00138 L     @06,LSDPTR(,ECDAPTR)                                0140
         L     @06,LSDEXEC(,@06)                                   0140
         ST    @06,GEXECDAT(,@06)                                  0140
*       SNTABFST=LSDEXEC+(LENGTH(EXECDATA)+CON7)/CON8*CON8;/* SPLIT  */
@RC00138 L     @06,LSDPTR(,ECDAPTR)                                0141
         L     @06,LSDEXEC(,@06)                                   0141
         LA    @15,64                                              0141
         ALR   @15,@06                                             0141
         ST    @15,SNTABFST(,@06)                                  0141
*       SNTABPTR=SNTABFST;          /* REMAINING CORE BETWEEN SNTAB
*                                      AND SVTAB                     */
         ST    @15,SNTABPTR                                        0142
*       SVTABFST=SNTABPTR+((((CPBLKPTR-SNTABBLK)/CON2)+CON7)/CON8*CON8)
*           ;                                                      0143
         L     @14,SNTABBLK-1                                      0143
         LA    @14,0(,@14)                                         0143
         L     @05,CPBLKPTR-1(,ECDAPTR)                            0143
         LA    @05,0(,@05)                                         0143
         LR    @00,@05                                             0143
         SLR   @00,@14                                             0143
         SRDA  @00,32                                              0143
         D     @00,@CF00064                                        0143
         LR    @02,@01                                             0143
         AL    @02,@CF00139                                        0143
         SRDA  @02,32                                              0143
         D     @02,@CF00078                                        0143
         SLA   @03,3                                               0143
         ALR   @03,@15                                             0143
         ST    @03,SVTABFST(,@06)                                  0143
*       SVTABPTR=SVTABFST;                                         0144
*                                                                  0144
         ST    @03,SVTABPTR                                        0144
*       /*************************************************************/
*       /*                                                           */
*       /* INITIALIZE FIRST SNTAB                                    */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0145
*       SNTABNXT=CON0;              /* ONLY ONE BLOCK                */
         SLR   @06,@06                                             0145
         ST    @06,SNTABNXT(,@15)                                  0145
*       SNTABLNG=SVTABBLK-SNTABBLK; /* TOTAL LENGTH                  */
         L     @04,SVTABBLK-1                                      0146
         LA    @04,0(,@04)                                         0146
         LCR   @14,@14                                             0146
         ALR   @14,@04                                             0146
         ST    @14,SNTABLNG(,@15)                                  0146
*       SNTABUSE=LENGTH(SNTAB);     /* 12 BYTES OCCUPIED BY HEADER   */
         MVC   SNTABUSE(4,@15),@CF01091                            0147
*       SNTELPTR=ADDR(SNTELFST);    /* FIRST AVAILABLE SLOT          */
*                                                                  0148
         LA    SNTELPTR,SNTELFST(,@15)                             0148
*       /*************************************************************/
*       /*                                                           */
*       /* INITIALIZE FIRST SVTAB                                    */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0149
*       SVTABNXT=CON0;              /* ONLY ONE BLOCK                */
         ST    @06,SVTABNXT(,@03)                                  0149
*       SVTABLNG=CPBLKPTR-SVTABBLK; /* TOTAL LENGTH                  */
         SLR   @05,@04                                             0150
         ST    @05,SVTABLNG(,@03)                                  0150
*       SVTABUSE=LENGTH(SVTAB);     /* 16 BYTES OCCUPIED BY HEADER   */
         MVC   SVTABUSE(4,@03),@CF01028                            0151
*       SVTABFRE=CON0;              /* NO UNUSED BYTES               */
         ST    @06,SVTABFRE(,@03)                                  0152
*       SVTELPTR=ADDR(SVTELFST);    /* ALL CONTROL BLOCKS ARE BUILT  */
         LA    SVTELPTR,SVTELFST(,@03)                             0153
*       SP78BLK=NO;                                                0154
         NI    SP78BLK(ECDAPTR),B'11111011'                        0154
*                                                                  0155
*       /*************************************************************/
*       /*                                                           */
*       /* INITIALIZE THE CONTROL VARIABLES INITIALIZE A NULL VALUE  */
*       /* ELEMENT AND ALL CONTROL VARIABLES WITH A NULL VALUE LIST  */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0155
*       SVTELPTR->SVTLNG=CON0;      /* NULL ELEMENT LENGTH           */
         STH   @06,SVTLNG(,SVTELPTR)                               0155
*       SVTELPTR->SVTORIG=CON0;     /* NULL ELEMENT FREE LENGTH      */
         STH   @06,SVTORIG(,SVTELPTR)                              0156
*       CALL SVTELUPT;                                             0157
         BAL   @14,SVTELUPT                                        0157
*       RFY                                                        0158
*        (R2,                                                      0158
*         R3,                                                      0158
*         R4,                                                      0158
*         R5) RSTD;                                                0158
*       R2=ADDR(CVARMAP);           /* ADDRESS OF CVAR AREA          */
         L     R2,SNTABPTR                                         0159
         LA    R2,CVARMAP(,R2)                                     0159
*       R3=LENGTH(CVARMAP);         /* LENGTH TO INITIALIZE          */
         LA    R3,272                                              0160
*       R4=R2;                      /* NO DATA TO MOVE               */
         LR    R4,R2                                               0161
*       R5=CON0;                    /* PADS CVARMAP WITH ZEROS       */
         SLR   R5,R5                                               0162
*       MVCL(R2,R4);                /* ZERO AREA                     */
         MVCL  R2,R4                                               0163
*       RFY                                                        0164
*        (R2,                                                      0164
*         R3,                                                      0164
*         R4,                                                      0164
*         R5) UNRSTD;                                              0164
*       DO ICTR=1 TO DIM(NULLVAR);  /* INITIALIZE NULL VALUE       0165
*                                      VARIABLES                     */
         LA    ICTR,1                                              0165
@DL00165 DS    0H                                                  0166
*         SNTDATA(1:NULLEN(ICTR))=NULLADR(ICTR)->NULLVDAT(1:NULLEN(ICTR
*             ));                                                  0166
         LR    @15,ICTR                                            0166
         SLA   @15,2                                               0166
         SLR   @14,@14                                             0166
         IC    @14,NULLEN-4(@15)                                   0166
         LR    @05,@14                                             0166
         BCTR  @05,0                                               0166
         L     @01,NULLADR-5(@15)                                  0166
         EX    @05,@SM01540                                        0166
*         SNTLNG=NULLEN(ICTR);                                     0167
         STH   @14,SNTLNG(,SNTELPTR)                               0167
*         SNTVLPTR=ADDR(SVTELFST);                                 0168
         L     @01,SVTABPTR                                        0168
         LA    @15,SVTELFST(,@01)                                  0168
         ST    @15,SNTVLPTR(,SNTELPTR)                             0168
*         IF ICTR<CON11 THEN        /* FIRST 10 VARIABLES REQUIRE    */
         C     ICTR,@CF01262                                       0169
         BNL   @RF00169                                            0169
*           SNTEVAL=YES;            /* IMMEDIATE EVALUATION          */
         OI    SNTEVAL(SNTELPTR),B'00000010'                       0170
*         IF ICTR<=CON15 THEN       /* FIRST 15 VARIABLES CAN NOT    */
@RF00169 C     ICTR,@CF00059                                       0171
         BH    @RF00171                                            0171
*           SNTNAUTH=YES;           /* BE SET BY USER                */
         OI    SNTNAUTH(SNTELPTR),B'00000100'                      0172
*         SNTABUSE=SNTABUSE+SNTLNG+LENGTH(SNTELEM);                0173
@RF00171 L     @15,SNTABPTR                                        0173
         LA    @14,8                                               0173
         LH    @05,SNTLNG(,SNTELPTR)                               0173
         L     @04,SNTABUSE(,@15)                                  0173
         ALR   @04,@05                                             0173
         ALR   @04,@14                                             0173
         ST    @04,SNTABUSE(,@15)                                  0173
*         SNTELPTR=SNTELPTR+SNTLNG+LENGTH(SNTELEM);                0174
         ALR   @05,SNTELPTR                                        0174
         ALR   @05,@14                                             0174
         LR    SNTELPTR,@05                                        0174
*       END;                                                       0175
         AL    ICTR,@CF00035                                       0175
         C     ICTR,@CF01397                                       0175
         BNH   @DL00165                                            0175
*       RFY                                                        0176
*         SVTELEM BASED(SVTELPTR);                                 0176
*                                                                  0177
*       /*************************************************************/
*       /*                                                           */
*       /* INITIALIZE THOSE CONTROL VARIABLES REQUIRING VALUES       */
*       /* -SYSUID                                                   */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0177
*       SVTDATA(1:PSCBUSRL)=PSCBUSER(1:PSCBUSRL);                  0177
         L     @06,CPPLPTR(,ECDAPTR)                               0177
         L     @06,PSCBPTR(,@06)                                   0177
         SLR   @15,@15                                             0177
         IC    @15,PSCBUSRL(,@06)                                  0177
         LR    @14,@15                                             0177
         BCTR  @14,0                                               0177
         EX    @14,@SM01544                                        0177
*       SVTLNG=PSCBUSRL;            /* USERID LENGTH                 */
         STH   @15,SVTLNG(,SVTELPTR)                               0178
*       SVTORIG=SVTLNG;             /* ORIGIONAL LENGTH SAME         */
         STH   @15,SVTORIG(,SVTELPTR)                              0179
*       CUIDVAL=SVTELPTR;           /* SNTAB VALUE POINTER           */
         L     @06,SNTABPTR                                        0180
         STCM  SVTELPTR,15,CUIDVAL(@06)                            0180
*       CALL SVTELUPT;                                             0181
*                                                                  0181
         BAL   @14,SVTELUPT                                        0181
*       /*************************************************************/
*       /*                                                           */
*       /* -SYSPROC                                                  */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0182
*       DO;                         /* EXTRACT (TIOTPTR,'S')       0182
*                                      FIELDS(TIOT)                  */
*         RESPECIFY                                                0183
*          (GPR01P) RESTRICTED;                                    0183
*         I040=I040&&I040;          /* ZERO OUT THE LIST             */
         XC    I040(12),I040                                       0184
*         I04001=ADDR(TIOTPTR);     /* SET ANSWER AREA ADDR IN LIST  */
         LA    @06,TIOTPTR                                         0185
         ST    @06,I04001                                          0185
*         I04002=0;                 /* INDICATE CURRENT TASK         */
         SLR   @06,@06                                             0186
         ST    @06,I04002                                          0186
*         I04003='0000001000000000'B;/* SET TCB EXTRACT FIELD BITS   */
         MVC   I04003(2),@CB01458                                  0187
*         GPR01P=ADDR(I040);        /* LOAD ADDRESS OF LIST          */
         LA    GPR01P,I040                                         0188
*         SVC(40);                  /* ISSUE EXTRACT SVC             */
         SVC   40                                                  0189
*         RESPECIFY                                                0190
*          (GPR01P) UNRESTRICTED;                                  0190
*       END;                        /* EXTRACT (TIOTPTR,'S')       0191
*                                      FIELDS(TIOT)                  */
*       DO ICTR=8 TO 1 BY-1 WHILE(TIOTPROC(ICTR)=BLANK);/* DETERMINE */
         LA    ICTR,8                                              0192
@DL00192 L     @01,TIOTPTR                                         0192
         ALR   @01,ICTR                                            0192
         CLI   TIOTPROC-1(@01),C' '                                0192
         BNE   @DC00192                                            0192
*       END;                        /* LENGTH OF THE PROCEDURE NAME  */
         BCTR  ICTR,0                                              0193
         LTR   ICTR,ICTR                                           0193
         BP    @DL00192                                            0193
@DC00192 DS    0H                                                  0194
*       IF ICTR>CON0 THEN           /* MOVE PROCNAME IN IF PRESENT   */
         LTR   ICTR,ICTR                                           0194
         BNP   @RF00194                                            0194
*         DO;                                                      0195
*           SVTDATA(1:ICTR)=TIOTPROC(1:ICTR);                      0196
         LR    @15,ICTR                                            0196
         BCTR  @15,0                                               0196
         L     @01,TIOTPTR                                         0196
         EX    @15,@SM01546                                        0196
*           SVTLNG=ICTR;                                           0197
         STH   ICTR,SVTLNG(,SVTELPTR)                              0197
*           SVTORIG=ICTR;                                          0198
         STH   ICTR,SVTORIG(,SVTELPTR)                             0198
*           CPROCVAL=SVTELPTR;                                     0199
         L     @06,SNTABPTR                                        0199
         STCM  SVTELPTR,15,CPROCVAL(@06)                           0199
*           CALL SVTELUPT;                                         0200
         BAL   @14,SVTELUPT                                        0200
*         END;                                                     0201
*       ELSE                        /* IF NO PROCNAME THEN SET TO    */
*         CPROCVAL=ADDR(SVTELFST);  /* NULL ELEMENT                  */
         B     @RC00194                                            0202
@RF00194 L     @06,SVTABPTR                                        0202
         LA    @06,SVTELFST(,@06)                                  0202
         L     @01,SNTABPTR                                        0202
         STCM  @06,15,CPROCVAL(@01)                                0202
*                                                                  0203
*       /*************************************************************/
*       /*                                                           */
*       /* - LASTCC                                                  */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0203
*       SVTDATA(1)=CONC0;           /* LASTCC INITIALIZED TO ZERO    */
@RC00194 MVI   SVTDATA(SVTELPTR),C'0'                              0203
*       SVTLNG=LENGTH(CONC0);                                      0204
         MVC   SVTLNG(2,SVTELPTR),@CH00035                         0204
