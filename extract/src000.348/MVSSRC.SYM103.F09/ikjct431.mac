         MVC   SVTLNG(2,SVTELPTR),@CH00035                         0210
*       SVTORIG=CON4;                                              0211
         MVC   SVTORIG(2,SVTELPTR),@CH00062                        0211
*       CMCCVAL=SVTELPTR;                                          0212
         L     @06,SNTABPTR                                        0212
         ST    SVTELPTR,CMCCVAL(,@06)                              0212
*       SVTABFRE=SVTABFRE+CON3;                                    0213
         L     @06,SVTABPTR                                        0213
         LA    @15,3                                               0213
         AL    @15,SVTABFRE(,@06)                                  0213
         ST    @15,SVTABFRE(,@06)                                  0213
*       CALL SVTELUPT;                                             0214
*                                                                  0214
         BAL   @14,SVTELUPT                                        0214
