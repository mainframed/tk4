         L     @14,OUTRPL(,OCMTBPTR)                               0438
         ST    REG1,RPLAREA(,@14)                                  0438
*       FREEBUF=YES;                /* FREEMAIN BEFORE END           */
         OI    FREEBUF,B'10000000'                                 0439
*     END;                                                         0440
