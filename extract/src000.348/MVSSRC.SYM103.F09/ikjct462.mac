*                                                                  0442
*ATTNRTN:                                                          0442
*   PROC;                           /* INTERNAL TO PRINTRTN - HANDLE
*                                      ATTENTION INTERRUPTS          */
ATTNRTN  STM   @14,@12,@SA00008                                    0442
*   EODSW=OFF;                      /* IN CASE EOD, IGNORE           */
         NI    EODSW(OCMTBPTR),B'11101111'                         0443
*   CALL TCLRQSTK;                  /* CALL RTN TO CLEARQ AND RESET
*                                      THE STACK                     */
*                                                                  0444
         BAL   @14,TCLRQSTK                                        0444
*   /*****************************************************************/
*   /*                                                               */
*   /* INVOKE IKJCT463 TO PROCESS THE SUBCMD. ANALYZE THE RESULTS.   */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0445
*   CALL IKJCT463(OPTR);            /* GO PROCESS SUBCOMMAND ENTERED
*                                      AFTER ATTN HIT                */
         L     @14,@PC00001                                        0445
         ST    @14,@AL00001                                        0445
         L     @15,@CV01405                                        0445
         LA    @01,@AL00001                                        0445
         BALR  @14,@15                                             0445
*   IF ERROR=YES THEN               /* IF IKJCT463 FAILED            */
*                                                                  0446
         TM    ERROR(OCMTBPTR),B'00001000'                         0446
         BNO   @RF00446                                            0446
*     /***************************************************************/
*     /*                                                             */
*     /* ERROR OCCURRED                                              */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0447
*     DO;                                                          0447
*       SSSOCHKP=YES;               /* TELL SUBSYS TO CHECKPOINT   0448
*                                      INTERRUPTED SYSOUT DS         */
         L     @14,OUTSOBSO(,OCMTBPTR)                             0448
         OI    SSSOCHKP(@14),B'01000000'                           0448
*       IF TERM=YES THEN            /* PRINT TO A TERMINAL, THERE- 0449
*                                      FORE TCAM BUFFERS WERE LOST 0449
*                                      AFTER ATTENTION.              */
         TM    TERM(OCMTBPTR),B'00001000'                          0449
         BNO   @RF00449                                            0449
*         SSSORBA=OUTBKAPX;         /* GIVE SUBSYS THE CKPT RBA    0450
*                                      CORRESPONDING TO APPROX 10 PUT
*                                      LINES BACK.                   */
         MVC   SSSORBA(8,@14),OUTBKAPX(OCMTBPTR)                   0450
*       ELSE                        /* PRINT TO A DATASET, BUFS NOT
*                                      LOST. RCDS REMAINING IN PUT 0451
*                                      BUFS ARE WRITTEN DURING CLOSE,
*                                      SO DON'T BACK UP.             */
*         SSSORBA=OUTHOLD;          /* USE CURRENT RBA IN OUTHOLD    */
         B     @RC00449                                            0451
@RF00449 L     @14,OUTSOBSO(,OCMTBPTR)                             0451
         MVC   SSSORBA(8,@14),OUTHOLD(OCMTBPTR)                    0451
*     END;                                                         0452
*   ELSE                            /* NO ERROR, SEE WHAT SUBCMD     */
*     IF SUBCONT=YES&SUBBEGN=YES THEN/* IF CONTINUE BEGIN SUBCMD     */
*                                                                  0453
         B     @RC00446                                            0453
@RF00446 TM    SUBCONT(OCMTBPTR),B'00101000'                       0453
         BNO   @RF00453                                            0453
*       /*************************************************************/
*       /*                                                           */
*       /* 'CONTINUE BEGIN' SUBCMD ENTERED                           */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0454
*       DO;                                                        0454
*         SUBCONT=OFF;              /* TURN OFF SUBCONT INDICATOR    */
*         SUBBEGN=OFF;              /* TURN OFF SUBBEGN INDICATOR    */
         NI    SUBCONT(OCMTBPTR),B'11010111'                       0456
