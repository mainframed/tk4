*     /*                                                             */
*     /***************************************************************/
*                                                                  0557
*     STCKLSTA(1:STCKLNGS)=STCKLSTS(1:STCKLNGS);/* LIST TO DYN CORE  */
         L     @14,STCKLNGS                                        0557
         BCTR  @14,0                                               0557
         EX    @14,@SM01595                                        0557
*     REG1=IOPLPTR;                 /* SET PARM REG FOR STACK      0558
*                                                            @OZ01550*/
         L     REG1,IOPLPTR(,OCMTBPTR)                             0558
