         TITLE 'IKJCT462   OUTPUT COMMAND JOB PROCESSOR                *
                        '
IKJCT462 CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'IKJCT462  74.364'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @12,0                                               0001
@PSTART  LA    @11,4095(,@12)                                      0001
         USING @PSTART,@12                                         0001
         USING @PSTART+4095,@11                                    0001
         L     @00,@SIZDATD                                        0001
         GETMAIN  R,LV=(0)
         LR    @10,@01                                             0001
         USING @DATD,@10                                           0001
         ST    @13,@SA00001+4                                      0001
         LM    @00,@01,20(@13)                                     0001
         ST    @10,8(,@13)                                         0001
         LR    @13,@10                                             0001
         XC    @ZTEMPS(@ZLEN),@ZTEMPS
         MVC   @PC00001(4),0(@01)                                  0001
*                                                                  0089
*/********************************************************************/
*/*                                                                  */
*/*  THE BASIC FLOW OF IKJCT462, THE JOB PROCESSOR, CAN BE OBTAINED  */
*/*  FROM THE FOLLOWING SECTION OF CODE:                             */
*/*                                                                  */
*/*          1. INITIALIZE OUTCOMTB'S BASE                           */
*/*          2. IF IKJCT469 HAS NO MORE JOBS TO PROCESS, INVOKE      */
*/*             SUBSYSTEM FOR FINAL CALL TO INDICATE PROCESSING      */
*/*             COMPLETE, THEN RETURN IKJCT469                       */
*/*          3. SET UP SSOB & SSSO CONTROL BLOCKS                    */
*/*          4. INVOKE THE SUBSYSTEM TO LOCATE, ROUTE, OR DELETE     */
*/*             A SYSOUT DATA SET                                    */
*/*          5. IF PRINT REQUESTED, ALLOCATE THE SYSOUT DATA SET,    */
*/*             READ SYSOUT & WRITE TO DATA SET, UNALLOCATE THE      */
*/*             SYSOUT DATA SET                                      */
*/*          6. RETURN TO CALLER                                     */
*/*                                                                  */
*/********************************************************************/
*                                                                  0089
*   OCMTBPTR=OPTR;                  /* INIT BASE OF OUTCOMTB (USE A
*                                      REGISTER FOR EFFICIENCY) WITH
*                                      PTR PASSED AS A PARM          */
*                                                                  0089
         L     @14,@PC00001                                        0089
         L     OCMTBPTR,OPTR(,@14)                                 0089
*   /*****************************************************************/
*   /*                                                               */
*   /* IF NO MORE WORK TO DO FOR THE PREVIOUS JOB- ISSUE FINAL CALL  */
*   /* TO SUBSYSTEM FOR THIS JOB                                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0090
*   IF NOWORK=ON THEN               /* NO MORE WORK TO DO            */
         TM    NOWORK(OCMTBPTR),B'00000010'                        0090
         BNO   @RF00090                                            0090
*     CALL FINISH;                  /* INVOKE SUBSYS - FINAL CALL    */
*                                                                  0091
         BAL   @14,FINISH                                          0091
*   /*****************************************************************/
*   /*                                                               */
*   /* ELSE WORK TO DO - PROCESS THE JOB                             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0092
*   ELSE                            /* JOB TO PROCESS - GET BUSY     */
*     DO;                                                          0092
         B     @RC00090                                            0092
@RF00090 DS    0H                                                  0093
*       SKIPDS=OFF;                 /* INIT SWITCH TO ZERO           */
         NI    SKIPDS,B'01111111'                                  0093
*       SKIPGET=OFF;                /* INIT SWITCH TO ZERO           */
         NI    SKIPGET,B'01111111'                                 0094
*       SUBSYS=OFF;                 /* INIT SWITCH TO ZERO           */
*                                                                  0095
         NI    SUBSYS(OCMTBPTR),B'01111111'                        0095
*       /*************************************************************/
*       /*                                                           */
*       /* INITIALIZE THE SUBSYSTEM CONTROL BLOCKS                   */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0096
*       CALL INITSSSO;              /* CALL RTN TO SET UP SSOB & SSSO
*                                      CTL BLKS                      */
         BAL   @14,INITSSSO                                        0096
*       FIRSTIME=ON;                /* SET SWITCH FOR FIRST CALL TO
*                                      SUBSYS FOR A JOB/CLASS      0097
*                                      COMBINATION-USED TO DECIDE  0097
*                                      WHAT SSOBRETN=4 MEANS (NO DS
*                                      OR NO MORE DS)                */
         OI    FIRSTIME,B'10000000'                                0097
*       LOOP=YES;                   /* INIT LOOP CONTROL-ALWAYS GO 0098
*                                      THRU CODE AT LEAST ONCE. FOR
*                                      DELETING OR ROUTINE, DON'T  0098
*                                      LOOP TO SUBSYS AS FOR       0098
*                                      PRINTING.                     */
*                                                                  0098
         OI    LOOP,B'10000000'                                    0098
*       /*************************************************************/
*       /*                                                           */
*       /* PROCESS THE JOB IKJCT469 PASSED: 1) IF GROUP REQUEST, NEED*/
*       /* ONLY CALL SUBSYSTEM ONCE. 2) IF PRINT REQUEST, CALL THE   */
*       /* SUBSYSTEM TO GET A DATA SET. PROCESS IT, THEN LOOP BACK TO*/
*       /* THE SUBSYSTEM AND GET ANOTHER SYSOUT DATASET. CONTINUE    */
*       /* LOOPING UNTIL NO MORE DATASETS.                           */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0099
*       DO WHILE ERROR=NO&ENDSW=NO&SSOBRETN=ZERO&LOOP=YES;/* THIS  0099
*                                      LOOP WILL ACCESS SYSOUT DATA
*                                      SETS FOR A JOB/CLASS UNTIL: AN
*                                      ERROR OCCURS, END IS        0099
*                                      REQUESTED, OR NO MORE DATA  0099
*                                      SETS CAN BE FOUND. FOR GROUP
*                                      REQUESTS THE LOOP IS EXECUTED
*                                      ONCE.                         */
         B     @DE00099                                            0099
@DL00099 DS    0H                                                  0100
*         CALL SUBSYSTM;            /* CALL RTN TO SET-UP & INVOKE 0100
*                                      SUBSYS & CHECK RETURN CODE    */
         BAL   @14,SUBSYSTM                                        0100
*         FIRSTIME=OFF;             /* NO LONGER 1ST CALL TO SUBSYS  */
         NI    FIRSTIME,B'01111111'                                0101
*         IF ERROR=NO&PRINT=YES&SSOBRETN=ZERO THEN/* NO ERROR & A  0102
*                                      SYSOUT DATASET WAS FOUND      */
         TM    ERROR(OCMTBPTR),B'00001000'                         0102
         BNZ   @RF00102                                            0102
         TM    PRINT(OCMTBPTR),B'00000010'                         0102
         BNO   @RF00102                                            0102
         L     @14,OUTSOBH(,OCMTBPTR)                              0102
         L     @14,SSOBRETN(,@14)                                  0102
         LTR   @14,@14                                             0102
         BNZ   @RF00102                                            0102
*           CALL ALLOCSO;           /* CALL RTN TO ALLOCATE THE    0103
*                                      SYSOUT DS                     */
         BAL   @14,ALLOCSO                                         0103
*         IF ERROR=NO&PRINT=YES&SSOBRETN=ZERO THEN/* SYSOUT WAS ALLOC
*                                      & PRINT REQUESTED             */
@RF00102 TM    ERROR(OCMTBPTR),B'00001000'                         0104
         BNZ   @RF00104                                            0104
         TM    PRINT(OCMTBPTR),B'00000010'                         0104
         BNO   @RF00104                                            0104
         L     @14,OUTSOBH(,OCMTBPTR)                              0104
         L     @14,SSOBRETN(,@14)                                  0104
         LTR   @14,@14                                             0104
         BNZ   @RF00104                                            0104
*           CALL PRINTRTN;          /* CALL RTN TO READ SYSOUT &   0105
*                                      WRITE RCDS TO PRINT DS        */
         BAL   @14,PRINTRTN                                        0105
*         IF ALLOC=YES THEN         /* SYSOUT WAS ALLOCATED          */
@RF00104 TM    ALLOC(OCMTBPTR),B'00000100'                         0106
         BNO   @RF00106                                            0106
*           CALL UNALCSO;           /* CALL RTN TO DETERMINE THE   0107
*                                      UNALLOCATION DISPOSITION &  0107
*                                      UNALLOC IT APPROPRIATELY      */
         BAL   @14,UNALCSO                                         0107
*       END;                        /* END OF DO WHILE LOOP TO     0108
*                                      RETRIEVE SYSOUT DATA SETS     */
*                                                                  0108
@RF00106 DS    0H                                                  0108
@DE00099 TM    ERROR(OCMTBPTR),B'00011000'                         0108
         BNZ   @DC00099                                            0108
         L     @14,OUTSOBH(,OCMTBPTR)                              0108
         L     @14,SSOBRETN(,@14)                                  0108
         LTR   @14,@14                                             0108
         BNZ   @DC00099                                            0108
         TM    LOOP,B'10000000'                                    0108
         BO    @DL00099                                            0108
@DC00099 DS    0H                                                  0109
*       /*************************************************************/
*       /*                                                           */
*       /* IF EARLY TERMINATION DUE TO END SUBCMD OR ERROR, WHILE    */
*       /* PROCESSING A PRINT REQUEST ISSUE FINAL CALL TO SUBSYSTEM. */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0109
*       IF SUBSYS=YES&(ENDSW=YES×ERROR=YES) THEN/* EARLY TERMINATION
*                                      PROCESSING A SET OF DATASETS.
*                                      EITHER END SUBCMD OR ERROR.   */
         TM    SUBSYS(OCMTBPTR),B'10000000'                        0109
         BNO   @RF00109                                            0109
         TM    ENDSW(OCMTBPTR),B'00011000'                         0109
         BZ    @RF00109                                            0109
*         CALL FINISH;              /* RETURN TO SUBSYS WHEN DONE  0110
*                                      WITH DATA SETS IN THIS      0110
*                                      JOB/CLASS COMBINATION         */
         BAL   @14,FINISH                                          0110
*     END;                          /* END OF CODE TO PROCESS JOB    */
@RF00109 DS    0H                                                  0112
*   RETURN CODE(RETNCODE);          /* RETURN TO IKJCT469            */
@RC00090 L     @05,RETNCODE                                        0112
         L     @13,4(,@13)                                         0112
         L     @00,@SIZDATD                                        0112
         LR    @01,@10                                             0112
         FREEMAIN R,LV=(0),A=(1)
         LR    @15,@05                                             0112
         L     @14,12(,@13)                                        0112
         LM    @00,@12,20(@13)                                     0112
         BR    @14                                                 0112
*                                                                  0113
*/********************************************************************/
*/*                                                                  */
*/*      INITIALIZE THE NECESSARY SSOB FIELDS                        */
*/*      SET UP THE SSSO FLAGS & FIELDS BASED ON THE FUNCTION        */
*/*         REQUESTED (PRINT, DELETE, ETC.).                         */
*/*                                                                  */
*/********************************************************************/
*                                                                  0113
*INITSSSO:                                                         0113
*   PROC;                           /* INTERNAL PROCEDURE TO       0113
*                                      INITIALIZE SSOB & SSSO        */
@EL00001 L     @13,4(,@13)                                         0113
@EF00001 L     @00,@SIZDATD                                        0113
         LR    @01,@10                                             0113
         FREEMAIN R,LV=(0),A=(1)
@ER00001 LM    @14,@12,12(@13)                                     0113
         BR    @14                                                 0113
INITSSSO STM   @14,@12,12(@13)                                     0113
*   SSOBFUNC=SSOBSOUT;              /* SSOB HEADER REQUESTS LOCATE 0114
*                                      FUNCTION FROM SUBSYSTEM       */
         L     @14,OUTSOBH(,OCMTBPTR)                              0114
         MVC   SSOBFUNC(2,@14),@CH00033                            0114
*   SSOBINDV=ADDR(SSSO);            /* SSOB HEADER POINTS TO THE   0115
*                                      LOCATE FUNCTION CNTRL BLK     */
         L     @05,OUTSOBSO(,OCMTBPTR)                             0115
         ST    @05,SSOBINDV(,@14)                                  0115
*   SSOBRETN=SSOBRETN&&SSOBRETN;    /* INIT TO ZERO                  */
         L     @02,SSOBRETN(,@14)                                  0116
         LR    @09,@02                                             0116
         XR    @09,@02                                             0116
         ST    @09,SSOBRETN(,@14)                                  0116
*   SSSO=SSSO&&SSSO;                /* INIT TO ZERO                  */
         XC    SSSO(120,@05),SSSO(@05)                             0117
*   SSSOLEN=LENGTH(SSSO);           /* LENGTH OF SSSO SECTION        */
*                                                                  0118
         MVC   SSSOLEN(2,@05),@CH01559                             0118
*   /*****************************************************************/
*   /*                                                               */
*   /* SET SELECTION CRITERIA BITS AND FIELDS IN SSSO                */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0119
*   SSSOSHLD=ON;                    /* INCLUDE HELD SYSOUT DS        */
*   SSSOSJBN=ON;                    /* USE JOBNAME                   */
         OI    SSSOSHLD(@05),B'10010000'                           0120
*   SSSOJOBN=JOBNAME(1:JOBNLEN);    /* PASS JOBNAME TO SUBSYSTEM.  0121
*                                      JOBNAME FOUND IN PDE.         */
         MVI   SSSOJOBN+1(@05),C' '                                0121
         MVC   SSSOJOBN+2(6,@05),SSSOJOBN+1(@05)                   0121
         L     @14,OUTJBPDE(,OCMTBPTR)                             0121
         LH    @02,JOBNLEN(,@14)                                   0121
         BCTR  @02,0                                               0121
         L     @09,JOBNMPTR(,@14)                                  0121
         EX    @02,@SM01575                                        0121
*   IF JOBIDLEN^=ZERO THEN          /* USER GAVE A JOBID             */
         LH    @02,JOBIDLEN(,@14)                                  0122
         LTR   @02,@02                                             0122
         BZ    @RF00122                                            0122
*     DO;                                                          0123
*       SSSOSJBI=ON;                /* USE JOBID                     */
         OI    SSSOSJBI(@05),B'00001000'                           0124
*       SSSOJOBI=JOBID(1:JOBIDLEN); /* PASS JOBID TO SUBSYSTEM. JOBID
*                                      IN PDE.                       */
         MVI   SSSOJOBI+1(@05),C' '                                0125
         MVC   SSSOJOBI+2(6,@05),SSSOJOBI+1(@05)                   0125
         BCTR  @02,0                                               0125
         L     @14,JOBIDPTR(,@14)                                  0125
         EX    @02,@SM01578                                        0125
*     END;                                                         0126
*   ELSE                                                           0127
*     ;                             /* NO JOBID GIVEN                */
@RF00122 DS    0H                                                  0128
*   IF CLASBUFF^=BLANKS THEN        /* USER ENTERED A CLASSNAME      */
         CLC   CLASBUFF(8,OCMTBPTR),@CC01391                       0128
         BE    @RF00128                                            0128
*     DO;                                                          0129
*       SSSOSCLS=ON;                /* USE CLASS                     */
         L     @14,OUTSOBSO(,OCMTBPTR)                             0130
         OI    SSSOSCLS(@14),B'01000000'                           0130
*       SSSOCLSL=CLASBUFF;          /* CLASS(ES) TO CHOOSE           */
         MVC   SSSOCLSL(8,@14),CLASBUFF(OCMTBPTR)                  0131
*     END;                                                         0132
*   ELSE                                                           0133
*     ;                             /* USE SUBSYS CLASS DEFAULT WHICH
*                                      IS ALL HELD CLASSES           */
*                                                                  0133
@RF00128 DS    0H                                                  0134
*   /*****************************************************************/
*   /*                                                               */
*   /* SET OTHER BITS AND FIELDS UNIQUE TO SPECIFIC FUNCTIONS        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0134
*   IF PRINT=YES THEN               /* PRINT REQUESTED               */
         TM    PRINT(OCMTBPTR),B'00000010'                         0134
         BNO   @RF00134                                            0134
*     SSSOCTRL=OFF;                 /* SUBSYSTEM TO RETURN A DATA SET
*                                      NAME                          */
         L     @14,OUTSOBSO(,OCMTBPTR)                             0135
         NI    SSSOCTRL(@14),B'01111111'                           0135
*   ELSE                                                           0136
*     IF DELETE=YES THEN            /* DELETE REQUESTED              */
         B     @RC00134                                            0136
@RF00134 TM    DELETE(OCMTBPTR),B'00000100'                        0136
         BNO   @RF00136                                            0136
*       DO;                                                        0137
*         SSSODELC=ON;              /* SUBSYSTEM TO DELETE DS        */
         L     @14,OUTSOBSO(,OCMTBPTR)                             0138
         OI    SSSODELC(@14),B'01000000'                           0138
*         SSSOCTRL=ON;              /* A 'DELETE' GROUP REQUEST IS 0139
*                                      REALLY A FINAL REQUEST        */
         OI    SSSOCTRL(@14),B'10000000'                           0139
*       END;                                                       0140
*     ELSE                          /* A ROUTING REQUEST             */
*       DO;                                                        0141
         B     @RC00136                                            0141
@RF00136 DS    0H                                                  0142
*         SSSOCTRL=ON;              /* A 'ROUTING' GROUP REQUEST IS
*                                      REALLY A FINAL REQUEST        */
         L     @14,OUTSOBSO(,OCMTBPTR)                             0142
         OI    SSSOCTRL(@14),B'10000000'                           0142
*         IF NEWCLASS=YES THEN      /* NEWCLASS KEYWORD SPECIFIED    */
         TM    NEWCLASS(OCMTBPTR),B'00000001'                      0143
         BNO   @RF00143                                            0143
*           DO;                                                    0144
*             SSSOSETC=ON;          /* SUBSYSTEM TO CHANGE CLASS     */
         OI    SSSOSETC(@14),B'10000000'                           0145
*             SSSOCLAS=NEWCLAS;     /* NEWCLASS CLASS                */
         L     @05,ONEWCPDE(,OCMTBPTR)                             0146
         L     @05,NEWCLPTR(,@05)                                  0146
         MVC   SSSOCLAS(1,@14),NEWCLAS(@05)                        0146
*           END;                                                   0147
*         IF DEST=YES THEN          /* DEST KEYWORD SPECIFIED        */
@RF00143 TM    DEST(OCMTBPTR),B'01000000'                          0148
         BNO   @RF00148                                            0148
*           DO;                                                    0149
*             SSSOROUT=ON;          /* SUBSYSTEM TO ROUTE TO DEST    */
         L     @14,OUTSOBSO(,OCMTBPTR)                             0150
         OI    SSSOROUT(@14),B'00100000'                           0150
*             SSSODEST=' ';         /* BLANK DESTID FIELD            */
         MVI   SSSODEST+1(@14),C' '                                0151
         MVC   SSSODEST+2(6,@14),SSSODEST+1(@14)                   0151
         MVI   SSSODEST(@14),C' '                                  0151
*             SSSODEST(1:DESTLEN)=DESTID(1:DESTLEN);/* REMOTE USERID */
         L     @05,ODESTPDE(,OCMTBPTR)                             0152
         LH    @02,DESTLEN(,@05)                                   0152
         BCTR  @02,0                                               0152
         L     @05,DESTPTR(,@05)                                   0152
         EX    @02,@SM01582                                        0152
*           END;                                                   0153
*         IF HOLD=YES THEN          /* HOLD KEYWORD SPECIFIED        */
@RF00148 TM    HOLD(OCMTBPTR),B'01000000'                          0154
         BNO   @RF00154                                            0154
*           SSSOHOLD=ON;            /* SUBSYSTEM SHOULD HOLD DS      */
         L     @14,OUTSOBSO(,OCMTBPTR)                             0155
         OI    SSSOHOLD(@14),B'00010000'                           0155
*         ELSE                      /* NOHOLD SPECIFIED OR DEFAULTED */
*           SSSORLSE=ON;            /* SUBSYS SHOULD RELEASE DS      */
         B     @RC00154                                            0156
@RF00154 L     @14,OUTSOBSO(,OCMTBPTR)                             0156
         OI    SSSORLSE(@14),B'00001000'                           0156
*       END;                                                       0157
*   END INITSSSO;                                                  0158
@EL00002 DS    0H                                                  0158
@EF00002 DS    0H                                                  0158
@ER00002 LM    @14,@12,12(@13)                                     0158
         BR    @14                                                 0158
*                                                                  0159
*/********************************************************************/
*/*                                                                  */
*/*    INVOKE THE SUBSYSTEM TO:                                      */
*/*      LOCATE SYSOUT DATA SETS FOR THE JOB/CLASS COMBINATION       */
*/*        PASSED BY IKJCT469.                                       */
*/*      DELETE SYSOUT DATA SETS IN THE JOB/CLASSLIST PASSED.        */
*/*      ROUTE SYSOUT DATA SETS IN THE JOB/CLASSLIST PASSED.         */
*/*      ANALYZE THE RETURN CODE FROM THE SUBSYSTEM AND TAKE         */
*/*        APPROPRIATE ACTION.                                       */
*/*                                                                  */
*/********************************************************************/
*                                                                  0159
*SUBSYSTM:                                                         0159
*   PROC;                           /* INTERNAL PROCEDURE TO INVOKE
*                                      THE SUBSYSTEM                 */
SUBSYSTM STM   @14,@12,@SA00003                                    0159
*/********************************************************************/
*/*   INVOKE SUBSYSTEM TO PERFORM THE REQUESTED FUNCTION             */
*/********************************************************************/
*                                                                  0160
*   REG1=ADDR(OUTSOBH);             /* PASS SSOB PTR TO SUBSYSTEM    */
         LA    REG1,OUTSOBH(,OCMTBPTR)                             0160
*   HASPABND=ON;                    /* IN CASE SUBSYS ABENDS, TELL 0161
*                                      ESTAE EXIT                    */
         OI    HASPABND(OCMTBPTR),B'00000001'                      0161
*/*  ? IEFSSREQ;                                                     */
*/*                                                                  */
*/*  PASS CONTROL TO JOB ENTRY SUBSYSTEM TO                          */
*/*  PROCESS REQUEST -                                               */
*/*                                                                  */
*/*    INPUT - REGISTER 1 POINTS TO A ONE-                           */
*/*            WORD PARAMETER LIST WHICH                             */
*/*            POINTS TO THE SSOB                                    */
*/*                                                                  */
*                                                                  0162
*   CALL SSREQ;                     /* INVOKE SUBSYSTEM              */
         L     @05,CVTPTR                                          0162
         L     @05,CVTJESCT(,@05)                                  0162
         L     @15,JESSSREQ(,@05)                                  0162
         BALR  @14,@15                                             0162
*   HASPABND=OFF;                   /* NO LONGER IN SUBSYS           */
         NI    HASPABND(OCMTBPTR),B'11111110'                      0163
*   OUTRTCD=REG15;                  /* SAVE SUBSYSTEM RETURN CODE    */
         STH   REG15,OUTRTCD(,OCMTBPTR)                            0164
*   SSSOCHKP=OFF;                   /* TURN OFF IN CASE SET BY     0165
*                                      PREVIOUS CALL                 */
         L     @14,OUTSOBSO(,OCMTBPTR)                             0165
         NI    SSSOCHKP(@14),B'10111111'                           0165
*                                                                  0166
*/********************************************************************/
*/*  ANALYZE THE SUBSYSTEM RETURN CODE                               */
*/********************************************************************/
*                                                                  0166
*   IF OUTRTCD=SSRTOK THEN          /* SUBSYSTEM COMPLETED OK        */
*                                                                  0166
         SLR   @14,@14                                             0166
         LH    @05,OUTRTCD(,OCMTBPTR)                              0166
         CR    @05,@14                                             0166
         BNE   @RF00166                                            0166
*     /***************************************************************/
*     /*                                                             */
*     /* SUBSYSTEM RETURN CODE IS ZERO                               */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0167
*     DO;                           /* CHECK SSOBRETN FIELD          */
*       IF SSOBRETN=SSSORTOK THEN   /* SSOBRETN=0 - COMPLETED OK     */
*                                                                  0168
         L     @05,OUTSOBH(,OCMTBPTR)                              0168
         C     @14,SSOBRETN(,@05)                                  0168
         BNE   @RF00168                                            0168
*         /***********************************************************/
*         /*                                                         */
*         /* SSOBRETN IS ZERO (SYSOUT DSN WAS RETURNED)              */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0169
*         DO;                                                      0169
*           RETNCODE=ZERO;          /* TELL 69 TO PASS NEXT JB/CL    */
         ST    @14,RETNCODE                                        0170
*           WORKDONE=YES;           /* WORK DONE FOR THIS JOB.     0171
*                                      IKJCT469 INITS THIS BIT FOR 0171
*                                      1ST CALL TO '62 FOR EACH    0171
*                                      JOBNAME.                      */
         OI    WORKDONE(OCMTBPTR),B'00010000'                      0171
*           IF PRINT=YES THEN       /* IF PRINT FUNCTION,            */
         TM    PRINT(OCMTBPTR),B'00000010'                         0172
         BNO   @RF00172                                            0172
*             SUBSYS=ON;            /* INDICATE SUBSYS PROCESSING A
*                                      SET OF DATA SETS              */
         OI    SUBSYS(OCMTBPTR),B'10000000'                        0173
*           ELSE                    /* ELSE DELETE OR ROUTE FUNC     */
*             LOOP=NO;              /* FOR ROUTING, DONT LOOP TO   0174
*                                      SUBSYS, GO BACK TO IKJCT469 IN
*                                      CASE MORE CLASSES FOR THIS  0174
*                                      JOB, OR ANOTHER JOB TO PROCESS*/
         B     @RC00172                                            0174
@RF00172 NI    LOOP,B'01111111'                                    0174
*         END;                                                     0175
*       ELSE                        /* SSOBRETN NON ZERO             */
*                                                                  0176
*         /***********************************************************/
*         /*                                                         */
*         /* SSOBRETN IS NON ZERO - FIND OUT WHAT IT IS              */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0176
*         IF SSOBRETN=SSSOEODS THEN /* SSOBRETN=4 - NO DATASETS OR NO
*                                      MORE DATASETS                 */
*                                                                  0176
         B     @RC00168                                            0176
@RF00168 L     @14,OUTSOBH(,OCMTBPTR)                              0176
         CLC   SSOBRETN(4,@14),@CF00053                            0176
         BNE   @RF00176                                            0176
*           /*********************************************************/
*           /*                                                       */
*           /* SSOBRETN=4 (NO DATASETS OR NO MORE DATASETS)          */
*           /*                                                       */
