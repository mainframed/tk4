         ST    OCMTBPTR,@AL00001                                   0430
         L     @15,@CV01406                                        0430
         LA    @01,@AL00001                                        0430
         BALR  @14,@15                                             0430
*       RETNCODE=TWELVE;            /* SET ERROR RETURN CODE         */
         MVC   RETNCODE(4),@CF00034                                0431
