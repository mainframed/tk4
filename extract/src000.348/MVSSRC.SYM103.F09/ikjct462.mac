         TM    TERM(OCMTBPTR),B'00001000'                          0470
         BNO   @RF00470                                            0470
*             DO;                                                  0471
*               OUTHOLD=OUTBKAPX;   /* SET POINT ARG FOR '70         */
         MVC   OUTHOLD(8,OCMTBPTR),OUTBKAPX(OCMTBPTR)              0472
