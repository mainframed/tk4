         NI    SUBCONT(OCMTBPTR),B'11011011'                       0481
*             IF KEEP=YES THEN      /* IF KEEP REQUESTED             */
         TM    KEEP(OCMTBPTR),B'10000000'                          0482
         BNO   @RF00482                                            0482
*               DO;                                                0483
*                 SSSOCHKP=YES;     /* TELL SUBSYS TO CKPT SYSOUT    */
         L     @14,OUTSOBSO(,OCMTBPTR)                             0484
         OI    SSSOCHKP(@14),B'01000000'                           0484
*                 IF TERM=YES THEN  /* PRINTING TO A TERMINAL        */
         TM    TERM(OCMTBPTR),B'00001000'                          0485
         BNO   @RF00485                                            0485
*                   SSSORBA=OUTBKAPX;/* GIVE SUSBYS THE CKPT RBA   0486
*                                      CORRESPONDING TO APPROX 10 PUT
*                                      LINES BACK.                   */
         MVC   SSSORBA(8,@14),OUTBKAPX(OCMTBPTR)                   0486
*                 ELSE              /* PRINTING TO A DATASET         */
*                   SSSORBA=OUTHOLD;                               0487
         B     @RC00485                                            0487
@RF00485 L     @14,OUTSOBSO(,OCMTBPTR)                             0487
         MVC   SSSORBA(8,@14),OUTHOLD(OCMTBPTR)                    0487
*               END;                                               0488
*             ELSE                                                 0489
*               ;                   /* DS TO BE DELETED, SO DON'T  0489
