         LA    @14,IOPLAREA(,OCMTBPTR)                             0551
         ST    @14,IOPLPTR(,OCMTBPTR)                              0551
*     IOPLUPT=CPPLUPT;              /* ADDR OF UPT                   */
         L     @05,CPPLPTR(,OCMTBPTR)                              0552
         MVC   IOPLUPT(4,@14),CPPLUPT(@05)                         0552
*     IOPLECT=CPPLECT;              /* ADDR OF ECT                   */
         MVC   IOPLECT(4,@14),CPPLECT(@05)                         0553
