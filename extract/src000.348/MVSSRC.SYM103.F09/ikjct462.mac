         LA    @02,ACBCORE                                         0413
         ST    @02,RPLDACB(,@14)                                   0413
*   RPLARG=ADDR(OUTHOLD);           /* ADDR SEARCH ARG FOR POINT     */
         LA    @09,OUTHOLD(,OCMTBPTR)                              0414
         ST    @09,RPLARG(,@14)                                    0414
*/********************************************************************/
*/*   INITIALIZE THE ACB AND ASSOCIATED OUTCOMTB FIELDS              */
*/********************************************************************/
*                                                                  0415
*   OUTACB=ADDR(ACBCORE);           /* POINTER TO ACB IN OUTCOMTB    */
         ST    @02,OUTACB(,OCMTBPTR)                               0415
*   OUTACBL=LENGTH(ACBCORE);        /* LENGTH OF ACB IN OUTCOMTB     */
         ST    @05,OUTACBL(,OCMTBPTR)                              0416
*   RFY                                                            0417
*     IFGACB BASED(OUTACB);         /* BASE ACB MAP ON ACB CORE      */
*   IFGACB=IFGACB&&IFGACB;          /* CLEAR CORE FOR THE ACB        */
         XC    IFGACB(76,@02),IFGACB(@02)                          0418
*   IFGACB=INPACB;                  /* MOVE ACB TO DYNAMIC CORE      */
         MVC   IFGACB(76,@02),INPACB                               0419
*   ACBDDNM=OSYSODDN;               /* DDNAME IN ACB FROM TXT UNT    */
         MVC   ACBDDNM(8,@02),OSYSODDN(OCMTBPTR)                   0420
*   ACBLENG=LENGTH(ACBCORE);        /* ACB LEN IN ACB FOR SUBSYS     */
         STH   @05,ACBLENG(,@02)                                   0421
*/********************************************************************/
*/*    OPEN THE ACB                                                  */
*/********************************************************************/
*                                                                  0422
*   OPENLSTA(1:LNGOLSTS)=OPENLSTS(1:LNGOLSTS);/* MOVE OPEN LIST TO 0422
*                                      DYN CORE                      */
         L     @14,LNGOLSTS                                        0422
         BCTR  @14,0                                               0422
         EX    @14,@SM01592                                        0422
*   REG4=ADDR(ACBCORE);             /* ADDR OF ACB TO OPEN           */
         LR    REG4,@02                                            0423
