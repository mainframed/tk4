*/********************************************************************/
*/*                                                                  */
*/*    DETERMINE WHERE TO BEGIN PRINTING BASED ON WHETHER 'HERE',    */
*/*      'BEGIN', OR 'NEXT' WAS ON THE COMMAND AND WHETHER THE       */
*/*      SYSOUT DATA SET HAS BEEN PREVIOUSLY CHECKPOINTED.           */
*/*    IF THE SYSOUT DATA SET IS NOT TO BE SKIPPED, INITIALIZE THE   */
*/*      RPL/ACB CONTROL BLOCKS AND OPEN THE SYSOUT DATA SET.        */
*/*    READ AND WRITE THE SYSOUT DATA SET UNTIL:                     */
*/*      ATTENTION AND 'CONTINUE NEXT' SUBCOMMAND ENTERED            */
*/*      ATTENTION AND 'END' SUBCOMMAND ENTERED                      */
*/*      END OF DATA AND NOPAUSE WAS SPECIFIED                       */
*/*      END OF DATA,PAUSE SPECIFIED, AND 'CONTINUE HERE' SUBCOMMAND */
*/*      END OF DATA,PAUSE SPECIFIED, AND 'CONTINUE NEXT' SUBCOMMAND */
*/*      END OF DATA,PAUSE SPECIFIED, AND 'END' SUBCOMMAND           */
*/*      ERROR OCCURS                                                */
*/*    IF EOD/PAUSE OR ATTENTION OCCURS, CALL IKJCT463 TO PROCESS    */
*/*      SUBCOMMANDS.                                                */
*/*    WHEN DONE WITH THE SYSOUT DATASET, CLOSE IT.                  */
*/*                                                                  */
*/********************************************************************/
*                                                                  0343
*PRINTRTN:                                                         0343
*   PROC;                           /* INTERNAL PROCEDURE TO READ AND
*                                      WRITE SYSOUT RCDS             */
PRINTRTN STM   @14,@12,@SA00005                                    0343
*   CALL STARTPT;                   /* CALL RTN TO DECIDE WHERE TO 0344
*                                      BEGIN PRINTING SYSOUT DS      */
         BAL   @14,STARTPT                                         0344
*   IF SKIPDS=NO THEN               /* SYSOUT DS NOT TO BE SKIPPED   */
         TM    SKIPDS,B'10000000'                                  0345
         BNZ   @RF00345                                            0345
*     DO;                                                          0346
*       CALL OPENSYSO;              /* CALL RTN TO INIT RPL & ACB, 0347
*                                      OPEN DS AND GET INPUT BUFFERS */
         BAL   @14,OPENSYSO                                        0347
*       DO WHILE ERROR=NO&SKIPGET=NO&ENDSW=NO;/* THIS LOOP GETS AND
*                                      PUTS SYSOUT RCDS UNTIL ERROR
*                                      OR FINISHED WITH THIS DATASET:
*                                      ATTN/CONT NEXT EOD-NOPAUSE  0348
*                                      EOD-PAUSE/CONT HERE         0348
*                                      EOD-PAUSE/CONT NEXT           */
         B     @DE00348                                            0348
@DL00348 DS    0H                                                  0349
*         CALL IKJCT470(OPTR);      /* GO GET A RECORD               */
         L     @14,@PC00001                                        0349
         ST    @14,@AL00001                                        0349
         L     @15,@CV01407                                        0349
         LA    @01,@AL00001                                        0349
         BALR  @14,@15                                             0349
*         IF POSTED=YES THEN        /* ATTN INTERRUPT OCCURRED       */
         TM    POSTED(OCMTBPTR),B'01000000'                        0350
         BNO   @RF00350                                            0350
*           CALL ATTNRTN;           /* HANDLE SUBCOMMANDS            */
         BAL   @14,ATTNRTN                                         0351
*         ELSE                      /* CHECK FURTHER                 */
*           IF EODSW=YES THEN       /* EOD REACHED ON SYSOUT         */
         B     @RC00350                                            0352
@RF00350 TM    EODSW(OCMTBPTR),B'00010000'                         0352
         BNO   @RF00352                                            0352
*             DO;                                                  0353
*               IF TERM=YES THEN    /* PRINTING TO A TERMINAL        */
         TM    TERM(OCMTBPTR),B'00001000'                          0354
         BNO   @RF00354                                            0354
*                 CALL IKJCT471(OPTR);/* CALL '71 TO PUT A BLANK   0355
*                                      SEPARATOR AFTER THIS DS       */
         L     @14,@PC00001                                        0355
         ST    @14,@AL00001                                        0355
         L     @15,@CV01408                                        0355
         LA    @01,@AL00001                                        0355
         BALR  @14,@15                                             0355
*               CALL EODRTN;        /* GO HANDLE END OF DATA         */
@RF00354 BAL   @14,EODRTN                                          0356
*             END;                                                 0357
*           ELSE                    /* ELSE GOT A RCD OR AN ERROR. IF
*                                      ERROR, SWITCH ALREADY SET TO
*                                      STOP FURTHER PROCESSING. IF 0358
*                                      SUCCESSFUL RETRIEVE OF RCD, 0358
*                                      CONTINUE WITH PUT.            */
*             IF ERROR=YES THEN     /* ERROR DETECTED DURING GET     */
         B     @RC00352                                            0358
@RF00352 TM    ERROR(OCMTBPTR),B'00001000'                         0358
         BNO   @RF00358                                            0358
*               DO;                                                0359
*                 SSSOCHKP=YES;     /* TELL SUBSYS TO CHECKPOINT     */
         L     @14,OUTSOBSO(,OCMTBPTR)                             0360
         OI    SSSOCHKP(@14),B'01000000'                           0360
*                 SSSORBA=OUTHOLD;  /* CKPT RBA CORRESPONDS TO LAST
*                                      RCD PUT. ANY RCDS STACKED IN
*                                      BUF WILL BE PUT AT CLOSE.     */
         MVC   SSSORBA(8,@14),OUTHOLD(OCMTBPTR)                    0361
*               END;                                               0362
*             ELSE                  /* SUCCESSFUL GET                */
*               DO;                                                0363
         B     @RC00358                                            0363
@RF00358 DS    0H                                                  0364
*                 CALL IKJCT471(OPTR);/* GO 'PUT' THE RECORD         */
         L     @14,@PC00001                                        0364
         ST    @14,@AL00001                                        0364
         L     @15,@CV01408                                        0364
         LA    @01,@AL00001                                        0364
         BALR  @14,@15                                             0364
*                 IF SYNADERR=YES THEN/* ERROR WHILE PUT OF RCD      */
         TM    SYNADERR(OCMTBPTR),B'10000000'                      0365
         BNO   @RF00365                                            0365
*                   DO;                                            0366
*                     RETNCODE=TWELVE;/* SET RETURN CODE FOR '69     */
         MVC   RETNCODE(4),@CF00034                                0367
*                     ERROR=YES;    /* INDICATE TERMINATING ERR      */
         OI    ERROR(OCMTBPTR),B'00001000'                         0368
*                     SSSOCHKP=YES; /* TELL SUBSYS TO CKPT           */
         L     @14,OUTSOBSO(,OCMTBPTR)                             0369
         OI    SSSOCHKP(@14),B'01000000'                           0369
*                     SSSORBA=OUTBKAPX;/* CKPT 10 PUT RCDS BACK SINCE
*                                      LAST BUFFER WAS NOT WRITTEN 0370
*                                      OUT                           */
         MVC   SSSORBA(8,@14),OUTBKAPX(OCMTBPTR)                   0370
*                   END;                                           0371
*               END;                                               0372
@RF00365 DS    0H                                                  0373
*       END;                        /* END OF GET-PUT DO WHILE       */
@RC00358 DS    0H                                                  0373
@RC00352 DS    0H                                                  0373
@RC00350 DS    0H                                                  0373
@DE00348 TM    ERROR(OCMTBPTR),B'00001000'                         0373
         BNZ   @DC00348                                            0373
         TM    SKIPGET,B'10000000'                                 0373
         BNZ   @DC00348                                            0373
         TM    ENDSW(OCMTBPTR),B'00010000'                         0373
         BZ    @DL00348                                            0373
@DC00348 DS    0H                                                  0374
*       SKIPGET=OFF;                /* TURN OFF SKIPGET SWITCH       */
         NI    SKIPGET,B'01111111'                                 0374
*       CALL CLSSYSO;               /* DONE WITH THIS DATASET. GO  0375
*                                      FREE BUFS AND CLOSE IT.       */
         BAL   @14,CLSSYSO                                         0375
*     END;                          /* END OPEN/PRINT/CLOSE GROUP    */
*   ELSE                            /* ELSE SKIP PRINTING THIS DS    */
*     SKIPDS=OFF;                   /* TURN OFF SKIPDS SWITCH        */
         B     @RC00345                                            0377
@RF00345 NI    SKIPDS,B'01111111'                                  0377
