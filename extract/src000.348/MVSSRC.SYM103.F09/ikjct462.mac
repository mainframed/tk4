         OI    POINT(OCMTBPTR),B'10000000'                         0457
*         OUTHOLD=ZEROS;            /* SET POINT ARG FOR '70         */
         MVC   OUTHOLD(8,OCMTBPTR),@CB01394                        0458
*         OUTBKAPX=ZEROS;           /* APPROX CTRS AT 1ST RCD AGAIN  */
         MVC   OUTBKAPX(8,OCMTBPTR),@CB01394                       0459
*         OUTBKNEW=ZEROS;           /* APPROX CTRS AT 1ST RCD AGAIN  */
         MVC   OUTBKNEW(8,OCMTBPTR),@CB01394                       0460
