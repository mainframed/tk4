         TITLE 'IKJEES70 - LISTBC COMMAND PROCESSOR                    *
                        '
IKJEES70 CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL2(@EP00001-@MAINENT)
IKJEES73 DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL2(@EP00319-IKJEES73)
         ENTRY IKJEES73
         DROP  @15
@PROLOG  ST    @14,12(,@13)                                        0001
         STM   @00,@12,20(@13)                                     0001
         BALR  @12,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@12                                         0001
         LR    @10,@15                                             0001
         L     @00,@SIZDATD                                        0001
         GETMAIN R,LV=(0)
         LR    @11,@01                                             0001
         USING @DATD,@11                                           0001
         LR    @15,@10                                             0001
         ST    @13,@SA00001+4                                      0001
         LM    @00,@01,20(@13)                                     0001
         ST    @11,8(,@13)                                         0001
         LR    @13,@11                                             0001
         XC    @ZTEMPS(@ZLEN),@ZTEMPS
         AH    @15,4(,@15)                                         0001
         BR    @15                                                 0001
@EP00001 MVC   @PC00001(4),0(@01)                                  0001
         MODID BRANCH=YES
REGS     DS    0H                                                  0004
         EJECT
*                                                                    */
*                                                                  0066
*/********************************************************************/
*/*                                                                  */
*/*    FOLLOWING ARE REGISTERS USED OFTEN AND NOT CHANGING:          */
*/*                                                                  */
*/*               REG2- POINTS TO PARMS FROM THE TMP                 */
*/*               REG4- POINTS TO BUFFER FOR THE DIRECTORY TO BE     */
*/*                     READ IN AND PROCESSED IN CORE                */
*/*               REG5- POINTS TO BUFFER FOR MESSAGES FROM           */
*/*                     SYS1.BRODCAST                                */
*/*                                                                  */
*/********************************************************************/
*/*      THIS CODE CLEARS AUTOMATIC CORE.                            */
*                                                                  0066
*   REG2=PARMRGR;                   /* SAVE PTR FROM TMP       Y02676*/
         LR    REG2,PARMRGR                                        0066
*   GENERATE;                                                      0067
*                                                                  0067
         L     @1,@SA00001+4        SAVE CALLER'S REG13
         XR    @3,@3                CLEAR REGISTER 3
         LA    @4,@DATD             START OF AUTOMATIC CORE
         LA    @5,@DATEND-@DATD     LENGTH OF AUTOMATIC CORE
         MVCL  @4,@2                MOVE LONG ZEROES AUTOMATIC CORE
         ST    @1,@SA00001+4        RESTORE CALLER'S REG13
*/*      INITIALIZE BASE PTRS WHICH REMAIN UNCHANGED IN THIS MODULE  */
*                                                                  0068
*BCINIT:                                                           0068
*   REG4=ADDR(BCDIRBF);             /* SET PTR TO DIR BUFFER         */
BCINIT   LA    REG4,BCDIRBF                                        0068
*   REG5=ADDR(BCBUF);               /* SET PTR TO MSG BUFFER   Y02676*/
         LA    REG5,BCBUF                                          0069
*   IOPLPTR=ADDR(IOPLAREA);         /* PTR TO PUTLINE PARM BLOCK     */
         LA    @10,IOPLAREA                                        0070
         ST    @10,IOPLPTR                                         0070
*   IOPLECB=ADDR(BCCECB);           /* PTR TO COMM ECB               */
         LA    @15,BCCECB                                          0071
         ST    @15,IOPLECB(,@10)                                   0071
*   STPBPTR=ADDR(STPBAREA);         /* PTR TO STACK PARM BLOCK M4092 */
         LA    @15,STPBAREA                                        0072
         ST    @15,STPBPTR                                         0072
*/********************************************************************/
*/*                                                                  */
*/*     OBTAIN DCB, ENQUE ON SYS1.BRODCAST (RECORD1),                */
*/*     INITIALIZE PUTLINE PARM LIST AND LOAD PUTLINE MODULES        */
*/*                                                                  */
*/********************************************************************/
*                                                                  0073
*   IOPLUPT=CPPLUPT;                /* MOVE UPT PTR TO PUTLINE LIST  */
         MVC   IOPLUPT(4,@10),CPPLUPT(CPPLPTR)                     0073
*   IOPLECT=CPPLECT;                /* MOVE ECT PTR TO PUTLINE LIST  */
         MVC   IOPLECT(4,@10),CPPLECT(CPPLPTR)                     0074
*   BCPUTLP=ADDR(IOPL);             /* PTR TO PUTLINE PARMS          */
         ST    @10,BCPUTLP                                         0075
*   PARMRGR=ADDR(BCIOPARM);         /* INIT PTR FOR I/O PARM         */
         LA    PARMRGR,BCIOPARM                                    0076
*   BCENQ='1'B;                     /* INDICATE ENQUEUE              */
         OI    BCENQ,B'01000000'                                   0077
*   BCLOAD='1'B;                    /* LOAD PUTLINE MODULES Y02676   */
         OI    BCLOAD,B'00100000'                                  0078
*   CALL IKJEES75;                  /* CALL I/O MODULE TO SET UP DCB
*                                      IN DYNAMIC CORE               */
         L     @15,@CV00933                                        0079
         BALR  @14,@15                                             0079
*/********************************************************************/
*/*     FOLLOWING IS THE INTERFACE WITH PARSE TO SYNTAX CHECK        */
*/*     OPERANDS.  THIS CODE WILL NOT BE EXECUTED IF THERE ARE NO    */
*/*     OPERANDS OR IF CONTROL COMES FROM LOGON THROUGH THE SPECIAL  */
*/*      LOGON ENTRY.                                                */
*/********************************************************************/
*                                                                  0080
*   IF BCLOGON='1'B                 /* ENTRY FROM LOGON        Y02676*/
*     THEN                          /* YES, DO NOT PARSE       Y02676*/
         TM    BCLOGON,B'00001000'                                 0080
         BO    @RT00080                                            0080
*     GOTO BCNXT;                   /* SKIP DAIR AND PARSE     Y02676*/
*   IF ECTNOPD='1'B THEN                                           0082
         L     @10,ECTPTR(,CPPLPTR)                                0082
         TM    ECTNOPD(@10),B'10000000'                            0082
         BNO   @RF00082                                            0082
*     DO;                           /* NO OPERANDS ON COMMAND        */
*       BCMAIL='1'B;                /* GIVE USER MESSAGES (MAIL)     */
*       BCNOTICE='1'B;              /* GIVE GENERAL MESSAGES (NOTICE)*/
         OI    BCMAIL,B'00110000'                                  0085
*       GO TO BCDAIR;               /* SKIP PARSE. BRANCH TO ALLOCATE
*                                      BROADCAST DATA SET            */
         B     BCDAIR                                              0086
*     END;                                                         0087
*   PPLPTR=ADDR(PLIST);             /* SET PTR TO AUTOMATIC CORE     */
@RF00082 LA    @10,PLIST                                           0088
         ST    @10,PPLPTR                                          0088
*   PPLUPT=CPPLUPT;                 /* MOVE UPT PTR FROM TMP LIST    */
         MVC   PPLUPT(4,@10),CPPLUPT(CPPLPTR)                      0089
*   PPLECT=CPPLECT;                 /* MOVE ECT PTR FROM TMP LIST    */
         MVC   PPLECT(4,@10),CPPLECT(CPPLPTR)                      0090
*   PPLECB=IOPLECB;                 /* MOVE PTR TO ECB               */
         L     @01,IOPLPTR                                         0091
         MVC   PPLECB(4,@10),IOPLECB(@01)                          0091
*   PPLCBUF=CPPLCBUF;               /* MOVE COMMAND BUFFER PTR FROM
*                                      TMP PARAMETER LIST            */
         MVC   PPLCBUF(4,@10),CPPLCBUF(CPPLPTR)                    0092
*   PPLANS=ADDR(BCANSPL);           /* PTR TO ANSWER PLACE           */
         LA    @15,BCANSPL                                         0093
         ST    @15,PPLANS(,@10)                                    0093
*   PPLPCL=ADDR(BCPARS1);           /* PTR TO GENERATED PCL          */
         L     @15,@CV00260                                        0094
         ST    @15,PPLPCL(,@10)                                    0094
*   PARMRGR=PPLPTR;                 /* INITIALIZE PARM REGISTER      */
         LR    PARMRGR,@10                                         0095
*   DO;                             /* CALLTSSR EP(IKJPARS)          */
*     RESPECIFY                                                    0097
*       GPR01P RSTD;                                               0097
*     IF CVTPARS=0 THEN                                            0098
         L     @10,CVTPTR                                          0098
         L     @10,CVTPARS(,@10)                                   0098
         LTR   @10,@10                                             0098
         BNZ   @RF00098                                            0098
*       GEN(LINK EP=IKJPARS);                                      0099
         LINK EP=IKJPARS
*     ELSE                                                         0100
*       CALL CVTPARS;                                              0100
         B     @RC00098                                            0100
@RF00098 L     @10,CVTPTR                                          0100
         L     @15,CVTPARS(,@10)                                   0100
         BALR  @14,@15                                             0100
*     RESPECIFY                                                    0101
*       GPR01P UNRSTD;                                             0101
@RC00098 DS    0H                                                  0102
*   END;                            /* PARSING ROUTINE YM2225        */
*/****************************************************************** */
*/*      UPON RETURN FROM PARSE, THE RETURN CODE WILL INDICATE       */
*/*      NORMAL RETURN OR ERROR RETURN.  IF ANY ERROR CONDITION      */
*/*      EXISTS, THE CAUSE OF THE ERROR IS DETERMINED, THEN THE      */
*/*      MODULE IKJEES75 IS CALLED TO ISSUE THE APPROPRIATE          */
*/*      ERROR MESSAGE TO THE TERMINAL.                              */
*/*      UPON NORMAL COMPLETION, THE RESULTS RETURNED                */
*/*      FROM PARSE ARE USED TO DETERMINE WHICH SET OR SETS OF       */
*/*      MESSAGES FROM THE SYS1.BRODCAST DATA SET HAVE BEEN          */
*/*      REQUESTED.                                                  */
*/****************************************************************** */
*                                                                  0103
*   RETCDC=RETCD;                   /* STORE REG 15 FOR TEST         */
         ST    RETCD,RETCDC                                        0103
*   IF RETCDC=4                     /* TEST FOR NO PROMPT M4092      */
*     THEN                          /* YES, M4092                    */
         L     @10,RETCDC                                          0104
         CH    @10,@CH00188                                        0104
         BE    @RT00104                                            0104
*     GOTO BCND;                    /* GO TO FLUSH STACK & CLEAR QUE
*                                      M4092                         */
*   IF RETCDC=8                     /* TEST RETURN CODE FROM PARSE   */
*     THEN                          /* YES,                          */
         LA    @15,8                                               0106
         CR    @10,@15                                             0106
         BE    @RT00106                                            0106
*     GO TO BCND;                   /* ATTENTION INTERRUPT           */
*   IF RETCDC>0                     /* WAS PARSE SUCCESSFUL?         */
*     THEN                          /* NO                            */
         LTR   @10,@10                                             0108
         BNP   @RF00108                                            0108
*     DO;                           /* DETERMINE THE REASON          */
*       BCERFLG='1'B;               /* INDICATE THIS IS AN ERROR   0110
*                                      CONDITION                     */
         OI    BCERFLG,B'10000000'                                 0110
*       BCPUTL='1'B;                /* INDICATE PUTLINE TO BE USED   */
*       BCDEQ='1'B;                 /* DEQ FROM BROADCAST DATA SET   */
         OI    BCPUTL,B'00000101'                                  0112
*       IF RETCDC=16                /* WAS THERE INSUFFICIENT CORE TO
*                                      EXECUTE THE PARSE COMMAND ?   */
*         THEN                      /* YES                           */
         CH    @10,@CH00069                                        0113
         BNE   @RF00113                                            0113
*         DO;                                                      0114
*           BCERCDE=8;              /* INDICATE ERROR MSG INDEX      */
         STH   @15,BCERCDE                                         0115
*           BC1LVL='1'B;            /* INDICATE ONE-LEVEL ERROR MSG  */
         OI    BC1LVL,B'01000000'                                  0116
*         END;                                                     0117
*       ELSE                        /* PARSE UNSUCCESSFUL DUE TO A 0118
*                                      SYSTEM ERROR                  */
*         DO;                                                      0118
         B     @RC00113                                            0118
@RF00113 DS    0H                                                  0119
*           BCTXTIN='1'B;           /* INDICATE TEXT INSERTION FOR 0119
*                                      THIS ERROR MESSAGE            */
         OI    BCTXTIN,B'00100000'                                 0119
*           BCERCDE=13;             /* INDICATE ERROR MSG INDEX      */
         MVC   BCERCDE(2),@CH00085                                 0120
*           BCRTCDE=RETCDC;         /* SAVE RETURN CODE FOR TEXT   0121
*                                      INSERTION INTO ERROR MSG      */
         MVC   BCRTCDE(2),RETCDC+2                                 0121
*         END;                                                     0122
*       PARMRGR=ADDR(BCIOPARM);                                    0123
@RC00113 LA    PARMRGR,BCIOPARM                                    0123
*       CALL IKJEES75;              /* CALL IKJEES75 TO ISSUE      0124
*                                      APPROPRIATE ERROR MESSAGE     */
         L     @15,@CV00933                                        0124
         BALR  @14,@15                                             0124
*       GOTO BCEND;                 /* TERMINATE               Y02676*/
         B     BCEND                                               0125
*     END;                          /* END DO                        */
*   IF BCMANS=1 THEN                /* WAS MAIL MSGS REQUESTED?      */
@RF00108 L     @10,BCANSPL                                         0127
         CLC   BCMANS(2,@10),@CH00034                              0127
         BNE   @RF00127                                            0127
*     BCMAIL='1'B;                  /* YES, INDICATE MAIL REQUESTED  */
         OI    BCMAIL,B'00100000'                                  0128
*   IF BCNANS=1 THEN                /* WAS NOTICE MSGS REQUESTED?    */
@RF00127 L     @10,BCANSPL                                         0129
         CLC   BCNANS(2,@10),@CH00034                              0129
         BNE   @RF00129                                            0129
*     BCNOTICE='1'B;                /* YES, INDICATE NOTICE REQUESTED*/
         OI    BCNOTICE,B'00010000'                                0130
*   IF BCMANS=2 THEN                /* CHECK FOR NULL SITUATION      */
@RF00129 LA    @10,2                                               0131
         L     @15,BCANSPL                                         0131
         CH    @10,BCMANS(,@15)                                    0131
         BNE   @RF00131                                            0131
*     DO;                                                          0132
*       IF BCNANS=2 THEN            /* NO MESSAGES ARE REQUESTED     */
         CH    @10,BCNANS(,@15)                                    0133
         BE    @RT00133                                            0133
*         GO TO BCND;               /* EXIT FROM ROUTINE             */
*     END;                                                         0135
*BCNEXT:                                                           0136
*   ;                                                              0136
@RF00131 DS    0H                                                  0136
BCNEXT   DS    0H                                                  0137
*   GENERATE(IKJRLSA BCANSPL);      /* FREE CORE USED BY PARSE       */
         IKJRLSA BCANSPL
*/********************************************************************/
*/* FOLLOWING CODE SETS UP AN INTERFACE WITH DAIR TO GET THE DDNAME  */
*/* BEING USED TO DESCRIBE THE SYS1.BRODCAST DATA SET, OR TO HAVE A  */
*/* DD CARD SET UP IF THERE IS NOT ONE ALREADY AND GET THAT NAME.    */
*/* THIS CODE WILL NOT BE EXECUTED IF CONTROL COMES FROM LOGON.      */
*/********************************************************************/
*                                                                  0138
*BCDAIR:                            /* BEGINNING OF INITIALIZATION 0138
*                                      CODE                          */
*   DAPLPTR=ADDR(DAIRLST);          /* PTR TO DAIR GENERAL PARM      */
BCDAIR   LA    @10,DAIRLST                                         0138
         ST    @10,DAPLPTR                                         0138
*   DAPB0PTR=ADDR(DAIR08);          /* PTR TO PARM BLOCK             */
         LA    @15,DAIR08                                          0139
         ST    @15,DAPB0PTR                                        0139
*   DAPLDAPB=DAPB0PTR;              /* PTR TO PARM BLOCK             */
         ST    @15,DAPLDAPB(,@10)                                  0140
*   DA08PDSN=ADDR(BCDSFLD);         /* PTR TO DSN FIELD              */
         LA    @14,BCDSFLD                                         0141
         ST    @14,DA08PDSN(,@15)                                  0141
*   DA08PSWD=' ';                   /* BLANK OUT FIELD               */
         MVI   DA08PSWD+1(@15),C' '                                0142
         MVC   DA08PSWD+2(6,@15),DA08PSWD+1(@15)                   0142
         MVI   DA08PSWD(@15),C' '                                  0142
*   DA08UNIT=' ';                   /* BLANK OUT FIELD               */
         MVI   DA08UNIT+1(@15),C' '                                0143
         MVC   DA08UNIT+2(6,@15),DA08UNIT+1(@15)                   0143
         MVI   DA08UNIT(@15),C' '                                  0143
*   DA08SER=' ';                    /* BLANK OUT FIELD               */
         MVI   DA08SER+1(@15),C' '                                 0144
         MVC   DA08SER+2(6,@15),DA08SER+1(@15)                     0144
         MVI   DA08SER(@15),C' '                                   0144
*   DA08DDN=' ';                    /* BLANK OUT FIELD               */
         MVI   DA08DDN+1(@15),C' '                                 0145
         MVC   DA08DDN+2(6,@15),DA08DDN+1(@15)                     0145
         MVI   DA08DDN(@15),C' '                                   0145
*   DA08MNM=' ';                    /* BLANK OUT FIELD               */
         MVI   DA08MNM+1(@15),C' '                                 0146
         MVC   DA08MNM+2(6,@15),DA08MNM+1(@15)                     0146
         MVI   DA08MNM(@15),C' '                                   0146
*   DA08SHR='1'B;                   /* INDICATE SHR DISP             */
         OI    DA08SHR(@15),B'00001000'                            0147
*   DA08KEEP='1'B;                  /* INDICATE KEEP DATA SET        */
         OI    DA08KEEP(@15),B'00001000'                           0148
*   DA08KEP='1'B;                   /* INDICATE COND KEEP DS         */
         OI    DA08KEP(@15),B'00001000'                            0149
*   DA08CD='0008'X;                 /* INDICATE TYPE OF INTERFACE    */
         MVC   DA08CD(2,@15),@CB00943                              0150
*   BCDSLEN=13;                     /* INIT LENGTH FIELD             */
         MVC   BCDSLEN(2),@CH00085                                 0151
*   BCDSNAM='SYS1.BRODCAST';        /* INITIALIZE DS NAME FIELD      */
         MVC   BCDSNAM(13),@CC00944                                0152
*   DAPLUPT=CPPLUPT;                /* MOVE UPT PTR FROM TMP         */
         MVC   DAPLUPT(4,@10),CPPLUPT(CPPLPTR)                     0153
*   DAPLECT=CPPLECT;                /* MOVE ECT PTR FROM TMP         */
         MVC   DAPLECT(4,@10),CPPLECT(CPPLPTR)                     0154
*   DAPLPSCB=CPPLPSCB;              /* MOVE PSCB PTR FROM TMP        */
         MVC   DAPLPSCB(4,@10),CPPLPSCB(CPPLPTR)                   0155
*   DAPLECB=IOPLECB;                /* PTR TO LISTBC ECB             */
         L     @01,IOPLPTR                                         0156
         MVC   DAPLECB(4,@10),IOPLECB(@01)                         0156
*   PARMRGR=DAPLPTR;                /* PTR TO PARMS                  */
         LR    PARMRGR,@10                                         0157
*/********************************************************************/
*/*     GO TO DAIR TO ALLOCATE THE SYS1.BRODCAST DATA SET. THE RETURN*/
*/*     CODE WILL INDICATE NORMAL RETURN OR ERROR RETURN. IF THE     */
*/*      RETURN CODE INDICATES AN ERROR CONDITION, THE DAIR          */
*/*      ERROR ANALYSIS ROUTINE (IKJEFF18) IS INVOKED.  HOWEVER,     */
*/*      UPON NORMAL RETURN FROM DAIR, THE DDNAME RETURNED IS        */
*/*      SAVED IN THE I/O PARM LIST.                                 */
*/*      'SYSLBC' IS THE DEFAULT USED IF ENTRY IS FROM LOGON.        */
*/********************************************************************/
*                                                                  0158
*   DO;                             /* CALLTSSR EP(IKJDAIR)          */
*     RESPECIFY                                                    0159
*       GPR01P RSTD;                                               0159
*     IF CVTDAIR=0 THEN                                            0160
         L     @10,CVTPTR                                          0160
         L     @10,CVTDAIR(,@10)                                   0160
         LTR   @10,@10                                             0160
         BNZ   @RF00160                                            0160
*       GEN(LINK EP=IKJDAIR);                                      0161
         LINK EP=IKJDAIR
*     ELSE                                                         0162
*       CALL CVTDAIR;                                              0162
         B     @RC00160                                            0162
@RF00160 L     @10,CVTPTR                                          0162
         L     @15,CVTDAIR(,@10)                                   0162
         BALR  @14,@15                                             0162
*     RESPECIFY                                                    0163
*       GPR01P UNRSTD;                                             0163
@RC00160 DS    0H                                                  0164
*   END;                            /* LINK TO DAIR YM2225           */
*   RETCDC=RETCD;                   /* STORE REG 15 FOR TEST         */
         ST    RETCD,RETCDC                                        0165
*   IF RETCDC>0                     /* TEST RETURN CODE FROM DAIR    */
*     THEN                          /* ALLOCATION UNSUCCESSFUL?      */
         L     @10,RETCDC                                          0166
         LTR   @10,@10                                             0166
         BNP   @RF00166                                            0166
*     DO;                           /* YES, DETERMINE CAUSE AND ISSUE
*                                      APPROPRIATE ERROR MESSAGE TO
*                                      THE TERMINAL                  */
*       F18DAPL=ADDR(DAPL);         /* SET PTR TO DAPL         Y02676*/
         MVC   F18DAPL(4),DAPLPTR                                  0168
*       F18RETCD=ADDR(RETCDC);      /* SET PTR TO RETURN CODE  Y02676*/
         LA    @10,RETCDC                                          0169
         ST    @10,F18RETCD                                        0169
*       F18F02A=ADDR(F18F02);       /* SET PTR TO IKJEFF02 PTR Y02676*/
         LA    @10,F18F02                                          0170
         ST    @10,F18F02A                                         0170
*       F18CLNOA=ADDR(F18CLNO);     /* SET PTR TO CALL NUMBER  Y02676*/
         LA    @10,F18CLNO                                         0171
         ST    @10,F18CLNOA                                        0171
*       PARMRGR=ADDR(F18PARMS);     /* LOAD PARAMETER REGISTER Y02676*/
         LA    PARMRGR,F18PARMS                                    0172
*       GEN(LINK EP=IKJEFF18);      /* LINK TO IKJEFF18 TO PROCESS 0173
*                                      ERROR                   Y02676*/
         LINK EP=IKJEFF18
*       GOTO BCND;                  /* CLEAN UP AND EXIT             */
         B     BCND                                                0174
*     END;                          /* END OF ERROR LOOP             */
*   ELSE                            /* DAIR ALLOCATION SUCCESSFUL    */
*     BCDDNM=DA08DDN;               /* SAVE DDNAME FROM DAIR   Y02676*/
@RF00166 L     @10,DAPB0PTR                                        0176
         MVC   BCDDNM(8),DA08DDN(@10)                              0176
*/********************************************************************/
*/*      OPEN BROADCAST DATA SET.  ENQUEUE UPON AND READ             */
*/*      FIRST RECORD WHICH CONTAINS THE RBAS OF THE FIRST DIRECTORY */
*/*      RECORDS OF BOTH THE USER AND GENERAL PORTIONS               */
*/********************************************************************/
*                                                                  0177
*BCNXT:                             /* BRANCHING POINT               */
*   PARMRGR=ADDR(BCIOPARM);         /* INIT PTR FOR I/O PARM         */
BCNXT    LA    PARMRGR,BCIOPARM                                    0177
*   BCOPEN='1'B;                    /* INDICATE OPEN           Y02676*/
*   BCRD1='1'B;                     /* INDICATE READ RECORD 1  Y02676*/
         OI    BCOPEN,B'10010000'                                  0179
*   BCDIRR='1'B;                    /* USE DIRECTORY BUFFER    Y02676*/
         OI    BCDIRR,B'10000000'                                  0180
*   CALL IKJEES75;                  /* CALL I/O MODULE TO READ RECORD
*                                      1 OF BROADCAST DATA SET       */
         L     @15,@CV00933                                        0181
         BALR  @14,@15                                             0181
*   RETCDC=RETCD;                   /* STORE REG 15                  */
         ST    RETCD,RETCDC                                        0182
*   IF RETCDC>0 THEN                /* TEST RTRN CODE FROM I/O MODULE*/
         L     @10,RETCDC                                          0183
         LTR   @10,@10                                             0183
         BP    @RT00183                                            0183
*     GO TO BCND;                   /* BRANCH ON BAD READ            */
*/********************************************************************/
*/*      CHECK CONTENTS OF THE FIRST RECORD IN THE DATA SET TO SEE IF*/
*/*      DATA SET IS INITIALIZED- IF IT IS NOT INTIIALIZED, A WARNING*/
*/*      MESSAGE IS ISSUED TO THE TERMINAL USER.                     */
*/********************************************************************/
*                                                                  0185
*   IF R1DSN^=' SYS1.BRODCAST DATA SET '×R1LEVEL^='LEVEL 2'/* DS NOT
*                                      INITIALIZED             Y02676*/
*     THEN                          /* WARN USER               Y02676*/
         CLC   R1DSN(24,R1PTR),@CC00947                            0185
         BNE   @RT00185                                            0185
         CLC   R1LEVEL(7,R1PTR),@CC00949                           0185
         BE    @RF00185                                            0185
@RT00185 DS    0H                                                  0186
*     DO;                           /* SET UP ERROR MESSAGE    Y02676*/
*       IF BCLOGON='1'B             /* ENTRY FROM LOGON        Y02676*/
*         THEN                      /* YES, USE SPECIAL MESSAGE    0187
*                                                              Y02676*/
         TM    BCLOGON,B'00001000'                                 0187
         BNO   @RF00187                                            0187
*         DO;                       /* SET UP FOR ONE LEVEL    Y02676*/
*           BCERCDE=17;             /* SELECT MESSAGE 17       Y02676*/
         MVC   BCERCDE(2),@CH00950                                 0189
*           BC1LVL='1'B;            /* INDICATE ONE LEVEL      Y02676*/
         OI    BC1LVL,B'01000000'                                  0190
*         END;                      /*                         Y02676*/
*       ELSE                        /* ENTRY NOT FROM LOGON          */
*         BCERCDE=14;               /* USE MESSAGE 14, TWO LEVEL     */
         B     @RC00187                                            0192
@RF00187 MVC   BCERCDE(2),@CH00951                                 0192
*       BCERFLG='1'B;               /* INDICATE THAT THIS IS AN ERROR
*                                      CONDITION                     */
@RC00187 OI    BCERFLG,B'10000000'                                 0193
*       BCPUTL='1'B;                /* INDICATE PUTLINE MODULES      */
         OI    BCPUTL,B'00000001'                                  0194
*       RETCDC=BCERCDE;             /* FORCE STACK             Y02676*/
