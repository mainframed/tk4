@09      EQU   09
@10      EQU   10
@11      EQU   11
@12      EQU   12
@13      EQU   13
@14      EQU   14
@15      EQU   15
RETNCODE EQU   @02
OCMTBPTR EQU   @03
REG1     EQU   @01
