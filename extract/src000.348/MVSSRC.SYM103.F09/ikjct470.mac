@DATA    DS    0H
@CH00045 DC    H'8'
@CH00134 DC    H'12'
@DATD    DSECT
         DS    0F
@SA00001 DS    18F
@PC00001 DS    1F
@SA00002 DS    15F
@SA00003 DS    15F
@AL00001 DS    1A
@ZTEMPS  DS    C
@ZTEMPND EQU   *
@ZLEN    EQU   @ZTEMPND-@ZTEMPS
IKJCT470 CSECT
         DS    0F
@DATD    DSECT
         DS    0D
GETSW    DS    BL1
         ORG   *+1-(*-@DATD)/(*-@DATD) INSURE DSECT DATA
@ENDDATD EQU   *
IKJCT470 CSECT
         DS    0F
@SIZDATD DC    AL1(1)
         DC    AL3(@ENDDATD-@DATD)
@CV00412 DC    V(IKJCT467)
         DS    0D
PATCH70  DS    CL100
         ORG   PATCH70
PTCHINIT DC    20CL5'ZAP70'
         ORG   PATCH70+100
@00      EQU   00                      EQUATES FOR REGISTERS 0-15
@01      EQU   01
