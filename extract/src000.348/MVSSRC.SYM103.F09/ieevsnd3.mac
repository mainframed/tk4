         TITLE 'IEEVSND3 - OPERATOR SEND COMMAND, LIST HANDLER         *
                        '
IEEVSND3 CSECT ,                                                   0001
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @11,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@11                                         0001
         L     @00,@SIZDATD                                        0001
         GETMAIN  R,LV=(0)
         LR    @12,@01                                             0001
         USING @DATD,@12                                           0001
         ST    @13,@SA00001+4                                      0001
         LM    @00,@01,20(@13)                                     0001
         ST    @12,8(,@13)                                         0001
         LR    @13,@12                                             0001
         XC    @ZTEMPS(@ZLEN),@ZTEMPS
         MVC   @PC00001(4),0(@01)                                  0001
         MODID BRANCH=YES
REGS     DS    0H                                                  0034
*         COMPTR = R1;              /* SAVE ADDR COMMON DATA AREA    */
         LR    COMPTR,R1                                           0040
*                                   /* MOVE LIST FORMS OF MACROS TO
*                                      AUTOMATIC STORAGE             */
*         GEN  (MVC   AUTOLIST(LISTEND-LISTBEG),LISTBEG );         0041
         MVC   AUTOLIST(LISTEND-LISTBEG),LISTBEG
*         R1PTR = PARMRCD1;         /* SET BASE FOR RECORD1    Y02676*/
         L     @10,PARMRCD1(,COMPTR)                               0042
         ST    @10,@TF00001                                        0042
         MVC   R1PTR(3),@TF00001+1                                 0042
*         IF FLISTALL = '1'B        /* TEST IF HANDLING 'LIST', ALL  */
*           THEN                    /* YES,                          */
         TM    FLISTALL(COMPTR),B'01000000'                        0043
         BO    @RT00043                                            0043
*             GOTO RD1BKL;          /* GO AND  READ BRODCAST DIRECTORY*
*         MSGNO = CBCMSGNO;         /* SET NOTICES MSGNO SPEC'D ON 0045
*                                      SEND CMD FROM COMMON, WHERE IT
*                                      WAS SAVED BY IEEVSEND          *
         LH    @05,CBCMSGNO(,COMPTR)                               0045
         STH   @05,MSGNO                                           0045
*         IF MSGNO > R1BCMAX        /* MSG NUMBER SPEC'D       Y02676
*                                      EXCEEDS MAXIMUM BRODCAST NOTICES
*                                      MSG NO. SPECIFIED IN RECORD 1  *
*           THEN                    /* MSG NO. EXCEEDS MAXIMUM ALLOWED*
         CH    @05,R1BCMAX(,@10)                                   0046
         BH    @RT00046                                            0046
*             GOTO WMSG6;           /* GIVE WARNING MESSAGE           *
*         GEN(EJECT);                                              0048
         EJECT
*RD1BKL:                            /* RECORD1 HAS BEEN READ          *
*         RNBCD = R1BCPTR;          /*SET RNAME FOR DIR RCD ENQY02676*/
RD1BKL   L     @10,R1PTR-1                                         0049
         MVC   RNBCD(3),R1BCPTR(@10)                               0049
*ENQBCD:                            /* ENQUEUE ON BRODCAST DIRECTORY  *
*            GEN  (ENQ (QNAME,RNBCD,S,3,SYSTEM),RET=HAVE,MF=(E,ENQSL) )
ENQBCD   ENQ (QNAME,RNBCD,S,3,SYSTEM),RET=HAVE,MF=(E,ENQSL)
*  /******************************************************************/
*  /*                                                                */
*  /*     READ A BROADCAST NOTICES DIRECTORY RECORD                  */
*  /*                                                                */
*  /******************************************************************/
*READDIR:                           /* READ A DIRECTORY RECORD USING
*                                      RNAME AS RELATIVE BLOCK ADDR  */
*         SDIOPARM = SDIOPARM && SDIOPARM;  /* CLEAR IO PARM AREA    */
READDIR  XC    SDIOPARM(20,COMPTR),SDIOPARM(COMPTR)                0051
*         P1READ = '1'B;            /* SET PARM FLAG FOR READ        */
         OI    P1READ(COMPTR),B'01000000'                          0052
*         PARMRBA = ADDR(RNBCD);    /* SET PARM RBA ADDRESS FOR READ */
         LA    @10,RNBCD                                           0053
         ST    @10,PARMRBA(,COMPTR)                                0053
*         PARMIOAD = ADDR(DIRAREA); /* SET PARM AREA ADDRESS FOR READ *
         LA    @10,DIRAREA                                         0054
         ST    @10,PARMIOAD(,COMPTR)                               0054
*         R1 = ADDR(SDIOPARM);      /* SET SDIOPARM PTR        Y02676*/
         LA    R1,SDIOPARM(,COMPTR)                                0055
*         CALL IEEVSND5;            /* CALL IO RTN */              0056
         L     @15,@CV00345                                        0056
         BALR  @14,@15                                             0056
*         RETC    = R15;            /* SAVE RETURN CODE FROM REG 15  */
         STH   R15,RETC                                            0057
*         IF RETC > 0               /* TEST RETURN CODE FROM IO RTN   *
*           THEN                    /* IF UNSUCCESSFUL OPERATION      *
         LH    @10,RETC                                            0058
         LTR   @10,@10                                             0058
         BP    @RT00058                                            0058
*             GOTO BADIO;           /* GO TO PROCESS ERROR            *
*         BCDPTR = PARMIOAD;        /* SET BASE FOR NOTICE     Y02676
*                                      DIRECTORY RECORD STRUCTURE    */
         MVC   BCDPTR(3),PARMIOAD+1(COMPTR)                        0060
* /*                                                                 */
* /*      THIS CODE SCANS THE BROADCAST DIRECTORY RECORD FOR         */
* /*      APPROPRIATE ENTRIES -- THE ENTRY REPRESENTING THE NOTICE   */
* /*      MSGNO, IF SPECIFIED, OR ANY ACTIVE ENTRY.                  */
* /*                                                                 */
*         DO I = 1 TO DIM(BCDENTRY); /* SCAN DIR RCD ENTRIES   Y02676*/
         LA    I,1                                                 0061
@DL00061 DS    0H                                                  0062
*         IF BCDMSGNO(I) = '0000'X  /* END OF DIRECTORY        Y02676*/
*            THEN                   /* YES, NO MORE DIRECTRY ENTRIES  *
         L     @10,BCDPTR-1                                        0062
         LA    @10,0(,@10)                                         0062
         LR    @05,I                                               0062
         MH    @05,@CH00160                                        0062
         ST    @05,@TF00001                                        0062
         ALR   @05,@10                                             0062
         AL    @05,@CF00382                                        0062
         MVC   @ZT00002+2(2),BCDMSGNO(@05)                         0062
         L     @05,@ZT00002                                        0062
         LTR   @05,@05                                             0062
         BZ    @RT00062                                            0062
*              GOTO LMAX;           /* CHECK IF AN ERROR              *
*         IF BCDNOMSG(I) = '1'B     /* MSG NUMBER UNUSED             */
*            THEN                   /* YES,NO MSG ASSIGNED TO THIS NO.*
         LR    @05,@10                                             0064
         AL    @05,@TF00001                                        0064
         AL    @05,@CF00382                                        0064
         TM    BCDNOMSG(@05),B'10000000'                           0064
         BO    @RT00064                                            0064
*              GOTO LSTMSGBK;       /* BYPASS TEST FOR MAXIMUM MSG NO.
*                                      BECAUSE HI ORDER BIT IS ON     *
*         IF FMN = '0'B             /* MSGNO SPECIFIED         Y02676*/
*           THEN                    /* NO, LIST ALL            Y02676*/
         TM    FMN(COMPTR),B'00010000'                             0066
         BZ    @RT00066                                            0066
*             GOTO LISTMSG;         /* LIST THIS NOTICE        Y02676*/
*         IF BCDMSGNO(I) = MSGNO    /* THIS  IS REQUESTED ENTRYY02676*/
*           THEN                    /* YES, NOTICE FOUND             */
         LH    @05,MSGNO                                           0068
         AL    @10,@TF00001                                        0068
         AL    @10,@CF00382                                        0068
         MVC   @ZT00002+2(2),BCDMSGNO(@10)                         0068
         L     @10,@ZT00002                                        0068
         CR    @10,@05                                             0068
         BE    @RT00068                                            0068
*             GOTO LISTMSG;         /* LIST THIS NOTICE        Y02676*/
*         IF BCDMSGNO(I) > MSGNO    /* SCANNED PAST MSGNO      Y02676*/
*           THEN                    /* YES, NO NOTICE EXISTS   Y02676*/
         CR    @10,@05                                             0070
         BH    @RT00070                                            0070
*             GOTO WMSG6;           /* TAKE ERROR EXIT         Y02676*/
* LSTMSGBK:               /* A MSG HAS BEEN PROCESSED, LISTED        */
*         END;                                                     0072
LSTMSGBK AH    I,@CH00047                                          0072
         CH    I,@CH00159                                          0072
         BNH   @DL00061                                            0072
*LRECEND: /* END OF DIRECTORY RECORD                                 */
*         IF BCDNEXT = 0            /* ALL DIR RCDS HANDLED    Y02676*/
*            THEN GOTO LMAX;        /*   YES, EXIT THIS SCAN ROUTINE */
LRECEND  L     @10,BCDPTR-1                                        0073
         MVC   @ZT00001+1(3),BCDNEXT(@10)                          0073
         L     @10,@ZT00001                                        0073
         LTR   @10,@10                                             0073
         BZ    @RT00073                                            0073
*            ELSE;                                                 0075
*                                   /* DEQ ON PREVIOUS RECORD        */
*         GEN  (DEQ   (QNAME,RNBCD,3,SYSTEM),RET=HAVE,MF=(E,DEQL));
         DEQ   (QNAME,RNBCD,3,SYSTEM),RET=HAVE,MF=(E,DEQL)
*         RNBCD = BCDNEXT;          /*SET RNAME FOR DIR RCD ENQY02676*/
         L     @10,BCDPTR-1                                        0077
         MVC   RNBCD(3),BCDNEXT(@10)                               0077
*         GOTO  ENQBCD;             /* GOTO SCAN NEXT BC DIRECTRY REC*/
*                                                                  0078
         B     ENQBCD                                              0078
*LMAX:    IF FYESBCM = '1'B         /* ANY NOTICES             Y02676*/
*           THEN                    /* YES                           */
LMAX     TM    FYESBCM(COMPTR),B'01000000'                         0079
         BO    @RT00079                                            0079
*             GOTO EXITCODE;        /* GO  CLEAN UP AND END PROCESSNG*/
*           ELSE                    /* NO MESSAGES PUT OUT           */
*             GOTO WMSG6;           /* ISSUE WARNING MESSAGE -     0081
*                                      'NO BROADCAST MESSAGES'       */
         B     WMSG6                                               0081
* /*******************************************************************/
* /*CALL IO RTN TO READ BRODCAST NOTICE MESSAGE RECORD FOR THIS MSGNO*/
* /*******************************************************************/
*LISTMSG:                                                          0082
*         FYESBCM = '1'B;           /* SET FLAG TO SAY THERE WAS A 0082
*                                      BRODCAST MSG IN DATA SET      */
LISTMSG  OI    FYESBCM(COMPTR),B'01000000'                         0082
*         SDIOPARM = SDIOPARM && SDIOPARM;  /* CLEAR IO PARM AREA    */
         XC    SDIOPARM(20,COMPTR),SDIOPARM(COMPTR)                0083
*         P1READ = '1'B;            /* SET PARM FLAG FOR READ        */
         OI    P1READ(COMPTR),B'01000000'                          0084
*         PARMIOAD = ADDR(MSGAREA); /* SET PARM AREA ADDRESS FOR READ*/
         LA    @10,MSGAREA                                         0085
         ST    @10,PARMIOAD(,COMPTR)                               0085
*         PARMRBA = ADDR(BCDMRBA(I)); /* SET PARM RBA ADDRESS  Y02676*/
         LR    @10,I                                               0086
         MH    @10,@CH00160                                        0086
         L     @05,BCDPTR-1                                        0086
         LA    @05,0(,@05)                                         0086
         AL    @05,@CF00383                                        0086
         LA    @10,BCDMRBA-2(@10,@05)                              0086
         ST    @10,PARMRBA(,COMPTR)                                0086
*         R1 = ADDR(SDIOPARM);      /* SET SDIOPARM PTR        Y02676*/
         LA    R1,SDIOPARM(,COMPTR)                                0087
*         CALL IEEVSND5;            /* CALL IO RTN                   */
         L     @15,@CV00345                                        0088
         BALR  @14,@15                                             0088
*         RETC    = R15;            /* SAVE RETURN CODE FROM REG 15  */
         STH   R15,RETC                                            0089
*         IF RETC > 0               /* TEST RETURN CODE FROM IO RTN  */
*           THEN                    /* IF UNSUCCESSFUL OPERATION     */
         LH    @10,RETC                                            0090
         LTR   @10,@10                                             0090
         BP    @RT00090                                            0090
*             GOTO BADIO;           /* GO TO PROCESS ERROR           */
* /*******************************************************************/
* /*      THIS CODE RECOVERS THE NOTICE TEXT FOR OTHER HANDLERS      */
* /*      OR FORMATS THE TEXT FOR SENDER DISPLAY.                    */
* /*******************************************************************/
*         BCMPTR = PARMIOAD;        /* SET BASE FOR NOTICE RCD Y02676*/
         L     @10,PARMIOAD(,COMPTR)                               0092
         ST    @10,@TF00001                                        0092
         MVC   BCMPTR(3),@TF00001+1                                0092
*         GMTEXT(1) = '40'X;        /* INSERT BLANK BEFORE MSG TEXT  */
         MVI   GMTEXT,X'40'                                        0093
*         GMTEXT(2:BCMLNG+1) = BCMTEXT; /* SET TEXT OF MESSAGE       */
         SLR   @05,@05                                             0094
         IC    @05,BCMLNG(,@10)                                    0094
         LR    @02,@05                                             0094
         BCTR  @02,0                                               0094
         EX    @02,@SM00384                                        0094
*         IF FLIST = '0'B           /* NO LISTING REQUESTED    Y02676*/
*           THEN                    /* NO, RECOVER ONLY        Y02676*/
         TM    FLIST(COMPTR),B'00100000'                           0095
         BNZ   @RF00095                                            0095
*             DO;                                                  0096
*               COMBUF(1:BCMLNG) = GMTEXT(2:BCMLNG+1);             0097
         EX    @02,@SM00386                                        0097
*                                   /* SAVE RECOVERED NOTICE   Y02676*/
*               TBEG = 1;           /* OFFSET TO TEXT IN COMBUFY02676*/
         MVI   TBEG(COMPTR),X'01'                                  0098
*               TEXTLENG = BCMLNG;  /* SET MESSAGE LENGTH      Y02676*/
         STC   @05,TEXTLENG(,COMPTR)                               0099
*               FTEXT = '1'B;       /* INDICATE TEXT READY     Y02676*/
         OI    FTEXT(COMPTR),B'00000010'                           0100
*               GOTO EXITCODE;      /* HANDLING DONE           Y02676*/
         B     EXITCODE                                            0101
*             END;                                                 0102
*         WORK4 = 0;                /* CLEAR WORK AREA               */
@RF00095 SLR   @10,@10                                             0103
         ST    @10,WORK4                                           0103
*         WORK8 = WORK8 && WORK8;   /* CLEAR WORK AREA               */
         XC    WORK8(8),WORK8                                      0104
*         WORK4(3:4) = BCDMSGNO(I); /* COPY MSG NUMBER         Y02676
*                                      FROM NOTICES DIRECTORY        */
         LR    @10,I                                               0105
         MH    @10,@CH00160                                        0105
         L     @05,BCDPTR-1                                        0105
         ALR   @05,@10                                             0105
         AL    @05,@CF00382                                        0105
         MVC   WORK4+2(2),BCDMSGNO(@05)                            0105
*         RESTRICT (R5);            /* RESTRICT REG 5 FOR 'GEN' USE  */
*         R5 = WORK4;               /* SET REGISTER FOR CVD          */
         L     R5,WORK4                                            0107
*         GEN;                                                     0108
          CVD   R5,WORK8            CONVERT BC MSG NO. TO DECIMAL
          UNPK  WORK4(4),WORK8+5(3) UNPACK INTO EBCDIC FORM
         MVZ   WORK4+3(1),WORK4+2   MAKE SIGN PRINTABLE
*         RELEASE (R5);             /* RELEASE REG 5 FOR COMPILER USE*/
*         MSGNOTXT = 'MSGNO ';      /* SET'MSGNO' IDENTIFIER         */
         MVI   MSGNOTXT+6,C' '                                     0110
         MVC   MSGNOTXT+7(2),MSGNOTXT+6                            0110
         MVC   MSGNOTXT(6),@CC00360                                0110
*         MSGNODEC = WORK4(2:4);    /* SET MSG NO. FROM WORK AREA    */
         MVC   MSGNODEC(3),WORK4+1                                 0111
*         GMNUM = MSGNOTXT;         /* SET BC MSG NO. FOR OUTPUT LIST*/
         MVC   GMNUM(9),MSGNOTXT                                   0112
*         GENLNG = BCMLNG+10;       /* SET LENGTH OF MSG TEXT PLUS 0113
*                                      MSG NO.                       */
         L     @10,BCMPTR-1                                        0113
         SLR   @05,@05                                             0113
         IC    @05,BCMLNG(,@10)                                    0113
         LA    @05,10(,@05)                                        0113
         STH   @05,GENLNG                                          0113
*         FLISTING = '1'B;          /* SET ROUTING FLAG              */
         OI    FLISTING(COMPTR),B'00000001'                        0114
*         GOTO WARNMSG;             /* GO TO LIST THIS BC MESSAGE    */
         B     WARNMSG                                             0115
*LIST3:                             /* MSG HAS BEEN LISTED OUT       */
*         FLISTING = '0'B;          /* TURN OFF ROUTING FLAG         */
LIST3    NI    FLISTING(COMPTR),B'11111110'                        0116
*         IF FMN = '1'B             /* IF MESSAGE NO. FORMAT         */
*           THEN                    /* YES, MSG NO. FORMAT OF SEND   */
         TM    FMN(COMPTR),B'00010000'                             0117
         BO    @RT00117                                            0117
*             GOTO EXITCODE;        /* LEAVE ROUTINE           Y02676*/
*         GOTO  LSTMSGBK;           /* GO BACK TO SCAN RTNE TO SCAN  */
         B     LSTMSGBK                                            0119
*WMSG6:   FMSG6 = '1'B;             /* SET INDICATOR FOR WARNING MSG-
*                                      'NO BROADCAST MSG'            */
WMSG6    OI    FMSG6(COMPTR),B'00001000'                           0120
*         GOTO EXITCODE;            /* ISSUE WARNING MSG       Y02676*/
*                                                                  0121
         B     EXITCODE                                            0121
*BADIO:   /* I/O ERROR IN IEEVSDIO PROCESSING                        */
*         FBADIO = '1'B;            /* SET FLAG FOR WARNING MSG      */
BADIO    OI    FBADIO(COMPTR),B'00000100'                          0122
*         SDIORC = RETC;            /* SAVE SDIO RETURN CODE IN COMMON
*                                      DATA AREA FOR USE BY IEEVSND4 */
         MVC   SDIORC(1,COMPTR),RETC+1                             0123
*         GOTO EXITCODE;            /* ISSUE WARNING MSG       Y02676*/
*                                                                  0124
         B     EXITCODE                                            0124
*BADTPUT:                 /* UNSUCCESSFUL TPUT RETURN CODE           */
*         IF RETC = 8 ×             /* SENDER HIT ATTN         Y02676*/
*            RETC = 12              /* NO INTERCOM MODE        Y02676*/
*           THEN                    /* NOT HARD ERROR          Y02676*/
BADTPUT  LH    @10,RETC                                            0125
         CH    @10,@CH00042                                        0125
         BE    @RT00125                                            0125
         CH    @10,@CH00033                                        0125
         BE    @RT00125                                            0125
*             GOTO EXITCODE;        /* TERMINATE PROCESSING    Y02676*/
*         FBADTPUT = '1'B;          /* SET ERROR INDICATOR IN COMMON */
         OI    FBADTPUT(COMPTR),B'00000001'                        0127
*         GOTO EXITCODE;            /* ISSUE WARNING MSG       Y02676*/
         B     EXITCODE                                            0128
* /*******************************************************************/
* /*      ISSUE MESSAGE TO SENDER -- W T O  / T P U T                */
* /*******************************************************************/
* WARNMSG:/* GIVE WARNING MSG TO ISSUER OF 'SEND'                    */
*         IF FSOURCE = '00'B        /* HAS SOURCE OF SEND COMMAND BEEN
*                                      DETERMINED?                   */
*           THEN                    /* NO, CHECK THE CSCB            */
WARNMSG  TM    FSOURCE(COMPTR),B'11000000'                         0129
         BNZ   @RF00129                                            0129
*             IF CHASID = 0         /* SEND COMMAND CAME       Y02676
*                                      FROM OPERATORS CONSOLE OR   0130
*                                      A TERMINAL IN OPERATOR MODE   */
*               THEN                /* SEND COMMAND CAME FROM OPERATOR
*                                      CONSOLE                       */
         L     @10,CSCBPTR-1(,COMPTR)                              0130
         LH    @10,CHASID(,@10)                                    0130
         LTR   @10,@10                                             0130
         BNZ   @RF00130                                            0130
*                 FCONSOL = '1'B;   /* SET INDICATOR FOR CONSOLE     */
         OI    FCONSOL(COMPTR),B'01000000'                         0131
*               ELSE                /* SEND COMMAND CAME FROM TERMINAL
*                                      IN OPERATOR MODE              */
*                 FTERM = '1'B;     /* SET INDICATOR FOR TERMINAL    */
         B     @RC00130                                            0132
@RF00130 OI    FTERM(COMPTR),B'10000000'                           0132
* SRCBK:  /* SOURCE OF SEND CMD HAS BEEN FOUND                       */
*         IF FCONSOL = '1'B         /* CHECK IF SENDER WAS OP.CONSOLE*/
*           THEN                    /* CMD CAME FROM OPERATOR CONSOLE*/
@RC00130 DS    0H                                                  0133
@RF00129 DS    0H                                                  0133
SRCBK    TM    FCONSOL(COMPTR),B'01000000'                         0133
         BNO   @RF00133                                            0133
*             DO;                   /* SET UP FOR WTO                */
*             MSGAREA = GENTEXT;    /* SAVE MSG TEXT AND MSG ID      */
         MVI   MSGAREA+124,C' '                                    0135
         MVC   MSGAREA+125(4),MSGAREA+124                          0135
         MVC   MSGAREA(124),GENTEXT                                0135
*             IF GENLNG > WTOMAX    /* IF WARN MSG LENGTH > 72       */
*               THEN                /* YES, MSG LENGTH > 72          */
         SLR   @10,@10                                             0136
         IC    @10,WTOMAX                                          0136
         CH    @10,GENLNG                                          0136
         BNL   @RF00136                                            0136
*                 WTOLNG = WTOMAX+4;/* SET WTO PARMLIST LENG TO MAX  */
         LA    @10,4(,@10)                                         0137
         STH   @10,WTOLNG                                          0137
*               ELSE                                               0138
*                 WTOLNG = GENLNG+4;/* SET LENGTH OF WTO PARM LIST   */
         B     @RC00136                                            0138
@RF00136 LH    @10,GENLNG                                          0138
         LA    @10,4(,@10)                                         0138
         STH   @10,WTOLNG                                          0138
*             GENTEXT(1:WTOLNG-4)=MSGAREA(1:WTOLNG-4); /* PUT TEXT   */
@RC00136 LH    @10,WTOLNG                                          0139
         LR    @05,@10                                             0139
         SH    @05,@CH00160                                        0139
         EX    @05,@SM00394                                        0139
*                                          /* SET DESCRIPTOR CODE    */
*             WTOLIST(WTOLNG+1:WTOLNG+5) = WTODESC(1:4);           0140
         LA    @05,WTOLIST(@10)                                    0140
         MVI   4(@05),C' '                                         0140
         MVC   0(4,@05),WTODESC                                    0140
*             WTOMCSFL = '11'B;    /* TURN ON HIGH ORDER BIT OF MCS
*                                      FLAG FIELD TO SAY REG 0     0141
*                                      CONTAINS CONSOLE ID           */
         OI    WTOMCSFL,B'11000000'                                0141
*             WTOMCSF2 = '1'B;     /* SET MCS CMD RESP FLAG  @ZA13845*/
         OI    WTOMCSF2,B'00100000'                                0142
*             R1 = ADDR(WTOLIST);  /* SET REG1 = ADDR WTO PARMLIST   */
         LA    R1,WTOLIST                                          0143
*             R0 = CHUCMP;         /* PUT CONSOLE ID INTO REG 0 FOR
*                                     WTO TO CONSOLE ISSUING 'SEND'  */
         L     @10,CSCBPTR-1(,COMPTR)                              0144
         SLR   R0,R0                                               0144
         IC    R0,CHUCMP(,@10)                                     0144
*             GEN  (WTO   ,MF=(E,(1))    ISSUE WTO TO SENDER    ); 0145
         WTO   ,MF=(E,(1))    ISSUE WTO TO SENDER
*             GMBEG = WTOMAX+1;    /* SET BEGINNING FOR NEXT WTO     */
         LA    @10,1                                               0146
         SLR   @05,@05                                             0146
         IC    @05,WTOMAX                                          0146
         ALR   @05,@10                                             0146
         STC   @05,GMBEG                                           0146
*             WTOCNT=(GENLNG-9)/WTOMAX2;/* CNT OF WTO'S NEEDED       */
         SLR   @05,@05                                             0147
         IC    @05,WTOMAX2                                         0147
         LH    @02,GENLNG                                          0147
         SH    @02,@CH00183                                        0147
         LR    @00,@02                                             0147
         SRDA  @00,32                                              0147
         DR    @00,@05                                             0147
         STC   @01,WTOCNT                                          0147
*             WTOREM=(GENLNG-9)//WTOMAX2;/* WTOREM = REMAINDER       */
         LR    @06,@02                                             0148
         SRDA  @06,32                                              0148
         DR    @06,@05                                             0148
         STC   @06,WTOREM                                          0148
*             IF WTOREM ^= 0       /* TEST IF REMAINDER IS NONZERO   */
*               THEN               /* YES, THERE IS A REMAINDER      */
         CLI   WTOREM,0                                            0149
         BE    @RF00149                                            0149
*                  WTOCNT=WTOCNT+1;/* ADD 1 TO CNT OF WTO'S NEEDED   */
         ALR   @01,@10                                             0150
         STC   @01,WTOCNT                                          0150
*WTOLOOP2:                         /* ISSUE ANOTHER WTO              */
*             WTOCNT = WTOCNT-1;   /* DECREMENT CNT OF WTO'S NEEDED  */
@RF00149 DS    0H                                                  0151
WTOLOOP2 SLR   @10,@10                                             0151
         IC    @10,WTOCNT                                          0151
         BCTR  @10,0                                               0151
         STC   @10,WTOCNT                                          0151
*             IF WTOCNT = 0        /* HAVE ALL WTO'S BEEN ISSUED     */
*               THEN               /* YES, ENTIRE MSG IS PUT OUT     */
         CLI   WTOCNT,0                                            0152
         BE    @RT00152                                            0152
*                 GOTO ROUTER;     /* GOTO ROUTE  CONTROL            */
*               ELSE;              /* ISSUE ANOTHER WTO              */
*             GMNUM = ' ';         /* BLANK OUT FIRST 10 CHAR        */
         MVI   GMNUM+1,C' '                                        0155
         MVC   GMNUM+2(7),GMNUM+1                                  0155
         MVI   GMNUM,C' '                                          0155
*             IF GENLNG > GMBEG+WTOMAX2/* IS MSG LNGTH MORE THAN CAN
*                                     BE PUT OUT IN 1 MORE WTO       */
*               THEN               /* YES, 2 MORE WTO'S NEEDED       */
         SLR   @10,@10                                             0156
         IC    @10,WTOMAX2                                         0156
         SLR   @05,@05                                             0156
         IC    @05,GMBEG                                           0156
         ALR   @05,@10                                             0156
         CH    @05,GENLNG                                          0156
         BNL   @RF00156                                            0156
*                 GMEND = WTOMAX2; /* SET END OF WTO MSG TO MAX      */
         STC   @10,GMEND                                           0157
*               ELSE               /* NO, 1 MORE WTO WILL DO IT      */
*                 GMEND=GENLNG-GMBEG+1;/*SET END OF MSG TO REMAINDER */
         B     @RC00156                                            0158
@RF00156 LH    @10,GENLNG                                          0158
         MVC   @ZT00003+3(1),GMBEG                                 0158
         SL    @10,@ZT00003                                        0158
         LA    @10,1(,@10)                                         0158
         STC   @10,GMEND                                           0158
*                                  /* PUT MSG INTO WTO PARMLIST      */
*             GMTEXT(1:GMEND) = MSGAREA(GMBEG:GMBEG + GMEND - 1 ); 0159
@RC00156 SLR   @10,@10                                             0159
         IC    @10,GMEND                                           0159
         LR    @05,@10                                             0159
         BCTR  @05,0                                               0159
         SLR   @02,@02                                             0159
         IC    @02,GMBEG                                           0159
         LA    @01,MSGAREA-1(@02)                                  0159
         EX    @05,@SM00398                                        0159
*             WTOLNG = GMEND+4+9;  /* SET WTO PARMLIST LENGTH        */
         AH    @10,@CH00380                                        0160
         STH   @10,WTOLNG                                          0160
*             WTOLIST(WTOLNG+1:WTOLNG+5) = WTODESC(1:4);           0161
         LA    @05,WTOLIST(@10)                                    0161
         MVI   4(@05),C' '                                         0161
         MVC   0(4,@05),WTODESC                                    0161
*                 WTOMCSFL = '11'B; /* TURN ON 2 HIGH ORDER BITS OF MCS
*                                      FLAG FIELD TO SAY REG 0     0162
*                                      CONTAINS CONSOLE ID           */
         OI    WTOMCSFL,B'11000000'                                0162
*                 WTOMCSF2 = '1'B;    /* SET MCS CMD RESP FLG@ZA13845*/
         OI    WTOMCSF2,B'00100000'                                0163
*                 R1 = ADDR(WTOLIST); /* SET REG1 = ADDR WTO PARMLIST*/
         LA    R1,WTOLIST                                          0164
*                 R0 = CHUCMP;      /* PUT CONSOLE ID INTO REG 0 FOR
*                                      WTO TO CONSOLE ISSUING 'SEND' */
         L     @10,CSCBPTR-1(,COMPTR)                              0165
         SLR   R0,R0                                               0165
         IC    R0,CHUCMP(,@10)                                     0165
*                 GEN;                                             0166
                  WTO   ,MF=(E,(1))          ISSUE WTO TO SENDER
*        GMBEG = GMBEG + GMEND    ;  /* BUMP GMBEG FOR NEXT WTO      */
         SLR   @10,@10                                             0167
         IC    @10,GMBEG                                           0167
         MVC   @ZT00003+3(1),GMEND                                 0167
         AL    @10,@ZT00003                                        0167
         STC   @10,GMBEG                                           0167
*        GOTO WTOLOOP2;            /* CHECK IF ANOTHER WTO NEEDED    */
         B     WTOLOOP2                                            0168
*             END;                                                 0169
*           ELSE                    /* SOURCE OF SEND IS A TERMINAL  */
*             DO;                   /* SET UP FOR TPUT               */
@RF00133 DS    0H                                                  0171
*                RECTJID = CHASID;  /* SET ASID INTO HI        Y02676
*                                      ORDER HALF OF REG 0 FOR TPUT  */
         L     @10,CSCBPTR-1(,COMPTR)                              0171
         MVC   RECTJID(2),CHASID(@10)                              0171
*               R1 = ADDR(GENTEXT); /* REG1 = MSGTEXT ADDR FOR TPUT  */
         LA    R1,GENTEXT                                          0172
*               R0 = TPUTREG0;      /* REG0 = TJID - HI ORDER AND  0173
*                                             BUFFER SIZE - LO ORDER */
         L     R0,TPUTREG0                                         0173
*               GEN;                                               0174
                TPUT  (1),(0),R     ISSUE TPUT TO OPERATOR TERMINAL
*             RETC = R15;           /* SAVE RETURN CODE FROM TPUT    */
         STH   R15,RETC                                            0175
*             IF RETC ^= 0          /* CHECK RETURN CODE FROM TPUT   */
*               THEN                /* IF UNSUCCESSFUL TPUT          */
         LH    @10,RETC                                            0176
         LTR   @10,@10                                             0176
         BNZ   @RT00176                                            0176
*                 GOTO BADTPUT;     /* ANALYZE ERROR CONDITION       */
*             END;                                                 0178
*ROUTER:                            /* ROUTE CONTROL TO PROPER RTN   */
*         GOTO LIST3;               /* GO TO CONTINUE LISTING MSGS   */
         B     LIST3                                               0179
