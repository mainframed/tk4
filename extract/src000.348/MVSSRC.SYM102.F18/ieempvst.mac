*                                                                  0086
*   /*****************************************************************/
*   /*                                                               */
*   /* MAINLINE CODE -VARY STORAGE. INITIALIZE WORKAREA              */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0086
*   RETPTR=R14;                     /* SAVE RETURN ADDRESS           */
         LR    RETPTR,R14                                          0086
*   CSCBPTR=R1;                     /* SAVE @ CSCB                   */
         ST    R1,CSCBPTR                                          0087
*   FUNCBITS=''B;                   /* CLEAR FUNCTION FLAGS          */
         XC    FUNCBITS(2),FUNCBITS                                0088
*   RSRWAP=0;                       /* NULL RSR WA                   */
         SLR   @14,@14                                             0089
         ST    @14,RSRWAP                                          0089
*   VALFRA=0;                       /* ZERO RETRY ADDR               */
         ST    @14,VALFRA                                          0090
*   INTERROR=-1;                    /* SET INTERNAL ERROR CODE       */
         MVC   INTERROR(4),@CF02438                                0091
*   AVALRTN=ADDR(IEEVALST);         /* SET @ TO VALIDATE'S PAGE      */
*                                                                  0092
         L     @14,@CV00128                                        0092
         ST    @14,AVALRTN                                         0092
*   /*****************************************************************/
*   /*                                                               */
*   /* SET ADDRESS CONSTANTS FOR DEBUGGING                           */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0093
*   ENTRYPT=ADDR(IEEMPVST);         /* ENTRY POINT LOAD MODULE       */
         L     @14,@CA00028                                        0093
         ST    @14,ENTRYPT                                         0093
*   STRTMWA=R10;                    /* START MAIN WORKAREA           */
         ST    R10,STRTMWA                                         0094
*   ENDMWA=R10+@SIZDATD;            /* END ADDR +1 OF WORKAREA       */
*                                                                  0095
         LR    @14,R10                                             0095
         AL    @14,@SIZDATD                                        0095
         ST    @14,ENDMWA                                          0095
*   /*****************************************************************/
*   /*                                                               */
*   /* SET CONSOLE ID FOR WTO                                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0096
*   IF(CHUCMP^=0)&(CHUCMP^='80'X) THEN                             0096
         L     @14,CSCBPTR                                         0096
         CLI   CHUCMP(@14),0                                       0096
         BE    @RF00096                                            0096
         CLI   CHUCMP(@14),X'80'                                   0096
         BE    @RF00096                                            0096
*     CONSOLID=CHUCMP;              /* WTO SENT TO ISSUING CONSOLE   */
*                                                                  0097
         MVC   CONSOLID(1),CHUCMP(@14)                             0097
*   /*****************************************************************/
*   /*                                                               */
*   /* WTO SENT TO MASTER FOR RDR OR INTERNAL CMDS                   */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0098
*   ELSE                                                           0098
*     CONSOLID=UCMID;                                              0098
*                                                                  0098
         B     @RC00096                                            0098
@RF00096 L     @14,CVTPTR                                          0098
         L     @14,UCMBASPT(,@14)                                  0098
         AL    @14,@CF02903                                        0098
         L     @14,UCMMCSPT(,@14)                                  0098
         L     @14,UCMDVEPT(,@14)                                  0098
         MVC   CONSOLID(1),UCMID(@14)                              0098
*   /*****************************************************************/
*   /*                                                               */
*   /* INIT ESTAE PARM LIST AND SET ESTAE ENVIRONMENT                */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0099
*   RFY                                                            0099
*    (R2) RSTD;                                                    0099
@RC00096 DS    0H                                                  0100
*   DATARG=R10;                     /* DATA REG OF MAIN WORKAREA     */
         ST    R10,DATARG                                          0100
*   CODERG1=R11;                    /* CODE REG FOR FIRST 4096       */
         ST    R11,CODERG1                                         0101
*   CODERG2=R12;                    /* CODE REG FOR NEXT 4096        */
         ST    R12,CODERG2                                         0102
*   R2=ADDR(EPARML);                /* ADDR OF USER PARM LIST        */
         LA    R2,EPARML                                           0103
*   ESTLIST=''B;                    /* ZERO ESTAE INTERFACE          */
         XC    ESTLIST(16),ESTLIST                                 0104
*   R1=ADDR(ESTLIST);               /* SET PTR TO CNTRL BLCK         */
         LA    R1,ESTLIST                                          0105
*   GEN(ESTAE ESTAERTN,PARAM=(2),MF=(E,(1)));                      0106
         ESTAE ESTAERTN,PARAM=(2),MF=(E,(1))
*   RFY                                                            0107
*    (R2) UNRSTD;                                                  0107
*                                                                  0107
*   /*****************************************************************/
*   /*                                                               */
*   /* IF ESTAE ESTABLISHED SUCCESSFULLY THEN PROCESS COMMAND ELSE   */
*   /* WRITE ERROR MSG AND EXIT                                      */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0108
*   IF R15=0 THEN                                                  0108
*                                                                  0108
         LTR   R15,R15                                             0108
         BNZ   @RF00108                                            0108
*     /***************************************************************/
*     /*                                                             */
*     /* ESTAE SUCCESSFUL. PARSE AND PROCESS COMMAND                 */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0109
*     DO;                                                          0109
*       CALL SYNCHK;                /* PARSE COMMAND                 */
*                                                                  0110
         BAL   @14,SYNCHK                                          0110
*       /*************************************************************/
*       /*                                                           */
*       /* CHECK IF GOOD PARSE                                       */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0111
*       IF RC=0 THEN                                               0111
*                                                                  0111
         ICM   @14,15,RC                                           0111
         BNZ   @RF00111                                            0111
*         /***********************************************************/
*         /*                                                         */
*         /* GOOD PARSE. PROCESS REQUEST AND CLEANUP                 */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0112
*         DO;                                                      0112
*           IF OFFLINE='1'B THEN    /* CHECK IF OFFLINE REQUEST      */
*                                                                  0113
         TM    OFFLINE,B'10000000'                                 0113
         BNO   @RF00113                                            0113
*             /*******************************************************/
*             /*                                                     */
*             /* PROCESS OFFLINE REQUEST                             */
*             /*                                                     */
*             /*******************************************************/
*                                                                  0114
*             CALL VSTOROFF;                                       0114
*                                                                  0114
         BAL   @14,VSTOROFF                                        0114
*           /*********************************************************/
*           /*                                                       */
*           /* PROCESS ONLINE REQUEST                                */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0115
*           ELSE                                                   0115
*             CALL VSTORON;                                        0115
*                                                                  0115
         B     @RC00113                                            0115
@RF00113 BAL   @14,VSTORON                                         0115
*           /*********************************************************/
*           /*                                                       */
*           /* FREE RSR WORKAREA IF ALLOCATED                        */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0116
*           IF RSRWAP^=0 THEN                                      0116
@RC00113 ICM   @14,15,RSRWAP                                       0116
         BZ    @RF00116                                            0116
*             DO;                                                  0117
*               DO;                 /* FREEMAIN(R)                 0118
*                                      LV(RSRWAL)SP(253)A(RSRPARML)  */
*                 RESPECIFY                                        0119
*                  (GPR01F,                                        0119
*                   GPR00F) RESTRICTED;                            0119
*                 GPR00F=RSRWAL;    /* LENGTH REQUESTED              */
         L     GPR00F,RSRWAL                                       0120
*                 GPR01F=253;       /* SP IN REG 1                   */
         LA    GPR01F,253                                          0121
*                 GEN(SLL   GPR01F,24)                             0122
*                 NODEFS NOREFS NOSETS NOEXIT NOFLOWS SEQFLOW;     0122
         SLL   GPR01F,24
*                 GPR00F=GPR00F×GPR01F;/* SP IN HI BYTE              */
         OR    GPR00F,GPR01F                                       0123
*                 GPR01F=ADDR(RSRPARML);/* SET REGISTER ONE          */
         L     GPR01F,RSRWAP                                       0124
*                 SVC(10);          /* R-FORM OF GETMAIN             */
         SVC   10                                                  0125
*                 RESPECIFY                                        0126
*                  (GPR01F,                                        0126
*                   GPR00F) UNRESTRICTED;                          0126
*               END;                /* FREEMAIN(R)                 0127
*                                      LV(RSRWAL)SP(253)A(RSRPARML)  */
*             END;                                                 0128
*         END;                      /* END OF PROCESS GOOD COMMAND   */
*     END;                                                         0130
*   ELSE                                                           0131
*                                                                  0131
*     /***************************************************************/
*     /*                                                             */
*     /* ESTAE FAILED. WRITE MESSAGE AND EXIT                        */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0131
*     DO;                                                          0131
         B     @RC00108                                            0131
@RF00108 DS    0H                                                  0132
*       CALL WTORTN(7);             /* WRITE ERROR MSG               */
         LA    @01,@AL00132                                        0132
         BAL   @14,WTORTN                                          0132
*     END;                                                         0133
*                                                                  0133
*   /*****************************************************************/
*   /*                                                               */
*   /* FREE CSCB AND EXIT                                            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0134
*   CHFC='1'B;                      /* SET FREE CSCB BIT             */
@RC00108 L     @14,CSCBPTR                                         0134
         OI    CHFC(@14),B'00000010'                               0134
*   R1=CSCBPTR;                     /* SET PTR TO CSCB               */
         LR    R1,@14                                              0135
*   GEN(MGCR (1),CHAIN);            /* FREE CSCB                     */
*                                                                  0136
         MGCR (1),CHAIN
*   /*****************************************************************/
*   /*                                                               */
*   /* RETURN TO SYSTEM                                              */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0137
*NRMEXIT:                                                          0137
*   R14=RETPTR;                     /* RESTORE RETURN REG            */
NRMEXIT  LR    R14,RETPTR                                          0137
*   RETURN;                         /* RETURN TO SYSTEM              */
@EL00001 L     @13,4(,@13)                                         0138
@EF00001 L     @00,@SIZDATD                                        0138
         LR    @01,@10                                             0138
         FREEMAIN R,LV=(0),A=(1)
@ER00001 BR    @14                                                 0138
*                                                                  0139
*/* SYNTAX CHECKING ROUTINE                                          */
*/*                                                                  */
*/* FUNCTION - CHECK COMMAND AUTHORIZATION AND PARSE COMMAND TEXT    */
*/*                                                                  */
*/* INPUT - COMMAND STRING IN CSCB                                   */
*/*                                                                  */
*/* OUTPUT -                                                         */
*/*   GOOD COMMAND:                                                  */
*/*    - RC=0                                                        */
*/*    - SET VALUE OF OFFLINE FLAG,START RANGE,END RANGE IN WORKAREA */
*/*   BAD COMMAND:                                                   */
*/*    - RC=4                                                        */
*/*    - ERROR MESSAGE WRITTEN TO OPERATOR                           */
*/*                                                                  */
*/* COMMAND SYNTAX                                                   */
*/*                                                                  */
*/*    V   STOR(XXXXXK,XXXXXK),OFFLINE                               */
*/*   VARY      YYYYYY YYYYYY  ONLINE                                */
*/*                                                                  */
*/*   XXXXXK - 1 TO 5 CHAR DECIMAL K ADDRESS                         */
*/*   YYYYYY - 1 TO 6 CHAR HEXADECIMAL ADDRESS                       */
*/*                                                                  */
*                                                                  0139
*SYNCHK:                                                           0139
*   PROC;                                                          0139
SYNCHK   STM   @14,@12,@SA00002                                    0139
*   DCL                                                            0140
*     PARSEP PTR,                   /* PARSE POINTER                 */
*     LPARSEP PTR;                  /* LOCAL PARSE PTR               */
*   DCL                                                            0141
*     CHARS CHAR(12) BASED(PARSEP); /* STRING MAP                    */
*   DCL                                                            0142
*     DIGIT CHAR(1) BASED(LPARSEP); /* CHARACTER MAP                 */
*   DCL                                                            0143
*     NUMBR FIXED(8) BASED(LPARSEP);/* ARITH VALUE OF CHAR           */
*   DCL                                                            0144
*     COUNT FIXED;                  /* NUM OF CHARS IN ADDR          */
*   DCL                                                            0145
*     BADDRP PTR;                   /* PTR TO ANSWER BUFFER FOR    0145
*                                      CONVERTED ADDRESS USED BY   0145
*                                      PARSE ADDR RTNS               */
*   DCL                                                            0146
*     BINADDR FIXED BASED(BADDRP);  /* MAP OF ANSWER BUFFER USED BY
*                                      PARSE ADDR RTNS               */
*                                                                  0146
*   /*****************************************************************/
*   /*                                                               */
*   /* ACCEPT COMMAND ONLY IF FROM MASTER CONSOLE, READER WITH       */
*   /* AUTHORITY OF E000, OR AN INTERNAL COMMAND. IF NONE OF THE     */
*   /* ABOVE THEN REJECT COMMAND.                                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0147
*   IF(CHUCMP=UCMID)×               /* MASTER CONSOLE?               */
*       ((CHUCMP='80'X)&(AUTHCODE='E000'X))×/* VALID READER?         */
*       (CHUCMP=0) THEN             /* INTERNAL COMMAND?             */
         L     @14,CSCBPTR                                         0147
         L     @09,CVTPTR                                          0147
         L     @09,UCMBASPT(,@09)                                  0147
         AL    @09,@CF02903                                        0147
         L     @09,UCMMCSPT(,@09)                                  0147
         L     @09,UCMDVEPT(,@09)                                  0147
         CLC   CHUCMP(1,@14),UCMID(@09)                            0147
         BE    @RT00147                                            0147
         CLI   CHUCMP(@14),X'80'                                   0147
         BNE   @GL00002                                            0147
         CLC   AUTHCODE(2,@14),@CB02462                            0147
         BE    @RT00147                                            0147
@GL00002 L     @14,CSCBPTR                                         0147
         CLI   CHUCMP(@14),0                                       0147
         BE    @RT00147                                            0147
*     GO TO PARSECMD;               /* PARSE VALID CALLER'S CMD      */
*                                                                  0148
*   /*****************************************************************/
*   /*                                                               */
*   /* REJECT COMMAND, INVALID CALLER                                */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0149
*   ELSE                                                           0149
*     GO TO INVCALLR;               /* INVALID CALLER EXIT           */
*                                                                  0149
         B     INVCALLR                                            0149
*   /*****************************************************************/
*   /*                                                               */
*   /* AUTHORIZED CALLER. PARSE COMMAND                              */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0150
*PARSECMD:                                                         0150
*   PARSEP=ADDR(CHBUF)+4;           /* SET PARSE PTR AFTER STOR      */
*                                                                  0150
PARSECMD L     PARSEP,CSCBPTR                                      0150
         LA    PARSEP,CHBUF(,PARSEP)                               0150
         AH    PARSEP,@CH00165                                     0150
*   /*****************************************************************/
*   /*                                                               */
*   /* PARSE FOR (                                                   */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0151
*   IF CHARS(1:1)^='(' THEN                                        0151
         CLI   CHARS(PARSEP),C'('                                  0151
         BNE   @RT00151                                            0151
*     GO TO INVSYN;                 /* INVALID SYNTAX EXIT           */
*   PARSEP=PARSEP+1;                /* INCR PTR PAST (               */
*                                                                  0153
         AH    PARSEP,@CH00066                                     0153
*   /*****************************************************************/
*   /*                                                               */
*   /* SET UP TO PARSE START ADDR                                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0154
*   TYPEADDR=''B;                   /* CLEAR ADRESS TYPE FLAGS       */
         MVI   TYPEADDR,X'00'                                      0154
*   BADDRP=ADDR(SRANGE);            /* @ START OF RANGE VAR          */
*                                                                  0155
         LA    BADDRP,SRANGE                                       0155
*   /*****************************************************************/
*   /*                                                               */
*   /* PARSE START ADDRESS                                           */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0156
*   CALL DECMADDR;                  /* PARSE FOR DECIMAL M ADDR      */
         BAL   @14,DECMADDR                                        0156
*   IF RC^=0 THEN                   /* NOT DECIMAL M ADDR            */
         ICM   @14,15,RC                                           0157
         BZ    @RF00157                                            0157
*     DO;                                                          0158
*       CALL DECKADDR;              /* PARSE FOR DECIMAL K ADDR      */
         BAL   @14,DECKADDR                                        0159
*       IF RC^=0 THEN               /* NOT DECIMAL K ADDR            */
         ICM   @14,15,RC                                           0160
         BZ    @RF00160                                            0160
*         DO;                                                      0161
*           CALL HEXADDR;           /* PARSE FOR HEXADECIMAL ADDR    */
         BAL   @14,HEXADDR                                         0162
*           IF RC^=0 THEN           /* NOT HEX ADDR                  */
         ICM   @14,15,RC                                           0163
         BNZ   @RT00163                                            0163
*             GO TO INVADDR;        /* INVALID ADDRESS EXIT          */
*         END;                                                     0165
*     END;                                                         0166
*                                                                  0166
@RF00160 DS    0H                                                  0167
*   /*****************************************************************/
*   /*                                                               */
*   /* ROUND DOWN START ADDRESS TO 4K PAGE                           */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0167
*   SRANGE=(SRANGE/4096)*4096;                                     0167
*                                                                  0167
@RF00157 L     @02,SRANGE                                          0167
         SRDA  @02,32                                              0167
         D     @02,@CF00139                                        0167
         SLA   @03,12                                              0167
         ST    @03,SRANGE                                          0167
*   /*****************************************************************/
*   /*                                                               */
*   /* PARSE FOR , BETWEEN START AND END ADDRESSES                   */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0168
*   IF CHARS(1:1)^=',' THEN                                        0168
         CLI   CHARS(PARSEP),C','                                  0168
         BNE   @RT00168                                            0168
*     GO TO INVSYN;                 /* INVALID SYNTAX EXIT           */
*   PARSEP=PARSEP+1;                /* INCR PAST ,                   */
*                                                                  0170
         AH    PARSEP,@CH00066                                     0170
*   /*****************************************************************/
*   /*                                                               */
*   /* SET UP TO PARSE END ADDR                                      */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0171
*   BADDRP=ADDR(ERANGE);            /* @ END OF RANGE VAR.           */
*                                                                  0171
         LA    BADDRP,ERANGE                                       0171
*   /*****************************************************************/
*   /*                                                               */
*   /* PARSE END ADDRESS                                             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0172
*   IF DECMA='1'B THEN                                             0172
*                                                                  0172
         TM    DECMA,B'10000000'                                   0172
         BNO   @RF00172                                            0172
*     /***************************************************************/
*     /*                                                             */
*     /* PARSE DECIMAL M ADDRESS                                     */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0173
*     CALL DECMADDR;                /* PARSE DECIMAL M ADDR          */
         BAL   @14,DECMADDR                                        0173
*   ELSE                                                           0174
*     IF DECKA='1'B THEN            /* DECIMAL K ADDR ?              */
         B     @RC00172                                            0174
@RF00172 TM    DECKA,B'01000000'                                   0174
         BNO   @RF00174                                            0174
*       CALL DECKADDR;              /* PARSE DECIMAL K ADDR          */
*                                                                  0175
         BAL   @14,DECKADDR                                        0175
*   /*****************************************************************/
*   /*                                                               */
*   /* PARSE HEX ADDRESS                                             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0176
*     ELSE                                                         0176
*       CALL HEXADDR;                                              0176
*                                                                  0176
         B     @RC00174                                            0176
@RF00174 BAL   @14,HEXADDR                                         0176
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK IF GOOD PARSE                                           */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0177
*   IF RC^=0 THEN                                                  0177
@RC00174 DS    0H                                                  0177
@RC00172 ICM   @14,15,RC                                           0177
         BNZ   @RT00177                                            0177
*     GO TO INVADDR;                /* REJECT IF FAILED              */
*                                                                  0178
*   /*****************************************************************/
*   /*                                                               */
*   /* ROUND END ADDR UP TO 4K PAGE                                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0179
*   ERANGE=((ERANGE+4096-1)/4096)*4096;                            0179
*                                                                  0179
         L     @02,ERANGE                                          0179
         SH    @02,@CH02883                                        0179
         SRDA  @02,32                                              0179
         D     @02,@CF00139                                        0179
         SLA   @03,12                                              0179
         ST    @03,ERANGE                                          0179
*   /*****************************************************************/
*   /*                                                               */
*   /* PARSE FUNCTION OPERAND                                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0180
*   IF CHARS(1:9)='),ONLINE ' THEN  /* ONLINE?                       */
         CLC   CHARS(9,PARSEP),@CC02473                            0180
         BNE   @RF00180                                            0180
*     OFFLINE='0'B;                 /* CLEAR OFFLINE FLAG            */
*                                                                  0181
         NI    OFFLINE,B'01111111'                                 0181
*   /*****************************************************************/
*   /*                                                               */
*   /* NOT ONLINE, TEST IF OFFLINE                                   */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0182
*   ELSE                                                           0182
*     IF CHARS(1:10)='),OFFLINE ' THEN/* OFFLINE?                    */
         B     @RC00180                                            0182
@RF00180 CLC   CHARS(10,PARSEP),@CC02476                           0182
         BNE   @RF00182                                            0182
*       OFFLINE='1'B;               /* SET OFFLINE FLAG              */
*                                                                  0183
         OI    OFFLINE,B'10000000'                                 0183
*   /*****************************************************************/
*   /*                                                               */
*   /* NO, THEN INVALID FUNCTION OPERAND                             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0184
*     ELSE                                                         0184
*       GO TO INVSYN;               /* INVALID SYNTAX EXIT           */
*                                                                  0184
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK IF VALID ADDRESS PAIR                                   */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0185
*   IF(ERANGE<=SRANGE)×(ERANGE>(CVTEORM+4096-1)) THEN              0185
@RC00180 L     @14,ERANGE                                          0185
         C     @14,SRANGE                                          0185
         BNH   @RT00185                                            0185
         L     @09,CVTPTR                                          0185
         L     @09,CVTEORM(,@09)                                   0185
         SH    @09,@CH02883                                        0185
         CR    @14,@09                                             0185
         BH    @RT00185                                            0185
*     GO TO INVADDR;                /* INVALID ERROR EXIT            */
*                                                                  0186
*   /*****************************************************************/
*   /*                                                               */
*   /* RETURN FROM GOOD PARSE                                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0187
*   RC=0;                           /* SET RETURN CODE               */
         SLR   @14,@14                                             0187
         ST    @14,RC                                              0187
*   RETURN;                         /* RETURN TO MAINLINE            */
*                                                                  0188
@EL00002 DS    0H                                                  0188
@EF00002 DS    0H                                                  0188
@ER00002 LM    @14,@12,@SA00002                                    0188
         BR    @14                                                 0188
*   /*****************************************************************/
*   /*                                                               */
*   /* INVALID SYNTAX ERROR EXIT                                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0189
*INVSYN:                                                           0189
*   RC=4;                           /* SET RETURN CODE               */
INVSYN   MVC   RC(4),@CF00165                                      0189
*   CALL WTORTN(9);                 /* INVALID SYNTAX ERROR MSG      */
         LA    @01,@AL00190                                        0190
         BAL   @14,WTORTN                                          0190
*   RETURN;                         /* RETURN TO MAINLINE            */
*                                                                  0191
         B     @EL00002                                            0191
*   /*****************************************************************/
*   /*                                                               */
*   /* INVALID ADDRESS ERROR EXIT                                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0192
*INVADDR:                                                          0192
