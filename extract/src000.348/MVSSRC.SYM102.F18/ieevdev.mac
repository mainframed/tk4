CHNTEST  STM   @14,@12,12(@13)                                     0160
*/****************************************************************** */
*/* CHNTEST ROUTINE WILL DETERMINE IF A CPU/CHANNEL COMBINATION IS   */
*/* INVOLVED IN A PATH TO A SPECIFIC DEVICE. RETURN CODE ZERO        */
*/* INDICATES THAT THE CHANNEL IS INVOLVED IN THE PATH, RETURN CODE  */
*/* FOUR INDICATES THAT THE CHANNEL IS NOT INVOLVED IN THE PATH.     */
*/****************************************************************** */
*                                                                  0161
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK FOR VALID CPU ADDRESS. ACCEPT ONLY 0 OR 1               */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0161
*   IF CPUID>1 THEN                 /* VALID?                        */
         L     @10,@PC00001                                        0161
         CLI   CPUID(@10),1                                        0161
         BNH   @RF00161                                            0161
*     DO;                                                          0162
*       RCODE=16;                   /* REASON CODE                   */
         MVI   RCODE,X'10'                                         0163
*       ABIND=1;                    /* SET ABEND FLAG                */
         MVI   ABIND,X'01'                                         0164
*     END;                                                         0165
*                                                                  0165
*   /*****************************************************************/
*   /*                                                               */
*   /* VALID CPU ADDRESS. CONTINUE                                   */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0166
*   ELSE                                                           0166
*                                                                  0166
*     /***************************************************************/
*     /*                                                             */
*     /* CHECK WHICH CPU IS TO BE TESTED                             */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0166
*     IF CPUID=0 THEN               /* CPU ZERO?                     */
*                                                                  0166
         B     @RC00161                                            0166
@RF00161 L     @10,@PC00001                                        0166
         CLI   CPUID(@10),0                                        0166
         BNE   @RF00166                                            0166
*       /*************************************************************/
*       /*                                                           */
*       /* DETERMINE IF A PATH EXISTS USING THE SPECIFIED CHANNEL    */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0167
*       IF CHNID=CHN00&PTH00='01'X  /* PATH AVAILABLE ON FIRST?      */
*           ×CHNID=CHN10&PTH10='01'X THEN/* PATH EXIST?              */
         CLC   CHNID(1,@10),CHN00                                  0167
         BNE   @GL00023                                            0167
         CLI   PTH00,X'01'                                         0167
         BE    @RT00167                                            0167
@GL00023 L     @10,@PC00001                                        0167
         CLC   CHNID(1,@10),CHN10                                  0167
         BNE   @RF00167                                            0167
         CLI   PTH10,X'01'                                         0167
         BNE   @RF00167                                            0167
@RT00167 DS    0H                                                  0168
*         RCODE=0;                  /* YES, SET RETURN CODE 0        */
*                                                                  0168
         MVI   RCODE,X'00'                                         0168
*   /*****************************************************************/
*   /*                                                               */
*   /* NO PATHS EXIST. SET RETURN CODE                               */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0169
*       ELSE                                                       0169
*         RCODE=4;                  /* SET RETURN CODE 4             */
*                                                                  0169
         B     @RC00167                                            0169
@RF00167 MVI   RCODE,X'04'                                         0169
*   /*****************************************************************/
*   /*                                                               */
*   /* CPUID WAS NOT ZERO, MUST THEN BE 1                            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0170
*     ELSE                                                         0170
*                                                                  0170
*       /*************************************************************/
*       /*                                                           */
*       /* CHECK IF A PATH EXISTS USING SAME CHANNEL, OTHER CPU      */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0170
*       IF CHNID=CHN00&PTH01='01'X×CHNID=CHN10&PTH11='01'X THEN/*  0170
*                                      PATH?                         */
         B     @RC00166                                            0170
@RF00166 L     @10,@PC00001                                        0170
         CLC   CHNID(1,@10),CHN00                                  0170
         BNE   @GL00026                                            0170
         CLI   PTH01,X'01'                                         0170
         BE    @RT00170                                            0170
@GL00026 L     @10,@PC00001                                        0170
         CLC   CHNID(1,@10),CHN10                                  0170
         BNE   @RF00170                                            0170
         CLI   PTH11,X'01'                                         0170
         BNE   @RF00170                                            0170
@RT00170 DS    0H                                                  0171
*         RCODE=0;                  /* SET RETURN CODE ZERO          */
         MVI   RCODE,X'00'                                         0171
*       ELSE                                                       0172
*         RCODE=4;                  /* SET RETURN CODE FOUR          */
         B     @RC00170                                            0172
@RF00170 MVI   RCODE,X'04'                                         0172
*   END;                            /* END OF CHANNEL TEST ROUTINE   */
