CPULEVEL STM   @14,@12,12(@13)                                     0144
*/* *************************************************************    */
*/*  CPU LEVEL CHECK WILL CHECK IF A PATH EXISTS TO THE DEVICE ON    */
*/*  A CPU OTHER THAN THE ONE THE CALLER HAS SPECIFIED IN THE        */
*/*  INPUT PARAMETERS A RCODE OF 0 INDICATES THAT A PATH DOES        */
*/*  EXIST ON ANOTHER CPU A RCODE OF 4 INDICATES THAT A PATH DOES NOT*/
*/*  EXIST ON ANOTHER CPU                                            */
*/* *************************************************************    */
*                                                                  0145
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK FOR A AVAILABLE PATH ANYWHERE                           */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0145
*   IF PTH00='01'X×PTH01='01'X×     /* CHECK FOR ANY AVAL PATHS      */
*       PTH10='01'X×PTH11='01'X THEN/* ANYWHERE                      */
*                                                                  0145
         CLI   PTH00,X'01'                                         0145
         BE    @RT00145                                            0145
         CLI   PTH01,X'01'                                         0145
         BE    @RT00145                                            0145
         CLI   PTH10,X'01'                                         0145
         BE    @RT00145                                            0145
         CLI   PTH11,X'01'                                         0145
         BNE   @RF00145                                            0145
@RT00145 DS    0H                                                  0146
*     /***************************************************************/
*     /*                                                             */
*     /* A AVAILABLE PATH DOES EXIST.. VERIFY INPUT PARAMETERS IF CPU*/
*     /* ADDRESS IS GREATER THAN 1 SET ABEND INDICATOR               */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0146
*     IF CPUID=>2 THEN              /* CHECK FOR VALID CPUID         */
         L     @10,@PC00001                                        0146
         CLI   CPUID(@10),2                                        0146
         BL    @RF00146                                            0146
*       DO;                                                        0147
*         RCODE=16;                 /* SET REASON CODE               */
         MVI   RCODE,X'10'                                         0148
*         ABIND=1;                  /* INVALID.. SET ABEND IND.      */
         MVI   ABIND,X'01'                                         0149
*       END;                                                       0150
*                                                                  0150
*   /*****************************************************************/
*   /*                                                               */
*   /* INPUT PARAMETERS ARE VALID.. CONTINUE                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0151
*     ELSE                          /* VALID CONTINUE                */
*                                                                  0151
*       /*************************************************************/
*       /*                                                           */
*       /* DETERMINE WHICH CPU IS BEING REMOVED..                    */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0151
*       IF CPUID=0 THEN             /* CPU 0 BEING REMOVED?          */
*                                                                  0151
         B     @RC00146                                            0151
@RF00146 L     @10,@PC00001                                        0151
         CLI   CPUID(@10),0                                        0151
         BNE   @RF00151                                            0151
*         /***********************************************************/
*         /*                                                         */
*         /* CPU 0 BEING REMOVED.. DOES AVAILABLE PATH EXIST ON CPU 1*/
*         /* ? IF SO SET RCODE 0                                     */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0152
*         IF PTH01='01'X×PTH11='01'X THEN/* IS PATH ON OTHER CPU     */
         CLI   PTH01,X'01'                                         0152
         BE    @RT00152                                            0152
         CLI   PTH11,X'01'                                         0152
         BNE   @RF00152                                            0152
@RT00152 DS    0H                                                  0153
*           RCODE=0;                /* PATH DOES NOT NEED CPU        */
*                                                                  0153
         MVI   RCODE,X'00'                                         0153
*   /*****************************************************************/
*   /*                                                               */
*   /* PATH DOES NOT EXIST SET RCODE 4                               */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0154
*         ELSE                      /* PATH DOES NEED THIS CPU       */
*           RCODE=4;                /* SET DEPENDENT RETURN CODE     */
*                                                                  0154
         B     @RC00152                                            0154
@RF00152 MVI   RCODE,X'04'                                         0154
*   /*****************************************************************/
*   /*                                                               */
*   /* WAS NOT CPU0 BEING REMOVED.. MUST BE CPU1 THEN                */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0155
*       ELSE                        /* CPU 1 BEING REMOVED THEN      */
*                                                                  0155
*         /***********************************************************/
*         /*                                                         */
*         /* CHECK IF PATH EXISTS ON CPU 0 IF SO SET RCODE = 0       */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0155
*         IF PTH00='01'X×PTH10='01'X THEN/* PATH ON OTHER CPU?       */
         B     @RC00151                                            0155
@RF00151 CLI   PTH00,X'01'                                         0155
         BE    @RT00155                                            0155
         CLI   PTH10,X'01'                                         0155
         BNE   @RF00155                                            0155
@RT00155 DS    0H                                                  0156
*           RCODE=0;                /* YES.. SET OK RETURN CODE      */
*                                                                  0156
         MVI   RCODE,X'00'                                         0156
*   /*****************************************************************/
*   /*                                                               */
*   /* OTHER PATH DOES NOT EXIST.. SET RCODE = 4                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0157
*         ELSE                      /* PATH DEPENDENT ON THIS        */
*           RCODE=4;                /* DEPENDENT RETURN CODE         */
*                                                                  0157
         B     @RC00155                                            0157
@RF00155 MVI   RCODE,X'04'                                         0157
*   /*****************************************************************/
*   /*                                                               */
*   /* NO AVAILABLE PATHS EXIST TO THE DEVICE.. SET RCODE 0 DEVICE   */
*   /* NOT DEPENDENT UPON SPECIFIED CPU                              */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0158
*   ELSE                            /* NO AVALIABLE PATHS            */
*     RCODE=0;                      /* PATH NOT DEPENDENT ON CPU     */
         B     @RC00145                                            0158
@RF00145 MVI   RCODE,X'00'                                         0158
*   END;                            /* END OF CPU LEVEL CHECK        */
@EL00003 DS    0H                                                  0159
@EF00003 DS    0H                                                  0159
