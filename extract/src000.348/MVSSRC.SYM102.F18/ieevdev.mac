         TITLE 'IEEVDEV DEVICE SUBROUTINE MAINLINE                     *
                        '
IEEVDEV  CSECT
@MAINENT DS    0H
         USING  *,15
IEEVDEV  MODID  BR=YES
@PROLOG  STM    @14,@12,12(@13)
         BALR   @12,0
@PSTART  DS     0H
         USING  @PSTART,@12
         L      @00,@SIZDATD
         GETMAIN R,LV=(0)
         LR     @11,@01
         USING  @DATD,@11
         IPK                           SAVE USERS KEY          YM07661
         SPKA   0(@00)                 CHANGE TO KEY 0         YM07661
         ST     @13,@SA00001+4
         LM     @00,@01,20(@13)
         ST     @11,8(@13)
         LR     @13,@11
         MVC    @PC00001(4),0(@01)
*   UCBPTR=UCBADD;                  /* INITILIZE UCB POINTER         */
         L     @15,@PC00001                                        0037
         L     UCBPTR,UCBADD(,@15)                                 0037
*   ABIND=0;                        /* RESET ABEND INDICATOR         */
*                                                                  0038
         MVI   ABIND,X'00'                                         0038
*   /*****************************************************************/
*   /*                                                               */
*   /* VERIFY THAT UCB ADDRESS POINTS TO UCB COMMON SEGEMENT BY      */
*   /* CHECKING FOR UCB IDENTIFIER                                   */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0039
*   IF UCBID^='FF'X THEN            /* CHECK FOR UCB IDENTIFIER      */
*                                                                  0039
         CLI   UCBID(UCBPTR),X'FF'                                 0039
         BE    @RF00039                                            0039
*     /***************************************************************/
*     /*                                                             */
*     /* UCB ADDRESS PASSED DID NOT POINT TO UCB COMMON SEGEMENT SET */
*     /* REASON CODE OF 4 AND ISSUE ABEND                            */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0040
*     DO;                                                          0040
*       RCODE=4;                    /* SET REASON CODE TO 4          */
         MVI   RCODE,X'04'                                         0041
*       ABIND=1;                    /* SET ABEND ERROR FLAG          */
         MVI   ABIND,X'01'                                         0042
*       GOTO ABEND1;                /* GO ISSUE ABEND INVALID ADDR   */
         B     ABEND1                                              0043
*     END;                                                         0044
*                                                                  0044
*   /*****************************************************************/
*   /*                                                               */
*   /* SET UP REGISTERS FOR IOSGEN MAP FUNCTION                      */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0045
*   R4=UCBPTR;                      /* ADDRESS OF UCB FROM PARM      */
@RF00039 LR    R4,UCBPTR                                           0045
*   REGSAVE=SAVEPTR;                /* SAVE ADDR OF OUR SAVE AREA    */
         ST    SAVEPTR,REGSAVE                                     0046
*   SAVEPTR=ADDR(REGSAVEA);         /* PTR TO SAVE AREA FOR IOSGEN   */
         LA    SAVEPTR,REGSAVEA                                    0047
*   R5=ADDR(PATHMAP);               /* ADDRESS TO BUILD PATHMAP      */
         LA    R5,PATHMAP                                          0048
*   R14=ADDR(ENDMAP);               /* ADDRESS FOR END OF BUILD      */
         LA    R14,ENDMAP                                          0049
*   GEN REFS(UCBCHA,UCBCHM,UCBFL5,UCBLCI,UCBUA,CVTILCH,CVTCSD,CVTPCCAT,
*       PCCACPUA,PCCACAFM,PCCACAT,CSDCPUAL,CATFLG,LCHTCH,CVTPTR,UCBBGN,
*       UCBBASE,UCBWGT,CVTCRCA)(IOSGEN MAP,VAR=1);                 0050
         IOSGEN MAP,VAR=1
*ENDMAP:                                                           0051
*   SAVEPTR=REGSAVE;                /* RELOAD SAVEAREA POINTER       */
ENDMAP   L     SAVEPTR,REGSAVE                                     0051
*   RFY                                                            0052
*    (R4,                                                          0052
*     R5,                                                          0052
*     R14,                                                         0052
*     SAVEPTR) UNRSTD;              /* RELEASE REGISTERS FOR USE     */
*                                                                  0052
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK FUCNTION CODE PASSED AS A PARAMETER FOR FUNCTION CODE 00*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0053
*   IF FCODE=0 THEN                 /* CHECK FUNCTION CODE ZERO      */
         L     @10,@PC00001                                        0053
         CLI   FCODE(@10),0                                        0053
         BNE   @RF00053                                            0053
*     DO;                                                          0054
*                                                                  0054
*       /*************************************************************/
*       /*                                                           */
*       /* IS FUNCTION CODE 00... CHECK IF DEVICE IS IN USE IN A     */
*       /* OFFLINE CONDITION                                         */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0055
*       IF UCBNALOC='1'B THEN       /* IF OLTEP HAS DEVICE           */
         TM    UCBNALOC(UCBPTR),B'00000100'                        0055
         BNO   @RF00055                                            0055
*         RCODE=16;                 /* SET OLTEP RETURN CODE         */
*                                                                  0056
         MVI   RCODE,X'10'                                         0056
*       /*************************************************************/
*       /*                                                           */
*       /* DEVICE NOT IN USE WHILE OFFLINE CHECK FURTHER             */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0057
*       ELSE                        /* OTHERWISE CONTINUE            */
*                                                                  0057
*         /***********************************************************/
*         /*                                                         */
*         /* DETERMINE IF DEVICE IS OFFLINE.. IF NOT THEN SET RETURN */
*         /* CODE TWENTY AND RETURN TO CALLER                        */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0057
*         IF UCBONLI='1'B THEN      /* IF UCB IS NOT OFFLINE         */
         B     @RC00055                                            0057
@RF00055 TM    UCBONLI(UCBPTR),B'10000000'                         0057
         BNO   @RF00057                                            0057
*           RCODE=20;               /* SET RETURN CODE OF TWENTY     */
*                                                                  0058
         MVI   RCODE,X'14'                                         0058
*       /*************************************************************/
*       /*                                                           */
*       /* DEVICE IS NOT ONLINE OR IN USE.. CONTINUE                 */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0059
*         ELSE                      /* OTHERWISE CONTINUE            */
*                                                                  0059
*           /*********************************************************/
*           /*                                                       */
*           /* CHECK PATHMAP TO DETERMINE IF A AVAILABLE PATH TO THE */
*           /* DEVICE EXISTS.. IF SO THEN DO A PATH TEST TO CHECK    */
*           /* OPERATIONAL STATUS OF PATHS                           */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0059
*           IF PTH00='01'X×PTH01='01'X×/* CHECK FOR ANY AVAILABLE    */
*               PTH10='01'X×PTH11='01'X THEN/* PATH ANYWHERE         */
         B     @RC00057                                            0059
@RF00057 CLI   PTH00,X'01'                                         0059
         BE    @RT00059                                            0059
         CLI   PTH01,X'01'                                         0059
         BE    @RT00059                                            0059
         CLI   PTH10,X'01'                                         0059
         BE    @RT00059                                            0059
         CLI   PTH11,X'01'                                         0059
         BNE   @RF00059                                            0059
@RT00059 DS    0H                                                  0060
*             DO;                   /* INSERT                        */
*                                                                  0060
*               /*****************************************************/
*               /*                                                   */
*               /* PATHS DO EXIST TO DEVICE. SET UP PARMS TO LINK TO */
*               /* IOS PATHS TEST ROUTINE                            */
*               /*                                                   */
*               /*****************************************************/
*                                                                  0061
*               FUNCPTR=UCBPTR;     /* UCB ADDRESS FOR PATH TEST     */
         ST    UCBPTR,FUNCPTR                                      0061
*               FUNCTION=0;         /* FUNCTION CODE FOR PATH TEST   */
*                                                                  0062
         MVI   FUNCTION,X'00'                                      0062
*               /*****************************************************/
*               /*                                                   */
*               /* REGISTER ONE MUST POINT DIRECTLY TO ONE WORD      */
*               /* PARAMETER                                         */
*               /*                                                   */
*               /*****************************************************/
*                                                                  0063
*               RFY                                                0063
*                (R1) RSTD;         /* RESTRICT REGISTER ONE         */
*               R1=ADDR(FUNCPTR);   /* PASS PARMS TO IOS             */
         LA    R1,FUNCPTR                                          0064
*               GEN(LINK EP=IECVIOPM);/* LINK TO IOS PATH TEST       */
         LINK EP=IECVIOPM
*               RFY                                                0066
*                (R1) UNRSTD;       /* RELEASE REGISTER ONE          */
*                                                                  0066
*               /*****************************************************/
*               /*                                                   */
*               /* UPON RETURN REGISTER 15 WILL HAVE RETURN CODE     */
*               /* INDICATING STATUS OF PATHS. CONVERT RETURN CODE TO*/
*               /* VALUE EXPECTED BY MY CALLER AND RETURN            */
*               /*                                                   */
*               /*****************************************************/
*                                                                  0067
*               RFY                                                0067
*                (R15) RSTD;        /* RESTRICT REG 15 FOR TESTS     */
*                                                                  0067
*               /*****************************************************/
*               /*                                                   */
*               /* CHECK FOR ANY OPERATIONAL PATH. RET CODE 0 MEANS  */
*               /* AT LEAST ONE PATH EXISTS TO DEVICE. RET CODE      */
*               /* TWENTY MEANS DEVICE IS RESERVED BUT PATH EXISTS.  */
*               /*                                                   */
*               /*****************************************************/
*                                                                  0068
*               IF R15=0×R15=20 THEN/* CHECK FOR OPERATIONAL PATH    */
         LTR   R15,R15                                             0068
         BZ    @RT00068                                            0068
         C     R15,@CF01784                                        0068
         BNE   @RF00068                                            0068
@RT00068 DS    0H                                                  0069
*                 DO;                                              0069
*                   UCBVHRSN='0'B;  /* RESET HIER REASON INDICATOR   */
         NI    UCBVHRSN(UCBPTR),B'11111101'                        0070
*                   RCODE=0;        /* INDICATE OPERATIONAL PATHS    */
         MVI   RCODE,X'00'                                         0071
*                 END;                                             0072
*                                                                  0072
*               /*****************************************************/
*               /*                                                   */
*               /* OPERATIONAL PATH DOESN'T EXIST CHECK FURTHER      */
*               /*                                                   */
*               /*****************************************************/
*                                                                  0073
*               ELSE                                               0073
*                                                                  0073
*                 /***************************************************/
*                 /*                                                 */
*                 /* CHECK IF NO OPERATIONAL PATHS EXIST TO THE      */
*                 /* DEVICE RET CODE 4 MEANS NO PATHS EXIST I/O WAS  */
*                 /* ISSUED, 8 MEANS NO PATHS EXIST I/O WAS NOT      */
*                 /* ISSUED                                          */
*                 /*                                                 */
*                 /***************************************************/
*                                                                  0073
*                 IF R15=4×R15=8 THEN/* CHECK FOR NO OPERATIONAL PATH*/
         B     @RC00068                                            0073
@RF00068 C     R15,@CF00086                                        0073
         BE    @RT00073                                            0073
         C     R15,@CF00039                                        0073
         BNE   @RF00073                                            0073
@RT00073 DS    0H                                                  0074
*                   DO;                                            0074
*                     UCBVORSN='1'B;/* SET OPERATOR REASON INDICATOR */
         OI    UCBVORSN(UCBPTR),B'00000100'                        0075
*                     RCODE=12;     /* SET NO OPERATIONAL PATH RCODE */
         MVI   RCODE,X'0C'                                         0076
*                   END;                                           0077
*                                                                  0077
*               /*****************************************************/
*               /*                                                   */
*               /* PATH IS NEITHER OPERATIONAL NOR INOPERATIONAL     */
*               /* CHECK FOR ERROR OF SOME KIND                      */
*               /*                                                   */
*               /*****************************************************/
*                                                                  0078
*                 ELSE                                             0078
*                                                                  0078
*                   /*************************************************/
*                   /*                                               */
*                   /* RETURN CODE 12 INDICATES NO STORAGE WAS       */
*                   /* AVAILABLE FOR PROCESSING                      */
*                   /*                                               */
*                   /*************************************************/
*                                                                  0078
*                   IF R15=12 THEN  /* CHECK FOR NO CORE             */
         B     @RC00073                                            0078
@RF00073 C     R15,@CF00033                                        0078
         BNE   @RF00078                                            0078
*                     RCODE=8;      /* INDICATE NOCORE               */
         MVI   RCODE,X'08'                                         0079
*                   ELSE            /* INVALID RETURN CODE           */
*                                                                  0080
*                     /***********************************************/
*                     /*                                             */
*                     /* INVALID RETURN CODE RECEIVED FROM IOS SET   */
*                     /* REASON CODE AND ABEND INDICATOR             */
*                     /*                                             */
*                     /***********************************************/
*                                                                  0080
*                     DO;                                          0080
         B     @RC00078                                            0080
@RF00078 DS    0H                                                  0081
*                       RCODE=20;   /* SET REASON CODE               */
         MVI   RCODE,X'14'                                         0081
*                       ABIND=1;    /* SET ABEND INDICATOR           */
         MVI   ABIND,X'01'                                         0082
*                       RFY                                        0083
*                        (R15) UNRSTD;                             0083
*                     END;                                         0084
*             END;                  /* END OF IECVIOPM PROCESSING    */
*                                                                  0085
*       /*************************************************************/
*       /*                                                           */
*       /* NO AVAILABLE PATHS TO THE DEVICE EXIST.. INDICATE TO      */
*       /* CALLER BY SETTING RETURN CODE 4 ALSO SET HIERACRY REASON  */
*       /* INDICATOR IN UCB                                          */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0086
*           ELSE                    /* THERE WERE NO PATHS           */
*             DO;                   /* SO SET HIERARCHY REASON       */
         B     @RC00059                                            0086
@RF00059 DS    0H                                                  0087
*               RCODE=4;            /* BIT IN UCB AND SET RETURN     */
         MVI   RCODE,X'04'                                         0087
*               UCBVHRSN='1'B;      /* CODE INDICATING NO PATHS      */
         OI    UCBVHRSN(UCBPTR),B'00000010'                        0088
*             END;                  /* ARE AVAILABLE                 */
*     END;                          /* END OF FUNCTION CODE 00     0090
*                                      PROCESSING                    */
*                                                                  0090
*   /*****************************************************************/
*   /*                                                               */
*   /* FUNCTION CODE WAS NOT 00.. CHECK FOR FUNCTION CODE 01 AND IF  */
*   /* SO CALL CPULEVEL ROUTINE                                      */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0091
*   ELSE                            /* FUNCTION CODE OTHER THAN 00   */
*     IF FCODE=1 THEN               /* IF FUNCTION CODE IS 01        */
         B     @RC00053                                            0091
@RF00053 L     @10,@PC00001                                        0091
         CLI   FCODE(@10),1                                        0091
         BNE   @RF00091                                            0091
*       CALL CPULEVEL;              /* THEN GO TO CPU ROUTINE        */
*                                                                  0092
         BAL   @14,CPULEVEL                                        0092
*   /*****************************************************************/
*   /*                                                               */
*   /* FUNCTION CODE WAS NOT 01.. CHECK FOR FUNCTION CODE 02 AND IF  */
*   /* SO CALL CHANNEL LEVEL ROUTINE.. OTHERWISE SET ABEND           */
*   /* INDICATOR... INVALID PARAMETERS                               */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0093
*     ELSE                          /* FUNCTION CODE NOT 00 OR 01    */
*       IF FCODE=2 THEN             /* IF FUNCTION CODE IS 02        */
         B     @RC00091                                            0093
@RF00091 L     @10,@PC00001                                        0093
         CLI   FCODE(@10),2                                        0093
         BNE   @RF00093                                            0093
*         CALL CHNLEVEL;            /* THEN GO TO CHANNEL ROUTINE    */
*                                                                  0094
         BAL   @14,CHNLEVEL                                        0094
*   /*****************************************************************/
*   /*                                                               */
*   /* WAS NOT FUNCTION CODE 0, 1, OR 2, CHECK FOR 3 OR 4            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0095
*       ELSE                        /* OTHERWISE IT IS AN            */
*                                                                  0095
*         /***********************************************************/
*         /*                                                         */
*         /* CHECK FOR 3 OR 4                                        */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0095
*         IF FCODE=3 THEN           /* FUNCTION CODE 3?              */
         B     @RC00093                                            0095
@RF00093 L     @10,@PC00001                                        0095
         CLI   FCODE(@10),3                                        0095
         BNE   @RF00095                                            0095
*           CALL CPUTEST;           /* CHECK CPU STATUS              */
         BAL   @14,CPUTEST                                         0096
*         ELSE                                                     0097
*           IF FCODE=4 THEN         /* FUNCTION CODE 4?              */
         B     @RC00095                                            0097
@RF00095 L     @10,@PC00001                                        0097
         CLI   FCODE(@10),4                                        0097
         BNE   @RF00097                                            0097
*             CALL CHNTEST;         /* CALL CHANNEL TEST             */
*                                                                  0098
         BAL   @14,CHNTEST                                         0098
*   /*****************************************************************/
*   /*                                                               */
*   /* FUNCTION CODE WAS NOT 0, 1, 2, 3, OR 4. SET ERROR FLAG        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0099
*           ELSE                                                   0099
*             DO;                                                  0099
         B     @RC00097                                            0099
@RF00097 DS    0H                                                  0100
*               RCODE=12;           /* REASON CODE FOR ABEND         */
         MVI   RCODE,X'0C'                                         0100
*               ABIND=1;            /* INVALID FUNCTION CODE         */
         MVI   ABIND,X'01'                                         0101
*             END;                                                 0102
*                                                                  0102
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK IF INVALID PARAMETERS WERE DETECTED ANYWHERE AND IF SO  */
*   /* ISSUE ABEND                                                   */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0103
*   IF ABIND=1 THEN                 /* IF INDICATOR IS ON THEN       */
@RC00097 DS    0H                                                  0103
@RC00095 DS    0H                                                  0103
@RC00093 DS    0H                                                  0103
@RC00091 DS    0H                                                  0103
@RC00053 CLI   ABIND,1                                             0103
         BNE   @RF00103                                            0103
*ABEND1:                                                           0104
*     DO;                                                          0104
ABEND1   DS    0H                                                  0105
*       REASON=RCODE;               /* RESTORE REASON CODE TO      0105
*                                      REGISTER 15                   */
         SLR   REASON,REASON                                       0105
         IC    REASON,RCODE                                        0105
*       GEN(SPKA  0(@02));          /* RESTORE TO KEY OF CALLER    0106
*                                                            @YM07661*/
         SPKA  0(@02)
*       DO;                         /* ABEND(126) SYSTEM             */
*         RESPECIFY                                                0108
*          (GPR01F) RESTRICTED;                                    0108
*         GPR01F=((126)&'00000FFF'X)*4096;/* COMP CODE IN BITS 8-19  */
         L     GPR01F,@CF01817                                     0109
*         SVC(13);                  /* ISSUE ABEND SVC               */
         SVC   13                                                  0110
*         RESPECIFY                                                0111
*          (GPR01F) UNRESTRICTED;                                  0111
*       END;                        /* ABEND(126) SYSTEM ISSUE SYSTEM
*                                      ABEND 07E                     */
*       RFY                                                        0113
*        (REASON) UNRSTD;           /* UNRESTRICT REGISTER 15        */
*     END;                                                         0114
*                                                                  0114
*   /*****************************************************************/
*   /*                                                               */
*   /* LOAD RETURN CODE AND PASS TO CALLER                           */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0115
*   ELSE                            /* OTHERWISE PASS RETURN         */
*     DO;                                                          0115
*                                                                  0115
         B     @RC00103                                            0115
@RF00103 DS    0H                                                  0116
*       /*************************************************************/
*       /*                                                           */
*       /* PICK UP ALL INFORMATION OUT OF WORK AREA, CHANGE BACK TO  */
*       /* CALLERS KEY, AND ISSUE FREEMAIN                           */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0116
*       GENERATE;                                                  0116
     SLR    @10,@10
     IC     @10,RCODE
     L      @13,4(,@13)
     L      @00,@SIZDATD
     LR     @01,@11
     SPKA   0(@02)                                       YM08605
     FREEMAIN R,LV=(0),A=(1)
     LR     @15,@10
     L      @14,12(,@13)
     LM     @00,@12,20(@13)
     BR     @14
*     END;                                                         0117
*CHNLEVEL:                                                         0118
*   PROC OPTIONS(NOSAVEAREA);       /* CHANNEL LEVEL PROCEDURE       */
@EL00001 L     @13,4(,@13)                                         0118
@EF00001 L     @00,@SIZDATD                                        0118
         LR    @01,@11                                             0118
         FREEMAIN R,LV=(0),A=(1)
@ER00001 LM    @14,@12,12(@13)                                     0118
         BR    @14                                                 0118
CHNLEVEL STM   @14,@12,12(@13)                                     0118
*/***************************************************************    */
*/*  CHANNEL LEVEL ROUTINE WILL DETERMINE IF A PATH EXISTS           */
*/*  TO THE REFERENCED DEVICE USING A CHANNEL OTHER THAN THE         */
*/*  ONE THE CALLER HAS SPECIFIED IN THE INPUT PARAMETERS            */
*/*                                                                  */
*/*  A RETURN CODE OF 00 INDICATES THAT ANOTHER PATH DOES EXIST      */
*/*  A RETURN CODE OF 04 INDICATES THAT ANOTHER PATH DOESNOT EXIST   */
*/***************************************************************    */
*                                                                  0119
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK IF A AVAILABLE PATH EXISTS ANYWHERE FOR THIS DEVICE BY  */
*   /* INTEROGATING THE PATHMAP                                      */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0119
*   IF PTH00='01'X×PTH01='01'X×     /* CHECK FOR AN AVAILABLE        */
*       PTH10='01'X×PTH11='01'X THEN/* PATH ON ANY CHANNEL           */
*                                                                  0119
         CLI   PTH00,X'01'                                         0119
         BE    @RT00119                                            0119
         CLI   PTH01,X'01'                                         0119
         BE    @RT00119                                            0119
         CLI   PTH10,X'01'                                         0119
         BE    @RT00119                                            0119
         CLI   PTH11,X'01'                                         0119
         BNE   @RF00119                                            0119
@RT00119 DS    0H                                                  0120
*     /***************************************************************/
*     /*                                                             */
*     /* CHECK IF THE CHANNEL IN QUESTION IS REPRESENTED IN THE PATH */
*     /* MAP FOR THIS DEVICE                                         */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0120
*     IF CHNID=CHN00×CHNID=CHN10 THEN/* CHANNEL ID MATCH MAP ?       */
*                                                                  0120
         L     @10,@PC00001                                        0120
         CLC   CHNID(1,@10),CHN00                                  0120
         BE    @RT00120                                            0120
         CLC   CHNID(1,@10),CHN10                                  0120
         BNE   @RF00120                                            0120
@RT00120 DS    0H                                                  0121
*       /*************************************************************/
*       /*                                                           */
*       /* A PATH DOES EXIST.. VERIFY THAT THE CPU ADDRESS PASSED IS */
*       /* VALID AND IF NOT SET ABEND INDICATOR                      */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0121
*       IF CPUID=>2 THEN            /* CPU ID VALID?                 */
         L     @10,@PC00001                                        0121
         CLI   CPUID(@10),2                                        0121
         BL    @RF00121                                            0121
*         DO;                                                      0122
*           RCODE=16;               /* SET REASON CODE               */
         MVI   RCODE,X'10'                                         0123
*           ABIND=1;                /* SET ABEND INDICATOR           */
         MVI   ABIND,X'01'                                         0124
*         END;                                                     0125
*                                                                  0125
*   /*****************************************************************/
*   /*                                                               */
*   /* CPU ADDRESS IS VALID                                          */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0126
*       ELSE                        /* OTHERWISE                     */
*                                                                  0126
*         /***********************************************************/
*         /*                                                         */
*         /* DETERMINE WHICH CPU CONTAINS THE CHANNEL IN QUESTION    */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0126
*         IF CPUID=0 THEN           /* IS THIS CPU ZERO              */
*                                                                  0126
         B     @RC00121                                            0126
@RF00121 L     @10,@PC00001                                        0126
         CLI   CPUID(@10),0                                        0126
         BNE   @RF00126                                            0126
*           /*********************************************************/
*           /*                                                       */
*           /* CHANNEL IS ON FIRST CPU.. CHECK IF CHANNEL IS FIRST   */
*           /* CHANNEL REPRESENTED IN PATHMAP                        */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0127
*           IF CHNID=CHN00 THEN     /* FIRST CHANNEL BEING REMOVED   */
*                                                                  0127
         CLC   CHNID(1,@10),CHN00                                  0127
         BNE   @RF00127                                            0127
*             /*******************************************************/
*             /*                                                     */
*             /* FIRST CHANNEL ON FIRST CPU BEING REMOVED... CHECK IF*/
*             /* PATH EXISTS ON ANY OTHER CHANNEL IF SO SET RCODE 0  */
*             /*                                                     */
*             /*******************************************************/
*                                                                  0128
*             IF PTH01='01'X×PTH10='01'X/* IS A PATH AVAILABLE ON ANY*/
*                 ×PTH11='01'X THEN /* OTHER CHANNEL                 */
         CLI   PTH01,X'01'                                         0128
         BE    @RT00128                                            0128
         CLI   PTH10,X'01'                                         0128
         BE    @RT00128                                            0128
         CLI   PTH11,X'01'                                         0128
         BNE   @RF00128                                            0128
@RT00128 DS    0H                                                  0129
*               RCODE=0;            /* SET ZERO RETURN CODE          */
*                                                                  0129
         MVI   RCODE,X'00'                                         0129
*   /*****************************************************************/
*   /*                                                               */
*   /* NO OTHER PATH EXISTS.. SET RCODE 4                            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0130
