         B     @RC00128                                            0130
@RF00128 MVI   RCODE,X'04'                                         0130
*   /*****************************************************************/
*   /*                                                               */
*   /* SECOND CHANNEL ON FIRST CPU BEING REMOVED... CHECK IF PATH    */
*   /* EXISTS ON ANY OTHER CHANNEL.. IF SO SET RCODE = 0             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0131
*           ELSE                    /*                               */
*             IF PTH00='01'X×PTH01='01'X×/* CHECK FOR AVAILABLE PATH */
*                 PTH11='01'X THEN  /* ON ANY OTHER CHANNEL          */
         B     @RC00127                                            0131
@RF00127 CLI   PTH00,X'01'                                         0131
         BE    @RT00131                                            0131
         CLI   PTH01,X'01'                                         0131
         BE    @RT00131                                            0131
         CLI   PTH11,X'01'                                         0131
         BNE   @RF00131                                            0131
@RT00131 DS    0H                                                  0132
*               RCODE=0;            /* IS.. NOT DEPENDENT ON CHAN    */
*                                                                  0132
         MVI   RCODE,X'00'                                         0132
*   /*****************************************************************/
*   /*                                                               */
*   /* NO OTHER PATH EXISTS.. SET RCODE 4                            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0133
*             ELSE                  /* ONLY PATH IS ON THIS CHAN     */
*               RCODE=4;            /* AVAILABLE RETURN CODE         */
*                                                                  0133
         B     @RC00131                                            0133
@RF00131 MVI   RCODE,X'04'                                         0133
*   /*****************************************************************/
*   /*                                                               */
*   /* CHANNEL BEING REMOVED WAS NOT ON CPU1.. MUST BE CPU1 THEN..   */
*   /* CHECK IF CHANNEL IS FIRST CHANNEL IN MAP OR SECOND            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0134
*         ELSE                      /* CHANNEL MUST BE ON CPU 1      */
*           IF CHNID=CHN00 THEN     /* FIRST CHANNEL ON THIS CPU     */
*                                                                  0134
         B     @RC00126                                            0134
@RF00126 L     @10,@PC00001                                        0134
         CLC   CHNID(1,@10),CHN00                                  0134
         BNE   @RF00134                                            0134
*             /*******************************************************/
*             /*                                                     */
*             /* FIRST CHANNEL IN MAP.. CHECK IF PATH EXISTS ON ANY  */
*             /* OF THE OTHER THREE POSSABILITIES IF SO SET RCODE = 0*/
*             /*                                                     */
*             /*******************************************************/
*                                                                  0135
*             IF PTH00='01'X×PTH10='01'X/* CHECK IF PATH IS AVAILABLE*/
*                 ×PTH11='01'X THEN /* ON ANY OTHER CHANNEL          */
         CLI   PTH00,X'01'                                         0135
         BE    @RT00135                                            0135
         CLI   PTH10,X'01'                                         0135
         BE    @RT00135                                            0135
         CLI   PTH11,X'01'                                         0135
         BNE   @RF00135                                            0135
@RT00135 DS    0H                                                  0136
*               RCODE=0;            /* PATH DOES NOT NEED THIS CHN   */
*                                                                  0136
         MVI   RCODE,X'00'                                         0136
*   /*****************************************************************/
*   /*                                                               */
*   /* NO OTHER PATHS EXIST SET RCODE = 4                            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0137
*             ELSE                  /* PATH DOES NEED THIS CHANNEL   */
*               RCODE=4;            /* THIS CHANNEL RCODE 4          */
