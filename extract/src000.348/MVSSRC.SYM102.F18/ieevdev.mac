*                                                                  0181
*       IF PTH00='01'X×PTH10='01'X THEN/* ANY PATHS?                 */
         CLI   PTH00,X'01'                                         0181
         BE    @RT00181                                            0181
         CLI   PTH10,X'01'                                         0181
         BNE   @RF00181                                            0181
@RT00181 DS    0H                                                  0182
*         RCODE=0;                  /* SET RETURN CODE ZERO          */
*                                                                  0182
         MVI   RCODE,X'00'                                         0182
