         BNE   @RF00929                                            0929
*     WPLTXT(INDEX+9:INDEX+11)=CHBUF(ENDCHARS-2:ENDCHARS);/* ',X)' 0930
*                                                            @G55AP2L*/
         L     @07,@PC00014                                        0930
         L     @07,INDEX(,@07)                                     0930
         ALR   @09,@07                                             0930
         LH    @07,ENDCHARS                                        0930
         ALR   @08,@07                                             0930
         MVC   WPLTXT+8(3,@09),CHBUF-3(@08)                        0930
*   ELSE                            /* CPU ID NOT SPECIFIED. @G55AP2L*/
*     WPLTXT(INDEX+9:INDEX+9)=CHBUF(ENDCHARS:ENDCHARS);/* ')'      0931
*                                                            @G55AP2L*/
         B     @RC00929                                            0931
@RF00929 L     @09,@PC00014                                        0931
         L     @01,INDEX(,@09)                                     0931
         L     @09,WPLPTR                                          0931
         ALR   @09,@01                                             0931
         LH    @01,ENDCHARS                                        0931
         L     @08,CSCBPTR                                         0931
         ALR   @08,@01                                             0931
         MVC   WPLTXT+8(1,@09),CHBUF-1(@08)                        0931
*   END DYNMSG;                                                    0932
