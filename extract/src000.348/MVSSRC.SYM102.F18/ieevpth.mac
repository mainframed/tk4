*/*                                                                  */
*                                                                  0329
*UCBCKOUT:                                                         0329
*   PROC;                                                          0329
*                                                                  0329
UCBCKOUT STM   @14,@03,@SA00003                                    0329
         STM   @05,@12,@SA00003+24                                 0329
*   /*****************************************************************/
*   /*                                                               */
*   /* PERFORM SERIALIZATION ON OTHER VARY PATH COMMANDS .           */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0330
*   IF VARYENQ^='1'B THEN                                          0330
         TM    VARYENQ,B'00000010'                                 0330
         BO    @RF00330                                            0330
*     DO;                           /* ENQ ONLY IF RESOURCE NOT HELD
*                                                            @G55AP2L*/
*       DO;                         /* ENQ (SYSZVARY,CPU,E,3,SYSTEM) */
*         RESPECIFY                                                0333
*          (GPR01P) RESTRICTED;                                    0333
*         I056(1:16)=I056(1:16)&&I056(1:16);/* ZERO OUT THE TABLE    */
         XC    I056(16),I056                                       0334
*         I05604(1)=3;              /* LENGTH OF MINOR RESOURCE NAME */
         MVI   I05604,X'03'                                        0335
*         I05606(1)=ADDR(SYSZVARY); /* ADDRESS OF MAJOR RESOURCE NAME*/
         LA    @05,SYSZVARY                                        0336
         ST    @05,I05606                                          0336
*         I05607(1)=ADDR(CPU);      /* ADDRESS OF MINOR RESOURCE NAME*/
         LA    @05,CPU                                             0337
         ST    @05,I05607                                          0337
*         I05603(1)='01000000'B;    /* SET BIT STRING 1 FOR RESOURCE
*                                      ELEMENT                       */
         MVI   I05603,B'01000000'                                  0338
*         I05605(1)='01000000'B;    /* SET BIT STRING 2 FOR RESOURCE
*                                      ELEMENT                       */
         MVI   I05605,B'01000000'                                  0339
*         I05603(1,1)='1'B;         /* SET END OF LIST BIT           */
         OI    I05603,B'10000000'                                  0340
*         GPR01P=ADDR(I05602);      /* LOAD LIST ADDRESS             */
         LA    GPR01P,I05602                                       0341
*         SVC(56);                  /* ISSUE ENQ SVC                 */
         SVC   56                                                  0342
*         RESPECIFY                                                0343
*          (GPR01P) UNRESTRICTED;                                  0343
*       END;                        /* ENQ (SYSZVARY,CPU,E,3,SYSTEM) */
*       VARYENQ='1'B;               /* SET FLAG INDICATING ENQ.      */
         OI    VARYENQ,B'00000010'                                 0345
*     END;                          /* END ENQ HELD TEST     @G55AP2L*/
*                                                                  0346
*   /*****************************************************************/
*   /*                                                               */
*   /* WE WILL OBTAIN ACCESS TO THE UCBS VIA ENQ-SHARED ONLY FOR     */
*   /* PLACING A PATH ONLINE (WHERE WE MIGHT SET UCBONLI TO ON). WE  */
*   /* WILL OBTAIN ACCESS TO THE UCB'S VIA ENQ EXCLUSIVE FOR AN      */
*   /* OFFLINE REQUEST, BUT WE WILL NOT LEAVE AN EXCLUSIVE (BUT      */
*   /* UNFULFILLED) REQUEST IN ENQ'S QUEUES FOR MORE THAN FIVE       */
*   /* SECONDS AT A TIME. AFTER 5 SECONDS WE WILL WITHDRAW THE ENQ   */
*   /* REQUESTS (VIA DEQ) THEREBY ALLOWING ANY ENQ-SHARED REQUESTORS */
*   /* (BELOW US IN THE QUEUE) TO GAIN ACCESS.                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0347
*   IF UCBENQ^='1'B THEN                                           0347
@RF00330 TM    UCBENQ,B'00000100'                                  0347
         BO    @RF00347                                            0347
*     DO;                           /* ENQ ONLY IF RESOURCE NOT HELD
*                                                            @G55AP2L*/
*       IF ONLINREQ='1'B THEN       /* ONLINE REQUEST?               */
         TM    ONLINREQ,B'00001000'                                0349
         BNO   @RF00349                                            0349
*         DO;                       /* YES,                          */
*           DO;                     /* ENQ (SYSIEFSD,Q4,S,2,SYSTEM)  */
*             RESPECIFY                                            0352
*              (GPR01P) RESTRICTED;                                0352
*             I056(1:16)=I056(1:16)&&I056(1:16);/* ZERO OUT THE TABLE*/
         XC    I056(16),I056                                       0353
*             I05604(1)=2;          /* LENGTH OF MINOR RESOURCE NAME */
         MVI   I05604,X'02'                                        0354
*             I05606(1)=ADDR(SYSIEFSD);/* ADDRESS OF MAJOR RESOURCE
*                                      NAME                          */
         LA    @05,SYSIEFSD                                        0355
         ST    @05,I05606                                          0355
*             I05607(1)=ADDR(Q4);   /* ADDRESS OF MINOR RESOURCE NAME*/
         LA    @05,Q4                                              0356
         ST    @05,I05607                                          0356
*             I05603(1)='01000000'B;/* SET BIT STRING 1 FOR RESOURCE
*                                      ELEMENT                       */
         MVI   I05603,B'01000000'                                  0357
*             I05605(1)='11000000'B;/* SET BIT STRING 2 FOR RESOURCE
*                                      ELEMENT                       */
         MVI   I05605,B'11000000'                                  0358
*             I05603(1,1)='1'B;     /* SET END OF LIST BIT           */
         OI    I05603,B'10000000'                                  0359
*             GPR01P=ADDR(I05602);  /* LOAD LIST ADDRESS             */
         LA    GPR01P,I05602                                       0360
*             SVC(56);              /* ISSUE ENQ SVC                 */
         SVC   56                                                  0361
*             RESPECIFY                                            0362
*              (GPR01P) UNRESTRICTED;                              0362
*           END;                    /* ENQ (SYSIEFSD,Q4,S,2,SYSTEM)  */
*           UCBENQ='1'B;            /* SET ENQ HELD.         @G55AP2L*/
         OI    UCBENQ,B'00000100'                                  0364
*         END;                                                     0365
*       ELSE                        /* NO, ATTEMPT ENQ EXCLUSIVE.    */
*         DO;                                                      0366
         B     @RC00349                                            0366
@RF00349 DS    0H                                                  0367
*ENQAGAIN:                                                         0367
*           ;                                                      0367
ENQAGAIN DS    0H                                                  0368
*           EUCBECB=''B;            /* ZERO ECB FOR ENQ              */
         XC    EUCBECB(4),EUCBECB                                  0368
*           DO;                     /* ENQ (SYSIEFSD,Q4,E,2,SYSTEM)
*                                      ECB(EUCBECB)                  */
*             RESPECIFY                                            0370
*              (GPR01P) RESTRICTED;                                0370
*             I056(1:16)=I056(1:16)&&I056(1:16);/* ZERO OUT THE TABLE*/
         XC    I056(16),I056                                       0371
*             I05604(1)=2;          /* LENGTH OF MINOR RESOURCE NAME */
         MVI   I05604,X'02'                                        0372
*             I05606(1)=ADDR(SYSIEFSD);/* ADDRESS OF MAJOR RESOURCE
*                                      NAME                          */
         LA    @05,SYSIEFSD                                        0373
         ST    @05,I05606                                          0373
*             I05607(1)=ADDR(Q4);   /* ADDRESS OF MINOR RESOURCE NAME*/
         LA    @05,Q4                                              0374
         ST    @05,I05607                                          0374
*             I05603(1)='01000000'B;/* SET BIT STRING 1 FOR RESOURCE
*                                      ELEMENT                       */
         MVI   I05603,B'01000000'                                  0375
*             I05605(1)='01000100'B;/* SET BIT STRING 2 FOR RESOURCE
*                                      ELEMENT                       */
         MVI   I05605,B'01000100'                                  0376
*             I05601=ADDR(EUCBECB); /* ECB ADDRESS                   */
         LA    @05,EUCBECB                                         0377
         ST    @05,I05601                                          0377
*             I05603(1,1)='1'B;     /* SET END OF LIST BIT           */
         OI    I05603,B'10000000'                                  0378
*             GPR01P=ADDR(I05602);  /* LOAD LIST ADDRESS             */
         LA    GPR01P,I05602                                       0379
*             SVC(56);              /* ISSUE ENQ SVC                 */
         SVC   56                                                  0380
*             RESPECIFY                                            0381
*              (GPR01P) UNRESTRICTED;                              0381
*           END;                    /* ENQ (SYSIEFSD,Q4,E,2,SYSTEM)
*                                      ECB(EUCBECB)                  */
*/*   MUST INDICATE THAT WE HAVE A REQUEST PENDING. WE DO THIS BY    */
*/*   SETTING UCBENQ TO ON. THIS USUALLY MEANS THAT WE HAVE THE      */
*/*   RESOURCE, BUT IN ALL CASES, UCBENQ SET TO ON INDICATES THAT    */
*/*   THE RESOURCE SHOULD BE DEQ'ED (RELEASED) WHEN IEEVPTH FINISHES */
*/*   NORMAL OR ABNORMAL PROCESSING. A DEQ WHEN WE DON'T HAVE THE RE-*/
*/*   SOURCE WILL REMOVE THE REQUEST FROM THE REQUEST QUEUE.         */
*                                                                  0383
*           UCBENQ='1'B;            /* HAVE REQUEST (PENDING) FOR  0383
*                                      UCB'S.                        */
*                                                                  0383
         OI    UCBENQ,B'00000100'                                  0383
*           /*********************************************************/
*           /*                                                       */
*           /* REGISTER 15 CONTAINING ZERO MEANS THAT WE HAVE THE    */
*           /* RESOURCE.                                             */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0384
*           IF REG15^=0 THEN        /* DID WE NOT GET RESOURCE?      */
         LTR   REG15,REG15                                         0384
         BZ    @RF00384                                            0384
*             DO;                   /* YES,                          */
*               GEN( STIMER WAIT,BINTVL=ASLEEP);/* SLEEP FOR 5     0386
*                                      SECONDS.                      */
*                                                                  0386
          STIMER WAIT,BINTVL=ASLEEP
*               /*****************************************************/
*               /*                                                   */
*               /* WE HAVE BEEN AWAKENED.                            */
*               /*                                                   */
*               /*****************************************************/
*                                                                  0387
*               IF POSTBIT='0'B THEN/* STILL DON'T HAVE RESOURCE?    */
         TM    POSTBIT,B'01000000'                                 0387
         BNZ   @RF00387                                            0387
*                 DO;               /* YES,                          */
*                   CALL DEQUCBS;   /* RELEASE UCB'S SO THAT THE   0389
*                                      ENQ-SHARES BELOW US CAN GET 0389
*                                      THEM.                         */
         BAL   @14,DEQUCBS                                         0389
*                   GOTO ENQAGAIN;  /* GET BACK ON ENQ'S QUEUES, NOW
*                                      WE'LL BE BEHIND THE ENQ-SHARES
*                                      WHICH WE WERE FORMERLY AHEAD
*                                      OF.                           */
         B     ENQAGAIN                                            0390
*                 END;                                             0391
*               ELSE                                               0392
*                 ;                 /* HAVE CONTROL OF RESOURCE.     */
@RF00387 DS    0H                                                  0393
*             END;                                                 0393
*           ELSE                                                   0394
*             ;                     /* HAVE CONTROL OF RESOURCE.     */
@RF00384 DS    0H                                                  0395
*         END;                                                     0395
*       RFY                                                        0396
*        (REG6,                                                    0396
*         REG7) RSTD;                                              0396
@RC00349 DS    0H                                                  0397
*     END;                          /* END ENQ HELD TEST     @G55AP2L*/
*   REG6=HEXDEVAD;                  /* UCBLOOK FUNCTION REQUIRES HEX.
*                                      DEVICE @ IN REG. 6            */
@RF00347 L     @05,VRYTPTR                                         0398
         LH    REG6,HEXDEVAD(,@05)                                 0398
*   SAVE13=REG13;                   /* SAVE OUR SAVE AREA PTR.       */
         ST    REG13,SAVE13                                        0399
*   REG13=ADDR(INTSAVE);            /* GIVE IOSGEN ANOTHER SAVE AREA
*                                      TO USE.                       */
*                                                                  0400
         LA    REG13,INTSAVE                                       0400
*   /*****************************************************************/
*   /*                                                               */
*   /* GET UCB ADDRESS                                               */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0401
*   GEN REFS(CVTILK1,CVTILK2,CVTIXAVL,CVTIXAVL,CVTMAP,IOCOMEX,CVTPTR,
*       UCBOB,UCBCHAN,UCBFL5,UCBLCI,IOXHICH,/*               @YM06032*/
*       CVTILCH,LCH,LCHTCH,IOCOM,IOX,IOCUCBBM,IOXCHMSK)(IOSGEN UCBLOOK)
*       ;                           /*                       @YM02128*/
         IOSGEN UCBLOOK
*   REG13=SAVE13;                   /* RESTORE POINTER TO OUR SAVE 0402
*                                      AREA                          */
         L     REG13,SAVE13                                        0402
*   IF REG15^=0 THEN                /* REG. 15 WILL BE ZERO WHEN THE
*                                      PATH IS VALID.                */
         LTR   REG15,REG15                                         0403
         BZ    @RF00403                                            0403
*     VALIDPTH='0'B;                /* INDICATE PATH NOT VALID       */
         NI    VALIDPTH,B'11111110'                                0404
*   IF VALIDPTH='1'B THEN           /* CONTINUE IF THE PATH WAS    0405
*                                      VALID.                        */
@RF00403 TM    VALIDPTH,B'00000001'                                0405
         BNO   @RF00405                                            0405
*     DO;                                                          0406
*       UCBADDR=REG7;               /* GET ADDRESS OF UCB'S BASE.    */
*                                                                  0407
         ST    REG7,UCBADDR                                        0407
*       /*************************************************************/
*       /*                                                           */
*       /* UCBPTR WILL POINT TO THE UCB BASE (COMMON SECTION). ALL   */
*       /* DISPLACEMENTS ARE FROM THE START OF THE UCB PREFIX BUT THE*/
*       /* ADDRESS OF THE PREFIX IS BASED ON UCBPTR-512 (512 IS THE  */
*       /* LENGTH OF THE PREFIX).                                    */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0408
*       UCBPTR=REG7;                /* IOSGEN RETURNS THE BASE UCB @
*                                      IN REG. 7. NOW HAVE ADDRESS OF
*                                      BASE.                         */
         LR    UCBPTR,REG7                                         0408
*       RFY                                                        0409
*        (REG6,                                                    0409
*         REG7) UNRSTD;                                            0409
*                                                                  0409
*       /*************************************************************/
*       /*                                                           */
*       /* IF WE ARE LOOKING AT A 2305'S UCB IT MUST BE THE BASE'S,  */
*       /* OR ELSE THE PATH ENTERED IS NOT VALID. THE UCBLOOK        */
*       /* FUNCTION WILL HAVE POINTED US TO AN EXPOSURE'S UCB IN THAT*/
*       /* CASE.                                                     */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0410
*       IF(UCBMTPXP='1'B)&          /* ARE WE ON A 2305'S UCB, AND   */
*           (UCBBASE^=(UCBPTR-512)) THEN/* IS IT NOT THE BASE?       */
         TM    UCBMTPXP(UCBPTR),B'00001000'                        0410
         BNO   @RF00410                                            0410
         LR    @05,UCBPTR                                          0410
         SH    @05,@CH01220                                        0410
         C     @05,UCBBASE(,UCBPTR)                                0410
         BE    @RF00410                                            0410
*         DO;                       /* YES, PATH NOT VALID.          */
*           VALIDPTH='0'B;          /* SET INVALID PATH BIT.         */
         NI    VALIDPTH,B'11111110'                                0412
*           GOTO FIN;               /* NOTHING ELSE TO DETERMINE.    */
         B     FIN                                                 0413
*         END;                                                     0414
*       EBCDEVAD=UCBNAME;           /* PUT BASE EBCDIC NAME IN THE 0415
*                                      TABLE FOR IOSGEN VARY       0415
*                                      FUNCTION.                     */
@RF00410 L     @05,VRYTPTR                                         0415
         MVC   EBCDEVAD(3,@05),UCBNAME(UCBPTR)                     0415
*       DEVCLASS=UCBTBYT3;          /* GET DEVICE CLASS.             */
         MVC   DEVCLASS(1),UCBTBYT3(UCBPTR)                        0416
*       DEVTYPE=UCBTBYT4;           /* GET UNIT TYPE.                */
*                                                                  0417
         MVC   DEVTYPE(1),UCBTBYT4(UCBPTR)                         0417
*       /*************************************************************/
*       /*                                                           */
*       /* IF THE REQUEST IS AN OFFLINE REQUEST, DETERMINE IF THE    */
*       /* LAST PATH CAN BE VARY'ED OFFLINE. WE WON'T ALLOW IT TO BE */
*       /* VARY'ED IF THE DEVICE IS A CONSOLE OR IT'S BEING USED.    */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0418
*       IF ONLINREQ='0'B THEN       /* IS IT AN OFFLINE REQUEST?     */
         TM    ONLINREQ,B'00001000'                                0418
         BNZ   @RF00418                                            0418
*         DO;                       /* YES, OFFLINE REQUEST.         */
*           IF((UCBONLI='0'B)&(UCBNALOC='1'B))×/* DEVICE OFFLINE & IN
*                                      USE, OR                       */
*               ((UCBONLI='1'B)&(UCBSYSR='1'B))×/* ONLINE AND A    0420
*                                      CONSOLE, OR                   */
*               ((UCBONLI='1'B)&(UCBALOC='1'B))/* ONLINE AND IN USE? */
*             THEN                  /* YES,                          */
         TM    UCBONLI(UCBPTR),B'10000000'                         0420
         BNZ   @GL00016                                            0420
         TM    UCBNALOC(UCBPTR),B'00000100'                        0420
         BO    @RT00420                                            0420
@GL00016 TM    UCBONLI(UCBPTR),B'10000010'                         0420
         BO    @RT00420                                            0420
         TM    UCBONLI(UCBPTR),B'10001000'                         0420
         BNO   @RF00420                                            0420
@RT00420 DS    0H                                                  0421
*             UNCONREQ='0'B;        /* WON'T VARY LAST PATH.         */
*                                                                  0421
         NI    UNCONREQ,B'11111011'                                0421
*           /*********************************************************/
*           /*                                                       */
*           /* WE WILL VARY THE LAST PATH TO AN OFFLINE DEVICE WHICH */
*           /* ISN'T IN USE.                                         */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0422
*           ELSE                    /* NONE OF THE ABOVE...          */
*             IF((UCBONLI='0'B)&(UCBNALOC='0'B)) THEN/* DEVICE     0422
*                                      OFFLINE, NOT IN USE?          */
         B     @RC00420                                            0422
@RF00420 TM    UCBONLI(UCBPTR),B'10000000'                         0422
         BNZ   @RF00422                                            0422
         TM    UCBNALOC(UCBPTR),B'00000100'                        0422
         BNZ   @RF00422                                            0422
*               UNCONREQ='1'B;      /* YES, SET INDICATOR.           */
         OI    UNCONREQ,B'00000100'                                0423
*         END;                      /* FINISHED DETERMINATIONS FOR 0424
*                                      UNCONDITIONAL FLAG FOR OFFLINE
*                                      REQUESTS.                     */
*     END;                          /* FINISHED PROCESSING FOR VALID
*                                      PATHS.                        */
*FIN:                                                              0426
*   END UCBCKOUT;                                                  0426
@EL00003 DS    0H                                                  0426
@EF00003 DS    0H                                                  0426
@ER00003 LM    @14,@03,@SA00003                                    0426
         LM    @05,@12,@SA00003+24                                 0426
         BR    @14                                                 0426
*                                                                  0427
*/* VARYOFF - CALL IOSGEN TO CONDITIONALLY VARY PATH OFFLINE.        */
*/*           CHECK THE RETURN CODES FROM THE IOSGEN VARY FUNCTION.  */
*/*           DETERMINE WHAT MESSAGES SHOULD BE ISSUED, SET BITS     */
*/*           IF NECESSARY IN THE UCB, AND WRITE AN SMF              */
*/*           RECORD IF NECESSARY. IF BAD ERROR EXIT THRU ESTAE RTN. */
*/*                                                                  */
*/*    INPUT-                                                        */
*/*          UCBONLI- ON, UCB (DEVICE) IS ON-LINE.                   */
*/*          UCBVHRSN- ON, HIERARCHY NOT AVAILABLE; NO PATH EXISTS TO*/
*/*                      THE DEVICE.                                 */
*/*                                                                  */
*/*    OUTPUT-                                                       */
*/*           PTHVARED- ON, ISSUE 'PATH VARYED' MESSAGE.             */
*/*                          OR                                      */
*/*           LASTPATH- ON, ISSUE 'LAST PATH- CAN'T VARY' MESSAGE.   */
*/*                          OR                                      */
*/*           VALIDPTH- OFF, ISSUE 'PATH DOES NOT EXIST ' MESSAGE.   */
*/*                          OR                                      */
*/*           RESERPTH- ON, ISSUE 'PATH RESERVED-CAN'T VARY MESSAGE. */
*/*                                                                  */
*                                                                  0427
*VARYOFF:                                                          0427
*   PROC;                                                          0427
*                                                                  0427
VARYOFF  STM   @14,@12,@SA00004                                    0427
*   /*****************************************************************/
*   /*                                                               */
*   /* SET UP CALL TO IOSGEN VARY. CALL IOSGEN TO CONDITIONALLY VARY */
*   /* PATH TO DEVICE OFFLINE                                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0428
*   ONOFFIND='00'X;                 /* SET FUNCTION CODE             */
         L     @05,VRYTPTR                                         0428
         MVI   ONOFFIND(@05),X'00'                                 0428
*   CALL IOSGENV;                   /* CALL VARY SUBROUTINE          */
*                                                                  0429
         BAL   @14,IOSGENV                                         0429
*   /*****************************************************************/
*   /*                                                               */
*   /* RETURN CODE OF ZERO INDICATES THAT THE PATH WAS VARIED, AND IT*/
*   /* WASN'T A LAST PATH. THE ONLY PROCESSING WHICH WILL BE         */
*   /* PERFORMED AFTER HERE WILL BE TO ISSUE THE PATH VARIED MESSAGE.*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0430
*   IF RETCODE=0 THEN               /* RETURN CODE ZERO?             */
         LTR   RETCODE,RETCODE                                     0430
         BNZ   @RF00430                                            0430
*     DO;                           /* YES,                          */
*       PTHVARED='1'B;              /* INDICATE PATH VARY'ED.        */
         OI    PTHVARED,B'10000000'                                0432
*     END;                                                         0433
*                                                                  0433
*   /*****************************************************************/
*   /*                                                               */
*   /* RETURN CODE OF NEITHER ZERO NOR EIGHT INDICATES THAT THE PATH */
*   /* WAS NOT VARY'ED DUE TO AN ERROR CONDITION DETECTED BY THE     */
*   /* IOSGEN VARY FUNCTION.. WE WILL NOT. SET ANY BITS IN THE UCB OR*/
*   /* WRITE ANY SMF RECORDS.                                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0434
*   IF(RETCODE^=0)&(RETCODE^=8) THEN/* IS RETURN CODE NOT ZERO, AND
*                                      NOT EIGHT?                    */
@RF00430 LTR   RETCODE,RETCODE                                     0434
         BZ    @RF00434                                            0434
         CH    RETCODE,@CH00082                                    0434
         BE    @RF00434                                            0434
*     DO;                           /* YES, CONTINUE.                */
*                                                                  0435
*       /*************************************************************/
*       /*                                                           */
*       /* A RETURN CODE OF TWELVE INDICATES THAT THE IOSGEN VARY    */
*       /* FUNCTION WAS PASSED A PATH WHICH DOES NOT EXIST. ( CAN    */
*       /* ONLY COME FROM AN INVALID CPU ADDRESS SINCE WE PASS HIM A */
*       /* VALID DEVICE ADDRESS- WE USE UCBLOOK ( AN IOS FUNCTION) TO*/
*       /* ENSURE THAT THE DEVICE ADDRESS IS VALID.                  */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0436
*       IF RETCODE=12 THEN          /* WAS RETURN CODE 12?           */
         CH    RETCODE,@CH00034                                    0436
         BNE   @RF00436                                            0436
*         VALIDPTH='0'B;            /* SET INDICATOR.                */
*                                                                  0437
         NI    VALIDPTH,B'11111110'                                0437
*       /*************************************************************/
*       /*                                                           */
*       /* A RETURN CODE OF TWENTY INDICATES THAT THE PATH REPRESENTS*/
*       /* A PATH TO A SHARED DASD DEVICE, AND IS THEREFORE RESERVED.*/
*       /* IT WAS NOT VARY'ED OFFLINE.                               */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0438
*       ELSE                        /* RETURN CODE NOT 12.           */
*         IF RETCODE=20 THEN        /* IS RETURN CODE 20?            */
         B     @RC00436                                            0438
@RF00436 CH    RETCODE,@CH02923                                    0438
         BNE   @RF00438                                            0438
*           RESERPTH='1'B;          /* YES, SET INDICATOR.           */
*                                                                  0439
         OI    RESERPTH,B'00100000'                                0439
*       /*************************************************************/
*       /*                                                           */
*       /* IF NOT 12 OR 20 RETURN CODE THEN ABEND                    */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0440
*         ELSE                                                     0440
*           DO;                     /* ABEND(183) SYSTEM             */
         B     @RC00438                                            0440
@RF00438 DS    0H                                                  0441
*             RESPECIFY                                            0441
*              (GPR01F) RESTRICTED;                                0441
*             GPR01F=((183)&'00000FFF'X)*4096;/* COMP CODE IN BITS 0442
*                                      8-19                          */
         L     GPR01F,@CF03248                                     0442
*             SVC(13);              /* ISSUE ABEND SVC               */
         SVC   13                                                  0443
*             RESPECIFY                                            0444
*              (GPR01F) UNRESTRICTED;                              0444
*           END;                    /* ABEND(183) SYSTEM EXIT THRU 0445
*                                      ESTAE RTN,CODE= 0B7   @YM03534*/
*     END;                          /* HANDLED RETURN CODES: 0,4,12,
*                                      16 AND 20.                    */
*                                                                  0446
@RC00438 DS    0H                                                  0446
@RC00436 DS    0H                                                  0447
*   /*****************************************************************/
*   /*                                                               */
*   /* RETURN CODE 8 MEANS THAT THE PATH TO BE VARIED IS A LAST PATH */
*   /* TO THE DEVICE. WE WILL INVOKE IOSGEN TO VARY IT ONLY IF WE'VE */
*   /* PREVIOUSLY DETERMINED IT SHOULD BE UNCONDITIONAL, AND THEN    */
*   /* AFTER WE DO ANY NECESSARY DEVICE-OFFLINE PROCESSING REWIND    */
*   /* TAPE, ETC. ).                                                 */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0447
*   IF RETCODE=8 THEN               /* WAS IT AN ATTEMPT TO VARY A 0447
*                                      LAST PATH?                    */
@RF00434 CH    RETCODE,@CH00082                                    0447
         BNE   @RF00447                                            0447
*     DO;                           /* YES,                          */
*       IF UNCONREQ='0'B THEN       /* REQUEST NOT TO BE           0449
*                                      UNCONDITIONAL ?               */
         TM    UNCONREQ,B'00000100'                                0449
         BNZ   @RF00449                                            0449
*         DO;                       /* YES, CONDITIONAL .            */
*           LASTPATH='1'B;          /* INDICATE TO ISSUE 'INVALID  0451
*                                      ATTEMPT' MESSAGE.             */
         OI    LASTPATH,B'01000000'                                0451
*         END;                                                     0452
*       ELSE                        /* REQUEST IS UNCONDITIONAL.     */
*         DO;                                                      0453
         B     @RC00449                                            0453
@RF00449 DS    0H                                                  0454
*           IF UCBONLI='1'B THEN    /* DEVICE ONLINE?                */
         TM    UCBONLI(UCBPTR),B'10000000'                         0454
         BNO   @RF00454                                            0454
*             DO;                   /* YES,                          */
*                                                                  0455
*               /*****************************************************/
*               /*                                                   */
*               /* CHECK IF DEVICE NEEDS TO BE UNLOADED              */
*               /*                                                   */
*               /*****************************************************/
*                                                                  0456
*               IF(UCB3DACC='1'B)×  /* DEVICE DIRECT-ACCESS, OR      */
*                   (UCB3TAPE='1'B) THEN/* TAPE?                     */
         TM    UCB3DACC(UCBPTR),B'10100000'                        0456
         BZ    @RF00456                                            0456
*                 DO;               /* YES,                          */
*                   VMVREQBP=ADDR(ALOCBLOK);/* GET ADDRESSABILITY TO
*                                      CONTROL BLOCK.                */
         LA    @05,ALOCBLOK                                        0458
         ST    @05,VMVREQBP                                        0458
*                   ALOCBLOK=ALOCBLOK&&ALOCBLOK;/* INITIALIZE CONTROL
*                                      BLOCK.                        */
         XC    ALOCBLOK(52),ALOCBLOK                               0459
*                   VMVUNLOD='1'B;  /* INDICATE TO UNLOAD DEVICE.    */
         OI    VMVUNLOD(@05),B'10000000'                           0460
*                   VMVUCBP=UCBADDR;/* GET @ OF DEVICE'S UCB.        */
         MVC   VMVUCBP(3,@05),UCBADDR+1                            0461
*                   VMVUCB='1'B;    /* FIND DEVICE WITH UCB ADDRESS. */
*                                                                  0462
         OI    VMVUCB(@05),B'01000000'                             0462
*                   /*************************************************/
*                   /*                                               */
*                   /* CALL ALLOCATION UNLOAD ROUTINE                */
*                   /*                                               */
*                   /*************************************************/
*                                                                  0463
*                   DO;             /* LINK EP('IEFAB49C')         0463
*                                      PARAM(VMVREQBP,0,0)           */
*                     I00601='00'X; /* EP/DE FLAG                    */
         MVI   I00601,X'00'                                        0464
*                     I00602=ADDR('IEFAB49C');/* ADDR OF EP/DE       */
         LA    @05,@CC03149                                        0465
         STCM  @05,7,I00602                                        0465
*                     I00604=0;     /* DCB PTR                       */
         SLR   @05,@05                                             0466
         STCM  @05,7,I00604                                        0466
*                     I00603='00'X; /* ERRET FLAG                    */
         MVI   I00603,X'00'                                        0467
*                     IOPTLIST(1)=ADDR(VMVREQBP);/* ADDR OF PROB PROG
*                                      PARM                          */
         LA    @05,VMVREQBP                                        0468
         ST    @05,IOPTLIST                                        0468
*                     IOPTLIST(2)=ADDR(0);/* ADDR OF PROB PROG PARM  */
         LA    @05,@CF00101                                        0469
         ST    @05,IOPTLIST+4                                      0469
*                     IOPTLIST(3)=ADDR(0);/* ADDR OF PROB PROG PARM  */
         ST    @05,IOPTLIST+8                                      0470
*                     IOPTLIST(3)=IOPTLIST(3)×'80000000'X;/* SET VL
*                                      BIT                           */
         O     @05,@CF03153                                        0471
         ST    @05,IOPTLIST+8                                      0471
*                     RESPECIFY                                    0472
*                      (GPR01P) RESTRICTED;                        0472
*                     GPR01P=ADDR(IOPTLIST);/* ADDR OF OPTLIST       */
         LA    GPR01P,IOPTLIST                                     0473
*                     RESPECIFY                                    0474
*                      (GPR15P) RESTRICTED;                        0474
*                     GPR15P=ADDR(I006);/* ADDR OF LINK LIST         */
         LA    GPR15P,I006                                         0475
*                     SVC(6);       /* ISSUE LINK SVC                */
         SVC   6                                                   0476
*                     RESPECIFY                                    0477
*                      (GPR01P) UNRESTRICTED;                      0477
*                     RESPECIFY                                    0478
*                      (GPR15P) UNRESTRICTED;                      0478
*                   END;            /* LINK EP('IEFAB49C')         0479
*                                      PARAM(VMVREQBP,0,0)           */
*                 END;              /* END PROC TAPE OR DISK         */
*                                                                  0480
*               /*****************************************************/
*               /*                                                   */
*               /* CHECK IF DEVICE HAS ERROR BUFFER WHICH MUST BE    */
*               /* RECORDED                                          */
*               /*                                                   */
*               /*****************************************************/
*                                                                  0481
*               ELSE                                               0481
*                 IF UCBERLOG='1'B THEN/* ERROR BUFFER?      @Z40MPXA*/
         B     @RC00456                                            0481
@RF00456 L     @05,UCBEXTPT(,UCBPTR)                               0481
         TM    UCBERLOG(@05),B'00000100'                           0481
         BNO   @RF00481                                            0481
*                   DO;             /*                       @Z40MPXA*/
*                     RFY                                          0483
*                      (REG0,                                      0483
*                       REG1) RSTD; /* RESTRICT REG 0        @Z40MPXA*/
*                     REG0=-UCBADDR;/* UCB ADDR IN COMPLEMENT FORM 0484
*                                                            @Z40MPXA*/
         L     REG0,UCBADDR                                        0484
         LCR   REG0,REG0                                           0484
*                     REG1=0;       /* ZERO REG1             @Y30MPXG*/
         SLR   REG1,REG1                                           0485
*                     SVC(91);      /* RECORD BUFFER         @Z40MPXA*/
         SVC   91                                                  0486
*                     RFY                                          0487
*                      (REG0,                                      0487
*                       REG1) UNRSTD;/* RELEASE REG 0        @Z40MPXA*/
*                   END;            /*                       @Z40MPXA*/
*               UCBONLI='0'B;       /* MARK UCB OFFLINE              */
@RF00481 DS    0H                                                  0489
@RC00456 NI    UCBONLI(UCBPTR),B'01111111'                         0489
*               DVCEOFF='1'B;       /* SET FLAG FOR DEVICE MSG       */
         OI    DVCEOFF,B'01000000'                                 0490
*               CALL SMF(11);       /* WRITE DEVICE OFFLINE SMF RCRD */
         LA    @01,@AL00491                                        0491
         BAL   @14,SMF                                             0491
*               UCBVHRSN='1'B;      /* HIERARCHY UNAVAILABLE         */
         OI    UCBVHRSN(UCBPTR),B'00000010'                        0492
*               UCBRESV='0'B;       /* CAN NO LONGER HAVE RESERVE.   */
*               UCBCHGS='0'B;       /* UCB NO LONGER CHANGING STATUS.*/
         NI    UCBRESV(UCBPTR),B'10011111'                         0494
*             END;                  /* END ONLINE DEVICE PROC        */
*                                                                  0495
*           /*********************************************************/
*           /*                                                       */
*           /* CALL IOSGEN VARY FUNCTION TO UNCONDITIONALLY VARY LAST*/
*           /* PATH OFFLINE.                                         */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0496
*           ONOFFIND='80'X;         /* SET FUNCTION CODE             */
@RF00454 L     @05,VRYTPTR                                         0496
         MVI   ONOFFIND(@05),X'80'                                 0496
*           CALL IOSGENV;                                          0497
         BAL   @14,IOSGENV                                         0497
*           IF RETCODE=4 THEN       /* WAS THE LAST PATH VARY'ED?    */
         CH    RETCODE,@CH00124                                    0498
         BNE   @RF00498                                            0498
*             DO;                   /* YES,                          */
*               PTHVARED='1'B;      /* INDICATE THAT PATH WAS VARIED.*/
         OI    PTHVARED,B'10000000'                                0500
*             END;                                                 0501
*           ELSE                    /* NO, ABEND CALLER              */
*             DO;                   /* ABEND(183) SYSTEM             */
         B     @RC00498                                            0502
@RF00498 DS    0H                                                  0503
*               RESPECIFY                                          0503
*                (GPR01F) RESTRICTED;                              0503
*               GPR01F=((183)&'00000FFF'X)*4096;/* COMP CODE IN BITS
*                                      8-19                          */
         L     GPR01F,@CF03248                                     0504
*               SVC(13);            /* ISSUE ABEND SVC               */
         SVC   13                                                  0505
*               RESPECIFY                                          0506
*                (GPR01F) UNRESTRICTED;                            0506
*             END;                  /* ABEND(183) SYSTEM EXIT THRU 0507
*                                      ESTAE RTN ABEND CODE = 0B7  0507
*                                                            @YM03534*/
*         END;                                                     0508
*     END;                                                         0509
*   END VARYOFF;                                                   0510
@EL00004 DS    0H                                                  0510
@EF00004 DS    0H                                                  0510
@ER00004 LM    @14,@12,@SA00004                                    0510
         BR    @14                                                 0510
*                                                                  0511
*/*  POSCONCK- DETERMINE IF DEVICE WAS SYSGEN'ED AS A CONSOLE.       */
*/*    INPUT-                                                        */
*/*           UCMVEA- ADDRESS OF FIRST UCME ENTRY.                   */
*/*           UCMVEZ- LENGTH OF EACH UCME.                           */
*/*           UCMVEL- ADDRESS OF LAST UCME.                          */
*/*    OUTPUT-                                                       */
*/*            SGCONSLE- ON, DEVICE IS A SYSGEN'ED CONSOLE.          */
*/*                                                                  */
*/* SEARCH THROUGH THE UCME'S SERIALLY, LOOKING FOR ONE WHOSE UCMUCB */
*/* FIELD POINTS TO THE SAME UCB AS OUR DEVICE. IF WE FIND SUCH A    */
*/* UCME, THE DEVICE IS A SYGEN'ED CONSOLE; IF WE EXHAUST THE        */
*/* UCME'S WITHOUT MATCHING THE UCB ADDRESSES, THEN THE DEVICE       */
*/* WASN'T SYSGEN'ED AS A CONSOLE.                                   */
*/*                                                                  */
*                                                                  0511
*POSCONCK:                                                         0511
*   PROC OPTIONS(NOSAVEAREA);                                      0511
POSCONCK STM   @14,@12,12(@13)                                     0511
*   UCMDVEPT=UCMVEA;                /* POINT TO FIRST UCME.          */
         L     UCMDVEPT,UCMVEA(,UCMBASPT)                          0512
*   DO WHILE((UCMDVEPT<=UCMVEL)&(SGCONSLE='0'B));/* SEARCH THROUGH 0513
*                                      THE UCME'S UP TO AND INCLUDING
*                                      THE LAST STOP THE SEARCH WHEN
*                                      WE'VE FOUND THAT THE DEVICE IS
*                                      A SYSGEN'ED CONSOLE OR WHEN 0513
*                                      WE'VE REACHED THE END OF THE
*                                      UCME'S.                       */
         B     @DE00513                                            0513
@DL00513 DS    0H                                                  0514
*     IF UCMUCB=UCBADDR THEN        /* DO THE UCB ADDRESSES MATCH?   */
         CLC   UCMUCB(4,UCMDVEPT),UCBADDR                          0514
         BNE   @RF00514                                            0514
*       SGCONSLE='1'B;              /* YES, SET INDICATOR.           */
         OI    SGCONSLE,B'01000000'                                0515
*     UCMDVEPT=UCMDVEPT+UCMVEZ;     /* POINT TO NEXT UCME.           */
@RF00514 AL    UCMDVEPT,UCMVEZ(,UCMBASPT)                          0516
*   END;                            /* FINISHED SEARCH.              */
@DE00513 C     UCMDVEPT,UCMVEL(,UCMBASPT)                          0517
         BH    @DC00513                                            0517
         TM    SGCONSLE,B'01000000'                                0517
         BZ    @DL00513                                            0517
@DC00513 DS    0H                                                  0518
*   END POSCONCK;                                                  0518
@EL00005 DS    0H                                                  0518
@EF00005 DS    0H                                                  0518
@ER00005 LM    @14,@12,12(@13)                                     0518
         BR    @14                                                 0518
*                                                                  0519
*/*                                                                  */
*/*  VARYON-CALL IOSGEN VARY TO VARY PATH ONLINE.                    */
*/*         CHECK THE RETURN CODES FROM THE IOSGEN VARY              */
*/*         FUNCTION TO DETERMINE WHICH OF THE FOLLOWING ACTIONS     */
*/*         SHOULD BE TAKEN:                                         */
*/*                                                                  */
*/*          RETURN CODE ZERO- ATTEMPT TO BRING THE DEVICE ONLINE,   */
*/*          AFTER FIRST ENSURING THAT IT IS OPERATIONAL ( IF IT IS  */
*/*          NOT OPERATIONAL, WE SET A BIT TO INDICATE THAT AN       */
*/*          APPROPRIATE MESSAGE SHOULD BE ISSUED.). WHEN THE DEVICE */
*/*          IS BROUGHT ONLINE, THE UCB HIERARCHY BIT IS TURNED OFF, */
*/*          THE ONLINE BIT IS TURNED ON, AND AN SMF RECORD IS       */
*/*          WRITTEN TO RECORD THESE FACTS.                          */
*/*          RETURN CODE 12- PATH DOES NOT EXIST: SET BIT TO INDI-   */
*/*          CATE THAT MESSAGE SHOULD BE WRITTEN.                    */
*/*          ALL OTHER RETURN CODES- IMPLY AN ERROR IN THE           */
*/*          PROCESSING OF AN ONLINE REQUEST. ABEND,                 */
*/*          ROUTINE WILL ISSUE AN ERROR MESSAGE.                    */
*/*   INPUT-                                                         */
*/*         UCBONLI- OFF, DEVICE IS OFFLINE.                         */
*/*         UCBNALOC-ON, DEVICE IS IN USE.                           */
*/*         UCBVORSN- ON, DEVICE IS OFFLINE BECAUSE OF A SPECIFIC    */
*/*                   OPERATOR REQUEST.                              */
*/*                                                                  */
*/*   OUTPUT-                                                        */
*/*          PTHVARED- ON,INDICATE TO ISSUE PATH VARY'ED MESSAGE.    */
*/*          VALIDPTH- OFF, GIVEN PATH TO DEVICE DOESN'T EXIST,      */
*/*                    HAVE CLEAN-UP ROUTINE ISSUE MESSAGE.          */
*/*          UCBVHRSN- OFF, (OPERATIONAL) PATH TO DEVICE EXISTS      */
*/*                    (HIERARCHY AVAILABLE.                         */
*/*          UCBONLI- ON, DEVICE ONLINE.                             */
*/*          UCBMRKED- ON, UCBONLI AND UCBVHRSN BITS SET.            */
*/*          PTHNOTOP- ON, PATH BROUGHT ONLINE BUT IS NOT            */
*/*                    OPERATIONAL; CLEAN-UP ROUTINE WILL ISSUE MSG. */
*/*                                                                  */
*                                                                  0519
*VARYON:                                                           0519
*   PROC;                                                          0519
*                                                                  0519
VARYON   STM   @14,@12,@SA00006                                    0519
*   /*****************************************************************/
*   /*                                                               */
*   /* DETERMINE IF REQUESTED PATH CAN BE VERIFIED                   */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0520
*   IF UCB3COMM='1'B&UCBACTV='1'B THEN/* ACTIVE TP DEVICE?   @ZA00826*/
         TM    UCB3COMM(UCBPTR),B'01000000'                        0520
         BNO   @RF00520                                            0520
         TM    UCBACTV(UCBPTR),B'00000010'                         0520
         BNO   @RF00520                                            0520
*     DO;                                                          0521
*                                                                  0521
*       /*************************************************************/
*       /*                                                           */
*       /* CHECK IF PATH IS ALREADY ONLINE, IE. IF I/O IS ON PATH TO */
*       /* BE VARIED, THEN PUT OUT ONLINE MESSAGE, ELSE BRING PATH   */
*       /* ONLINE, AND INFORM OPERATOR THAT PATH WAS NOT VERIFIED    */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0522
*       IF UCBCPU=LOGCPUID&UCBCHAN=HEXDEVAD THEN/* I/O FROM PATH     */
         CLC   UCBCPU(1,UCBPTR),LOGCPUID                           0522
         BNE   @RF00522                                            0522
         SLR   @05,@05                                             0522
         ICM   @05,3,UCBCHAN(UCBPTR)                               0522
         L     @01,VRYTPTR                                         0522
         CH    @05,HEXDEVAD(,@01)                                  0522
         BNE   @RF00522                                            0522
*         DO;                                                      0523
*           PTHVARED='1'B;          /* INDICATE PATH ONLINE  @ZA00826*/
         OI    PTHVARED,B'10000000'                                0524
*           GOTO ONEXIT;            /* EXIT TO CLEANUP       @ZA00826*/
         B     ONEXIT                                              0525
*         END;                                                     0526
*       ELSE                                                       0527
*         DO;                                                      0527
@RF00522 DS    0H                                                  0528
*           NOTVER='1'B;            /* INDICATE PATH NOT VERIFIED  0528
*                                                            @ZA00826*/
         OI    NOTVER,B'00001000'                                  0528
*           GOTO PTHVARY;           /*                       @ZA00826*/
         B     PTHVARY                                             0529
*         END;                                                     0530
*     END;                                                         0531
*                                                                  0531
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK IF REQUESTED PATH IS OPERATIONAL                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0532
*   IOPMFC=2;                       /* VARY PATH FUNCTION CODE     0532
*                                                            @ZA00826*/
@RF00520 MVI   IOPMFC,X'02'                                        0532
*   IOPMUCB=UCBPTR;                 /* UCB ADDRESS           @ZA00826*/
         STCM  UCBPTR,7,IOPMUCB                                    0533
*   IOPMCPU=CSDMASK(LOGCPUID+1);    /* CPU ADDRESS           @ZA00826*/
         SLR   @05,@05                                             0534
         IC    @05,LOGCPUID                                        0534
         ALR   @05,@05                                             0534
         L     @01,CVTPTR                                          0534
         L     @01,CVTCSD(,@01)                                    0534
         ALR   @01,@05                                             0534
         MVC   IOPMCPU(2),CSDMASK(@01)                             0534
*   IOPMCHN=HEXCHNAD;               /* CHANNEL ADDRESS       @ZA00826*/
*                                                                  0535
         L     @05,VRYTPTR                                         0535
         SLR   @09,@09                                             0535
         IC    @09,HEXCHNAD(,@05)                                  0535
         STH   @09,IOPMCHN                                         0535
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK IF PATHS HAVE BEEN VALIDATED BEFORE                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0536
*   IF UCBVALPH='1'B THEN           /* PATHS BEEN VALIDATED? @ZA00826*/
         TM    UCBVALPH(UCBPTR),B'00000010'                        0536
         BNO   @RF00536                                            0536
*     DO;                                                          0537
*       REG1=ADDR(IOPMPARM);        /* POINT TO PARMS        @ZA00826*/
         LA    REG1,IOPMPARM                                       0538
*       GEN(LINK EP=IECVIOPM);      /* LINK TO IOPM          @ZA00826*/
         LINK EP=IECVIOPM
*     END;                                                         0540
*                                                                  0540
*   /*****************************************************************/
*   /*                                                               */
*   /* MAKE LINK TO VERIFY THAT REQUESTED PATH IS OPERATIONAL        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0541
*   REG15=20;                       /* INIT FOR RESERVED PATH RETRY
*                                                            @ZA20196*/
@RF00536 LA    REG15,20                                            0541
*   RETRYCTR=RETRY;                 /* INIT COUNTER FROM STATIC RETRY
*                                      COUNT                 @ZA20196*/
         LA    RETRYCTR,5                                          0542
*   DO WHILE RETRYCTR^=0&REG15=20;  /* RETRY 5 TIMES IF PATH RESERVED
*                                      CONDITION             @ZA20196*/
         B     @DE00543                                            0543
@DL00543 DS    0H                                                  0544
*     REG1=ADDR(IOPMPARM);          /* ADDRESS OF PARMS      @ZA00826*/
         LA    REG1,IOPMPARM                                       0544
*     REG15=IOPMADDR;               /* GET IECVIOPM ENTRY ADDRESS  0545
*                                                            @ZA20196*/
         L     REG15,IOPMADDR                                      0545
*     GEN( BALR REG14,REG15);       /* BRANCH TO IECVIOPM    @ZA20196*/
          BALR REG14,REG15
*     RETRYCTR=RETRYCTR-1;          /* DECREMENT RETRY COUNT BY ONE
*                                                            @ZA20196*/
         BCTR  RETRYCTR,0                                          0547
*   END;                            /* END BRANCH TO TEST PATH     0548
*                                                            @ZA20196*/
*                                                                  0548
@DE00543 LTR   RETRYCTR,RETRYCTR                                   0548
         BZ    @DC00543                                            0548
         CH    REG15,@CH02923                                      0548
         BE    @DL00543                                            0548
@DC00543 DS    0H                                                  0549
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK IF PATH IS OPERATIONAL                                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0549
*   IF REG15^=0 THEN                /* IS PATH OPERATIONAL?  @ZA00826*/
         LTR   REG15,REG15                                         0549
         BZ    @RF00549                                            0549
*     IF REG15=20 THEN                                             0550
         CH    REG15,@CH02923                                      0550
         BNE   @RF00550                                            0550
*       RESERPTH='1'B;              /* SET PATH RESERVED     @ZA20196*/
         OI    RESERPTH,B'00100000'                                0551
*     ELSE                                                         0552
*       PTHNOTOP='1'B;              /* SET NOT OPERATIONAL FLAG    0552
*                                                            @ZA20196*/
         B     @RC00550                                            0552
@RF00550 OI    PTHNOTOP,B'00010000'                                0552
*   ELSE                                                           0553
*     DO;                                                          0553
*                                                                  0553
         B     @RC00549                                            0553
@RF00549 DS    0H                                                  0554
*       /*************************************************************/
*       /*                                                           */
*       /* SET UP AND CALL IOSGEN VARY TO VARY PATH ONLINE           */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0554
*PTHVARY:                                                          0554
*       ONOFFIND='04'X;             /* SET FUNCTION CODE             */
PTHVARY  L     @05,VRYTPTR                                         0554
         MVI   ONOFFIND(@05),X'04'                                 0554
