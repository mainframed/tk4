*                                                                  0748
*   IF VALIDPTH='0'B THEN           /* WAS PATH INVALID?             */
@RF00741 TM    VALIDPTH,B'00000001'                                0748
         BNZ   @RF00748                                            0748
*     DO;                           /* YES,                          */
*       WPLLGH=LENGTH(MSGNEXST)+LENBAS;/* SET LENGTH OF WPL.         */
         L     @03,WPLPTR                                          0750
         LA    @09,49                                              0750
         ALR   @09,LENBAS                                          0750
         STH   @09,WPLLGH(,@03)                                    0750
*       WPLTXT=MSGNEXST;            /* MOVE TEXT TO WTO BUFFER.      */
         MVI   WPLTXT+49(@03),C' '                                 0751
         MVC   WPLTXT+50(76,@03),WPLTXT+49(@03)                    0751
         MVC   WPLTXT(49,@03),MSGNEXST                             0751
*       CALL DYNMSG(LENGTH(NEXSTSTA));/* FILL IN PATH FIELD.         */
         LA    @01,@AL00752                                        0752
         BAL   @14,DYNMSG                                          0752
*       CALL WTO;                   /* ISSUE WTO.                    */
         BAL   @14,WTO                                             0753
*       GOTO NEXTDEV;               /* CHECK FOR ANOTHER DEVICE.   0754
