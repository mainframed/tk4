*   WPLMCSFA='1'B;                  /* INDICATE ROUTE, DESCRIPTOR  0898
*                                      CODES PRESENT.                */
*   WPLMCSFC='1'B;                  /* MESSAGE IS COMMAND RESPONSE.  */
*   WPLMCSFB='1'B;                  /* CONSOLE ID WILL BE IN REG0.   */
*                                                                  0900
         L     @09,WPLPTR                                          0900
