*   WPLPTR=WPLPTR+WPLLGH;           /* ROUTE DESCRIPTOR CODE FIELDS
*                                      ARE BASED ON WPLPTR WHICH IS
*                                      NOW INCREMENTED BY THE LENGTH
*                                      OF THE BASE.                  */
*                                                                  0901
         LH    @08,WPLLGH(,@09)                                    0901
         ALR   @08,@09                                             0901
         ST    @08,WPLPTR                                          0901
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE ROUTE, DESCRIPTOR CODE FIELDS, AND MSGTYPE, QID    */
*   /* FIELDS.                                                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0902
*   WPLDESC=WPLDESC&&WPLDESC;       /* INITIALIZE DESCRIPTOR CODES.  */
         XC    WPLDESC(2,@08),WPLDESC(@08)                         0902
*   WPLROUT=WPLROUT&&WPLROUT;       /* INITIALIZE ROUTING CODES.     */
         XC    WPLROUT(2,@08),WPLROUT(@08)                         0903
*   WPLMSGTY=WPLMSGTY&&WPLMSGTY;    /* INITIALIZE MESSAGE TYPES.     */
         XC    WPLMSGTY(2,@08),WPLMSGTY(@08)                       0904
*   WPLQID=WPLQID&&WPLQID;          /* INITIALIZE QUEUE ID FIELD.    */
         LH    @09,WPLQID(,@08)                                    0905
