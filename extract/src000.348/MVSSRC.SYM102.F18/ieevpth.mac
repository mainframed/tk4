*/*   DYNMSG- FILL IN THE DYNAMIC PORTION OF A MESSAGE. WE WILL FILL */
*/*           EITHER OF THESE TWO PATH-FIELD TYPES:                  */
*/*           PATH(DDD,X)   OR                                       */
*/*           PATH(DDD) .                                            */
*/*    INPUT-                              @G55AP2L                  */
*/*           ENDCHARS- POINTS TO END OF PATH FIELD.                 */
*/*           BLANK- CHARACTER CONSTANT (' ').                       */
*/*           CHBUF- HAS INPUT COMMAND.                              */
*/*           INDEX- HAS INDEX INTO WPLTXT WHERE PATH TEXT SHOULD    */
*/*                  START.                                          */
*/*    OUTPUT- WPLTXT- WILL HAVE CHARACTER POSITIONS INDEX+1 TO      */
*/*                         INDEX+11                                 */
*/*                    FILLED IN WITH THE PATH FIELD, PADDED WITH    */
*/*                    BLANKS IF FIELD-TYPE IS PATH(DDD).            */
*                                                                  0912
*DYNMSG:                                                           0912
*   PROC(INDEX) OPTIONS(NOSAVEAREA);                               0912
DYNMSG   STM   @14,@12,12(@13)                                     0912
         MVC   @PC00014(4),0(@01)                                  0912
*   DCL                                                            0913
*     INDEX FIXED;                  /* INDEX INTO WPLTXT FOR PATH  0913
*                                      CHARACTERS.                   */
*   WPLTXT(INDEX+1:INDEX+11)=BLANK; /* BLANK OUT ENTIRE FIELD.       */
         L     @09,WPLPTR                                          0914
         L     @08,@PC00014                                        0914
         L     @08,INDEX(,@08)                                     0914
         ALR   @08,@09                                             0914
         MVI   WPLTXT+1(@08),C' '                                  0914
         MVC   WPLTXT+2(9,@08),WPLTXT+1(@08)                       0914
         MVC   WPLTXT(1,@08),BLANK                                 0914
*   WPLTXT(INDEX+1:INDEX+5)=CHBUF(1:5);/* 'PATH('            @G55AP2L*/
*                                                                  0915
         L     @08,@PC00014                                        0915
         L     @08,INDEX(,@08)                                     0915
         ALR   @09,@08                                             0915
         L     @08,CSCBPTR                                         0915
         MVC   WPLTXT(5,@09),CHBUF(@08)                            0915
*   /*****************************************************************/
*   /*                                                               */
*   /* CONVERT THE UCB HEX ADDRESS TO EBICID. THE EBICID DEVICE      */
*   /* ADDRESS MAY NOT BE IN THE UCB LOOKUP TABLE AND CANNOT BE FOUND*/
*   /* IN THE INPUT BUFFER IF WITHIN A RANGE.                @G55AP2L*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0916
*   ZONES='F0F0F0'X;                /* INIT WORK AREA        @G55AP2L*/
         MVC   ZONES(3),@CB03107                                   0916
*   REG4=VRYTPTR;                   /* GET DEVICE ENTRY PTR FOR BAL
*                                      CODE.                 @G55AP2L*/
         L     REG4,VRYTPTR                                        0917
*   GEN( MVN ZONES(2),HEXDEVAD(REG4));/* CONVERT 2ND AND 4TH HEX   0918
*                                      DIGITS. 2ND HEX = 1ST CHAR. 0918
*                                                            @G55AP2L*/
          MVN ZONES(2),HEXDEVAD(REG4)
*   GEN( MVC ZONES+2(1),ZONES+1);   /* 4TH HEX = 3RD CHAR.   @G55AP2L*/
          MVC ZONES+2(1),ZONES+1
*   GEN( LH REG1,HEXDEVAD(REG4));   /* GET HEX ADDRESS INTO REG.   0920
*                                                            @G55AP2L*/
          LH REG1,HEXDEVAD(REG4)
*   GEN( SRL REG1,4);               /* MUST GET 3RD HEX DIGIT FOR 2ND
*                                      CHAR.                 @G55AP2L*/
          SRL REG1,4
*   TEMP=REG1;                      /* SAVE FOR CONVERSION.  @G55AP2L*/
         ST    REG1,TEMP                                           0922
*   GEN( MVN ZONES+1(1),TEMP+3);    /* 3RD HEX = 2ND CHAR.   @G55AP2L*/
          MVN ZONES+1(1),TEMP+3
*   DO I=1 TO 3;                    /* CORRECT ZONE C-TYPE         0924
*                                      CHARACTERS.           @G55AP2L*/
         LA    I,1                                                 0924
@DL00924 DS    0H                                                  0925
*     IF ZONESX(I)>='FA'X THEN      /*                       @G55AP2L*/
         LA    @03,ZONESX-1(I)                                     0925
         CLI   0(@03),X'FA'                                        0925
         BL    @RF00925                                            0925
*       ZONESX(I)=ZONESX(I)-'39'X;  /*                       @G55AP2L*/
         SLR   @03,@03                                             0926
         IC    @03,ZONESX-1(I)                                     0926
         SH    @03,@CH03230                                        0926
         STC   @03,ZONESX-1(I)                                     0926
*   END;                            /* END ZONE CORRECTION.  @G55AP2L*/
*                                                                  0927
@RF00925 AH    I,@CH00042                                          0927
         CH    I,@CH00152                                          0927
         BNH   @DL00924                                            0927
*   /*****************************************************************/
*   /*                                                               */
*   /* END HEX TO EBICID CONVERSION.                         @G55AP2L*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0928
*   WPLTXT(INDEX+6:INDEX+8)=ZONES;  /* 'DDD'                 @G55AP2L*/
         L     @09,WPLPTR                                          0928
         L     @08,@PC00014                                        0928
         L     @08,INDEX(,@08)                                     0928
         ALR   @08,@09                                             0928
         MVC   WPLTXT+5(3,@08),ZONES                               0928
*   IF CHBUF(ENDCHARS-2:ENDCHARS-2)=','/* CPU ID?            @G55AP2L*/
*     THEN                                                         0929
         L     @08,CSCBPTR                                         0929
         LH    @07,ENDCHARS                                        0929
         ALR   @07,@08                                             0929
