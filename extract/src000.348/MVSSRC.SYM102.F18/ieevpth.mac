*                 /* MUST BE WRITTEN.                                */
*                 /*                                                 */
*                 /***************************************************/
*                                                                  0579
*                 CALL SMF(9);      /* WRITE TYPE-9 SMF RECORD.      */
         LA    @01,@AL00579                                        0579
         BAL   @14,SMF                                             0579
*               END;                /* FINISHED TAKING THE DEVICE  0580
*                                      ONLINE.                       */
*         END;                      /* FINISHED DETERMINING IF DEVICE
*                                      SHOULD BE TAKEN ONLINE.       */
*     END;                          /* END PROCESS RC OF 0 FROM    0582
*                                      IOSGEN                        */
