         XR    @07,@09                                             0905
         STH   @07,WPLQID(,@08)                                    0905
*   WPLDESCE='1'B;                  /* MSG IS RESPONSE TO A COMMAND. */
         OI    WPLDESCE(@08),B'00001000'                           0906
*   REG0=CONSLEID;                  /* SEND MSG TO CORRECT CONSOLE.  */
         LH    REG0,CONSLEID                                       0907
*   WPLPTR=ADDR(WTOPLIST);          /* POINT TO BASE OF PARM. LIST   */
