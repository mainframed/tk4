         TITLE ' IEEVPTH: VARY PATH COMMAND PROCESSOR'
IEEVPTH  CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'IEEVPTH   78.209'                                 0001
         DROP  @15
@PROLOG  BALR  @11,0                                               0001
@PSTART  LA    @12,4095(,@11)                                      0001
         USING @PSTART,@11                                         0001
         USING @PSTART+4095,@12                                    0001
         LR    @09,@01                                             0001
         L     @00,@SIZDATD                                        0001
         GETMAIN  R,LV=(0)
         LR    @10,@01                                             0001
         USING @DATD,@10                                           0001
         LR    @01,@09                                             0001
         ST    @13,@SA00001+4                                      0001
         LR    @13,@10                                             0001
         MVC   @PC00001(4),0(@01)                                  0001
*                                      DCL                   @ZA20333*/
*   CSCBPTR=REG1;                   /* GET PTR TO CSCB.              */
         ST    REG1,CSCBPTR                                        0129
*   SAVE14=REG14;                   /* SAVE RETURN POINT.            */
*                                                                  0130
         ST    REG14,SAVE14                                        0130
*   /*****************************************************************/
*   /*                                                               */
