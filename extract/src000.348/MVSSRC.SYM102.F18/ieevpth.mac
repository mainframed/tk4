*                                                                  0708
*/*   CLEANUP-                                                       */
*/*           CLEAN-UP AND RECOVERY ROUTINE. WITH INFORMATION ALREADY*/
*/*           DETERMINED, ISSUE APPROPRIATE MESSAGE DESCRIBING THE   */
*/*           ACTIONS TAKEN, AND FREE THE RESOURCES OWNED.           */
*/*    INPUT-                                                        */
*/*          CHUCMP- '00'X, COMMAND ISSUER UNAUTHORIZED.             */
*/*          UCBENQ- ON, HOLD SERIALIZATION ON UCB'S.                */
*/*          VARYENQ- ON, HOLD VARY COMMAND SERIALIZATION.           */
*/*          ERRENTRY- ON, ROUTINE CALLED BY ESTAE.                  */
*/*          SYNTXVAL- OFF, ISSUE 'SYNTAX INVALID' MESSAGE.          */
*/*          VALIDPTH- OFF, ISSUE 'PATH DOESN'T EXIST' MESSAGE.      */
*/*          PTHVARED- ON, PATH WAS VARY'ED.                         */
*/*          LASTPATH- ON, ISSUE 'LAST PATH- CAN'T VARY' MESSAGE.    */
*/*          RESERPTH- ON, ISSUE 'PATH RESERVED, CAN'T VARY' MESSAGE.*/
*/*          PTHNOTOP- ON, ISSUE 'PATH NOT OPERATIONAL' MESSAGE.     */
*/*     OUTPUT-                                                      */
*/*            ONE OR MORE MESSAGES (WTO'S) AS DESCRIBED IN THE MODULE
*                                                                    */
*/*            SPECIFICATIONS.                                       */
*                                                                  0708
*CLEANUP:                                                          0708
*   PROC;                                                          0708
*                                                                  0708
CLEANUP  STM   @14,@12,@SA00011                                    0708
*   /*****************************************************************/
*   /*                                                               */
*   /* SET UP TO FILL IN WTO PARAMETERS.                             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0709
*   LENBAS=LENGTH(WPLLGH)+LENGTH(WPLMCSF);/* GET LENGTH OF BASE OF 0709
*                                      WTO PARAMETER LIST.           */
         LA    LENBAS,4                                            0709
*   WTOPLIST=WTOPLIST&&WTOPLIST;    /* ZERO OUT ENTIRE FIELD.        */
         XC    WTOPLIST(130),WTOPLIST                              0710
*   WPLPTR=ADDR(WTOPLIST);          /* ESTABLISH ADDRESSABILITY TO 0711
*                                      WTO PARAMETER LIST.           */
*                                                                  0711
         LA    @03,WTOPLIST                                        0711
         ST    @03,WPLPTR                                          0711
*   /*****************************************************************/
*   /*                                                               */
*   /* READER & INTERNAL MESSAGES GO TO THE MASTER CONSOLE.          */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0712
*   IF CHUCMP='80'X×CHUCMP='00'X THEN/* FROM RDR OR INT. ?           */
         L     @03,CSCBPTR                                         0712
         CLI   CHUCMP(@03),X'80'                                   0712
         BE    @RT00712                                            0712
         CLI   CHUCMP(@03),X'00'                                   0712
         BNE   @RF00712                                            0712
@RT00712 DS    0H                                                  0713
*     DO;                           /* YES,                          */
*       UCMDVEPT=UCMMCENT;          /* GET ADDRESS OF MASTER CONSOLE
*                                      UCME.                         */
         L     @01,UCMMCSPT                                        0714
         L     UCMDVEPT,UCMMCENT(,@01)                             0714
*       CONSLEID=UCMID;             /* GET MASTER CONSOLE'S ID       */
         SLR   @09,@09                                             0715
         IC    @09,UCMID(,UCMDVEPT)                                0715
         STH   @09,CONSLEID                                        0715
*     END;                                                         0716
*   ELSE                            /* NO, COMMAND FROM CONSOLE.     */
*     CONSLEID=CHUCMP;              /* SET CONSOLE ID.               */
         B     @RC00712                                            0717
@RF00712 L     @03,CSCBPTR                                         0717
         SLR   @09,@09                                             0717
         IC    @09,CHUCMP(,@03)                                    0717
         STH   @09,CONSLEID                                        0717
*   IF NOAUTH='1'B THEN             /* CMD CALLER UN-AUTHORIZED?   0718
*                                                            @G55AP2L*/
@RC00712 TM    NOAUTH,B'00000001'                                  0718
         BNO   @RF00718                                            0718
*     DO;                           /* YES,                          */
*       WPLTXT=MSGNAUTH;            /* MOVE MSG TEXT.                */
         L     @03,WPLPTR                                          0720
         MVI   WPLTXT+43(@03),C' '                                 0720
         MVC   WPLTXT+44(82,@03),WPLTXT+43(@03)                    0720
         MVC   WPLTXT(43,@03),MSGNAUTH                             0720
*       WPLLGH=LENGTH(MSGNAUTH)+LENBAS;/* ASSIGN WPL ITS LENGTH.     */
         LA    @09,43                                              0721
         ALR   @09,LENBAS                                          0721
         STH   @09,WPLLGH(,@03)                                    0721
*       CALL WTO;                   /* ISSUE MSG.                    */
         BAL   @14,WTO                                             0722
*       GOTO FREE;                  /* FREE RESOURCES.               */
         B     FREE                                                0723
*     END;                          /* OF PROCESSING FOR UNAUTHORIZED
*                                      COMMANDS.                     */
*                                                                  0724
*   /*****************************************************************/
*   /*                                                               */
*   /* IF ISSUER HAS SOME AUTHORITY BUT NOT FOR THIS DEVICE, ISSUE   */
*   /* MESSAGE.                                              @G55AP2L*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0725
*   IF NOTAUTH='1'B THEN                                           0725
@RF00718 TM    NOTAUTH,B'10000000'                                 0725
         BNO   @RF00725                                            0725
*     DO;                           /* NOT AUTHORIZED FOR DEVICE?  0726
*                                                            @G55AP2L*/
*       WPLTXT=MSGNAUTH;            /* BASIC MSG TEXT.       @G55AP2L*/
         L     @03,WPLPTR                                          0727
         MVI   WPLTXT+43(@03),C' '                                 0727
         MVC   WPLTXT+44(82,@03),WPLTXT+43(@03)                    0727
         MVC   WPLTXT(43,@03),MSGNAUTH                             0727
*       WPLTXT(36:40)=' FOR ';      /* OPTIONAL TEXT.        @G55AP2L*/
         MVC   WPLTXT+35(5,@03),@CC03208                           0728
*       WPLTXT(41:43)=UCBNAME;      /* OPTIONAL 'DDD'.       @G55AP2L*/
         MVC   WPLTXT+40(3,@03),UCBNAME(UCBPTR)                    0729
*       WPLLGH=LENGTH(MSGNAUTH)+LENBAS;/* ASSIGN WPL ITS LENGTH.   0730
*                                                            @G55AP2L*/
         LA    @09,43                                              0730
         ALR   @09,LENBAS                                          0730
         STH   @09,WPLLGH(,@03)                                    0730
*       CALL WTO;                   /* ISSUE MESSAGE         @ZA20382*/
         BAL   @14,WTO                                             0731
*       GOTO NEXTDEV;               /* CHECK FOR ANOTHER DEVICE    0732
*                                                            @ZA20382*/
         B     NEXTDEV                                             0732
*     END;                          /* END NOT AUTH. FOR DEVICE.   0733
*                                                            @G55AP2L*/
*                                                                  0733
*   /*****************************************************************/
*   /*                                                               */
*   /* IF GETMAIN FAILED, ISSUE MESSAGE.                     @G55AP2L*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0734
*   IF GETMFAIL='1'B THEN                                          0734
@RF00725 TM    GETMFAIL,B'00000010'                                0734
         BNO   @RF00734                                            0734
*     DO;                           /* WRITE MESSAGE IF NO CORE    0735
*                                                            @G55AP2L*/
*       WPLTXT=MSGNSTOR;            /* MOVE IN TEXT TO WTO BUFFER  0736
*                                                            @G55AP2L*/
         L     @03,WPLPTR                                          0736
         MVI   WPLTXT+56(@03),C' '                                 0736
         MVC   WPLTXT+57(69,@03),WPLTXT+56(@03)                    0736
         MVC   WPLTXT(56,@03),MSGNSTOR                             0736
*       WPLLGH=LENGTH(MSGNSTOR)+LENBAS;/* ASSIGN LENGTH      @G55AP2L*/
         LA    @09,56                                              0737
         ALR   @09,LENBAS                                          0737
         STH   @09,WPLLGH(,@03)                                    0737
*       CALL WTO;                   /* ISSUE MESSAGE         @G55AP2L*/
         BAL   @14,WTO                                             0738
*       GOTO FREE;                  /* FREE RESOURCES        @G55AP2L*/
         B     FREE                                                0739
*     END;                          /* END GETMAIN FAILED MESSAGE  0740
*                                                            @G55AP2L*/
*                                                                  0740
*   /*****************************************************************/
*   /*                                                               */
*   /* IF SYNTAX INVALID, ISSUE MESSAGE.                             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0741
*   IF SYNTXVAL='0'B THEN           /* SYNTAX INVALID?               */
@RF00734 TM    SYNTXVAL,B'00000010'                                0741
         BNZ   @RF00741                                            0741
*     DO;                           /* YES,                          */
*       WPLTXT=MSGSYNTX;            /* MOVE TEXT.                    */
         L     @03,WPLPTR                                          0743
         MVI   WPLTXT+43(@03),C' '                                 0743
         MVC   WPLTXT+44(82,@03),WPLTXT+43(@03)                    0743
         MVC   WPLTXT(43,@03),MSGSYNTX                             0743
*       WPLLGH=LENGTH(MSGSYNTX)+LENBAS;/* ASSIGN LENGTH.             */
         LA    @09,43                                              0744
         ALR   @09,LENBAS                                          0744
         STH   @09,WPLLGH(,@03)                                    0744
*       CALL WTO;                   /* ISSUE MESSAGE.                */
         BAL   @14,WTO                                             0745
*       GOTO FREE;                  /* FREE RESOURCES.               */
         B     FREE                                                0746
*     END;                                                         0747
*                                                                  0747
*   /*****************************************************************/
*   /*                                                               */
*   /* IF SPECIFIED PATH INVALID, ISSUE MESSAGE.                     */
*   /*                                                               */
