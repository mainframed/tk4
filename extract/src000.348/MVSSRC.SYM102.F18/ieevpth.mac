*                                                                  0883
*/*    SMF- WRITE AN SMF RECORDFOR DEVICES WHICH HAVE BECOME ONLINE OR
*                                                                    */
*/*         OFFLINE ( RECORD TYPES 9 AND 11 RESPECTIVELY).           */
*/*                                                                  */
*/*     INPUT- RECTYPE- TYPE OF SMF RECORD.                          */
*/*     OUTPUT- ENDSMFRC- ON, AN SMF RECORD WAS WRITTEN.             */
*/*                                                                  */
*                                                                  0883
*SMF:                                                              0883
*   PROC(RECTYPE) OPTIONS(NOSAVEAREA);                             0883
SMF      STM   @14,@12,12(@13)                                     0883
         MVC   @PC00012(4),0(@01)                                  0883
*   DCL                                                            0884
*     RECTYPE FIXED;                /* =9, ONLINE RECORD TYPE. =11,
*                                      OFFLINE RECORD TYPE.          */
*   RECSMF=RECSMF&&RECSMF;          /* INITIALIZE SMF RECORD.        */
*                                                                  0885
         XC    RECSMF(24),RECSMF                                   0885
*   /*****************************************************************/
*   /*                                                               */
*   /* NOTE: SINCE RECORD TYPES 9 AND 11 BOTH HAVE THE SAME FORMAT,  */
*   /* MAPPING OF TYPE-11 RECORD IS DONE FOR BOTH.                   */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0886
*   SMF11PTR=ADDR(RECSMF);          /* ESTABLISH ADDRESSABILITY TO 0886
*                                      SMF RECORD.                   */
         LA    @05,RECSMF                                          0886
         LR    SMF11PTR,@05                                        0886
*   SMF11RTY=RECTYPE;               /* GET RECORD TYPE.              */
         L     @03,@PC00012                                        0887
         MVC   SMF11RTY(1,SMF11PTR),RECTYPE+3(@03)                 0887
*   SMF11LEN=24;                    /* TOTAL RECORD LENGTH.          */
         MVC   SMF11LEN(2,SMF11PTR),@CH00054                       0888
*   SMF11SEG=0;                     /* NO RECORD SPANNING.           */
         SLR   @03,@03                                             0889
         STH   @03,SMF11SEG(,SMF11PTR)                             0889
*   SMF11LN=6;                      /* THIS FIELD + DEVICE SECTION= 6
*                                      BYTES OF STORAGE.             */
*                                                                  0890
         MVC   SMF11LN(2,SMF11PTR),@CH00253                        0890
*   /*****************************************************************/
*   /*                                                               */
*   /* NOW, FILL IN DEVICE CLASS AND TYPE, AND CHANNEL AND UNIT      */
*   /* ADDRESS FIELDS IN THE DEVICE SECTION OF THE SMF RECORD.       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0891
*   SMF11PTR=ADDR(RECSMF)+LENGTH(SMFRCD11);/* ESTABLISH            0891
*                                      ADDRESSABILITY TO DEVICE    0891
*                                      SECTION.                      */
         AH    @05,@CH02923                                        0891
         LR    SMF11PTR,@05                                        0891
*   SMF11PTR->SMF11DUT(1:1)=DEVCLASS;/* FILL IN DEVICE CLASS.        */
         MVC   SMF11DUT(1,SMF11PTR),DEVCLASS                       0892
*   SMF11PTR->SMF11DUT(2:2)=DEVTYPE;/* FILL IN DEVICE TYPE.          */
*                                                                  0893
         MVC   SMF11DUT+1(1,SMF11PTR),DEVTYPE                      0893
