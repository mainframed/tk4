         OI    UCBONLI(UCBPTR),B'10000000'                         0576
*                 DVCEON='1'B;      /* SET FLAG FOR DEVICE ONLIN MSG */
         OI    DVCEON,B'00100000'                                  0577
*                 UCBVHRSN='0'B;    /* (OPERATIONAL) PATH TO DEVICE
