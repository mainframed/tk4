*   /*                                                               */
*   /*****************************************************************/
*                                                                  0131
*   ENTRYPT=ADDR(IEEVPTH);          /* GET START OF MODULE ADDRESS.  */
         L     @09,@CA00028                                        0131
         ST    @09,ENTRYPT                                         0131
*   STARTMWA=REG10;                 /* REG10 IS DATAREG- HAS ADDRESS
*                                      OF START OF MAIN WORK AREA.   */
         ST    REG10,STARTMWA                                      0132
*   ENDMWA=REG10+@SIZDATD;          /* ADDRESS OF END= ADDRESS OF  0133
*                                      START + LENGTH.               */
*                                                                  0133
         LR    @09,REG10                                           0133
         AL    @09,@SIZDATD                                        0133
         ST    @09,ENDMWA                                          0133
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE LOOP-CONTROL SWITCHES.                             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0134
*   IOPMLOAD='0'B;                  /* IECVIOPM NOT LOADED YET     0134
*                                                            @ZA20196*/
         NI    IOPMLOAD,B'01111111'                                0134
*   MAINSWCH='0'B;                  /* TURNED ON ONLY IN CASE OF   0135
*                                      ERROR.                        */
*                                                                  0135
         NI    MAINSWCH,B'01111111'                                0135
*   /*****************************************************************/
*   /*                                                               */
*   /* ASSUMPTIONS INDICATED BY FLAG SETTINGS ARE:                   */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0136
*   NOTAUTH='0'B;                   /* COMMAND'S ISSUER IS         0136
*                                      AUTHROIZED.                   */
         NI    NOTAUTH,B'01111111'                                 0136
*   NOAUTH='0'B;                    /* ISSUER IS AUTHROIZED. @G55AP2L*/
         NI    NOAUTH,B'11111110'                                  0137
*   SGCONSLE='0'B;                  /* PATH DOESN'T REPRESENT A PATH
*                                      TO A SYSGEN'ED CONSOLE.       */
*   IOAUTH2='0'B;                   /* ISSUER NOT AUTHORIZED TO VARY
*                                      PATHS OF NON-SYSGEN'ED CONSOLE
*                                      I/O DEVICES.                  */
*   CONAUTH3='0'B;                  /* ISSUER NOT AUTHORIZED TO VARY
*                                      PATHS OF SYSGEN'ED CONSOLE  0140
*                                      DEVICES.                      */
*   ONLINREQ='0'B;                  /* ASSUME AN OFF-LINE REQUEST.   */
         NI    SGCONSLE,B'10000111'                                0141
*   NOTCMPLT='0'B;                  /* PATH IS VALID         @ZA00826*/
*   NOTVER='0'B;                    /* PATH NOT VERIFIED     @ZA00826*/
         NI    NOTCMPLT,B'11100111'                                0143
*   UNCONREQ='0'B;                  /* THE LAST PATH TO THE DEVICE 0144
*                                      MAY NOT BE VARY'ED.           */
         NI    UNCONREQ,B'11111011'                                0144
*   UNCOND='0'B;                    /* NOT UNCONDITIONAL REQUEST   0145
*                                                            @G55AP2L*/
*   GETMFAIL='0'B;                  /* SET GETMAIN NOT FAILED YET. 0146
*                                                            @G55AP2L*/
         NI    UNCOND,B'11111001'                                  0146
*   SYNTXVAL='1'B;                  /* COMMAND IS SYNTACTICALLY    0147
*                                      CORRECT.                      */
*   VALIDPTH='1'B;                  /* PATH WAS SYSGEN'ED.           */
         OI    SYNTXVAL,B'00000011'                                0148
*   PTHVARED='0'B;                  /* IOSGEN VARY FUNCTION HASN'T 0149
*                                      BEEN (SUCCESSFULLY) INVOKED.  */
*   LASTPATH='0'B;                  /* THERE HAS NOT BEEN AN INVALID
*                                      ATTEMPT TO REMOVE A LAST PATH
*                                      TO A DEVICE.                  */
*   RESERPTH='0'B;                  /* PATH NOT A PATH TO SHARED   0151
*                                      DASD.                         */
*   PTHNOTOP='0'B;                  /* PATH TO A DEVICE BROUGHT ON-
*                                      LINE IS OPERATIONAL.          */
*   ERRENTRY='0'B;                  /* ESTAE HASN'T BEEN INVOKED.    */
*   VARYENQ='0'B;                   /* HAVEN'T ENQ'ED ON THE       0154
*                                      OPERATION OF OTHER VARY     0154
*                                      FUNCTIONS.                    */
*   UCBENQ='0'B;                    /* HAVE NOT ISSUED ENQ ON UCB'S. */
*   HIERFLG='0'B;                   /* HIERARCHY BIT SHOULDN'T BE SET*/
         NI    PTHVARED,B'00000000'                                0156
*   OFFLINFG='0'B;                  /* DON'T TURN UCBONLI OFF.       */
*   DVCEOFF='0'B;                   /* HAVEN'T TAKEN A DEVICE OFFLINE*/
*   DVCEON='0'B;                    /* HAVEN'T PLACED DEVICE ONLINE  */
*                                                                  0159
         NI    OFFLINFG,B'00011111'                                0159
*   /*****************************************************************/
*   /*                                                               */
*   /* SET UP THE RECOVERY ENVIRONMENT WITH AN ESTAE EXIT. GET VALUES*/
*   /* FOR CODE AND DATA REGISTERS TO BE RESTORED BY ESTAE ROUTINE.  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0160
*   DREG=REG10;                     /* GET DYNAMIC DATA REGISTER.    */
         ST    REG10,DREG                                          0160
*   CREG1=REG11;                    /* SAVE BASE REGISTER CONTENTS.  */
         ST    REG11,CREG1                                         0161
*   CREG2=REG12;                    /* GET SECOND BASE REG. CONTENTS.*/
         ST    REG12,CREG2                                         0162
*   RFY                                                            0163
*    (REG2) RSTD;                                                  0163
*   REG2=ADDR(PARMESTA);            /* GET ADDRESS OF PARAMETER LIST
*                                      FOR ESTAE EXIT ROUTINE.       */
         LA    REG2,PARMESTA                                       0164
*   REG1=ADDR(ESTLIST);             /* GET ADDRESS OF ESTAE SVC    0165
*                                      PARAM- ETER LIST.             */
         LA    REG1,ESTLIST                                        0165
*   ESTLIST=ESTLIST&&ESTLIST;       /* INITIALIZE PARAMETER LIST.    */
         XC    ESTLIST(16),ESTLIST                                 0166
*   GEN( ESTAE VPTHESTA,PARAM=(2),MF=(E,(1)));/* MAKE ESTAE EXIT   0167
*                                      KNOWN TO SYSTEM.              */
          ESTAE VPTHESTA,PARAM=(2),MF=(E,(1))
*   RFY                                                            0168
*    (REG2) UNRSTD;                                                0168
*                                                                  0168
*/*  WILL SKIP FURTHER PROCESSING OF COMMAND IF ESTAE NOT SET UP.    */
*                                                                  0169
*   IF REG15^=0 THEN                /* IF FAILURE TO SET UP ESTAE  0169
*                                      ROUTINE                       */
         LTR   REG15,REG15                                         0169
         BZ    @RF00169                                            0169
*     MAINSWCH='1'B;                /* THEN INDICATE TO BYPASS OTHER
*                                      PROCESSING AND THEN TO ENTER
*                                      THE CLEAN-UP ROUTINE.         */
         OI    MAINSWCH,B'10000000'                                0170
*/*  CONTINUE ONLY IF NO ERRORS SO FAR.                              */
*                                                                  0171
*   IF MAINSWCH='0'B THEN           /* NO ERRORS THUS FAR?           */
@RF00169 TM    MAINSWCH,B'10000000'                                0171
         BNZ   @RF00171                                            0171
*     DO;                           /* YES,                          */
*                                                                  0172
*       /*************************************************************/
*       /*                                                           */
*       /* ESTABLISH ADDRESSABILITY TO UCM BASE AND UCM PREFIX.      */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0173
*       UCMBASPT=CVTCUCB;           /* UCMBASPT PTS TO THE UCM.      */
         L     @09,CVTPTR                                          0173
         L     UCMBASPT,CVTCUCB(,@09)                              0173
*       UCMMCSPT=UCMPRFXP;          /* GET ADDRESS OF UCM PREFIX.    */
*                                                                  0174
         LR    @09,UCMBASPT                                        0174
         AL    @09,@CF03287                                        0174
         MVC   UCMMCSPT(4),UCMPRFXP(@09)                           0174
*       /*************************************************************/
*       /*                                                           */
*       /* DETERMINE IF THE ISSUER OF THE COMMAND IS AUTHORIZED. IF  */
*       /* IS AUTHORIZED, SAVE THE CODES INDICATING THE TYPES OF     */
*       /* DEVICES FOR WHICH HE IS AUTHORIZED: SYSGEN'ED CONSOLE     */
*       /* DEVICES ONLY, NON-SYSGEN'ED CONSOLE DEVICES, OR BOTH      */
*       /* (IOAUTH2 AND CONAUTH3 BOTH ON ).                          */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0175
*       CALL CMDAUTH;               /* ROUTINE DETERMINES COMMAND  0175
*                                      ISSUER'S AUTHORIZATION.       */
*                                                                  0175
         BAL   @14,CMDAUTH                                         0175
*       /*************************************************************/
*       /*                                                           */
*       /* STOP FURTHER PROCESSING IF COMMAND ISSUER HAS NO          */
*       /* AUTHORIZATIONS AT ALL.                                    */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0176
*       IF NOAUTH='1'B THEN         /* WAS THE ISSUER NOT AUTHORIZED?
*                                                            @G55AP2L*/
         TM    NOAUTH,B'00000001'                                  0176
         BNO   @RF00176                                            0176
*         MAINSWCH='1'B;            /* YES, THEN SET THE SWITCH      */
         OI    MAINSWCH,B'10000000'                                0177
*     END;                                                         0178
*                                                                  0178
@RF00176 DS    0H                                                  0179
*   /*****************************************************************/
*   /*                                                               */
*   /* CONTINUE ONLY IF NO ERRORS FOUND SO FAR.                      */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0179
*   IF MAINSWCH='0'B THEN           /* HAVE THERE BEEN NO ERRORS SO
*                                      FAR?                          */
@RF00171 TM    MAINSWCH,B'10000000'                                0179
         BNZ   @RF00179                                            0179
*     CALL SYNTXCHK;                /* CHECK THE COMMAND FOR       0180
*                                      SYNTACTIC CORRECTNESS.        */
*                                                                  0180
         BAL   @14,SYNTXCHK                                        0180
*   /*****************************************************************/
*   /*                                                               */
*   /* CONTINUE PROCESSING ONLY IF NO SYNTAX ERRORS FOUND.           */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0181
*   IF SYNTXVAL='0'B×GETMFAIL='1'B THEN/* IF ERROR IN SYNTAX ROUTINE
*                                      THEN                  @G55AP2L*/
@RF00179 TM    SYNTXVAL,B'00000010'                                0181
         BZ    @RT00181                                            0181
         TM    GETMFAIL,B'00000010'                                0181
         BNO   @RF00181                                            0181
@RT00181 DS    0H                                                  0182
*     MAINSWCH='1'B;                /* INDICATE SKIP OTHER MAINLINE  */
*                                                                  0182
         OI    MAINSWCH,B'10000000'                                0182
*   /*****************************************************************/
*   /*                                                               */
*   /* MAKE FURTHER SUBROUTINE CALLS ONLY IF NO ERRORS ENCOUNTERED UP*/
*   /* TO THIS POINT.                                                */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0183
*   GEN( LOAD EP=IECVIOPM);         /* LOAD IECVIOPM         @ZA20196*/
@RF00181 DS    0H                                                  0183
          LOAD EP=IECVIOPM
*   IOPMADDR=REG0;                  /* SAVE IECVIOPM ENTRY ADDR    0184
*                                                            @ZA20196*/
         ST    REG0,IOPMADDR                                       0184
*   IOPMLOAD='1'B;                  /* INDICATE TO DELETE    @ZA20196*/
         OI    IOPMLOAD,B'10000000'                                0185
*   VRYTPTR=VARYPTR;                /* POINT TO FIRST DEVICE TO    0186
*                                      PROCESS               @G55AP2L*/
*                                                                  0186
         MVC   VRYTPTR(4),VARYPTR                                  0186
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS IS THE START OF THE LOOP TO PROCESS ALL THE DEVICES      */
*   /* ENTERED AS INPUT. IT IS BRANCHED DIRECTLY TO FROM THE CLEANUP */
*   /* ROUTINE WHEN THE CLEANUP ROUTINE IS NOT ENTERED FOR ESTAE     */
*   /* PROCESSING OR A SYNTAX ERROR OR AN AUTHORITY ERROR AND ANOTHER*/
*   /* DEVICE HAS TO BE PROCESSED.                           @G55AP2L*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0187
*DEVLOOP:                                                          0187
*   ;                               /* START OF DEVICE(S) PROCESSING
*                                      LOOP                  @G55AP2L*/
DEVLOOP  DS    0H                                                  0188
*   IF MAINSWCH='0'B THEN           /* NO ERRORS THUS FAR?           */
         TM    MAINSWCH,B'10000000'                                0188
         BNZ   @RF00188                                            0188
*     DO;                           /* YES,                          */
*       CALL UCBCKOUT;              /* OBTAIN UCB ADDRESS FOR THE  0190
*                                      DEVICE, DETERMINE IF THE LAST
*                                      PATH TO THE DEVICE CAN BE   0190
*                                      VARY'ED (I.E., SHOULD THE VARY
*                                      BE UNCONDITIONAL?).           */
*                                                                  0190
         BAL   @14,UCBCKOUT                                        0190
*       /*************************************************************/
*       /*                                                           */
*       /* IF PATH NOT VALID, SIGNAL TO BYPASS FURTHER SUBROUTINE    */
*       /* CALLS                                                     */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0191
*       IF VALIDPTH='0'B THEN       /* IF PATH ISN'T VALID, THEN WE  */
         TM    VALIDPTH,B'00000001'                                0191
         BNZ   @RF00191                                            0191
*         MAINSWCH='1'B;            /* WANT TO BYPASS OTHER        0192
*                                      PROCESSING.                   */
         OI    MAINSWCH,B'10000000'                                0192
*     END;                                                         0193
*                                                                  0193
@RF00191 DS    0H                                                  0194
*   /*****************************************************************/
*   /*                                                               */
*   /* PERFORM FURTHER SUBROUTINE CALLS ONLY IF NO ERRORS ENCOUNTERED*/
*   /* UP TO THIS POINT.                                             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0194
*   IF MAINSWCH='0'B THEN           /* NO ERRORS YET?                */
@RF00188 TM    MAINSWCH,B'10000000'                                0194
         BNZ   @RF00194                                            0194
*     DO;                                                          0195
*       CALL POSCONCK;              /* DETERMINE IF THE DEVICE IS  0196
*                                      (SYSGEN'ED AS A ) CONSOLE. IF
*                                      SO, SPECIAL AUTHORIZATION IS
*                                      NEEDED TO 'VARY' ITS PATHS.   */
*                                                                  0196
         BAL   @14,POSCONCK                                        0196
*       /*************************************************************/
*       /*                                                           */
*       /* BYPASS OTHER PROCESSING IF COMMAND ISSUER DOESN'T HAVE THE*/
*       /* REQUIRED AUTHORIZATION.                                   */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0197
*       IF((SGCONSLE='1'B)&(CONAUTH3='0'B))×/* DO WE HAVE A SYSGEN'ED
*                                      CONSOLE, COMMAND ISSUER     0197
*                                      DOESN'T HAVE CONSOLE        0197
*                                      AUTHORIZATION, OR             */
*           ((SGCONSLE='0'B)&(IOAUTH2='0'B))/* DEVICE IS NOT A     0197
*                                      CONSOLE, BUT THE COMMAND    0197
*                                      ISSUER ISN'T AUTH- ORIZED FOR
*                                      VARYING THE OTHER I/O DEVICE
*                                      PATHS?                        */
*         THEN                      /* YES, COMMAND ISSUER NOT     0197
*                                      AUTHORIZED.                   */
         TM    SGCONSLE,B'01000000'                                0197
         BNO   @GL00002                                            0197
         TM    CONAUTH3,B'00010000'                                0197
         BZ    @RT00197                                            0197
@GL00002 TM    SGCONSLE,B'01100000'                                0197
         BNZ   @RF00197                                            0197
@RT00197 DS    0H                                                  0198
*         DO;                                                      0198
*           NOTAUTH='1'B;           /* SET THE NOT-AUTHORIZED FLAG: A
*                                      WT0 WILL BE ISSUED INDICATING
*                                      THAT THE COMMAND ISSUER WASN'T
*                                      AUTHORIZED.                   */
         OI    NOTAUTH,B'10000000'                                 0199
*           MAINSWCH='1'B;          /* BYPASS OTHER MAINLINE       0200
*                                      PROCESSING.                   */
         OI    MAINSWCH,B'10000000'                                0200
*         END;                                                     0201
*     END;                                                         0202
*                                                                  0202
@RF00197 DS    0H                                                  0203
*   /*****************************************************************/
*   /*                                                               */
*   /* PROCESS VARY PATH REQUEST IF NO ERRORS                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0203
*   IF MAINSWCH='0'B THEN           /* UNLESS ERRORS WERE FOUND,     */
@RF00194 TM    MAINSWCH,B'10000000'                                0203
         BNZ   @RF00203                                            0203
*     DO;                           /* VARY THE PATH.                */
*       CPUIDLOG=LOGCPUID;          /* PUT CPU ID INTO VARYTABL.   0205
*                                                            @G55AP2L*/
*                                                                  0205
         L     @09,VRYTPTR                                         0205
         MVC   CPUIDLOG(1,@09),LOGCPUID                            0205
*       /*************************************************************/
*       /*                                                           */
*       /* ROUTE PROCESSING TO OFFLINE OR ONLINE CONTROLLER, AS      */
*       /* REQUIRED.                                                 */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0206
*       IF ONLINREQ='0'B THEN                                      0206
         TM    ONLINREQ,B'00001000'                                0206
         BNZ   @RF00206                                            0206
*         CALL VARYOFF;             /* OFFLINE REQUEST               */
*                                                                  0207
         BAL   @14,VARYOFF                                         0207
*       /*************************************************************/
*       /*                                                           */
*       /* VARYON HANDLES REQUESTS FOR ONLINE REQUESTS.              */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0208
*       ELSE                        /* ONLINE REQUESTS.              */
*         CALL VARYON;              /* PROCESS ONLINE REQUEST      0208
*                                                            @ZA20196*/
         B     @RC00206                                            0208
@RF00206 BAL   @14,VARYON                                          0208
*     END;                                                         0209
@RC00206 DS    0H                                                  0210
*   CALL CLEANUP;                   /* FREE RESOURCES WHICH WE     0210
*                                      OBTAINED AND ISSUE THE PROPER
*                                      MESSAGE(S) IN RESPONSE TO THE
*                                      INPUT COMMAND.                */
@RF00203 BAL   @14,CLEANUP                                         0210
*   GEN( ESTAE 0);                  /* REMOVE ESTAE FROM STACK.      */
          ESTAE 0
*   REG14=SAVE14;                   /* RESTORE RETURN POINT.         */
         L     REG14,SAVE14                                        0212
*   RETURN;                         /* RETURN TO SYSTEM.             */
@EL00001 L     @13,4(,@13)                                         0213
@EF00001 L     @00,@SIZDATD                                        0213
         LR    @01,@10                                             0213
         FREEMAIN R,LV=(0),A=(1)
@ER00001 BR    @14                                                 0213
*                                                                  0214
*/* STARTOF SYNTAX CHECKING ROUTINE.                                 */
*/*SYNTXCHK - EXAMINE THE COMMAND TEXT LOOKING FOR THE               */
*/* FOLLOWING INFORMATION: DEVICE ADDRESS, CPU ADDRESS,              */
*/* ONLINE OR OFFLINE REQUEST, WHETHER THE REQUEST IS                */
*/* A REQUEST FOR A PATH TO BE VARY'ED OFFLINE UNCOND-              */
*/* DITIONALLY.                                                      */
*/*INPUT - CHBUF (IN THE CSCB) HAS THE COMMAND.                      */
*/*OUTPUT -                                                          */
*/* . HEXDEVAD-DEVICE ADDRESS IN HEXADECIAML                         */
*/* . OFFONIND-X'00'IF OFFLINE REQUEST,                              */
*/* .         -X'04' IF ONLINE REQUEST                               */
*/* . LOGCPUID- LOGICAL CPU IDENTIFIER.                              */
*/* . EBCDEVAD-DEVICE ADDRESS IN EBCDIC.                             */
*/* . SYNTXVAL-SET OFF IF THERE IS A SYNTAX ERROR.                   */
*/* . ONLINREQ-SET ON IF THERE IS AN ONLINE REQUEST.                 */
*                                                                  0214
*SYNTXCHK:                                                         0214
*   PROC OPTIONS(NOSAVEAREA);                                      0214
SYNTXCHK STM   @14,@12,12(@13)                                     0214
*   SCANPTR=5;                      /* INIT THE INDEX TO POINT TO THE
*                                      FIRST CHAR AFTER 'PATH'       */
         LA    @09,5                                               0215
         ST    @09,SCANPTR                                         0215
*   VARYPTR=0;                      /* INIT TO ZERO IN CASE GETMAIN
*                                      FAILS.                @G55AP2L*/
*                                                                  0216
         SLR   @05,@05                                             0216
         ST    @05,VARYPTR                                         0216
*   /*****************************************************************/
*   /*                                                               */
*   /* MUST BE POINTING AT A LEFT PARENTHESIS, ELSE THERE IS AN      */
*   /* ERROR.                                                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0217
*   IF BUFFER(SCANPTR)^='('         /* IF NOT A LEFT PAREN, THERE IS
*                                      AN ERROR                      */
*     THEN                                                         0217
         L     @05,CSCBPTR                                         0217
         ALR   @05,@09                                             0217
         CLI   BUFFER-1(@05),C'('                                  0217
         BE    @RF00217                                            0217
*     SYNTXVAL='0'B;                                               0218
         NI    SYNTXVAL,B'11111101'                                0218
*   IF SYNTXVAL='1'B THEN                                          0219
@RF00217 TM    SYNTXVAL,B'00000010'                                0219
         BNO   @RF00219                                            0219
*     DO;                           /* CONTINUE ON NO ERROR. @G55AP2L*/
*                                                                  0220
*       /*************************************************************/
*       /*                                                           */
*       /* ALLOCATE STORAGE TO CREATE TABLE OF DEVICE ADDRESSES TO BE*/
*       /* PROCESSED BY THIS COMMAND. THE MAXIMUM NUMBER OF ENTRIES  */
*       /* IS CALCULATED AS FOLLOWS: (NUMBER OF DASHES, I.E. DEVICE  */
*       /* RANGE, MULTIPLIED BY 16 (MAX IN RANGE)) ADDED TO (NUMBER  */
*       /* OF COMMAS, I.E. POSSIBLE SINGLE DEVICE, MINUS NUMBER OF   */
*       /* DASHES (TO GET ACTUAL NUMBER OF SINGLE DEVICES)). @G55AP2L*/
*       /*                                                           */
*       /*************************************************************/
*                                                                  0221
*       DEVCNT=1;                   /* INIT NO. OF DEVICES.  @G55AP2L*/
         LA    DEVCNT,1                                            0221
*       DO SCANPTR=SCANPTR TO LENGTH(CHBUF) WHILE BUFFER(SCANPTR)^=')';
         L     @09,SCANPTR                                         0222
         B     @DE00222                                            0222
@DL00222 L     @03,CSCBPTR                                         0222
         ALR   @09,@03                                             0222
         CLI   BUFFER-1(@09),C')'                                  0222
         BE    @DC00222                                            0222
*         IF BUFFER(SCANPTR)='-' THEN                              0223
         L     @09,SCANPTR                                         0223
         ALR   @03,@09                                             0223
         CLI   BUFFER-1(@03),C'-'                                  0223
         BNE   @RF00223                                            0223
*           DEVCNT=DEVCNT+1;        /* DASH?                 @G55AP2L*/
         AH    DEVCNT,@CH00042                                     0224
*         IF BUFFER(SCANPTR)=',' THEN                              0225
@RF00223 L     @09,CSCBPTR                                         0225
         AL    @09,SCANPTR                                         0225
         CLI   BUFFER-1(@09),C','                                  0225
         BNE   @RF00225                                            0225
*           DEVCNT=DEVCNT+1;        /* COMMA?                @G55AP2L*/
         AH    DEVCNT,@CH00042                                     0226
*       END;                        /* END NO. OF DEVICES ENTERED  0227
*                                      SEARCH                @G55AP2L*/
*                                                                  0227
@RF00225 LA    @09,1                                               0227
         AL    @09,SCANPTR                                         0227
         ST    @09,SCANPTR                                         0227
@DE00222 CH    @09,@CH01185                                        0227
         BNH   @DL00222                                            0227
@DC00222 DS    0H                                                  0228
*       /*************************************************************/
*       /*                                                           */
*       /* LOCATE POSITION IN INPUT BUFFER WHERE CPU ID SHOULD START */
*       /* SO AS TO CONTROL LOOP FOR PROCESSING DEVICES.     @G55AP2L*/
*       /*                                                           */
*       /*************************************************************/
*                                                                  0228
*       IF BUFFER(SCANPTR)=')'      /* ABOVE LOOP TERM ON RIGHT    0228
*                                      PAREN?                @G55AP2L*/
*         THEN                                                     0228
         L     @09,CSCBPTR                                         0228
