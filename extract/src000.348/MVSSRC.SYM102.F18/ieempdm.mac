         TITLE 'IEEMPDM - DISPLAY MATRIX COMMAND PROCESSOR             *
                        '
IEEMPDM  CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(22)                                             0001
         DC    C'IEEMPDM  UZ11015 77157'                           0001
         DROP  @15
@PROLOG  BALR  @11,0                                               0001
@PSTART  LA    @12,4095(,@11)                                      0001
         USING @PSTART,@11                                         0001
         USING @PSTART+4095,@12                                    0001
         LR    @07,@01                                             0001
         L     @00,@SIZDATD                                        0001
         GETMAIN  R,LV=(0)
         LR    @10,@01                                             0001
         USING @DATD,@10                                           0001
         LR    @01,@07                                             0001
         ST    @13,@SA00001+4                                      0001
         LR    @13,@10                                             0001
         MVC   @PC00001(4),0(@01)                                  0001
*                                                                  0093
*   /*****************************************************************/
*   /*                                                               */
*   /* MAINLINE CODE -DISPLAY MATRIX. SAVE INPUT PARAMETERS          */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0093
*   CSCBPTR=R1;                     /* SAVE @ CSCB                   */
         ST    R1,CSCBPTR                                          0093
*   RETPTR=R14;                     /* SAVE RETURN ADDRESS           */
*                                                                  0094
         ST    R14,RETPTR                                          0094
*   /*****************************************************************/
*   /*                                                               */
*   /* INIT VARIABLES IN WORKAREA                                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0095
*   GLBSWS='00'X;                   /* SET GLOBAL SWITCHES   @ZA20381*/
         MVI   GLBSWS,X'00'                                        0095
*   JBWKSAVE=0;                     /* ZERO SAVE OF JOBNAME WORKAREA
*                                      POINTER               @ZA20381*/
         SLR   @14,@14                                             0096
         ST    @14,JBWKSAVE                                        0096
*   INTERROR=-1;                    /* SET INTERNAL ERROR CODE       */
*                                                                  0097
         MVC   INTERROR(4),@CF02869                                0097
*   /*****************************************************************/
*   /*                                                               */
*   /* SET ADDRESS CONSTANTS FOR DEBUGGING                           */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0098
*   ENTRYPT=ADDR(IEEMPDM);          /* ENTRY POINT LOAD MODULE       */
         L     @14,@CA00028                                        0098
         ST    @14,ENTRYPT                                         0098
*   STRTMWA=R10;                    /* START MAIN WORKAREA           */
         ST    R10,STRTMWA                                         0099
*   ENDMWA=R10+@SIZDATD;            /* END ADDR +1 OF WORKAREA       */
*                                                                  0100
         LR    @14,R10                                             0100
         AL    @14,@SIZDATD                                        0100
         ST    @14,ENDMWA                                          0100
*   /*****************************************************************/
*   /*                                                               */
*   /* INIT ESTAE PARM LIST AND SET ESTAE ENVIRONMENT                */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0101
*   RFY                                                            0101
*    (R2) RSTD;                                                    0101
*   DATARG=R10;                     /* DATA REG OF MAIN WORKAREA     */
         ST    R10,DATARG                                          0102
*   CODERG1=R11;                    /* CODE REG FOR FIRST 4096       */
         ST    R11,CODERG1                                         0103
*   CODERG2=R12;                    /* CODE REG FOR NEXT 4096        */
         ST    R12,CODERG2                                         0104
*   R2=ADDR(EPARML);                /* ADDR OF USER PARM LIST        */
         LA    R2,EPARML                                           0105
*   ESTLIST=''B;                    /* ZERO ESTAE INTERFACE          */
         XC    ESTLIST(16),ESTLIST                                 0106
*   R1=ADDR(ESTLIST);               /* SET PTR TO CNTRL BLCK         */
         LA    R1,ESTLIST                                          0107
*   GEN(ESTAE ESTAERTN,PARAM=(2),MF=(E,(1)));                      0108
         ESTAE ESTAERTN,PARAM=(2),MF=(E,(1))
*   RFY                                                            0109
*    (R2) UNRSTD;                                                  0109
*                                                                  0109
*   /*****************************************************************/
*   /*                                                               */
*   /* IF ESTAE ESTABLISHED SUCCESSFULLY THEN PROCESS COMMAND ELSE   */
*   /* WRITE ERROR MSG AND EXIT                                      */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0110
*   IF R15=0 THEN                                                  0110
*                                                                  0110
         LTR   R15,R15                                             0110
         BNZ   @RF00110                                            0110
*     /***************************************************************/
*     /*                                                             */
*     /* ESTAE SUCCESSFUL. PROCESS COMMAND                           */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0111
*     DO;                                                          0111
*                                                                  0111
*       /*************************************************************/
*       /*                                                           */
*       /* SERIALIZE COMMAND WITH OTHER RECONFIGURATION COMMANDS     */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0112
*       DO;                         /* ENQ (QNAME,RNAME,S,3,SYSTEM)  */
*         RESPECIFY                                                0113
*          (GPR01P) RESTRICTED;                                    0113
*         I056(1:20)=I056(1:20)&&I056(1:20);/* ZERO OUT THE TABLE    */
         XC    I056(20),I056                                       0114
*         I05604(1)=3;              /* LENGTH OF MINOR RESOURCE NAME */
         MVI   I05604,X'03'                                        0115
*         I05606(1)=ADDR(QNAME);    /* ADDRESS OF MAJOR RESOURCE NAME*/
         LA    @14,QNAME                                           0116
         ST    @14,I05606                                          0116
*         I05607(1)=ADDR(RNAME);    /* ADDRESS OF MINOR RESOURCE NAME*/
         LA    @14,RNAME                                           0117
         ST    @14,I05607                                          0117
*         I05603(1)='01000000'B;    /* SET BIT STRING 1 FOR RESOURCE
*                                      ELEMENT                       */
         MVI   I05603,B'01000000'                                  0118
*         I05605(1)='11000000'B;    /* SET BIT STRING 2 FOR RESOURCE
*                                      ELEMENT                       */
         MVI   I05605,B'11000000'                                  0119
*         I05603(1,1)='1'B;         /* SET END OF LIST BIT           */
         OI    I05603,B'10000000'                                  0120
*         GPR01P=ADDR(I05602);      /* LOAD LIST ADDRESS             */
         LA    GPR01P,I05602                                       0121
*         SVC(56);                  /* ISSUE ENQ SVC                 */
         SVC   56                                                  0122
*         RESPECIFY                                                0123
*          (GPR01P) UNRESTRICTED;                                  0123
*       END;                        /* ENQ (QNAME,RNAME,S,3,SYSTEM)  */
*                                                                  0124
*       /*************************************************************/
*       /*                                                           */
*       /* GET TIME OF DAY VALUE AND UNPACK                          */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0125
*       GEN(TIME DEC,ZONE=LT,ERRET=CLCKFAIL);                      0125
         TIME DEC,ZONE=LT,ERRET=CLCKFAIL
*       GO TO SAVETIME;             /* GOOD CLOCK VALUE, SAVE IT     */
*                                                                  0126
         B     SAVETIME                                            0126
*       /*************************************************************/
*       /*                                                           */
*       /* CLOCK FAIL EXIT                                           */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0127
*CLCKFAIL:                                                         0127
*       R0=0;                       /* BAD CLOCK, SET TIME=0         */
*                                                                  0127
CLCKFAIL SLR   R0,R0                                               0127
*       /*************************************************************/
*       /*                                                           */
*       /* CONVERT CLOCK VALUE                                       */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0128
*SAVETIME:                                                         0128
*       TBUF03=R0;                  /* SAVE PACKED VALUE             */
SAVETIME ST    R0,TBUF03                                           0128
*       UNPK(TBUF,TBUF03);          /* UNPACK TIME                   */
*                                                                  0129
         UNPK  TBUF(8),TBUF03(4)                                   0129
*       /*************************************************************/
*       /*                                                           */
*       /* PARSE COMMAND TEXT IN CSCB                                */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0130
*       CALL PARSE;                                                0130
*                                                                  0130
         BAL   @14,PARSE                                           0130
*       /*************************************************************/
*       /*                                                           */
*       /* CHECK IF GOOD PARSE                                       */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0131
*       IF RC=0 THEN                                               0131
*                                                                  0131
         ICM   @14,15,RC                                           0131
         BNZ   @RF00131                                            0131
*         /***********************************************************/
*         /*                                                         */
*         /* GOOD PARSE. PROCESS REQUEST AND CLEANUP                 */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0132
*         DO;                                                      0132
*           IF CPUCHF='1'B THEN     /* CHECK IF CPU/CHAN REQ.        */
         TM    CPUCHF(GLBFF),B'10000000'                           0133
         BNO   @RF00133                                            0133
*             CALL CPUCHAN;         /* GET CPU/CHAN STATUS           */
         BAL   @14,CPUCHAN                                         0134
*           IF DEVF='1'B THEN       /* CHECK IF DEVICE STATUS REQ.   */
@RF00133 TM    DEVF(GLBFF),B'01000000'                             0135
         BNO   @RF00135                                            0135
*             CALL DEVICE;          /* GET DEVICE STATUS             */
         BAL   @14,DEVICE                                          0136
*           IF HIGHF='1'B THEN      /* CHECK IF HIGH ADDR REQ.       */
@RF00135 TM    HIGHF(GLBFF),B'00100000'                            0137
         BNO   @RF00137                                            0137
*             DO;                                                  0138
*               CALL WTORTN(4);     /* BLANK LINE                    */
         LA    @01,@AL00139                                        0139
         BAL   @14,WTORTN                                          0139
*               CALL WTORTN(8);     /* HIGH STORAGE ADDR HEADER      */
         LA    @01,@AL00140                                        0140
         BAL   @14,WTORTN                                          0140
*               CALL WTORTN(18);    /* HIGH REAL STOR ADDRESS        */
         LA    @01,@AL00141                                        0141
         BAL   @14,WTORTN                                          0141
*             END;                                                 0142
*           IF STORF='1'B THEN      /* STORAGE STATUS REQ.?          */
@RF00137 TM    STORF(GLBFF),B'00010000'                            0143
         BNO   @RF00143                                            0143
*             CALL STORAGE;         /* GET STORAGE STATUS            */
*                                                                  0144
         BAL   @14,STORAGE                                         0144
*           /*********************************************************/
*           /*                                                       */
*           /* END MULTI-LINE WTO                                    */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0145
*           CALL WTORTN(0);                                        0145
@RF00143 LA    @01,@AL00145                                        0145
         BAL   @14,WTORTN                                          0145
*         END;                      /* END OF PROCESS GOOD COMMAND   */
*                                                                  0146
*       /*************************************************************/
*       /*                                                           */
*       /* DEQUE OFF RESOURCE NAME                                   */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0147
*       DO;                         /* DEQ (QNAME,RNAME,3,SYSTEM)    */
@RF00131 DS    0H                                                  0148
*         I048(1:20)=I048(1:20)&&I048(1:20);                       0148
         XC    I048(20),I048                                       0148
*         RFY                                                      0149
*           GPR01F RESTRICTED;                                     0149
*         I04804(1)='11000000'B;    /* LISTEND BYTE                  */
         MVI   I04804,B'11000000'                                  0150
*         I04805(1)=3;              /* SET LENGTH                    */
         MVI   I04805,X'03'                                        0151
*         I04806(1)='01000000'B;    /* OPTIONS                       */
         MVI   I04806,B'01000000'                                  0152
*         I04807(1)=ADDR(QNAME);    /* QNAME ADDRESS                 */
         LA    @14,QNAME                                           0153
         ST    @14,I04807                                          0153
*         I04808(1)=ADDR(RNAME);    /* RNAME ADDRESS                 */
         LA    @14,RNAME                                           0154
         ST    @14,I04808                                          0154
*         GPR01F=ADDR(I04802);                                     0155
         LA    GPR01F,I04802                                       0155
*         SVC(48);                                                 0156
         SVC   48                                                  0156
*         RFY                                                      0157
*           GPR01F UNRESTRICTED;                                   0157
*       END;                        /* DEQ (QNAME,RNAME,3,SYSTEM)    */
*     END;                                                         0159
*                                                                  0159
*   /*****************************************************************/
*   /*                                                               */
*   /* ESTAE FAILED. WRITE MESSAGE AND EXIT                          */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0160
*   ELSE                                                           0160
*     DO;                                                          0160
         B     @RC00110                                            0160
@RF00110 DS    0H                                                  0161
*       CALL WTORTN(17);            /* CMD PROCESSOR FAILED          */
         LA    @01,@AL00161                                        0161
         BAL   @14,WTORTN                                          0161
*     END;                                                         0162
*                                                                  0162
*   /*****************************************************************/
*   /*                                                               */
*   /* FREE CSCB AND EXIT                                            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0163
*   CHFC='1'B;                      /* SET FREE CSCB BIT             */
@RC00110 L     @14,CSCBPTR                                         0163
         OI    CHFC(@14),B'00000010'                               0163
*   R1=CSCBPTR;                     /* SET PTR TO CSCB               */
         LR    R1,@14                                              0164
*   GEN(MGCR (1),CHAIN);            /* FREE CSCB                     */
*                                                                  0165
         MGCR (1),CHAIN
*   /*****************************************************************/
*   /*                                                               */
*   /* RETURN TO SYSTEM                                              */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0166
*NRMEXIT:                                                          0166
*   R14=RETPTR;                     /* RETORE REG 14                 */
NRMEXIT  L     R14,RETPTR                                          0166
*   RETURN;                         /* RETURN TO SYSTEM              */
@EL00001 L     @13,4(,@13)                                         0167
@EF00001 L     @00,@SIZDATD                                        0167
         LR    @01,@10                                             0167
         FREEMAIN R,LV=(0),A=(1)
@ER00001 BR    @14                                                 0167
*                                                                  0168
*   /*****************************************************************/
*   /*                                                               */
*   /* PARSE ROUTINE                                                 */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0168
*PARSE:                                                            0168
*   PROC;                                                          0168
*                                                                  0168
PARSE    STM   @14,@02,@SA00002                                    0168
         STM   @04,@12,@SA00002+20                                 0168
*/*                                                                  */
*/*   FUNCTION - PARSE COMMAND TEXT IN CSCB AND SET APPROPIATE       */
*/*    GLOBAL FUNCTION SWITCHES. IF ANY ERROR ARE DETECTED THEN      */
*/*    WRITE APPROPIATE ERROR MESSAGE TO OPERATOR AND EXIT.          */
*/*                                                                  */
*/*   INPUT-                                                         */
*/*    .COMMAND TEXT                                                 */
*/*    .REQUESTING CONSOLE ID                                        */
*/*                                                                  */
*/*   OUTPUT-                                                        */
*/*    .RC                                                           */
*/*     0  - SUCCESSFUL                                              */
*/*     12 - DELIMITER ERROR                                         */
*/*     13 - UNIDENTIFIABLE KEYWORD                                  */
*/*     14 - PARAMETERS CONFLICT                                     */
*/*     15 - IMPROPER CHANNEL NUMBER                                 */
*/*    .GLOBAL FUNCTION FLAGS                                        */
*/*                                                                  */
*/*   COMMAND SYNTAX                                                 */
*/*  FORM 1    M                     ALL OPTIONS                     */
*/*  FORM 2    M=OPRND               SINGLE OPERAND                  */
*/*  FORM 3    M=(OPRND ...,OPRND)   LIST OF OPERANDS                */
*/*                                                                  */
*/*   SEE POPRND SUBROUTINE FOR LIST OF POSSIBLE OPERANDS            */
*/*                                                                  */
*/*                                                                  */
*                                                                  0169
*   DCL                                                            0169
*     PPTR PTR,                     /* PARSE PTR                     */
*     CHAR CHAR(1) BASED(PPTR),     /* 1 CHAR IN TEXT                */
*     CHARS CHAR(*) BASED(PPTR),    /* MULTIPLE CHARS IN TEXT        */
*     LCLFF PTR,                    /* PTR TO LOCAL COPY OF FUNCTION
*                                      FLAGS                         */
*     LCLBUF CHAR(LENGTH(FUNCFLGS));/* LOCAL COPY OF FUNC FLAGS      */
*                                                                  0169
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE PARSE VARIABLES                                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0170
*   RC=0;                           /* SET RETURN CODE               */
         SLR   @14,@14                                             0170
         ST    @14,RC                                              0170
*   GLBFF=ADDR(GLBBUF);             /* SET MAP PTR TO GLOBAL COPY OF
*                                      FUNC FLAGS                    */
         LA    GLBFF,GLBBUF                                        0171
*   FUNCFLGS=''B;                   /* ZERO OUT FLAG FIELD           */
         XC    FUNCFLGS(17,GLBFF),FUNCFLGS(GLBFF)                  0172
*   LCLFF=ADDR(LCLBUF);             /* SET MAP PTR TO LOCAL COPY OF
*                                      FUNC FLAGS                    */
*                                                                  0173
         LA    LCLFF,LCLBUF                                        0173
*   /*****************************************************************/
*   /*                                                               */
*   /* SET PARSE PTR PAST 'M'                                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0174
*   PPTR=ADDR(CHBUF)+1;                                            0174
*                                                                  0174
         L     PPTR,CSCBPTR                                        0174
         LA    PPTR,CHBUF(,PPTR)                                   0174
         AH    PPTR,@CH00068                                       0174
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK IF DEFAULT CODED                                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0175
*   IF CHAR=' ' THEN                                               0175
*                                                                  0175
         CLI   CHAR(PPTR),C' '                                     0175
         BNE   @RF00175                                            0175
*     /***************************************************************/
*     /*                                                             */
*     /* DEFAULT OPTIONS CODED, SET DEFAULT VALUES                   */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0176
*     DO;                                                          0176
*       CCDHSF='F0'X;               /* REQ. ALL FUNCTIONS            */
         MVI   CCDHSF(GLBFF),X'F0'                                 0177
*       DO CHANID=0 TO CSDCHAD;     /* REQUEST STATUS FOR / DEVICES
*                                      ON ALL VAILD CHANNELS         */
         B     @DE00178                                            0178
@DL00178 DS    0H                                                  0179
*         DEVONCHF(CHANID+1)='1'B;  /* SET CHAN FLAG ON              */
         ALR   @14,GLBFF                                           0179
         OI    DEVONCHF(@14),B'10000000'                           0179
*       END;                                                       0180
         L     @07,CHANID                                          0180
         AH    @07,@CH00068                                        0180
         LR    @14,@07                                             0180
@DE00178 ST    @14,CHANID                                          0180
         L     @07,CVTPTR                                          0180
         L     @07,CVTCSD(,@07)                                    0180
         CH    @14,CSDCHAD(,@07)                                   0180
         BNH   @DL00178                                            0180
*     END;                                                         0181
*                                                                  0181
*   /*****************************************************************/
*   /*                                                               */
*   /* DEFAULT NOT CODED, PARSE EACH INDIVIDUAL OPERAND              */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0182
*   ELSE                                                           0182
*     DO;                                                          0182
*                                                                  0182
         B     @RC00175                                            0182
@RF00175 DS    0H                                                  0183
*       /*************************************************************/
*       /*                                                           */
*       /* CHECK FOR '='                                             */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0183
*       IF CHAR^='=' THEN                                          0183
*                                                                  0183
         CLI   CHAR(PPTR),C'='                                     0183
         BE    @RF00183                                            0183
*         /***********************************************************/
*         /*                                                         */
*         /* SET UP DELIMITER ERROR EXIT                             */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0184
*         DO;                                                      0184
*           RC=12;                  /* DELIMITER ERROR               */
         MVC   RC(4),@CF00037                                      0185
*           GO TO ERROREXT;         /* TAKE ERROR EXIT               */
         B     ERROREXT                                            0186
*         END;                                                     0187
*                                                                  0187
*       /*************************************************************/
*       /*                                                           */
*       /* INCR PAST '='                                             */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0188
*       PPTR=PPTR+1;                                               0188
*                                                                  0188
@RF00183 AH    PPTR,@CH00068                                       0188
*       /*************************************************************/
*       /*                                                           */
*       /* CHECK IF SUBLIST OF OPERANDS CODED                        */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0189
*       IF CHAR='(' THEN                                           0189
*                                                                  0189
         CLI   CHAR(PPTR),C'('                                     0189
         BNE   @RF00189                                            0189
*         /***********************************************************/
*         /*                                                         */
*         /* PROCESS SUBLIST OF OPERANDS                             */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0190
*         DO;                                                      0190
*           CHAR=',';               /* REPLACE ( WITH ,              */
         MVI   CHAR(PPTR),C','                                     0191
*           DO WHILE((CHAR=',')&(RC=0));/* PARSE EACH OPERAND        */
         B     @DE00192                                            0192
@DL00192 DS    0H                                                  0193
*             PPTR=PPTR+1;          /* INCR PAST ,                   */
         AH    PPTR,@CH00068                                       0193
*             CALL POPRND;          /* PARSE OPERAND                 */
         BAL   @14,POPRND                                          0194
*           END;                                                   0195
*                                                                  0195
@DE00192 CLI   CHAR(PPTR),C','                                     0195
         BNE   @DC00192                                            0195
         ICM   @14,15,RC                                           0195
         BZ    @DL00192                                            0195
@DC00192 DS    0H                                                  0196
*           /*********************************************************/
*           /*                                                       */
*           /* CHECK IF ANY OPERANDS IN ERROR                        */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0196
*           IF RC^=0 THEN                                          0196
         ICM   @14,15,RC                                           0196
         BNZ   @RT00196                                            0196
*             GO TO ERROREXT;       /* TAKE ERROR EXIT               */
*                                                                  0197
*           /*********************************************************/
*           /*                                                       */
*           /* CHECK THAT SUBLIST ENDS WITH ')'                      */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0198
*           IF CHAR^=')' THEN                                      0198
*                                                                  0198
         CLI   CHAR(PPTR),C')'                                     0198
         BE    @RF00198                                            0198
*             /*******************************************************/
*             /*                                                     */
*             /* NO, TAKE DELIMITER ERROR EXIT                       */
*             /*                                                     */
*             /*******************************************************/
*                                                                  0199
*             DO;                                                  0199
*               RC=12;              /* DELIMITER ERROR RC            */
         MVC   RC(4),@CF00037                                      0200
*               GO TO ERROREXT;     /* TAKE ERROR EXIT               */
         B     ERROREXT                                            0201
*             END;                                                 0202
*                                                                  0202
*           /*********************************************************/
*           /*                                                       */
*           /* INCR PAST ')'                                         */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0203
*           PPTR=PPTR+1;                                           0203
@RF00198 AH    PPTR,@CH00068                                       0203
*         END;                                                     0204
*                                                                  0204
*       /*************************************************************/
*       /*                                                           */
*       /* PROCESS SINGLE OPERAND                                    */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0205
*       ELSE                                                       0205
*         DO;                                                      0205
         B     @RC00189                                            0205
@RF00189 DS    0H                                                  0206
*           CALL POPRND;            /* PARSE OPERAND                 */
*                                                                  0206
         BAL   @14,POPRND                                          0206
*           /*********************************************************/
*           /*                                                       */
*           /* CHECK IF OPERNAND ERROR                               */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0207
*           IF RC^=0 THEN                                          0207
         ICM   @14,15,RC                                           0207
         BNZ   @RT00207                                            0207
*             GO TO ERROREXT;       /* YES, TAKE ERROR EXIT          */
*         END;                                                     0209
*                                                                  0209
*       /*************************************************************/
*       /*                                                           */
*       /* PARSE FOR FINAL BLANK AT END OF TEXT                      */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0210
*       IF CHAR^=' ' THEN                                          0210
*                                                                  0210
@RC00189 CLI   CHAR(PPTR),C' '                                     0210
         BE    @RF00210                                            0210
*         /***********************************************************/
*         /*                                                         */
*         /* FINAL BLANK MISSING, SET UP ERROR EXIT                  */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0211
*         DO;                                                      0211
*           RC=12;                  /* DELIMITER ERROR               */
         MVC   RC(4),@CF00037                                      0212
*           GO TO ERROREXT;         /* TAKE ERROR EXIT               */
         B     ERROREXT                                            0213
