*   /*****************************************************************/
*   /*                                                               */
*   /* PAGEFIX ROUTINE WILL FIX STORAGE FOR CALLER TO RUN DISABLED.  */
*   /* CALLER WILL SET UP REGS BEFORE CALLING ROUTINE. IF NECESSARY  */
*   /* CONTROL WILL NOT BE RETURNED TO CALLER UNTIL FUNCTION HAS BEEN*/
*   /* PERFORMED. IF AN ERROR OCCURS A MESSAGE WILL BE WRITTEN AND   */
*   /* RETURN MADE TO CALLER.                                @ZA20381*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0965
*   RFY                                                            0965
*    (R7,                                                          0965
*     R8,                                                          0965
*     R9) RESTRICTED;               /*                       @ZA20381*/
*   PGFXECB=0;                      /* ZERO OUT THE ECB      @ZA20381*/
         SLR   @14,@14                                             0966
         ST    @14,PGFXECB                                         0966
*   R9=ADDR(PGFXECB);               /* PAGE FIX ECB          @ZA20381*/
         LA    R9,PGFXECB                                          0967
*   GEN(PGFIX R,A=(7),EA=(8),ECB=(9),LONG=N);/* PAGEFIX @ZA20381W    */
*                                                                  0968
         PGFIX R,A=(7),EA=(8),ECB=(9),LONG=N
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK IF WAIT SHOULD BE ISSUED.. CHECK POST BIT IN ECB        */
*   /*                                                       @ZA20381*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0969
*   IF POSTED^='1'B THEN            /* ECB POSTED YET        @ZA20381*/
         TM    POSTED,B'01000000'                                  0969
         BO    @RF00969                                            0969
*     GEN(WAIT ECB=PGFXECB);        /* WAIT ON ECB           @ZA20381*/
*                                                                  0970
         WAIT ECB=PGFXECB
*   /*****************************************************************/
*   /*                                                               */
*   /* AFTER WE COME OUT OF WAIT CHECK POST CODE TO SEE IF FIX WAS   */
*   /* SUCCESSFUL @ZA20381W                                          */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0971
*   IF PGFXCODE^=0 THEN                                            0971
@RF00969 ICM   @14,7,PGFXCODE                                      0971
         BZ    @RF00971                                            0971
*     DO;                           /* PAGEFIX FAILED        @ZA20381*/
*       CALL WTORTN(17);            /* WRITE MSG CMD PROCESSOR FAILED
*                                                            @ZA20381*/
         LA    @01,@AL00973                                        0973
         BAL   @14,WTORTN                                          0973
*       RC=4;                       /* SET BAD RETURN CODE TO CALLER
*                                                            @ZA20381*/
         MVC   RC(4),@CF00109                                      0974
*     END;                          /* PAGEFIX FAILED        @ZA20381*/
*   ELSE                                                           0976
*     RC=0;                         /* PAGEFIX SUCCESSFUL    @ZA20381*/
         B     @RC00971                                            0976
@RF00971 SLR   @14,@14                                             0976
         ST    @14,RC                                              0976
*   RFY                                                            0977
*    (R7,                                                          0977
*     R8,                                                          0977
*     R9) UNRESTRICTED;             /*                       @ZA20381*/
*   RETURN;                         /* RETURN TO CALLER      @ZA20381*/
@EL00017 DS    0H                                                  0978
