*/* SUBPROCEDURE-                                                    */
*/*    MLEOT                                                         */
*/*                                                                  */
*/* FUNCTION-                                                        */
*/*    MAINLINE TASK TERMINATION                                     */
*/*                                                                  */
*/* INPUT : R10=ADDRESS OF WE OR ZERO                                */
*/*         R8=FUNCTION CODE: ABEND OR NORMAL END IF R10=0           */
*/*         R9 =TERMINATION REASON CODE IF R10=0                     */
*/*                                                                  */
*/* THIS ROUTINE GENERATES CODE TO NORMAL END OR                     */
*/* ABEND THE TASK. THE CONTROL DOES NOT RETURN TO                   */
*/* MAINLINE .                                                       */
*/*                                                                  */
*/********************************************************************/
*                                                                  0437
*MLEOT:                                                            0437
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0437
MLEOT    DS    0H                                                  0438
*   IF R10^=0 THEN                  /* IS THERE WE PASSED            */
         LTR   R10,R10                                             0438
         BZ    @RF00438                                            0438
*     DO;                           /* YES                           */
*       RFY                                                        0440
*         WESTD BASED(R10);         /* GET WE BASE                   */
*       R9=WERC;                    /* RETURN CODE                   */
         LH    R9,WERC(,R10)                                       0441
         N     R9,@CF01700                                         0441
*       R8=WECODE1F;                /* WHAT TYPE OF END              */
         SLR   R8,R8                                               0442
         IC    R8,WECODE1F(,R10)                                   0442
*     END;                          /* REASON CODE                   */
*   IF TWAPTR^=0 THEN               /* IF THERE IS TWA               */
@RF00438 LTR   TWAPTR,TWAPTR                                       0444
         BZ    @RF00444                                            0444
*     TWARSON=R9;                   /* YES STORE REASON CODE         */
         STH   R9,TWARSON(,TWAPTR)                                 0445
*   MLWA22=R9;                      /* USE LAST BYTE AS RETURN       */
@RF00444 STC   R9,MLWA22                                           0446
*   R9=MLWA22;                      /* CODE AND REASON CODE          */
         SLR   @05,@05                                             0447
         IC    @05,MLWA22                                          0447
         LR    R9,@05                                              0447
*   IF R8=FCMLAT THEN               /* IS IT ABEND                   */
         C     R8,@CF01460                                         0448
         BNE   @RF00448                                            0448
*     DO;                           /* YES                           */
*       R1=2;                       /* MSG 2                         */
         LA    R1,2                                                0450
*       CALL MSGBLK;                /* CALL MSG BLOCK GET M          */
         L     @15,TWAMSG(,TWAPTR)                                 0451
         BALR  @14,@15                                             0451
*       R8=R1->MLWA3;               /* GET LENGTH                    */
         L     R8,MLWA3(,R1)                                       0452
*       R1=R1+4;                    /* SKIP LEN FIELD                */
         AL    R1,@CF00080                                         0453
*       MSGTXT=R1->AREA(1:R8);      /* MOVE MSG OUT                  */
         MVI   MSGTXT+1,C' '                                       0454
         MVC   MSGTXT+2(98),MSGTXT+1                               0454
         LR    @05,R8                                              0454
         BCTR  @05,0                                               0454
         EX    @05,@SM01694                                        0454
*       CVD(R9,MLWAD);              /* FOR MSG TERM REASON           */
         CVD   R9,MLWAD                                            0455
*       GEN;                                                       0456
         UNPK  MSGTXT+45(4),MLWAD  UNPACK
         OI    MSGTXT+48,X'F0'     PRINTABLE LAST CHAR
         WTO   MF=(E,MSGTXT)       SEND TERMINATING MSG
         LR    R15,R9              REG 15 REASON              @ZA26725
         ABEND X'0AC',DUMP,,SYSTEM    ISSUE SYSTEM ABEND
*     END;                                                         0457
*   GEN;                                                           0458
@RF00448 DS    0H                                                  0458
         WTO   MF=(E,MSG6)            ISSUE TERMINATION MSG
*   GEN;                                                           0459
*                                          /* NO NORMAL TERM        */
         L     13,4(13)                LOAD SAVE AREA ADDR
         ST    9,16(13)                STORE RETURN CODE IN R15
         LM    14,12,12(13)            RESTORE REG
         BR    14                      RETURN
*   END MLEOT;                                                     0460
