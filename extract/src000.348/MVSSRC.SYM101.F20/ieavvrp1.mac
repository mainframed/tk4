*                                             /*ISSUE SET FRR       */
        SETFRR A,FRRAD=RECEXADD,WRKREGS=(8,9),PARMAD=(8)
*   PARMPTR=SRBPTR;                 /* BASE FOR PARM LIST            */
         LR    PARMPTR,SRBPTR                                      0107
*   PARMID='VRP1';                  /* SET MODULE ID                 */
         MVC   PARMID(4,PARMPTR),@CC02055                          0108
*   ERRCDE=0;                       /* INIT ERROR CODE TO ZERO       */
         SLR   ERRCDE,ERRCDE                                       0109
*   UCMMCSPT=UCMPRFXP;              /* ADDR OF MCS PREFIX            */
         L     @10,CVTPTR                                          0110
         L     @10,UCMBASPT(,@10)                                  0110
         LR    @09,@10                                             0110
         AL    @09,@CF02083                                        0110
         L     @09,UCMPRFXP(,@09)                                  0110
         ST    @09,UCMMCSPT                                        0110
*   UCMEXTPT=UCM2PTR;               /* ADDR OF UCM EXTENSION         */
         AL    @10,@CF02084                                        0111
         L     UCMEXTPT,UCM2PTR(,@10)                              0111
*   BUFPTR=XAL;                     /* SET PTR TO INPUT BUFFER       */
         L     BUFPTR,XAL-1(,R2)                                   0112
         LA    BUFPTR,0(,BUFPTR)                                   0112
*   IDSAV=BUFID;                    /* SAVE REPLY ID                 */
         MVC   IDSAV+32(2,R2),BUFID(BUFPTR)                        0113
*   TCBREG=PSATOLD;                 /* SET ADDR OF TCB       @ZA08774*/
         L     TCBREG,PSATOLD                                      0114
*   ZEROREG=288;                    /* SET LEN OF GETMAIN    @ZA08774*/
         LA    ZEROREG,288                                         0115
*   ASCBREG=PSAAOLD;                /* SET ADDR OF ASCB      @ZA08774*/
         L     ASCBREG,PSAAOLD                                     0116
