*/********************************************************************/
*/*           SETLOCKS   SEGMENT                                     */
*/*                                                                  */
*/* IN ORDER TO SERIALIZE THE USE OF RESOURCES ACROSS REPLY          */
*/* PROCESSING, THE FOLLOWING SEGMENT ISSUES SETLOCKS FOR THE        */
*/* LOCAL AND CMS LOCKS.                                             */
*/*                                                                  */
*/********************************************************************/
*                                                                  0099
*   RECEXADD=UCMFRRAD;              /* SET ADDR OF RECOVERY EXIT     */
         L     @10,CVTPTR                                          0099
         L     @10,UCMBASPT(,@10)                                  0099
         L     @10,UCMFRRAD(,@10)                                  0099
         ST    @10,RECEXADD                                        0099
*   XAV(5:8)=RTNREG;                /* SAVE RETURN ADDR              */
         STCM  RTNREG,15,XAV+4(R2)                                 0100
*   XAD=BASREG;                     /* SAVE BASE REGISTER            */
         ST    BASREG,XAD(,R2)                                     0101
*   WQEPTR=R11;                     /* SAVE DATA REGISTER            */
         LR    WQEPTR,R11                                          0102
*   GEN REFS(PSALITA,FLC);                                         0103
        SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,                        XX
               RELATED=(ORE,IEAVVRP1(RELEASE))
        SETLOCK OBTAIN,TYPE=CMS,MODE=UNCOND,                          XX
               RELATED=(ORE,IEAVVRP1(RELEASE))
*   BASREG=XAD;                     /* RESTORE BASE REG              */
