         ICM   RTNREG,15,XAV+4(R2)                                 0129
*       XAV='REPLY   ';             /* SET REPLY VERB                */
         MVC   XAV(8,R2),@CC02059                                  0130
*       R15=ADDR(IEE0503D);         /* SET ADDR OF MSG MODULE        */
         L     R15,@CV00204                                        0131
*       GEN(BR  R15);               /* BRANCH TO MSG MODULE          */
         BR  R15
*     END;                          /* END IF NO OPERAND FIELD       */
*   ELSE                            /* IF OPERAND FIELD SPECIFIED    */
*     DO;                           /* CONTINUE PROCESSING           */
