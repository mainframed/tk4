         L     @10,XAL-1(,R2)                                      0120
         LA    @10,0(,@10)                                         0120
         LTR   @10,@10                                             0120
         BNZ   @RF00120                                            0120
*     DO;                           /* IF YES,                       */
*       ERRCDE=1;                   /* SET ERROR INDICATOR           */
         LA    ERRCDE,1                                            0122
