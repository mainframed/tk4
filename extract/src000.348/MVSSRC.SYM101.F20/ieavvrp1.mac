*/********************************************************************/
*/*         RELEASE                    SEGMENT                       */
*/*                                                                  */
*/* THE FOLLOWING SEGMENT WILL RELEASE ALL LOCKS CURRENTLY           */
*/* HELD BY THIS MODULE BEFORE ISSUEING A WTOR FOR ACCEPTED          */
*/* REPLY.                                                           */
*/********************************************************************/
*                                                                  0124
*       XAD=BASREG;                 /* SAVE BASE REGISTER            */
         ST    BASREG,XAD(,R2)                                     0124
*       OREPTR=R11;                 /* SAVE DATA REGISTER            */
         LR    OREPTR,R11                                          0125
*       GEN REFS(PSALITA,FLC,PSACSTK,FRRSCURR,FRRSEMP,FRRSELEN);   0126
*                                     /*RELEASE LOCKS               */
        SETFRR  D,WRKREGS=(8,9)
        SETLOCK  RELEASE,TYPE=CMS,RELATED=(ORE,IEAVVRP1(SETLOCKS))
        SETLOCK  RELEASE,TYPE=LOCAL,RELATED=(ORE,IEAVVRP1(SETLOCKS))
*       BASREG=XAD;                 /* RESTORE BASE REGISTER         */
         L     BASREG,XAD(,R2)                                     0127
*       R11=OREPTR;                 /* RESTORE DATA REGISTER         */
         LR    R11,OREPTR                                          0128
