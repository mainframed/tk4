*       IF UCMRPYQ=0 THEN           /* IS ORE CHAIN EMPTY ?          */
         L     @10,CVTPTR                                          0135
         L     @10,UCMBASPT(,@10)                                  0135
         L     @10,UCMRPYQ(,@10)                                   0135
         LTR   @10,@10                                             0135
         BNZ   @RF00135                                            0135
*         DO;                       /* IF YES,                       */
*           ERRCDE=1;               /* INDICATE ERROR MSG 1          */
         LA    ERRCDE,1                                            0137
*           CALL ERRORTN;           /* CALL ERROR ROUTINE            */
         BAL   @14,ERRORTN                                         0138
*         END;                      /* END - NO ORE'S                */
*       ELSE                                                       0140
*         DO;                                                      0140
*                                                                  0140
         B     @RC00135                                            0140
@RF00135 DS    0H                                                  0141
*/********************************************************************/
*/*           VALIDITY  CHECK  SEGMENT                               */
*/*                                                                  */
*/* THE FOLLOWING SEGMENT OF CODE WILL VALIDATE THE REPLY ID. THE    */
*/* REPLY ID CAN BE A ONE OR TWO DIGIT DECIMAL NUMBER FOLLOWED BY    */
*/* A BLANK OR COMMA.IF THE ID IS INVALID, AN ERROR MESSAGE WILL BE  */
*/* ISSUED TO THE OPERATOR. IF VALID, REPLY PROCESSING WILL CONTINUE */
*/* NORMALLY.                                                        */
*/*                                                                  */
*/********************************************************************/
*                                                                  0141
*           /*********************************************************/
*           /*                                                       */
*           /* TEST THE FIRST PART OF THE REPLY ID FOR DECIMAL       */
*           /* CHARACTER                                             */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0141
*           IF BUFID1(1:4)^='1111'B THEN/* FIRST PART OF ID DECIMAL ?*/
         TM    BUFID1(BUFPTR),B'11110000'                          0141
         BO    @RF00141                                            0141
*             DO;                   /* IF NO,                        */
*               ERRCDE=2;           /* INDICATE ERROR MESSAGE 2      */
         LA    ERRCDE,2                                            0143
*               CALL ERRORTN;       /* CALL ERROR ROUTINE            */
         BAL   @14,ERRORTN                                         0144
*             END;                  /* END FIRST PART OF ID CHECK    */
*           ELSE                                                   0146
*             DO;                                                  0146
*                                                                  0146
         B     @RC00141                                            0146
@RF00141 DS    0H                                                  0147
*               /*****************************************************/
*               /*                                                   */
*               /* TEST THE SECOND PART OF ID FOR DECIMAL CHARACTER  */
*               /*                                                   */
*               /*****************************************************/
*                                                                  0147
*               IF BUFID2(1:4)^='1111'B THEN/* SECOND PART ID DEC    */
         TM    BUFID2(BUFPTR),B'11110000'                          0147
         BO    @RF00147                                            0147
*                 DO;               /* IF NO,                        */
*                   ID1='F0'X;      /* FIRST PART OF ID EQ DECIMAL   */
         LA    @10,32                                              0149
         ALR   @10,R2                                              0149
         MVI   ID1(@10),X'F0'                                      0149
*                   ID2=BUFID1;     /* SAVE ID IN XSA                */
         MVC   ID2(1,@10),BUFID1(BUFPTR)                           0150
*                   BUFPTR=BUFPTR-1;/* DECREASE ADDR OF ID BY 1      */
         BCTR  BUFPTR,0                                            0151
*                 END;              /* END SECOND PART OF ID CHECK   */
*                                                                  0152
*               /*****************************************************/
*               /*                                                   */
*               /* TEST FOR TEXT AFTER REPLY ID                      */
*               /*                                                   */
*               /*****************************************************/
*                                                                  0153
*               IF BUFCOMMA^=',' THEN/* TEST FOR COMMA AFTER ID      */
*                                                                  0153
@RF00147 CLI   BUFCOMMA(BUFPTR),C','                               0153
         BE    @RF00153                                            0153
*                 /***************************************************/
*                 /*                                                 */
*                 /* TEST FOR NULL REPLY                             */
*                 /*                                                 */
*                 /***************************************************/
*                                                                  0154
*                 IF BUFCOMMA^=' ' THEN/* NULL REPLY ?               */
         CLI   BUFCOMMA(BUFPTR),C' '                               0154
         BE    @RF00154                                            0154
*                   DO;             /* IF NO,                        */
*                     ERRCDE=5;     /* INDICATE ERROR MESSAGE 5      */
         LA    ERRCDE,5                                            0156
*                     CALL ERRORTN; /* CALL ERROR ROUTINE            */
         BAL   @14,ERRORTN                                         0157
*                   END;            /* END CHECKING FOR TEXT         */
*             END;                  /* END CHECKING REPLY ID         */
*                                                                  0159
@RF00154 DS    0H                                                  0159
@RF00153 DS    0H                                                  0160
*           /*********************************************************/
*           /*                                                       */
*           /* TEST TO DETERMINE IF AN ERROR HAS BEEN DETECTED.      */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0160
*           IF ERRCDE=0 THEN        /* IS THE ERROR CODE SET ?       */
@RC00141 LTR   ERRCDE,ERRCDE                                       0160
         BNZ   @RF00160                                            0160
*             DO;                   /* IF NO,                        */
*/********************************************************************/
*/*           SCAN  ID  SEGMENT                                      */
*/*                                                                  */
*/* THE FOLLOWING SEGMENT WILL SCAN THE ORE CHAIN FOR A REPLY ID     */
*/* MATCH. IF ANY ONE OF THE FOLLOWING CONDITIONS ARE TRUE, THE      */
*/* REPLY ID SPECIFIED IS NOT OUTSTANDING:                           */
*/*     1) NO ORES ON CHAIN                                          */
*/*     2) NO ID MATCH                                               */
*/*     3) ORE MARKED TO BE DELETED                                  */
*/*     4) ORE PARTIALLY PROCESSED                                   */
*/*     5) NO ASSOCIATED WQE                                         */
*/*     6) ASSOCIATED WQE MARKED SUSPENDED                           */
*/********************************************************************/
*                                                                  0162
*               RFY                                                0162
*                (BYPASSW) RSTD;    /* RESTRICT SWITCH REG           */
*               BYPASSW=0;          /* TURN BYPASS SWITCH OFF        */
         SLR   BYPASSW,BYPASSW                                     0163
*               OREPTR=UCMRPYQ;     /* SAVE ADDR OF ORE CHAIN        */
*                                                                  0164
         L     @10,CVTPTR                                          0164
         L     @10,UCMBASPT(,@10)                                  0164
         L     OREPTR,UCMRPYQ(,@10)                                0164
*               /*****************************************************/
*               /*                                                   */
*               /* SCAN ORE CHAIN FOR REPLY ID MATCH                 */
*               /*                                                   */
*               /*****************************************************/
*                                                                  0165
*               DO WHILE(OREPTR^=0&BYPASSW^=1);/* SCAN FOR ID MATCH  */
*                                                                  0165
         B     @DE00165                                            0165
@DL00165 DS    0H                                                  0166
*                 /***************************************************/
*                 /*                                                 */
*                 /* CHECK REPLY ID WIYH ORE ID                      */
*                 /*                                                 */
*                 /***************************************************/
*                                                                  0166
*                 IF IDSAV=OREID THEN/* IDS MATCH ?                  */
         CLC   IDSAV+32(2,R2),OREID(OREPTR)                        0166
         BNE   @RF00166                                            0166
*                   DO;             /* IF YES,                       */
*                                                                  0167
*                     /***********************************************/
*                     /*                                             */
*                     /* HAS THIS ORE BEEN MARKED TO BE DELETED ?    */
*                     /*                                             */
*                     /***********************************************/
*                                                                  0168
*                     IF OREBUFC^='1'B THEN/* DELETE ORE ?           */
         TM    OREBUFC(OREPTR),B'00100000'                         0168
         BO    @RF00168                                            0168
*                       DO;         /* IF NO,                        */
*                                                                  0169
*                         /*******************************************/
*                         /*                                         */
*                         /* HAS THIS ORE BEEN PROCESSED BY REPLY ?  */
*                         /*                                         */
*                         /*******************************************/
*                                                                  0170
*                         IF OREOPBUF=0 THEN/* ORE PROCESSED ?       */
         SLR   @10,@10                                             0170
         C     @10,OREOPBUF(,OREPTR)                               0170
         BNE   @RF00170                                            0170
*                           DO;     /* IF NO,                        */
*                             WQEPTR=OREWQE;/* GET ADDR OF WQE       */
*                                                                  0172
         L     WQEPTR,OREWQE(,OREPTR)                              0172
*                             /***************************************/
*                             /*                                     */
*                             /* TEST TO DETERMINE IF A WQE EXIST    */
*                             /*                                     */
*                             /***************************************/
*                                                                  0173
*                             IF WQEPTR^=0 THEN/* WQE EXIST ?        */
         CR    WQEPTR,@10                                          0173
         BE    @RF00173                                            0173
*                               DO; /* IF YES,                       */
*                                                                  0174
*                                 /***********************************/
*                                 /*                                 */
*                                 /* WQE MARKED SUSPENDED ?          */
*                                 /*                                 */
*                                 /***********************************/
*                                                                  0175
*                                 IF WQESUSP^='1'B THEN/* SUSPENDED ?*/
         TM    WQESUSP(WQEPTR),B'00000010'                         0175
         BO    @RF00175                                            0175
*                                   BYPASSW=1;/* TURN BYPASS SWITCH
*                                      ON                            */
         LA    BYPASSW,1                                           0176
*                                 ELSE/* SUSPEND NOT INDICATED       */
*                                   OREPTR=0;/* GET OUT OF LOOP      */
         B     @RC00175                                            0177
@RF00175 SLR   OREPTR,OREPTR                                       0177
*                               END;/* END WQE TEST                  */
*                             ELSE  /* NO WQE EXIST                  */
*                               OREPTR=0;/* GET OUT OF LOOP          */
         B     @RC00173                                            0179
@RF00173 SLR   OREPTR,OREPTR                                       0179
*                           END;    /* END ORE PROCESS TEST          */
*                         ELSE      /* ORE NOT PROCESSED             */
*                           OREPTR=0;/* GET OUT OF LOOP              */
         B     @RC00170                                            0181
@RF00170 SLR   OREPTR,OREPTR                                       0181
*                       END;        /* END DELETE ORE TEST           */
*                     ELSE          /* IF ERROR WAS DETECTED         */
*                       OREPTR=0;   /* SET CONDITION TO BYPASS LOOP  */
         B     @RC00168                                            0183
@RF00168 SLR   OREPTR,OREPTR                                       0183
*                   END;            /* END ID MATCH                  */
*                 ELSE              /* IF NO ID MATCH                */
*                   OREPTR=ORELKP;  /* GET POINTER TO NEXT ORE       */
         B     @RC00166                                            0185
@RF00166 L     OREPTR,ORELKP(,OREPTR)                              0185
*               END;                /* END DO WHILE LOOP             */
*                                                                  0186
@RC00166 DS    0H                                                  0186
@DE00165 LTR   OREPTR,OREPTR                                       0186
         BZ    @DC00165                                            0186
         C     BYPASSW,@CF00047                                    0186
         BNE   @DL00165                                            0186
@DC00165 DS    0H                                                  0187
*               /*****************************************************/
*               /*                                                   */
*               /* TEST TO ISSUE ERROR CONDITION                     */
*               /*                                                   */
*               /*****************************************************/
*                                                                  0187
*               IF BYPASSW=0 THEN   /* BYPASS SWITCH SET             */
         LTR   BYPASSW,BYPASSW                                     0187
         BNZ   @RF00187                                            0187
*                 DO;               /* IF NO,                        */
*                   ERRCDE=4;       /* INDICATE ERROR MESSAGE 4      */
         LA    ERRCDE,4                                            0189
*                   CALL ERRORTN;   /* CALL ERROR ROUTINE            */
         BAL   @14,ERRORTN                                         0190
*                 END;              /* END ERROR PROCESSING          */
*               RFY                                                0192
*                (BYPASSW) UNRSTD;  /* MAKE REG AVAILABLE            */
*                                                                  0192
@RF00187 DS    0H                                                  0193
*               /*****************************************************/
*               /*                                                   */
*               /* TEST TO DETERMINE IF AN ERROR HAS BEEN DETECTED.  */
*               /*                                                   */
*               /*****************************************************/
*                                                                  0193
*               IF ERRCDE=0 THEN    /* IS ERROR CODE SET ?           */
         LTR   ERRCDE,ERRCDE                                       0193
         BNZ   @RF00193                                            0193
*                 DO;               /* IF NO,                        */
*                                                                  0194
*                   /*************************************************/
*                   /*                                               */
*                   /* TEST TO DETERMINE IF REPLY WAS ENTERED THROUGH*/
*                   /* INPUT STREAM.                                 */
*                   /*                                               */
*                   /*************************************************/
*                                                                  0195
*                   IF XAU^='00'X THEN/* REPLY FROM INPUT STREAM ?   */
         CLI   XAU(R2),X'00'                                       0195
         BE    @RF00195                                            0195
*                     DO;           /* IF NO,                        */
*/********************************************************************/
*/*            AUTHORITY   CHECKING   SEGMENT                        */
*/*                                                                  */
*/* THE FOLLOWING CODE CHECKS THE AUTHORIZATION OF THE RESPONDING    */
*/* CONSOLE TO ANSWER THE WTOR MESSAGE. ANY ONE OF THE FOLLOWING     */
*/* CONDITIONS ENABLES THE CONSOLE TO ANSWER.                        */
*/*                                                                  */
*/* 1. THE RESPONDING CONSOLE IS A MASTER CONSOLE.                   */
*/*                                                                  */
*/* 2. THE RESPONDING CONSOLE'S UNIQUE UCM ENTRY ID MATCHES.         */
*/*                                                                  */
*/* 3. THE RESPONDING CONSOLE IS MARKED TO ACCEPT ONE OF THE ROUTING */
*/*    CODES TO WHICH THE WTOR WAS ISSUED.                           */
*/*                                                                  */
*/* 4. THE WTOR WAS ISSUED TO ALL CONSOLES.                          */
*/*                                                                  */
*/* 5. THE WTOR WAS OF THE MSG TYPE WHICH THIS CONSOLE RECEIVES.     */
*/*                                                                  */
*/* INPUT:                                                           */
*/*  WQE,UCME,XSA,UCM                                                */
*/*                                                                  */
*/* OUTPUT: ERRMSG3 OR CONTINUE PROCESSING                           */
*/*  IEE703I REPLY ID NOT REQUESTED FROM THIS CONSOLE.               */
*/*                                                                  */
*/********************************************************************/
*                                                                  0197
*                       ENDSEG=0;   /* INIT END INDICATOR            */
         SLR   ENDSEG,ENDSEG                                       0197
*                       UCMDVEPT=UCMVEA;/* ADDR OF FIRST UCME        */
*                                                                  0198
         L     @10,CVTPTR                                          0198
         L     @10,UCMBASPT(,@10)                                  0198
         L     UCMDVEPT,UCMVEA(,@10)                               0198
*                       /*********************************************/
*                       /*                                           */
*                       /* SCAN UCME CHAIN FOR MATCHING IDS.         */
*                       /*                                           */
*                       /*********************************************/
*                                                                  0199
*                       DO WHILE(UCMDVEPT<=UCMVEL&UCMID^=XAU);     0199
         B     @DE00199                                            0199
@DL00199 DS    0H                                                  0200
*                         UCMDVEPT=UCMDVEPT+UCMVEZ;/* GET ADDR OF  0200
*                                      NEXT UCME                     */
         L     @10,CVTPTR                                          0200
         L     @10,UCMBASPT(,@10)                                  0200
         AL    UCMDVEPT,UCMVEZ(,@10)                               0200
*                       END;        /* END UCME SCAN                 */
*                                                                  0201
@DE00199 L     @10,CVTPTR                                          0201
         L     @10,UCMBASPT(,@10)                                  0201
         C     UCMDVEPT,UCMVEL(,@10)                               0201
         BH    @DC00199                                            0201
         CLC   UCMID(1,UCMDVEPT),XAU(R2)                           0201
         BNE   @DL00199                                            0201
@DC00199 DS    0H                                                  0202
*                       /*********************************************/
*                       /*                                           */
*                       /* TEST FOR END OF UCME CHAIN.IF SO,CONSOLE  */
*                       /* NOT AUTHORIXED.                           */
*                       /*                                           */
*                       /*********************************************/
*                                                                  0202
*                       IF UCMDVEPT>UCMVEL THEN/* END OF UCME CHAIN  */
         L     @10,CVTPTR                                          0202
         L     @10,UCMBASPT(,@10)                                  0202
         C     UCMDVEPT,UCMVEL(,@10)                               0202
         BNH   @RF00202                                            0202
*                         DO;       /* IF YES,                       */
*                           ERRCDE=3;/* INDICATE ERROR MSG3          */
         LA    ERRCDE,3                                            0204
*                           CALL ERRORTN;/* CALL ERROR RTN           */
         BAL   @14,ERRORTN                                         0205
*                           ENDSEG=1;/* END PROCESSING               */
         LA    ENDSEG,1                                            0206
*                         END;      /* UCME NOT ON CHAIN             */
*                       ELSE        /* IF NOT END OF CHAIN           */
*                                                                  0208
*                         /*******************************************/
*                         /*                                         */
*                         /* TEST FOR MASTER CONSOLE.IF SO, VALID    */
*                         /* CONSOLE.                                */
*                         /*                                         */
*                         /*******************************************/
*                                                                  0208
*                         IF UCMDISPA^='1'B THEN/* MASTER CONSOLE ?  */
         B     @RC00202                                            0208
@RF00202 TM    UCMDISPA(UCMDVEPT),B'10000000'                      0208
         BO    @RF00208                                            0208
*                           DO;     /* IF NO,                        */
*                                                                  0209
*                             /***************************************/
*                             /*                                     */
*                             /* TEST FOR QUEUE CONDITIONALLY OR     */
*                             /* UNCONDITIONALLY.                    */
*                             /*                                     */
*                             /***************************************/
*                                                                  0210
*                             IF WQEMCSB='1'B×/* Q CONDITIONALLY ON
*                                      REG 0                         */
*                                 WQEMCSH='1'B THEN/* Q            0210
*                                      UNCONDITIONALLY ON REG 0      */
         TM    WQEMCSB(WQEPTR),B'01000001'                         0210
         BZ    @RF00210                                            0210
*                               DO; /* IF YES,                       */
*                                                                  0211
*                                 /***********************************/
