         TITLE 'IEAVNP01 - CONSOLE INITIALIZATION ROUTINE              *
                        '
IEAVNP01 CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(19)                                             0001
         DC    C'IEAVNP01R0300076040'                              0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @12,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@12                                         0001
*
** REFS ARE GENERATED FOR NVT FIELDS THAT CONTAIN ADDRESSES OF
** NIP SERVICE ROUTINES, SO IEAPMNIP MACRO CALLS TO THESE ROUTINES
** WILL ASSEMBLE PROPERLY.
*   NPBTAPAD=CVTXAPG;               /* PUT IO-APNDG ADDR IN TEST DEB */
*                                                                  0133
         MVC   NPBTAPAD(3),CVTXAPG+1(RCVT)                         0133
*   /*****************************************************************/
*   /*                                                               */
*   /* TESTING OF CONSOLE AVAILABILITY IS BEGUN WITH THE CONSOLE     */
*   /* SELECTED AS MASTER DURING SYSTEM GENERATION.                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0134
*   UCMBASPT=CVTCUCB;               /* OBTAIN POINTER TO UCM BASE    */
         L     UCMBASPT,CVTCUCB(,RCVT)                             0134
*   UCMDVEPT=UCMMCENT;              /* POINTER TO MASTER CONSOLE MAP */
         LR    @08,UCMBASPT                                        0135
         AL    @08,@CF02045                                        0135
         L     @08,UCMMCSPT(,@08)                                  0135
         L     UCMDVEPT,UCMMCENT(,@08)                             0135
*   MSTRCNSL=UCMDVEPT;              /* SAVE SYS GEN MASTER POINTER   */
         LR    MSTRCNSL,UCMDVEPT                                   0136
*NP1ALT:                                                           0137
*   UCMENTRY=UCMDVEPT;              /* SAVE UCM ENTRY ADDRESS        */
NP1ALT   LR    UCMENTRY,UCMDVEPT                                   0137
*NP1BLT:                            /* THIS ROUTINE CONDUCTS CONSOLE
*                                      AVAILABILITY TESTING          */
*   RPARM=UCMUCB;                   /* GET EBCDIC UNIT ADDRESS       */
NP1BLT   L     RPARM,UCMUCB(,UCMDVEPT)                             0138
*   IF UCMDISPK='1'B                /* IF JES3 DUMMY CONS    @ZA05194*/
*     THEN                          /* THEN DONT CALL NIPM   @ZA05194*/
         TM    UCMDISPK(UCMDVEPT),B'00100000'                      0139
         BO    @RT00139                                            0139
*     GO TO NP1CMNT;                /* GET NEXT CHECK END    @ZA05194*/
*                                                                  0140
*   /*****************************************************************/
*   /*                                                               */
*   /* CALL IEAVNIPM UCB FIND ROUTINE                                */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0141
*   GEN(IEAPMNIP  TYPE=UCBFN,PARAM=RPARM)                          0141
*   REFS(NVTUCBFN);                                                0141
         IEAPMNIP  TYPE=UCBFN,PARAM=RPARM
*   UCBPTR=RPARM;                   /* STORE UCB ADDRESS - THIS UNIT */
         LR    UCBPTR,RPARM                                        0142
*   UCMUCB=UCBPTR;                  /* STORE UCB ADDRESS IN UCM      */
         ST    UCBPTR,UCMUCB(,UCMDVEPT)                            0143
*   UCMXOR='01'X;                   /* FLAG UCM ENTRY AS TESTED      */
*                                                                  0144
         MVI   UCMXOR(UCMDVEPT),X'01'                              0144
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK FOR UCB MARKED ONLINE AND READY                 @ZA01929*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0145
*   IF UCBONLI='1'B&                /* UCB MARKED ONLINE AND @ZA01929*/
*       UCBDUC^='1'B&               /* UNIT CHECK IND.       @ZA04124*/
*       UCBNRY^='1'B                /* DEVICE READY ?        @ZA01929*/
*     THEN                          /* IF SO, BRANCH TO --           */
         TM    UCBONLI(UCBPTR),B'10000000'                         0145
         BNO   @RF00145                                            0145
         TM    UCBDUC(UCBPTR),B'00100000'                          0145
         BO    @RF00145                                            0145
         TM    UCBNRY(UCBPTR),B'01000000'                          0145
         BNO   @RT00145                                            0145
*     GO TO NP1INIT;                /* ACTIVE CONSOLE INITIALIZATION */
*NP1RSTR:                           /* THIS ROUTINE PREPARES FOR NEXT
*                                      CONSOLE TEST                  */
*   UCMDVEPT=UCMENTRY;              /* RESTORE UCM ENTRY ADDRESS     */
@RF00145 DS    0H                                                  0147
NP1RSTR  LR    UCMDVEPT,UCMENTRY                                   0147
*   COMPSWH='0'B;                   /* RESET COMPOSITE CONSOLE SWITCH*/
*                                                                  0148
         NI    COMPSWH,B'11101111'                                 0148
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK FOR MASTER/ALTERNATE CONSOLE QUEUE SEARCH COMPLETE      */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0149
*   IF ALTSRCH='1'B                 /* ALTERNATE SEARCH COMPLETED ?  */
*     THEN                          /* IF SO -                       */
         TM    ALTSRCH,B'00100000'                                 0149
         BO    @RT00149                                            0149
*     GO TO NP1CMNT;                /* CONTINUE UCM ENTRIES CHECK    */
*                                                                  0150
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK FOR NEXT ALTERNATE CONSOLE ADDRESS                      */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0151
*   IF UCMALTEN=0                   /* NO MASTER ALTERNATE CONSOLE ? */
*     THEN                          /* IF NONE, THEN -               */
         L     @08,UCMALTEN(,UCMDVEPT)                             0151
         LTR   @08,@08                                             0151
         BZ    @RT00151                                            0151
*     GO TO NP1UCMS;                /* BRANCH TO UCM SEARCH ROUTINE  */
*                                                                  0152
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK THAT THIS ALTERNATE IS UNTESTED                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0153
*   IF UCMALTEN->UCMXOR='01'X       /* IF ALTERNATE UNIT IS          */
*     THEN                          /* ALREADY TESTED, THEN -        */
         CLI   UCMXOR(@08),X'01'                                   0153
         BE    @RT00153                                            0153
*     GO TO NP1UCMS;                /* BRANCH TO UCM SEARCH ROUTINE  */
*   UCMDVEPT=UCMALTEN;              /* SET POINTER TO CONSOLE MAP    */
         LR    UCMDVEPT,@08                                        0155
*   GO TO NP1ALT;                   /* BRANCH TO CONSOLE TEST ROUTINE*/
         B     NP1ALT                                              0156
*NP1UCMS:                           /* ALTERNATE SEARCH COMPLETED- 0157
*                                      PREPARE TO SEARCH UCM TABLE   */
*   ALTSRCH='1'B;                   /* SET ALTERNATE SEARCH SWTCH OFF*/
NP1UCMS  OI    ALTSRCH,B'00100000'                                 0157
*   UCMDVEPT=UCMVEA;                /* SET PTR TO 1ST ENTRY IN UCM   */
         L     UCMDVEPT,UCMVEA(,UCMBASPT)                          0158
*NP1XOR:                            /* CHECK FOR THIS UNIT ALREADY 0159
*                                      TESTED                        */
*   IF UCMXOR='00'X                 /* UNIT NOT TESTED ?             */
*     THEN                          /* IF NOT -                      */
NP1XOR   CLI   UCMXOR(UCMDVEPT),X'00'                              0159
         BE    @RT00159                                            0159
*     GO TO NP1ALT;                 /* BRANCH TO CONSOLE TEST ROUTINE*/
*NP1CMNT:                                                          0161
*   UCMDVEPT=UCMDVEPT+UCMVEZ;       /* INCREMENT TO NEXT ENTRY       */
*                                                                  0161
NP1CMNT  AL    UCMDVEPT,UCMVEZ(,UCMBASPT)                          0161
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK FOR END OF UCM TABLE                                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0162
*   IF UCMDVEPT^>UCMVEL             /* PAST END OF UCM ENTRIES ?     */
*     THEN                          /* IF NOT -                      */
         C     UCMDVEPT,UCMVEL(,UCMBASPT)                          0162
         BNH   @RT00162                                            0162
*     GO TO NP1XOR;                 /* LOOP THRU TESTS AGAIN         */
*                                                                  0163
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK FOR MASTER CONSOLE ALREADY DETERMINED                   */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0164
*   IF MSTCNFD='0'B                 /* NO MASTER CONSOLE FOUND ?     */
*     THEN                          /* IF NOT -                      */
         TM    MSTCNFD,B'01000000'                                 0164
         BNZ   @RF00164                                            0164
*     DO;                           /* INITIALIZATION CANNOT CONTINUE*/
*       NVTFLWSC=NVTFLWNC;          /* SET WAIT STATE CODE           */
*                                                                  0166
         MVI   NVTFLWSC(RNVT),X'07'                                0166
*       /*************************************************************/
*       /*                                                           */
*       /* EXIT TO THE NIPSWAIT RTN (OF IEAVNIPM)                    */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0167
*       GEN(IEAPMNIP  TYPE=SWAIT)                                  0167
*       REFS(NVTSWAIT);                                            0167
         IEAPMNIP  TYPE=SWAIT
*     END;                                                         0168
*   UCMDVEPT=UCMVEA;                /* RESET PTR TO 1ST ENTRY IN UCM */
@RF00164 L     UCMDVEPT,UCMVEA(,UCMBASPT)                          0169
*NP1RXOR:                           /* RESET UNIT TESTED FLAG FOR ALL
*                                      UCM ENTRIES                   */
*   UCMXOR='00'X;                   /* TURN OFF UNIT TESTED FLAG     */
NP1RXOR  MVI   UCMXOR(UCMDVEPT),X'00'                              0170
*   UCMDVEPT=UCMDVEPT+UCMVEZ;       /* INCREMENT TO NEXT ENTRY       */
*                                                                  0171
         AL    UCMDVEPT,UCMVEZ(,UCMBASPT)                          0171
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK FOR END OF UCM TABLE                                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0172
*   IF UCMDVEPT^>UCMVEL             /* PAST LAST UCM ENTRY ?         */
*     THEN                          /* IF NOT -                      */
         C     UCMDVEPT,UCMVEL(,UCMBASPT)                          0172
         BNH   @RT00172                                            0172
*     GO TO NP1RXOR;                /* LOOP THRU FLAG-RESET ROUTINE  */
*   RETURN;                         /* RETURN TO CALLER (IEAVNIPM)   */
@EL00001 DS    0H                                                  0174
@EF00001 DS    0H                                                  0174
@ER00001 LM    @14,@12,12(@13)                                     0174
         BR    @14                                                 0174
*NP1INIT:                           /* THIS ROUTINE BEGINS ACIVE   0175
*                                      CONSOLE INITIALIZATION        */
*                                                                  0175
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK FOR A COMPOSITE CONSOLE                                 */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0175
*   IF UCMCOMPC=0                   /* COMPOSITE CONSOLE ?           */
*     THEN                          /* IF NOT -                      */
NP1INIT  L     @11,UCMCOMPC(,UCMDVEPT)                             0175
         LTR   @11,@11                                             0175
         BZ    @RT00175                                            0175
*     GO TO NP1ACT;                 /* BRANCH TO FLAG-AS-ACTIVE RTN  */
*                                                                  0176
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK FOR SWITCH INDICATING BOTH HALVES JUST TESTED           */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0177
*   IF COMPSWH='1'B                 /* COMPOSITE CONSOLE SWITCH ON ? */
*     THEN                          /* IF SO -                       */
         TM    COMPSWH,B'00010000'                                 0177
         BNO   @RF00177                                            0177
*     DO;                           /* SET IT AS ACTIVE :            */
*       COMPSWH='0'B;               /* SET COMP CONSOLE SWITCH OFF   */
*                                                                  0179
         NI    COMPSWH,B'11101111'                                 0179
*       /*************************************************************/
*       /*                                                           */
*       /* CHECK FOR OUTPUT HALF OF COMPOSITE CONSOLE                */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0180
*       IF UCMOF='1'B               /* THIS HALF OUTPUT ?            */
*         THEN                      /* IF SO, SET POINTERS TO        */
         TM    UCMOF(UCMDVEPT),B'10000000'                         0180
         BNO   @RF00180                                            0180
*         DO;                       /* INPUT UCB, UCM                */
*           RWORK=UCBSAVED;         /* SET POINTER TO UCB            */
         L     RWORK,UCBSAVED                                      0182
*           UCBSAVED=UCBPTR;        /* FOR INPUT HALF                */
         ST    UCBPTR,UCBSAVED                                     0183
*           UCBPTR=RWORK;           /* OF THIS CONSOLE               */
         LR    UCBPTR,RWORK                                        0184
*           RWORK=UCMSAVED;         /* SET POINTER TO UCM            */
         L     RWORK,UCMSAVED                                      0185
*           UCMSAVED=UCMDVEPT;      /* ENTRY FOR INPUT HALF          */
         ST    UCMDVEPT,UCMSAVED                                   0186
*           UCMDVEPT=RWORK;         /* OF THIS CONSOLE               */
         LR    UCMDVEPT,RWORK                                      0187
*         END;                                                     0188
*                                                                  0188
*       /*************************************************************/
*       /*                                                           */
*       /* SET ACTIVE FLAG IN UCBSTAT OF OUTPUT UCB                  */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0189
*       UCBSAVED->UCBSYSR='1'B;                                    0189
*                                                                  0189
@RF00180 L     @11,UCBSAVED                                        0189
         OI    UCBSYSR(@11),B'00000010'                            0189
*       /*************************************************************/
*       /*                                                           */
*       /* SET DEVICE ACTIVE FLAG IN OUTPUT UCME                     */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0190
*       UCMSAVED->UCMUF='1'B;                                      0190
*                                                                  0190
         L     @11,UCMSAVED                                        0190
         OI    UCMUF(@11),B'00010000'                              0190
*       /*************************************************************/
*       /*                                                           */
*       /* SET OPEN PENDING FLAG IN OUTPUT UCME                      */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0191
*       UCMSAVED->UCMTA='1'B;                                      0191
*                                                                  0191
         OI    UCMTA(@11),B'00001000'                              0191
*       /*************************************************************/
*       /*                                                           */
*       /* HAS MASTER CONSOLE ALREADY BEEN FOUND                     */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0192
*       IF MSTCNFD='1'B             /* MASTER FOUND                  */
*         THEN                                                     0192
         TM    MSTCNFD,B'01000000'                                 0192
         BNO   @RF00192                                            0192
*         IF UCMSAVED->UCMALTEN=0&UCMDVEPT^=MSTRCNSL/* CONSOLE HAS NO
*                                      ALT AND IS NOT MASTER         */
*           THEN                                                   0193
         SLR   @04,@04                                             0193
         C     @04,UCMALTEN(,@11)                                  0193
         BNE   @RF00193                                            0193
         CR    UCMDVEPT,MSTRCNSL                                   0193
         BE    @RF00193                                            0193
*           IF MSTRCNSL->UCMCOMPC=0 /* IS MASTER A COMPOSITE         */
*             THEN                  /* NO SET ALTERNATE TO MASTER    */
         C     @04,UCMCOMPC(,MSTRCNSL)                             0194
         BNE   @RF00194                                            0194
*             UCMSAVED->UCMALTEN=MSTRCNSL;                         0195
         ST    MSTRCNSL,UCMALTEN(,@11)                             0195
*           ELSE                    /* YES SET ALTERNATE TO OUTPUT 0196
*                                      HALF OF MASTER                */
*             UCMSAVED->UCMOAOEN=MSTRCNSL->UCMCOMPC;               0196
         B     @RC00194                                            0196
@RF00194 L     @11,UCMCOMPC(,MSTRCNSL)                             0196
         L     @04,UCMSAVED                                        0196
         ST    @11,UCMOAOEN(,@04)                                  0196
*       GO TO NP1ACT;               /* BRANCH TO FLAG-AS-ACTIVE RTN  */
         B     NP1ACT                                              0197
*     END;                                                         0198
*                                                                  0198
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK FOR OTHER HALF OF COMPOSITE TESTED                      */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0199
*   IF UCMCOMPC->UCMXOR='01'X       /* OTHER HALF TESTED ?           */
*     THEN                          /* IF SO -                       */
@RF00177 L     @11,UCMCOMPC(,UCMDVEPT)                             0199
         CLI   UCMXOR(@11),X'01'                                   0199
         BE    @RT00199                                            0199
*     GO TO NP1RSTR;                /* BRANCH TO CHECK NEXT CONSOLE  */
*   UCMSAVED=UCMDVEPT;              /* SAVE PTR TO OTHER HALF'S UCM  */
         ST    UCMDVEPT,UCMSAVED                                   0201
*   UCBSAVED=UCBPTR;                /* SAVE PTR TO OTHER HALF'S UCB  */
         ST    UCBPTR,UCBSAVED                                     0202
*   UCMDVEPT=UCMCOMPC;              /* SET UCM ENTRY ADDRESS         */
         LR    UCMDVEPT,@11                                        0203
*   COMPSWH='1'B;                   /* SET COMP CONSOLE SWITCH ON    */
         OI    COMPSWH,B'00010000'                                 0204
*   GO TO NP1BLT;                   /* BRANCH TO CONSOLE-TEST ROUTINE*/
         B     NP1BLT                                              0205
*NP1HCPY:                                                          0206
*   UCMSYSB='1'B;                   /* SET HARDCOPY REQUIRED FLAG    */
NP1HCPY  LR    @11,UCMBASPT                                        0206
         AL    @11,@CF02045                                        0206
         L     @11,UCMMCSPT(,@11)                                  0206
         OI    UCMSYSB(@11),B'01000000'                            0206
*   GO TO NP1RSTR;                  /* BRANCH TO CHECK NEXT CONSOLE  */
         B     NP1RSTR                                             0207
*NP1ACT:                                                           0208
*   UCBSYSR='1'B;                   /* SET ACTIVE CONSOLE FLAG IN UCB*/
NP1ACT   OI    UCBSYSR(UCBPTR),B'00000010'                         0208
*   UCMTA='1'B;                     /* SET ACTIVE FLAG IN UCM        */
         OI    UCMTA(UCMDVEPT),B'00001000'                         0209
*   UCMUF='1'B;                     /* SET OPEN PENDING FLAG IN UCM  */
*                                                                  0210
         OI    UCMUF(UCMDVEPT),B'00010000'                         0210
*   /*****************************************************************/
*   /*                                                               */
*   /* IS MASTER CONSOLE ALREADY FOUND ?                             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0211
*   IF MSTCNFD='1'B                 /* MASTER CONSOLE ALREADY FOUND ?*/
*     THEN                          /* IF SO -                       */
         TM    MSTCNFD,B'01000000'                                 0211
         BNO   @RF00211                                            0211
*     DO;                           /* DOES CONSOLE HAVE ALTERNATE   */
*       IF UCMALTEN=0&UCMDVEPT^=MSTRCNSL THEN                      0213
         SLR   @11,@11                                             0213
         C     @11,UCMALTEN(,UCMDVEPT)                             0213
         BNE   @RF00213                                            0213
         CR    UCMDVEPT,MSTRCNSL                                   0213
         BE    @RF00213                                            0213
*         DO;                                                      0214
*           UCMALTEN=MSTRCNSL;                                     0215
*                                                                  0215
         ST    MSTRCNSL,UCMALTEN(,UCMDVEPT)                        0215
*           /*********************************************************/
*           /*                                                       */
*           /* SET ALTERNATE TO MASTER                               */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0216
*           IF MSTRCNSL->UCMCOMPC^=0                               0216
*                                                                  0216
*           /*********************************************************/
*           /*                                                       */
*           /* IS MASTER A COMPOSITE                                 */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0216
*             THEN                  /* MASTER IS NOT COMPOSITE       */
         L     @08,UCMCOMPC(,MSTRCNSL)                             0216
         CR    @08,@11                                             0216
         BE    @RF00216                                            0216
*             UCMOAOEN=MSTRCNSL->UCMCOMPC;                         0217
*                                                                  0217
         ST    @08,UCMOAOEN(,UCMDVEPT)                             0217
*           /*********************************************************/
*           /*                                                       */
*           /* POINT TO OUTPUT HALF OF MSTR                          */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0218
*         END;                                                     0218
*       GO TO NP1HCPY;              /* BRANCH TO SET HARDCOPY FLAG   */
         B     NP1HCPY                                             0219
*     END;                                                         0220
*                                                                  0220
*   /*****************************************************************/
*   /*                                                               */
*   /* IS THIS AN OUTPUT ONLY CONSOLE ?                              */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0221
*   IF UCMDISPD='1'B                /* THIS CONSOLE OUTPUT ONLY ?    */
*     THEN                          /* IF SO -                       */
@RF00211 TM    UCMDISPD(UCMDVEPT),B'00010000'                      0221
         BO    @RT00221                                            0221
*     GO TO NP1HCPY;                /* BRANCH TO SET HARDCOPY FLAG   */
*                                                                  0222
*   /*****************************************************************/
*   /*                                                               */
*   /* IS THIS ACTIVE MASTER SAME AS SYSGENED MASTER ?               */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0223
*   IF UCMDVEPT=MSTRCNSL            /* THE ACTIVE MASTER IS SAME AS  */
*     THEN                          /* SYS-GEN MASTER - BRANCH TO    */
         CR    UCMDVEPT,MSTRCNSL                                   0223
         BE    @RT00223                                            0223
*     GO TO NP1CMCK;                /* CHECK FOR COMPOSITE CONSOLE   */
*   UCMMCENT=UCMENTRY;              /* RESET PTR TO ACTIVE MASTER    */
         LR    @11,UCMBASPT                                        0225
         AL    @11,@CF02045                                        0225
         L     @11,UCMMCSPT(,@11)                                  0225
         ST    UCMENTRY,UCMMCENT(,@11)                             0225
*   ROLMSTR=MSTRCNSL;               /* SET PTR TO SYS-GEN (OLD)    0226
*                                      MASTER                        */
         LR    ROLMSTR,MSTRCNSL                                    0226
*   ROLMSTR->UCMUF='0'B;            /* SET DEVICE ACTIVE FLAG OFF IN
*                                      REPLACED MASTER               */
         NI    UCMUF(ROLMSTR),B'11101111'                          0227
*   ROLMSTR->UCMDISPA='0'B;         /* SET MASTER CONSOLE FLAG OFF   */
         NI    UCMDISPA(ROLMSTR),B'01111111'                       0228
*   MSTRCNSL=UCMDVEPT;              /* POINT ER TO NEW MASTER        */
*                                                                  0229
         LR    MSTRCNSL,UCMDVEPT                                   0229
*   /*****************************************************************/
*   /*                                                               */
*   /* IS THIS SYSGENED MASTER A COMPOSITE CONSOLE ?                 */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0230
*   IF ROLMSTR->UCMCOMPC^=0         /* IS IT COMPOSITE ?             */
*     THEN                          /* SET OTHER HALF'S DEVICE AND   */
         L     @11,UCMCOMPC(,ROLMSTR)                              0230
         LTR   @11,@11                                             0230
         BZ    @RF00230                                            0230
*     DO;                           /* MASTER CONSOLE FLAGS OFF      */
*                                                                  0231
*       /*************************************************************/
*       /*                                                           */
*       /* SET OTHER HALF AS NOT ACTIVE                              */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0232
*       ROLMSTR->UCMCOMPC->UCMUF='0'B;                             0232
*                                                                  0232
         NI    UCMUF(@11),B'11101111'                              0232
*       /*************************************************************/
*       /*                                                           */
*       /* SET OTHER HALF AS NOT A MASTER CONSOLE                    */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0233
*       ROLMSTR->UCMCOMPC->UCMDISPA='0'B;                          0233
         NI    UCMDISPA(@11),B'01111111'                           0233
*     END;                                                         0234
*   UCMUF='1'B;                     /* SET NEW MASTER DEVICE ACTIVE  */
@RF00230 OI    UCMUF(UCMDVEPT),B'00010000'                         0235
*   UCMDISPA='1'B;                  /* AND MASTER CONSOLE FLAGS ON   */
*                                                                  0236
         OI    UCMDISPA(UCMDVEPT),B'10000000'                      0236
*   /*****************************************************************/
*   /*                                                               */
*   /* THE FOLLOWING IF CLAUSE IS EXECUTED WHEN THE NEW ACTIVE MASTER*/
*   /* CONSOLE IS NOT COMPOSITE. THE AUTHORIZATION AND ROUTING CODES */
*   /* OF THE NEW MASTER ARE COPIED FROM THE SYS-GEN MASTER.         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0237
*   IF UCMCOMPC=0                   /* NEW MASTER NOT COMPOSITE ?    */
*     THEN                          /* IF NOT, TEST OLD MASTER       */
*                                                                  0237
         SLR   @11,@11                                             0237
         C     @11,UCMCOMPC(,UCMDVEPT)                             0237
         BNE   @RF00237                                            0237
*     /***************************************************************/
*     /*                                                             */
