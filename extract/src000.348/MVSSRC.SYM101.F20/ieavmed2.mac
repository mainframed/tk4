         TITLE 'IEAVMED2 - WQE/ORE PURGE ROUTINE                       *
                        '
IEAVMED2 CSECT ,                                                01S0001
IEECVPRG DS    0H                                               01S0001
         ENTRY IEECVPRG
@MAINENT DS    0H                                               01S0001
         USING *,@15                                            01S0001
         B     @PROLOG                                          01S0001
         DC    AL2(@EP00001-@MAINENT)
         DC    AL1(19)                                          01S0001
         DC    C'IEAVMED2R0300076049'                           01S0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                  01S0001
         BALR  @10,0                                            01S0001
@PSTART  DS    0H                                               01S0001
         USING @PSTART,@10                                      01S0001
         L     @00,@SIZDATD                                     01S0001
         GETMAIN  R,LV=(0)
         LR    @09,@01                                          01S0001
         USING @DATD,@09                                        01S0001
         ST    @13,@SA00001+4                                   01S0001
         LM    @15,@01,16(@13)                                  01S0001
         ST    @09,8(,@13)                                      01S0001
         LR    @13,@09                                          01S0001
         AH    @15,4(,@15)                                      01S0001
         BR    @15                                              01S0001
@EP00001 MVC   @PC00001(4),0(@01)                               01S0001
*                                                               03S0076
*/*                    END OF TCB                                    */
*                                                               03S0077
*   LRMPLPTR=ADDR(PARMRMPL);        /* INITIALIZE LOCAL COPY OF RMPL
*                                      ADDR                          */
         L     LRMPLPTR,@PC00001                                01S0077
*   LWKAPTR=RMPLRMWA;               /* INIT LOCAL COPY OF WORKAREA
*                                      ADDR                          */
         L     LWKAPTR,RMPLRMWA(,LRMPLPTR)                      01S0078
*   LUCMPTR=UCMBASPT;               /* INIT LOCAL ADDR OF THE UCM
*                                      BASE                          */
*                                                               01S0079
         L     @12,CVTPTR                                       01S0079
         L     LUCMPTR,UCMBASPT(,@12)                           01S0079
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS ROUTINE IS A NOOP FOR NORMAL MEMORY TERMINATION          */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0080
*   IF^(RMPLTYPE=NORMAL&RMPLTERM=MEMORY) THEN                   01S0080
         TM    RMPLTYPE(LRMPLPTR),B'10000000'                   01S0080
         BNZ   @RT00080                                         01S0080
         TM    RMPLTERM(LRMPLPTR),B'01000000'                   01S0080
         BO    @RF00080                                         01S0080
@RT00080 DS    0H                                               01S0081
*     DO;                           /* THIS IS NOT A NORMAL MEMORY
*                                      TERM.                         */
*                                                               01S0081
*       /*************************************************************/
*       /*                                                           */
*       /* SET THE LOCAL AND CMS LOCKS                               */
*       /*                                                           */
*       /*************************************************************/
*                                                               01S0082
*       CALL SETLCKS;                                           01S0082
*                                                               01S0082
         BAL   @14,SETLCKS                                      01S0082
*       /*************************************************************/
*       /*                                                           */
*       /* INIT FRR FOOTPRINT AND SAVE REGS FOR RETRY                */
*       /*                                                           */
*       /*************************************************************/
*                                                               01S0083
*       STM(REG0,REG15,RECREGS);    /* STORE REGS IN ORDER 0-15      */
         STM   REG0,REG15,RECREGS                               01S0083
*       RECRGMAP=ALLREGS;           /* INDICATE ALL REGS ARE TO BE
*                                      RESTORED AT RETRY             */
         MVC   RECRGMAP(2),@CB00163                             01S0084
*       PARMFTPT=FTPTWWB;           /* INDICATE WWB PROCESSING  01S0085
*                                      STARTED                       */
         L     @12,PARMPTR                                      01S0085
         MVI   PARMFTPT(@12),X'01'                              01S0085
*       PARMRTAD=ADDR(IEAVORE);     /* IF ERROR, SKIP TO ORE    01S0086
*                                      PROCESSING                    */
*                                                               01S0086
         LA    @08,IEAVORE                                      01S0086
         ST    @08,PARMRTAD(,@12)                               01S0086
*       /*************************************************************/
*       /*                                                           */
*       /* INITIALIZE M2SUBCHN AND M2FLAGS                           */
*       /*                                                           */
*       /*************************************************************/
*                                                               01S0087
*       M2SUBCHN=ZERO4;             /* INITIALIZE PTR TO DOMCBS FOR
*                                      THE SUBSYSTEM EXIT            */
         SLR   @12,@12                                          01S0087
         STCM  @12,15,M2SUBCHN(LWKAPTR)                         01S0087
*       M2FLAGS=ZERO1;              /* ZERO OUT THE FLAGS BYTE       */
*                                                               01S0088
         MVI   M2FLAGS(LWKAPTR),X'00'                           01S0088
*       /*************************************************************/
*       /*                                                           */
*       /* SCAN THE WQE ECB CHAIN AND REMOVE WWB-S FOR THIS TASK OR  */
*       /* MEMORY.                                                   */
*       /*                                                           */
*       /*************************************************************/
*                                                               01S0089
*       DO CHAINPTR=UCMWECBH BY ZERO WHILE(CHAINPTR^=ADDR(UCMWECBH));
         L     CHAINPTR,UCMWECBH(,LUCMPTR)                      01S0089
         B     @DE00089                                         01S0089
@DL00089 DS    0H                                               01S0090
*         CALL WWBRMOVR;            /* CHECK WWB AND REMOVE IT IF IT
*                                      BELONGS TO THIS TASK/MEMORY   */
         BAL   @14,WWBRMOVR                                     01S0090
*         CHAINPTR=M2WWBSAV;        /* PT TO NEXT WWB ON THE CHAIN   */
         ICM   CHAINPTR,15,M2WWBSAV(LWKAPTR)                    01S0091
*       END;                                                    01S0092
*                                                               01S0092
@DE00089 LA    @08,UCMWECBH(,LUCMPTR)                           01S0092
         CR    CHAINPTR,@08                                     01S0092
         BNE   @DL00089                                         01S0092
*       /*************************************************************/
*       /*                                                           */
*       /* SCAN THE ORE ECB CHAIN AND REMOVE WWB-S FOR THIS TASK OR  */
*       /* MEMORY.                                                   */
*       /*                                                           */
*       /*************************************************************/
*                                                               01S0093
*       DO CHAINPTR=UCMOECBH BY ZERO WHILE(CHAINPTR^=ADDR(UCMOECBH));
         L     CHAINPTR,UCMOECBH(,LUCMPTR)                      01S0093
         B     @DE00093                                         01S0093
@DL00093 DS    0H                                               01S0094
*         CALL WWBRMOVR;            /* CHECK WWB AND REMOVE IT IF IT
*                                      BELONGS TO THIS TASK/MEMORY.  */
         BAL   @14,WWBRMOVR                                     01S0094
*         CHAINPTR=M2WWBSAV;        /* PT TO NEXT WWB ON THE CHAIN   */
         ICM   CHAINPTR,15,M2WWBSAV(LWKAPTR)                    01S0095
*       END;                                                    01S0096
*                                                               01S0096
@DE00093 LA    @08,UCMOECBH(,LUCMPTR)                           01S0096
         CR    CHAINPTR,@08                                     01S0096
         BNE   @DL00093                                         01S0096
*       /*************************************************************/
*       /*                                                           */
*       /* SET UP THE ORE FOOTPRINT                                  */
*       /*                                                           */
*       /*************************************************************/
*                                                               01S0097
*IEAVORE:                                                       01S0097
*       PARMFTPT=FTPTORE;           /* SHOW ORE BEING PROCESSED NOW  */
IEAVORE  L     @12,PARMPTR                                      01S0097
         MVI   PARMFTPT(@12),X'02'                              01S0097
*       PARMRTAD=ADDR(IEAVWQE);     /* IF ERROR THEN SKIP TO WQE
*                                      ROUTINE                       */
*                                                               01S0098
         LA    @08,IEAVWQE                                      01S0098
         ST    @08,PARMRTAD(,@12)                               01S0098
*       /*************************************************************/
*       /*                                                           */
*       /* SCAN THE ORE CHAIN AND MARK ORE-S FOR THIS TASK/MEMORY AS */
*       /* 'DELETED'.                                                */
*       /*                                                           */
*       /*************************************************************/
*                                                               01S0099
*       DO CHAINPTR=UCMRPYQ BY ZERO WHILE(CHAINPTR^=ZERO);      01S0099
         L     CHAINPTR,UCMRPYQ(,LUCMPTR)                       01S0099
         B     @DE00099                                         01S0099
@DL00099 DS    0H                                               01S0100
*                                                               02S0100
*/********************************************************************/
*/*                                                                  */
*/*   OREPROC SEGMENT                                                */
*/*                                                                  */
*/*   FUNCTION - THIS SEGMENT PROCESSES AND PURGES ORES BELONGING TO */
*/*   THE TERMINATING TASK/MEMORY. THE ORE WILL BE MARKED AS         */
*/*   'DELETED' AND A DOMCB WILL BE BUILT WITH THE WQE-S MESSAGE ID. */
*/*                                                                  */
*/*   INPUT - CHAINPTR IS POINTING AT THE ORE TO PROCESS.            */
*/*                                                                  */
*/*   OUTPUT- THE OREBUFC BIT WILL BE SET MARKING THIS ORE FOR       */
*/*   DELETION. A DOMCB WILL BE BUILT AND ADDED TO THE DOM CHAIN.    */
*/*                                                                  */
*/*   ROUTINES - THIS SEGMENT USES THE DOMCBLD ROUTINE               */
*/*                                                                  */
*/********************************************************************/
*                                                               02S0100
*         /***********************************************************/
*         /*                                                         */
*         /* CHECK IF MEMORY IDENTIFIERS MATCH UP.                   */
*         /*                                                         */
*         /***********************************************************/
*                                                               02S0100
*         IF OREASID=RMPLASID THEN  /* DOES ORE BELONG TO THIS MEMORY*/
         LH    @08,RMPLASID(,LRMPLPTR)                          02S0100
         N     @08,@CF02556                                     02S0100
         CH    @08,OREASID(,CHAINPTR)                           02S0100
         BNE   @RF00100                                         02S0100
*           IF RMPLTERM=MEMORY      /* YES, IS THIS A MEMORY    02S0101
*                                      TERMINATION                   */
*               ×ORETCBA=RMPLTCBA   /* OR DO TCB ADDRESSES MATCH     */
*             THEN                  /* YES, PROCESS THIS ORE         */
         TM    RMPLTERM(LRMPLPTR),B'01000000'                   02S0101
         BO    @RT00101                                         02S0101
         L     @08,ORETCBA-1(,CHAINPTR)                         02S0101
         LA    @08,0(,@08)                                      02S0101
         C     @08,RMPLTCBA(,LRMPLPTR)                          02S0101
         BNE   @RF00101                                         02S0101
@RT00101 DS    0H                                               02S0102
*             DO;                                               02S0102
*                                                               02S0102
*               /*****************************************************/
*               /*                                                   */
*               /* MARK THE ORE AS 'TO BE DELETED' SO THAT REPLY AND */
*               /* D R WONT PROCESS THIS ORE.                        */
*               /*                                                   */
*               /*****************************************************/
*                                                               02S0103
*               OREBUFC=YES;        /* TURN ON 'DELETED' BIT         */
         OI    OREBUFC(CHAINPTR),B'00100000'                    02S0103
*               OREWQE->WQEDOM=YES; /* MARK THE MSG TO BE DOMED      */
*               OREWQE->WQEPURGE=YES;/* MARK THE MSG PURGABLE        */
*                                                               02S0105
         L     @08,OREWQE(,CHAINPTR)                            02S0105
         OI    WQEDOM(@08),B'10000100'                          02S0105
*               /*****************************************************/
*               /*                                                   */
*               /* BUILD A DOMCB WITH THE MESSAGE ID FROM THE RELATED*/
*               /* WQE.                                      @ZA06808*/
*               /*                                                   */
*               /*****************************************************/
*                                                               02S0106
*               M2DOMTYP=TWOMSGID;  /* ONE FOR DOM CHAIN AND ONE FOR
*                                      THE SUBSYSTEM EXIT    @ZA06808*/
         MVC   M2DOMTYP(2,LWKAPTR),@CH00078                     02S0106
*               CALL DOMCBLD(M2DOMTYP,OREWQE->WQESEQN);         02S0107
         LA    @07,M2DOMTYP(,LWKAPTR)                           02S0107
         ST    @07,@AL00001                                     02S0107
         LA    @08,WQESEQN(,@08)                                02S0107
         ST    @08,@AL00001+4                                   02S0107
         LA    @01,@AL00001                                     02S0107
         BAL   @14,DOMCBLD                                      02S0107
*               DOMCWR=YES;         /* MSG ID IS FOR A WTOR          */
*                                                               02S0108
         OI    DOMCWR(DOMCPTR),B'00000100'                      02S0108
*               /*****************************************************/
*               /*                                                   */
*               /* ADD DOMCB TO START OF CHAIN                       */
*               /*                                                   */
*               /*****************************************************/
*                                                               02S0109
*               DOMCLNKA=UCMDOME;   /* PT THIS DOMCB TO FIRST ON
*                                      CHAIN                         */
         L     @08,CVTPTR                                       02S0109
         L     @08,UCMBASPT(,@08)                               02S0109
         AL    @08,@CF02559                                     02S0109
         L     @08,UCMMCSPT(,@08)                               02S0109
         MVC   DOMCLNKA(3,DOMCPTR),UCMDOME+1(@08)               02S0109
*               UCMDOME=DOMCPTR;    /* PT DOME CHAIN HEAD AT THIS
*                                      DOMCB                         */
         ST    DOMCPTR,UCMDOME(,@08)                            02S0110
*               M2DOMBLT=YES;       /* TURN ON DOM CHAIN ADDED TO BIT*/
*                                                               02S0111
         OI    M2DOMBLT(LWKAPTR),B'00100000'                    02S0111
*               /*****************************************************/
*               /*                                                   */
*               /* LINK SECOND DOMCB TO THE SUBSYSTEM CHAIN  @ZA06808*/
*               /*                                                   */
*               /*****************************************************/
*                                                               02S0112
*               DOMCPTR=DOMCPTR+IDSIZE;                         02S0112
*                                                               02S0112
         AL    DOMCPTR,@CF00097                                 02S0112
*               /*****************************************************/
*               /*                                                   */
*               /* THE SECOND DOMCB IS RIGHT AFTER THE FIRST         */
*               /*                                                   */
*               /*****************************************************/
*                                                               02S0113
*               DOMCWR=YES;         /* MSG ID IS FOR A WTOR          */
         OI    DOMCWR(DOMCPTR),B'00000100'                      02S0113
*               DOMCLNKA=M2SUBCHN;  /* PUT NEW DOMCB ON THE CHAIN FOR*/
         MVC   DOMCLNKA(3,DOMCPTR),M2SUBCHN+1(LWKAPTR)          02S0114
*               M2SUBCHN=DOMCPTR;   /* SUBSYSTEM EXIT                */
         STCM  DOMCPTR,15,M2SUBCHN(LWKAPTR)                     02S0115
*             END;                  /* OF ORE PROCESSING             */
*         CHAINPTR=ORELKP;          /* PROCESS NEXT ORE ON THE CHAIN */
@RF00101 DS    0H                                               01S0117
@RF00100 L     CHAINPTR,ORELKP(,CHAINPTR)                       01S0117
*       END;                                                    01S0118
*                                                               01S0118
@DE00099 LTR   CHAINPTR,CHAINPTR                                01S0118
         BNZ   @DL00099                                         01S0118
*       /*************************************************************/
*       /*                                                           */
*       /* SET UP THE WQE FOOTPRINT                                  */
*       /*                                                           */
*       /*************************************************************/
*                                                               01S0119
*IEAVWQE:                                                       01S0119
*       PARMFTPT=FTPTWQE;           /* SHOW WQES BEING PROCESSED NOW */
IEAVWQE  L     @12,PARMPTR                                      01S0119
         MVI   PARMFTPT(@12),X'03'                              01S0119
*       PARMRTAD=ADDR(IEAVCALL);    /* IF ERROR, SKIP TO CALL OF
*                                      IEAVMNTR                      */
*                                                               01S0120
         LA    @08,IEAVCALL                                     01S0120
         ST    @08,PARMRTAD(,@12)                               01S0120
*       /*************************************************************/
*       /*                                                           */
*       /* SCAN THE WQE CHAIN FOR MULTI-LINE MESSAGES AND DESCRIPTOR */
*       /* CODE 7 MESSAGES. END THE MULTI-LINE AND 'DOM' THE         */
*       /* DESCRIPTOR CODE 7-S.                                      */
*       /*                                                           */
*       /*************************************************************/
*                                                               01S0121
*       DO CHAINPTR=UCMWTOQ BY ZERO WHILE(CHAINPTR^=ZERO);      01S0121
*                                                               01S0121
         L     CHAINPTR,UCMWTOQ(,LUCMPTR)                       01S0121
         B     @DE00121                                         01S0121
@DL00121 DS    0H                                               01S0122
*         /***********************************************************/
*         /*                                                         */
*         /* CHECK IF THIS MESSAGE IS A MULTI-LINE WTO               */
*         /*                                                         */
*         /***********************************************************/
*                                                               01S0122
*         IF WMJMMLW^=ZERO1 THEN    /* IS THIS WQE FOR AN MLWTO      */
         CLI   WMJMMLW(CHAINPTR),0                              01S0122
         BE    @RF00122                                         01S0122
*           DO;                     /* YES, INSURE MESSAGE IS ENDED  */
*                                                               02S0124
*/********************************************************************/
*/*   MWTOPROC SEGMENT                                               */
*/*                                                                  */
*/*   FUNCTION - PROCESS THE MULTI-LINE WTO-S. THIS SEGMENT WILL     */
*/*   CHECK THAT THIS MWTO BELONGS TO THE TERMINATING TASK/MEMORY.   */
*/*   IT THEN INSURES THAT THE MWTO IS ENDED. IF THERE IS NO ROOM    */
*/*   FOR AN ENDLINE IN ONE OF THE MINORS, IT USES AN AREA IN THE    */
*/*   MAJOR.                                                         */
*/*                                                                  */
*/*   INPUT - CHAINPTR PTS AT THE WQE BEING PROCESSED.               */
*/*                                                                  */
*/*   OUTPUT- THE MLWTO IS ENDED IF IT WAS ISSUED BY THE TERMINATING */
*/*   MEMORY OR TASK                                                 */
*/*                                                                  */
*/********************************************************************/
*                                                               02S0124
*             /*******************************************************/
*             /*                                                     */
*             /* CHECK IF MLWTO WAS ISSUED BY THIS TASK/MEMORY       */
*             /*                                                     */
*             /*******************************************************/
*                                                               02S0124
*             IF WMJMASID=RMPLASID THEN/* DO MEMORY IDS MATCH        */
         CLC   WMJMASID(2,CHAINPTR),RMPLASID(LRMPLPTR)          02S0124
         BNE   @RF00124                                         02S0124
*               IF RMPLTERM=MEMORY  /* YES. IS THIS MEMORY ABEND OR
*                                      DO                            */
*                   ×WMJMTCB=RMPLTCBA THEN/* TCBS MATCH              */
         TM    RMPLTERM(LRMPLPTR),B'01000000'                   02S0125
         BO    @RT00125                                         02S0125
         CLC   WMJMTCB(4,CHAINPTR),RMPLTCBA(LRMPLPTR)           02S0125
         BNE   @RF00125                                         02S0125
@RT00125 DS    0H                                               02S0126
*                 DO;               /* YES. END THE MLWTO            */
*                                                               02S0126
*                   /*************************************************/
*                   /*                                               */
*                   /* INSURE THAT THIS MLWTO ISN'T WAITING FOR A WQE*/
*                   /* TO BE RELEASED                                */
*                   /*                                               */
*                   /*************************************************/
*                                                               02S0127
*                   WMJMWAIT=NO;    /* TURN OFF WAITING FOR WQE BIT  */
*                                                               02S0127
         NI    WMJMWAIT(CHAINPTR),B'01111111'                   02S0127
*                   /*************************************************/
*                   /*                                               */
*                   /* INSURE THE MAJOR IS ENDED                     */
*                   /*                                               */
*                   /*************************************************/
*                                                               02S0128
*                   MINORPTR=WMJMMIN;/* POINT AT FIRST MINOR WQE     */
*                                                               02S0128
         L     MINORPTR,WMJMMIN(,CHAINPTR)                      02S0128
*                   /*************************************************/
*                   /*                                               */
*                   /* CHECK IF MINOR CAN BE USED FOR AN END LINE.   */
*                   /* THERE IS ALWAYS A MINOR WQE HUNG OFF THE MAJOR*/
*                   /*                                               */
*                   /*************************************************/
*                                                               02S0129
*                   IF WMJMMLWH=YES THEN/* 'MINOR HAS NO TEXT' SET
*                                      ON?                           */
         TM    WMJMMLWH(CHAINPTR),B'00000001'                   02S0129
         BNO   @RF00129                                         02S0129
*                     DO;           /* YES. PUT END LINE IN MINOR    */
*                       WMJMMLWH=NO;/* MINOR NOW HAS TEXT            */
*                       WMJMMLWF=YES;/* INDICATE START AT TOP OF
*                                      CHAIN                         */
         OI    WMJMMLWF(CHAINPTR),B'00000100'                   02S0132
         NI    WMJMMLWH(CHAINPTR),B'11111110'                   02S0132
*                       CALL BUILDEND;/* BUILD END LINE IN MINOR     */
         BAL   @14,BUILDEND                                     02S0133
*                     END;                                      02S0134
*                                                               02S0134
*                   /*************************************************/
*                   /*                                               */
*                   /* NO. FIRST MINOR HAS TEXT. SCAN FOR END OF     */
*                   /* MINOR CHAIN                                   */
*                   /*                                               */
*                   /*************************************************/
*                                                               02S0135
*                   ELSE                                        02S0135
*                     DO;                                       02S0135
         B     @RC00129                                         02S0135
@RF00129 DS    0H                                               02S0136
*                       M2ENDFND=NO;/* TURN OFF LOOP BIT             */
         NI    M2ENDFND(LWKAPTR),B'01111111'                    02S0136
*                       DO WHILE(M2ENDFND=NO);                  02S0137
*                                                               02S0137
         B     @DE00137                                         02S0137
@DL00137 DS    0H                                               02S0138
*                         /*******************************************/
*                         /*                                         */
*                         /* CONTINUE TO SCAN THE MINOR CHAIN UNTIL  */
*                         /* THE END IS FOUND CHECK IF 1ST LINE IN   */
*                         /* MINOR IS LAST LINE                      */
*                         /*                                         */
*                         /*******************************************/
*                                                               02S0138
*                         IF WMNMNX1=ZERO THEN/* IS SECOND MINOR NOT
*                                      USED                          */
*                                                               02S0138
         L     @08,WMNMNX1-1(,MINORPTR)                         02S0138
         LA    @08,0(,@08)                                      02S0138
         LTR   @08,@08                                          02S0138
         BNZ   @RF00138                                         02S0138
*                           /*****************************************/
*                           /*                                       */
*                           /* YES. CHECK IF LINE TYPE CONTAINS AN   */
*                           /* END                                   */
*                           /*                                       */
*                           /*****************************************/
*                                                               02S0139
*                           IF WMNMLT1D=ENDLINE THEN            02S0139
         TM    WMNMLT1D(MINORPTR),B'00010000'                   02S0139
         BNO   @RF00139                                         02S0139
*                             DO;   /* YES. END OF LOOP AND NO MINOR
*                                      NEEDED                        */
*                               M2ENDFND=YES;/* GET OUT OF LOOP      */
         OI    M2ENDFND(LWKAPTR),B'10000000'                    02S0141
*                               MINORPTR=ZERO;/* DON'T BUILD A MINOR */
         SLR   MINORPTR,MINORPTR                                02S0142
*                             END;                              02S0143
*                                                               02S0143
*                         /*******************************************/
*                         /*                                         */
*                         /* NO. THE LAST LINE DOESN'T CONTAIN AN    */
*                         /* END. PT AT SECOND HALF AS LOCATION OF   */
*                         /* WHERE TO BUILD END LINE                 */
*                         /*                                         */
*                         /*******************************************/
*                                                               02S0144
*                           ELSE                                02S0144
*                             DO;                               02S0144
         B     @RC00139                                         02S0144
@RF00139 DS    0H                                               02S0145
*                               M2ENDFND=YES;/* GET OUT OF LOOP PT
*                                      1ST HALF AT 2ND HALF          */
         OI    M2ENDFND(LWKAPTR),B'10000000'                    02S0145
*                               WMNMNX1=ADDR(WMNMUC2);          02S0146
         LA    @08,WMNMUC2(,MINORPTR)                           02S0146
         STCM  @08,7,WMNMNX1(MINORPTR)                          02S0146
*                               MINORPTR=WMNMNX1;/* PT AT LOC FOR END
*                                      LINE                          */
         LR    MINORPTR,@08                                     02S0147
*                             END;                              02S0148
*                                                               02S0148
*                         /*******************************************/
*                         /*                                         */
*                         /* NO, THE SECOND HALF IS USED             */
*                         /*                                         */
*                         /*******************************************/
*                                                               02S0149
*                         ELSE                                  02S0149
*                           DO;     /* CHECK IF THERE IS ANOTHER
*                                      MINOR                         */
         B     @RC00138                                         02S0149
@RF00138 DS    0H                                               02S0150
*                             IF WMNMNX2^=ZERO THEN             02S0150
         L     @08,WMNMNX2-1(,MINORPTR)                         02S0150
         LA    @08,0(,@08)                                      02S0150
         LTR   @08,@08                                          02S0150
         BZ    @RF00150                                         02S0150
*                               MINORPTR=WMNMNX2;/* LOOP TO CHECK
*                                      NEXT MINOR                    */
*                                                               02S0151
         LR    MINORPTR,@08                                     02S0151
*                             /***************************************/
*                             /*                                     */
*                             /* THE SECOND HALF IS THE LAST LINE.   */
*                             /* CHECK IF IT IS AN END LINE          */
*                             /*                                     */
*                             /***************************************/
*                                                               02S0152
*                             ELSE                              02S0152
*                               IF WMNMLT2D=ENDLINE THEN        02S0152
         B     @RC00150                                         02S0152
@RF00150 TM    WMNMLT2D(MINORPTR),B'00010000'                   02S0152
         BNO   @RF00152                                         02S0152
*                                 DO;/* YES THE MLWTO IS ENDED       */
*                                   M2ENDFND=YES;/* END OF CHAIN IS
*                                      FOUND                         */
         OI    M2ENDFND(LWKAPTR),B'10000000'                    02S0154
*                                   MINORPTR=ZERO;/* NO END NEEDED TO
