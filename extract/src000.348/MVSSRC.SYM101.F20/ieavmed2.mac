         SLR   GPR00F,GPR00F                                    02S0242
*           GPR01P=ADDR(M2MPL);     /* PARMLIST ADDR                 */
         LR    GPR01P,LWKAPTR                                   02S0243
*           GPR01P=GPR01P×'80000000'X;/* SET LIST FORM IND           */
         O     GPR01P,@CF02519                                  02S0244
*           SVC(2);                 /* ISSUE POST SVC                */
         SVC   2                                                02S0245
*           RESPECIFY                                           02S0246
*            (GPR00F,                                           02S0246
*             GPR01P) UNRESTRICTED;                             02S0246
*         END;                      /* POST (UCMDECB)           02S0247
*                                      MF(E,M2MPL,COMPLETE)     02S0247
*                                      ASCB(UCMASCB->ASCB)      02S0247
*                                      ERRET(POSTRECY) PUT IN ADDR OF
*                                      RECOVERY ROUTINE THAT WILL
*                                      WAKE UP THE COMMTASK IF XMPOST
*                                      DOESNT WORK.                  */
*                                                               01S0248
*       /*************************************************************/
*       /*                                                           */
*       /* CHECK IF THERE ARE ANY DOMCB-S TO BE PASSED TO THE        */
*       /* SUBSYSTEM EXIT.                                           */
*       /*                                                           */
*       /*************************************************************/
*                                                               01S0248
*       IF M2SUBCHN^=ZERO1 THEN     /* ARE THERE ANY DOMCBS ?   01S0248
*                                                            @ZA06808*/
@RF00235 SLR   @12,@12                                          01S0248
         ICM   @11,15,M2SUBCHN(LWKAPTR)                         01S0248
         CR    @11,@12                                          01S0248
         BE    @RF00248                                         01S0248
*         DO;                       /* YES. THEN TAKE THE SUBSYSTEM
*                                      EXIT                          */
*                                                               02S0250
*/********************************************************************/
*/*                                                                  */
*/*   SUBEXIT SEGMENT                                                */
*/*                                                                  */
*/*   FUNCTION - THIS SEGMENT WILL BUILD THE SUBSYSTEM INTERFACE     */
*/*   CONTROL BLOCKS AND WILL PASS THE SUBSYSTEM ALL THE DOMCBS      */
*/*   POINTED AT BY M2SUBCHN. THE DOMCBS ARE THEN FREED.             */
*/*                                                                  */
*/*   INPUT - M2SUBCHN   PTS AT A CHAIN OF DOMCBS. THE CHAIN IS NOT  */
*/*   EMPTY.                                                         */
*/*   OUTPUT - THE SUBSYSTEM EXIT IS CALLED AND THE CONTROL BLOCKS   */
*/*   ARE FREED.                                                     */
*/*                                                                  */
*/********************************************************************/
*                                                               02S0250
*           /*********************************************************/
*           /*                                                       */
*           /* FILL IN THE SSOB                                      */
*           /*                                                       */
*           /*********************************************************/
*                                                               02S0250
*           SSOBID=NAMESSOB;        /* PUT IN SSOB IDENTIFIER  Y02757*/
         MVC   SSOBID(4,LWKAPTR),@CC00107                       02S0250
*           SSOBLEN=LENGTH(SSOB);   /* FILL IN LENGTH FIELD    Y02757*/
         MVC   SSOBLEN(2,LWKAPTR),@CH00131                      02S0251
*           SSOBFUNC=SSOBDOM;       /* INDICATE DELETE OPER MSG EXIT
*                                                              Y02757*/
         MVC   SSOBFUNC(2,LWKAPTR),@CH00662                     02S0252
*           SSOBSSIB=ZERO;          /* NO SSIB                 Y02757*/
         STCM  @12,15,SSOBSSIB(LWKAPTR)                         02S0253
*           SSOBINDV=ADDR(SSDM);    /* POINT AT DOM PART       Y02757*/
*                                                               02S0254
         LA    @12,SSDM(,LWKAPTR)                               02S0254
         STCM  @12,15,SSOBINDV(LWKAPTR)                         02S0254
*           /*********************************************************/
*           /*                                                       */
*           /* FILL IN THE SSDM                                Y02757*/
*           /*                                                       */
*           /*********************************************************/
*                                                               02S0255
*           SSDMLEN=LENGTH(SSDM);   /* FILL IN LENGTH OF SSDM  Y02757*/
         MVC   SSDMLEN(2,LWKAPTR),@CH00070                      02S0255
*           SSDMDMCB=M2SUBCHN;      /* PT AT CHAIN OF DOMCBS FOR THE
*                                      SUBSYSTEM EXIT          Y02757*/
*                                                               02S0256
         STCM  @11,15,SSDMDMCB(LWKAPTR)                         02S0256
*           /*********************************************************/
*           /*                                                       */
*           /* ? IEFSSREQ (SSOB);                                    */
*           /*                                                       */
*           /*********************************************************/
*                                                               02S0257
*/*                                                                  */
*/*  PASS CONTROL TO JOB ENTRY SUBSYSTEM TO                          */
*/*  PROCESS REQUEST -                                               */
*/*                                                                  */
*/*    INPUT - REGISTER 1 POINTS TO A ONE-                           */
*/*            WORD PARAMETER LIST WHICH                             */
*/*            POINTS TO THE SSOB                                    */
*/*                                                                  */
*                                                               02S0257
*           CALL SSREQ(SSOB);       /* CALL THE SUBSYSTEM Y02757     */
*                                                               02S0257
         LA    @12,SSOB(,LWKAPTR)                               02S0257
         ST    @12,@AL00001                                     02S0257
         MVI   @AL00001,X'80'                                   02S0257
         L     @12,CVTPTR                                       02S0257
         L     @12,CVTJESCT(,@12)                               02S0257
         L     @15,JESSSREQ(,@12)                               02S0257
         LA    @01,@AL00001                                     02S0257
         BALR  @14,@15                                          02S0257
*           /*********************************************************/
*           /*                                                       */
*           /* NOW FREE THE DOMCBS ON THE SUBSYSTEM CHAIN      Y02757*/
*           /*                                                       */
*           /*********************************************************/
*                                                               02S0258
*           DO DOMCPTR=M2SUBCHN BY ZERO WHILE(DOMCPTR^=ZERO);/* 02S0258
*                                                              Y02757*/
*                                                               02S0258
         ICM   DOMCPTR,15,M2SUBCHN(LWKAPTR)                     02S0258
         B     @DE00258                                         02S0258
@DL00258 DS    0H                                               02S0259
*             /*******************************************************/
*             /*                                                     */
*             /* CHECK IF DOM BY ASID. IF SO THEN DOMCB IS 8 BYTES.  */
*             /* IF NOT THEN DOM IS 16 BYTES                   Y02757*/
*             /*                                                     */
*             /*******************************************************/
*                                                               02S0259
*             IF DOMCDBL=YES THEN   /* IS THIS A DOM BY ASID   Y02757*/
         TM    DOMCDBL(DOMCPTR),B'00000010'                     02S0259
         BNO   @RF00259                                         02S0259
*               M2DOMSIZ=ASIZE;     /* THE DOM BY ASID DOMCB IS 8
*                                      BYTES LONG.             Y02757*/
         MVI   M2DOMSIZ(LWKAPTR),X'08'                          02S0260
*             ELSE                                              02S0261
*               M2DOMSIZ=IDSIZE;    /* ALL OTHER DOMCBS ARE 16 BYTES
*                                                              Y02757*/
*                                                               02S0261
         B     @RC00259                                         02S0261
@RF00259 MVI   M2DOMSIZ(LWKAPTR),X'10'                          02S0261
*             /*******************************************************/
*             /*                                                     */
*             /* NOW LOAD REG4 AND REG7 WITH TCB AND ASCB ADDR Y02757*/
*             /*                                                     */
*             /*******************************************************/
*                                                               02S0262
*             RFY                                               02S0262
*              (REG4,                                           02S0262
*               REG7) RSTD;                                     02S0262
@RC00259 DS    0H                                               02S0263
*             REG4=PSATOLD;         /* GET ADDR OF OLD TCB FROM PSA
*                                                              Y02757*/
         L     REG4,PSATOLD                                     02S0263
*             REG7=PSAAOLD;         /* GET ADDR OF OLD ASCB    Y02757*/
         L     REG7,PSAAOLD                                     02S0264
*             M2SUBCHN=DOMCLNKA;    /* SAVE PTR TO NEXT DOMCB  Y02757*/
*                                                               02S0265
         L     @12,DOMCLNKA-1(,DOMCPTR)                         02S0265
         LA    @12,0(,@12)                                      02S0265
         STCM  @12,15,M2SUBCHN(LWKAPTR)                         02S0265
*             /*******************************************************/
*             /*                                                     */
*             /* CODE ADDED TO CHANGE BRANCH ENTRY FREEMAIN TO SVC   */
*             /*                                              YM05043*/
*             /*                                                     */
*             /*******************************************************/
*                                                               02S0266
*             DO;                   /* FREEMAIN(R) LV(M2DOMSIZ)SP(SUB
*                                      PL231)A(DOMC)                 */
*               RESPECIFY                                       02S0267
*                (GPR01F,                                       02S0267
*                 GPR00F) RESTRICTED;                           02S0267
*               GPR00F=M2DOMSIZ;    /* LENGTH REQUESTED              */
         SLR   GPR00F,GPR00F                                    02S0268
         IC    GPR00F,M2DOMSIZ(,LWKAPTR)                        02S0268
*               GPR01F=SUBPL231;    /* SP IN REG 1                   */
         LA    GPR01F,231                                       02S0269
*               GEN(SLL   GPR01F,24)                            02S0270
*               NODEFS NOREFS NOSETS NOEXIT NOFLOWS SEQFLOW;    02S0270
         SLL   GPR01F,24
*               GPR00F=GPR00F×GPR01F;/* SP IN HI BYTE                */
         OR    GPR00F,GPR01F                                    02S0271
*               GPR01F=ADDR(DOMC);  /* SET REGISTER ONE              */
         LR    GPR01F,DOMCPTR                                   02S0272
*               SVC(10);            /* R-FORM OF GETMAIN             */
         SVC   10                                               02S0273
*               RESPECIFY                                       02S0274
*                (GPR01F,                                       02S0274
*                 GPR00F) UNRESTRICTED;                         02S0274
*             END;                  /* FREEMAIN(R) LV(M2DOMSIZ)SP(SUB
*                                      PL231)A(DOMC)                 */
*             RFY                                               02S0276
*              (REG4,                                           02S0276
*               REG7) UNRSTD;                                   02S0276
*             DOMCPTR=M2SUBCHN;     /* POINT TO MEXT DOMCB ON THE CHN
*                                                              Y02757*/
         ICM   DOMCPTR,15,M2SUBCHN(LWKAPTR)                     02S0277
*           END;                    /*                         Y02757*/
@DE00258 LTR   DOMCPTR,DOMCPTR                                  02S0278
         BNZ   @DL00258                                         02S0278
*         END;                                                  01S0279
*     END;                          /* OF PROCESSING FOR IEAVMED2    */
*   RETURN;                         /* TO CALLER                     */
@EL00001 L     @13,4(,@13)                                      01S0281
@EF00001 L     @00,@SIZDATD                                     01S0281
         LR    @01,@09                                          01S0281
         FREEMAIN R,LV=(0),A=(1)
@ER00001 LM    @14,@12,12(@13)                                  01S0281
         BR    @14                                              01S0281
*                                                               02S0282
*/********************************************************************/
*/*   WWBRMOVR SUBROUTINE                                            */
*/*                                                                  */
*/*   FUNCTION - THIS ROUTINE WILL CHECK THE WWB BEING POINTED AT BY */
*/*   CHAINPTR. IF IT BELONGS TO THE TERMINATING TASK/MEMORY THE WWB */
*/*   IS REMOVED FROM THE CHAIN AND FREED.                           */
*/*                                                                  */
*/*   INPUT -  CHAINPTR PTS AT THE WWB TO BE PROCESSED.              */
*/*                                                                  */
*/*   OUTPUT-  IF THE WWB IS FREED, THE FORWARD AND BACKWARD PTRS    */
*/*   OF THE WWB-S ABOVE AND BELOW THE FREED ONE ARE UPDATED.        */
*/*   M2WWBSAV CONTAINS THE ADDRESS OF THE NEXT WWB ON THE CHAIN.    */
*/*                                                                  */
*/********************************************************************/
*                                                               02S0282
*WWBRMOVR:                                                      02S0282
*   PROC OPTIONS(NOSAVEAREA);                                   02S0282
*                                                               02S0282
         B     @EL00001                                         02S0282
WWBRMOVR STM   @14,@12,12(@13)                                  02S0282
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK IF MEMORY ADDRESSES MATCH BETWEEN TERMINATING MEMORY AND*/
*   /* WWB.                                                          */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S0283
*   M2WWBSAV=WWBFWDPT;              /* SAVE ADDR OF NEXT WWB ON THE
*                                      CHAIN                         */
         MVC   M2WWBSAV(4,LWKAPTR),WWBFWDPT(CHAINPTR)           02S0283
*   IF WWBASCB=RMPLASCB THEN        /* DOES WWB BELONG TO TERMINATING
*                                      MEMRY                         */
*                                                               02S0284
         CLC   WWBASCB(4,CHAINPTR),RMPLASCB(LRMPLPTR)           02S0284
         BNE   @RF00284                                         02S0284
*     /***************************************************************/
*     /*                                                             */
*     /* YES, CHECK IF MEMORY TERM OR IF TCB ADDRESSES MATCH         */
*     /*                                                             */
*     /***************************************************************/
*                                                               02S0285
*     IF RMPLTERM=MEMORY            /* IS THIS A MEMORY TERMINATION  */
*         ×WWBTCBAD=RMPLTCBA THEN   /* OR DO TCB ADDRESSES MATCH.
*                                      NOTE THAT WE NEED NOT CHECK
*                                      FOR TASK TERM AS THAT IS 02S0285
*                                      CERTAIN IF WE GET TO TCB 02S0285
*                                      ADDRESS TEST                  */
         TM    RMPLTERM(LRMPLPTR),B'01000000'                   02S0285
         BO    @RT00285                                         02S0285
         CLC   WWBTCBAD(4,CHAINPTR),RMPLTCBA(LRMPLPTR)          02S0285
         BNE   @RF00285                                         02S0285
@RT00285 DS    0H                                               02S0286
*       DO;                         /* YES, REMOVE AND FREE THE WWB  */
*                                                               02S0286
*         /***********************************************************/
*         /*                                                         */
*         /* TAKE THE WWB POINTED AT BY CHAINPTR OFF THE CHAIN. POINT*/
*         /* NEXT WWB AT BACK WWB                                    */
*         /*                                                         */
*         /***********************************************************/
*                                                               02S0287
*         WWBFWDPT->WWBBCKPT=WWBBCKPT;                          02S0287
*                                                               02S0287
         L     @08,WWBFWDPT(,CHAINPTR)                          02S0287
         L     @04,WWBBCKPT(,CHAINPTR)                          02S0287
         ST    @04,WWBBCKPT(,@08)                               02S0287
*         /***********************************************************/
*         /*                                                         */
*         /* NOW PT BACK WWB AT NEXT WWB                             */
*         /*                                                         */
*         /***********************************************************/
*                                                               02S0288
*         WWBBCKPT->WWBFWDPT=WWBFWDPT;                          02S0288
*                                                               02S0288
         ST    @08,WWBFWDPT(,@04)                               02S0288
*         /***********************************************************/
*         /*                                                         */
*         /* THE WWB HAS NOW BEEN REMOVED FROM THE CHAIN. FREE THE   */
*         /* WWB. LOAD REG4 AND REG7 WITH THE ADDRESSES OF THE       */
*         /* CURRENT TCB AND ASCB.                                   */
*         /*                                                         */
*         /***********************************************************/
*                                                               02S0289
*         RFY                                                   02S0289
*          (REG4,                                               02S0289
*           REG7) RSTD;                                         02S0289
*         REG4=PSATOLD;             /* GET ADDR OF OLD TCB FROM PSA  */
         L     REG4,PSATOLD                                     02S0290
*         REG7=PSAAOLD;             /* GET ADDR OF OLD ASCB FROM PSA */
         L     REG7,PSAAOLD                                     02S0291
*         DO;                       /* FREEMAIN(R) LV(LENGTH(WWB))SP(
*                                      SUBPL231)A(WWB) BRANCH        */
*           RESPECIFY                                           02S0293
*            (GPR01F,                                           02S0293
*             GPR00F) RESTRICTED;                               02S0293
*           GPR00F=LENGTH(WWB);     /* LENGTH REQUESTED              */
         LA    GPR00F,24                                        02S0294
*           GPR01F=SUBPL231;        /* SP IN REG 1                   */
         LA    GPR01F,231                                       02S0295
*           GEN(SLL   GPR01F,24)                                02S0296
*           NODEFS NOREFS NOSETS NOEXIT NOFLOWS SEQFLOW;        02S0296
         SLL   GPR01F,24
*           GPR00F=GPR00F×GPR01F;   /* SP IN HI BYTE                 */
         OR    GPR00F,GPR01F                                    02S0297
*           GPR01F=ADDR(WWB);       /* SET REGISTER ONE              */
         LR    GPR01F,CHAINPTR                                  02S0298
*           CALL CVTRMBR;           /* BRANCH ENTRY POINT            */
         L     @12,CVTPTR                                       02S0299
         L     @15,CVTRMBR(,@12)                                02S0299
         BALR  @14,@15                                          02S0299
*           RESPECIFY                                           02S0300
*            (GPR01F,                                           02S0300
*             GPR00F) UNRESTRICTED;                             02S0300
*         END;                      /* FREEMAIN(R) LV(LENGTH(WWB))SP(
*                                      SUBPL231)A(WWB) BRANCH        */
*         RFY                                                   02S0302
*          (REG4,                                               02S0302
*           REG7) UNRSTD;           /* REG4 & REG7 NO LONGER NEEDED  */
*       END;                                                    02S0303
*   END WWBRMOVR;                                               02S0304
@EL00002 DS    0H                                               02S0304
@EF00002 DS    0H                                               02S0304
@ER00002 LM    @14,@12,12(@13)                                  02S0304
         BR    @14                                              02S0304
*                                                               02S0305
*/********************************************************************/
*/*        DOMCBLD ROUTINE                                           */
*/*                                                                  */
*/*        FUNCTION - THIS ROUTINE WILL BUILD EITHER ONE OR TWO      */
*/*        DOM CONTROL BLOCKS. IF TWO ARE BUILT, THE SECOND WILL BE  */
*/*        A DUPLICATE OF THE FIRST.                                 */
*/*                                                                  */
*/*        INPUT - THIS ROUTINE HAS TWO INPUT PARAMETERS 'BLDTYPE'   */
*/*        AND 'BLDID'. THE VALUES FOR THESE PARAMETERS ARE AS       */
*/*        FOLLOWS:                                                  */
*/*          PARAMETER      CONSTANT  VALUE    MEANING               */
*/*                         NAME                                     */
*/*                                                                  */
*/*          BLDTYPE        ONEMSGID   1   BUILD A DOMCB THAT IS 16  */
*/*                                        BYTES LONG WITH A MSG ID  */
*/*                         TWOMSGID   2   BUILD TWO IDENTICAL DOMCBS*/
*/*                                       THAT ARE EACH 16 BYTES LONG*/
*/*                         ASIDONLY   3   BUILD TWO 8 BYTE DOMCBS   */
*/*                                        WITH ASID                 */
*/*                         ONEASID    4   BUILD ONE DOMCB OF 8 BYTES*/
*/*                                        WITH ASID ONLY            */
*/*                         ASIDJTCB   5   BUILD TWO DOMCBS EACH 16  */
*/*                                        BYTES LONG WITH ASID &    */
*/*                                        JSTCB ADDRESS             */
*/*                         ONEAJ      6   BUILD ONE DOM BY ASID AND */
*/*                                        JOBSTEP TCB.              */
*/*                                                          @ZA06808*/
*/*          BLDID        CONTAINS THE MESSAGE ID OF THE MESSAGE TO  */
*/*                       BE DOMED BY THE DOMCB. IT WILL BE ZERO FOR */
*/*                       TYPES 3,4,5&6                      @ZA06808*/
*/*        OUTPUT - THIS ROUTINE WILL BUILD THE REQUIRED CONTROL     */
*/*        BLOCK(S) AND RETURN THE ADDRESS IN DOMCPTR.               */
*/*                                                          @ZA06808*/
*/********************************************************************/
*                                                               02S0305
*DOMCBLD:                                                       02S0305
*   PROC(BLDTYPE,BLDID) OPTIONS(NOSAVEAREA);                    02S0305
DOMCBLD  STM   @14,@05,12(@13)                                  02S0305
         STM   @07,@12,48(@13)                                  02S0305
         MVC   @PC00003(8),0(@01)                               02S0305
*   DCL                                                         02S0306
*     BLDTYPE FIXED(15),            /* TYPE OF DOMCB TO BUILD. ALSO
*                                      USED TO DETERMINE SIZE        */
*     BLDID PTR(24);                /* MESSAGE ID OF MESSAGE TO BE
*                                      DELETED                       */
*   RFY                                                         02S0307
*    (REG4,                                                     02S0307
*     REG7) RSTD;                   /* REG 4 AND 7 ARE NEEDED FOR
*                                      GETMAIN                       */
*   REG4=PSATOLD;                   /* PUT IN ADDR OF OUR TCB        */
         L     REG4,PSATOLD                                     02S0308
*   REG7=PSAAOLD;                   /* PUT IN ADDR OF OUR ASCB       */
         L     REG7,PSAAOLD                                     02S0309
*                                                               02S0310
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK IF THIS REQUEST IS TO BUILD A DOMCB WITH A MSGID. IF SO */
*   /* BUILD IT                                                      */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S0310
*   IF BLDTYPE=ONEMSGID×            /* IS REQUEST FOR ONE OR @ZA06808*/
*       BLDTYPE=TWOMSGID THEN       /* TWO MSGID DOMCBS ?    @ZA06808*/
         L     @12,@PC00003                                     02S0310
         LH    @12,BLDTYPE(,@12)                                02S0310
         C     @12,@CF00060                                     02S0310
         BE    @RT00310                                         02S0310
         C     @12,@CF00078                                     02S0310
         BNE   @RF00310                                         02S0310
@RT00310 DS    0H                                               02S0311
*     DO;                           /* YES. GET CORE FOR THE DOMCBS  */
*       IF BLDTYPE=ONEMSGID THEN    /* ONE DOMCB TO BE BUILT ?  02S0312
*                                                            @ZA06808*/
         L     @12,@PC00003                                     02S0312
         CLC   BLDTYPE(2,@12),@CH00060                          02S0312
         BNE   @RF00312                                         02S0312
*         M2DOMSIZ=IDSIZE*ONE;      /* YES. GET 16 BYTES     @ZA06808*/
         MVI   M2DOMSIZ(LWKAPTR),X'10'                          02S0313
*       ELSE                        /* TWO DOMCBS TO BE BUILT   02S0314
*                                                            @ZA06808*/
*         M2DOMSIZ=IDSIZE*TWO;      /* GET 32 BYTES          @ZA06808*/
         B     @RC00312                                         02S0314
@RF00312 MVI   M2DOMSIZ(LWKAPTR),X'20'                          02S0314
*       DO;                         /* GETMAIN (R)              02S0315
*                                      LV(M2DOMSIZ)SP(SUBPL231) 02S0315
*                                      BRANCH                        */
@RC00312 DS    0H                                               02S0316
*         RESPECIFY                                             02S0316
*          (GPR01F,                                             02S0316
*           GPR00F) RESTRICTED;                                 02S0316
*         GPR00F=M2DOMSIZ;          /* LENGTH REQUESTED              */
         SLR   GPR00F,GPR00F                                    02S0317
         IC    GPR00F,M2DOMSIZ(,LWKAPTR)                        02S0317
*         GPR01F=SUBPL231;          /* SP IN REG 1                   */
         LA    GPR01F,231                                       02S0318
*         GEN(SLL   GPR01F,24)                                  02S0319
*         NODEFS NOREFS NOSETS NOEXIT NOFLOWS SEQFLOW;          02S0319
         SLL   GPR01F,24
*         GPR00F=GPR00F×GPR01F;     /* SP IN HIGH BYTE               */
         OR    GPR00F,GPR01F                                    02S0320
*         GPR01F=-1;                /* INDICATE A GETMAIN REQUEST    */
         SLR   GPR01F,GPR01F                                    02S0321
         BCTR  GPR01F,0                                         02S0321
*         CALL CVTRMBR;             /* BRANCH ENTRY POINT            */
         L     @12,CVTPTR                                       02S0322
         L     @15,CVTRMBR(,@12)                                02S0322
         BALR  @14,@15                                          02S0322
*         RESPECIFY                                             02S0323
*          (GPR01F,                                             02S0323
*           GPR00F) UNRESTRICTED;                               02S0323
*       END;                        /* GETMAIN (R)              02S0324
*                                      LV(M2DOMSIZ)SP(SUBPL231) 02S0324
*                                      BRANCH                        */
*                                                               02S0324
*       /*************************************************************/
*       /*                                                           */
*       /* ZERO THE AREA OF ONE DOMCB                                */
*       /*                                                           */
*       /*************************************************************/
*                                                               02S0325
*       DOMCPTR=REG1;               /* SET UP PTR TO THE AREA        */
         LR    DOMCPTR,REG1                                     02S0325
*       DOM4WORD=NULLB;             /* INSURE BLOCK IS ZERO          */
         XC    DOM4WORD+1(15,DOMCPTR),DOM4WORD+1(DOMCPTR)       02S0326
         MVI   DOM4WORD(DOMCPTR),B'00000000'                    02S0326
*       DOMCEND=YES;                /* INDICATE ONLY ONE MSGID IN
*                                      DOMCB                         */
         LA    @12,4                                            02S0327
         LR    @08,DOMCPTR                                      02S0327
         ALR   @08,@12                                          02S0327
         OI    DOMCEND(@08),B'10000000'                         02S0327
*       DOMCIDA=BLDID;              /* MOVE IN MESSAGE ID OF MSG TO
*                                      DELETE                        */
         LR    @08,DOMCPTR                                      02S0328
         ALR   @08,@12                                          02S0328
         L     @15,@PC00003+4                                   02S0328
         MVC   DOMCIDA(3,@08),BLDID(@15)                        02S0328
*       DOMCSIZE=IDSIZE;            /* FILL IN LENGTH OF THE DOMCB   */
         LR    @08,DOMCPTR                                      02S0329
         ALR   @08,@12                                          02S0329
         MVI   DOMCSIZE(@08),X'10'                              02S0329
*       DOMCASID=RMPLASID;          /* FILL IN THE ASID FROM THE RMPL*/
*                                                               02S0330
         LR    @08,DOMCPTR                                      02S0330
         ALR   @08,@12                                          02S0330
         LH    @15,RMPLASID(,LRMPLPTR)                          02S0330
         N     @15,@CF02556                                     02S0330
         STH   @15,DOMCASID(,@08)                               02S0330
*       /*************************************************************/
*       /*                                                           */
*       /* AND ALSO THE JOBSTEP TCB ADDR                             */
*       /*                                                           */
*       /*************************************************************/
*                                                               02S0331
*       IF RMPLTERM^=MEMORY THEN    /* DO NOT MOVE IN JSTCB ADDR IF
*                                      THIS IS MEMORY TERMINATION
*                                                            @ZA06808*/
         TM    RMPLTERM(LRMPLPTR),B'01000000'                   02S0331
         BO    @RF00331                                         02S0331
*         DOMCJTCB=RMPLTCBA->TCBJSTCB;                          02S0332
*                                                               02S0332
         ALR   @12,DOMCPTR                                      02S0332
         L     @08,RMPLTCBA(,LRMPLPTR)                          02S0332
         L     @08,TCBJSTCB(,@08)                               02S0332
         ST    @08,DOMCJTCB(,@12)                               02S0332
*       /*************************************************************/
*       /*                                                           */
*       /* CHECK IF EXTRA DOMCB TO BE BUILT FOR SUBSYSTEM            */
*       /*                                                           */
*       /*************************************************************/
*                                                               02S0333
*       IF BLDTYPE=TWOMSGID THEN    /* DO WE BUILD THE SECOND DOMCB  */
*                                                               02S0333
@RF00331 L     @12,@PC00003                                     02S0333
         CLC   BLDTYPE(2,@12),@CH00078                          02S0333
         BNE   @RF00333                                         02S0333
