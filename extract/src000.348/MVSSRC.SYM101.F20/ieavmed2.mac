*/*   INPUT - CHAINPTR PTS AT WQE TO HANDLE                          */
*/*                                                                  */
*/*   OUTPUT - WQEDOM BIT TURNED ON IF WQE IS TO BE DELETED AT       */
*/*   JOB STEP TERM. DOMCB BUILT FOR SUBSYSTEM.                      */
*/*                                                                  */
*/********************************************************************/
*                                                               02S0173
*         IF WQEASID=RMPLASID       /* DO MEMORY IDS MATCH           */
*             &(WQEJSTCB=RMPLTCBA   /* AND TCB ADDR MATCH FOR JOB
*                                      STEP TERM                     */
*             ×RMPLTERM=MEMORY)     /* OR THIS IS A MEMORY      02S0173
*                                      TERMINATION                   */
*             &WQEDCG=YES           /* AND IS THIS A DESC CODE 7
*                                      MESSAGE                       */
*             &WQEDOM=NO THEN       /* AND NOT DELETED ALREADY BY
*                                      OREPROC                       */
@RF00122 CLC   WQEASID(2,CHAINPTR),RMPLASID(LRMPLPTR)           02S0173
         BNE   @RF00173                                         02S0173
         CLC   WQEJSTCB(4,CHAINPTR),RMPLTCBA(LRMPLPTR)          02S0173
         BE    @GL00004                                         02S0173
         TM    RMPLTERM(LRMPLPTR),B'01000000'                   02S0173
         BNO   @RF00173                                         02S0173
@GL00004 TM    WQEDCG(CHAINPTR),B'00000010'                     02S0173
         BNO   @RF00173                                         02S0173
         TM    WQEDOM(CHAINPTR),B'00000100'                     02S0173
         BNZ   @RF00173                                         02S0173
*           DO;                     /* YES.                          */
*             WQEDOM=YES;           /* SET DOM FLAG IN WQE           */
*             WQEPURGE=YES;         /* MARK THE MSG PURGABLE         */
         OI    WQEDOM(CHAINPTR),B'10000100'                     02S0176
*             IF WMJMMLW=ZERO1 THEN /* IS THIS A SINGLE LINE WTO     */
         CLI   WMJMMLW(CHAINPTR),0                              02S0177
         BNE   @RF00177                                         02S0177
*               DO;                 /* YES.                          */
*                 CALL DOMCBLD(ONEMSGID,WQESEQN);/* BUILD A DOMCB FOR
*                                      SUBSYSTEM             @ZA06808*/
         LA    @08,@CH00060                                     02S0179
         ST    @08,@AL00001                                     02S0179
         LA    @08,WQESEQN(,CHAINPTR)                           02S0179
         ST    @08,@AL00001+4                                   02S0179
         LA    @01,@AL00001                                     02S0179
         BAL   @14,DOMCBLD                                      02S0179
*                 DOMCLNKA=M2SUBCHN;/* CHAIN TO SUBSYSTEM DOMCB CHAIN*/
         MVC   DOMCLNKA(3,DOMCPTR),M2SUBCHN+1(LWKAPTR)          02S0180
*                 M2SUBCHN=DOMCPTR; /* PT HEAD OF CHAIN AT NEW DOM
*                                      BLOCK                         */
         STCM  DOMCPTR,15,M2SUBCHN(LWKAPTR)                     02S0181
*               END;                                            02S0182
*           END;                                                02S0183
@RF00177 DS    0H                                               02S0184
