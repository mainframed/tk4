         B     @RC00194                                         02S0204
@RF00194 DS    0H                                               02S0205
*             CALL DOMCBLD(ONEASID,ZERO3);/* BUILD A DOMCB WITH ASID
*                                      ONLY                  @ZA06808*/
         LA    @01,@AL00205                                     02S0205
         BAL   @14,DOMCBLD                                      02S0205
*             DOMCLNKA=M2SUBCHN;    /* PUT THE DOMCB ON THE  @ZA06808*/
         MVC   DOMCLNKA(3,DOMCPTR),M2SUBCHN+1(LWKAPTR)          02S0206
*             M2SUBCHN=DOMCPTR;     /* SUBSYSTEM CHAIN       @ZA06808*/
         STCM  DOMCPTR,15,M2SUBCHN(LWKAPTR)                     02S0207
*           END;                    /*                       @ZA06808*/
*       ELSE                        /* NO, THIS IS A TASK       02S0209
*                                      TERMINATION. CHECK IF FOR A
*                                      JOB STEP                      */
*         IF RMPLJST=YES THEN       /* IS TERMINATION FOR A JOB STEP */
         B     @RC00193                                         02S0209
@RF00193 TM    RMPLJST(LRMPLPTR),B'00000100'                    02S0209
         BNO   @RF00209                                         02S0209
*           IF M2GRAPHC=YES THEN    /* IS THERE AN ACTIVE GRAPHIC
*                                      CONSOLE                       */
         TM    M2GRAPHC(LWKAPTR),B'00010000'                    02S0210
         BNO   @RF00210                                         02S0210
*             DO;                   /* YES. BUILD A DOM BY ASID AND
*                                      JSTCB                         */
*               CALL DOMCBLD(ASIDJTCB,ZERO3);                   02S0212
         LA    @01,@AL00212                                     02S0212
         BAL   @14,DOMCBLD                                      02S0212
*               DOMCLNKA=UCMDOME;   /* PUT DOMCB ON UCM CHAIN        */
         L     @12,CVTPTR                                       02S0213
         L     @12,UCMBASPT(,@12)                               02S0213
         AL    @12,@CF02559                                     02S0213
         L     @12,UCMMCSPT(,@12)                               02S0213
         MVC   DOMCLNKA(3,DOMCPTR),UCMDOME+1(@12)               02S0213
*               UCMDOME=DOMCPTR;                                02S0214
         ST    DOMCPTR,UCMDOME(,@12)                            02S0214
*               DOMCPTR=DOMCPTR+AJSIZE;/* PT AT SUBSYSTEM COPY       */
         AL    DOMCPTR,@CF00097                                 02S0215
*               DOMCLNKA=M2SUBCHN;  /* PUT THE DOMCB ON SUBSYSTEM
*                                      CHAIN                         */
         MVC   DOMCLNKA(3,DOMCPTR),M2SUBCHN+1(LWKAPTR)          02S0216
*               M2SUBCHN=DOMCPTR;                               02S0217
         STCM  DOMCPTR,15,M2SUBCHN(LWKAPTR)                     02S0217
*               M2DOMBLT=YES;       /* A DOMCB WAS PUT ON THE UCMDOME
*                                      CHAIN                         */
         OI    M2DOMBLT(LWKAPTR),B'00100000'                    02S0218
