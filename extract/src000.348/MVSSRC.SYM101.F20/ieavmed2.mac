         L     @12,ASCBPTR(,LRMPLPTR)                           01S0227
         L     @12,ASCBTSB(,@12)                                01S0227
         LTR   @12,@12                                          01S0227
         BZ    @RF00227                                         01S0227
*         DO;                       /* YES. PASS AN MPL TO THE  01S0228
