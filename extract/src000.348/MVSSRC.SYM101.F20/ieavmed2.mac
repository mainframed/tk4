         LA    @12,8                                            02S0358
         ALR   @12,DOMCPTR                                      02S0358
         MVC   DOM2WORD(8,@12),DOM2WORD(DOMCPTR)                02S0358
*       END;                                                    02S0359
*     ELSE                          /* THIS IS A DOM BY ASID & JSTCB
*                                      FOR ONE OR TWO DOMCBS @ZA06808*/
*       DO;                         /*                       @ZA06808*/
         B     @RC00336                                         02S0360
@RF00336 DS    0H                                               02S0361
*         IF BLDTYPE=ONEAJ THEN     /* ONE DOMCBS WANTED BUILT ?
*                                                            @ZA06808*/
         L     @12,@PC00003                                     02S0361
         CLC   BLDTYPE(2,@12),@CH00086                          02S0361
         BNE   @RF00361                                         02S0361
*           M2DOMSIZ=AJSIZE*ONE;    /* GET 16 BYTES          @ZA06808*/
         MVI   M2DOMSIZ(LWKAPTR),X'10'                          02S0362
*         ELSE                      /* TWO DOMCBS WANTED     @ZA06808*/
*           M2DOMSIZ=AJSIZE*TWO;    /* GET 32 BYTES          @ZA06808*/
         B     @RC00361                                         02S0363
@RF00361 MVI   M2DOMSIZ(LWKAPTR),X'20'                          02S0363
*         DO;                       /* GETMAIN (R)              02S0364
*                                      LV(M2DOMSIZ)SP(SUBPL231) 02S0364
*                                      BRANCH                        */
@RC00361 DS    0H                                               02S0365
*           RESPECIFY                                           02S0365
*            (GPR01F,                                           02S0365
*             GPR00F) RESTRICTED;                               02S0365
*           GPR00F=M2DOMSIZ;        /* LENGTH REQUESTED              */
         SLR   GPR00F,GPR00F                                    02S0366
         IC    GPR00F,M2DOMSIZ(,LWKAPTR)                        02S0366
*           GPR01F=SUBPL231;        /* SP IN REG 1                   */
         LA    GPR01F,231                                       02S0367
*           GEN(SLL   GPR01F,24)                                02S0368
*           NODEFS NOREFS NOSETS NOEXIT NOFLOWS SEQFLOW;        02S0368
         SLL   GPR01F,24
*           GPR00F=GPR00F×GPR01F;   /* SP IN HIGH BYTE               */
         OR    GPR00F,GPR01F                                    02S0369
*           GPR01F=-1;              /* INDICATE A GETMAIN REQUEST    */
         SLR   GPR01F,GPR01F                                    02S0370
         BCTR  GPR01F,0                                         02S0370
*           CALL CVTRMBR;           /* BRANCH ENTRY POINT            */
         L     @12,CVTPTR                                       02S0371
         L     @15,CVTRMBR(,@12)                                02S0371
         BALR  @14,@15                                          02S0371
*           RESPECIFY                                           02S0372
*            (GPR01F,                                           02S0372
*             GPR00F) UNRESTRICTED;                             02S0372
*         END;                      /* GETMAIN (R)              02S0373
*                                      LV(M2DOMSIZ)SP(SUBPL231) 02S0373
*                                      BRANCH                @ZA06808*/
*         DOMCPTR=REG1;             /* SAVE LOCATION                 */
         LR    DOMCPTR,REG1                                     02S0374
*         DOM4WORD=NULLB;           /* ZERO DOMCB                    */
         XC    DOM4WORD+1(15,DOMCPTR),DOM4WORD+1(DOMCPTR)       02S0375
         MVI   DOM4WORD(DOMCPTR),B'00000000'                    02S0375
*         DOMCDBAJ=YES;             /* DOM BY ASID AND JSTCB ADDR    */
         OI    DOMCDBAJ(DOMCPTR),B'00001000'                    02S0376
*         DOMCASID=RMPLASID;        /* GET ASID FOR THIS MEMORY      */
         LH    @12,RMPLASID(,LRMPLPTR)                          02S0377
         N     @12,@CF02556                                     02S0377
         STH   @12,DOMCASID(,DOMCPTR)                           02S0377
*         DOMCSIZE=AJSIZE;          /* PUT IN SIZE FOR FREEMAIN      */
         MVI   DOMCSIZE(DOMCPTR),X'10'                          02S0378
*         DOMCJTCB=RMPLTCBA;        /* PUT IN TCB ADDR OF THIS JOB
*                                      STEP                          */
         L     @12,RMPLTCBA(,LRMPLPTR)                          02S0379
         ST    @12,DOMCJTCB(,DOMCPTR)                           02S0379
