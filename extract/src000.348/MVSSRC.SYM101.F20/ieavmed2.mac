         SLR   GPR01F,GPR01F                                    02S0348
         BCTR  GPR01F,0                                         02S0348
*           CALL CVTRMBR;           /* BRANCH ENTRY POINT            */
         L     @12,CVTPTR                                       02S0349
         L     @15,CVTRMBR(,@12)                                02S0349
         BALR  @14,@15                                          02S0349
*           RESPECIFY                                           02S0350
