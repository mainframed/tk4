*                           END;    /* OF 2ND HALF PROCESSING        */
@RC00152 DS    0H                                               02S0163
@RC00150 DS    0H                                               02S0164
*                       END;        /* OF DO WHILE(ENDFOUND = NO)
*                                      LOOP                          */
*                                                               02S0164
@RC00138 DS    0H                                               02S0164
@DE00137 TM    M2ENDFND(LWKAPTR),B'10000000'                    02S0164
         BZ    @DL00137                                         02S0164
