*               &UCMTA=NO           /* OPEN PENDING OFF      @ZA00065*/
*               &UCMDISPG=NO THEN   /* AND NOT STATUS ONLY    YM03456*/
         TM    UCMDISPC(CHAINPTR),B'00100000'                   02S0188
         BNO   @RF00188                                         02S0188
         TM    UCMUF(CHAINPTR),B'00010000'                      02S0188
         BNO   @RF00188                                         02S0188
         TM    UCMTA(CHAINPTR),B'00001000'                      02S0188
         BNZ   @RF00188                                         02S0188
         TM    UCMDISPG(CHAINPTR),B'00000010'                   02S0188
         BNZ   @RF00188                                         02S0188
*             DO;                                               02S0189
*               M2GRAPHC=YES;       /* INDICATE GRAPHIC DEVICE WAS
*                                      FOUND.                        */
         OI    M2GRAPHC(LWKAPTR),B'00010000'                    02S0190
*             END;                                              02S0191
*         END;                                                  02S0192
@RF00188 AL    CHAINPTR,UCMVEZ(,LUCMPTR)                        02S0192
@DE00187 C     CHAINPTR,UCMVEL(,LUCMPTR)                        02S0192
         BNH   @DL00187                                         02S0192
*       IF RMPLTERM=MEMORY THEN     /* IS THIS A MEMORY TERMINATION
*                                                            @ZA06808*/
@RF00186 TM    RMPLTERM(LRMPLPTR),B'01000000'                   02S0193
         BNO   @RF00193                                         02S0193
*         IF M2GRAPHC=YES THEN      /* IS THERE AN ACTIVE GRAPHIC
*                                      CONSOLE ?             @ZA06808*/
         TM    M2GRAPHC(LWKAPTR),B'00010000'                    02S0194
         BNO   @RF00194                                         02S0194
*           DO;                     /* YES. BUILD 2 DOMS BY ASID &
*                                      PUT THEM ON THE UCMDOME AND
*                                      SUBSYSTEM CHAINS      @ZA06808*/
*             CALL DOMCBLD(ASIDONLY,ZERO3);/* BUILD A DOM BY ASID    */
         LA    @01,@AL00196                                     02S0196
         BAL   @14,DOMCBLD                                      02S0196
