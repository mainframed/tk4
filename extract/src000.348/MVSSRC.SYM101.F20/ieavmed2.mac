         IC    GPR00F,M2DOMSIZ(,LWKAPTR)                        02S0344
*           GPR01F=SUBPL231;        /* SP IN REG 1                   */
         LA    GPR01F,231                                       02S0345
*           GEN(SLL   GPR01F,24)                                02S0346
