         L     @12,CVTPTR                                       02S0197
         L     @12,UCMBASPT(,@12)                               02S0197
         AL    @12,@CF02559                                     02S0197
         L     @12,UCMMCSPT(,@12)                               02S0197
         MVC   DOMCLNKA(3,DOMCPTR),UCMDOME+1(@12)               02S0197
