*/*                                                                  */
*/*  SETLCKS SEGMENT                                                 */
*/*                                                                  */
*/*  FUNCTION: THIS SEGMENT WILL OBTAIN THE LOCAL AND CMS LOCKS.     */
*/*  THE SETLOCK ROUTINE DESTROYS REGISTERS 11 TO 14. REG 13 WILL BE */
*/*  SAVED AND RESTORED. REGISTERS 11 AND 12 SHOULD NOT BE USED AS   */
*/*  GLOBAL PTRS. THIS ROUTINE ALSO SETS UP THE FRR AND USER PARM    */
*/*                                                                  */
*/*  INPUT - NONE                                                    */
*/*                                                                  */
*/*  OUTPUT- NONE                                                    */
*/*                                                                  */
*/********************************************************************/
*                                                               02S0394
*SETLCKS:                                                       02S0394
*   PROC OPTIONS(NOSAVEAREA);                                   02S0394
SETLCKS  STM   @14,@12,12(@13)                                  02S0394
*   RFY                                                         02S0395
*     REG0 RSTD;                    /* REG0 WILL HOLD REG13    Y02751*/
*   REG0=REG13;                     /* SAVE PTR TO SAVEAREA    Y02751*/
         LR    REG0,REG13                                       02S0396
*   GENERATE REFS(PSALITA,FLC) SETS(REG11,REG12,REG13,REG14);   02S0397
*                                                               02S0397
*                                                          /*  Y02751*/
         SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,                        X
               RELATED=(UCM,IEAVMED2(FRELCKS))
         SETLOCK OBTAIN,TYPE=CMS,MODE=UNCOND,                          X
               RELATED=(UCM,IEAVMED2(FRELCKS))
*   /*****************************************************************/
*   /*                                                               */
*   /* RESTORE THE REGISTERS FROM THE SETLOCK                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S0398
*   REG13=REG0;                     /* RESTORE SAVEAREA PTR    Y02751*/
         LR    REG13,REG0                                       02S0398
*   GEN(  LM REG14,REG12,12(REG13));/* RESET REGS FROM STORAGE  02S0399
*                                      LOCATION USED BY PROC PROLOG.
*                                      THIS IS DONE SO THAT REGS USED
*                                      BY OPTIMIZER WILL BE RESET.   */
*                                                               02S0399
           LM REG14,REG12,12(REG13)
*   /*****************************************************************/
*   /*                                                               */
*   /* NOW SET UP THE FRR PROTECTION FOR THIS MODULE.                */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S0400
*   RFY                                                         02S0400
*     REG1 RSTD;                    /* WE NEED REG 1                 */
*   REG1=UCMFRRAD;                  /* GET ADDR OF IEAVMFRR FROM CUM */
         L     REG1,UCMFRRAD(,LUCMPTR)                          02S0401
*   GEN REFS(PSACSTK,FRRSCURR,FRRSLAST,PSALSFCC,FRRSENTR,FRRSELEN,
*       FRRSFRRA,FRRSPARM);                                     02S0402
*                                                               02S0402
         SETFRR A,FRRAD=(REG1),PARMAD=PARMPTR,WRKREGS=(REG11,REG12)
*   /*****************************************************************/
*   /*                                                               */
*   /* FRR USES REGS 11 AND 12 AS WORK REGS. THE PARM LIST PTED AT BY*/
*   /* PARMPTR IS ZEROED BY SETFRR                                   */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S0403
*   PARMRGAD=ADDR(RECSAVE);         /* PUT ADDR OF SAVE AREA INTO
*                                      PARM LIST                     */
         L     @12,PARMPTR                                      02S0403
         LA    @11,RECSAVE                                      02S0403
         ST    @11,PARMRGAD(,@12)                               02S0403
*   PARMID=MODULEID;                /* INSERT LAST FOUR LETTERS OF
*                                      NAME                          */
         MVC   PARMID(4,@12),@CC00105                           02S0404
*   RFY                                                         02S0405
*    (REG0,                                                     02S0405
*     REG1) UNRSTD;                 /*                         Y02751*/
*   END;                                                        02S0406
@EL00005 DS    0H                                               02S0406
@EF00005 DS    0H                                               02S0406
@ER00005 LM    @14,@12,12(@13)                                  02S0406
         BR    @14                                              02S0406
*                                                               02S0407
*/********************************************************************/
*/*                                                                  */
*/*  FRELCKS SEGMENT                                                 */
*/*                                                                  */
*/*  FUNCTION: THIS SEGMENT WILL FREE THE CMS AND LOCAL LOCKS, THE   */
*/*  SETLOCK ROUTINE WILL DESTROY REGISTERS 11 THRU 14 SO REG13 IS   */
*/*  SAVED ACROSS THE SETLOCK. THIS SEGMENT WILL ALSO FREE THE FRR   */
*/* USED TO PROTECT THIS MODULE.                                     */
*/*                                                                  */
*/*  INPUT - NONE                                                    */
*/*                                                                  */
*/*  OUTPUT - NONE                                                   */
*/*                                                                  */
*/********************************************************************/
*                                                               02S0407
*FRELCKS:                                                       02S0407
*   PROC OPTIONS(NOSAVEAREA);                                   02S0407
FRELCKS  STM   @14,@12,12(@13)                                  02S0407
*   RFY                                                         02S0408
*     REG0 RSTD;                    /* REG0 WILL BE USED TO HOLD
*                                      SAVEAREA PTR            Y02751*/
*   REG0=REG13;                     /* SAVEAREA PTR GOES IN REG0
*                                                              Y02751*/
         LR    REG0,REG13                                       02S0409
*   GEN(SETFRR  D,WRKREGS=(REG11,REG12))                        02S0410
*   REFS(FRRSEMP);                  /* FREE THE FRR. USE 11 & 12 AS
*                                      WORK REGS.                    */
         SETFRR  D,WRKREGS=(REG11,REG12)
*   GENERATE REFS(PSALITA,FLC) SETS(REG11,REG12,REG13,REG14);   02S0411
*                                                           /* Y02751*/
         SETLOCK  RELEASE,TYPE=CMS,RELATED=(UCM,IEAVMED2(SETLCKS))
         SETLOCK  RELEASE,TYPE=LOCAL,RELATED=(UCM,IEAVMED2(SETLCKS))
*   REG13=REG0;                     /* RESTORE SAVEAREA PTR    Y02751*/
         LR    REG13,REG0                                       02S0412
*   RFY                                                         02S0413
*     REG0 UNRSTD;                  /*                         Y02751*/
*   END;                                                        02S0414
@EL00006 DS    0H                                               02S0414
@EF00006 DS    0H                                               02S0414
@ER00006 LM    @14,@12,12(@13)                                  02S0414
         BR    @14                                              02S0414
