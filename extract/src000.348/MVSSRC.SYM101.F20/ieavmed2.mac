*/*                                                                  */
*/*   BUILDEND SUBROUTINE                                            */
*/*                                                                  */
*/*   FUNCTION - THIS ROUTINE WILL BUILD A MINOR WQE END LINE AT     */
*/*   THE LOCATION POINTED AT BY MINORPTR. THIS ROUTINE DEPENDS ON   */
*/*   THE FORMAT FOR THE 1ST AND 2ND HALFS OF THE MINOR WQE BEING    */
*/*   IDENTICAL. THE MAPPING FOR THE FIRST HALF IS USED EVEN THOUGH  */
*/*   THE MINORPTR PTS AT THE SECOND HALF OR THE RESA (FAKE)         */
*/*   AREA IN THE MAJOR WQE.                                         */
*/*                                                                  */
*/*   INPUT - MINORPTR PTS AT THE END LINE AND CHAINPTR PTS AT MAJOR.*/
*/*                                                                  */
*/*   OUTPUT - THE MLWTO IS ENDED                                    */
*/*                                                                  */
*/********************************************************************/
*                                                               02S0386
*BUILDEND:                                                      02S0386
*   PROC OPTIONS(NOSAVEAREA);                                   02S0386
BUILDEND STM   @14,@12,12(@13)                                  02S0386
*   WMNMUC1=WMJMUC;                 /* PICK UP USE COUNT FROM MAJOR  */
         IC    @11,WMJMUC(,CHAINPTR)                            02S0387
         STC   @11,WMNMUC1(,MINORPTR)                           02S0387
*   WMNMNX1=ZERO;                   /* END OF THE MINOR CHAIN        */
         SLR   @11,@11                                          02S0388
         STCM  @11,7,WMNMNX1(MINORPTR)                          02S0388
*   WMNMLT1=ENDTYPE;                /* MARK LINE TYPE BYTE AS END
*                                      LINE                          */
         MVI   WMNMLT1(MINORPTR),X'10'                          02S0389
*   WMNMTL1=ZERO;                   /* ZERO TEXT                     */
         MVI   WMNMTL1(MINORPTR),X'00'                          02S0390
*   WMNMML1C=YES;                   /* INDICATE MINOR WQE            */
         OI    WMNMML1C(MINORPTR),B'00100000'                   02S0391
*   WMJMMLWD=YES;                   /* INDICATE CHAIN WAS ALTERED    */
         OI    WMJMMLWD(CHAINPTR),B'00010000'                   02S0392
*   END BUILDEND;                                               02S0393
@EL00004 DS    0H                                               02S0393
@EF00004 DS    0H                                               02S0393
@ER00004 LM    @14,@12,12(@13)                                  02S0393
         BR    @14                                              02S0393
