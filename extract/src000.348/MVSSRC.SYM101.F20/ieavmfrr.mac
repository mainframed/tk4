*               PARMCWT='1'B;       /* INDICATE CONT. WITH TERM.     */
         OI    PARMCWT(PARMPTR),B'01000000'                        0139
*             END;                  /* END IF RECURSION BIT SET      */
*           ELSE                    /* IF RECURSION IS NOT SET       */
*             DO;                   /* SETUP FOR RECURSION ERROR     */
         B     @RC00137                                            0141
@RF00137 DS    0H                                                  0142
*               PARMRECU='1'B;      /* TURN RECURSION BIT ON         */
         OI    PARMRECU(PARMPTR),B'00100000'                       0142
*               IF PARMNDMP^='1'B THEN/* IS NO-DUMP IND SET? @ZA19994*/
         TM    PARMNDMP(PARMPTR),B'00000100'                       0143
         BO    @RF00143                                            0143
*                 DO;               /* IF NOT SET.           @ZA19994*/
