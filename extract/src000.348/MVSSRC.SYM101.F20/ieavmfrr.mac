         TITLE 'IEAVMFRR - FRR/ESTAE CONTROLLER                        *
                        '
*                           /*SET UP MODULE INDENTIFIER   */
IEAVMFRR CSECT                MAIN ENTRY POINT
        USING     *,R15     ENTRY FROM RTM
        MODID     BR=YES    MODULE ID
        DROP      R15       RELEASE REG 15 AS BASE
        BALR      BASEREG,0 SET REG12 AS BASE
L1      DS        0H        BOUNDARY ALIGNMENT
        USING     *,BASEREG ADDRESSABILITY
*   R8=R14;                         /* SAVE RETURN REGISTER          */
*                                                                  0077
         LR    R8,R14                                              0077
*   /*****************************************************************/
*   /*                                                               */
*   /* EXAMINE THE CONTENTS OF REGISTER 0 TO DETERMINE IF THIS IS AN */
*   /* ESTAE OR FRR ENTRY. IF REGISTER 0 IS GREATER THAN 16, THIS    */
*   /* MODULE WAS ENTERED FOR FRR RECOVERY.                          */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0078
*   IF R0>16 THEN                   /* FRR ENTRY ?                   */
         C     R0,@CF00065                                         0078
         BNH   @RF00078                                            0078
*     DO;                           /* IF YES,                       */
*                                                                  0079
*       /*************************************************************/
*       /*                                                           */
*       /* TEST FOR ENTRY IN SRB MODE                                */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0080
*       IF PSATOLD=0 THEN           /* SRB MODE IF ZERO              */
         L     @07,PSATOLD                                         0080
         LTR   @07,@07                                             0080
         BNZ   @RF00080                                            0080
*         DO;                       /* IF YES                        */
*           R4=SRBMDTCB;            /* GET TCB FROM LCCA             */
         L     @07,LCCAPTR                                         0082
         L     R4,SRBMDTCB(,@07)                                   0082
*         END;                      /* END IF SRB MODE               */
*       ELSE                        /* IF NOT SRB MODE               */
*         R4=PSATOLD;               /* GET CURRENT TCB ADDRESS       */
         B     @RC00080                                            0084
@RF00080 L     R4,PSATOLD                                          0084
*       R6=R1;                      /* SAVE REG1 ACROSS GETMAIN      */
@RC00080 LR    R6,R1                                               0085
*       R5=R0;                      /* SAVE REG0 ACROSS GETMAIN      */
         LR    R5,R0                                               0086
*       R7=PSAAOLD;                 /* GET CURRENT ASCB ADDRESS      */
         L     R7,PSAAOLD                                          0087
*       R1=0;                       /* GETMAIN INTERFACE             */
         SLR   R1,R1                                               0088
*       GEN REFS(CVTCRMN);                                         0089
*                                     /*GET CORE FOR DATA AREA */
        L     R2,@SIZDATD         SET SIZE OF GETMAIN
           GETMAIN  RU,LV=(R2),SP=229,BRANCH=YES ISSUE GETMAIN
        LR        R9,R1     SET DATA REGISTER
        USING     @DATD,R9  ADDRESSABILITY FOR DATA AREA
        GETMAIN  RU,LV=64,SP=229,BRANCH=YES GET SAVEAREA
*       R13=R1;                     /* SET SAVEAREA ADDRESS          */
         LR    R13,R1                                              0090
*       SAVTCB=R4;                  /* SAVE CONTENT OF REG 4         */
         ST    R4,SAVTCB                                           0091
*       R1=R6;                      /* RESTORE REGISTER 1            */
         LR    R1,R6                                               0092
*       R0=R5;                      /* RESTORE REGISTER 0            */
         LR    R0,R5                                               0093
*/********************************************************************/
*/*                INITIAL  FRR  SEGMENT                             */
*/* THE FOLLOWING WILL CREATE A RECOVERY ENVIRONMENT TO              */
*/* PROTECT THIS FRR ROUTINE FROM ERROR SITUATIONS WITHIN ITS        */
*/* OWN CODE.                                                        */
*/****************************************************************   */
*                                                                  0094
*       RECEXADD=ADDR(RECOVEX);     /* SET ADDR OF RECOVERY EXIT     */
         LA    @07,RECOVEX                                         0094
         ST    @07,RECEXADD                                        0094
*       GEN REFS(PSACSTK,FRRSCURR,FRRSLAST,PSALSFCC,FRRSENTR,FRRSELEN,
*           FRRSFRRA);                                             0095
*                                     /*ISSUE SETFRR                */
        SETFRR  A,FRRAD=RECEXADD,WRKREGS=(R5,R6),PARMAD=(R5) FRR EXIT
*       PARMPTR=R5;                 /* GET ADDR OF PARM AREA         */
         LR    PARMPTR,R5                                          0096
*       PARMSYSR=0;                 /* INDICATE NO XMPOST ENTRY      */
         MVI   PARMSYSR(PARMPTR),X'00'                             0097
*       PARMID='MFRR';              /* SET FRR ID                    */
         MVC   PARMID(4,PARMPTR),@CC02075                          0098
*       PARMRTAD=BASEREG;           /* SET BASE REGISTER             */
         ST    BASEREG,PARMRTAD(,PARMPTR)                          0099
*       PARMRGAD=R9;                /* SET DATA REGISTER             */
         ST    R9,PARMRGAD(,PARMPTR)                               0100
*       PARMPTR=SDWAPARM;           /* GET ADDR OF PARM LIST         */
         L     PARMPTR,SDWAPARM(,SDWAPTR)                          0101
*       PARMFRID='1'B;              /* INDICATE FRR ENTRY            */
         OI    PARMFRID(PARMPTR),B'00010000'                       0102
*       IF PARMNDMP^='1'B THEN      /* IS NO-DUMP IND SET?   @ZA19994*/
         TM    PARMNDMP(PARMPTR),B'00000100'                       0103
         BO    @RF00103                                            0103
*         DO;                       /* IF NOT SET.           @ZA19994*/
*                                                                  0104
