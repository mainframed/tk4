         LR    R6,R1                                               0146
*                   DUMPBUF(1:DUMPLEN)=DUMPHEAD(1:DUMPLEN);/* MOVE 0147
*                                      DUMP                  @ZA10488*/
         SLR   @11,@11                                             0147
         IC    @11,DUMPLEN                                         0147
         BCTR  @11,0                                               0147
         EX    @11,@SM02104                                        0147
*                   R10=ADDR(DUMPBUF);/* BUFFER              @ZA10488*/
         LA    R10,DUMPBUF                                         0148
*                   GEN REFS(R10);                                 0149
*                                   /* EXECUTE SDUMP MACRO  @ZA10488*/
         SDUMP HDR='IEAVMFRR ESTAE COMMUNICATIONS TASK DUMP',         XX
               MF=(E,(10))                                    @ZA10488
*                   RFY                                            0150
*                    (R6,                                          0150
*                     R10) UNRSTD;  /* RELEASE REGS          @ZA10488*/
*                   R1=R6;          /* RESTORE R1 SDWA PTR   @ZA10488*/
         LR    R1,R6                                               0151
*                 END;              /* END PROCESSING DUMP   @ZA19994*/
*             END;                  /* END IF RECURSION BIT SET      */
@RF00143 DS    0H                                                  0154
*         END;                      /* END IF NOT PERC. ERROR        */
@RC00137 DS    0H                                                  0155
*     END;                          /* END IF ESTEA RETRY            */
*                                                                  0155
@RC00130 DS    0H                                                  0156
*   /*****************************************************************/
*   /*                                                               */
*   /* TEST TO DETERMINE IF CONTINUE WITH TERMINATION HAS BEEN       */
