*           /* FRR ENTRY SDUMP SEGMENT                       @ZA10488*/
*           /* THIS SEGMENT ISSUES A BRANCH ENTRY SDUMP MACRO TO     */
*           /*                                               @ZA10488*/
*           /* OBTAIN A DUMP OF THE PROBLEM CAUSING ENTRY TO @ZA10488*/
*           /* IEAVMFRR.                                     @ZA10488*/
*           /*                                                       */
*           /*********************************************************/
*                                                                  0105
*           RFY                                                    0105
*            (R5,                                                  0105
*             R6,                                                  0105
*             R10) RSTD;            /* RESTRICT REGS         @ZA10488*/
*           R5=R0;                  /* SAVE R0 ACROSS SDUMP  @ZA10488*/
         LR    R5,R0                                               0106
*           R6=R1;                  /* SAVE R1 SDWA PTR      @ZA10488*/
         LR    R6,R1                                               0107
*           DUMPBUF(1:DUMPLEN)=DUMPHEAD(1:DUMPLEN);/* MOVE DUMP    0108
*                                                            @ZA10488*/
         SLR   @11,@11                                             0108
         IC    @11,DUMPLEN                                         0108
         BCTR  @11,0                                               0108
         EX    @11,@SM02104                                        0108
*           R10=ADDR(DUMPBUF);      /* BUFFER                @ZA10488*/
         LA    R10,DUMPBUF                                         0109
*           GEN REFS(R10,ASID0,CVTSDMP);                           0110
*                                   /* EXECUTE SDUMP MACRO  @ZA10488*/
         SDUMP HDR='IEAVMFRR FRR COMMUNICATIONS TASK DUMP',           XX
               BRANCH=YES,ASID=ASID0,MF=(E,(10))              @ZA10488
*           RFY                                                    0111
*            (R5,                                                  0111
*             R6,                                                  0111
*             R10) UNRSTD;          /* RELEASE REGS          @ZA10488*/
*           R0=R5;                  /* RESTORE R0 VALUE      @ZA10488*/
         LR    R0,R5                                               0112
*           R1=R6;                  /* RESTORE R1 SDWA PTR   @ZA10488*/
         LR    R1,R6                                               0113
*         END;                      /* END PROCESSING DUMP   @ZA19994*/
*     END;                          /* END IF FRR ENTRY              */
*   ELSE                            /* ESTAE ENTRY ?                 */
*     DO;                           /* IF YES,                       */
         B     @RC00078                                            0116
@RF00078 DS    0H                                                  0117
*       R6=R1;                      /* SAVE REG 1 ACROSS GETMAIN     */
         LR    R6,R1                                               0117
*       R5=R0;                      /* SAVE REG 0 ACROSS GETMAIN     */
         LR    R5,R0                                               0118
*       GEN;                                                       0119
*                                     /*PREPARE TO ISSUE GETMAIN  */
           L    R3,@SIZDATD            SIZE OF AREA
           GETMAIN  R,LV=(R3),SP=229   ISSUE GETMAIN
           LR   R9,R1                SET DATA REGISTER
           USING @DATD,R9            ADDRESSABILITY FOR DATA AREA
        GETMAIN  RU,LV=64,SP=229       GET SAVEAREA
*       R13=R1;                     /* SET SAVEAREA ADDRESS          */
         LR    R13,R1                                              0120
*       R1=R6;                      /* RESTORE REGISTER 1            */
         LR    R1,R6                                               0121
*       R0=R5;                      /* RESTORE REGISTER 0            */
         LR    R0,R5                                               0122
*/********************************************************************/
*/*                INITIAL  ESTAE  SEGMENT                           */
*/* THE FOLLOWING SEGMENT WILL CREATE A RECURSION COUNTER TO PROTECT */
*/* THIS ESTAE ROUTINE FROM ERRORS WITHIN ITS OWN CODE.              */
*/********************************************************************/
*                                                                  0123
*       IF R0=12 THEN               /* SDWA PRESENT ?                */
         C     R0,@CF01033                                         0123
         BNE   @RF00123                                            0123
*         DO;                       /* IF NO,                        */
*           PARMSDWA='1'B;          /* INDICATE NO SDWA              */
         OI    PARMSDWA(PARMPTR),B'10000000'                       0125
*         END;                      /* END IF NO SDWA                */
*       ELSE                        /* IF SDWA IS PRESENT            */
*         DO;                       /* SET UP ADDRESSABILITY         */
         B     @RC00123                                            0127
@RF00123 DS    0H                                                  0128
*           PARMPTR=SDWAPARM;       /* GET ADDRESS OF PARM LIST      */
         L     PARMPTR,SDWAPARM(,SDWAPTR)                          0128
*         END;                      /* END IF SDWA PRESENT           */
*                                                                  0129
*       /*************************************************************/
*       /*                                                           */
*       /* TEST USERS PARAMETER TO DETERMINE IF THIS IS A PERCOLATED */
*       /* ERROR FROM A LOWER LEVEL FRR RECOVERY.                    */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0130
*       IF PARMFRID='1'B THEN       /* FRR INDICATOR SET             */
@RC00123 TM    PARMFRID(PARMPTR),B'00010000'                       0130
         BNO   @RF00130                                            0130
*         DO;                       /* IF YES,                       */
*           PARMCWT='1'B;           /* SET CONTINUE WITH TERMINATIO  */
*           PARMSDWA='1'B;          /* SET NO SDWA (TO BYPASS REC)   */
*           PARMFRID='0'B;          /* TURN FRR INDIC. OFF    YM05436*/
         OI    PARMCWT(PARMPTR),B'11000000'                        0134
         NI    PARMFRID(PARMPTR),B'11101111'                       0134
*         END;                      /* END IF PERCOLATED ERROR       */
*       ELSE                        /* IF NOT PERCOLATED             */
*         DO;                       /* TEST FOR RECURSION            */
