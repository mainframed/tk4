*/*       ZERO      PARAMETER ALL NUMERIC AND                        */
*/*                 LENGTH DOES NOT EXCEED PCT SPECIFICATION         */
*/*       FOUR      PARAMETER NOT ALL NUMERIC OR                     */
*/*                 LENGTH EXCEEDS PCT SPECIFICATION                 */
*/***** WARNING *****                                                */
*/* THE MAXIMUM VALUE FOR PCT IS 4                                   */
*/************************************************************       */
*                                                                  0409
*NPAVNUM:                                                          0409
*   PROCEDURE OPTIONS(NOSAVEAREA);  /* NPAVNUM ENTRY POINT           */
         B     @PB00003                                            0409
NPAVNUM  STM   @14,@12,12(@13)                                     0409
*   RESPECIFY                                                      0410
*    (RNVT,                                                        0410
*     RCVT,                                                        0410
*     RPTAB,                                                       0410
*     RSCAN,                                                       0410
*     RVALUE) RESTRICTED;                                          0410
*   RSCAN=SCANP;                    /* PICK UP ADDR OF PARAMETER     */
         L     RSCAN,SCANP                                         0411
*   M=PCT+1;                        /* CALCULATE INDEX AFTER END     */
         LA    @10,1                                               0412
         LH    @08,PCT                                             0412
         ALR   @08,@10                                             0412
         STH   @08,M                                               0412
*   DO N=1 TO M;                    /* SCAN FOR A DELIMITER          */
         B     @DE00413                                            0413
@DL00413 DS    0H                                                  0414
*     IF SCAN(N)=''''SCAN(N)=' 'SCAN(N)=','SCAN(N)=')'/* TEST FOR
*                                      A VALID DELIMITER - BLANK,  0414
*                                      COMMA, OR CLOSE PARENTHESIS   */
*       THEN                        /* DELIMITER FOUND               */
         ALR   @10,RSCAN                                           0414
         BCTR  @10,0                                               0414
         CLI   SCAN(@10),C''''                                     0414
         BE    @RT00414                                            0414
         LH    @10,N                                               0414
         ALR   @10,RSCAN                                           0414
         BCTR  @10,0                                               0414
         CLI   SCAN(@10),C' '                                      0414
         BE    @RT00414                                            0414
         LH    @10,N                                               0414
         ALR   @10,RSCAN                                           0414
         BCTR  @10,0                                               0414
         CLI   SCAN(@10),C','                                      0414
         BE    @RT00414                                            0414
         LH    @10,N                                               0414
         ALR   @10,RSCAN                                           0414
         BCTR  @10,0                                               0414
         CLI   SCAN(@10),C')'                                      0414
         BE    @RT00414                                            0414
*       GO TO NPAVNUMB;             /* CONVERT PARAMETER TO BINARY   */
*     ELSE                          /* DELIMITER NOT FOUND           */
*       IF SCANZ(N)^='1111'B        /* TEST FOR NUMERIC CHARACTER    */
*         THEN                      /* NOT NUMERIC                   */
         LH    @10,N                                               0416
         ALR   @10,RSCAN                                           0416
         BCTR  @10,0                                               0416
         TM    SCANZ(@10),B'11110000'                              0416
         BNO   @RT00416                                            0416
*         GO TO NPAVNINV;           /* INVALID PARAMETER             */
*   END;                            /* END OF SCAN LOOP - TEST NEXT
*                                      CHARACTER                     */
         LH    @08,N                                               0418
         AH    @08,@CH00051                                        0418
         LR    @10,@08                                             0418
@DE00413 STH   @10,N                                               0418
         CH    @10,M                                               0418
         BNH   @DL00413                                            0418
*NPAVNINV:                          /* INVALID PARAMETER             */
*   RETURN CODE(FOURRC);            /* RETURN TO CALLER              */
NPAVNINV LA    @15,4                                               0419
         L     @14,12(,@13)                                        0419
         LM    @00,@12,20(@13)                                     0419
         BR    @14                                                 0419
*NPAVNUMB:                          /* A DELIMITER WAS FOUND AFTER A
*                                      POSSIBLY GOOD PARAMETER       */
*   IF N=1 THEN                     /* TEST FOR NO DIGITS SPEC       */
NPAVNUMB LH    @10,N                                               0420
         CH    @10,@CH00051                                        0420
         BE    @RT00420                                            0420
*     GO TO NPAVNINV;               /* NO VALUE GIVEN - INVALID    0421
*                                      PARAMETER SPECIFIED           */
*   M=N-1;                          /* NUMBER OF BYTES IN PARAMETER  */
         BCTR  @10,0                                               0422
         STH   @10,M                                               0422
*   I=MAXPCTD-M;                    /* FIRST BYTE TO BE CONVERTED    */
         LCR   @10,@10                                             0423
         AH    @10,@CH00054                                        0423
         STH   @10,I                                               0423
*   CONV=CONV&&CONV;                /* CLEAR CONVERSION WORK AREA    */
         XC    CONV(8),CONV                                        0424
*   CONV2(I:MAXPCT)=SCAN(1:M);      /* MOVE PARAMETER TO WORK AREA   */
         LA    @08,CONV2-1(@10)                                    0425
         LCR   @10,@10                                             0425
         AH    @10,@CH00046                                        0425
         EX    @10,@SM02048                                        0425
*   GENERATE REFS(CONV1,CONV2) SETS(CONV2,RVALUE)/* CONVERT PARAMETER
*                                      FROM EBCDIC TO BINARY         */
*       ;                                                          0426
          PACK  CONV2,CONV2            PACK PARAMETER
          CVB   RVALUE,CONV1           CONVERT TO BINARY
*   VALUE=RVALUE;                   /* VALUE OF PARAMETER            */
         STH   RVALUE,VALUE                                        0427
*   SCANP=RSCAN+M;                  /* CALCULATE ADDRESS OF DELIMITER*/
         LR    @10,RSCAN                                           0428
         AH    @10,M                                               0428
         ST    @10,SCANP                                           0428
*   RETURN CODE(ZERORC);            /* RETURN TO CALLER              */
         SLR   @15,@15                                             0429
         L     @14,12(,@13)                                        0429
         LM    @00,@12,20(@13)                                     0429
         BR    @14                                                 0429
*   RESPECIFY                                                      0430
*    (RNVT,                                                        0430
*     RCVT,                                                        0430
*     RPTAB,                                                       0430
*     RSCAN,                                                       0430
*     RVALUE) UNRESTRICTED;                                        0430
*   END NPAVNUM;                    /* END SUBROUTINE                */
@PB00003 DS    0H                                                  0432
