*     TSCAN CHAR(LENGTH(CONTCK)) BASED(RTSCAN);/* PARAMETER BUFFER 0175
*                                      FOR CONT CHECK                */
*   GENERATE DATA NOREFS NOSETS DEFS(MSG363I,MSG1P1,MSG1P2,MSG1P3, 0176
*       MSG322A,MSG2P,MSG2S,MSG2C,MSG330A,MSG4P,MSG332A,MSG5P,MSG371I,
*       MSG370I1,MSG370I2,M13LTH,M13RPCT,M13NFX,M13MTIM,MSG370I3,M14LRC
*       ,M14HRC,M14LRD,M14HRD,MSG338A,MSG152I,MSG153I,MSG355A,MSGL322A,
*       MSGL330A,MSGL332A,MSGL338A,MSGL355A,MSG904I,MSG905I,LOADNVT,
*       STOREPVT);                                                 0176
*                                                                  0176
*   /*****************************************************************/
*   /*                                                               */
*   /* RESPECIFY BASE POINTERS FOR CONTROL BLOCKS                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0177
*   RESPECIFY                                                      0177
*     PARMTAB BASED(RPTAB);         /* NIP'S PARAMETER TABLE DSECT   */
*   RESPECIFY                                                      0178
*     CVTMAP BASED(RCVT);           /* COMMUNICATIONS VECTOR TABLE 0178
*                                      TABLE DSECT                   */
*   RESPECIFY                                                      0179
*     NIPMNTPL BASED(RSCAN);        /* NIP MOUNT PARAMETER LIST DSECT*/
*   RESPECIFY                                                      0180
*     NIPOPNPL BASED(RSCAN);        /* NIP OPEN PARAMETER LIST DSECT */
*   RESPECIFY                                                      0181
*     UCMLIST BASED(RUCME);         /* UCM ENTRY DSECT               */
*   GENERATE NOREFS NOSETS(EJECT);                                 0182
         EJECT
*   RESPECIFY                                                      0183
*    (RNVT,                                                        0183
*     RCVT,                                                        0183
*     RPTAB) RESTRICTED;                                           0183
*   RPTAB=NVTPTAB;                  /* GET ADDRESS OF NIP PARAMETER
*                                      TABLE                         */
*                                                                  0184
         L     RPTAB,NVTPTAB(,RNVT)                                0184
*   /*****************************************************************/
*   /*                                                               */
*   /* DEFINE HARDCOPY CONSOLE                                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0185
*                                                                  0185
*/************************************************************       */
*/*    SUBROUTINE NP7HDCPY                                           */
*/*       DEFINES THE HARDCOPY CONSOLE                               */
*/************************************************************       */
*/*    INPUT                                                         */
*/*       CVTCUCB                                                    */
*/*       HARDCPY SYSTEM PARAMETER                                   */
*/*       UCM                                                        */
*/*    OUTPUT                                                        */
*/*       UCMDISPB                                                   */
*/*       UCMHCUCM                                                   */
*/*       UCMHRDRT                                                   */
*/*       UCMSDS1A                                                   */
*/*       UCMSDS1B                                                   */
*/*       UCMSYSC                                                    */
*/*       UCMSYSG                                                    */
*/*    INTERNAL INTERFACES                                           */
*/*       NP7DEVSR                                                   */
*/*       NP7CONT                                                    */
*/*       NP7VNUM                                                    */
*/*    EXTERNAL INTERFACES                                           */
*/*       NIP WTOR                                                   */
*/************************************************************       */
*                                                                  0185
*   RESPECIFY                                                      0185
*    (RPARM,                                                       0185
*     RNVT,                                                        0185
*     RCVT,                                                        0185
*     RPTAB,                                                       0185
*     RCODE,                                                       0185
*     UCMBASPT,                                                    0185
*     UCMMCSPT,                                                    0185
*     RSCAN,                                                       0185
*     RTFIELD,                                                     0185
*     RTCODE,                                                      0185
*     UCBPTR) RESTRICTED;                                          0185
*   UCMBASPT=CVTCUCB;               /* SET UP UCM BASE POINTER       */
         L     UCMBASPT,CVTCUCB(,RCVT)                             0186
*   UCMMCSPT=UCMPRFXP;              /* SET UP UCM PREFIX POINTER     */
         LR    @14,UCMBASPT                                        0187
         AL    @14,@CF02043                                        0187
         L     UCMMCSPT,UCMPRFXP(,@14)                             0187
*   RSCAN=HARDCPY;                  /* PICK UP ADDRESS OF HARDCOPY 0188
*                                      PARAMETER                     */
         L     RSCAN,HARDCPY(,RPTAB)                               0188
*   IF RSCAN=0                      /* TEST FOR HARDCPY SPECIFIED    */
*     THEN                          /* HARDCPY NOT SPECIFIED         */
         LTR   RSCAN,RSCAN                                         0189
         BNZ   @RF00189                                            0189
*     IF UCMSYSB='1'B THEN          /* TEST FOR HARDCOPY REQUIRED    */
         TM    UCMSYSB(UCMMCSPT),B'01000000'                       0190
         BO    @RT00190                                            0190
*       GO TO NPAHCREQ;             /* YES, ASK FOR HARDCPY SPEC     */
*     ELSE                          /* NO, HARDCOPY NOT NEEDED       */
*       GO TO NPA1WTOB;             /* GO PROCESS BUFFER LIMITS      */
         B     NPA1WTOB                                            0192
*NPAHCRTY:                          /* SET UP TO PROCESS HARDCPY   0193
*                                      PARAMETER                     */
*   NPAHCWA=NPAHCWA&&NPAHCWA;       /* CLEAR WORK AREA               */
@RF00189 DS    0H                                                  0193
NPAHCRTY XC    NPAHCWA(8),NPAHCWA                                  0193
*   IF SCAN(1)^='('                 /* TEST FOR OPEN PARENTHESIS     */
*     THEN                          /* HARDCPY= NOT FOLLOWED BY OPEN
*                                      PARENTHESIS                   */
         CLI   SCAN(RSCAN),C'('                                    0194
         BNE   @RT00194                                            0194
*     GO TO NPAHCINV;               /* ASK FOR A NEW PARAM           */
*   RSCAN=RSCAN+1;                  /* POINT PAST OPEN PAREN         */
         AH    RSCAN,@CH00051                                      0196
*   IF SCAN(1:LENGTH(SYSLOGCH))=SYSLOGCH/* TEST FOR 'SYSLOG'       0197
*                                      SPECIFIED                     */
*     THEN                          /* YES, HARDCOPY ON SYSLOG       */
         CLC   SCAN(6,RSCAN),SYSLOGCH                              0197
         BNE   @RF00197                                            0197
*     DO;                           /* SET UP FOR SYSLOG             */
*       DEVICE=SLOGCH;              /* SET DEVICE ADDR TO 'SLOG'     */
         MVC   DEVICE(4),SLOGCH                                    0199
*       RSCAN=RSCAN+LENGTH(SYSLOGCH);/* POINT PAST 'SYSLOG' IN     0200
*                                      PARAMETER LIST                */
         AH    RSCAN,@CH00181                                      0200
*       GO TO NPAHCDL1;             /* CHECK FOR NEXT SUBPARAMETER   */
         B     NPAHCDL1                                            0201
*     END;                                                         0202
*   ELSE                                                           0203
*     ;                             /* NO, DEVICE SPECIFIED          */
@RF00197 DS    0H                                                  0204
*   PCT=MAXHCDEV;                   /* SET UP MAXIMUM CHARACTERS   0204
*                                      ALLOWED IN DEVICE ADDRESS     */
         MVC   PCT(2),@CH00044                                     0204
*NPAHCADR:                          /* PROCESS DEVICE ADDRESS        */
*   IF SCAN(1)=','                  /* TEST FOR COMMA                */
*       SCAN(1)=')'                /* TEST FOR CLOSE PARENTHESIS    */
*     THEN                          /* IF EITHER, END OF ADDRESS     */
NPAHCADR CLI   SCAN(RSCAN),C','                                    0205
         BE    @RT00205                                            0205
         CLI   SCAN(RSCAN),C')'                                    0205
         BE    @RT00205                                            0205
*     GO TO NPAHCDEL;               /* CHECK FOR NEXT SUBPARAMETER   */
*   IF SCAN(1)<'A'(SCAN(1)>'Z'&SCAN(1)<'0')SCAN(1)>'9'/* TEST FOR A
*                                      VALID HEX DIGIT               */
*     THEN                          /* NOT A HEX DIGIT               */
         CLI   SCAN(RSCAN),C'A'                                    0207
         BL    @RT00207                                            0207
         CLI   SCAN(RSCAN),C'Z'                                    0207
         BNH   @GL00002                                            0207
         CLI   SCAN(RSCAN),C'0'                                    0207
         BL    @RT00207                                            0207
@GL00002 CLI   SCAN(RSCAN),C'9'                                    0207
         BH    @RT00207                                            0207
*     GO TO NPAHCINV;               /* ASK FOR A NEW PARAM           */
*   IF PCT=0 THEN                   /* TEST FOR TOO MANY DIGITS      */
         LH    @14,PCT                                             0209
         LTR   @14,@14                                             0209
         BZ    @RT00209                                            0209
*     GO TO NPAHCINV;               /* TOO MANY DIGITS IN DEVICE   0210
*                                      ADDRESS - ASK FOR NEW SPEC    */
*   M=MAXHCDEM-PCT;                 /* ADDR OF DIGIT IN WORK AREA    */
         LA    @10,4                                               0211
         SLR   @10,@14                                             0211
         STH   @10,M                                               0211
*   DEVICE(M)=SCAN(1);              /* SAVE DIGIT IN WORK AREA       */
         LA    @08,DEVICE-1(@10)                                   0212
         MVC   0(1,@08),SCAN(RSCAN)                                0212
*   PCT=PCT-1;                      /* NUMBER OF DIGITS REMAINING    */
         BCTR  @14,0                                               0213
         STH   @14,PCT                                             0213
*   RSCAN=RSCAN+1;                  /* POINT PAST DIGIT PROCESSED    */
         AH    RSCAN,@CH00051                                      0214
*   GO TO NPAHCADR;                 /* TEST NEXT CHAR IN SPEC        */
         B     NPAHCADR                                            0215
*NPAHCDEL:                          /* END OF DEVICE ADDRESS SCAN    */
*   IF PCT^=0 THEN                  /* TEST FOR PROPER LENGTH OF   0216
*                                      DEVICE ADDRESS SPECIFIED      */
NPAHCDEL ICM   @14,3,PCT                                           0216
         BNZ   @RT00216                                            0216
*     GO TO NPAHCINV;               /* INVALID LENGTH - ASK FOR NEW
*                                      SPECIFICATION                 */
*NPAHCDL1:                          /* CHECK FOR SUBPARAMETER AFTER
*                                      'SYSLOG' OR DEVICE ADDRESS    */
*   IF SCAN(1)=',' THEN             /* TEST FOR COMMA                */
NPAHCDL1 CLI   SCAN(RSCAN),C','                                    0218
         BNE   @RF00218                                            0218
*     DO;                           /* COMMA FOUND - MORE TO COME    */
*       RSCAN=RSCAN+1;              /* POINT PAST COMMA              */
         AH    RSCAN,@CH00051                                      0220
*       GO TO NPAHCPR2;             /* CHECK SUBPARAMETER            */
         B     NPAHCPR2                                            0221
*     END;                                                         0222
*   IF SCAN(1)^=')' THEN            /* TEST FOR CLOSE PARENTHESIS    */
@RF00218 CLI   SCAN(RSCAN),C')'                                    0223
         BNE   @RT00223                                            0223
*     GO TO NPAHCINV;               /* INVALID DELIMITER - ASK FOR 0224
*                                      NEW SPECIFICATION             */
*   ELSE                            /* END OF SPECIFICATION          */
*     GO TO NPAHCCMD;               /* SET DEFAULT ROUTING CODES AND
*                                      COMMAND OPTION                */
         B     NPAHCCMD                                            0225
*NPAHCPR2:                          /* CONTINUE PARAM SCAN           */
*   IF SCAN(1:LENGTH(CONTCK))=CONTCK/* TEST FOR CONTINUATION         */
*     THEN                          /* 'CONT' SPECIFIED              */
NPAHCPR2 CLC   SCAN(4,RSCAN),CONTCK                                0226
         BNE   @RF00226                                            0226
*     RSCAN=ADDR(CONTBUF);          /* GET ADDRESS OF CONTINUATION 0227
*                                      BUFFER                        */
         LA    RSCAN,CONTBUF                                       0227
*   IF SCAN(1:LENGTH(ALLCH))=ALLCH  /* TEST FOR 'ALL' SPECIFIED      */
*     THEN                          /* YES, ALL ROUTING CODES REQ    */
@RF00226 CLC   SCAN(3,RSCAN),ALLCH                                 0228
         BNE   @RF00228                                            0228
*     DO;                           /* SET UP FOR ALL ROUT CODES     */
*       NPAHCODE=RCALL;             /* SET ALL ROUT FLAGS ON         */
         MVC   NPAHCODE(2),@CH00038                                0230
*       RSCAN=RSCAN+LENGTH(ALLCH);  /* POINT PAST 'ALL'              */
         AH    RSCAN,@CH00044                                      0231
*       GO TO NPAHCDL2;             /* PROCESS NEXT SUBPARAMETER     */
         B     NPAHCDL2                                            0232
*     END;                                                         0233
*   IF SCAN(1)^='(' THEN            /* TEST FOR ROUTING CODE LIST    */
@RF00228 CLI   SCAN(RSCAN),C'('                                    0234
         BNE   @RT00234                                            0234
*     GO TO NPAHCPR4;               /* ROUTINE CODES NOT SPECIFIED -
*                                      PROCESS NEXT SUBPARAMETER     */
*NPAHCRTC:                          /* CONVERT NEXT ROUTING CODE     */
*   RSCAN=RSCAN+1;                  /* POINT PAST DELIMITER          */
NPAHCRTC AH    RSCAN,@CH00051                                      0236
*   SCANP=RSCAN;                    /* ADDRESS OF ROUTING CODE       */
         ST    RSCAN,SCANP                                         0237
*   PCT=MAXHCRTC;                   /* SET MAXIMUM NUMBER OF DIGITS
*                                      ALLOWED IN ROUTING CODE       */
*                                                                  0238
         MVC   PCT(2),@CH00048                                     0238
*   /*****************************************************************/
*   /*                                                               */
*   /* CALL NPAVNUM TO VALIDITY CHECK ROUTING CODE AND CONVERT IT TO */
*   /* ITS BINARY VALUE                                              */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0239
*   CALL NPAVNUM;                                                  0239
         BAL   @14,NPAVNUM                                         0239
*   GENERATE                        /* CHECK RETURN CODE             */
*       REFS(RCODE) NOSETS FLOWS(NPAHCROK,NPAHCINV);               0240
         B     NPAHBTAB(RCODE)         /* INDEX THRU BRANCHES        */
NPAHBTAB B     NPAHCROK                /* CODE 0 PARM VALID          */
         B     NPAHCINV                /* CODE 4 PARM INVALID        */
*NPAHCROK:                          /* ROUTINE CODE IS THE PROPER  0241
*                                      NUMBER OF DIGITS              */
*   IF VALUE>MAXHCRTVVALUE<MINHCRTV/* CHECK FOR VALID RANGE         */
*     THEN                          /* INVALID VALUE SPECIFIED       */
NPAHCROK LH    @14,VALUE                                           0241
         CH    @14,@CH00037                                        0241
         BH    @RT00241                                            0241
         LTR   @14,@14                                             0241
         BNP   @RT00241                                            0241
*     GO TO NPAHCINV;               /* ASK FOR NEW SPECIFICATION     */
*   RTCODE=VALUE;                   /* GET ROUTING CODE              */
         LR    RTCODE,@14                                          0243
*   RTFIELD=RTCONV;                 /* PICK UP MASK                  */
         L     RTFIELD,@CF00042                                    0244
*   GEN REFS(RTFIELD,RTCODE) SETS(RTFIELD)(SRL    RTFIELD,ZERO(RTCODE))
*       ;                           /* SHIFT THE MASK THE PROPER   0245
*                                      NUMBER OF BITS TO SET THE   0245
*                                      CORRESPONDING ROUTING CODE  0245
*                                      FLAG                          */
         SRL    RTFIELD,ZERO(RTCODE)
*   NPAHCODE=NPAHCODERTFIELD;      /* SET THE ROUTING CODE FLAG IN
*                                      THE WORK AREA                 */
         SLR   @14,@14                                             0246
         ICM   @14,3,NPAHCODE                                      0246
         OR    @14,RTFIELD                                         0246
         STH   @14,NPAHCODE                                        0246
*   RSCAN=SCANP;                    /* POINT PAST ROUTING CODE       */
         L     RSCAN,SCANP                                         0247
*   IF SCAN(1)=',' THEN             /* TEST FOR COMMA                */
         CLI   SCAN(RSCAN),C','                                    0248
         BE    @RT00248                                            0248
*     GO TO NPAHCRTC;               /* COMMA FOUND - PROCESS NEXT  0249
*                                      ROUTING CODE                  */
*   IF SCAN(1)^=')' THEN            /* TEST FOR CLOSE PARENTHESIS    */
         CLI   SCAN(RSCAN),C')'                                    0250
         BNE   @RT00250                                            0250
*     GO TO NPAHCINV;               /* INVALID DELIMITER - ASK FOR 0251
*                                      NEW SPECIFICATION             */
*   RSCAN=RSCAN+1;                  /* POINT PAST CLOSE PAREN        */
         AH    RSCAN,@CH00051                                      0252
*NPAHCDL2:                          /* CHECK FOR LAST SUBPARAMETER   */
*   IF SCAN(1)^=','                 /* TEST FOR COMMA                */
*     THEN                          /* NO COMMA - CAN BE NO MORE   0253
*                                      SUBPARAMETERS                 */
NPAHCDL2 CLI   SCAN(RSCAN),C','                                    0253
         BE    @RF00253                                            0253
*     IF SCAN(1)=')' THEN           /* TEST FOR CLOSE PARENTHESIS    */
         CLI   SCAN(RSCAN),C')'                                    0254
         BE    @RT00254                                            0254
*       GO TO NPAHCCMD;             /* CLOSE PAREN - END OF HARDCPY
*                                      SPECIFICATION - DEFAULT     0255
*                                      COMMAND OPTION                */
*     ELSE                          /* INVALID DELIMITER             */
*       GO TO NPAHCINV;             /* ASK FOR NEW SPECIFICATION     */
         B     NPAHCINV                                            0256
*   RSCAN=RSCAN+1;                  /* POINT PAST COMMA              */
@RF00253 AH    RSCAN,@CH00051                                      0257
*NPAHCPR4:                          /* PROCESS LAST SUBPARAMETER     */
*   IF SCAN(1:LENGTH(CONTCK))=CONTCK/* TEST FOR 'CONT'               */
*     THEN                          /* CONTINUATION SPECIFIED        */
NPAHCPR4 CLC   SCAN(4,RSCAN),CONTCK                                0258
         BNE   @RF00258                                            0258
*     RSCAN=ADDR(CONTBUF);          /* GET ADDRESS OF CONTINUATION 0259
*                                      BUFFER                        */
         LA    RSCAN,CONTBUF                                       0259
*   IF SCAN(1:LENGTH(CMDSCH))=CMDSCH/* TEST FOR 'CMDS'               */
*     THEN                          /* ALL COMMANDS SPECIFIED        */
@RF00258 CLC   SCAN(6,RSCAN),CMDSCH                                0260
         BE    @RT00260                                            0260
*     GO TO NPAHCCMD;               /* SET COMMAND CODES             */
*   IF SCAN(1:LENGTH(INCMDSCH))=INCMDSCH/* TEST FOR 'INCMDS'         */
*     THEN                          /* INCMDS SPECIFIED              */
         CLC   SCAN(8,RSCAN),INCMDSCH                              0262
         BNE   @RF00262                                            0262
*     DO;                                                          0263
*       XINCMDS='1'B;               /* SET INCMDS FLAG               */
         OI    XINCMDS,B'01000000'                                 0264
*       GO TO NPAHCCOM;             /* SET COMMANDS FLAG             */
         B     NPAHCCOM                                            0265
*     END;                                                         0266
*   IF SCAN(1:LENGTH(STCMDSCH))=STCMDSCH/* TEST FOR 'STCMDS'         */
*     THEN                          /* STCMDS SPECIFIED              */
@RF00262 CLC   SCAN(8,RSCAN),STCMDSCH                              0267
         BNE   @RF00267                                            0267
*     DO;                                                          0268
*       XSTCMDS='1'B;               /* SET STCMDS FLAG               */
         OI    XSTCMDS,B'10000000'                                 0269
*       GO TO NPAHCCOM;             /* SET COMMANDS FLAG             */
         B     NPAHCCOM                                            0270
*     END;                                                         0271
*   IF SCAN(1:LENGTH(NOCMDSCH))^=NOCMDSCH/* TST FOR 'NOCMDS'         */
*     THEN                          /* INVALID SUBPARAMETER          */
@RF00267 CLC   SCAN(8,RSCAN),NOCMDSCH                              0272
         BNE   @RT00272                                            0272
*     GO TO NPAHCINV;               /* ASK FOR NEW SPECIFICATION     */
*   ELSE                            /* NO COMMANDS REQUESTED         */
*     IF UCMSYSB^='1'B THEN         /* TEST FOR HARDCOPY REQUIRED    */
         TM    UCMSYSB(UCMMCSPT),B'01000000'                       0274
         BNO   @RT00274                                            0274
*       GO TO NPAHCTS3;             /* HC NOT REQUIRED - NO COMMANDS
*                                      IS ACCEPTED                   */
*NPAHCCMD:                          /* SET COMMAND CODE FLAG         */
*   XCMDS='1'B;                     /* HARDCOPY COMMANDS             */
NPAHCCMD OI    XCMDS,B'00010000'                                   0276
*NPAHCCOM:                          /* DEFAULT ROUTING CODES IF    0277
*                                      HARDCOPY REQUIRED             */
*   IF UCMSYSB='1'B THEN            /* TEST FOR HARDCOPY REQUIRED    */
NPAHCCOM TM    UCMSYSB(UCMMCSPT),B'01000000'                       0277
         BNO   @RF00277                                            0277
*     NPAHCODE=NPAHCODEHCRQRC;     /* HARDCOPY REQUIRED - SET     0278
*                                      DEFAULT ROUTING CODES         */
         L     @14,@CF00040                                        0278
         SLR   @10,@10                                             0278
         ICM   @10,3,NPAHCODE                                      0278
         OR    @14,@10                                             0278
         STH   @14,NPAHCODE                                        0278
*NPAHCTS3:                          /* PUT WORK AREA INFORMATION INTO
*                                      UCM                           */
*   IF DEVICE=SLOGCH THEN           /* TEST FOR 'SYSLOG' SPEC        */
@RF00277 DS    0H                                                  0279
NPAHCTS3 CLC   DEVICE(4),SLOGCH                                    0279
         BNE   @RF00279                                            0279
*     DO;                           /* 'SYSLOG' SPECIFIED            */
*       UCMSYSG='1'B;               /* SET HARDCOPY ON SYSLOG FLAG IN
*                                      UCM MCS PREFIX                */
         OI    UCMSYSG(UCMMCSPT),B'00000010'                       0281
*       GO TO NPAHCEND;             /* PUT COMMAND OPTIONS AND     0282
*                                      ROUTING CODES INTO UCM        */
         B     NPAHCEND                                            0282
*     END;                                                         0283
*                                                                  0283
*   /*****************************************************************/
*   /*                                                               */
*   /* CALL NPADEVSR SUBROUTINE TO LOCATE DEVICE                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0284
*   CALL NPADEVSR;                                                 0284
@RF00279 BAL   @14,NPADEVSR                                        0284
*   GENERATE                        /* GENERATE BRANCH TAB           */
*       REFS(RCODE) NOSETS FLOWS(NPAHCEND,NPAHCSPC,NPAHCUNA);      0285
         B     NPAHDTAB(RCODE)         /* INDEX THRU BRANCHES        */
NPAHDTAB B     NPAHCEND                /* LOCATED                    */
         B     NPAHCSPC                /* INVALID HC SPEC            */
         B     NPAHCUNA                /* HC CONSOLE UNAVAILABLE     */
*NPAHCSPC:                          /* DEVICE IS NOT A VALID CONSOLE
*                                      FOR HARDCOPY OUTPUT           */
*                                                                  0286
*   /*****************************************************************/
*   /*                                                               */
*   /* IEA152I HARDCPY SPECIFICATION INVALID                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0286
*   GENERATE REFS(RNVT,NVTWTO,MSG152I) SETS(RPARM,REXIT,RENTRY)    0286
*       (IEAPMNIP TYPE=WTO,PARAM=MSG152I);/* WTO MESSAGE             */
NPAHCSPC IEAPMNIP TYPE=WTO,PARAM=MSG152I
*   GOTO NPAHCDER;                  /* ASK FOR NEW SPECIFICATION     */
         B     NPAHCDER                                            0287
*NPAHCUNA:                          /* DEVICE NOT AVAILABLE FOR USE
*                                      AS HARDCOPY OUTPUT            */
*                                                                  0288
*   /*****************************************************************/
*   /*                                                               */
*   /* IEA153I HARDCPY CONSOLE UNAVAILABLE                           */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0288
*   GENERATE REFS(RNVT,NVTWTO,MSG153I) SETS(RPARM,REXIT,RENTRY)    0288
*       (IEAPMNIP TYPE=WTO,PARAM=MSG153I);/* WTO MESSAGE             */
NPAHCUNA IEAPMNIP TYPE=WTO,PARAM=MSG153I
*NPAHCDER:                          /* ASK FOR NEW HARDCOPY SPEC     */
*   IF UCMSYSB='1'B THEN            /* IS HARDCPY REQUIRED?          */
NPAHCDER TM    UCMSYSB(UCMMCSPT),B'01000000'                       0289
         BO    @RT00289                                            0289
*     GOTO NPAHCREQ;                /* YES, ASK FOR NEW SPEC         */
*   ELSE                                                           0291
*     ;                             /* NO, HC NOT REQUIRED           */
*   MSGBUF=MSGBUF&&MSGBUF;          /* CLEAR BUFFER AREA             */
*                                                                  0292
         XC    MSGBUF(80),MSGBUF                                   0292
*   /*****************************************************************/
*   /*                                                               */
*   /* IEA355A RESPECIFY HARDCPY OR CANCEL                           */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0293
*   GENERATE REFS(RNVT,NVTWTOR,MSGL355A) SETS(RPARM,REXIT,RENTRY,MSGBUF
*       )(IEAPMNIP TYPE=WTOR,PARAM=MSGL355A);/* ASK FOR NEW SPEC     */
         IEAPMNIP TYPE=WTOR,PARAM=MSGL355A
*   GOTO NPAHCRCT;                  /* VALIDITY CHECK REPLY          */
         B     NPAHCRCT                                            0294
*NPAHCINV:                          /* HARDCOPY SPECFICATION INVALID
*                                      - ASK FOR NEW SPECIFICATION   */
*   IF UCMSYSB='1'B THEN            /* IS HARDCOPY REQUIRED          */
NPAHCINV TM    UCMSYSB(UCMMCSPT),B'01000000'                       0295
         BO    @RT00295                                            0295
*     GO TO NPAHCRSP;               /* YES, ASK FOR NEW SPEC         */
*   MSGBUF=MSGBUF&&MSGBUF;          /* CLEAR MESSAGE BUF             */
         XC    MSGBUF(80),MSGBUF                                   0297
