*/*    SUBROUTINE NPADEVSR                                           */
*/*      DETERMINES IF THE SPECIFIED HARDCOPY DEVICE                 */
*/*      OR LOG IS USABLE                                            */
*/************************************************************       */
*/*    INPUT                                                         */
*/*       DEVICE    THREE CHARACTER HEX DEVICE ADDRESS               */
*/*       UCMBASPT  UCM FOUNDATION BASE REGISTER                     */
*/*       UCMMCSPT  UCM PREFIX BASE REGISTER                         */
*/*    OUTPUT                                                        */
*/*       UCMDISPB                                                   */
*/*       UCMHCUCM                                                   */
*/*    RETURN CODES                                                  */
*/*       ZERO      VALID HARDCOPY DEVICE                            */
*/*       FOUR      INVALID HARDCOPY DEVICE                          */
*/*    EXTERNAL INTERFACES                                           */
*/*       UCBFN                                                      */
*/************************************************************       */
*                                                                  0381
*NPADEVSR:                                                         0381
*   PROCEDURE OPTIONS(NOSAVEAREA);  /* NPADEVSR ENTRY POINT          */
         B     @PB00002                                            0381
NPADEVSR STM   @14,@12,12(@13)                                     0381
*   RESPECIFY                                                      0382
*    (RPARM,                                                       0382
*     RNVT,                                                        0382
*     RCVT,                                                        0382
*     RPTAB,                                                       0382
*     RCODE,                                                       0382
*     UCMBASPT,                                                    0382
*     UCMMCSPT,                                                    0382
*     RUCME,                                                       0382
*     RSCAN,                                                       0382
*     UCBPTR) RESTRICTED;                                          0382
*   RPARM=DEVICE;                   /* GET EBCDIC DEVICE ADDR. FOR 0383
*                                      NIPUCBFN                      */
*                                                                  0383
         L     RPARM,DEVICE                                        0383
*   /*****************************************************************/
*   /*                                                               */
*   /* CALL NIP UCB FIND ROUTINE                                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0384
*   GEN REFS(RNVT,NVTUCBFN,RPARM) SETS(RPARM,REXIT,RENTRY)         0384
*       (IEAPMNIP     TYPE=UCBFN,PARAM=RPARM);                     0384
         IEAPMNIP     TYPE=UCBFN,PARAM=RPARM
*   UCBPTR=RPARM;                   /* PICK UP ADDRESS OF UCB        */
         LR    UCBPTR,RPARM                                        0385
*   IF UCBPTR=0 THEN                /* TEST FOR UCB FOUND            */
         LTR   UCBPTR,UCBPTR                                       0386
         BZ    @RT00386                                            0386
*     GO TO NPADEINV;               /* UCB NOT FOUND - DEVICE IS   0387
*                                      UNACCEPTABLE                  */
*   IF UCBONLI='0'B                /* UCB ONLINE?           @ZA01929*/
*       UCBDUC='1'B                /* UNIT CHECK IND.       @ZA04124*/
*       UCBNRY='1'B THEN            /* OR NOT READY          @ZA01929*/
         TM    UCBONLI(UCBPTR),B'10000000'                         0388
         BZ    @RT00388                                            0388
         TM    UCBDUC(UCBPTR),B'00100000'                          0388
         BO    @RT00388                                            0388
         TM    UCBNRY(UCBPTR),B'01000000'                          0388
         BO    @RT00388                                            0388
*     GOTO NPADEUNA;                /* DEVICE IS OFFLINE     @ZA01929
*                                      UNIT CHECK STATUS     @ZA04124
*                                      OR NOT READY - NOT    @ZA01929
*                                      AVAILABLE AS          @ZA01929
*                                      HARDCOPY              @ZA01929*/
*   DO RUCME=UCMVEA TO UCMVEL BY UCMVEZ;/* LOOP THROUGH ALL UCM    0390
*                                      DEVICE ENTRIES UNTIL THE    0390
*                                      SPECIFIED CONSOLE IS FOUND    */
         L     RUCME,UCMVEA(,UCMBASPT)                             0390
         B     @DE00390                                            0390
@DL00390 DS    0H                                                  0391
*     IF UCBPTR=UCMUCB THEN         /* IS THIS SPECIFIED CONSOLE     */
         C     UCBPTR,UCMUCB(,RUCME)                               0391
         BNE   @RF00391                                            0391
*       DO;                         /* YES, THIS IS SPEC DEVICE      */
*         IF UCMDISPC='1'B THEN     /* TEST FOR GRAPHICS CONSOLE     */
         TM    UCMDISPC(RUCME),B'00100000'                         0393
         BO    @RT00393                                            0393
*           GO TO NPADEINV;         /* YES, IT IS A GRAPHICS CONSOLE,
*                                      WHICH IS INVALID AS THE     0394
*                                      HARDCOPY CONSOLE              */
*         IF UCMOF='0'B THEN        /* TEST FOR OUTPUT CAPABILITY    */
         TM    UCMOF(RUCME),B'10000000'                            0395
         BZ    @RT00395                                            0395
*           GO TO NPADEINV;         /* INPUT ONLY CONSOLE - INVALID
*                                      AS THE HARDCOPY CONSOLE       */
*         ELSE                      /* VALID HARDCOPY CONSOLE        */
*           DO;                     /* FLAG DEVICE AS HARDCOPY       */
*             UCMDISPB='1'B;        /* FLAG ENTRY AS HARDCOPY CONSOLE*/
         OI    UCMDISPB(RUCME),B'01000000'                         0398
*             UCMHCUCM=RUCME;       /* PUT ENTRY ADDRESS IN UCM    0399
*                                      PREFIX                        */
         ST    RUCME,UCMHCUCM(,UCMMCSPT)                           0399
*             RETURN CODE(ZERORC);  /* RETURN TO CALLER - VALID    0400
*                                      HARDCOPY CONSOLE SPECIFIED    */
         SLR   @15,@15                                             0400
         L     @14,12(,@13)                                        0400
         LM    @00,@12,20(@13)                                     0400
         BR    @14                                                 0400
*           END;                                                   0401
*       END;                                                       0402
*   END;                            /* END OF UCM DEVICE ENTRY SCAN
*                                      LOOP - TEST NEXT DEVICE ENTRY */
@RF00391 AL    RUCME,UCMVEZ(,UCMBASPT)                             0403
@DE00390 C     RUCME,UCMVEL(,UCMBASPT)                             0403
         BNH   @DL00390                                            0403
*NPADEINV:                          /* INVALID HARDCOPY DEVICE       */
*   RETURN CODE(FOURRC);            /* RETURN TO CALLER              */
NPADEINV LA    @15,4                                               0404
         L     @14,12(,@13)                                        0404
         LM    @00,@12,20(@13)                                     0404
         BR    @14                                                 0404
*NPADEUNA:                          /* HARDCOPY CONSOLE OFFLINE      */
*   RETURN CODE(EIGHTRC);           /* RETURN TO CALLER              */
NPADEUNA LA    @15,8                                               0405
         L     @14,12(,@13)                                        0405
         LM    @00,@12,20(@13)                                     0405
         BR    @14                                                 0405
*   RESPECIFY                                                      0406
*    (RPARM,                                                       0406
*     RNVT,                                                        0406
*     RCVT,                                                        0406
*     RPTAB,                                                       0406
*     RCODE,                                                       0406
*     UCMBASPT,                                                    0406
*     UCMMCSPT,                                                    0406
*     RUCME,                                                       0406
*     RSCAN,                                                       0406
*     UCBPTR) UNRESTRICTED;                                        0406
*   END NPADEVSR;                   /* END OF SUBROUTINE             */
@PB00002 DS    0H                                                  0408
*   GENERATE NOREFS NOSETS(EJECT);                                 0408
*                                                                  0408
         EJECT
