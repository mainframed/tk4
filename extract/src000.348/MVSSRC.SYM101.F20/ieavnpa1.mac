*/************************************************************       */
*/*    INPUT                                                         */
*/*       RSCAN     ADDRESS OF PARAMETER                             */
*/*       MSG4P     PARAMETER TYPE FOR WTOR                          */
*/*    RETURN CODES                                                  */
*/*       ZERO      NO CONTINUATION                                  */
*/*       FOUR      VALID CONTINUATION                               */
*/*       EIGHT     INVALID CONTINUATION                             */
*/************************************************************       */
*                                                                  0433
*NPACONT:                                                          0433
*   PROCEDURE OPTIONS(NOSAVEAREA);  /* NPACONT ENTRY POINT           */
         B     @PB00004                                            0433
NPACONT  STM   @14,@12,12(@13)                                     0433
*   RESPECIFY                                                      0434
*    (RNVT,                                                        0434
*     RCVT,                                                        0434
*     RPTAB,                                                       0434
*     RSCAN,                                                       0434
*     RTSCAN) RESTRICTED;                                          0434
*   DO RTSCAN=RSCAN TO BUFEND;      /* LOOP THROUGH PARAMETER BUFFER
*                                      LOOKING FOR 'CONT'            */
         LR    RTSCAN,RSCAN                                        0435
         B     @DE00435                                            0435
@DL00435 DS    0H                                                  0436
*     IF TSCAN=CONTCK THEN          /* TEST FOR 'CONT'               */
         CLC   TSCAN(4,RTSCAN),CONTCK                              0436
         BE    @RT00436                                            0436
*       GO TO NPACOCON;             /* 'CONT' FOUND - LET OPERATOR 0437
*                                      CONTINUE SPECIFICATION        */
*   END;                            /* END OF LOOP - CONTINUE SCAN   */
         AH    RTSCAN,@CH00051                                     0438
@DE00435 C     RTSCAN,BUFEND                                       0438
         BNH   @DL00435                                            0438
*   RETURN CODE(ZERORC);            /* 'CONT' NOT FOUND - RETURN TO
*                                      CALLER - INDICATE NO        0439
*                                      CONTINUATION                  */
         SLR   @15,@15                                             0439
         L     @14,12(,@13)                                        0439
         LM    @00,@12,20(@13)                                     0439
         BR    @14                                                 0439
*NPACOCON:                          /* 'CONT' FOUND IN INPUT -     0440
*                                      REQUEST CONTINUATION          */
*   CONTBUF=CONTBUF&&CONTBUF;       /* CLEAR BUFFER                  */
*                                                                  0440
NPACOCON XC    CONTBUF(80),CONTBUF                                 0440
*   /*****************************************************************/
*   /*                                                               */
*   /* IEA330A CONTINUE PRM PARM                                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0441
*   GENERATE REFS(RNVT,NVTWTOR,MSGL330A,MSG4P) SETS(RPARM,REXIT,RENTRY,
*       MSGBUF)(IEAPMNIP TYPE=WTOR,PARAM=MSGL330A);/* WTOR MESSAGE   */
         IEAPMNIP TYPE=WTOR,PARAM=MSGL330A
*   DO RTSCAN=ADDR(CONTBUF) TO CONTEND;/* SCAN CONTINUATION BUFFER 0442
*                                      FOR 'CONT'                    */
         LA    RTSCAN,CONTBUF                                      0442
         B     @DE00442                                            0442
@DL00442 DS    0H                                                  0443
*     IF TSCAN=CONTCK THEN          /* TEST FOR 'CONT'               */
         CLC   TSCAN(4,RTSCAN),CONTCK                              0443
         BNE   @RF00443                                            0443
*       RETURN CODE(EIGHTRC);       /* 'CONT' FOUND - INVALID      0444
*                                      CONTINUATION - RETURN TO    0444
*                                      CALLER WITH RC = 8            */
         LA    @15,8                                               0444
         L     @14,12(,@13)                                        0444
         LM    @00,@12,20(@13)                                     0444
         BR    @14                                                 0444
*   END;                            /* END OF CONTINUATION BUFFER  0445
*                                      SCAN LOOP                     */
@RF00443 AH    RTSCAN,@CH00051                                     0445
@DE00442 C     RTSCAN,CONTEND                                      0445
         BNH   @DL00442                                            0445
*   RETURN CODE(FOURRC);            /* RETURN TO CALLER - VALID    0446
*                                      CONTINUATION SPECIFIED        */
         LA    @15,4                                               0446
         L     @14,12(,@13)                                        0446
         LM    @00,@12,20(@13)                                     0446
         BR    @14                                                 0446
