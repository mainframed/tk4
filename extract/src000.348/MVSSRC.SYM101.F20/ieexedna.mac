         TITLE 'IEEXEDNA - DISPLAY CONSOLES COMMAND PROCESSOR          *
                        '
IEEXEDNA CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(24)                                             0001
         DC    C'IEEXEDNA 78061  UZ17073 '                         0001
         DROP  @15
@PROLOG  BALR  @09,0                                               0001
@PSTART  LA    @03,4095(,@09)                                      0001
         USING @PSTART,@09                                         0001
         USING @PSTART+4095,@03                                    0001
         LR    @12,@01                                             0001
         L     @00,@SIZDATD                                        0001
         GETMAIN  R,LV=(0)
         LR    @10,@01                                             0001
         USING @DATD,@10                                           0001
         LR    @01,@12                                             0001
         ST    @13,@SA00001+4                                      0001
         LR    @13,@10                                             0001
         MVC   @PC00001(4),0(@01)                                  0001
TOPSEG   DS    0H                                                  0148
*   CSCBPTR=R1;                     /* SAVE POINTER TO CSCB          */
         LR    CSCBPTR,R1                                          0148
*   RETADDR=R14;                    /* SAVE RETURN ADDRESS           */
         ST    R14,RETADDR                                         0149
*   UCMBASPT=CVTCUCB;               /* SET UCM BASE PTR      @ZA11392*/
         L     @12,CVTPTR                                          0150
         L     UCMBASPT,CVTCUCB(,@12)                              0150
*   UCMMCSPT=UCMPRFXP;              /* SET UCM PREFIX REG            */
         LR    @12,UCMBASPT                                        0151
         AL    @12,@CF02811                                        0151
         L     @12,UCMPRFXP(,@12)                                  0151
         ST    @12,UCMMCSPT                                        0151
*   MAXUCMES=(UCMVEL-UCMVEA)/UCMVEZ+1;/* UCMES IN SYS                */
         L     @12,UCMVEL(,UCMBASPT)                               0152
         SL    @12,UCMVEA(,UCMBASPT)                               0152
         LR    @06,@12                                             0152
         SRDA  @06,32                                              0152
         D     @06,UCMVEZ(,UCMBASPT)                               0152
         AL    @07,@CF00068                                        0152
         ST    @07,MAXUCMES                                        0152
*   GEN(LOAD EP=IEECB860);          /* LOAD STAE CREATION ROUTINE    */
         LOAD EP=IEECB860
*   CBADDR=R0;                      /* SAVE ADDRESS OF CB860         */
         LR    CBADDR,R0                                           0154
*   WORD1=CSCBPTR;                  /* CSCB PTR INTO PARMLIST+0      */
         ST    CSCBPTR,WORD1                                       0155
*   WORD2=ADDR(COMMNAME);           /* PTR TO D CONSOLES COMMAND     */
         LA    @12,COMMNAME                                        0156
         ST    @12,WORD2                                           0156
*   WORD3=RETADDR;                  /* RETURN PT ADDR INTO PARMLST   */
         L     @12,RETADDR                                         0157
         ST    @12,WORD3                                           0157
*   R15=CBADDR;                     /* GET CB860 EP ADDR             */
         LR    R15,CBADDR                                          0158
*   R1=ADDR(WORD1);                 /* SET POINTER TO PARMLIST       */
         LA    R1,WORD1                                            0159
*   GENERATE;                                                      0160
*                                      /* CODE TO GO TO CB860 YM02650*/
         STM   14,12,12(13)        SAVE REGS BEFORE CB860   YM02650
         BALR 14,15                  GO ESTAB. STAE ENVIRON.  YM02650
         LM    0,12,20(13)         RESTORE REGS             YM02650
*   RCODE=R15;                      /* SAVE RETURN CODE              */
         LR    RCODE,R15                                           0161
*   TARGID=CHCNID;                  /* SAVE TARGET. CONSOLE ID     0162
*                                                            @ZA15315*/
         SLR   @12,@12                                             0162
         IC    @12,CHCNID(,CSCBPTR)                                0162
         STH   @12,TARGID                                          0162
*   R0SAVE=TARGID;                  /* SET R0SAVE FOR 1ST WTO      0163
*                                                            @ZA15315*/
         ST    @12,R0SAVE                                          0163
*   TARGAREA=CHARID;                /* SAVE DISP. SCREEN AREA ID   0164
*                                                            @ZA15315*/
         MVC   TARGAREA(1),CHARID(CSCBPTR)                         0164
*   CHFC='1'B;                      /* SIGNAL FREE THIS CSCB         */
         OI    CHFC(CSCBPTR),B'00000010'                           0165
*   R15=CSCBPTR;                    /* GET CSCB ADDR. ISSUE THE      */
         LR    R15,CSCBPTR                                         0166
*   GEN(MGCR (15),CHAIN);           /* MGCR MACRO TO FREE THE CORE   */
         MGCR (15),CHAIN
*   RFY                                                            0168
*     R0 UNRSTD;                    /*                       @ZA11392*/
*                                                                  0168
*   /*****************************************************************/
*   /*                                                               */
*   /* HAVE NOW FINISHED INITIAL SETUP. NOW CHECK FOR GOOD RET. CODE */
*   /* FROM CB860. IF ZERO, CONTINUE. IF NOT, DELETE CB860, FREE OUR */
*   /* CORE, AND RETURN, TO CALLER.                                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0169
*   BITBYTE=0;                      /* INIT ALL BITS TO ZERO @ZA15750*/
         MVI   BITBYTE,X'00'                                       0169
*   IF RCODE^=0                     /* IF BAD RET. CODE FROM CB860   */
*     THEN                          /* THEN GOTO CLEANUP RTN. THE    */
         LTR   RCODE,RCODE                                         0170
         BZ    @RF00170                                            0170
*     CALL CLEANUP;                 /* CLEANUP RTN. WILL NOT RETURN  */
         BAL   @14,CLEANUP                                         0171
*   RFY                                                            0172
*    (R4,                                                          0172
*     R5) RSTD;                     /*                       @ZA11392*/
@RF00170 DS    0H                                                  0173
*   R5=MAXUCMES*(LENGTH(CONSENT))+  /* # UCMES * DATA SAVE AREA LEN
*                                      FOR EACH CONS         @ZA11392*/
*       LENGTH(WPLFLGS)+            /* PLUS LEN OF ROUT + DESC CODES
*                                      FOR CNTL LINE         @ZA11392*/
*       LENGTH(WPLLS01)+            /* PLUS LEN OF LINE TYPE,AREA ID
*                                      AND # LINES FOR CNTL LINE   0173
*                                                            @ZA11392*/
*       10*LENGTH(DLINE);           /* PLUS LEN OF TEN DATA LINES  0173
*                                                            @ZA11392*/
         L     R5,MAXUCMES                                         0173
         MH    R5,@CH00077                                         0173
         AL    R5,@CF02802                                         0173
*   WKLEN=R5;                       /* SAVE GOTTEN CORE LENGTH       */
         STH   R5,WKLEN                                            0174
*   GEN(GETMAIN R,LV=(5),SP=229);   /* GET AREA FROM SUBPOOL 229     */
         GETMAIN R,LV=(5),SP=229
*   CONSPTR=R1;                     /* SET BASE TO DATA-SAVE-ENT     */
         LR    CONSPTR,R1                                          0176
*   R4=R1;                          /* SET POINTER TO THE CORE       */
         LR    R4,R1                                               0177
*   R14=R1;                         /* SET POINTER TO THE CORE       */
         LR    R14,R1                                              0178
*   R15=0;                          /* SIGNAL ZERO OPERATION         */
         SLR   R15,R15                                             0179
*   GEN(MVCL R4,R14);               /* ZERO THE CORE GOTTEN          */
         MVCL R4,R14
*   RFY                                                            0181
*    (R1,                                                          0181
*     R4,                                                          0181
*     R5,                                                          0181
*     R14,                                                         0181
*     R15) UNRSTD;                  /*                       @ZA11392*/
*                                                                  0181
*   /*****************************************************************/
*   /*                                                               */
*   /* NOW SET WPLBASE TO POINT INTO THE AREA GOTTEN, TO THE SECTION */
*   /* TO BE USED FOR MULTI-LINE WTO PARAMETER LIST                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0182
*   GCORE='1'B;                     /* SIGNAL CORE GOTTEN            */
         OI    GCORE,B'00000100'                                   0182
*   WPLBASE=CONSPTR+MAXUCMES*LENGTH(CONSENT);/* SET BASE     @ZA15315*/
         L     @12,MAXUCMES                                        0183
         MH    @12,@CH00077                                        0183
         ALR   @12,CONSPTR                                         0183
         ST    @12,WPLBASE                                         0183
*   WPLPTR=WPLBASE;                 /* SET PTR TO 1ST WPL    @ZA15315*/
*                                                                  0184
         LR    WPLPTR,@12                                          0184
*   /*****************************************************************/
*   /*                                                               */
*   /* NOW CALL TITLEBLD AND LABELBLD TO BUILD THE FIRST THREE LINES */
*   /* OF THE MULTI-LINE WTO IN THE PARAMETER LIST                   */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0185
*   CALL TITLEBLD;                  /* BUILD CONTROL LINE FOR WTO    */
         BAL   @14,TITLEBLD                                        0185
*   CALL SETLOCK;                   /* OBTAIN LOCAL/CMS LOCKS      0186
*                                                            @ZA11392*/
         BAL   @14,SETLOCK                                         0186
*   CALL LABELBLD;                  /* BUILD LABEL LINE FOR WTO      */
*                                                                  0187
         BAL   @14,LABELBLD                                        0187
*   /*****************************************************************/
*   /*                                                               */
*   /* CALL GETDATA TO GATHER DATA FROM THE UCMES,UCBS       @ZA11392*/
*   /* AND DCMS.                                             @ZA11392*/
*   /* THEN CALL FREELOCK TO RELEASE THE LOCKS HELD.                 */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0188
*   CALL GETDATA;                   /* GATHER DATA FOR MESSAGES      */
         BAL   @14,GETDATA                                         0188
*   CALL FREELOCK;                  /* FREE LOCAL/CMS LOCKS          */
         BAL   @14,FREELOCK                                        0189
*   INDEX=0;                        /* CLEAR COUNT OF # OF CONSOLES
*                                      PROCESSED. NO ENTRY INTO ARRAY
*                                      EXISTS. THIS CODE WAS ADDED 0190
*                                      FOR ZA07158.          @ZA15315*/
         SLR   INDEX,INDEX                                         0190
*   CALL MSGSET;                    /* INIT NEXT WPL         @ZA15315*/
*                                                                  0191
         BAL   @14,MSGSET                                          0191
*   /*****************************************************************/
*   /*                                                               */
*   /* CALL HARDCOPY TO SEE IF A HARDCOPY MESSAGE SHOULD BE WRITTEN. */
*   /* IF SO, THEN FINISH THE FIELDS IN THE HARDCOPY MESSAGE. IF NOT */
*   /* THEN CONTINUE TO NORMAL PROCESSING OF EACH SEPARATE UCM ENTRY,*/
*   /* TO BUILD ONE DATA LINE PER UCME.                              */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0192
*   CALL HARDCOPY;                  /* PREPARE HC. STATUS MSG.       */
*                                                                  0192
         BAL   @14,HARDCOPY                                        0192
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK TO SEE IF HARDCOPY MESSAGE IS BEING BUILT               */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0193
*   IF HDCOPY='1'B                  /* IF THIS IS THE HARDCOPY MSG   */
*     THEN                          /* BEING WRITTEN, THEN IF HC.    */
         TM    HDCOPY,B'01000000'                                  0193
         BNO   @RF00193                                            0193
*     DO;                           /*                       @ZA12702*/
*                                                                  0194
*       /*************************************************************/
*       /*                                                           */
*       /* CHECK FOR HARDCOPY TO SYSLOG IF TO SYSLOG, THEN NO CONSOLE*/
*       /* ID WILL BE PUT IN THE MESSAGE. ALSO NO AREA ID WILL BE PUT*/
*       /* INTO THE MESSAGE.                                         */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0195
*       IF HCUCM=0                  /* IS TO SYSLOG, GO FINISH MSG   */
*         THEN                      /* IF SYSLOG,BYPASS CONSOLE ID   */
         LTR   HCUCM,HCUCM                                         0195
         BNZ   @RF00195                                            0195
*         CALL MSGPROC2;            /* AND AREA ID PROCESSING. IF    */
         BAL   @14,MSGPROC2                                        0196
*       ELSE                        /* TO A CONSOLE, INCLUDE ID &    */
*         CALL MSGPROC;             /* AREA ID SECTIONS OF MSG       */
         B     @RC00195                                            0197
@RF00195 BAL   @14,MSGPROC                                         0197
*       HDCOPY='0'B;                /* HC LINE HAS BEEN BUILT      0198
*                                                            @ZA12702*/
@RC00195 NI    HDCOPY,B'10111111'                                  0198
*     END;                          /*                       @ZA12702*/
*                                                                  0199
*   /*****************************************************************/
*   /*                                                               */
*   /* THE FOLLOWING LOOP IS EXECUTED ONCE FOR EACH UCM ENTRY IN THE */
*   /* SYSTEM. THE UCME IS SKIPPED IF IT IS PART OF A COMPOSITE      */
*   /* CONSOLE AND HAS ONLY OUTPUT CAPABILITY. IN THAT CASE ITS      */
*   /* STATUS WILL BE REFLECTED IN THE MESSAGE BUILT TO REPRESENT THE*/
*   /* STATUS OF THE INPUT HALF OF THE COMPOSITE CONSOLE. IF THE     */
*   /* OUTPUT HALF'S UCME IS                                 @ZA15315*/
*   /* THE LAST ONE THEN THE PREVIOUS LINE WILL HAVE         @ZA15315*/
*   /* ALREADY BEEN MARKED AS THE END LINE.                  @ZA15315*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0200
*   DO INDEX=1 TO MAXUCMES;         /* LOOP OVER ALL UCME DATA-      */
*                                                                  0200
@RF00193 LA    INDEX,1                                             0200
         B     @DE00200                                            0200
@DL00200 DS    0H                                                  0201
*     /***************************************************************/
*     /*                                                             */
*     /* CHECK FOR PART OF COMPOSITE CONSOLE AND OUTPUT HALF         */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0201
*     IF COMPC(INDEX)=0             /* SAVE-AREAS. IF PART OF COMP   */
*         ×                         /* OSITE CONSOLE AND OUTPUT      */
*         MIF(INDEX)='1'B           /* ONLY DEVICE, IGNORE IT, AND   */
*       THEN                        /* GO TO NEXT ENTRY. OTHERWISE   */
         LR    @12,INDEX                                           0201
         MH    @12,@CH00077                                        0201
         LR    @05,CONSPTR                                         0201
         AL    @05,@CF02815                                        0201
         LH    @05,COMPC-14(@12,@05)                               0201
         LTR   @05,@05                                             0201
         BZ    @RT00201                                            0201
         ALR   @12,CONSPTR                                         0201
         AL    @12,@CF02816                                        0201
         TM    MIF-5(@12),B'01000000'                              0201
         BNO   @RF00201                                            0201
@RT00201 DS    0H                                                  0202
*       DO;                         /* FILL IN UCB NAMES IN MSG      */
*         CALL UNITNAME;            /* FILL IN CONSOLE CONDI-        */
         BAL   @14,UNITNAME                                        0203
*         CALL MSGCOND;             /* TION AND AUTHORITY            */
         BAL   @14,MSGCOND                                         0204
*         CALL AUTHCHK;             /* COLUMNS IN MESSAGE            */
         BAL   @14,AUTHCHK                                         0205
*         INDEXSVE=INDEX;           /* SAVE INDEX OF CONS    @ZA11392*/
         LR    INDEXSVE,INDEX                                      0206
*         IF COMPC(INDEX)^=0 THEN   /* IS THIS A COMPOSITE CONSOLE?
*                                                            @ZA11392*/
         LR    @12,INDEX                                           0207
         MH    @12,@CH00077                                        0207
         AL    @12,@CF02815                                        0207
         LH    @12,COMPC-14(@12,CONSPTR)                           0207
         LTR   @12,@12                                             0207
         BZ    @RF00207                                            0207
*           INDEX=COMPC(INDEX);     /* YES, POINT TO OUTPUT HALF   0208
*                                                            @ZA11392*/
         LR    INDEX,@12                                           0208
*         CALL TOEBCDIC(WQECNT(INDEX),NWQES);/* CONVERT NUMBER OF  0209
*                                      WQES TO EBCDIC AND PUT IN MSG
*                                                            @ZA11392*/
@RF00207 LR    @12,INDEX                                           0209
         MH    @12,@CH00077                                        0209
         AL    @12,@CF02817                                        0209
         LA    @12,WQECNT-2(@12,CONSPTR)                           0209
         ST    @12,@AL00001                                        0209
         LA    @12,NWQES(,WPLPTR)                                  0209
         ST    @12,@AL00001+4                                      0209
         LA    @01,@AL00001                                        0209
         BAL   @14,TOEBCDIC                                        0209
*         INDEX=INDEXSVE;           /* RESTORE INDEX TO INPUT HALF 0210
*                                                            @ZA11392*/
         LR    INDEX,INDEXSVE                                      0210
*         CALL MSGPROC;             /* GO FINISH MESSAGE INFO        */
         BAL   @14,MSGPROC                                         0211
*       END;                        /* END GOOD UCME PROCESSING      */
*   END;                            /* END UCME LOOP                 */
@RF00201 AL    INDEX,@CF00068                                      0213
@DE00200 C     INDEX,MAXUCMES                                      0213
         BNH   @DL00200                                            0213
         EJECT
*                                                                  0214
*/********************************************************************/
*/*                                                                  */
*/* ENTRY - CLEANUP                                          @ZA15750*/
*/*                                                                  */
*/* INPUTS - WRKLEN, LENGTH OF GETMAIN AREA TO FREE          @ZA15750*/
*/* OUTPUTS - NONE.  RETURN IS TO THE SYSTEM VIA BR 14.              */
*/*                                                                  */
*/********************************************************************/
*                                                                  0214
*CLEANUP:                                                          0214
*   ;                               /* CLEANUP AND EXIT RTN  @ZA15750*/
CLEANUP  DS    0H                                                  0215
*   RFY                                                            0215
*    (R11,                                                         0215
*     R12) RSTD;                    /*                       @ZA15750*/
*   GEN(DELETE EP=IEECB860);        /* DELETE STAE ROUTINE           */
         DELETE EP=IEECB860
*   R11=WKLEN;                      /* GET LENGTH TO FREE            */
         LH    R11,WKLEN                                           0217
*   R12=CONSPTR;                    /* GET START ADDR TO FREE      0218
*                                                            @ZA15750*/
         LR    R12,CONSPTR                                         0218
*   IF GCORE='1'B                   /* IF CORE WAS GOTTEN            */
*     THEN                          /* THEN WE MUST FREE IT          */
         TM    GCORE,B'00000100'                                   0219
         BNO   @RF00219                                            0219
*     GEN(FREEMAIN R,LV=(11),A=(12),SP=229);/* FREE WORKAREA         */
         FREEMAIN R,LV=(11),A=(12),SP=229
*   RETURN TO RETADDR;              /* RETURN TO SYSTEM      @ZA15750*/
@RF00219 L     @02,RETADDR                                         0221
         L     @13,4(,@13)                                         0221
         L     @00,@SIZDATD                                        0221
         LR    @01,@10                                             0221
         FREEMAIN R,LV=(0),A=(1)
         LR    @14,@02                                             0221
         BR    @14                                                 0221
*   RFY                                                            0222
*    (R11,                                                         0222
*     R12) UNRSTD;                  /*                       @ZA15750*/
         EJECT
*                                                                  0223
*/********************************************************************/
*/*                                                                  */
*/* PROCEDURE - MSGPROC                                              */
*/*                                                                  */
*/* INPUTS - CONSOLE DATA-SAVE-ENTRIES.                              */
*/*                                                                  */
*/* OUTPUTS - BUILDS CONSOLE ID AND GRAPHICS AREA ID COLUMNS OF LINE */
*/*                                                                  */
*/********************************************************************/
*                                                                  0223
*MSGPROC:                                                          0223
*   PROC;                           /* CONSOLE ID/GRAPHICS PROCESS   */
@EL00001 L     @13,4(,@13)                                         0223
@EF00001 L     @00,@SIZDATD                                        0223
         LR    @01,@10                                             0223
         FREEMAIN R,LV=(0),A=(1)
@ER00001 BR    @14                                                 0223
MSGPROC  STM   @14,@03,@SA00002                                    0223
         STM   @05,@10,@SA00002+24                                 0223
         ST    @12,@SA00002+48                                     0223
*   CALL CONSID;                    /* FILL CONSOLE ID INTO MSG      */
*                                                                  0224
         BAL   @14,CONSID                                          0224
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK TO SEE IF GRAPHICS IS IN THE SYSTEM                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0225
*   IF UCMSYSF='1'B                 /* IF THIS SYSTEM HAS GRAPHICS   */
*     THEN                          /* THEN FILL IN AREA ID          */
         L     @12,UCMMCSPT                                        0225
         TM    UCMSYSF(@12),B'00000100'                            0225
         BNO   @RF00225                                            0225
*     CALL GRAPHICS;                /* SET AREA IDS                  */
         BAL   @14,GRAPHICS                                        0226
*   CALL MSGPROC2;                  /* GO FINISH MSG                 */
@RF00225 BAL   @14,MSGPROC2                                        0227
*   RETURN;                         /* RETURN TO CALLER              */
@EL00002 DS    0H                                                  0228
@EF00002 DS    0H                                                  0228
@ER00002 LM    @14,@03,@SA00002                                    0228
         LM    @05,@10,@SA00002+24                                 0228
         L     @12,@SA00002+48                                     0228
         BR    @14                                                 0228
*   END MSGPROC;                    /* END CONSID/GRAPHICS PROC.     */
         EJECT
*                                                                  0230
*/********************************************************************/
*/*                                                                  */
*/* PROCEDURE - MSGPROC2                                             */
*/*                                                                  */
*/* INPUTS - CONSOLE DATA-SAVE-ENTRIES.                              */
*/*                                                                  */
*/* OUTPUTS - BUILDS ROUTE CODES COLUMN OF LINE & SETS WPL FOR THE   */
*/*           NEXT LINE.                                             */
*/*                                                                  */
*/********************************************************************/
*                                                                  0230
*MSGPROC2:                                                         0230
*   PROC;                           /* RCODES PROCESSING             */
MSGPROC2 STM   @14,@03,@SA00003                                    0230
         STM   @05,@10,@SA00003+24                                 0230
         ST    @12,@SA00003+48                                     0230
*   CALL RCODES;                    /* PROCESS ROUTE CODES           */
         BAL   @14,RCODES                                          0231
*   CALL MSGSET;                    /* SET WPL FOR NEXT LINE         */
         BAL   @14,MSGSET                                          0232
*   RETURN;                         /* RETURN TO CALLER              */
@EL00003 DS    0H                                                  0233
@EF00003 DS    0H                                                  0233
@ER00003 LM    @14,@03,@SA00003                                    0233
         LM    @05,@10,@SA00003+24                                 0233
         L     @12,@SA00003+48                                     0233
         BR    @14                                                 0233
*   END MSGPROC2;                   /* END RCODES PROCESSING         */
         EJECT
*                                                                  0235
*/********************************************************************/
*/*                                                                  */
*/*  PROCEDURE NAME - TITLEBLD                                       */
*/*                                                                  */
*/*  FUNCTION - BUILD CONTROL LINE FOR THE CONSOLE DISPLAY           */
*/*                                                                  */
*/*  INPUTS - WPLPTR,  POINTING TO BEGINNING OF THE WTO PARAMETER    */
*/*           LIST.                                                  */
*/*                                                                  */
*/*  OUTPUTS - CONTROL LINE BUILT IN WPL WITH TIME STAMP.            */
*/*                                                                  */
*/*  MACROS USED - TIME                                              */
*/*                                                                  */
*/********************************************************************/
*                                                                  0235
*TITLEBLD:                                                         0235
*   PROC OPTIONS(NOSAVEAREA);       /* BUILD CONTROL LINE RTN.       */
TITLEBLD STM   @14,@10,12(@13)                                     0235
         ST    @12,68(,@13)                                        0235
*   WPLMCSFA='1'B;                  /* ROUT/DESC CODES EXIST         */
*   WPLMCSFB='1'B;                  /* REG0 -Q TO CONS IF ACTIVE     */
*   WPLMCSFC='1'B;                  /* COMMAND RESPONSE INDIC        */
         OI    WPLMCSFA(WPLPTR),B'11100000'                        0238
*   WPLMCSFJ='1'B;                  /* TURN ON MLWTO INDICATOR       */
         OI    WPLMCSFJ(WPLPTR),B'01000000'                        0239
*   MCSFSAVE=WPLMCSF;               /* SAVE MCS FLAGS                */
*                                                                  0240
         LH    @12,WPLMCSF(,WPLPTR)                                0240
         N     @12,@CF02819                                        0240
         STH   @12,MCSFSAVE                                        0240
*   /*****************************************************************/
*   /*                                                               */
*   /* GEN A STORE CLOCK INSTRUCTION TO SEE IF THE CLOCK IS WORKING. */
*   /* IF A ZERO RETURN CODE, THEN ITS OKAY TO GO AHEAD AND ISSUE THE*/
*   /* TIME MACRO. IF NOT, MOVE IN TEXT WITHOUT THE TIME STAMP.      */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0241
*   GEN(STCK CONVERT);              /* IS THE CLOCK WORKING ?        */
         STCK CONVERT
*   BC(8,TIMEON);                   /* YES, GO GET TIME              */
         BC    8,TIMEON                                            0242
*   WPLTXT(1:LENGTH(CTEXTNOT))=CTEXTNOT;/* FILL IN SHORT TEXT        */
         MVC   WPLTXT(23,WPLPTR),CTEXTNOT                          0243
*   WPLLGH=LENGTH(CTEXTNOT)+4;      /* SET CNTL LINE LENGTH          */
         MVC   WPLLGH(2,WPLPTR),@CH02803                           0244
*TITLEND:                                                          0245
*   WPLPTR=WPLPTR+WPLLGH;           /* UPDATE PAST TEXT IN WPL       */
TITLEND  AH    WPLPTR,WPLLGH(,WPLPTR)                              0245
*   WPLDESCE='1'B;                  /* DESC. CODE 5                  */
*   WPLDESCH='1'B;                  /* DESC. CODE 8                  */
         OI    WPLDESCE(WPLPTR),B'00001001'                        0247
*   WPLDESCI='1'B;                  /* DESC. CODE 9                  */
         OI    WPLDESCI(WPLPTR),B'10000000'                        0248
*   DESCSAVE=WPLDESC;               /* SAVE THE DESCRIPTOR FLAGS     */
         MVC   DESCSAVE(2),WPLDESC(WPLPTR)                         0249
*   WPLROUT=0;                      /* ZERO ROUTE CODES              */
         SLR   @12,@12                                             0250
         STCM  @12,3,WPLROUT(WPLPTR)                               0250
*   WPLPTR=WPLPTR+4;                /* UPDATE TO NEXT WPL SECTION    */
         LA    @12,4                                               0251
         ALR   WPLPTR,@12                                          0251
*   WPLLTFA='1'B;                   /* LINE TYPE = CONTROL LINE      */
         OI    WPLLTFA(WPLPTR),B'10000000'                         0252
*   WPLAREA=TARGAREA;               /* SET AREA ID-ISSUING CONSOLE   */
         MVC   WPLAREA(1,WPLPTR),TARGAREA                          0253
*   LINEBASE=ADDR(WPLLINES);        /* SAVE PTR TO # OF LINES FLD    */
         LA    @08,WPLLINES(,WPLPTR)                               0254
         ST    @08,LINEBASE                                        0254
*   WPLPTR=WPLPTR+4;                /* UPDATE TO NEXT WPL SECTION    */
         ALR   WPLPTR,@12                                          0255
*   RETURN;                         /* RETURN TO CALLER              */
@EL00004 DS    0H                                                  0256
@EF00004 DS    0H                                                  0256
@ER00004 LM    @14,@10,12(@13)                                     0256
         L     @12,68(,@13)                                        0256
         BR    @14                                                 0256
*   RFY                                                            0257
*     R0 RSTD;                      /*                       @ZA11392*/
*TIMEON:                                                           0258
*   GEN(TIME DEC);                  /* GET THE TIME                  */
TIMEON   TIME DEC
*   CONVERT1=R0;                    /* SAVE TIME FROM REG0           */
         LA    @12,CONVERT                                         0259
         ST    R0,CONVERT1(,@12)                                   0259
*   RFY                                                            0260
*     R0 UNRSTD;                    /*                       @ZA11392*/
*   GEN(UNPK UNPACK(7),CONVERT(4)); /* CONV. TO EBCDIC               */
         UNPK UNPACK(7),CONVERT(4)
*   WPLTXT(1:LENGTH(CNTLTEXT))=CNTLTEXT;/* CONTROL LINE TEXT         */
         MVC   WPLTXT(32,WPLPTR),CNTLTEXT                          0262
*   WPLTXT(9:10)=UNPACK(1:2);       /* SET HOURS IN TIME STAMP       */
         MVC   WPLTXT+8(2,WPLPTR),UNPACK                           0263
*   WPLTXT(12:13)=UNPACK(3:4);      /* SET MINUTES IN STAMP          */
         MVC   WPLTXT+11(2,WPLPTR),UNPACK+2                        0264
*   WPLTXT(15:16)=UNPACK(5:6);      /* SET SECONDS IN TIME STAMP     */
         MVC   WPLTXT+14(2,WPLPTR),UNPACK+4                        0265
*   WPLLGH=LENGTH(CNTLTEXT)+4;      /* SET CNTL LINE LENGTH          */
         MVC   WPLLGH(2,WPLPTR),@CH02804                           0266
*   GOTO TITLEND;                   /* GO FINISH BUILDING LINE       */
         B     TITLEND                                             0267
*   END TITLEBLD;                   /* END CONTROL LINE BUILD RTN    */
         EJECT
