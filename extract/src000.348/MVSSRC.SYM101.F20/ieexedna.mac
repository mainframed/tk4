*/*                                                                  */
*/*  PROCEDURE NAME - LABELBLD                                       */
*/*                                                                  */
*/*  FUNCTION - BUILDS LABEL LINES FOR THE CONSOLE DISPLAY   @ZA11392*/
*/*                                                                  */
*/*  INPUTS - WPLPTR,  POINTING TO NEXT SLOT IN THE WTO PARAMETER    */
*/*           LIST.                                                  */
*/*                                                                  */
*/*  OUTPUTS - 2 LABEL LINES BUILT IN WPLS.                  @ZA11392*/
*/*                                                                  */
*/*  MACROS USED - NONE                                              */
*/*                                                                  */
*/*  CALLS       - TOEBCDIC                                  @ZA11392*/
*/*                                                                  */
*/********************************************************************/
*                                                                  0269
*LABELBLD:                                                         0269
*   PROC;                           /* BUILD LABEL LINE ROUTINE      */
*                                                                  0269
LABELBLD STM   @14,@10,@SA00005                                    0269
         ST    @12,@SA00005+52                                     0269
*   /*****************************************************************/
*   /*                                                               */
*   /* BUILD THE FIRST LABEL LINE                            @ZA11392*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0270
*   WPLMLTXT(1:LENGTH(LAB1TEXT))=LAB1TEXT;/* MOVE TEXT IN WPL      0270
*                                                            @ZA11392*/
         MVC   WPLMLTXT(44,WPLPTR),LAB1TEXT                        0270
*   IF UCMWQNR^=0 THEN              /* IF COUNT IS NOT ZERO THEN   0271
*                                      CONVERT EBCDIC        @ZA11392*/
         LH    @12,UCMWQNR(,UCMBASPT)                              0271
         LTR   @12,@12                                             0271
         BZ    @RF00271                                            0271
*     CALL TOEBCDIC(UCMWQNR,WPLMLTXT(25:28));/* CONVERT CURRENT COUNT
*                                      OF WQE'S TO EBCDIC AND PUT IN
*                                      WPL                   @ZA11392*/
         LA    @12,UCMWQNR(,UCMBASPT)                              0272
         ST    @12,@AL00001                                        0272
         LA    @12,WPLMLTXT+24(,WPLPTR)                            0272
         ST    @12,@AL00001+4                                      0272
         LA    @01,@AL00001                                        0272
         BAL   @14,TOEBCDIC                                        0272
*   IF UCMWQLM1^=0 THEN             /* IF COUNT IS NOT ZERO THEN   0273
*                                      CONVERT TO EBCDIC     @ZA11392*/
@RF00271 LH    @12,UCMWQLM1(,UCMBASPT)                             0273
         LTR   @12,@12                                             0273
         BZ    @RF00273                                            0273
*     CALL TOEBCDIC(UCMWQLM1,WPLMLTXT(41:44));/* CONVERT WQE LIMIT TO
*                                      EBCDIC                @ZA11392*/
         LA    @12,UCMWQLM1(,UCMBASPT)                             0274
         ST    @12,@AL00001                                        0274
         LA    @12,WPLMLTXT+40(,WPLPTR)                            0274
         ST    @12,@AL00001+4                                      0274
         LA    @01,@AL00001                                        0274
         BAL   @14,TOEBCDIC                                        0274
*   WPLMLLTB='1'B;                  /* SET LINE TYPE = LABEL LINE  0275
*                                                            @ZA11392*/
@RF00273 OI    WPLMLLTB(WPLPTR),B'01000000'                        0275
*   WPLMLLEN=LENGTH(LAB1TEXT)+4;    /* LENGTH IN WPL = TEXT LEN +  0276
*                                      HEADER LENGTH         @ZA11392*/
         LA    @12,48                                              0276
         STC   @12,WPLMLLEN(,WPLPTR)                               0276
*   WPLPTR=WPLPTR+WPLMLLEN;         /* NEXT WPL SECTION      @ZA11392*/
*                                                                  0277
         ALR   WPLPTR,@12                                          0277
*   /*****************************************************************/
*   /*                                                               */
*   /* BUILD SECOND LABEL LINE                               @ZA11392*/
*   /* CHECK TO SEE IF GRAPHICS IS IN THE SYSTEM                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0278
*   IF UCMSYSF='1'B                 /* IF GRAPHICS IN SYSTEM,        */
*     THEN                          /* MOVE IN TEXT WITH AREA ID     */
         L     @12,UCMMCSPT                                        0278
         TM    UCMSYSF(@12),B'00000100'                            0278
         BNO   @RF00278                                            0278
*     DO;                           /* COLUMN HEADER.                */
*       WPLMLTXT(1:LENGTH(LABLTEXT))=LABLTEXT;/* MOVE TEXT           */
         MVC   WPLMLTXT(51,WPLPTR),LABLTEXT                        0280
*       WPLMLLEN=LENGTH(LABLTEXT)+4;/* SET LENGTH IN                 */
         MVI   WPLMLLEN(WPLPTR),X'37'                              0281
*     END;                          /* WPL. TEXT LEN + 4 (HEADER)    */
*   ELSE                            /* IF NO GRAPHICS IN SYSTEM      */
*     DO;                           /* THEN MUST USE TEXT WITHOUT  0283
*                                      AREA ID COLUMN HEADER. SET    */
         B     @RC00278                                            0283
@RF00278 DS    0H                                                  0284
*       WPLMLTXT(1:LENGTH(LABLNOGP))=LABLNOGP;/*                     */
         MVC   WPLMLTXT(51,WPLPTR),LABLNOGP                        0284
*       WPLMLLEN=LENGTH(LABLNOGP)+4;/* LENGTH IN WPL                 */
         MVI   WPLMLLEN(WPLPTR),X'37'                              0285
*     END;                          /* =TEXT LEN + HEADER LENGTH     */
*   WPLMLLTB='1'B;                  /* SET LINE TYPE = LABEL LINE    */
@RC00278 OI    WPLMLLTB(WPLPTR),B'01000000'                        0287
*   LINECT=2;                       /* NUMBER OF LINES BUILT-1. COUNT
*                                      WILL BE INCREASED BY A CALL TO
*                                      MSGSET MADE LATER ON. @ZA15315*/
         MVC   LINECT(2),@CH00080                                  0288
*   END LABELBLD;                   /* END LABEL LINE BUILD RTN.     */
@EL00005 DS    0H                                                  0289
@EF00005 DS    0H                                                  0289
@ER00005 LM    @14,@10,@SA00005                                    0289
         L     @12,@SA00005+52                                     0289
         BR    @14                                                 0289
         EJECT
