*/*                                                          @ZA11392*/
*/*  PROCEDURE NAME: IEEXEDNA.SYSLGWQE                       @ZA11392*/
*/*                                                          @ZA11392*/
*/*  FUNCTION: COUNTS THE NUMBER OF WQE'S QUEUED TO HARDCOPY @ZA11392*/
*/*            WHEN SYSLOG IS HARDCOPY.                      @ZA11392*/
*/*                                                          @ZA11392*/
*/*  INPUTS:   UCMWTOQ - ADDRESS OF WQE QUEUE                @ZA11392*/
*/*                                                          @ZA11392*/
*/*  OUTPUTS:  HCWQECNT - NUMBER OF WQE'S QUEUED TO HARDCOPY @ZA11392*/
*/*                                                          @ZA11392*/
*/*  CALLS:    SCANMINR                                      @ZA11392*/
*/*                                                          @ZA11392*/
*/***********************************************************@ZA11392*/
*                                                                  0652
*SYSLGWQE:                                                         0652
*   PROC;                           /*                       @ZA11392*/
SYSLGWQE STM   @14,@12,@SA00019                                    0652
*   HCWQECNT=0;                     /* INIT NUMBER WQE'S TO HARDCOPY
*                                                            @ZA11392*/
         SLR   @12,@12                                             0653
         STH   @12,HCWQECNT                                        0653
*   WQEPTR=UCMWTOQ;                 /* POINT TO START OF QUEUE     0654
*                                                            @ZA11392*/
         L     @12,UCMWTOQ(,UCMBASPT)                              0654
         ST    @12,WQEPTR                                          0654
*   DO WHILE WQEPTR^=0;             /* DO UNTIL END OF QUEUE @ZA11392*/
         B     @DE00655                                            0655
@DL00655 DS    0H                                                  0656
*     IF(WQEQFHC='1'B&              /* WQE QUEUE TO HARDCOPY ON    0656
*                                                            @ZA11392*/
*         WQEBUFC='1'B&             /* WQE READY TO GO TO HARDCOPY 0656
*                                                            @ZA11392*/
*         WQEQDFHC='0'B)            /* WQE NOT QUEUED FOR HARDCOPY 0656
*                                                            @ZA11392*/
*       THEN                        /* SHOULD WQE BE COUNTED TO GO TO
*                                      HARDCPY               @ZA11392*/
         L     @12,WQEPTR                                          0656
         TM    WQEQFHC(@12),B'01000000'                            0656
         BNO   @RF00656                                            0656
         TM    WQEBUFC(@12),B'00100000'                            0656
         BNO   @RF00656                                            0656
         TM    WQEQDFHC(@12),B'00010000'                           0656
         BNZ   @RF00656                                            0656
*       HCWQECNT=HCWQECNT+1;        /* YES, COUNT THE WQE    @ZA11392*/
         LH    @12,HCWQECNT                                        0657
         LA    @12,1(,@12)                                         0657
         STH   @12,HCWQECNT                                        0657
*     IF(WMJMMLWB='1'B&             /* IS THIS A MAJOR WQE?  @ZA11392*/
*         WMJMMIN^=0) THEN          /* AND IS THERE A MINOR  @ZA11392*/
@RF00656 L     @12,WMJPTR                                          0658
         TM    WMJMMLWB(@12),B'01000000'                           0658
         BNO   @RF00658                                            0658
         L     @12,WMJMMIN(,@12)                                   0658
         LTR   @12,@12                                             0658
         BZ    @RF00658                                            0658
*       CALL SCANMINR(HCWQECNT);    /* YES TO BOTH, COUNT NO. OF   0659
*                                      WQE'S WQE'S USED BY MLWTO   0659
*                                                            @ZA11392*/
         LA    @12,HCWQECNT                                        0659
         ST    @12,@AL00001                                        0659
         LA    @01,@AL00001                                        0659
         BAL   @14,SCANMINR                                        0659
*     WQEPTR=WQELKPA;               /* POINT TO NEXT WQE     @ZA11392*/
@RF00658 L     @12,WQEPTR                                          0660
         L     @12,WQELKPA-1(,@12)                                 0660
         LA    @12,0(,@12)                                         0660
         ST    @12,WQEPTR                                          0660
*   END;                            /* OF DO WHILE           @ZA11392*/
@DE00655 L     @12,WQEPTR                                          0661
         LTR   @12,@12                                             0661
         BNZ   @DL00655                                            0661
*   END SYSLGWQE;                   /*                       @ZA11392*/
@EL00019 DS    0H                                                  0662
@EF00019 DS    0H                                                  0662
@ER00019 LM    @14,@12,@SA00019                                    0662
         BR    @14                                                 0662
         EJECT
