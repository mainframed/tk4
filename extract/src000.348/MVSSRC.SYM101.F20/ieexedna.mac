*/*                                                          @ZA11392*/
*/*  PROCEDURE NAME: IEEXEDNA.SCANCQES                       @ZA11392*/
*/*                                                          @ZA11392*/
*/*  FUNCTION: SCANS A CONSOLES CQE CHAIN AND COUNTS THE     @ZA11392*/
*/*            NUMBER OF WQE'S QUEUE TO THE CONSOLE          @ZA11392*/
*/*                                                          @ZA11392*/
*/*  INPUTS:   UCMOUTQ - ADDRESS OF CQE QUEUE                @ZA11392*/
*/*            I - INDEX INTO CONSOLE-DATA-SAVE-ENTRY ARRAY  @ZA11392*/
*/*                                                          @ZA11392*/
*/*  OUTPUTS:  WQECNT(I) - NUMBER OF WQE'S QUEUED TO THIS    @ZA11392*/
*/*                        CONSOLE.                          @ZA11392*/
*/*                                                          @ZA11392*/
*/*  CALLS:    SCANMINR                                      @ZA11392*/
*/*                                                          @ZA11392*/
*/***********************************************************@ZA11392*/
*                                                                  0634
*SCANCQES:                                                         0634
*   PROC;                           /*                       @ZA11392*/
SCANCQES STM   @14,@12,@SA00018                                    0634
*   WQECNT(I)=0;                    /* INIT NUMBER OF WQE'S  @ZA11392*/
         LR    @12,I                                               0635
         MH    @12,@CH00077                                        0635
         SLR   @11,@11                                             0635
         AL    @12,@CF02817                                        0635
         STH   @11,WQECNT-2(@12,CONSPTR)                           0635
*   CQEPTR=UCMOUTQ;                 /* GET ADDR OF 1ST CQE   @ZA11392*/
         L     CQEPTR,UCMOUTQ(,UCMDVEPT)                           0636
*   DO WHILE CQEPTR^=0;             /* DO UNTIL END OF CQE CHAIN   0637
*                                                            @ZA11392*/
         B     @DE00637                                            0637
@DL00637 DS    0H                                                  0638
*     IF CQEEOB='11'B THEN          /* IS THIS END OF CQE BLOCK?   0638
*                                                            @ZA11392*/
         TM    CQEEOB(CQEPTR),B'11000000'                          0638
         BNO   @RF00638                                            0638
*       CQEPTR=CQEWQEA;             /* YES, POINT TO NEXT BLOCK    0639
*                                                            @ZA11392*/
         L     @12,CQEWQEA-1(,CQEPTR)                              0639
         LA    @12,0(,@12)                                         0639
         LR    CQEPTR,@12                                          0639
*     IF CQEENTR='1'B THEN          /* DOES CQE CONTAIN VALID WQE  0640
*                                                            @ZA11392*/
@RF00638 TM    CQEENTR(CQEPTR),B'00000001'                         0640
         BNO   @RF00640                                            0640
*       DO;                         /* YES                   @ZA11392*/
*         WQECNT(I)=WQECNT(I)+1;    /* INCREASE COUNT OF WQE'S     0642
*                                                            @ZA11392*/
         LR    @12,I                                               0642
         MH    @12,@CH00077                                        0642
         LA    @11,1                                               0642
         LR    @07,CONSPTR                                         0642
         AL    @07,@CF02817                                        0642
         AH    @11,WQECNT-2(@12,@07)                               0642
         LR    @07,CONSPTR                                         0642
         AL    @07,@CF02817                                        0642
         STH   @11,WQECNT-2(@12,@07)                               0642
*         WQEPTR=CQEWQEA;           /* ADDR OF WQE           @ZA11392*/
         L     @11,CQEWQEA-1(,CQEPTR)                              0643
         LA    @11,0(,@11)                                         0643
         ST    @11,WQEPTR                                          0643
*         IF CQEMAJOR='1'B&         /* WQE ALSO A MAJOR FOR MLWTO? 0644
*                                                            @ZA11392*/
*             WMJMMIN^=0            /* AND AT LEAST ONE MINOR EXISTS
*                                                            @ZA11392*/
*           THEN                    /*                       @ZA11392*/
         TM    CQEMAJOR(CQEPTR),B'00000100'                        0644
         BNO   @RF00644                                            0644
         L     @11,WMJPTR                                          0644
         L     @11,WMJMMIN(,@11)                                   0644
         LTR   @11,@11                                             0644
         BZ    @RF00644                                            0644
*           CALL SCANMINR(WQECNT(I));/* YES COUNT NUMBER OF WQE'S  0645
*                                      THAT THE MLWTO IS USING     0645
*                                                            @ZA11392*/
         AL    @12,@CF02817                                        0645
         LA    @12,WQECNT-2(@12,CONSPTR)                           0645
         ST    @12,@AL00001                                        0645
         LA    @01,@AL00001                                        0645
         BAL   @14,SCANMINR                                        0645
*       END;                        /*                       @ZA11392*/
@RF00644 DS    0H                                                  0647
*     IF CQEEOB='10'B THEN          /* WAS THIS CQE THE LAST @ZA11392*/
@RF00640 TM    CQEEOB(CQEPTR),B'10000000'                          0647
         BNO   @RF00647                                            0647
         TM    CQEEOB(CQEPTR),B'01000000'                          0647
         BNZ   @RF00647                                            0647
*       CQEPTR=0;                   /* YES, GET OUT OF LOOP  @ZA11392*/
         SLR   CQEPTR,CQEPTR                                       0648
*     ELSE                          /* NO, MORE CQES         @ZA11392*/
*       CQEPTR=CQEPTR+4;            /* POINT TO NEXT CQE     @ZA11392*/
         B     @RC00647                                            0649
@RF00647 AL    CQEPTR,@CF00060                                     0649
*   END;                            /* OF DO WHILE           @ZA11392*/
@RC00647 DS    0H                                                  0650
@DE00637 LTR   CQEPTR,CQEPTR                                       0650
         BNZ   @DL00637                                            0650
*   END SCANCQES;                   /*                       @ZA11392*/
@EL00018 DS    0H                                                  0651
@EF00018 DS    0H                                                  0651
@ER00018 LM    @14,@12,@SA00018                                    0651
         BR    @14                                                 0651
         EJECT
