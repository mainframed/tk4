*/********************************************************************/
*/*                                                                  */
*/*  PROCEDURE - SETLOCK                                             */
*/*                                                                  */
*/*  FUNCTION - OBTAIN LOCAL AND CMS LOCKS TO SERIALIZE USE OF UCM,  */
*/*             UCB'S,DCM'S.                                         */
*/*                                                                  */
*/*  NOTES - SETLOCK MACRO DESTROYS R11-R14. R13 WILL BE SAVED AND   */
*/*          RESTORED.                                               */
*/*                                                                  */
*/*  INPUTS - NONE.                                                  */
*/*                                                                  */
*/*  OUTPUTS - NONE.                                                 */
*/*                                                                  */
*/********************************************************************/
*                                                                  0434
*SETLOCK:                                                          0434
*   PROC OPTIONS(NOSAVEAREA);                                      0434
SETLOCK  STM   @14,@12,12(@13)                                     0434
*   RFY                                                            0435
*    (R0,                                                          0435
*     R13) RSTD;                    /* USE R0 TO SAVE R13    @ZA11392*/
*   R0=R13;                         /* SAVE IT.                Y02751*/
         LR    R0,R13                                              0436
*   GEN REFS(PSALITA,FLC) SETS(R11,R12,R13,R14);                   0437
*                                                     /*       Y02751*/
         SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,                       **
               RELATED=(UCM,IEEXEDNA(FREELOCK))
         SETLOCK OBTAIN,TYPE=CMS,MODE=UNCOND,                          *
               RELATED=(UCM,IEEXEDNA(FREELOCK))
*   R13=R0;                         /* RESTORE R13             Y02751*/
         LR    R13,R0                                              0438
*   RFY                                                            0439
*    (R0,                                                          0439
*     R13) UNRSTD;                  /* FREE R0 FOR USE       @ZA11392*/
*   RETURN;                         /* RETURN TO CALLER        Y02751*/
@EL00010 DS    0H                                                  0440
@EF00010 DS    0H                                                  0440
@ER00010 LM    @14,@12,12(@13)                                     0440
         BR    @14                                                 0440
*   END SETLOCK;                    /* END OBTAIN LOCKS RTN    Y02751*/
         EJECT
