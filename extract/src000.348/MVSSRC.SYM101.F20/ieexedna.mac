*/*                                                                  */
*/*  PROCEDURE NAME - GETDATA                                        */
*/*                                                                  */
*/*  FUNCTION - GATHER DATA FROM UCMES, UCBS, DCMS, TO BUILD         */
*/*             MESSAGES FROM LATER.                                 */
*/*                                                                  */
*/*  INPUTS - CONSOLE DATA SAVE ENTRIES.                             */
*/*                                                                  */
*/*  OUTPUTS - CONSOLE DATA-SAVE-ENTRIES FILLED IN.                  */
*/*                                                                  */
*/*  MACROS USED - NONE                                              */
*/*                                                                  */
*/*  CALLS       - SCANCQES, SYSLGWQE                        @ZA11392*/
*/*                                                                  */
*/********************************************************************/
*                                                                  0290
*GETDATA:                                                          0290
*   PROC;                           /*                       @ZA11392*/
GETDATA  STM   @14,@04,@SA00006                                    0290
         STM   @06,@12,@SA00006+28                                 0290
*   UCMDVEPT=UCMVEA;                /* SET BASE TO FIRST UCME        */
         L     UCMDVEPT,UCMVEA(,UCMBASPT)                          0291
*   DO I=1 TO MAXUCMES;             /* LOOP THRU ALL UCMES IN SYS    */
         LA    I,1                                                 0292
         B     @DE00292                                            0292
@DL00292 DS    0H                                                  0293
*     IDCON(I)=UCMID;               /* SAVE CONSOLE ID       @ZA11392*/
         LR    @12,I                                               0293
         MH    @12,@CH00077                                        0293
         SLR   @11,@11                                             0293
         IC    @11,UCMID(,UCMDVEPT)                                0293
         LR    @07,CONSPTR                                         0293
         AL    @07,@CF02821                                        0293
         STH   @11,IDCON(@12,@07)                                  0293
*     STS(I)=UCMSTS;                /* SAVE CONSOLE STATUS FLAGS     */
         ST    @12,@TF00001                                        0294
         ALR   @12,CONSPTR                                         0294
         AL    @12,@CF02822                                        0294
         MVC   STS-4(1,@12),UCMSTS(UCMDVEPT)                       0294
*     ATR(I)=UCMATR;                /* SAVE CONSOLE ATTRIBUTE FLGS   */
         LR    @12,CONSPTR                                         0295
         AL    @12,@TF00001                                        0295
         AL    @12,@CF02816                                        0295
         MVC   ATR-5(1,@12),UCMATR(UCMDVEPT)                       0295
*     AUTHA(I)=UCMAUTHA;            /* SAVE FIRST BYTE AUTHORIZAT.   */
         LR    @12,CONSPTR                                         0296
         AL    @12,@TF00001                                        0296
         AL    @12,@CF02823                                        0296
         MVC   AUTHA-6(1,@12),UCMAUTHA(UCMDVEPT)                   0296
*     AUTHB(I)=UCMAUTHB;            /* SAVE 2ND BYTE AUTHORIZATION   */
         LR    @12,CONSPTR                                         0297
         AL    @12,@TF00001                                        0297
         AL    @12,@CF02824                                        0297
         MVC   AUTHB-7(1,@12),UCMAUTHB(UCMDVEPT)                   0297
*     DISP(I)=UCMDISP;              /* SAVE DISPOSITION FLAGS        */
         LR    @12,CONSPTR                                         0298
         AL    @12,@TF00001                                        0298
         AL    @12,@CF02825                                        0298
         MVC   DISP-8(2,@12),UCMDISP(UCMDVEPT)                     0298
*     RTCD(I)=UCMRTCD;              /* SAVE CONSOLE ROUTE CODES      */
         LH    @12,UCMRTCD(,UCMDVEPT)                              0299
         N     @12,@CF02819                                        0299
         L     @11,@TF00001                                        0299
         AL    @11,@CF02826                                        0299
         STH   @12,RTCD-10(@11,CONSPTR)                            0299
*     CALL SCANCQES;                /* SCAN CONSOLES CQE CHAIN AND 0300
*                                      COUNT THE NUMBER OF WQE'S   0300
*                                      QUEUED TO THE CONS    @ZA11392*/
*                                                                  0300
         BAL   @14,SCANCQES                                        0300
*     /***************************************************************/
*     /*                                                             */
*     /* CHECK TO SEE IF COMPOSITE CONS INDEX SHOULD BE COMPUTED     */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0301
*     IF UCMCOMPC^=0                /* IF COMPOSITE, FIGURE INDEX    */
*       THEN                        /* TO THE UCME FOR THE COMPOS-   */
         L     @12,UCMCOMPC(,UCMDVEPT)                             0301
         LTR   @12,@12                                             0301
         BZ    @RF00301                                            0301
*       COMPC(I)=(UCMCOMPC-UCMVEA)/UCMVEZ+1;/* ITE CONSOLE           */
*                                                                  0302
         LR    @11,I                                               0302
         MH    @11,@CH00077                                        0302
         SL    @12,UCMVEA(,UCMBASPT)                               0302
         LR    @00,@12                                             0302
         SRDA  @00,32                                              0302
         D     @00,UCMVEZ(,UCMBASPT)                               0302
         AL    @01,@CF00068                                        0302
         AL    @11,@CF02815                                        0302
         STH   @01,COMPC-14(@11,CONSPTR)                           0302
*     /***************************************************************/
*     /*                                                             */
*     /* CHECK TO SEE IF ALTERNATE CONS INDEX TO BE COMPUTED         */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0303
*     IF UCMALTEN^=0                /* IF ALTERNATE POINTER, FIG-    */
*       THEN                        /* URE INDEX TO UCME OF ALTER-   */
@RF00301 L     @12,UCMALTEN(,UCMDVEPT)                             0303
         LTR   @12,@12                                             0303
         BZ    @RF00303                                            0303
*       ALTEN(I)=(UCMALTEN-UCMVEA)/UCMVEZ+1;/* NATE CONSOLE          */
         LR    @11,I                                               0304
         MH    @11,@CH00077                                        0304
         SL    @12,UCMVEA(,UCMBASPT)                               0304
         LR    @00,@12                                             0304
         SRDA  @00,32                                              0304
         D     @00,UCMVEZ(,UCMBASPT)                               0304
         AL    @01,@CF00068                                        0304
         AL    @11,@CF02827                                        0304
         STH   @01,ALTEN-12(@11,CONSPTR)                           0304
*     UCBPTR=UCMUCB;                /* SET BASE FOR UCB FOR CONS.    */
@RF00303 L     UCBPTR,UCMUCB(,UCMDVEPT)                            0305
*     SRTEFLGS(I)=UCBSTAT;          /* SAVE STATUS FLAGS             */
         LR    @12,I                                               0306
         MH    @12,@CH00077                                        0306
         ST    @12,@TF00001                                        0306
         ALR   @12,CONSPTR                                         0306
         AL    @12,@CF02828                                        0306
         MVC   SRTEFLGS-19(1,@12),UCBSTAT(UCBPTR)                  0306
*     NAME(I)=UCBNAME;              /* SAVE DEVICE NAME (EBCDIC)     */
*                                                                  0307
         LR    @12,CONSPTR                                         0307
         AL    @12,@TF00001                                        0307
         AL    @12,@CF02829                                        0307
         MVC   NAME-16(3,@12),UCBNAME(UCBPTR)                      0307
*     /***************************************************************/
*     /*                                                             */
*     /* CHECK TO SEE IF WE NEED TO GET AN AREA ID FOR THIS CONS     */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0308
*     IF UCMXB^=0                   /* IF THERE IS A DCM FOR THIS    */
*       THEN                        /* CONSOLE UCM ENTRY,            */
         L     @12,UCMXB(,UCMDVEPT)                                0308
         LTR   @12,@12                                             0308
         BZ    @RF00308                                            0308
*       DO;                         /* THEN SET THE BASE POINTER     */
*         DCMTSPTR=UCMXB;           /* TO REFERENCE THE DCM, AND     */
         LR    DCMTSPTR,@12                                        0310
*         TOPAR(I)=DCMTOPAR;        /* SAVE THE TOP AREA ID FROM     */
         LR    @12,CONSPTR                                         0311
         AL    @12,@TF00001                                        0311
         AL    @12,@CF02811                                        0311
         MVC   TOPAR-20(1,@12),DCMTOPAR(DCMTSPTR)                  0311
*       END;                        /* THE DCM.                      */
*     UCMDVEPT=UCMDVEPT+UCMVEZ;     /* UPDATE TO NEXT UCME           */
@RF00308 AL    UCMDVEPT,UCMVEZ(,UCMBASPT)                          0313
*   END;                            /* END UCME SCAN LOOP            */
*                                                                  0314
         AL    I,@CF00068                                          0314
@DE00292 C     I,MAXUCMES                                          0314
         BNH   @DL00292                                            0314
*   /*****************************************************************/
*   /*                                                               */
*   /* IF HARDCOPY IS IN THE SYSTEM, THEN FIGURE INDEX TO THE CONSOLE*/
*   /* (IF ANY) TO WHICH HARDCOPY IS TO GO                           */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0315
*   IF UCMHCUCM^=0                  /* IF HARDCOPY TO A DEVICE       */
*     THEN                          /* FIGURE INDEX TO HARDCOPY      */
         L     @12,UCMMCSPT                                        0315
         L     @12,UCMHCUCM(,@12)                                  0315
         LTR   @12,@12                                             0315
         BZ    @RF00315                                            0315
*     HCUCM=(UCMHCUCM-UCMVEA)/UCMVEZ+1;/* UCME                       */
         SL    @12,UCMVEA(,UCMBASPT)                               0316
         LR    @00,@12                                             0316
         SRDA  @00,32                                              0316
         D     @00,UCMVEZ(,UCMBASPT)                               0316
         AL    @01,@CF00068                                        0316
         LR    HCUCM,@01                                           0316
*   ELSE                            /* OTHERWISE, ZERO               */
*     DO;                           /*                       @ZA11392*/
         B     @RC00315                                            0317
@RF00315 DS    0H                                                  0318
*       HCUCM=0;                    /* THE FIELD                     */
         SLR   HCUCM,HCUCM                                         0318
*       IF UCMSYSG='1'B THEN        /* IS SYSLOG HARDCPY?    @ZA11392*/
         L     @12,UCMMCSPT                                        0319
         TM    UCMSYSG(@12),B'00000010'                            0319
         BNO   @RF00319                                            0319
*         CALL SYSLGWQE;            /* YES, COUNT THE NUMBER OF WQE'S
