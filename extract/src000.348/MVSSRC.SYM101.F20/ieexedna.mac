*/*                                                          @ZA11392*/
*/*  PROCEDURE NAME: IEEXEDNA.SCANMINR                       @ZA11392*/
*/*                                                          @ZA11392*/
*/*  FUNCTION: COUNTS THE NUMBER OF WQE'S ON THE MINOR CHAIN @ZA11392*/
*/*            FOR A MLWTO                                   @ZA11392*/
*/*                                                          @ZA11392*/
*/*  INPUTS:   WQEPTR - POINT TO THE MAJOR WQE OF IMLWTO     @ZA11392*/
*/*                     A MINOR WILL ALWAYS BE PRESENT       @ZA11392*/
*/*            COUNT  - THE COUNT WHICH SHOULD BE INCREASED  @ZA11392*/
*/*                     FOR EACH WQE FOUND                   @ZA11392*/
*/*                                                          @ZA11392*/
*/*  OUTPUT:   COUNT  - NUMBER OF WQE'S FOUND SO FAR         @ZA11392*/
*/*                                                          @ZA11392*/
*/*  CALLS:    NONE                                          @ZA11392*/
*/*                                                          @ZA11392*/
*/***********************************************************@ZA11392*/
*                                                                  0663
*SCANMINR:                                                         0663
*   PROC(COUNT);                    /*                       @ZA11392*/
SCANMINR STM   @14,@12,12(@13)                                     0663
         MVC   @PC00020(4),0(@01)                                  0663
*   DCL                                                            0664
*     COUNT FIXED(15);              /* TO BE INCREASED FOR EACH WQE
*                                      IN MLWTO MINOR CHAIN  @ZA11392*/
*   WMNPTR=WMJMMIN;                 /* POINT TO MINOR        @ZA11392*/
         L     @12,WMJPTR                                          0665
         L     WMNPTR,WMJMMIN(,@12)                                0665
*   COUNT=COUNT+1;                  /* ONE MINOR EXISTS SINCE CONTROL
*                                      REACHED HERE          @ZA11392*/
         L     @12,@PC00020                                        0666
         LA    @11,1                                               0666
         AH    @11,COUNT(,@12)                                     0666
         STH   @11,COUNT(,@12)                                     0666
*   DO WHILE WMNMNX1^=0&            /* DO WHILE A MINOR EXISTS IN 2ND
*                                      HALF OF WQE AND       @ZA11392*/
*         WMNMNX2^=0;               /* 2ND MINOR IN WQE IS NOT LAST
*                                      MINOR                 @ZA11392*/
         B     @DE00667                                            0667
@DL00667 DS    0H                                                  0668
*     WMNPTR=WMNMNX2;               /* POINT TO NEXT MINOR PAIR    0668
*                                                            @ZA11392*/
         L     @12,WMNMNX2-1(,WMNPTR)                              0668
         LA    @12,0(,@12)                                         0668
         LR    WMNPTR,@12                                          0668
*     COUNT=COUNT+1;                /* ONE MORE WQE FOUND    @ZA11392*/
         L     @12,@PC00020                                        0669
         LA    @11,1                                               0669
         AH    @11,COUNT(,@12)                                     0669
         STH   @11,COUNT(,@12)                                     0669
*   END;                            /* OF DO WHILE           @ZA11392*/
@DE00667 L     @12,WMNMNX1-1(,WMNPTR)                              0670
         LA    @12,0(,@12)                                         0670
         LTR   @12,@12                                             0670
         BZ    @DC00667                                            0670
         L     @12,WMNMNX2-1(,WMNPTR)                              0670
         LA    @12,0(,@12)                                         0670
         LTR   @12,@12                                             0670
         BNZ   @DL00667                                            0670
@DC00667 DS    0H                                                  0671
*   END SCANMINR;                   /*                       @ZA11392*/
@EL00020 DS    0H                                                  0671
@EF00020 DS    0H                                                  0671
@ER00020 LM    @14,@12,12(@13)                                     0671
         BR    @14                                                 0671
*   END IEEXEDNA                                                   0672
