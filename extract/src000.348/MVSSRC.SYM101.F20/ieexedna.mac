*/********************************************************************/
*/*                                                                  */
*/*  PROCEDURE - CONSID                                              */
*/*                                                                  */
*/*  FUNCTION - CHANGE CONSOLE ID TO EBCDIC & FILL IT INTO MESSAGE.  */
*/*                                                                  */
*/*  INPUTS - ARRAY OF CONSOLE DATA-SAVE-ENTRIED, AND INDEX TO THE   */
*/*             ENTRY CURRENTLY BEING PROCESSED.                     */
*/*                                                                  */
*/*  OUTPUTS - CONSOLE ID IN THE ID COLUMN OF THE MESSAGE.           */
*/*                                                                  */
*/*  CALLS - NONE.                                                   */
*/*                                                                  */
*/********************************************************************/
*                                                                  0450
*CONSID:                                                           0450
*   PROC OPTIONS(NOSAVEAREA);       /* CONSOLE ID ROUTINE            */
CONSID   STM   @14,@03,12(@13)                                     0450
         STM   @05,@12,40(@13)                                     0450
*   INDEXSVE=INDEX;                 /* SAVE CURRENT INDEX            */
*                                                                  0451
         LR    INDEXSVE,INDEX                                      0451
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK TO SEE IF THIS IS A COMPOSITE CONSOLE                   */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0452
*   IF COMPC(INDEX)^=0              /* IF COMPOSITE, WE ASSUME IT    */
*     THEN                          /* IS INPUT HALF SINCE TOPSEG  0452
*                                      SKIPS OUTPUT ONLY COMPC CON   */
         LR    @12,INDEX                                           0452
         MH    @12,@CH00077                                        0452
         AL    @12,@CF02815                                        0452
         LH    @12,COMPC-14(@12,CONSPTR)                           0452
         LTR   @12,@12                                             0452
         BZ    @RF00452                                            0452
*     INDEX=COMPC(INDEX);           /* SET INDEX TO OUTPUT HALF.     */
         LR    INDEX,@12                                           0453
*   RFY                                                            0454
*     R0 RSTD;                      /*                       @ZA11392*/
@RF00452 DS    0H                                                  0455
*   R0=IDCON(INDEX);                /* PUT ID IN REG 0 FOR CVD     0455
*                                                            @ZA11392*/
         LR    @12,INDEX                                           0455
         MH    @12,@CH00077                                        0455
         AL    @12,@CF02821                                        0455
         LH    R0,IDCON(@12,CONSPTR)                               0455
*   GEN(CVD  0,UNPACK);             /* CONVERT TO PACKED DECIMAL     */
         CVD  0,UNPACK
*   RFY                                                            0457
*     R0 UNRSTD;                    /*                       @ZA11392*/
*   GEN(UNPK UNPACK(3),UNPACK+6(2));/* CONV. TO EBCDIC               */
         UNPK UNPACK(3),UNPACK+6(2)
*   GEN(MVZ UNPACK+2(1),UNPACK+1);  /* FIX ZONE                      */
         MVZ UNPACK+2(1),UNPACK+1
*   IDMSG=UNPACK(2:3);              /* MOVE ID TO MESSAGE            */
         MVC   IDMSG(2,WPLPTR),UNPACK+1                            0460
*   INDEX=INDEXSVE;                 /* RESTORE ORIGINAL INDEX        */
         LR    INDEX,INDEXSVE                                      0461
*   RETURN;                         /* RETURN TO CALLER              */
@EL00012 DS    0H                                                  0462
@EF00012 DS    0H                                                  0462
@ER00012 LM    @14,@03,12(@13)                                     0462
         LM    @05,@12,40(@13)                                     0462
         BR    @14                                                 0462
*   END CONSID;                     /* END CONSOLE ID ROUTINE.       */
         EJECT
*                                                                  0464
*/********************************************************************/
*/*                                                                  */
*/*  PROCEDURE - MSGCOND.                                            */
*/*                                                                  */
*/*  FUNCTION - FILL IN CONSOLE CONDITION COL. OF MESSAGE.           */
*/*                                                                  */
*/*  INPUTS - CONSOLE DATA-SAVE-ENTRIES,AND INDEX OF CURRENT ENTRY.  */
*/*                                                                  */
*/*  OUTPUTS - CONDITION COL. OF MESSAGE. CAN BE:                    */
*/*                                                                  */
*/*     (1) M - MASTER CONSOLE.                                      */
*/*     (2) A - ACTIVE, NOT MASTER.                                  */
*/*     (3) N - INACTIVE, NOT MASTER.                                */
*/*     (4) S - SUBSYSTEM CONSOLE                            @Y02668P*/
*/*                                                                  */
*/*     (5) ,P - STATUS PENDING. IE. A,P = PENDING TO BECOME ACTIVE  */
*/*     (6) ,T - REQUESTING JOBNAMES & TIME STAMPS BEFORE MESSAGES.  */
*/*     (7) ,J - REQUESTING ONLY JOBNAMES BEFORE MESSAGES.           */
*/*                                                                  */
*/*  CALLS - NONE.                                                   */
*/*                                                                  */
*/********************************************************************/
*                                                                  0464
*MSGCOND:                                                          0464
*   PROC OPTIONS(NOSAVEAREA);       /* CONSOLE CONDITION ROUTINE     */
MSGCOND  STM   @14,@12,12(@13)                                     0464
*   L=1;                            /* SET COL. INDEX TO BEGIN       */
*                                                                  0465
         LA    @12,1                                               0465
         LR    L,@12                                               0465
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK TO SEE IF THIS IS NOT THE MASTER CONSOLE                */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0466
*   IF DISPA(INDEX)^='1'B           /* IF NOT MASTER CONSOLE, THEN   */
*     THEN                          /* ASSUME ACTIVE CONSOLE. PUT    */
         LR    @02,INDEX                                           0466
         MH    @02,@CH00077                                        0466
         ST    @02,@TF00001                                        0466
         ALR   @02,CONSPTR                                         0466
         AL    @02,@CF02825                                        0466
         TM    DISPA-8(@02),B'10000000'                            0466
         BO    @RF00466                                            0466
*     DO;                           /* 'A' IN FIRST COLUMN.          */
*                                                                  0467
*       /*************************************************************/
*       /*                                                           */
*       /* CHECK TO SEE IF IT IS A SUBSYSTEM CONSOLE                 */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0468
*       CONDITN(1)='A';             /* SET ACTIVE                    */
         MVI   CONDITN(WPLPTR),C'A'                                0468
*       L=L+1;                      /* UPDATE COLUMN INDEX.          */
*                                                                  0469
         ALR   L,@12                                               0469
*       /*************************************************************/
*       /*                                                           */
*       /* SEE IF THE CONSOLE IS NOT CURRENTLY ACTIVE                */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0470
*       IF UF(INDEX)^='1'B          /* IF IT IS NOT ACTIVE           */
*         THEN                      /* THEN CHECK TO SEE IF IT IS    */
*                                                                  0470
         LR    @12,CONSPTR                                         0470
         AL    @12,@TF00001                                        0470
         AL    @12,@CF02816                                        0470
         TM    UF-5(@12),B'00010000'                               0470
         BO    @RF00470                                            0470
*         /***********************************************************/
*         /*                                                         */
*         /* CHECK TO SEE IF STATUS TO CHANGE                        */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0471
*         IF DADI(INDEX)='1'B       /* PENDING TO BECOME ACTIVE.     */
*           THEN                    /* IF SO, THEN WE                */
         LR    @12,CONSPTR                                         0471
         AL    @12,@TF00001                                        0471
         AL    @12,@CF02828                                        0471
         TM    DADI-19(@12),B'00000001'                            0471
         BNO   @RF00471                                            0471
*           DO;                     /* MUST PUT IN A ',P' TO         */
*             CONDITN(L:L+1)=',P';  /* SHOW PENDING.                 */
         LR    @12,WPLPTR                                          0473
         ALR   @12,L                                               0473
         MVC   CONDITN-1(2,@12),@CC02745                           0473
*             L=L+2;                /* UPDATE COLUMN INDEX           */
         AL    L,@CF00080                                          0474
*           END;                    /* END A,P PROCESSING.           */
*         ELSE                      /* IF NOT PENDING ACTIVE THEN    */
*           IF DISPK(INDEX)='1'B THEN/* IF SUBSYSTEM CONSOLE @Y02668P*/
         B     @RC00471                                            0476
@RF00471 LR    @12,INDEX                                           0476
         MH    @12,@CH00077                                        0476
         ALR   @12,CONSPTR                                         0476
         AL    @12,@CF02830                                        0476
         TM    DISPK-9(@12),B'00100000'                            0476
         BNO   @RF00476                                            0476
*             DO;                   /* THEN THE CONDITION    @Y02668P*/
*               CONDITN(1)='S';     /* WILL BE 'S'           @Y02668P*/
         MVI   CONDITN(WPLPTR),C'S'                                0478
*               L=L+1;              /* UPDATE COLUMN INDEX   @Y02668P*/
         AL    L,@CF00068                                          0479
*             END;                  /*                       @Y02668P*/
*           ELSE                    /* SYSTEM CONSOLE        @Y02668P*/
*             DO;                   /* WE PUT IN AN 'N' TO MEAN      */
         B     @RC00476                                            0481
@RF00476 DS    0H                                                  0482
*               CONDITN(1)='N';     /* INACTIVE CONSOLE              */
         MVI   CONDITN(WPLPTR),C'N'                                0482
*               L=2;                /* SET COLUMN INDEX TO TWO       */
         LA    L,2                                                 0483
*             END;                  /* END INACTIVE CONS PROC        */
*       ELSE                        /* IF IT IS ACTIVE, THEN WE      */
*                                                                  0485
*         /***********************************************************/
*         /*                                                         */
*         /* CHECK TO SEE IF STATUS IS CHANGING                      */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0485
*         IF DADI(INDEX)='1'B       /* ALREADY HAVE THE 'A' IN       */
*           THEN                    /* THE TEXT. BUT IF PENDING      */
         B     @RC00470                                            0485
@RF00470 LR    @12,INDEX                                           0485
         MH    @12,@CH00077                                        0485
         ALR   @12,CONSPTR                                         0485
         AL    @12,@CF02828                                        0485
         TM    DADI-19(@12),B'00000001'                            0485
         BNO   @RF00485                                            0485
*           DO;                     /* CHANGE BIT IS ON, IT IS       */
*             CONDITN(1:3)='N,P';   /* PEND. INACT.                  */
         MVC   CONDITN(3,WPLPTR),@CC02748                          0487
*             L=4;                  /* SET COL. INDEX AFTER 'N,P'    */
         LA    L,4                                                 0488
*           END;                    /* END 'N,P' CONS. PROCESSING    */
*     END;                          /* END ACTIVE CONS. PROCESSING   */
*   ELSE                            /* IF IT IS THE MASTER CONSOLE   */
*     DO;                           /* THEN WE MOVE AN 'M' INTO      */
         B     @RC00466                                            0491
@RF00466 DS    0H                                                  0492
*       CONDITN(1)='M';             /* THE MSG.                      */
         MVI   CONDITN(WPLPTR),C'M'                                0492
*       L=2;                        /* SET COL. INDEX AFTER 'M'.     */
         LA    L,2                                                 0493
*     END;                          /* END MASTER CONS. PROCESSING   */
*                                                                  0494
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK FOR REQUESTING TIMESTAMPS & JOBNAMES                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0495
*   IF DISPI(INDEX)='1'B            /* IF REQ. JBNMS & TIME STMP     */
*     THEN                          /* THEN ADD ',T' TO MSG.         */
@RC00466 LR    @12,INDEX                                           0495
         MH    @12,@CH00077                                        0495
         ALR   @12,CONSPTR                                         0495
         AL    @12,@CF02830                                        0495
         TM    DISPI-9(@12),B'10000000'                            0495
         BNO   @RF00495                                            0495
*     CONDITN(L:L+1)=',T';          /* TO SHOW IT.                   */
         LR    @12,WPLPTR                                          0496
         ALR   @12,L                                               0496
         MVC   CONDITN-1(2,@12),@CC02750                           0496
*   ELSE                            /* ELSE IF                       */
*                                                                  0497
*     /***************************************************************/
*     /*                                                             */
*     /* CHECK FOR REQUESTING ONLY JOBNAMES                          */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0497
*     IF DISPJ(INDEX)='1'B          /* REQ. ONLY JOBNAMES            */
*       THEN                        /* THEN ADD ',J' TO MSG.         */
         B     @RC00495                                            0497
@RF00495 LR    @12,INDEX                                           0497
         MH    @12,@CH00077                                        0497
         ALR   @12,CONSPTR                                         0497
         AL    @12,@CF02830                                        0497
         TM    DISPJ-9(@12),B'01000000'                            0497
         BNO   @RF00497                                            0497
*       CONDITN(L:L+1)=',J';        /* TO SHOW IT.                   */
         LR    @12,WPLPTR                                          0498
         ALR   @12,L                                               0498
         MVC   CONDITN-1(2,@12),@CC02751                           0498
*     ELSE                                                         0499
*       ;                           /* ELSE LEAVE MSG ALONE          */
@RF00497 DS    0H                                                  0500
*   END MSGCOND;                    /* END CONSOLE COND. ROUTINE     */
@EL00013 DS    0H                                                  0500
@EF00013 DS    0H                                                  0500
@ER00013 LM    @14,@12,12(@13)                                     0500
         BR    @14                                                 0500
         EJECT
*                                                                  0501
*/********************************************************************/
*/*                                                                  */
*/*  PROCEDURE - AUTHCHK                                             */
*/*                                                                  */
*/*  FUNCTION - CHECK CONSOLE COMMAND AUTHORITY AND PUT IT IN MSG.   */
*/*                                                                  */
*/*  INPUTS - CONSOLE-DATA-SAVE-ENTRIES AND INDEX TO CURRENT ONE.    */
*/*                                                                  */
*/*  OUTPUTS - CONSOLE AUTHORITY COLUMN OF MESSAGE AS FOLLOWS:       */
*/*                                                                  */
*/*            POSSIBLE VALUES:                                      */
*/*               ONE OF: ALL,NONE,SYS,IO,CONS.                      */
*/*               TWO OF: SYS,IO,CONS.                               */
*/*                                                                  */
*/*  CALLS - NONE.                                                   */
*/*                                                                  */
*/********************************************************************/
*                                                                  0501
*AUTHCHK:                                                          0501
*   PROC OPTIONS(NOSAVEAREA);       /* AUTHORITY CHECK ROUTINE       */
AUTHCHK  STM   @14,@12,12(@13)                                     0501
*   L=1;                            /* SET COLUMN INDEX TO ONE       */
*                                                                  0502
         LA    L,1                                                 0502
*   /*****************************************************************/
*   /*                                                               */
*   /* SEE IF THE CONSOLE HAS INPUT CAPABILITY                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0503
*   IF MIF(INDEX)='1'B              /* IF CONS. HAS INPUT ABILITY    */
*     THEN                          /* THEN CHECK FOR AUTH           */
*                                                                  0503
         LR    @12,INDEX                                           0503
         MH    @12,@CH00077                                        0503
         ST    @12,@TF00001                                        0503
         ALR   @12,CONSPTR                                         0503
         AL    @12,@CF02816                                        0503
         TM    MIF-5(@12),B'01000000'                              0503
         BNO   @RF00503                                            0503
*     /***************************************************************/
*     /*                                                             */
*     /* CHECK TO SEE IF AUTH = ALL                                  */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0504
*     IF AUTHA(INDEX)>='E0'X        /* = ALL. IF ALL                 */
*       THEN                        /* THEN                          */
         LR    @12,CONSPTR                                         0504
         AL    @12,@TF00001                                        0504
         AL    @12,@CF02823                                        0504
         CLI   AUTHA-6(@12),X'E0'                                  0504
         BL    @RF00504                                            0504
*       AUTH='  ALL';               /* PUT 'ALL' INTO MESSAGE.       */
         MVI   AUTH+5(WPLPTR),C' '                                 0505
         MVC   AUTH+6(2,WPLPTR),AUTH+5(WPLPTR)                     0505
         MVC   AUTH(5,WPLPTR),@CC02756                             0505
*     ELSE                          /* IF NOT ALL                    */
*                                                                  0506
*       /*************************************************************/
*       /*                                                           */
*       /* SEE IF THERE IS NO AUTH SPECIFIED                         */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0506
*       IF AUTHA(INDEX)=0           /* IF NO AUTH                    */
*         THEN                      /* THEN SIGNAL                   */
         B     @RC00504                                            0506
@RF00504 LR    @12,INDEX                                           0506
         MH    @12,@CH00077                                        0506
         AL    @12,@CF02823                                        0506
         SLR   @02,@02                                             0506
         IC    @02,AUTHA-6(@12,CONSPTR)                            0506
         C     @02,@CF00044                                        0506
         BNE   @RF00506                                            0506
*         AUTH='  INFO';            /* INFO ONLY                     */
         MVI   AUTH+6(WPLPTR),C' '                                 0507
         MVI   AUTH+7(WPLPTR),C' '                                 0507
         MVC   AUTH(6,WPLPTR),@CC02757                             0507
*       ELSE                        /* OTHERWISE SCAN AUTH CODES     */
*         DO;                       /* THEN CHECK EACH CODE    SEPAR-*/
         B     @RC00506                                            0508
@RF00506 DS    0H                                                  0509
*           FRSTPARM='0'B;          /* SET INDICATOR OFF.            */
*                                                                  0509
         NI    FRSTPARM,B'01111111'                                0509
*           /*********************************************************/
*           /*                                                       */
*           /* SEE IF 'SYS' WAS SPECIFIED                            */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0510
*           IF AUTH1(INDEX)='1'B    /* IF FIRST ONE ON               */
*             THEN                  /* THEN                          */
         LR    @12,INDEX                                           0510
         MH    @12,@CH00077                                        0510
         ALR   @12,CONSPTR                                         0510
         AL    @12,@CF02823                                        0510
         TM    AUTH1-6(@12),B'10000000'                            0510
         BNO   @RF00510                                            0510
*             DO;                   /* MOVE 'SYS' INTO THE NEXT      */
*               AUTH(L:L+2)='SYS';  /* AVAILABLE COLS.               */
         LR    @12,WPLPTR                                          0512
         ALR   @12,L                                               0512
         MVC   AUTH-1(3,@12),@CC02758                              0512
*               L=4;                /* SET COL. INDEX PAST 'SYS'     */
         LA    L,4                                                 0513
*               FRSTPARM='1'B;      /* SIGNAL 1ST PARM WAS           */
         OI    FRSTPARM,B'10000000'                                0514
*             END;                  /* FOUND. END SYS PROCESSING.    */
*                                                                  0515
*           /*********************************************************/
*           /*                                                       */
*           /* CHECK TO SEE IF 'IO' WAS SPECIFIED                    */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0516
*           IF AUTH2(INDEX)='1'B    /* IF 2ND BIT ON                 */
*             THEN                  /* THEN                          */
@RF00510 LR    @12,INDEX                                           0516
         MH    @12,@CH00077                                        0516
         ALR   @12,CONSPTR                                         0516
         AL    @12,@CF02823                                        0516
         TM    AUTH2-6(@12),B'01000000'                            0516
         BNO   @RF00516                                            0516
*             DO;                   /* SEE IF THIS WILL BE 1ST       */
*                                                                  0517
*               /*****************************************************/
*               /*                                                   */
*               /* SEE IF IO IS FIRST PARAMETER                      */
*               /*                                                   */
*               /*****************************************************/
*                                                                  0518
*               IF FRSTPARM='0'B    /* PARM. IF SO                   */
*                 THEN              /* THEN SIGNAL                   */
         TM    FRSTPARM,B'10000000'                                0518
         BNZ   @RF00518                                            0518
*                 FRSTPARM='1'B;    /* FIRST PARM END.               */
         OI    FRSTPARM,B'10000000'                                0519
*               ELSE                /* IF NOT FIRST PARM             */
*                 DO;               /* THEN SEPARATE THIS PARM       */
         B     @RC00518                                            0520
@RF00518 DS    0H                                                  0521
*                   AUTH(L)=',';    /* WITH COMMA                    */
         LR    @12,WPLPTR                                          0521
         ALR   @12,L                                               0521
         MVI   AUTH-1(@12),C','                                    0521
*                   L=L+1;          /* UP INDEX BY COMMA             */
         AL    L,@CF00068                                          0522
*                 END;              /* END NOT FIRST PARM PROC.      */
*               AUTH(L:L+1)='IO';   /* MOVE IO IN NEXT               */
@RC00518 LR    @12,WPLPTR                                          0524
         ALR   @12,L                                               0524
