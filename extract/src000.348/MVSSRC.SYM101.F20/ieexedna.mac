*/********************************************************************/
*/*                                                                  */
*/* PROCEDURE NAME: IEEXEDNA.UNITNAME                                */
*/*                                                                  */
*/* FUNCTION: FILLS IN EBCDIC UNIT NAMES OF CONSOLE NOW BEING        */
*/*           PROCESSED (BOTH UNIT NAMES IF A COMPOSITE), AND ALSO   */
*/*           FILLS IN UNIT NAME OF ALTERNATE CONSOLE (BOTH NAMES IF */
*/*           A COMPOSITE).    THESE NAMES ARE PLACED INTO THE MSG   */
*/*           CURRENTLY BEING BUILT.                                 */
*/*                                                                  */
*/* INPUTS: 1.WPLPTR  - BASEPTR FOR THE MESSAGE BEING BUILT. MESSAGE */
*/*           DSECT IS BASED ON THIS POINTER.                        */
*/*                                                                  */
*/*         2.CONSPTR - BASEPTR FOR THE DATA ENTRY            FOR    */
*/*           THE CONSOLE BEING PROCESSED                            */
*/*                                                                  */
*/* OUTPUTS:  PORTION OF MESSAGE TEXT IN THE FOLLOWING FORMAT:       */
*/*                                                                  */
*/*            XXX,YYY/AAA,BBB                                       */
*/*                                                                  */
*/*           WHERE: XXX IS NAMEOF INPUT-HALF IF COMPOSITE CONSOLE   */
*/*                  YYY IS NAMEOF OUTPUT-HALF IF COMPOSITE. IF NOT  */
*/*                      COMPOSITE,YYY WILLBE CONSOLE UNIT NAME.     */
*/*                  AAA IS NAME OF INPUT HALF OF ALTERNATE IF A     */
*/*                      COMPOSITE. IF NOT COMPOSITE THEN AAA WILL BE*/
*/*                      UNIT NAME OF THE ALTERNATE CONSOLE.         */
*/*                  BBB IS THE NAME OF THE OUTPUT HALF OF THE ALTER-*/
*/*                      NATE CONSOLE, IF IT IS A COMPOSITE.         */
*/*                                                                  */
*/*                                                                  */
*/****************************************************************** */
*                                                                  0323
*UNITNAME:                                                         0323
*   PROC OPTIONS(NOSAVEAREA);       /* SEARCH FOR AND FILL IN UNIT   */
UNITNAME STM   @14,@03,12(@13)                                     0323
         STM   @05,@12,40(@13)                                     0323
*   INDEXSVE=INDEX;                 /* SAVE CURRENT ENTRY INDEX      */
*                                                                  0324
         LR    INDEXSVE,INDEX                                      0324
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK FOR COMPOSITE CONSOLE                                   */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0325
*   IF COMPC(INDEX)^=0              /* IF THIS IS A COMPOSITE        */
*     THEN                          /* THEN WE ASSUME IT IS THE      */
         LR    @12,INDEX                                           0325
         MH    @12,@CH00077                                        0325
         LR    @07,CONSPTR                                         0325
         AL    @07,@CF02815                                        0325
         LH    @07,COMPC-14(@12,@07)                               0325
         LTR   @07,@07                                             0325
         BZ    @RF00325                                            0325
*     DO;                           /* INPUT HALF.                   */
*       ICONS=NAME(INDEX);          /* FILL UNIT NAME INTO MSG       */
         ALR   @12,CONSPTR                                         0327
         AL    @12,@CF02829                                        0327
         MVC   ICONS(3,WPLPTR),NAME-16(@12)                        0327
*       COMMA1=',';                 /* PUT COMMA AFTER UNIT NAME     */
         MVI   COMMA1(WPLPTR),C','                                 0328
*       INDEX=COMPC(INDEX);         /* SET INDEX TO 0-HALF           */
         LR    INDEX,@07                                           0329
*     END;                          /* CONTINUE                      */
*   OCONS=NAME(INDEX);              /* SET UNIT NAME IN MESSAGE      */
@RF00325 LR    @12,INDEX                                           0331
         MH    @12,@CH00077                                        0331
         ST    @12,@TF00001                                        0331
         ALR   @12,CONSPTR                                         0331
         AL    @12,@CF02829                                        0331
         MVC   OCONS(3,WPLPTR),NAME-16(@12)                        0331
*   IF DISPK(INDEX)='1'B THEN       /* SUBSYSTEM CONSOLE     @Y02668P*/
         LR    @12,CONSPTR                                         0332
         AL    @12,@TF00001                                        0332
         AL    @12,@CF02830                                        0332
         TM    DISPK-9(@12),B'00100000'                            0332
         BNO   @RF00332                                            0332
*     DO;                           /*                       @Y02668P*/
*       IALTRNT='JES';              /* ALTERNATE NAME-JES    @Y02668P*/
         MVC   IALTRNT(3,WPLPTR),@CC02711                          0334
*       ALTEN(INDEX)=0;             /* JES ALTERNATE         @Y02668P*/
         SLR   @12,@12                                             0335
         L     @07,@TF00001                                        0335
         AL    @07,@CF02827                                        0335
         STH   @12,ALTEN-12(@07,CONSPTR)                           0335
*     END;                          /*                       @Y02668P*/
*   INDEX=INDEXSVE;                 /* RESTORE INDEX                 */
@RF00332 LR    INDEX,INDEXSVE                                      0337
*   SLASH='/';                      /* SET SLASH IN MSG.             */
*                                                                  0338
         MVI   SLASH(WPLPTR),C'/'                                  0338
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK TO SEE IF AN ALTERNATE EXISTS IF THE ALTERNATE IS A     */
*   /* SUBSYSTEM CONSOLE, UCMDISPK                           @Y02668P*/
*   /* IS ON, THEN THE ALTERNATE CONSOLE NAME WILL BE        @Y02668P*/
*   /* 'JES'                                                 @Y02668P*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0339
*   IF ALTEN(INDEX)^=0              /* IF THERE IS AN ALTERNATE      */
*     THEN                          /* CONSOLE, GO GET ITS UNIT      */
         SLR   @12,@12                                             0339
         LA    @07,24                                              0339
         LR    @01,INDEX                                           0339
         MR    @00,@07                                             0339
         AL    @01,@CF02827                                        0339
         LH    @05,ALTEN-12(@01,CONSPTR)                           0339
         CR    @05,@12                                             0339
         BE    @RF00339                                            0339
*     DO;                           /* NAME.                         */
*       INDEX=ALTEN(INDEX);         /* SET INDEX TO ALTEN ENTRY      */
         LR    INDEX,@05                                           0341
*       IALTRNT=NAME(INDEX);        /* PUT UNIT NAME IN MESSAGE      */
*                                                                  0342
         LR    @01,INDEX                                           0342
         MR    @00,@07                                             0342
         ST    @01,@TF00001                                        0342
         ALR   @01,CONSPTR                                         0342
         AL    @01,@CF02829                                        0342
         MVC   IALTRNT(3,WPLPTR),NAME-16(@01)                      0342
*       /*************************************************************/
*       /*                                                           */
*       /* SEE IF ALTERNATE HAS A COMPOSITE                          */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0343
*       IF COMPC(INDEX)^=0          /* IF ALTEN HAS A COMPOSITE      */
*         THEN                      /* THEN WE MUST GET UNIT NAME    */
         L     @05,@TF00001                                        0343
         AL    @05,@CF02815                                        0343
         LH    @05,COMPC-14(@05,CONSPTR)                           0343
         CR    @05,@12                                             0343
         BE    @RF00343                                            0343
*         DO;                       /* OF THE OUTPUT HALF.           */
*           INDEX=COMPC(INDEX);     /* SET INDEX TO COMPC ENT.       */
         LR    INDEX,@05                                           0345
*           COMMA2=',';             /* SET COMMA AFTER INPUT NAME    */
         MVI   COMMA2(WPLPTR),C','                                 0346
*           OALTRNT=NAME(INDEX);    /* GET THE NAME                  */
         LR    @01,@07                                             0347
         MR    @00,INDEX                                           0347
         ALR   @01,CONSPTR                                         0347
         AL    @01,@CF02829                                        0347
         MVC   OALTRNT(3,WPLPTR),NAME-16(@01)                      0347
*         END;                      /* END COMPC PROCESSING.         */
*       INDEX=INDEXSVE;             /* RESTORE ORIGINAL INDEX.       */
@RF00343 LR    INDEX,INDEXSVE                                      0349
*     END;                          /* END ALTERNATE PROCESSING      */
*   END UNITNAME;                   /* END UNIT NAME PROCESSING      */
@EL00007 DS    0H                                                  0351
@EF00007 DS    0H                                                  0351
@ER00007 LM    @14,@03,12(@13)                                     0351
         LM    @05,@12,40(@13)                                     0351
         BR    @14                                                 0351
         EJECT
*                                                                  0352
*/********************************************************************/
*/*                                                                  */
*/* PROCEDURE NAME: IEEXEDNA.RCODES                                  */
*/*                                                                  */
*/* FUNCTION: FILL IN CONSOLE ROUTE CODES COLUMN IN DATA LINES.      */
*/*                                                                  */
*/* INPUTS:   CURRENT CONSOLE ENTRY INDEX. RTCD FIELD IS ONLY FIELD  */
*/*           USED FOR OUTPUT PRODUCTION.                            */
*/*                                                                  */
*/* OUTPUTS:  ROUTE CODES IN MESSAGE TEXT IN THE FOLLOWING GENERAL   */
*/*           FORMAT:                                                */
*/*                                                                  */
*/*           RC,RC,RC-RC     WHERE  RC IS A SPECIFIC ROUTE CODE,    */
*/*                             AND RC-RC IS A RANGE OF SEQUENTIAL   */
*/*                             ROUTE CODES.                         */
*/*                                                                  */
*/* CALLS:    NONE.                                                  */
*/*                                                                  */
*/********************************************************************/
*                                                                  0352
*RCODES:                                                           0352
*   PROC OPTIONS(NOSAVEAREA);       /* ROUTING CODES PROCESSOR       */
RCODES   STM   @14,@03,12(@13)                                     0352
         STM   @05,@12,40(@13)                                     0352
*   RSAVE=0;                        /* INITIALIZE RSAVE TO ZERO      */
         SLR   RSAVE,RSAVE                                         0353
*   SKIP='0'B;                      /* INITIALIZE SKIP TO ZERO       */
         NI    SKIP,B'11101111'                                    0354
*   L=1;                            /* INIT COLUMN INDEX TO 1        */
*                                                                  0355
         LA    L,1                                                 0355
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK TO SEE IF HARDCOPY MESSAGE BEING BUILT                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0356
*   IF HDCOPY='1'B                  /* IF ON THE HARDCOPY MSG        */
*     THEN                          /* THEN GET ROUTE CODES          */
         TM    HDCOPY,B'01000000'                                  0356
         BNO   @RF00356                                            0356
*     RTCODES=UCMHRDRT;             /* FROM THE UCM BASE             */
         L     @12,UCMMCSPT                                        0357
         LH    @12,UCMHRDRT(,@12)                                  0357
         N     @12,@CF02819                                        0357
         STH   @12,RTCODES                                         0357
*   ELSE                            /* IF NOT HARDCOPY MSG, THEN     */
*                                                                  0358
*     /***************************************************************/
*     /*                                                             */
*     /* CHECK TO SEE IF CONSOLE IS A COMPOSITE                      */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0358
*     IF COMPC(INDEX)=0             /* IF NOT A COMPOSITE            */
*       THEN                        /* THEN GET RCODES FROM UCME     */
         B     @RC00356                                            0358
@RF00356 LR    @12,INDEX                                           0358
         MH    @12,@CH00077                                        0358
         LR    @06,CONSPTR                                         0358
         AL    @06,@CF02815                                        0358
         LH    @06,COMPC-14(@12,@06)                               0358
         LTR   @06,@06                                             0358
         BNZ   @RF00358                                            0358
*       RTCODES=RTCD(INDEX);        /* GET THEM FROM THE UCME        */
         AL    @12,@CF02826                                        0359
         LH    @12,RTCD-10(@12,CONSPTR)                            0359
         STH   @12,RTCODES                                         0359
*     ELSE                          /* IF IT IS COMPOSITE            */
*       DO;                         /* THEN WE MUST GET THE          */
         B     @RC00358                                            0360
@RF00358 DS    0H                                                  0361
*         INDEXSVE=INDEX;           /* SAVE CURRENT INDEX            */
         LR    INDEXSVE,INDEX                                      0361
*         INDEX=COMPC(INDEX);       /* SET INDEX TO OUTPUT           */
         LA    @12,24                                              0362
         LR    @01,INDEX                                           0362
         MR    @00,@12                                             0362
         AL    @01,@CF02815                                        0362
         LH    @02,COMPC-14(@01,CONSPTR)                           0362
         LR    INDEX,@02                                           0362
*         RTCODES=RTCD(INDEX);      /* HALF OF COMPOSITE             */
         LR    @01,@12                                             0363
         MR    @00,INDEX                                           0363
         AL    @01,@CF02826                                        0363
         LH    @12,RTCD-10(@01,CONSPTR)                            0363
         STH   @12,RTCODES                                         0363
*         INDEX=INDEXSVE;           /* RESTORE INDEX                 */
         LR    INDEX,INDEXSVE                                      0364
*       END;                        /* END GET RCODES SECTION        */
*                                                                  0365
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK FOR NO ROUTE CODES                                      */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0366
*   IF RTCODES=0                    /* IF NO ROUTE CODES             */
*     THEN                                                         0366
@RC00358 DS    0H                                                  0366
@RC00356 LH    @12,RTCODES                                         0366
         N     @12,@CF02819                                        0366
         LTR   @12,@12                                             0366
         BNZ   @RF00366                                            0366
*     ROUTCDES(1:4)='NONE';         /* IN THE MESSAGE                */
         MVC   ROUTCDES(4,WPLPTR),@CC02716                         0367
*   ELSE                            /* IF NOT NONE, IS IT ALL ?      */
*                                                                  0368
*     /***************************************************************/
*     /*                                                             */
*     /* CHECK FOR ROUTE CODES = ALL                                 */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0368
*     IF RTCODES='FFFE'X            /* IF ALL SPECIFIED              */
*         ×                         /* AS 1-15 OR 1-16               */
*         RTCODES='FFFF'X           /* IN EITHER WAY                 */
*       THEN                        /* THEN PLACE ALL                */
         B     @RC00366                                            0368
@RF00366 CLC   RTCODES(2),@CH02717                                 0368
         BE    @RT00368                                            0368
         CLC   RTCODES(2),@CH02718                                 0368
         BNE   @RF00368                                            0368
@RT00368 DS    0H                                                  0369
*       ROUTCDES(1:3)='ALL';        /* IN THE MESSAGE                */
         MVC   ROUTCDES(3,WPLPTR),@CC02719                         0369
*     ELSE                          /* OTHERWISE CHECK EACH BIT      */
*       DO I=1 TO 16;               /* LOOP OVER ALL 16 R/C BITS     */
*                                                                  0370
         B     @RC00368                                            0370
@RF00368 LA    I,1                                                 0370
@DL00370 DS    0H                                                  0371
*         /***********************************************************/
*         /*                                                         */
*         /* CHECK TO SEE IF CURRENT BIT IS ON                       */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0371
*         IF(BMASK(I)&RTCODES)^=0   /* IF CURRENT BIT ON             */
*           THEN                    /* THEN FIRST DECIDE IF WORK-    */
         SLR   @12,@12                                             0371
         LR    @02,I                                               0371
         ALR   @02,@02                                             0371
         LH    @15,RTCODES                                         0371
         N     @15,@CF02819                                        0371
         LH    @14,BMASK-2(@02)                                    0371
         N     @14,@CF02819                                        0371
         NR    @15,@14                                             0371
         CR    @15,@12                                             0371
         BE    @RF00371                                            0371
*           DO;                     /* ON A SERIES. IF RSAVE=0       */
*                                                                  0372
*             /*******************************************************/
*             /*                                                     */
*             /* CHECK FOR PART OF A SERIES OF ROUTCODES             */
*             /*                                                     */
*             /*******************************************************/
*                                                                  0373
*             IF RSAVE=0            /* THEN THIS IS NOT PART OF A    */
*               THEN                /* SERIES,SO IT IS TO GO INTO    */
         CR    RSAVE,@12                                           0373
         BNE   @RF00373                                            0373
*               DO;                 /* THE MESSAGE. INITIALIZE THE   */
*                 SKIP='0'B;        /* SKIP FLAG TO SHOW NONE SKIP   */
         NI    SKIP,B'11101111'                                    0375
*                 ROUTCDES(L:L+1)=RCODS(I);/* PED,MOVE R/C           */
*                                                                  0376
         LR    @12,WPLPTR                                          0376
         ALR   @12,L                                               0376
         LA    @07,RCODS-2(@02)                                    0376
         MVC   ROUTCDES-1(2,@12),0(@07)                            0376
*                 /***************************************************/
*                 /*                                                 */
*                 /* CHECK FOR LAST R/C LESS THAN TEN                */
*                 /*                                                 */
*                 /***************************************************/
*                                                                  0377
*                 IF I<10           /* INTO MSG. IF A ONE DIGIT NO   */
*                   THEN            /* THEN ONLY INDEX UP BY ONE     */
         C     I,@CF00035                                          0377
         BNL   @RF00377                                            0377
*                   L=L+1;          /* TO SET THE COMMA. IF TWO      */
         AL    L,@CF00068                                          0378
*                 ELSE              /* DIGIT NO. THEN INDEX UP BY    */
*                   L=L+2;          /* TWO TO GET CORRECT COMMA      */
         B     @RC00377                                            0379
@RF00377 AL    L,@CF00080                                          0379
*                 ROUTCDES(L)=',';  /* POSITION. COMMA IN MSG        */
@RC00377 LR    @12,WPLPTR                                          0380
         ALR   @12,L                                               0380
         MVI   ROUTCDES-1(@12),C','                                0380
*                 L=L+1;            /* INDEX PAST THE COMMA          */
         AL    L,@CF00068                                          0381
*                 RSAVE=I;          /* SET RSAVE IN CASE NEXT R/C    */
         LR    RSAVE,I                                             0382
*               END;                /* IS SEQUENTIAL.                */
*             ELSE                  /* IF THIS IS IN A SERIES,THEN   */
*               SKIP='1'B;          /* SIGNAL R/C SKIPPED & CONTIN   */
         B     @RC00373                                            0384
@RF00373 OI    SKIP,B'00010000'                                    0384
*           END;                    /* UE TO NEXT BIT POSITION       */
*         ELSE                      /* IF CURRENT BIT NOT ON,THEN    */
*           DO;                     /* CHECK TO SEE IF WE WERE IN    */
*                                                                  0386
         B     @RC00371                                            0386
@RF00371 DS    0H                                                  0387
*             /*******************************************************/
*             /*                                                     */
*             /* CHECK TO SEE IF WE WERE IN A SERIES AFTER LAST R/C  */
*             /*                                                     */
*             /*******************************************************/
*                                                                  0387
*             IF RSAVE^=0           /* A SERIES. IF SO, AND IF THE   */
*               THEN                /* SKIP FLAG IS ON, THEN WE      */
         C     RSAVE,@CF00044                                      0387
         BE    @RF00387                                            0387
*               DO;                 /* MUST FILL IN THE LAST R/C     */
*                                                                  0388
*                 /***************************************************/
*                 /*                                                 */
*                 /* SEE IF ANY ROUTE CODES SKIPPED IN SERIES        */
*                 /*                                                 */
*                 /***************************************************/
*                                                                  0389
*                 IF SKIP='1'B      /* OF THE SERIES. IT IS IN       */
*                   THEN            /* RCODS(I-1) SINCE THIS IS      */
         TM    SKIP,B'00010000'                                    0389
         BNO   @RF00389                                            0389
*                   DO;             /* THE FIRST NON-MATCH FOUND     */
*                     L=L-1;        /* BACK UP OVER COMMA            */
         BCTR  L,0                                                 0391
*                     ROUTCDES(L)='-';/* AFTER SERIES. PUT           */
         LR    @12,WPLPTR                                          0392
         ALR   @12,L                                               0392
         MVI   ROUTCDES-1(@12),C'-'                                0392
*                     ROUTCDES(L+1:L+2)=RCODS(I-1);/* DASH           */
*                                                                  0393
         LR    @12,WPLPTR                                          0393
         ALR   @12,L                                               0393
         LR    @07,I                                               0393
         ALR   @07,@07                                             0393
         LA    @02,RCODS-4(@07)                                    0393
         MVC   ROUTCDES(2,@12),0(@02)                              0393
*                     /***********************************************/
*                     /*                                             */
*                     /* SEE IF PREVIOUS R/C WAS LESS THAN 10        */
*                     /*                                             */
*                     /***********************************************/
*                                                                  0394
*                     IF I<11       /* PUT THE PREVIOUS R/C IN MSG   */
*                       THEN        /* IF 1 DIGIT, INDEX UP BY 2     */
         C     I,@CF02721                                          0394
         BNL   @RF00394                                            0394
*                       L=L+2;      /* TO SKIP DASH & DIGIT. IF      */
         AL    L,@CF00080                                          0395
*                     ELSE          /* 2 DIGIT R/C, INDEX BY 3 TO    */
*                       L=L+3;      /* SKIP DASH & 2 DIGITS          */
         B     @RC00394                                            0396
@RF00394 AL    L,@CF00034                                          0396
*                     ROUTCDES(L)=',';/* SET COMM AFTER R/C          */
@RC00394 LR    @12,WPLPTR                                          0397
         ALR   @12,L                                               0397
         MVI   ROUTCDES-1(@12),C','                                0397
*                     L=L+1;        /* INDEX UP BY 1 PAST THE        */
         AL    L,@CF00068                                          0398
*                   END;            /* COMMA JUST SET                */
*                 RSAVE=0;          /* SIGNAL END OF SERIES          */
@RF00389 SLR   RSAVE,RSAVE                                         0400
*               END;                /* END END-OF-SERIES PROCESS.    */
*           END;                    /* END CURRENT BIT NOT ON PROC   */
@RF00387 DS    0H                                                  0403
*       END;                        /* END SEARCH LOOP AFTER 16      */
@RC00371 AL    I,@CF00068                                          0403
         C     I,@CF00041                                          0403
         BNH   @DL00370                                            0403
*   ROUTCDES(L-1)=' ';              /* BLANK OUT EXTRA LAST COMMA    */
@RC00368 DS    0H                                                  0404
@RC00366 LR    @12,WPLPTR                                          0404
         ALR   @12,L                                               0404
         MVI   ROUTCDES-2(@12),C' '                                0404
*   RETURN;                         /* RETURN TO CALLER              */
@EL00008 DS    0H                                                  0405
@EF00008 DS    0H                                                  0405
@ER00008 LM    @14,@03,12(@13)                                     0405
         LM    @05,@12,40(@13)                                     0405
         BR    @14                                                 0405
*   END RCODES;                     /* END ROUTE CODE PROCESS. RTN   */
         EJECT
*                                                                  0407
*/********************************************************************/
*/*                                                                  */
*/*  PROCEDURE - HARDCOPY                                            */
*/*                                                                  */
*/*  IF HCUCM IS NOT ZERO THEN IT WILL BE THE INDEX OF THE DATA-SAVE-*/
*/*  ENTRY FOR THE HARDCOPY CONSOLE(OUTPUT HALF IF ITS A COMPOSITE). */
*/*                                                                  */
*/*  IF HCUCM IS ZERO, THEN IF UCMSYSG IS ON, HARDCOPY IS TO SYSLOG. */
*/*  IF UCMSYSG IS NOT ON, THEN THERE IS NO HARDCOPY IN THE SYSTEM.  */
*/*                                                                  */
*/*  INPUTS - HCUCM, AND THE ARRAY OF CONSOLE DATA-SAVE-AREAS.       */
*/*                                                                  */
*/*  OUTPUTS - IF HARDCOPY IS NOT IN THE SYSTEM, THEN THIS ROUTINE   */
*/*            HAS NO OUTPUTS. HC. INDICATOR WILL BE OFF.            */
*/*                                                                  */
*/*            IF HARDCOPY IS TO SYSLOG, THE UNIT NAME IN THE MESSAGE*/
*/*            WILL BE 'SYSLOG'. THE AUTHORITY PORTION OF THE MESSAGE*/
*/*            WILL BE ONE OF THE FOLLOWING: INCMDS,STCMDS,CMDS, OR  */
*/*            NOCMDS. THE CONDITION PART OF THE MESSAGE WILL BE 'H'.*/
*/*                                                                  */
*/*            IF HARDCOPY IS TO A CONSOLE, THE UNIT NAME(S) OF THE  */
*/*            CONSOLE,ITS COMPOSITE (IF ANY), ITS ALTERNATE (IF ANY)*/
*/*            AND THE ALTERNATE'S COMPOSITE (IF ANY). THESE ARE     */
*/*            FILLED IN BY CALLING THE UNITNAME SUBROUTINE. OTHER   */
*/*            PARTS OF THE MESSAGE WILL BE FILLED IN WITH THE SAME  */
*/*            VALUES STATED ABOVE FOR HARDCOPY TO SYSLOG.           */
*/*                                     --                           */
*/*  CALLS - UNITNAME -TO PROCESS UNIT ADDRESSE(S) OF THE CONSOLE.   */
*/*          TOEBCIDC - TO CONVERT A 4 DIGIT VALUE TO EBCDIC @ZA11392*/
*/*                     AND PUT RESULT IN A MESSAGE.         @ZA11392*/
*/*                                                                  */
*/********************************************************************/
*                                                                  0407
*HARDCOPY:                                                         0407
*   PROC;                                                          0407
*                                                                  0407
HARDCOPY STM   @14,@03,@SA00009                                    0407
         STM   @05,@12,@SA00009+24                                 0407
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK TO SEE IF HARDCOPY IS TO A CONSOLE                      */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0408
*   IF HCUCM^=0                     /* IF HARDCOPY TO A CONSOLE      */
*     THEN                          /* THEN SET HC. MSG INDICATOR.   */
         LTR   HCUCM,HCUCM                                         0408
         BZ    @RF00408                                            0408
*     DO;                           /* NEXT CHECK FOR COMPOSITE,     */
*       HDCOPY='1'B;                /* AFTER SETTING INDEX INTO      */
         OI    HDCOPY,B'01000000'                                  0410
*       INDEX=HCUCM;                /* ARRAY OF DATA-SAVE AREAS.     */
         LR    INDEX,HCUCM                                         0411
*       CALL TOEBCDIC(WQECNT(INDEX),NWQES);/* CONVERT NUMBER OF WQE'S
*                                      TO EBCDIC AND PUT IN MSG    0412
*                                                            @ZA11392*/
*                                                                  0412
         LR    @12,INDEX                                           0412
         MH    @12,@CH00077                                        0412
         AL    @12,@CF02817                                        0412
         LA    @12,WQECNT-2(@12,CONSPTR)                           0412
         ST    @12,@AL00001                                        0412
         LA    @12,NWQES(,WPLPTR)                                  0412
         ST    @12,@AL00001+4                                      0412
         LA    @01,@AL00001                                        0412
         BAL   @14,TOEBCDIC                                        0412
*       /*************************************************************/
*       /*                                                           */
*       /* CHECK FOR COMPOSITE CONSOLE                               */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0413
*       IF COMPC(INDEX)^=0          /* IF COMPOSITE, THEN THIS IS    */
*         THEN                      /* THE OUTPUT HALF. GET THE      */
         LR    @12,INDEX                                           0413
         MH    @12,@CH00077                                        0413
         AL    @12,@CF02815                                        0413
         LH    @12,COMPC-14(@12,CONSPTR)                           0413
         LTR   @12,@12                                             0413
         BZ    @RF00413                                            0413
*         INDEX=COMPC(INDEX);       /* INDEX TO INPUT HALF.          */
         LR    INDEX,@12                                           0414
*       CALL UNITNAME;              /* PROCESS DEVICE NAMES.         */
@RF00413 BAL   @14,UNITNAME                                        0415
*       CONDITN='H    ';            /* FILL IN HCPY CONDITION.       */
         MVC   CONDITN(5,WPLPTR),@CC02726                          0416
*     END;                          /* END HC. TO CONSOLE PROCESS.   */
*   ELSE                            /* IF HC. NOT TO A CONSOLE       */
*                                                                  0418
*     /***************************************************************/
*     /*                                                             */
*     /* SEE IF HARDCOPY IS TO SYSLOG                                */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0418
*     IF UCMSYSG='1'B               /* THEN IF ITS TO SYSLOG         */
*       THEN                        /* THEN FIRST FILL IN THE        */
         B     @RC00408                                            0418
@RF00408 L     @12,UCMMCSPT                                        0418
         TM    UCMSYSG(@12),B'00000010'                            0418
         BNO   @RF00418                                            0418
*       DO;                         /* UNIT NAME FIELD WITH THE      */
*         CONSOLE='SYSLOG';         /* WORD 'SYSLOG'. SIGNAL @ZA11392*/
         MVI   CONSOLE+6(WPLPTR),C' '                              0420
         MVC   CONSOLE(6,WPLPTR),@CC02727                          0420
*         HDCOPY='1'B;              /* HC. MSG. BEING BUILT.         */
         OI    HDCOPY,B'01000000'                                  0421
*         CONDITN='H    ';          /* FILL IN HC. CONDITION.        */
         MVC   CONDITN(5,WPLPTR),@CC02726                          0422
*         CALL TOEBCDIC(HCWQECNT,NWQES);/* CONVERT NUMBER OF WQE'S TO
*                                      EBCDIC AND PUT IN MSG @ZA11392*/
         LA    @12,HCWQECNT                                        0423
         ST    @12,@AL00001                                        0423
         LA    @12,NWQES(,WPLPTR)                                  0423
         ST    @12,@AL00001+4                                      0423
         LA    @01,@AL00001                                        0423
         BAL   @14,TOEBCDIC                                        0423
*       END;                        /* END HC. TO SYSLOG PROCESS.    */
*                                                                  0424
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK TO SEE IF THIS IS THE HARDCOPY MESSAGE BEING BUILT      */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0425
*   IF HDCOPY='1'B                  /* IF HC. WAS IN SYSTEM          */
*     THEN                          /* THEN CHECK AUTHORITY PARMS.   */
*                                                                  0425
@RF00418 DS    0H                                                  0425
@RC00408 TM    HDCOPY,B'01000000'                                  0425
         BNO   @RF00425                                            0425
*     /***************************************************************/
*     /*                                                             */
*     /* CHECK FOR INCMDS TO HARDCOPY                                */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0426
*     IF UCMSDS1B='1'B              /* IF INCMDS TO HARDCOPY         */
*       THEN                        /* THEN MOVE 'INCMDS' TO         */
         L     @12,UCMMCSPT                                        0426
         TM    UCMSDS1B(@12),B'01000000'                           0426
         BNO   @RF00426                                            0426
*       AUTH='INCMDS';              /* THE AUTH PART OF MESSAGE      */
         MVI   AUTH+6(WPLPTR),C' '                                 0427
         MVI   AUTH+7(WPLPTR),C' '                                 0427
         MVC   AUTH(6,WPLPTR),@CC02728                             0427
*     ELSE                          /* ELSE IF STCMDS TO             */
*                                                                  0428
*       /*************************************************************/
*       /*                                                           */
*       /* CHECK FOR STCMDS TO HARDCOPY                              */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0428
*       IF UCMSDS1A='1'B            /* HARDCOPY THEN MOVE            */
*         THEN                      /* 'STCMDS' TO AUTH PART OF      */
         B     @RC00426                                            0428
@RF00426 L     @12,UCMMCSPT                                        0428
         TM    UCMSDS1A(@12),B'10000000'                           0428
         BNO   @RF00428                                            0428
*         AUTH='STCMDS';            /* MESSAGE.                      */
         MVI   AUTH+6(WPLPTR),C' '                                 0429
         MVI   AUTH+7(WPLPTR),C' '                                 0429
         MVC   AUTH(6,WPLPTR),@CC02729                             0429
*       ELSE                        /* ELSE IF CMDS TO HARDCOPY      */
*                                                                  0430
*         /***********************************************************/
*         /*                                                         */
*         /* CHECK FOR CMDS TO HARDCOPY                              */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0430
*         IF UCMSYSC='1'B           /* THEN MOVE 'CMDS'              */
*           THEN                    /* TO THE AUTH PART OF THE       */
         B     @RC00428                                            0430
@RF00428 L     @12,UCMMCSPT                                        0430
         TM    UCMSYSC(@12),B'00100000'                            0430
         BNO   @RF00430                                            0430
*           AUTH=' CMDS ';          /* MESSAGE.                      */
         MVI   AUTH+6(WPLPTR),C' '                                 0431
         MVI   AUTH+7(WPLPTR),C' '                                 0431
         MVC   AUTH(6,WPLPTR),@CC02730                             0431
*         ELSE                      /* ELSE IF NONE OF THE ABOVE,    */
*           AUTH='NOCMDS';          /* MOVE 'NOCMDS' TO MESSAGE      */
         B     @RC00430                                            0432
@RF00430 MVI   AUTH+6(WPLPTR),C' '                                 0432
         MVI   AUTH+7(WPLPTR),C' '                                 0432
         MVC   AUTH(6,WPLPTR),@CC02731                             0432
*   END HARDCOPY;                   /* END HC. MSG. ROUTINE.         */
@EL00009 DS    0H                                                  0433
@EF00009 DS    0H                                                  0433
@ER00009 LM    @14,@03,@SA00009                                    0433
         LM    @05,@12,@SA00009+24                                 0433
         BR    @14                                                 0433
         EJECT
