         BAL   @14,GETMQE                                       03S0170
*                 /***************************************************/
*                 /*                                                 */
*                 /* DETERMINE IF SPACE WAS AVAILABLE, IF NOT IGNORE */
*                 /* REQUEST. A RETURN CODE OF 4 IS PLACED IN RCAREA */
*                 /* BY GETMAIN AND WILL BE PASSED BACK TO THE       */
*                 /* CALLER.                                         */
*                 /*                                                 */
*                 /***************************************************/
*                                                               03S0171
*                 IF RCAREA=4 THEN  /* GETMAIN UNSUCCESSFUL?         */
         CLC   RCAREA(4),@CF00045                               03S0171
         BNE   @RF00171                                         03S0171
*                   PROCFLAG='0'B;  /* YES, IGNORE REQUEST           */
         NI    PROCFLAG,B'11011111'                             03S0172
*                 ELSE              /* GETMAIN SUCCESSFUL, REG1 03S0173
*                                      CONTAINS THE ADDR OF THE 03S0173
*                                      OBTAINED SPACE                */
*                   DO;                                         03S0173
         B     @RC00171                                         03S0173
@RF00171 DS    0H                                               03S0174
*                     MQEPTR=REG1;  /* SET PTR TO NEW MQE            */
         LR    MQEPTR,REG1                                      03S0174
*                     RFY                                       03S0175
*                       REG1 UNRSTD;/* RETURN REG 1 TO COMPILER USE  */
*                     IF UCMMQPTR=ZERO THEN/* IS MQE QUEUE EMPTY?    */
         L     @01,LCVTPTR                                      03S0176
         L     @15,UCMBASPT(,@01)                               03S0176
         L     @14,UCMMQPTR(,@15)                               03S0176
         LTR   @14,@14                                          03S0176
         BNZ   @RF00176                                         03S0176
*                       UCMMQPTR=MQEPTR;/* YES, SET PTR TO BEGINNING
*                                      OF MQE QUEUE                  */
         ST    MQEPTR,UCMMQPTR(,@15)                            03S0177
*                     ELSE                                      03S0178
*                       UCMMQEND->MQELINK=MQEPTR;/* NO, CHAIN MQE TO
*                                      END OF QUEUE                  */
         B     @RC00176                                         03S0178
@RF00176 L     @01,LCVTPTR                                      03S0178
         L     @01,UCMBASPT(,@01)                               03S0178
         L     @01,UCMMQEND(,@01)                               03S0178
         ST    MQEPTR,MQELINK(,@01)                             03S0178
*                     UCMMQEND=MQEPTR;/* CHANGE PTR TO END OF MQE
*                                      QUEUE TO ADDRESS OF THIS MQE  */
@RC00176 L     @01,LCVTPTR                                      03S0179
         L     @15,UCMBASPT(,@01)                               03S0179
         ST    MQEPTR,UCMMQEND(,@15)                            03S0179
*                     MQEZERO(1:LENGTH(MQE)*EIGHT)=''B;/* ZERO MQE   */
         XC    MQEZERO(12,MQEPTR),MQEZERO(MQEPTR)               03S0180
*                     MQENAME=MQECHAR;/* FILL IN MQE IDENTIFIER      */
         MVC   MQENAME(4,MQEPTR),@CC01717                       03S0181
*                     MQEID=MPLID;  /* COPY ASID FROM MPL            */
         MVC   MQEID(2,MQEPTR),MPLID(MPLPTR)                    03S0182
*                     IF UCMMQNXT=ZERO THEN/* WHEN THE PTR TO NEXT
*                                      MQE IS NULL, SET IT TO THE
*                                      ADDR OF THIS MQE              */
         L     @14,UCMMQNXT(,@15)                               03S0183
         LTR   @14,@14                                          03S0183
         BNZ   @RF00183                                         03S0183
*                       UCMMQNXT=MQEPTR;                        03S0184
         ST    MQEPTR,UCMMQNXT(,@15)                            03S0184
*                   END;                                        03S0185
*                                                               03S0185
*/*** END OF CREATMQE SEGMENT ****************************************/
*                                                               03S0186
*               END;                                            02S0186
*             ELSE                                              02S0187
*               PROCFLAG='0'B;      /* MATCH NOT FOUND AND IT'S NOT A
*                                      START REQUEST SO IGNORE  02S0187
*                                      REQUEST                       */
         B     @RC00167                                         02S0187
@RF00167 NI    PROCFLAG,B'11011111'                             02S0187
*           IF PROCFLAG='1'B THEN   /* CONTINUE PROCESSING?          */
@RC00167 DS    0H                                               02S0188
@RF00166 TM    PROCFLAG,B'00100000'                             02S0188
         BNO   @RF00188                                         02S0188
