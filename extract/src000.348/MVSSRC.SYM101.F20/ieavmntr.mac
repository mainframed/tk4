*                   /* JOBNAMES?                                     */
*                   /*                                               */
*                   /*************************************************/
*                                                               03S0211
*                   IF MPLJBNM='1'B&MQEJBNM='1'B THEN           03S0211
         TM    MPLJBNM(MPLPTR),B'10000000'                      03S0211
         BNO   @RF00211                                         03S0211
         TM    MQEJBNM(MQEPTR),B'10000000'                      03S0211
         BNO   @RF00211                                         03S0211
*                     DO;           /* YES                           */
*                       MQEJBNM='0'B;/* INDICATE TERMINAL IS NO 03S0213
*                                      LONGER MONITORING JOBNAMES    */
         NI    MQEJBNM(MQEPTR),B'01111111'                      03S0213
*                       BAJNCNT=BAJNCNT-ONE;/* DECREMENT COUNT OF
