         TITLE 'IEAVMNTR, COMMTASK - MONITOR COMMAND INTERFACE PROGRAM *
                        '
IEAVMNTR CSECT ,                                                01S0001
@MAINENT DS    0H                                               01S0001
         USING *,@15                                            01S0001
         B     @PROLOG                                          01S0001
         DC    AL1(19)                                          01S0001
         DC    C'IEAVMNTRR0200073309'                           01S0001
         DROP  @15
@PROLOG  ST    @14,12(,@13)                                     01S0001
         STM   @00,@12,20(@13)                                  01S0001
         BALR  @10,0                                            01S0001
@PSTART  DS    0H                                               01S0001
         USING @PSTART,@10                                      01S0001
         MODESET EXTKEY=SUPR,SAVEKEY=(2)
         LR    REG8,REG13          SAVE REG 13 - SETLOCK DESTROYS IT
         SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,                        *
               RELATED=(UCM,IEAVMNTR(EPILOG))
         LR    REG5,REG13          SAVE RETURN CODE FROM SETLOCK
         SETLOCK OBTAIN,TYPE=CMS,MODE=UNCOND,                          *
               RELATED=(UCM,IEAVMNTR(EPILOG))
         LR    REG6,REG13          SAVE RETURN CODE FROM SETLOCK
         L     REG11,CVTPTR        GET PTR TO CVT
         L     REG11,CVTCUCB-CVT(REG11) GET PTR TO UCM BASE
         L     REG11,UCMFRRAD-UCM(REG11) GET FRR ADDR
         SETFRR A,FRRAD=(REG11),PARMAD=(REG12),WRKREGS=(REG12,REG13)
         MVC   PARMID-PARMLIST(4,REG12),=CL4'MNTR' MODULE ID
         L     REG4,PSATOLD        SET UP REG 4 WITH TCB ADDR
         L     REG7,PSAAOLD        SET UP REG 7 WITH ASCB ADDR
         L     REG0,@SIZDATD       SET UP SUBPOOL AND LENGTH FOR
*                                  BRANCH ENTRY TO GETMAIN
         GETMAIN R,LV=(0),BRANCH=YES
         LR    REG9,REG1           SET UP DATAREG
         USING @DATD,REG9          ADDRESSABILITY TO DATA AREA
         ST    REG8,@SA00001+4     FORWARD CHAIN SAVEAREAS
         STC   REG2,@OLDKEY        SAVE CALLERS KEY
         SPACE
*  CHANGE BACK TO CALLERS KEY IN ORDER TO CHAIN SAVEAREAS
         SPACE
         MODESET KEYADDR=(2)
         LM    REG0,REG2,20(REG8)  RELOAD REGS 0-2 FROM CALLERS
*                                  SAVEAREA
         ST    REG9,8(REG8)        BACKCHAIN SAVEAREAS
         LR    REG13,REG9          SET UP R13 WITH OWN SAVEAREA ADDR
         MODESET EXTKEY=SUPR       CHANGE BACK TO SUPERVISOR KEY
         MVC   @PC00001(4),0(@01)  MOVE PARAMETER TO DATA AREA
*                                                               01S0061
*   /*****************************************************************/
*   /*                                                               */
*   /* SET UP LOCAL CVTPTR                                           */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0062
*   LCVTPTR=CVTPTR;                 /* COPY CVTPTR                   */
         L     @11,CVTPTR                                       01S0062
         ST    @11,LCVTPTR                                      01S0062
*   RFY                                                         01S0063
*     CVTMAP BASED(LCVTPTR);        /* BASE FOR CVT IS LOCAL CVTPTR  */
*   MPLPTR=ADDR(MPLPARM);           /* SET PTR TO MONITOR PARAMETER
*                                      LIST                          */
*                                                               01S0064
         L     MPLPTR,@PC00001                                  01S0064
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE FLAGS TO CONTROL PROGRAM FLOW                      */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0065
*   IDFLAG='0'B;                    /* INVALID ID                    */
*   MATCHFLG='0'B;                  /* MATCH ON MPLID NOT FOUND      */
*   PROCFLAG='1'B;                  /* PROCESS REQUEST               */
*   LOCLOBT='0'B;                   /* LOCAL LOCK ALREADY HELD       */
*   CMSLOBT='0'B;                   /* CMS LOCK ALREADY HELD         */
         OI    PROCFLAG,B'00100000'                             01S0069
         NI    IDFLAG,B'00100111'                               01S0069
*   IF REG5=ZERO THEN               /* IS RETURN CODE 0              */
         LTR   REG5,REG5                                        01S0070
         BNZ   @RF00070                                         01S0070
*     LOCLOBT='1'B;                 /* INDICATE LOCAL LOCK WAS  01S0071
*                                      OBTAINED                      */
         OI    LOCLOBT,B'00010000'                              01S0071
*   IF REG6=ZERO THEN               /* IS RETURN CODE FROM SETLOCK
*                                      FOR CMS LOCK ZERO ?           */
@RF00070 LTR   REG6,REG6                                        01S0072
         BNZ   @RF00072                                         01S0072
*     CMSLOBT='1'B;                 /* INDICATE CMS LOCK WAS OBTAINED*/
         OI    CMSLOBT,B'00001000'                              01S0073
*   RFY                                                         01S0074
*    (REG5,                                                     01S0074
*     REG6) UNRSTD;                 /* RETURN REGS 5 AND 6 TO   01S0074
*                                      COMPILER USE                  */
@RF00072 DS    0H                                               01S0075
*   RCAREA=ZERO;                    /* INITIALIZE RETURN CODE TO 0   */
*                                                               01S0075
         SLR   @11,@11                                          01S0075
         ST    @11,RCAREA                                       01S0075
*   /*****************************************************************/
*   /*                                                               */
*   /* OBTAIN CMS LOCK TO SERIALIZE USE OF THE UCM                   */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0076
*   RFY                                                         01S0076
*     BASE BASED(BASEPTR);          /* MAKE PTR TO BASEA A REGISTER  */
*                                                               01S0076
*   /*****************************************************************/
*   /*                                                               */
*   /* DETERMINE IF THIS ROUTINE WAS CALLED FOR A CONSOLE OR TERMINAL*/
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0077
*   IF MPLCONS='1'B THEN            /* IS CONSOLE BIT SET IN MPL     */
         TM    MPLCONS(MPLPTR),B'10000000'                      01S0077
         BNO   @RF00077                                         01S0077
*     DO;                           /* YES, DO CONSOLE PROCESSING
*                                                                    */
*                                                               02S0079
*/*** START OF CONSPROC SEGMENT **************************************/
*/*                                                                  */
*/* CONSPROC - PROCESSING OF A MONITOR REQUEST FOR A CONSOLE         */
*/*                                                                  */
*/* FUNCTION - DETERMINES WHETHER THE CONSOLE ID PASSED IN THE       */
*/*    MONITOR PARAMETER LIST IS VALID.  IF THE ID IS VALID,         */
*/*    PASSES CONTROL TO SEGMENT SETCONS IN ORDER TO SET THE         */
*/*    COMMTASK - MONITOR INTERFACE FIELDS IN BASEA AND THE UCME.    */
*/*                                                                  */
*/* INPUT -                                                          */
*/*    IDFLAG IS INITIALIZED TO INVALID.                             */
*/*    LOCAL AND CMS LOCKS ARE HELD.                                 */
*/*    ADDRESSABILITY TO MPL AND UCM.                                */
*/*                                                                  */
*/* OUTPUT -                                                         */
*/*    IDFLAG INDICATES THE VALIDITY OF THE CONSOLE ID.              */
*/*    UCMDVEPT SET TO ADDRESS OF UCME FOR THE CONSOLE ID.           */
*/*                                                                  */
*/********************************************************************/
*                                                               02S0079
*       /*************************************************************/
*       /*                                                           */
*       /* DETERMINE IF CONSOLE ID IS 0 INDICATING A MONITOR REQUEST */
*       /* FOR THE MASTER CONSOLE                                    */
*       /*                                                           */
*       /*************************************************************/
*                                                               02S0079
*       IF MPLID(2)=ZERO THEN       /* IS CONSOLE ID 0?              */
         SLR   @08,@08                                          02S0079
         IC    @08,MPLID+1(,MPLPTR)                             02S0079
         CR    @11,@08                                          02S0079
         BNE   @RF00079                                         02S0079
*         DO;                       /* YES                           */
*           UCMDVEPT=UCMMCENT;      /* SET UCME PTR TO UCME ADDR OF
*                                      THE MASTER CONSOLE            */
         L     @11,LCVTPTR                                      02S0081
         L     @11,UCMBASPT(,@11)                               02S0081
         AL    @11,@CF01761                                     02S0081
         L     @11,UCMMCSPT(,@11)                               02S0081
         L     UCMDVEPT,UCMMCENT(,@11)                          02S0081
*           IDFLAG='1'B;            /* INDICATE ID IS VALID          */
         OI    IDFLAG,B'10000000'                               02S0082
*         END;                                                  02S0083
*       ELSE                        /* CONSOLE ID IS NOT ZER0        */
*                                                               02S0084
*         /***********************************************************/
*         /*                                                         */
*         /* LOOP THROUGH UCMES LOOKING FOR A MATCH ON UCMID WITH    */
*         /* MPLID                                                   */
*         /*                                                         */
*         /***********************************************************/
*                                                               02S0084
*         DO;                                                   02S0084
         B     @RC00079                                         02S0084
@RF00079 DS    0H                                               02S0085
*           UCMDVEPT=UCMVEA;        /* INITIALIZE UCME PTR TO ADDR OF
*                                      FIRST UCME                    */
         L     @11,LCVTPTR                                      02S0085
         L     @11,UCMBASPT(,@11)                               02S0085
         L     UCMDVEPT,UCMVEA(,@11)                            02S0085
*           LOOPTR=UCMDVEPT;        /* INITIALIZE LOOP CONTROL TO
*                                      ADDR OF FIRST UCME            */
*                                                               02S0086
         LR    LOOPTR,UCMDVEPT                                  02S0086
*           /*********************************************************/
*           /*                                                       */
*           /* ATTEMPT TO FIND THE UCME WHOSE UCMID MATCHES THE MPLID*/
*           /*                                                       */
*           /*********************************************************/
*                                                               02S0087
*           DO WHILE(LOOPTR^>UCMVEL);/* STOP WHEN LOOP CONTROL IS
*                                      GREATER THAN THE LAST UCME
*                                      ADDR                          */
         B     @DE00087                                         02S0087
@DL00087 DS    0H                                               02S0088
*             IF MPLID(2)=UCMID THEN/* DO CONSOLE ID'S MATCH?        */
         CLC   MPLID+1(1,MPLPTR),UCMID(UCMDVEPT)                02S0088
         BNE   @RF00088                                         02S0088
*               DO;                 /* YES                           */
*                 IDFLAG='1'B;      /* INDICATE ID IS VALID          */
         OI    IDFLAG,B'10000000'                               02S0090
*                 LOOPTR=UCMVEL+ONE;/* SET UP LOOP CONTROL TO EXIT
*                                      FROM LOOP                     */
         L     @11,LCVTPTR                                      02S0091
         L     @11,UCMBASPT(,@11)                               02S0091
         LA    LOOPTR,1                                         02S0091
         AL    LOOPTR,UCMVEL(,@11)                              02S0091
*               END;                                            02S0092
*             ELSE                  /* CONSOLE ID'S DON'T MATCH      */
*               DO;                                             02S0093
         B     @RC00088                                         02S0093
@RF00088 DS    0H                                               02S0094
*                 UCMDVEPT=UCMDVEPT+UCMVEZ;/* INCREMENT UCMEPTR TO
*                                      ADDR OF NEXT UCME             */
         L     @11,LCVTPTR                                      02S0094
         L     @11,UCMBASPT(,@11)                               02S0094
         AL    UCMDVEPT,UCMVEZ(,@11)                            02S0094
*                 LOOPTR=UCMDVEPT;  /* SET LOOP CONTROL TO ADDR OF
*                                      NEXT UCME                     */
         LR    LOOPTR,UCMDVEPT                                  02S0095
*               END;                                            02S0096
*           END;                                                02S0097
@RC00088 DS    0H                                               02S0097
@DE00087 L     @11,LCVTPTR                                      02S0097
         L     @11,UCMBASPT(,@11)                               02S0097
         C     LOOPTR,UCMVEL(,@11)                              02S0097
         BNH   @DL00087                                         02S0097
*         END;                                                  02S0098
*                                                               02S0098
*       /*************************************************************/
*       /*                                                           */
*       /* IF THE ID WAS FOUND TO BE VALID PASS CONTROL TO THE       */
*       /* SETCONS SEGMENT                                           */
*       /*                                                           */
*       /*************************************************************/
*                                                               02S0099
*       IF IDFLAG='1'B THEN         /* IS ID VALID?                  */
@RC00079 TM    IDFLAG,B'10000000'                               02S0099
         BNO   @RF00099                                         02S0099
*         DO;                       /* YES                           */
*                                                               02S0100
*           /*********************************************************/
*           /*                                                       */
*           /* SET THE MASTER SCHEDULER - COMMTASK INTERFACE FIELDS  */
*           /* IN THE UCM AND BASEA                                  */
*           /*                                                       */
*           /*********************************************************/
*                                                               02S0101
*                                                               03S0101
*/*** START OF SETCONS SEGMENT ***************************************/
*/*                                                                  */
*/* SETCONS - SET COMMTASK - MONITOR INTERFACE FIELDS FOR A CONSOLE  */
*/*                                                                  */
*/* FUNCTION - DETERMINE WHETHER REQUEST IS FOR START OR STOP.       */
*/*    SET UCME FLAGS FOR MONITOR TYPE(S) SPECIFIED AND ADJUST       */
*/*    APPROPRIATE MONITOR COUNT FIELDS IN BASEA.                    */
*/*                                                                  */
*/* INPUT -                                                          */
*/*    UCMDVEPT = ADDRESS OF THE UCME FOR THE CONSOLE                */
*/*    ADDRESSABILITY TO MPL, UCM, BASEA.                            */
*/*                                                                  */
*/* OUTPUT -                                                         */
*/*    MONITOR TYPE FLAGS IN UCME (UCMMSGA, UCMMSGB, UCMMSGF) SET.   */
*/*    MONITOR COUNTS IN BASEA (BAJNCNT, BASTCNT, BASESCT) ADJUSTED. */
*/*                                                                  */
*/********************************************************************/
*                                                               03S0101
*           BASEPTR=CVTMSER;        /* PTR TO BASEA IS IN CVT        */
         L     @11,LCVTPTR                                      03S0101
         L     BASEPTR,CVTMSER(,@11)                            03S0101
*           IF MPLSTRT='1'B THEN    /* START MONITOR REQUEST?        */
         TM    MPLSTRT(MPLPTR),B'00100000'                      03S0102
         BNO   @RF00102                                         03S0102
*             DO;                   /* YES                           */
*                                                               03S0103
*               /*****************************************************/
*               /*                                                   */
*               /* IS THERE A REQUEST FOR MONITOR JOBNAMES AND THE   */
*               /* CONSOLE IS NOT ALREADY MONITORING JOBNAMES?       */
*               /*                                                   */
*               /*****************************************************/
*                                                               03S0104
*               IF MPLJBNM='1'B&UCMMSGA='0'B THEN               03S0104
         TM    MPLJBNM(MPLPTR),B'10000000'                      03S0104
         BNO   @RF00104                                         03S0104
         TM    UCMMSGA(UCMDVEPT),B'10000000'                    03S0104
         BNZ   @RF00104                                         03S0104
*                 DO;               /* YES                           */
*                   UCMMSGA='1'B;   /* INDICATE CONSOLE IS MONITORING
*                                      JOBNAMES                      */
         OI    UCMMSGA(UCMDVEPT),B'10000000'                    03S0106
*                   BAJNCNT=BAJNCNT+ONE;/* INCREMENT COUNT OF   03S0107
*                                      CONSOLES AND TERMINALS   03S0107
*                                      MONITORING JOBNAMES           */
         LA    @11,1                                            03S0107
         AH    @11,BAJNCNT(,BASEPTR)                            03S0107
         STH   @11,BAJNCNT(,BASEPTR)                            03S0107
*                 END;                                          03S0108
*                                                               03S0108
*               /*****************************************************/
*               /*                                                   */
*               /* IS THERE A REQUEST FOR MONITOR STATUS AND THE     */
*               /* CONSOLE IS NOT ALREADY MONITORING STATUS?         */
*               /*                                                   */
*               /*****************************************************/
*                                                               03S0109
*               IF MPLSTAT='1'B&UCMMSGB='0'B THEN               03S0109
@RF00104 TM    MPLSTAT(MPLPTR),B'01000000'                      03S0109
         BNO   @RF00109                                         03S0109
         TM    UCMMSGB(UCMDVEPT),B'01000000'                    03S0109
         BNZ   @RF00109                                         03S0109
*                 DO;               /* YES                           */
*                   UCMMSGB='1'B;   /* INDICATE CONSOLE IS MONITORING
*                                      STATUS                        */
         OI    UCMMSGB(UCMDVEPT),B'01000000'                    03S0111
*                   BASTCNT=BASTCNT+ONE;/* INCREMENT COUNT OF   03S0112
*                                      CONSOLES AND TERMINALS   03S0112
*                                      MONITORING STATUS             */
         LA    @11,1                                            03S0112
         AH    @11,BASTCNT(,BASEPTR)                            03S0112
         STH   @11,BASTCNT(,BASEPTR)                            03S0112
*                 END;                                          03S0113
*                                                               03S0113
*               /*****************************************************/
*               /*                                                   */
*               /* IS THERE A REQUEST FOR MONITOR SESSIONS AND THE   */
*               /* CONSOLE IS NOT ALREADY MONITORING SESSIONS?       */
*               /*                                                   */
*               /*****************************************************/
*                                                               03S0114
*               IF MPLSESS='1'B&UCMMSGF='0'B THEN               03S0114
@RF00109 TM    MPLSESS(MPLPTR),B'00000100'                      03S0114
         BNO   @RF00114                                         03S0114
         TM    UCMMSGF(UCMDVEPT),B'00000100'                    03S0114
         BNZ   @RF00114                                         03S0114
*                 DO;                                           03S0115
*                   UCMMSGF='1'B;   /* INDICATE CONSOLE IS MONITORING
*                                      SESSIONS                      */
         OI    UCMMSGF(UCMDVEPT),B'00000100'                    03S0116
*                   BASESCT=BASESCT+ONE;/* INCREMENT COUNT OF   03S0117
*                                      CONSOLES AND TERMINALS   03S0117
*                                      MONITORING SESSIONS           */
         LA    @11,1                                            03S0117
         AH    @11,BASESCT(,BASEPTR)                            03S0117
         STH   @11,BASESCT(,BASEPTR)                            03S0117
*                 END;                                          03S0118
*             END;                                              03S0119
@RF00114 DS    0H                                               03S0120
*           IF MPLSTOP='1'B THEN    /* STOP MONITOR REQUEST?         */
@RF00102 TM    MPLSTOP(MPLPTR),B'00010000'                      03S0120
         BNO   @RF00120                                         03S0120
*             DO;                   /* YES                           */
*                                                               03S0121
*               /*****************************************************/
*               /*                                                   */
*               /* IS THERE A REQUEST FOR STOP MONITOR JOBNAMES AND  */
*               /* THE CONSOLE IS MONITORING JOBNAMES?               */
*               /*                                                   */
*               /*****************************************************/
*                                                               03S0122
*               IF MPLJBNM='1'B&UCMMSGA='1'B THEN               03S0122
         TM    MPLJBNM(MPLPTR),B'10000000'                      03S0122
         BNO   @RF00122                                         03S0122
         TM    UCMMSGA(UCMDVEPT),B'10000000'                    03S0122
         BNO   @RF00122                                         03S0122
*                 DO;               /* YES                           */
*                   UCMMSGA='0'B;   /* INDICATE CONSOLE IS NO LONGER
*                                      MONITORING JOBNAMES           */
         NI    UCMMSGA(UCMDVEPT),B'01111111'                    03S0124
*                   BAJNCNT=BAJNCNT-ONE;/* DECREMENT COUNT OF   03S0125
*                                      CONSOLES AND TERMINALS   03S0125
*                                      MONITORING JOBNAMES           */
         LH    @11,BAJNCNT(,BASEPTR)                            03S0125
         BCTR  @11,0                                            03S0125
         STH   @11,BAJNCNT(,BASEPTR)                            03S0125
*                 END;                                          03S0126
*                                                               03S0126
*               /*****************************************************/
*               /*                                                   */
*               /* IS THERE A REQUEST FOR STOP MONITOR STATUS AND THE*/
*               /* CONSOLE IS MONITORING STATUS?                     */
*               /*                                                   */
*               /*****************************************************/
*                                                               03S0127
*               IF MPLSTAT='1'B&UCMMSGB='1'B THEN               03S0127
@RF00122 TM    MPLSTAT(MPLPTR),B'01000000'                      03S0127
         BNO   @RF00127                                         03S0127
         TM    UCMMSGB(UCMDVEPT),B'01000000'                    03S0127
         BNO   @RF00127                                         03S0127
*                 DO;               /* YES                           */
*                   UCMMSGB='0'B;   /* INDICATE CONSOLE IS NO LONGER
*                                      MONITORING STATUS             */
         NI    UCMMSGB(UCMDVEPT),B'10111111'                    03S0129
*                   BASTCNT=BASTCNT-ONE;/* DECREMENT COUNT OF   03S0130
*                                      CONSOLES AND TERMINALS   03S0130
*                                      MONITORING STATUS             */
         LH    @11,BASTCNT(,BASEPTR)                            03S0130
         BCTR  @11,0                                            03S0130
         STH   @11,BASTCNT(,BASEPTR)                            03S0130
*                 END;                                          03S0131
*                                                               03S0131
*               /*****************************************************/
*               /*                                                   */
*               /* IS THERE A REQUEST FOR STOP MONITOR SESSIONS AND  */
*               /* THE CONSOLE IS MONITORING SESSIONS                */
*               /*                                                   */
*               /*****************************************************/
*                                                               03S0132
*               IF MPLSESS='1'B&UCMMSGF='1'B THEN               03S0132
@RF00127 TM    MPLSESS(MPLPTR),B'00000100'                      03S0132
         BNO   @RF00132                                         03S0132
         TM    UCMMSGF(UCMDVEPT),B'00000100'                    03S0132
         BNO   @RF00132                                         03S0132
*                 DO;               /* YES                           */
*                   UCMMSGF='0'B;   /* INDICATE CONSOLE IS NO LONGER
*                                      MONITORING SESSIONS           */
         NI    UCMMSGF(UCMDVEPT),B'11111011'                    03S0134
*                   BASESCT=BASESCT-ONE;/* DECREMENT COUNT OF   03S0135
*                                      CONSOLES AND TERMINALS   03S0135
*                                      MONITORING SESSIONS           */
         LH    @11,BASESCT(,BASEPTR)                            03S0135
         BCTR  @11,0                                            03S0135
         STH   @11,BASESCT(,BASEPTR)                            03S0135
*                 END;                                          03S0136
*             END;                                              03S0137
*                                                               03S0137
@RF00132 DS    0H                                               03S0138
*/*** END OF SETCONS SEGMENT *****************************************/
*                                                               03S0138
*         END;                                                  02S0138
*                                                               02S0138
@RF00120 DS    0H                                               02S0139
*/*** END OF CONSPROC SEGMENT ****************************************/
*                                                               02S0139
*     END;                                                      01S0139
@RF00099 DS    0H                                               01S0140
*   IF MPLTERM='1'B THEN            /* IS TERMINAL BIT SET IN MPL    */
@RF00077 TM    MPLTERM(MPLPTR),B'01000000'                      01S0140
         BNO   @RF00140                                         01S0140
*     DO;                           /* YES, DO TERMINAL PROCESSING
*                                                                    */
*                                                               02S0142
*/*** START OF TERMPROC SEGMENT **************************************/
*/*                                                                  */
*/* TERMPROC - PROCESSING OF A MONITOR REQUEST FOR A TERMINAL.       */
*/*                                                                  */
*/* FUNCTION - DETERMINES IF THE ID SPECIFIED IN THE MPL IS THE      */
*/*    ASID OF A TSO USER.  IF SO, LOOKS FOR AN EXISTING MONITOR     */
*/*    QUEUE ELEMENT FOR THE TSO USER.  IF ONE IS NOT FOUND AND      */
*/*    THE MPL SPECIFIES START MONITOR, SEGMENT CREATMQE IS PASSED   */
*/*    CONTROL TO CREATE AN MQE.  WHEN AN MQE IS NOT FOUND AND       */
*/*    THE MPL SPECIFIES STOP MONITOR, THE REQUEST IS IGNORED.       */
*/*    THE MQE ADDRESS IS PASSED TO SEGMENT SETTERM WHICH SETS       */
*/*    COMMTASK - MONITOR FIELDS IN THE MQE AND BASEA.               */
*/*                                                                  */
*/*                                                                  */
*/* INPUT -                                                          */
*/*    IDFLAG IS INITIALIZED TO INVALID.                             */
*/*    PROCESS FLAG IS INITIALIZED TO OK.                            */
*/*    MATCH FLAG IS INITIALIZED TO NOT-FOUND.                       */
*/*    ADDRESSABILITY TO ASVT, MPL, UCM                              */
*/*                                                                  */
*/* OUTPUT                                                           */
*/*    IDFLAG INDICATES VALIDITY OF ASID.                            */
*/*    MQEPTR SET TO ADDRESS OF MQE FOR THE TSO USER.                */
*/*                                                                  */
*/********************************************************************/
*/********************************************************************/
*/*                                                                  */
*/* DETERMINE IF ASID PASSED IN MPLID BELONGS TO A TSO USER          */
*/*                                                                  */
*/********************************************************************/
*                                                               02S0142
*       IF MPLID>ZERO&MPLID^>ASVTMAXU THEN/* IS THE ASID GREATER THAN
*                                      ZERO BUT NOT GREATER THAN THE
*                                      MAXIMUM NUMBER OF ADDRESS
*                                      SPACES?                       */
         SLR   @11,@11                                          02S0142
         SLR   @08,@08                                          02S0142
         ICM   @08,3,MPLID(MPLPTR)                              02S0142
         CR    @08,@11                                          02S0142
         BNH   @RF00142                                         02S0142
         L     @01,LCVTPTR                                      02S0142
         L     @15,ASVTPTR(,@01)                                02S0142
         C     @08,ASVTMAXU(,@15)                               02S0142
         BH    @RF00142                                         02S0142
*         IF ATABFLAG(MPLID)='0'B THEN/* YES, IS THE ASVT ENTRY FOR
*                                      THIS ASID ASSIGNED            */
         SLA   @08,2                                            02S0143
         ST    @08,@TF00001                                     02S0143
         ALR   @08,@15                                          02S0143
         TM    ATABFLAG-4(@08),B'10000000'                      02S0143
         BNZ   @RF00143                                         02S0143
*           DO;                     /* YES                           */
*             ASCBPTR=ATABLE(MPLID);/* GET THE ASCB PTR FROM THE ASVT
