*                   /*                                               */
*                   /*************************************************/
*                                                               03S0203
*                   IF MPLSESS='1'B&MQESESS='0'B THEN           03S0203
@RF00198 TM    MPLSESS(MPLPTR),B'00000100'                      03S0203
         BNO   @RF00203                                         03S0203
         TM    MQESESS(MQEPTR),B'00000100'                      03S0203
         BNZ   @RF00203                                         03S0203
*                     DO;           /* YES                           */
*                       MQESESS='1'B;/* INDICATE TERMINAL IS    03S0205
*                                      MONITORING SESSIONS           */
         OI    MQESESS(MQEPTR),B'00000100'                      03S0205
*                       BASESCT=BASESCT+ONE;/* INCREMENT COUNT OF
*                                      CONSOLES AND TERMINALS   03S0206
*                                      MONITORING SESSIONS           */
         LA    @03,1                                            03S0206
         AH    @03,BASESCT(,BASEPTR)                            03S0206
         STH   @03,BASESCT(,BASEPTR)                            03S0206
*                     END;                                      03S0207
*                 END;                                          03S0208
@RF00203 DS    0H                                               03S0209
*               IF MPLSTOP='1'B THEN/* STOP MONITOR REQUEST?         */
@RF00191 TM    MPLSTOP(MPLPTR),B'00010000'                      03S0209
         BNO   @RF00209                                         03S0209
*                 DO;               /* YES                           */
*                                                               03S0210
*                   /*************************************************/
*                   /*                                               */
*                   /* IS THERE A REQUEST FOR STOP MONITOR JOBNAMES  */
