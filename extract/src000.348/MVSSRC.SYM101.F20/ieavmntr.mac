@RF00216 TM    MPLSESS(MPLPTR),B'00000100'                      03S0221
         BNO   @RF00221                                         03S0221
         TM    MQESESS(MQEPTR),B'00000100'                      03S0221
         BNO   @RF00221                                         03S0221
*                     DO;                                       03S0222
*                       MQESESS='0'B;/* INDICATE TERMINAL IS NO 03S0223
