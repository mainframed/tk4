@RF00193 TM    MPLSTAT(MPLPTR),B'01000000'                      03S0198
         BNO   @RF00198                                         03S0198
         TM    MQESTAT(MQEPTR),B'01000000'                      03S0198
         BNZ   @RF00198                                         03S0198
*                     DO;           /* YES                           */
*                       MQESTAT='1'B;/* INDICATE TERMINAL IS    03S0200
*                                      MONITORING STATUS             */
         OI    MQESTAT(MQEPTR),B'01000000'                      03S0200
*                       BASTCNT=BASTCNT+ONE;/* INCREMENT COUNT OF
*                                      CONSOLES AND TERMINALS   03S0201
*                                      MONITORING STATUS             */
         LA    @03,1                                            03S0201
         AH    @03,BASTCNT(,BASEPTR)                            03S0201
         STH   @03,BASTCNT(,BASEPTR)                            03S0201
*                     END;                                      03S0202
*                                                               03S0202
*                   /*************************************************/
*                   /*                                               */
*                   /* IS THERE A REQUEST FOR MONITOR SESSIONS AND   */
