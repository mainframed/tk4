         BAL   @14,POSTPUTR                                     04S0233
*                         END;                                  04S0234
*                       ELSE        /* MQE QUEUE NOT EMPTY           */
*                         IF UCMMQEND=MQEPTR THEN/* WAS THIS THE LAST
*                                      MQE                           */
         B     @RC00229                                         04S0235
@RF00229 L     @12,LCVTPTR                                      04S0235
         L     @12,UCMBASPT(,@12)                               04S0235
         C     MQEPTR,UCMMQEND(,@12)                            04S0235
         BNE   @RF00235                                         04S0235
*                           UCMMQEND=BACKPTR;/* YES, PTR TO LAST MQE
