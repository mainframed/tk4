*                   IF MPLJBNM='1'B&MQEJBNM='0'B THEN           03S0193
         TM    MPLJBNM(MPLPTR),B'10000000'                      03S0193
         BNO   @RF00193                                         03S0193
         TM    MQEJBNM(MQEPTR),B'10000000'                      03S0193
         BNZ   @RF00193                                         03S0193
*                     DO;           /* YES                           */
*                       MQEJBNM='1'B;/* INDICATE TERMINAL IS    03S0195
*                                      MONITORING JOBNAMES           */
         OI    MQEJBNM(MQEPTR),B'10000000'                      03S0195
*                       BAJNCNT=BAJNCNT+ONE;/* INCREMENT COUNT OF
