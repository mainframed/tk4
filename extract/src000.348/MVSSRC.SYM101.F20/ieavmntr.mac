*                                                               04S0228
*                       BACKPTR->MQELINK=MQEPTR->MQELINK;/* DECHAIN
*                                      MQE FROM MQE QUEUE            */
         L     @12,MQELINK(,MQEPTR)                             04S0228
         ST    @12,MQELINK(,BACKPTR)                            04S0228
*                       IF UCMMQPTR=ZERO THEN/* IS THE MQE QUEUE
*                                      EMPTY NOW?                    */
         SLR   @12,@12                                          04S0229
