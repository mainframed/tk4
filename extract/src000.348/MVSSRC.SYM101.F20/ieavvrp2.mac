*                                                                  0224
*           IF UCMRQNR<UCMRQLM THEN /* ORE COUNT LESS THEN MAX ?     */
         L     @08,CVTPTR                                          0224
         L     @08,UCMBASPT(,@08)                                  0224
         LH    @07,UCMRQNR(,@08)                                   0224
         SLR   @05,@05                                             0224
         IC    @05,UCMRQLM(,@08)                                   0224
         CR    @07,@05                                             0224
         BNL   @RF00224                                            0224
*             DO;                   /* SCAN WWB CHAIN                */
