         OI    WWBPOSTD(WWBPTR),B'10000000'                        0232
*                     WWBPTR=ADDR(UCMOECBH);/* GET OUT OF LOOP       */
         L     @08,CVTPTR                                          0233
         L     WWBPTR,UCMBASPT(,@08)                               0233
         LA    WWBPTR,UCMOECBH(,WWBPTR)                            0233
*                   END;            /* IF POST BIT IS ON             */
