*               SETS(R14,R15);      /*                       @ZA02375*/
         FREEMAIN R,LV=(R0),A=(R1),BRANCH=YES
*           CONTSW=NO;              /* DONT CONTINUE,BUILD EXTENT  0346
*                                      FAILED                @ZA02375*/
         MVI   CONTSW,X'00'                                        0346
