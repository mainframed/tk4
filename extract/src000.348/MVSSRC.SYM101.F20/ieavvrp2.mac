         TITLE 'IEAVVRP2 - REPLY PROCESSOR - STAGE 2 -                 *
                        '
*                                     /*SAVE SRB POINTER            */
IEAVVRP2 CSECT                        ENTRY POINT OF MODULE
        USING *,15                    ADDRESSABILITY
        MODID BR=YES                  MODULE ID
        LR    SRBPTR,ZEROREG          SAVE SRB ADDRESS
        LR    R8,RTNREG               SAVE RETURN REGISTER
        LR    R8,RTNREG               SAVE RETURN REGISTER
        DROP  15
        BALR  BASREG,ZEROREG          MODULE ADDRESSABILITY
        DS    0H
        USING *,12                    ADDRESSABILITY
*   ERRCDE=BASREG;                  /* SAVE BASE REGISTER            */
         LR    ERRCDE,BASREG                                       0091
*   GEN REFS(PSALITA,FLC);                                         0092
*                                     /*ISSUE SETLOCKS              */
        SETLOCK  OBTAIN,TYPE=LOCAL,MODE=UNCOND,                       XX
               RELATED=(ORE,IEAVVRP2(CLEANUP))
        SETLOCK  OBTAIN,TYPE=CMS,MODE=UNCOND,                         XX
               RELATED=(ORE,IEAVVRP2(CLEANUP))
        LR    BASREG,ERRCDE           RESTORE BASE REGISTER
        L     R4,SRBPTCB(,SRBPTR)     SET TCB ADDRESS
        L     R2,@SIZDATD             SIZE OF GETMAIN
        L     R7,SRBASCB(,SRBPTR)     SET ASCB ADDRESS
        SR    R1,R1                   GETMAIN INTERFACE
        LR    BASREG,ERRCDE           RESTORE BASE REGISTER
        L     R4,SRBPTCB(,SRBPTR)     SET TCB ADDRESS
        L     R2,@SIZDATD             SIZE OF GETMAIN
        L     R7,SRBASCB(,SRBPTR)     SET ASCB ADDRESS
        SR    R1,R1                   GETMAIN INTERFACE
        GETMAIN RU,LV=(R2),SP=229,BRANCH=YES                   YM04721
        LR    R9,R1                   SET DATA REGISTER
        USING @DATD,R9                ADDRESSABILITY FOR DATA AREA
        LR    RTNREG,R8               RESTORE RETURN REGISTER
        LR    RTNREG,R8               RESTORE RETURN REGISTER
*   SAVREG=RTNREG;                  /* SAVE RETURN ADDRESS           */
         ST    RTNREG,SAVREG                                       0093
*   RECEXADD=UCMFRRAD;              /* SET ADDR OF RECOVERY EXIT     */
         L     @07,CVTPTR                                          0094
         L     @07,UCMBASPT(,@07)                                  0094
         L     @07,UCMFRRAD(,@07)                                  0094
         ST    @07,RECEXADD                                        0094
*   GEN REFS(PSACSTK,FRRSCURR,FRRSLAST,PSALSFCC,FRRSPARM,FRRSENTR, 0095
*       FRRSELEN,FRRSFRRA,RECEXADD);                               0095
*                                                     /*ISSUE FRR   */
        SETFRR  A,FRRAD=RECEXADD,WRKREGS=(2,4),PARMAD=(2)
*   PARMPTR=R2;                     /* BASE FOR PARM LIST            */
         LR    PARMPTR,R2                                          0096
*   PARMID='VRP2';                  /* SET MODULE ID                 */
         MVC   PARMID(4,PARMPTR),@CC01901                          0097
*   ERRCDE=0;                       /* INIT ERROR CODE TO ZERO       */
         SLR   ERRCDE,ERRCDE                                       0098
*   UCMMCSPT=UCMPRFXP;              /* ADDR OF MCS PREFIX            */
         L     @07,CVTPTR                                          0099
         L     @07,UCMBASPT(,@07)                                  0099
         LR    @01,@07                                             0099
         AL    @01,@CF01936                                        0099
         L     @15,UCMPRFXP(,@01)                                  0099
         ST    @15,UCMMCSPT                                        0099
*   UCMEXTPT=UCM2PTR;               /* ADDR OF UCM EXTENSION         */
         LR    @01,@07                                             0100
         AL    @01,@CF01937                                        0100
         L     UCMEXTPT,UCM2PTR(,@01)                              0100
*   RFY                                                            0101
*    (R8) UNRSTD;                   /* UNRESTRICT REG3       @ZM30120*/
*/********************************************************************/
*/*                          SCANID    SEGMENT                       */
*/* THE FOLLOWING SEGMENT SCANS THE ORE/WQE CHAIN IN AN ATTEMPT TO   */
*/* FIND A REPLY ID MATCH. THE REPLY ID,WHICH WAS PASSED AS A        */
*/* PARAMETER BY THE FIRST STAGE OF REPLY (IEAVVRP1), IS COMPARED    */
*/* AGAINST THE ORE IDS. IF A MATCH IS FOUND, THE ORE IS CHECKED TO  */
*/* DETERMINE IF A DOM HAS BEEN ISSUED. IF THE ORE HAS BEEN DOMED OR */
*/* IF THERE ARE NO MORE ORES ON THE CHAIN, THE TEMPORARY BUFFER WILL*/
*/* BE FREED AND REPLY PROCESSING TERMINATED.                        */
*/*                                                                  */
*/* IF THE REPLY IDS MATCH AND THE ORE HAS NOT BEEN DOMED, THE       */
*/* MESSAGE ID IN THE ASSOCIATED WQE IS COMPARED AGAINST THE MESSAGE */
*/* ID PASSED AS A PARAMETER BY THE FIRST STAGE OF REPLY. IF THE IDS */
*/* MATCH, REPLY PROCESSING CONTINUES NORMALLY.IF THEY DO NOT MATCH, */
*/* THE TEMPORARY BUFFER IS FREED AND PROCESSING ENDS.               */
*/********************************************************************/
*                                                                  0102
*   RFY                                                            0102
*    (BYPASSW) RSTD;                /* RESTRICT SWITCH REGISTER      */
*   OREPTR=UCMRPYQ;                 /* GET ADDR OF FIRST ORE         */
         L     @08,CVTPTR                                          0103
         L     @08,UCMBASPT(,@08)                                  0103
         L     OREPTR,UCMRPYQ(,@08)                                0103
*   NXTORE=ADDR(UCMRPYQ);           /* SAVE ADDR OF ORE              */
         LA    @08,UCMRPYQ(,@08)                                   0104
         ST    @08,NXTORE                                          0104
*   BYPASSW=0;                      /* TURN BYPASS SWITCH OFF        */
*                                                                  0105
         SLR   BYPASSW,BYPASSW                                     0105
*   /*****************************************************************/
*   /*                                                               */
*   /* SCAN ORE CHAIN FOR REPLY ID MATCH.                            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0106
*   DO WHILE(BYPASSW=0);            /* SCAN FOR ID MATCH             */
*                                                                  0106
         B     @DE00106                                            0106
@DL00106 DS    0H                                                  0107
*     /***************************************************************/
*     /*                                                             */
*     /* CHECK TO DETERMINE IF THERE ARE ANY MORE ORES ON THE CHAIN. */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0107
*     IF OREPTR^=0 THEN             /* ANY ORES ?                    */
         LTR   OREPTR,OREPTR                                       0107
         BZ    @RF00107                                            0107
*       DO;                         /* IF YES,                       */
*                                                                  0108
*         /***********************************************************/
*         /*                                                         */
*         /* CHECK REPLY ID WITH ORE ID                              */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0109
*         IF RPYID=OREID THEN       /* IDS MATCH ?                   */
         L     @08,SRBPARM(,SRBPTR)                                0109
         CLC   RPYID(2,@08),OREID(OREPTR)                          0109
         BNE   @RF00109                                            0109
*           DO;                     /* IF YES,                       */
*                                                                  0110
*             /*******************************************************/
*             /*                                                     */
*             /* HAS THIS ORE BEEN MARKED TO BE DELETED ?            */
*             /*                                                     */
*             /*******************************************************/
*                                                                  0111
*             IF OREBUFC^='1'B THEN /* DELETE ORE ?                  */
         TM    OREBUFC(OREPTR),B'00100000'                         0111
         BO    @RF00111                                            0111
*               DO;                 /* IF NO,                        */
*                 WQEPTR=OREWQE;    /* GET ADDR OF WQE               */
*                                                                  0113
         L     @03,OREWQE(,OREPTR)                                 0113
         ST    @03,WQEPTR                                          0113
*                 /***************************************************/
*                 /*                                                 */
*                 /* CHECK MESSAGE ID PASSED BY REPLY 1 WITH MESSAGE */
*                 /* ID IN WQE.                                      */
*                 /*                                                 */
*                 /***************************************************/
*                                                                  0114
*                 IF WQESEQN^=RPYSEQ THEN/* MESSAGE IDS MATCH ?      */
         CLC   WQESEQN(3,@03),RPYSEQ(@08)                          0114
         BE    @RF00114                                            0114
*                   DO;             /* IF NO,                        */
*                     ERRCDE=1;     /* TURN ERROR INDICATOR ON       */
         LA    ERRCDE,1                                            0116
*                   END;            /* END IF MSG IDS DON'T MATCH    */
*                 BYPASSW=1;        /* TURN BYPASS SWITCH ON         */
@RF00114 LA    BYPASSW,1                                           0118
*               END;                /* END IF DOM BIT IS NOT ON      */
*             ELSE                  /* IF THE DOM BIT IS ON          */
*               DO;                 /* END REPLY 2 PROCESSING        */
*                                                                  0120
         B     @RC00111                                            0120
@RF00111 DS    0H                                                  0121
*                 /***************************************************/
*                 /*                                                 */
*                 /* TEST TO SEE IF TEMPORARY BUFFER EXIST           */
*                 /*                                                 */
*                 /***************************************************/
*                                                                  0121
*                 IF OREOPBUF^=0 THEN/* TEMPORARY BUFFER ?           */
         SLR   @08,@08                                             0121
         L     @05,OREOPBUF(,OREPTR)                               0121
         CR    @05,@08                                             0121
         BE    @RF00121                                            0121
*                   DO;             /* FREE TEMP BUFFER              */
*/********************************************************************/
*/*                FREE  BUFFER  SEGMENT                             */
*/*                                                                  */
*/* THE FOLLOWING SEGMENT FREES THE TEMPORARY BUFFER ASSOCIATED WITH */
*/* THE REPLY WHICH WAS CREATED BY THE FIRST STAGE OF REPLY          */
*/* PROCESSING. THE TEMPORARY BUFFER WHICH IS POINTED TO OUT OF THE  */
*/* ORE CONTAINS THE ACTUAL REPLY TEXT AS SPECIFIED BY THE USER WHO  */
*/* IS RESPONDING TO THE ORIGINAL WTOR MESSAGE.                      */
*/*                                                                  */
*/* INPUT:                                                           */
*/*      OREOPBUF - POINTER TO THE TEMPORARY BUFFER                  */
*/*                                                                  */
*/********************************************************************/
*                                                                  0123
*                     RFY                                          0123
*                      (R3,                                        0123
*                       R7) RSTD;   /* RESTRICT REG 3        @ZM30120*/
*                     R4=ORETCB;    /* SET TCB ADDRESS               */
         L     R4,ORETCB(,OREPTR)                                  0124
*                     ZEROREG=TEMPLNT+2;/* LENGTH OF FREEMAIN        */
         L     @15,OREOPBUF(,OREPTR)                               0125
         LA    ZEROREG,2                                           0125
         AH    ZEROREG,TEMPLNT(,@15)                               0125
*                     SAVREG2=R7;   /* SAVE CONTENT OF REG 7         */
         LR    SAVREG2,R7                                          0126
*                     R7=ASCBTAB(OREASID);/* SET ADDRESS OF ASCB     */
         LH    @05,OREASID(,OREPTR)                                0127
         SLA   @05,2                                               0127
         L     @01,CVTPTR                                          0127
         L     @01,ASVTPTR(,@01)                                   0127
         L     R7,ASCBTAB+524(@05,@01)                             0127
*                     R1=OREOPBUF;  /* ADDR OF TEMPORARY BUFFER      */
         LR    R1,@15                                              0128
*                     OREOPBUF=0;   /* ZERO POINTER TO TEMP BUFFER   */
         ST    @08,OREOPBUF(,OREPTR)                               0129
*                     GEN REFS(CVTCRMN);                           0130
*                                     /*ISSUE FREEMAIN              */
        FREEMAIN  RU,LV=(0),A=(1),SP=231,BRANCH=YES
*                     RFY                                          0131
*                      (R3,                                        0131
*                       R7) UNRSTD; /* UNRESTRICT REG 3      @ZM30120*/
*                   END;            /* IF NO TEMP BUFFER             */
*                 ERRCDE=1;         /* TURN ERROR INDICATOR ON       */
@RF00121 LA    @08,1                                               0133
         LR    ERRCDE,@08                                          0133
*                 BYPASSW=1;        /* TURN BYPASS SWITCH ON         */
         LR    BYPASSW,@08                                         0134
*               END;                /* END IF DOM BIT IS ON          */
*           END;                    /* END IF REPLY IDS MATCH        */
*         ELSE                      /* IF REPLY IDS DO NOT MATCH     */
*           DO;                     /* CHECK NEXT ORE                */
         B     @RC00109                                            0137
@RF00109 DS    0H                                                  0138
*             NXTORE=OREPTR;        /* SAVE CURRENT ORE POINTER      */
         ST    OREPTR,NXTORE                                       0138
*             OREPTR=ORELKP;        /* GET NEXT ORE POINTER          */
         L     OREPTR,ORELKP(,OREPTR)                              0139
*           END;                    /* CHECK FOR ID MATCH            */
*       END;                        /* END CHECK IF ORE EXIST        */
*     ELSE                          /* IF NO MORE ORES ON CHAIN      */
*       DO;                         /* END PROCESSING                */
         B     @RC00107                                            0142
@RF00107 DS    0H                                                  0143
*         ERRCDE=1;                 /* TURN ERROR INDICATOR ON       */
         LA    @08,1                                               0143
         LR    ERRCDE,@08                                          0143
*         BYPASSW=1;                /* TURN BYPASS SWITCH ON         */
         LR    BYPASSW,@08                                         0144
*       END;                        /* END IF NO ORES                */
*   END;                            /* END ID TEST                   */
@RC00107 DS    0H                                                  0146
@DE00106 SLR   @08,@08                                             0146
         CR    BYPASSW,@08                                         0146
         BE    @DL00106                                            0146
*   RFY                                                            0147
*    (BYPASSW) UNRSTD;              /* UNRESTRICT REG 7              */
*                                                                  0147
*   /*****************************************************************/
*   /*                                                               */
*   /* TEST TO DETERMINE IF AN ERROR HAS BEEN DETECTED WHILE SCANNING*/
*   /* FOR AN ID MATCH.                                              */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0148
*   IF ERRCDE=0 THEN                /* ERROR INDICATOR SET           */
         CR    ERRCDE,@08                                          0148
         BNE   @RF00148                                            0148
*     DO;                           /* IF NO,                        */
*                                                                  0149
*       /*************************************************************/
*       /*                                                           */
*       /* CHECK FOR USER PROTECK KEY ZERO BEFORE VALIDITY CHECKING  */
*       /* USER'S BUFFER AND ECB.                                    */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0150
*       IF OREKEY0^='1'B THEN       /* KEY0 USER ?                   */
         TM    OREKEY0(OREPTR),B'01000000'                         0150
         BO    @RF00150                                            0150
*         DO;                       /* IF NO,                        */
*/********************************************************************/
*/*                VALIDITY  CHECK  SEGMENT                          */
*/*                                                                  */
*/* THE FOLLOWING SEGMENT CHECKS THE VALIDITY OF THE USER'S BUFFER   */
*/* AND ECB ADDRESSES. IF THEY ARE NOT VALID, THE USER WILL BE       */
*/* ABENDED WITH A 'E23' ABEND CODE.                                 */
*/*                                                                  */
*/* INPUT:                                                           */
*/*      CVT0VL01 - VALIDITY CHECK ENTRY POINT                       */
*/*      ORERPYA  - ADDRESS OF REPLY BUFFER                          */
*/*      ORELNTH  - MAXIMUM LENGTH OF REPLY                          */
*/*      ORETCB   - POINTER TO TCB                                   */
*/*      OREECB   - POINTER TO REQUESTOR'S REPLY ECB                 */
*/*                                                                  */
*/* OUTPUT:                                                          */
*/*   NORMAL - VALID ADDRESSES (CONTINUE PROCESSING)                 */
*/*                                                                  */
*/*   ERROR  - ABEND CODE 'E23', ERROR INDICATOR SET                 */
*/*                                                                  */
*/********************************************************************/
*                                                                  0152
*           RFY                                                    0152
*            (BYPASSW) RSTD;        /* RESTRICT REG 7                */
*           BYPASSW=OREPTR;         /* SAVE ORE POINTER              */
         LR    BYPASSW,OREPTR                                      0153
*           R4=ORETCB;              /* USER'S TCB                    */
         L     R4,ORETCB(,OREPTR)                                  0154
*           R15=CVT0VL00;           /* ENTRY PTR TO VALIDITY CHECK   */
         L     @08,CVTPTR                                          0155
         L     R15,CVT0VL00(,@08)                                  0155
*           R1=ORERPYA;             /* START OF USER BUFFER          */
         L     @08,ORERPYA-1(,OREPTR)                              0156
         LA    @08,0(,@08)                                         0156
         LR    R1,@08                                              0156
*           R2=ORERPYA+ORELNTH-1;   /* SET END OF USER BUFFER      0157
*                                                            @ZA02399*/
         SLR   @03,@03                                             0157
         IC    @03,ORELNTH(,OREPTR)                                0157
         ALR   @08,@03                                             0157
         BCTR  @08,0                                               0157
         LR    R2,@08                                              0157
*           GEN;                                                   0158
*                                                                  0158
*                                    /* VALIDITY CHECK USER BUFFER  */
        BALR  RTNREG,R15              BRANCH TO VALIDITY CHECK
*           /*********************************************************/
*           /*                                                       */
*           /* SINCE VALIDITY CHECK SETS THE CONDITION CODE, THE     */
*           /* FOLLOWING BRANCH WILL BE TAKEN IF THE CONDITION CODE  */
*           /* IS NON-ZERO. THEREFORE, THIS SEGMENT NON-STRUCTURED.  */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0159
*           R2=BYPASSW;             /* RESTORE ORE POINTER           */
         LR    R2,BYPASSW                                          0159
*           BC(7,ERROR);            /* BR IF NON-ZERO RTN CODE       */
         BC    7,ERROR                                             0160
*           R15=CVT0VL01;           /* ENTRY PTR TO VALIDITY CHECK   */
         L     @08,CVTPTR                                          0161
         L     R15,CVT0VL01(,@08)                                  0161
*           R1=OREECB;              /* SET ADDR OF ECB               */
         L     R1,OREECB(,OREPTR)                                  0162
*           R2=0;                   /* ZERO LAST ADDR POINTER        */
         SLR   R2,R2                                               0163
*           GEN;                                                   0164
*                                                                  0164
*                                     /*VALIDITY CHECK USER ECB     */
        BALR  RTNREG,R15              BRANCH TO VALIDITY CHECK
*           /*********************************************************/
*           /*                                                       */
*           /* SINCE VALIDITY CHECK SETS THE CONDITION CODE, THE     */
*           /* FOLLOWING BRANCH WILL BE TAKEN IF THE CONDITION CODE  */
*           /* IS ZERO. THEREFORE, THIS SEGMENT NON-STRUCTURED.      */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0165
*           R2=BYPASSW;             /* RESTORE ORE POINTER           */
         LR    R2,BYPASSW                                          0165
*           BC(8,ENDSEG);           /* BRANCH IF ZERO RTN CODE       */
         BC    8,ENDSEG                                            0166
*ERROR:                             /* SET UP TO ABEND USER          */
*/********************************************************************/
*/*                FREE  BUFFER  SEGMENT                             */
*/*                                                                  */
*/* THE FOLLOWING SEGMENT FREES THE TEMPORARY BUFFER ASSOCIATED WITH */
*/* THE REPLY WHICH WAS CREATED BY THE FIRST STAGE OF REPLY          */
*/* PROCESSING. THE TEMPORARY BUFFER WHICH IS POINTED TO OUT OF THE  */
*/* ORE CONTAINS THE ACTUAL REPLY TEXT AS SPECIFIED BY THE USER WHO  */
*/* IS RESPONDING TO THE ORIGINAL WTOR MESSAGE.                      */
*/*                                                                  */
*/* INPUT:                                                           */
*/*      OREOPBUF - POINTER TO THE TEMPORARY BUFFER                  */
*/*                                                                  */
*/********************************************************************/
*                                                                  0167
*           RFY                                                    0167
*            (R3,                                                  0167
*             R7) RSTD;             /* RESTRICT REG 3        @ZM30120*/
ERROR    DS    0H                                                  0168
*           R4=ORETCB;              /* SET TCB ADDRESS               */
         L     R4,ORETCB(,OREPTR)                                  0168
*           ZEROREG=TEMPLNT+2;      /* LENGTH OF FREEMAIN            */
         L     @08,OREOPBUF(,OREPTR)                               0169
         LA    ZEROREG,2                                           0169
         AH    ZEROREG,TEMPLNT(,@08)                               0169
*           SAVREG2=R7;             /* SAVE CONTENT OF REG 7         */
         LR    SAVREG2,R7                                          0170
*           R7=ASCBTAB(OREASID);    /* SET ADDRESS OF ASCB           */
         LH    @15,OREASID(,OREPTR)                                0171
         SLA   @15,2                                               0171
         L     @01,CVTPTR                                          0171
         L     @01,ASVTPTR(,@01)                                   0171
         L     R7,ASCBTAB+524(@15,@01)                             0171
*           R1=OREOPBUF;            /* ADDR OF TEMPORARY BUFFER      */
         LR    R1,@08                                              0172
*           OREOPBUF=0;             /* ZERO POINTER TO TEMP BUFFER   */
         SLR   @08,@08                                             0173
         ST    @08,OREOPBUF(,OREPTR)                               0173
*           GEN REFS(CVTCRMN);                                     0174
*                                     /*ISSUE FREEMAIN              */
        FREEMAIN  RU,LV=(0),A=(1),SP=231,BRANCH=YES
*           RFY                                                    0175
*            (R3,                                                  0175
*             R7) UNRSTD;           /* UNRESTRICT REG 3      @ZM30120*/
*           GEN REFS(CVTBTERM);                                    0176
*                                     /*ABEND USER                  */
        CALLRTM  TYPE=ABTERM,COMPCOD=X'E23',TCB=(4)
*           BYPASSW=SAVREG2;        /* RESTORE SAVED ORE POINTER     */
         LR    BYPASSW,SAVREG2                                     0177
*           ERRCDE=1;               /* TURN ERROR INDICATOR ON       */
         LA    ERRCDE,1                                            0178
*ENDSEG:                                                           0179
*           OREPTR=BYPASSW;         /* RESTORE ORE POINTER           */
ENDSEG   LR    OREPTR,BYPASSW                                      0179
*           RFY                                                    0180
*            (BYPASSW) UNRSTD;      /* UNRESTRICT REG 7              */
*         END;                      /* END VALIDITY CHECK            */
*                                                                  0181
*       /*************************************************************/
*       /*                                                           */
*       /* TEST TO DETERMINE IF AN ERROR HAS BEEN DETECTED BY THE    */
*       /* VALIDITY CHECK SEGMENT.                                   */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0182
*       IF ERRCDE=0 THEN            /* ERROR INDICATOR SET ?         */
@RF00150 SLR   @08,@08                                             0182
         CR    ERRCDE,@08                                          0182
         BNE   @RF00182                                            0182
*         DO;                       /* IF NO,                        */
*/********************************************************************/
*/*                MOVE AND POST   SEGMENT                           */
*/*                                                                  */
*/* THIS SEGMENT MOVES THE REPLY TEXT FROM THE TEMPORARY BUFFER TO   */
*/* THE USER SPECIFIED BUFFER AREA. AFTER THE MOVE OPERATION, THE    */
*/* USER'S ECB IS POSTED.                                            */
*/*                                                                  */
*/* INPUT:                                                           */
*/*      OREECB - ADDRESS OF USER'S ECB                              */
*/*      ORERPYA- ADDRESS OF USER'S REPLY BUFFER                     */
*/*      CVT0PT01-POST ENTRY POINT                                   */
*/*                                                                  */
*/* OUTPUT:                                                          */
*/*      POST ISSUED FOR USER'S ECB                                  */
*/*                                                                  */
*/********************************************************************/
*                                                                  0184
*           IF TEMPLNT^=0 THEN      /* TEST FOR NULL REPLT           */
         L     @07,OREOPBUF(,OREPTR)                               0184
         LH    @05,TEMPLNT(,@07)                                   0184
         CR    @05,@08                                             0184
         BE    @RF00184                                            0184
*             DO;                   /* IF NO,                        */
*               USERBUF(1:TEMPLNT)=TEMPTXT(1:TEMPLNT);/* MOVE TEXT   */
         L     @08,ORERPYA-1(,OREPTR)                              0186
         BCTR  @05,0                                               0186
         EX    @05,@SM01938                                        0186
*             END;                  /* END IF REPLY NOT NULL         */
*           RFY                                                    0188
*            (BYPASSW) RSTD;        /* RESTRICT REG 7                */
@RF00184 DS    0H                                                  0189
*           BYPASSW=BASREG;         /* SAVE BASE REGISTER            */
         LR    BYPASSW,BASREG                                      0189
*           R10=0;                  /* POST INTERFACE                */
         SLR   R10,R10                                             0190
*           R11=0;                  /* POST INTERFACE/NORMAL POST    */
         SLR   R11,R11                                             0191
*           R13=0;                  /* POST INTERFACE                */
         SLR   R13,R13                                             0192
*           R11=OREECBA;            /* ADDRESS OF ECB                */
         L     R11,OREECBA-1(,OREPTR)                              0193
         LA    R11,0(,R11)                                         0193
*           R15=CVT0PT01;           /* POST ENTRY POINT              */
         L     @08,CVTPTR                                          0194
         L     R15,CVT0PT01(,@08)                                  0194
*           BASREG=0;               /* POST INTERFACE                */
         SLR   BASREG,BASREG                                       0195
*           GEN;                                                   0196
*                                     /*ISSUE NORMAL POST           */
        BALR  RTNREG,R15              BRANCH TO POST
*           BASREG=BYPASSW;         /* RESTORE BASE REGISTER         */
         LR    BASREG,BYPASSW                                      0197
*           RFY                                                    0198
*            (BYPASSW) UNRSTD;      /* UNRESTRICT REG 7              */
*/********************************************************************/
*/*                AVAILID       SEGMENT                             */
*/*                                                                  */
*/* THE FOLLOWING SEGMENT MARKS THE REPLY ID ASSIGNMENT PATTERN IN   */
*/* THE UCM TO INDICATE THAT THE REPLY ID WHICH HAS BEEN PROCESSED   */
*/* CAN BE REASSIGNED.                                               */
*/*                                                                  */
*/* INPUT:                                                           */
*/*      OREID - REPLY ID                                            */
*/*      UCMRPYI - REPLY ID ASSIGNMENT BIT PATTERN                   */
*/*                                                                  */
*/* OUTPUT:                                                          */
*/*      UCM ASSIGNMENT BIT PATTERN MASKED                           */
*/*                                                                  */
*/********************************************************************/
*                                                                  0199
*           PACK(PAREA,OREID);      /* CONVERT REPLY ID              */
         PACK  PAREA(8),OREID(2,OREPTR)                            0199
*           CVB(CREG,PAREA);        /* CONVERT ID TO BINARY FORM     */
         CVB   CREG,PAREA                                          0200
