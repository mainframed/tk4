*                                                            @ZA02375*/
         LTR   R15,R15                                             0335
         BNZ   @RF00335                                            0335
*     DO;                           /* YES                   @ZA02375*/
*       CPEXTSAV=R1;                /* SAVE ADDR OF CORE     @ZA02375*/
         LR    CPEXTSAV,R1                                         0337
*       R0=UCMWQECP;                /* GET CELL POOL ID      @ZA02375*/
         L     @11,CVTPTR                                          0338
         L     @11,UCMBASPT(,@11)                                  0338
         L     R0,UCMWQECP(,@11)                                   0338
*       R15=LENGTH(WQE);            /* GET SIZE OF CELL NEED @ZA02375*/
         LA    R15,192                                             0339
*       GEN REFS(R0,R1,R2,R3,R13,CVTBLDCP,CVTMAP)/*          @ZA02375*/
*           SETS(R15);                                             0340
*                                                        /* @ZA02375*/
        BLDCPOOL CPID=(R0),SP=231,CSIZE=(R15),CPADDR=(R1),BRANCH=YES, XX
               AUTODEL=YES,POOLSIZ=4,SERIAL=YES               @ZA20159
*       IF R15^=0 THEN              /* WAS CELL POOL BUILT?  @ZA02375*/
         LTR   R15,R15                                             0341
         BZ    @RF00341                                            0341
*         DO;                       /* NO                    @ZA02375*/
*           R0=FRWQEXT;             /* GET SUBPOOL # AND LENGTH    0343
*                                                            @ZA02375*/
         L     R0,FRWQEXT                                          0343
*           R1=CPEXTSAV;            /* GET ADDR OF CORE TO FREE    0344
*                                                            @ZA02375*/
*                                                                  0344
         LR    R1,CPEXTSAV                                         0344
*           /*********************************************************/
*           /*                                                       */
*           /* FREE THE CORE. AN EXTESNION COULD NOT BE BUILT        */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0345
*           GEN(FREEMAIN R,LV=(R0),A=(R1),BRANCH=YES)/*      @ZA02375*/
