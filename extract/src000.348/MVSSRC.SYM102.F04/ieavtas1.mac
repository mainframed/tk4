         TITLE 'IEAVTAS1 - PRE EXIT PROCESSING                         *
                        '
IEAVTAS1 CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'IEAVTAS1  77.327'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @10,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@10                                         0001
RBPRFXLN EQU 64
RBPRFLNA EQU 32
PRBLEN   EQU 136
SIRBLEN  EQU 200
TIRBLEN  EQU 136
IRBLEN   EQU 128
SVRBLEN  EQU 224
*                                                                  0098
*   /*****************************************************************/
*   /*                                                               */
*   /* SEGMENT IEAVTAS1 IS THE TOP TASK RECOVERY SEGMENT. IT INCLUDES*/
*   /* FINDSCB TO LOCATE THE SCB TO BE SCHEDULED, WKUPDAT TO         */
*   /* INITIALIZE RTM2 WORKAREA WITH APPLICABLE DATA AND SDWAINIT TO */
*   /* ACQUIRE AND INITIALIZE THE SDWA(RTCA) FOR THE USER EXIT. IT   */
*   /* ALSO PERFORMS USER I/O OPTIONS (USEROPTS), HANDLES RECURSION  */
*   /* (RECURSE) AND SETS UP THE INTERFACE TO THE USER EXIT ROUTINE  */
*   /* (EXITINTR). WHEN CONTROL RETURNS TO IEAVTAS1 FROM THE USER    */
*   /* EXIT IEAVTAS2 IS CALLED TO PERFORM USER RECOVERY REQUESTS, I.E*/
*   /* RETRY OR CONTINUE WITH TERMINATION.                           */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0099
*   RTM2PTR=INPUTREG;               /* SAVE INPUT PTR TO RTM2      0099
*                                      WORKAREA                      */
         LR    RTM2PTR,INPUTREG                                    0099
*   IF RTM2RETR=ON THEN             /* THIS CALL TO SETUP RBS FOR  0100
*                                      RETRY                 @G33SPHW*/
         TM    RTM2RETR(RTM2PTR),B'01000000'                       0100
         BNO   @RF00100                                            0100
*     DO;                           /* INVOKE TAS3 TO DO ACTUAL SETUP
*                                                            @G33SPHW*/
*       SCBPTR=RTM2SCBC;            /* USE SCB FOUND IN LAST PASS  0102
*                                                            @G33SPHW*/
         L     SCBPTR,RTM2SCBC(,RTM2PTR)                           0102
*       R13=ADDR(RTM2TRSA);         /* SAVEAREA FOR IEAVTAS3 @G33SPHW*/
         LA    R13,RTM2TRSA(,RTM2PTR)                              0103
*       CALL IEAVTAS3;              /* INVOKE IEAVTAS3 FOR SETUP   0104
*                                                            @G33SPHW*/
         L     @15,@CV03262                                        0104
         BALR  @14,@15                                             0104
*     END;                          /* END OF RETRY SETUP    @G33SPHW*/
*   RESPECIFY                                                      0106
*     INPUTREG UNRESTRICTED;                                       0106
@RF00100 DS    0H                                                  0107
*   RTM2WARG=R3INDEX;               /* INDICATE REGISTER FOR RTM2PTR
*                                      DURING RECURSION              */
         MVI   RTM2WARG(RTM2PTR),X'04'                             0107
*TOP:                                                              0108
*   DO WHILE(RTM2RETR=OFF&RTM2SCBC^=ZERO);                         0108
*                                                                  0108
TOP      B     @DE00108                                            0108
@DL00108 DS    0H                                                  0109
*     /***************************************************************/
*     /*                                                             */
*     /* SEGMENT FINDSCB WILL SEARCH THE SCB QUEUE WITHIN THE LIMITS */
*     /* DEFINED IN THE RTM2 WORKAREA FIELDS RTM2SCBO AND RTM2SCBN   */
*     /* UNITIL AN ELIGIBLE SCB ( SCHEDULEABLE USER EXIT) IS FOUND.  */
*     /* THE SCB ADDRESS IS PUT IN RTM2SCBC UNLESS THERE IS NO       */
*     /* ELIGIBLE SCB, IN WHICH CASE RTM2SCBC IS ZEROED              */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0109
*     DO;                           /* BEGIN SEGMENT FINSCB          */
*       IF RTM2SCBC=SCBSTART THEN   /* TEST FOR INITIAL ENTRY        */
         CLC   RTM2SCBC(4,RTM2PTR),@CF00057                        0110
         BNE   @RF00110                                            0110
*         RTM2SCBC=RTM2SCBN;        /* INITIALIZE RTM2SCBC           */
         L     @12,RTM2SCBN(,RTM2PTR)                              0111
         ST    @12,RTM2SCBC(,RTM2PTR)                              0111
*       ELSE                                                       0112
*         RTM2SCBC=RTM2SCBC->SCBCHAIN;/* ACCESS CHAIN FIELD POINTER  */
         B     @RC00110                                            0112
@RF00110 L     @12,RTM2SCBC(,RTM2PTR)                              0112
         L     @12,SCBCHAIN(,@12)                                  0112
         ST    @12,RTM2SCBC(,RTM2PTR)                              0112
*       RTM2TRSW=OFF;               /* INITIALIZE FOR LOOP CONTROL   */
*                                                                  0113
@RC00110 NI    RTM2TRSW(RTM2PTR),B'11101111'                       0113
*       /*************************************************************/
*       /*                                                           */
*       /* SEARCH FOR AN ELIGIBLE SCB                                */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0114
*       DO WHILE(RTM2TRSW=OFF);     /* SWITCH WILL BE TURNED ON WHEN
*                                      AN ELIGIBLE SCB IS FOUND OR 0114
*                                      THE END OF THE SCB Q IS     0114
*                                      ENCOUNTERED                   */
         B     @DE00114                                            0114
@DL00114 DS    0H                                                  0115
*         IF RTM2SCBO^=RTM2SCBC&RTM2SCBC->SCBID=SCBDB THEN/* TEST FOR
*                                      END OF QUEUE AND VALID SCB    */
         L     @12,RTM2SCBC(,RTM2PTR)                              0115
         C     @12,RTM2SCBO(,RTM2PTR)                              0115
         BE    @RF00115                                            0115
         CLI   SCBID(@12),X'DB'                                    0115
         BNE   @RF00115                                            0115
*           IF RTM2SCBC->SCBINUSE^=ON THEN/* IF INUSE BIT IS ON, SCB
*                                      HAS BEEN PREVIOUSLY SCHEDULED */
         TM    SCBINUSE(@12),B'00010000'                           0116
         BO    @RF00116                                            0116
*             IF RTM2TRME=ON THEN   /* TEST IF TERM PROCESSING IS IN
*                                      PROGRESS                      */
         TM    RTM2TRME(RTM2PTR),B'10000000'                       0117
         BNO   @RF00117                                            0117
*               IF RTM2SCBC->SCBTERMI^=ON THEN/* TEST IF TERM SCB    */
         TM    SCBTERMI(@12),B'01000000'                           0118
         BO    @RF00118                                            0118
*                 RTM2SCBC=RTM2SCBC->SCBCHAIN;/* NOT A TERM SCB - SO
*                                      GET NEXT SCB                  */
         L     @12,SCBCHAIN(,@12)                                  0119
         ST    @12,RTM2SCBC(,RTM2PTR)                              0119
*               ELSE                                               0120
*                 RTM2TRSW=ON;      /* ELIGIBLE SCB FOUND            */
         B     @RC00118                                            0120
@RF00118 OI    RTM2TRSW(RTM2PTR),B'00010000'                       0120
*             ELSE                                                 0121
*               IF RTM2STA2=ON THEN /* IF ON THEN ONLY AN ESTAE SCB
*                                      IS ELIGIBLE                   */
         B     @RC00117                                            0121
@RF00117 TM    RTM2STA2(RTM2PTR),B'10000000'                       0121
         BNO   @RF00121                                            0121
*                 IF RTM2SCBC->SCBSTAI=OFF&RTM2SCBC->SCBESTAE=OFF THEN
         L     @12,RTM2SCBC(,RTM2PTR)                              0122
         TM    SCBSTAI(@12),B'10010000'                            0122
         BNZ   @RF00122                                            0122
*                                   /* INELIGIBLE IF THIS IS NOT AN
*                                      ESTAE                         */
*                   RTM2SCBC=RTM2SCBC->SCBCHAIN;/* GET NEXT SCB      */
         L     @12,SCBCHAIN(,@12)                                  0123
         ST    @12,RTM2SCBC(,RTM2PTR)                              0123
*                 ELSE              /* ESTAE TYPE SCB                */
*                   RTM2TRSW=ON;    /* ELIGIBLE SCB FOUND            */
         B     @RC00122                                            0124
@RF00122 OI    RTM2TRSW(RTM2PTR),B'00010000'                       0124
*               ELSE                                               0125
*                 RTM2TRSW=ON;      /* STAE SCB IS ELIGIBLE          */
         B     @RC00121                                            0125
@RF00121 OI    RTM2TRSW(RTM2PTR),B'00010000'                       0125
*           ELSE                    /* IN USE FLAG ON                */
*             RTM2SCBC=RTM2SCBC->SCBCHAIN;/* GET NEXT SCB            */
         B     @RC00116                                            0126
@RF00116 L     @12,RTM2SCBC(,RTM2PTR)                              0126
         L     @12,SCBCHAIN(,@12)                                  0126
         ST    @12,RTM2SCBC(,RTM2PTR)                              0126
*         ELSE                      /* END OF SCB QUEUE              */
*           DO;                                                    0127
         B     @RC00115                                            0127
@RF00115 DS    0H                                                  0128
*             RTM2SCBC=ZERO;        /* INDICATE END OF QUEUE         */
         SLR   @12,@12                                             0128
         ST    @12,RTM2SCBC(,RTM2PTR)                              0128
*             RTM2TRSW=ON;          /* INDICATE END OF SEARCH        */
         OI    RTM2TRSW(RTM2PTR),B'00010000'                       0129
*           END;                                                   0130
*       END;                                                       0131
@RC00115 DS    0H                                                  0131
@DE00114 TM    RTM2TRSW(RTM2PTR),B'00010000'                       0131
         BZ    @DL00114                                            0131
*       SCBPTR=RTM2SCBC;            /* INITIALIZE CURRENT SCB PTR    */
         L     SCBPTR,RTM2SCBC(,RTM2PTR)                           0132
*       IF SCBPTR^=0 THEN                                          0133
         LTR   SCBPTR,SCBPTR                                       0133
         BZ    @RF00133                                            0133
*         IF SCBSTAI=OFF&SCBSTAR=OFF THEN/* TEST FOR (E)STAE SCB     */
         TM    SCBSTAI(SCBPTR),B'11000000'                         0134
         BNZ   @RF00134                                            0134
*           DO;                     /* VALIDITY SCBOWNRA FIELD       */
*             RFY                                                  0136
*               RBSECPTR RSTD;                                     0136
*             RBSECPTR=RTM2VRBC;    /* START RB QUEUE SEARCH WITH  0137
*                                      ABEND SVRB                    */
         L     RBSECPTR,RTM2VRBC(,RTM2PTR)                         0137
*             DO WHILE(RBTCBNXT^=ON&SCBOWNRA^=RBSECPTR);/* SEARCH FOR
*                                      RB MATCH                      */
         B     @DE00138                                            0138
@DL00138 DS    0H                                                  0139
*               RBSECPTR=RBLINKB;   /* UPDATE RB POINTER             */
         L     @12,RBLINKB-1(,RBSECPTR)                            0139
         LA    @12,0(,@12)                                         0139
         LR    RBSECPTR,@12                                        0139
*             END;                  /* END RB SEARCH                 */
@DE00138 TM    RBTCBNXT(RBSECPTR),B'10000000'                      0140
         BO    @DC00138                                            0140
         L     @12,SCBOWNRA-1(,SCBPTR)                             0140
         LA    @12,0(,@12)                                         0140
         CR    RBSECPTR,@12                                        0140
         BNE   @DL00138                                            0140
@DC00138 DS    0H                                                  0141
*             IF SCBOWNRA^=RBSECPTR THEN                           0141
         L     @12,SCBOWNRA-1(,SCBPTR)                             0141
         LA    @12,0(,@12)                                         0141
         CR    RBSECPTR,@12                                        0141
         BE    @RF00141                                            0141
*               RTM2SCBC=ZERO;      /* TEST IF RB MATCH FOUND        */
         SLR   @12,@12                                             0142
         ST    @12,RTM2SCBC(,RTM2PTR)                              0142
*             RFY                                                  0143
*               RBSECPTR UNRSTD;                                   0143
@RF00141 DS    0H                                                  0144
*           END;                                                   0144
*     END;                          /* END FINDSCB                   */
@RF00134 DS    0H                                                  0145
@RF00133 DS    0H                                                  0146
*     IF RTM2SCBC^=ZERO THEN        /* TEST IF ELIGIBLE SCB FOUND    */
         SLR   @12,@12                                             0146
         C     @12,RTM2SCBC(,RTM2PTR)                              0146
         BE    @RF00146                                            0146
*       DO;                         /* ELIGIBLE SCB FOUND            */
*                                                                  0147
*         /***********************************************************/
*         /*                                                         */
*         /* SEGMENT WKUPDAT WILL UPDATE RTM2 WORKAREA FOR THE USER  */
*         /* EXIT. IT FIRST INCLUDES IRBSRCH WHICH WILL DETERMINE IF */
*         /* THERE IS AN IRB PRECEDING THE RB WHOSE (E)STAE EXIT IS  */
*         /* NOW IN CONTROL. FLAG RTM2IRB IS TURNED ON IF THERE IS   */
*         /* ONE. NEXT IT IS DETERMINED IF THE RB ASSOCIATED WITH THE*/
*         /* CURRENT SCB WAS IN CONTROL AT THE TIME OF ERROR. IF IT  */
*         /* WAS NOT IN CONTROL FLAG RTM2NRBE IS SET. THE RB ADDRESS */
*         /* OF THE FAILING PROGRAM IS PUT IN RTM2 WORKAREA IF IT IS */
*         /* SUPERVISOR MODE. IF IT IS PP MODE THE NAME AND ENTRY    */
*         /* POINT OF THE ABENDING PROGRAM IS ENTERED IN RTM2        */
*         /* WORKAREA                                                */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0148
*         ;                         /* BEGIN WKUPDAT                 */
*         IF RTM2IRB=OFF THEN       /* TEST IF INTERVENING IRB     0149
*                                      PREVIOUSLY ENCOUNTERD         */
*                                                                  0149
         TM    RTM2IRB(RTM2PTR),B'00100000'                        0149
         BNZ   @RF00149                                            0149
*           /*********************************************************/
*           /*                                                       */
*           /* SEGMENT IRBSRCH DETERMINES IF THERE IS AN IRB         */
*           /* PRECEDING THE RB WHOSE (E)STA EXIT IS NOW IN CONTROL. */
*           /* IT ACCOMPLISHES THIS BY TESTING EACH RB UP TO THE     */
*           /* CURRENT RB TO SEE IF IT IS AN IRB. FOR (E)STAE EXITS  */
*           /* THE CURRENT RB IS THE RB ADDRESS IN THE CURRENT SCB.  */
*           /* FOR (E)STAI/ESTAR EXITS THE CURRENT RB IS DEFINED AS  */
*           /* THE ONE WITH THE RBTCBNXT FLAG ON.                    */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0150
*           DO;                     /* BEGIN IRBSRCH                 */
*             IF RTM2RBST=ZERO THEN /* RTM2RBST IS THE STARTING    0151
*                                      ADDRESS FOR THE RB QUEUE    0151
*                                      SEARCH. IF IT IS ZERO,        */
         C     @12,RTM2RBST(,RTM2PTR)                              0151
         BNE   @RF00151                                            0151
*               RTM2RBST=RTM2VRBC;  /* INITIALIZE IT WITH THE TOP RB
*                                      ON THE QUEUE RESUME SEARCH  0152
*                                      WITH NEXT RB ON THE QUEUE     */
         L     @12,RTM2VRBC(,RTM2PTR)                              0152
         ST    @12,RTM2RBST(,RTM2PTR)                              0152
*             RESPECIFY                                            0153
*               RBSECPTR RESTRICTED;                               0153
@RF00151 DS    0H                                                  0154
*             RBSECPTR=RTM2RBST;    /* MAKE RTM2RBST CURRENT         */
         L     RBSECPTR,RTM2RBST(,RTM2PTR)                         0154
*             IF(SCBSTAI=ON×SCBSTAR=ON) THEN/* TEST IF (E)STAI OR  0155
*                                      ESTAR                         */
         TM    SCBSTAI(SCBPTR),B'11000000'                         0155
         BZ    @RF00155                                            0155
*               DO WHILE(RTM2IRB^=ON&/* SEARCH FOR AN IRB UNTIL THE  */
*                     RBTCBNXT^=ON);/* END OF THE RB QUEUE IS REACHED*/
         B     @DE00156                                            0156
@DL00156 DS    0H                                                  0157
*                 IF RBFTP='010'B THEN/* DETERMINE IF IRB            */
         TM    RBFTP(RBSECPTR),B'01000000'                         0157
         BNO   @RF00157                                            0157
         TM    RBFTP(RBSECPTR),B'10100000'                         0157
         BNZ   @RF00157                                            0157
*                   RTM2IRB=ON;     /* INDICATE IRB IN RTM2 WORKAREA */
         OI    RTM2IRB(RTM2PTR),B'00100000'                        0158
*                 ELSE                                             0159
*                   RBSECPTR=RBLINKB;/* UPDATE CURRENT RB POINTER    */
         B     @RC00157                                            0159
@RF00157 L     @12,RBLINKB-1(,RBSECPTR)                            0159
         LA    @12,0(,@12)                                         0159
         LR    RBSECPTR,@12                                        0159
*               END;                /* END IRB SEARCH                */
@RC00157 DS    0H                                                  0160
@DE00156 TM    RTM2IRB(RTM2PTR),B'00100000'                        0160
         BO    @DC00156                                            0160
         TM    RBTCBNXT(RBSECPTR),B'10000000'                      0160
         BNO   @DL00156                                            0160
@DC00156 DS    0H                                                  0161
*             ELSE                                                 0161
*               DO WHILE(RTM2IRB^=ON&/* SEARCH FOR AN IRB UNTIL THE  */
*                     SCBOWNRA^=RBSECPTR);/* RB RELATED TO CURRENT 0161
*                                      SCB IS ENCOUNTERED            */
         B     @RC00155                                            0161
@RF00155 B     @DE00161                                            0161
@DL00161 DS    0H                                                  0162
*                 IF RBFTP='010'B THEN/* DETERMINE IF IRB            */
         TM    RBFTP(RBSECPTR),B'01000000'                         0162
         BNO   @RF00162                                            0162
         TM    RBFTP(RBSECPTR),B'10100000'                         0162
         BNZ   @RF00162                                            0162
*                   RTM2IRB=ON;     /* INDICATE IRB IN RTM2 WKAREA   */
         OI    RTM2IRB(RTM2PTR),B'00100000'                        0163
*                 ELSE                                             0164
*                   RBSECPTR=RBLINKB;/* UPDATE CURRENT RB POINTER    */
         B     @RC00162                                            0164
@RF00162 L     @12,RBLINKB-1(,RBSECPTR)                            0164
         LA    @12,0(,@12)                                         0164
         LR    RBSECPTR,@12                                        0164
*               END;                                               0165
@RC00162 DS    0H                                                  0165
@DE00161 TM    RTM2IRB(RTM2PTR),B'00100000'                        0165
         BO    @DC00161                                            0165
         L     @12,SCBOWNRA-1(,SCBPTR)                             0165
         LA    @12,0(,@12)                                         0165
         CR    RBSECPTR,@12                                        0165
         BNE   @DL00161                                            0165
@DC00161 DS    0H                                                  0166
*             RTM2RBST=RBSECPTR;                                   0166
@RC00155 ST    RBSECPTR,RTM2RBST(,RTM2PTR)                         0166
*             RESPECIFY                                            0167
*               RBSECPTR UNRESTRICTED;                             0167
*           END;                                                   0168
*                                                                  0168
*/*  DETERMINE IF CURRENT RB WAS IN CONTROL AT TIME OF ERROR         */
*                                                                  0169
*         RESPECIFY                                                0169
*           RBSECPTR RESTRICTED;                                   0169
@RF00149 DS    0H                                                  0170
*         IF SCBSTAR=OFF&SCBSTAI=OFF THEN/* TEST FOR (E)STAE         */
         TM    SCBSTAR(SCBPTR),B'11000000'                         0170
         BNZ   @RF00170                                            0170
*           IF SCBOWNRA^=RTM2VRBC->RBLINKB THEN                    0171
         L     @12,RTM2VRBC(,RTM2PTR)                              0171
         CLC   SCBOWNRA(3,SCBPTR),RBLINKB(@12)                     0171
         BE    @RF00171                                            0171
*             RTM2NRBE=ON;          /* CURRENT RB NOT IN CONTROL     */
         OI    RTM2NRBE(RTM2PTR),B'01000000'                       0172
*           ELSE                                                   0173
*             ;                     /* TEST FOR ESTAR OR (E)STAI     */
@RF00171 DS    0H                                                  0174
*         ELSE                                                     0174
*           IF SCBSTAR=ON THEN      /* TEST FOR ESTAR                */
         B     @RC00170                                            0174
@RF00170 TM    SCBSTAR(SCBPTR),B'01000000'                         0174
         BNO   @RF00174                                            0174
*             DO;                                                  0175
*               RBSECPTR=RTM2VRBC;  /* INITIALIZE WITH CURRENT RB    */
         L     RBSECPTR,RTM2VRBC(,RTM2PTR)                         0176
*               DO WHILE(RBTCBNXT^=ON);/* BEGIN SEARCH FOR OLDEST RB */
         B     @DE00177                                            0177
@DL00177 DS    0H                                                  0178
*                 RBSECPTR=RBLINKB; /* GET NEXT RB                   */
         L     @12,RBLINKB-1(,RBSECPTR)                            0178
         LA    @12,0(,@12)                                         0178
         LR    RBSECPTR,@12                                        0178
*               END;                /* END OF RB SEARCH              */
@DE00177 TM    RBTCBNXT(RBSECPTR),B'10000000'                      0179
         BNO   @DL00177                                            0179
*               RTM2STAR=RBSECPTR;  /* PUT RB ADDRESS IN RTM2      0180
*                                      WORKAREA                      */
         ST    RBSECPTR,RTM2STAR(,RTM2PTR)                         0180
*               IF RTM2STAR^=RTM2VRBC->RBLINKB THEN                0181
         L     @12,RTM2STAR(,RTM2PTR)                              0181
         L     @11,RTM2VRBC(,RTM2PTR)                              0181
         L     @11,RBLINKB-1(,@11)                                 0181
         LA    @11,0(,@11)                                         0181
         CR    @12,@11                                             0181
         BE    @RF00181                                            0181
*                 RTM2NRBE=ON;                                     0182
         OI    RTM2NRBE(RTM2PTR),B'01000000'                       0182
*             END;                                                 0183
*           ELSE                                                   0184
*             RTM2NRBE=ON;          /* (E)STAI IS TCB RELATED        */
         B     @RC00174                                            0184
@RF00174 OI    RTM2NRBE(RTM2PTR),B'01000000'                       0184
*         IF RTM2BFTL=OFF THEN      /* TEST IF RTM2ABNM PREVIOUSLY 0185
*                                      INITIALIZED                   */
@RC00174 DS    0H                                                  0185
@RC00170 TM    RTM2BFTL(RTM2PTR),B'00001000'                       0185
         BNZ   @RF00185                                            0185
*           DO;                     /* IF NOT BEGIN SEARCH           */
*             RBSECPTR=RTM2VRBC->RBLINKB;                          0187
         L     @12,RTM2VRBC(,RTM2PTR)                              0187
         L     RBSECPTR,RBLINKB-1(,@12)                            0187
         LA    RBSECPTR,0(,RBSECPTR)                               0187
*             IF ADDR(RBOPSW)->PSWPPSUP^=ON THEN/* TEST FOR SUPRVSOR
*                                      MODE                          */
         TM    PSWPPSUP+16(RBSECPTR),B'00000001'                   0188
         BO    @RF00188                                            0188
*               RTM2ABNM(1:4)=RBSECPTR;/* SUPERVISOR MODE - ACCESS 0189
*                                      ABENDING RB ADDRESS           */
         STCM  RBSECPTR,15,RTM2ABNM(RTM2PTR)                       0189
*             ELSE                                                 0190
*                                                                  0190
*               /*****************************************************/
*               /*                                                   */
*               /* SEGMENT CDESRCH IS ENTERED IF THE USER IS IN PP   */
*               /* MODE. IF THE ABENDING RB IS A PRB THE NAME AND    */
*               /* ENTRY POINT OF THE PROGRAM ARE OBTAINED FROM THE  */
*               /* RELATED CDE IF ONE EXISTS. IF THE ABENDING RB IS  */
*               /* AN SIRB NO NAME IS PROVIDED AND THE ENTRY POINT IS*/
*               /* OBTAINED FROM THE FAILING IQE. IF THE ABENDING RB */
*               /* IS AN SVRB THE NAME FIELD IS SET TO ZEROES. IF THE*/
*               /* ABENDING RB IS AN IRB THE CDE CHAIN OF THE JOB    */
*               /* PACK AREA QUEUE AND THE LINK PACK AREA QUEUE ARE  */
*               /* SEARCHED IN AN ATTEMPT TO RELATE AN ENTRY POINT   */
*               /* ADDRESS IN THE IRB TO THE STORAGE DESCRIBED BY A  */
*               /* CDE.                                              */
*               /*                                                   */
*               /*****************************************************/
*                                                                  0190
*               ;                   /* BEGIN CDE SEARCH              */
@RF00188 DS    0H                                                  0191
*             IF RBFTP='000'B THEN  /* TEST FOR PRB                  */
         TM    RBFTP(RBSECPTR),B'11100000'                         0191
         BNZ   @RF00191                                            0191
*               IF RBCDE1^=NULL THEN/* TEST IF A CDE EXISTS FOR PRB  */
         L     @12,RBCDE1-1(,RBSECPTR)                             0192
         LA    @12,0(,@12)                                         0192
         LTR   @12,@12                                             0192
         BZ    @RF00192                                            0192
*                 DO;               /* A CDE EXISTS                  */
*                   RTM2ABNM=RBCDE1->CDNAME;/* ACCESS PROGRAM NAME   */
         MVC   RTM2ABNM(8,RTM2PTR),CDNAME(@12)                     0194
*                   RTM2ABEP=RBCDE1->CDENTPT;/* ACCESS ENTRY POINT   */
         L     @12,CDENTPT(,@12)                                   0195
         ST    @12,RTM2ABEP(,RTM2PTR)                              0195
*                 END;                                             0196
*               ELSE                                               0197
*                 ;                                                0197
@RF00192 DS    0H                                                  0198
*             ELSE                                                 0198
