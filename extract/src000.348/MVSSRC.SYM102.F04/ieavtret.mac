         TITLE '          IEAVTRET RECORDING TASK                      *
                        '
IEAVTRET CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'IEAVTRET  77.334'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @08,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@08                                         0001
         L     @00,@SIZDATD                                        0001
         GETMAIN  R,LV=(0)
         LR    @09,@01                                             0001
         USING @DATD,@09                                           0001
         LM    @00,@01,20(@13)                                     0001
HALFREG EQU   16                   /* BITS TO SHIFT FOR HALFREG      */
ZIP     EQU   0                    /* ZERO USED IN GEND CODE         */
*                                                                  0104
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS SECTION OF CODE IS EXECUTED ONLY ONCE DURING THE LIFE OF */
*   /* THE RECORDING TASK. IT S PURPOSE IS TO INITIALIZE THE SRB USED*/
*   /* BY THE NUCLEUS ROUTINE AND TO ESTABLISH ESTAE PROTECTION FOR  */
*   /* THE RECORDING TASK. IF INITIALIZATUIN IS SUCCESSFUL, THE ECB  */
*   /* PASSED BY IEAVTMSI ON THE ATTACH IS POSTED WITH A CODE OF     */
*   /* ZERO. ANY FAILURE TO INITIALIZE (BAD ESTAE RETURN CODE, ENTRY */
*   /* TO ESTAE EXIT DURING INITIALIZATION, ABEND BEFORE ESTAE       */
*   /* ESTABLISHED) WILL CAUSE THE ECB TO BE POSTED WITH A NON-ZERO  */
*   /* RETURN CODE.                                                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0105
*   PARMLIST=PARMLIST&&PARMLIST;    /* INIT RECOVERY LIST            */
         XC    PARMLIST(72),PARMLIST                               0105
*   RFY                                                            0106
*    (R1,                                                          0106
*     R5) RSTD;                                                    0106
*   R5=R1;                          /* SAVE INPUT ECB ADDR.          */
         LR    R5,R1                                               0107
*   RFY                                                            0108
*    (R1) UNRESTRICTED;                                            0108
*   SVC60LST=MFLESTAE;              /* LIST FORM OF ESTAE            */
         MVC   SVC60LST(16),MFLESTAE                               0109
*   RFY                                                            0110
*    (R1) RSTD;                     /* RESTRICT REG FOR ESTAE PTR    */
*   R1=ADDR(SVC60LST);              /* SET FOR MF = E FORM OF ESTAE  */
         LA    R1,SVC60LST                                         0111
*   GEN(ESTAE PARAM=PARMLIST,MF=(E,(1)));/* GET ESTAE EXIT           */
         ESTAE PARAM=PARMLIST,MF=(E,(1))
*   RFY                                                            0113
*    (R1) UNRSTD;                                                  0113
*                                                                  0113
*   /*****************************************************************/
*   /*                                                               */
*   /* IF ESTAE REQUEST FAILED INFORM IEAVTMSI                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0114
*   GEN SETS(PARMLIST)(STM R8,R7,PARMLIST);/* SAVE FOR EXIT USE      */
         STM R8,R7,PARMLIST
*   RETRYADR=ADDR(BADINIT);         /* SET FIRST RETRY POINT         */
         LA    @14,BADINIT                                         0115
         ST    @14,RETRYADR                                        0115
*   IF R15^=ZERO THEN               /* ESTAE ESTABLISHED...          */
         LTR   R15,R15                                             0116
         BZ    @RF00116                                            0116
*     DO;                           /* NO,ABORT INITIALIZATION       */
*BADINIT:                           /* RETRY LABEL FOR ESTAE         */
*       CVTRTMCT->RTCTRPER=YES;                                    0118
BADINIT  L     @14,CVTPTR                                          0118
         L     @14,CVTRTMCT(,@14)                                  0118
         OI    RTCTRPER(@14),B'01000000'                           0118
*       GEN(POST (R5),4);           /* SHOW INIT FAILURE.            */
         POST (R5),4
*     END;                                                         0120
*   SVC51LST=MFLSDUMP;              /* MOVE SDUMP PARM LIST TO     0121
*                                      AUTOMATIC STORAGE     @ZA13432*/
*                                                                  0121
@RF00116 MVC   SVC51LST(24),MFLSDUMP                               0121
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE THE SRB USED BY NUCLEUS RECORDING ROUTINE          */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0122
*   RFY                                                            0122
*     R2 RSTD;                                                     0122
*   R2=ADDR(RCBSRB);                                               0123
         L     @14,CVTPTR                                          0123
         L     @14,CVTRTMCT(,@14)                                  0123
         L     @14,RTCTRCB(,@14)                                   0123
         LA    R2,RCBSRB(,@14)                                     0123
*   SRBPKF=ZERO;                    /* KEY 0 SRB                     */
         MVI   SRBPKF(R2),X'00'                                    0124
*   SRBRMTR=ZERO;                   /* NO RESOURCE MANAGER.          */
         SLR   @12,@12                                             0125
         ST    @12,SRBRMTR(,R2)                                    0125
*   SRBSAVE=ZERO;                   /* NO SAVE AREA.                 */
         ST    @12,SRBSAVE(,R2)                                    0126
*   SRBCPAFF=ZERO;                  /* RUN ON ANY CPU.               */
         STH   @12,SRBCPAFF(,R2)                                   0127
*   SRBFLNK=ZERO;                   /* NO FORWARD CHAIN PTR.         */
         ST    @12,SRBFLNK(,R2)                                    0128
*   SRBPRIOR=ZERO;                  /* PRIORITY ZERO.                */
         MVI   SRBPRIOR(R2),X'00'                                  0129
*   SRBPARM=R9;                     /* PASS DATA REG TO SRB RTN      */
         ST    R9,SRBPARM(,R2)                                     0130
*   SRBID='SRB ';                   /* EBCDIC ID.                    */
         MVC   SRBID(4,R2),@CC02080                                0131
*   SRBASCB=PSAAOLD;                /* MASTERS ASCB ADDRESS          */
         L     @06,PSAAOLD                                         0132
         ST    @06,SRBASCB(,R2)                                    0132
*   SRBPASID=ZERO;                  /* NO TASK AFFINITY              */
         STH   @12,SRBPASID(,R2)                                   0133
*   SRBPTCB=ZERO;                   /* NO TASK AFFINITY              */
         ST    @12,SRBPTCB(,R2)                                    0134
*   SRBEP=ADDR(RETSRBEP);           /* SRBS ENTRY POINT.             */
         LA    @12,RETSRBEP                                        0135
         ST    @12,SRBEP(,R2)                                      0135
*   RFY                                                            0136
*     R2 UNRSTD;                                                   0136
*   RCBSRBIU=NO;                    /* SHOW SRB CAN BE USED.         */
*                                                                  0137
         NI    RCBSRBIU(@14),B'01111111'                           0137
*   /*****************************************************************/
*   /*                                                               */
*   /* SHOW GOOD INITIALIZATION TO IEAVTMSI                          */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0138
*   GEN( POST (R5),ZIP);            /* CLEAN POST CODE               */
          POST (R5),ZIP
*   RFY                                                            0139
*     R5 UNRSTD;                                                   0139
*   RETINIT=YES;                    /* SHOW TASK IS INITIALIZED IN 0140
*                                      ESTAE PARMLIST                */
         OI    RETINIT,B'10000000'                                 0140
*   RETRYADR=ADDR(AWAKE);           /* NEXT RETRY OUT OF ESTAE       */
         LA    @14,AWAKE                                           0141
         ST    @14,RETRYADR                                        0141
*   GEN SETS(PARMLIST)(STM R8,R7,PARMLIST);/* SAVE FOR EXIT USE      */
         STM R8,R7,PARMLIST
*SLEEP:                             /* ENTER WAIT                    */
*   DO;                             /* WAIT ECB(RTCTRECB)            */
SLEEP    DS    0H                                                  0144
*     RESPECIFY                                                    0144
*      (GPR00P,                                                    0144
*       GPR01P) RESTRICTED;                                        0144
*     GPR00P=1;                     /* LOAD WAIT COUNT               */
         LA    GPR00P,1                                            0145
*     GPR01P=ADDR(RTCTRECB);        /* ADDRESS OF ECB                */
         L     @14,CVTPTR                                          0146
         L     GPR01P,CVTRTMCT(,@14)                               0146
         LA    GPR01P,RTCTRECB(,GPR01P)                            0146
*     SVC(1);                       /* WAIT SVC                      */
         SVC   1                                                   0147
*     RESPECIFY                                                    0148
*      (GPR00P,                                                    0148
*       GPR01P) UNRESTRICTED;                                      0148
*   END;                            /* WAIT ECB(RTCTRECB) WAIT FOR 0149
*                                      RER TO POST                   */
*AWAKE:                             /* FIRST INSTRUCTION AFTER WAIT  */
*   EXTRTN=NO;                      /* SHOW RECORD MAINLINE IN     0150
*                                      CONTROL               @YM02750*/
AWAKE    NI    EXTRTN,B'10111111'                                  0150
*   RETRYADR=ADDR(AWAKE);           /* SET UP CORRECT RETRY ADDRESS
*                                                            @ZA20615*/
         LA    @14,AWAKE                                           0151
         ST    @14,RETRYADR                                        0151
*   RTCTRECB=ZERO;                  /* RESET THE RECORDING ECB       */
         SLR   @14,@14                                             0152
         L     @12,CVTPTR                                          0152
         L     @12,CVTRTMCT(,@12)                                  0152
         ST    @14,RTCTRECB(,@12)                                  0152
*   BUFLENS=BUFLENS&&BUFLENS;       /* ZERO BUFFER LENGTHS   @G17EP5W*/
         XC    BUFLENS(8),BUFLENS                                  0153
*   BUFS=BUFS&&BUFS;                /* ZERO BUFFER ADDRESSES @G17EP5W*/
         XC    BUFS(8),BUFS                                        0154
*   R7=RTCTRCB;                                                    0155
         L     R7,RTCTRCB(,@12)                                    0155
*   RESPECIFY                       /* BASE FOR RCB                  */
*     RCB BASED(R7);                                               0156
*   IF RTCTRTER=YES THEN            /* IF TEMP ERROR SUFFERED        */
         TM    RTCTRTER(@12),B'10000000'                           0157
         BNO   @RF00157                                            0157
*/********************************************************************/
*/*                                                                  */
*/* THE PURPOSE OF THE TEMPERR SEGMENT IS TO CLEANUP & REINITIALIZE  */
*/* THE RCB IF A TEMPORARY ERROR HAS BEEN SUFFERED. IT FIRST CHECKS  */
*/* TO SEE IF BUFFER MANIPULATION IS COMPLETE (RCBACNT = 0).IF SO IT */
*/* WILL POST REQUESTORS WHO MAY BE WAITING AND REINITIALIZE THE RCB */
*/*                                                                  */
*/********************************************************************/
*                                                                  0158
*     DO;                                                          0158
*       IF RCBACNT<=ZERO THEN       /* IF RCB BUFFER AREA IS NOT   0159
*                                      BEING                         */
         CH    @14,RCBACNT(,R7)                                    0159
         BL    @RF00159                                            0159
*         DO;                       /* MANIPULATED BY REQUEST ROUTINE*/
*                                                                  0160
*           /*********************************************************/
*           /*                                                       */
*           /* NOTE - SERIALIZATION OF THE RCB NOT NESESSARY AS AT   */
*           /* THIS POINT NOBODY ELSE IS WORKING WITH IT             */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0161
*           RFY                                                    0161
*            (GPR01P) RSTD;         /*                       @ZA13432*/
*           GPR01P=ADDR(SVC51LST);  /* SDUMP PARAMETER LIST  @ZA13432*/
         LA    GPR01P,SVC51LST                                     0162
*           GEN(SDUMP MF=(E,(1)),HDR='RECORD TEMPORARY ERROR');/*  0163
*                                                            @ZA13432*/
         SDUMP MF=(E,(1)),HDR='RECORD TEMPORARY ERROR'
*           RFY                                                    0164
*            (GPR01P) UNRSTD;       /*                       @ZA13432*/
*           RCBTLNG=LENGTH(RCBBUFRS);/* CONTROL POSSIBLE BAD LOOP    */
         MVC   RCBTLNG(2,R7),@CH02223                              0165
*           TRYPOST=YES;            /* INDICATE TRYING TO POST       */
         OI    TRYPOST,B'00100000'                                 0166
*           RETRYADR=ADDR(SKIPPOST);/* RETRY AFTER POST              */
         LA    @14,SKIPPOST                                        0167
         ST    @14,RETRYADR                                        0167
*           DO WHILE(RCBFLNG<RCBTLNG);/* LOOP THRU BUFFER ENTRIES TO
*                                      POSTWAITING REQUESTORS        */
         B     @DE00168                                            0168
@DL00168 DS    0H                                                  0169
*             OLDEST=RCBFREE+RCBFLNG;/* FIND THE OLDEST BUFFER ENTRY */
         LH    @14,RCBFLNG(,R7)                                    0169
         N     @14,@CF02236                                        0169
         AL    @14,RCBFREE(,R7)                                    0169
         ST    @14,OLDEST                                          0169
*             IF OLDEST>=RCBBUFE THEN/* IF BUFFER WAS WRAPPED        */
         C     @14,RCBBUFE(,R7)                                    0170
         BL    @RF00170                                            0170
*               OLDEST=OLDEST-RCBTLNG;/* UPDATE THE REC POINTER      */
         LH    @12,RCBTLNG(,R7)                                    0171
         N     @12,@CF02236                                        0171
         SLR   @14,@12                                             0171
         ST    @14,OLDEST                                          0171
*             RESPECIFY             /* BASE BUFFER ENTRY MAP ON ADDR */
*              (RCBBUF) BASED(OLDEST);/* OF OLDEST BUFFER ENTRY      */
*                                                                  0172
@RF00170 DS    0H                                                  0173
*             /*******************************************************/
*             /*                                                     */
*             /* THIS SEGMENT WILL POST THE USERS ECB TO INDICATE    */
*             /* THAT THE DATA HAS BEEN WRITTEN. POSTING WILL NOT BE */
*             /* DONE IF IT WAS NOT REQUESTED OR IF THE ASID IS      */
*             /* INVALID.                                            */
*             /*                                                     */
*             /*******************************************************/
*                                                                  0173
*             RFY                                                  0173
*               ASVT BASED(CVTASVT);                               0173
*             IF RCBBPOST=YES&      /* WANT POSTING...               */
*                 RCBBNBFR=YES THEN /* RECORD UNBUFFED..     @YM02617*/
         L     @14,OLDEST                                          0174
         TM    RCBBPOST(@14),B'00001100'                           0174
         BNO   @RF00174                                            0174
*               DO;                 /* -YES.                         */
*                 IF RCBBASID>ZERO& /* VALID NUMERICAL ASID...       */
*                     RCBBASID<=ASVTMAXU&ASVTAVAL(RCBBASID)=NO THEN/*
*                                      ...AND ASID IN USE...         */
         LH    @12,RCBBASID(,@14)                                  0176
         N     @12,@CF02236                                        0176
         LTR   @12,@12                                             0176
         BNP   @RF00176                                            0176
         L     @06,CVTPTR                                          0176
         L     @06,CVTASVT(,@06)                                   0176
         C     @12,ASVTMAXU(,@06)                                  0176
         BH    @RF00176                                            0176
         SLA   @12,2                                               0176
         ALR   @06,@12                                             0176
         TM    ASVTAVAL-4(@06),B'10000000'                         0176
         BNZ   @RF00176                                            0176
*                   DO;             /* YES,POST THE ECB.             */
*                     RFY                                          0178
*                       R5 RSTD;    /* PTR TO THE ECB TO POST TO   0178
*                                      HANDLE WRAP HEADERS   @ZA19199*/
*                     R5=ADDR(RCBBECB);/* ADDR OF ECB TO POST      0179
*                                                            @ZA19199*/
         LA    R5,RCBBECB(,@14)                                    0179
*                     RFY                                          0180
*                       USERECB BASED(ECBADR);/* TO INSURE PROPER  0180
*                                      ADDRESS OF ECB        @ZA19199*/
*                     IF R5>=RCBBUFE THEN/* DOES THE HDR WRAP      0181
*                                                            @ZA19199*/
         C     R5,RCBBUFE(,R7)                                     0181
         BL    @RF00181                                            0181
*                       R5=R5-RCBTLNG;/* YES - RESET POINTER @ZA19199*/
         LH    @14,RCBTLNG(,R7)                                    0182
         N     @14,@CF02236                                        0182
         SLR   R5,@14                                              0182
*                     ELSE                                         0183
*                       ;           /* HDR DOES NOT WRAP     @ZA19199*/
@RF00181 DS    0H                                                  0184
*                     EXTRTN=YES;   /* SHOW IN EXTERNAL ROUTINE FOR
*                                      RECOVERY                      */
         OI    EXTRTN,B'01000000'                                  0184
*                     RETRYADR=ADDR(AFTPSTA);/* PRESET RETRY ADDR    */
         LA    @14,AFTPSTA                                         0185
         ST    @14,RETRYADR                                        0185
*                     GEN SETS(PARMLIST)(STM R8,R7,PARMLIST);/*    0186
*                                                            @YM02750*/
         STM R8,R7,PARMLIST
*                     DO;           /* POST (USERECB,0) ASCB(ASVTENTY
*                                      (RCBBASID)->UASCBADR)       0187
*                                      ERRET(CVTBRET)                */
*                       I002=I002&&I002;/* INITIALIZE LIST           */
         XC    I002(12),I002                                       0188
*                       I00201=ADDR(USERECB);/* ECB ADDRESS          */
         L     @14,ECBADR(,R5)                                     0189
         ST    @14,I00201                                          0189
*                       I00202=ADDR(ASVTENTY(RCBBASID)->UASCBADR);/*
*                                      ASCB ADDRESS                  */
         L     @14,OLDEST                                          0190
         LH    @14,RCBBASID(,@14)                                  0190
         N     @14,@CF02236                                        0190
         SLA   @14,2                                               0190
         L     @12,CVTPTR                                          0190
         L     @06,CVTASVT(,@12)                                   0190
         L     @14,ASVTENTY-4(@14,@06)                             0190
         ST    @14,I00202                                          0190
*                       I00203=ADDR(CVTBRET);/* ERRET ADDRESS        */
         LA    @14,CVTBRET(,@12)                                   0191
         ST    @14,I00203                                          0191
*                       RESPECIFY                                  0192
*                        (GPR00F,                                  0192
*                         GPR01P) RESTRICTED;                      0192
*                       GPR00F=0;   /* POST CODE IN R0               */
         SLR   GPR00F,GPR00F                                       0193
*                       GPR01P=ADDR(I002);/* PARMLIST ADDR           */
         LA    GPR01P,I002                                         0194
*                       GPR01P=GPR01P×'80000000'X;/* SET LIST FORM 0195
*                                      IND                           */
         O     GPR01P,@CF00108                                     0195
*                       SVC(2);     /* ISSUE POST SVC                */
         SVC   2                                                   0196
*                       RESPECIFY                                  0197
*                        (GPR00F,                                  0197
*                         GPR01P) UNRESTRICTED;                    0197
*                     END;          /* POST (USERECB,0) ASCB(ASVTENTY
*                                      (RCBBASID)->UASCBADR)       0198
*                                      ERRET(CVTBRET)                */
*AFTPSTA:                                                          0199
*                     ;             /* RETRY IF POST FAILURE         */
AFTPSTA  DS    0H                                                  0200
*                     RFY                                          0200
*                       USERECB BASED(RCBBECB);/* REBASE BACK      0200
*                                                            @ZA19199*/
*                     RFY                                          0201
*                       R5 UNRSTD;  /* FREE THE ECB POINTER  @ZA19199*/
*                     EXTRTN=NO;    /* BACK TO RECORD                */
         NI    EXTRTN,B'10111111'                                  0202
*                     RETRYADR=ADDR(AWAKE);/* RESET RETRY ADDR       */
         LA    @14,AWAKE                                           0203
         ST    @14,RETRYADR                                        0203
*                   END;                                           0204
*               END;                                               0205
@RF00176 DS    0H                                                  0206
*             RFY                                                  0206
*               ASVT BASED;                                        0206
@RF00174 DS    0H                                                  0207
*             IF RCBBNBFR=NO THEN   /* RECORD BUFFERED...            */
         L     @14,OLDEST                                          0207
         TM    RCBBNBFR(@14),B'00001000'                           0207
         BNZ   @RF00207                                            0207
*               RCBFLNG=RCBFLNG+    /* YES - RELEASE IT              */
*                   (RCBBLEN+       /* DATA PLUS                     */
*                   CONTLEN+        /* CONTROL INFORMATION           */
*                   SEVEN&          /* ROUNDED TO A DOUBLE WORD      */
*                   'FFF8'X);                                      0208
         LA    @12,23                                              0208
         LH    @14,RCBBLEN(,@14)                                   0208
         N     @14,@CF02236                                        0208
         ALR   @12,@14                                             0208
         N     @12,@CF02103                                        0208
         LH    @14,RCBFLNG(,R7)                                    0208
         N     @14,@CF02236                                        0208
         ALR   @12,@14                                             0208
         STH   @12,RCBFLNG(,R7)                                    0208
*             ELSE                  /* RECORD IS NOT BUFFERED        */
*               DO;                                                0209
         B     @RC00207                                            0209
@RF00207 DS    0H                                                  0210
*                 RFY                                              0210
*                   R1 RSTD;        /* RESTRICT REGISTER FOR ERRORID
*                                      LENGTH                @G17EP5W*/
*                 IF RCBBERFG=YES THEN/* IF ERRORID APPENDED IN RCB
*                                                            @G17EP5W*/
         L     @14,OLDEST                                          0211
         TM    RCBBERFG(@14),B'00000010'                           0211
         BNO   @RF00211                                            0211
*                   R1=LENGTH(NBFRERID);/* THEN INCLUDE ITS LENGTH 0212
*                                      FOR FREEING           @G17EP5W*/
         LA    R1,10                                               0212
*                 ELSE              /* OTHERWISE             @G17EP5W*/
*                   R1=ZERO;                                       0213
*                                                                  0213
         B     @RC00211                                            0213
@RF00211 SLR   R1,R1                                               0213
*                 /***************************************************/
*                 /*                                                 */
*                 /* FREE NO ADDITIONAL SPACE                @G17EP5W*/
*                 /*                                                 */
*                 /***************************************************/
*                                                                  0214
*                 RCBFLNG=RCBFLNG+  /* FREE ONLY                     */
*                     (NBFRLEN+     /* NON-BUFFERED LENGTH           */
*                     R1+           /* LENGTH OF ERRORID IF PRESENT
*                                                            @G17EP5W*/
*                     SEVEN&        /* ROUNDED TO A DOUBLE WORD      */
*                     'FFF8'X);                                    0214
@RC00211 LA    @14,27                                              0214
         ALR   @14,R1                                              0214
         N     @14,@CF02103                                        0214
         LH    @12,RCBFLNG(,R7)                                    0214
         N     @12,@CF02236                                        0214
         ALR   @14,@12                                             0214
         STH   @14,RCBFLNG(,R7)                                    0214
*                 RFY                                              0215
*                   R1 UNRSTD;      /* UNRESTRICT LENGTH REGISTER  0215
*                                                            @G17EP5W*/
*               END;                                               0216
*           END;                    /* END POST LOOP                 */
@RC00207 DS    0H                                                  0217
@DE00168 CLC   RCBFLNG(2,R7),RCBTLNG(R7)                           0217
         BL    @DL00168                                            0217
*SKIPPOST:                          /* RETRY IF FAIL IN POSTING    0218
*                                      REINIT                        */
*           TRYPOST=NO;             /* RESET SWITCH                  */
*                                                                  0218
SKIPPOST NI    TRYPOST,B'11011111'                                 0218
*           /*********************************************************/
*           /*                                                       */
*           /* THIS SECTION WILL INITIALIZE THE BUFFER AREA. THIS CAN*/
*           /* BE EITHER THE BUFFER ACQUIRED BY THIS MODULE OR THE   */
*           /* FIXED BUFFER USED BY THE RECORDING REQUEST ROUTINE    */
*           /* (IEAVTRER).                                           */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0219
*           RCBCNTL=''B;            /* CLEAR THE CONTROL AREA.       */
         XC    RCBCNTL(96,R7),RCBCNTL(R7)                          0219
*           RCBBUFE=ADDR(RCB)+LENGTH(RCB);/* END ADDR OF BUFFER.     */
         LR    @14,R7                                              0220
         AL    @14,@CF00098                                        0220
         ST    @14,RCBBUFE(,R7)                                    0220
*           RCBBUFB=ADDR(RCBBUFRS); /* START ADDR OF BUFFER AREA     */
         LA    @14,RCBBUFRS(,R7)                                   0221
         ST    @14,RCBBUFB(,R7)                                    0221
*           RCBFREE=ADDR(RCBBUFRS); /* START OF FREE AREA.           */
         ST    @14,RCBFREE(,R7)                                    0222
*           RCBFLNG=LENGTH(RCBBUFRS);/* SIZE OF FREE AREA.           */
         LA    @14,4000                                            0223
         STH   @14,RCBFLNG(,R7)                                    0223
*           RCBTLNG=LENGTH(RCBBUFRS);/* TOTAL SIZE OF FREE AREA.     */
         STH   @14,RCBTLNG(,R7)                                    0224
*           RCBDUM=RCBTLNG+1;       /* OFFSET TO DUMMY ALLOC.        */
         AL    @14,@CF00040                                        0225
         STH   @14,RCBDUM(,R7)                                     0225
*           RCBLCNT=ZERO;           /* RESET LOST RCD COUNT.         */
         MVI   RCBLCNT(R7),X'00'                                   0226
*           RCBACNT=ZERO;           /* ACTIVE COUNT IS ZERO.         */
*                                                                  0227
         SLR   @14,@14                                             0227
         STH   @14,RCBACNT(,R7)                                    0227
*           /*********************************************************/
*           /*                                                       */
*           /* SET UP LOST RECORD HEADER                             */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0228
*           RFY                                                    0228
