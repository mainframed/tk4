*/*                                                                  */
*/* THE MOVEREC SEGMENT MOVES THE DATA FROM THE RCB TO A GETMAINED   */
*/* BUFFER. ALLOWING FOR WRAPPED RECORDS IN THE RCB                  */
*/*                                                                  */
*/********************************************************************/
*                                                                  0354
*                       DO;                                        0354
*                         RESPECIFY /* RESTRICT REGISTERS FOR        */
*                          (R2,     /* POINTER TO MOVE FROM          */
*                           R3,     /* LENGTH TO MOVE FROM           */
*                           R4,     /* POINTER TO MOVE TO            */
*                           R5) RESTRICTED;/* LENGTH TO MOVE TO      */
*                         R2=OLDEST;/* MOVE DATA FROM THE OLDEST   0356
*                                      RECORD IN THE FIXED BUFFER    */
         L     @06,OLDEST                                          0356
         LR    R2,@06                                              0356
*                         R4=REBUFADR->RCBFREE;/* MOVE DATA TO NEXT
*                                      FREE SPACE IN GETMAINED BUFFER*/
         L     R4,RCBFREE(,@12)                                    0357
*                         IF RCBBWRAP=YES THEN/* IF THE RECORD WRAPS
*                                      THE BUFFER                    */
         TM    RCBBWRAP(@06),B'10000000'                           0358
         BNO   @RF00358                                            0358
*                           DO;     /* MOVE IN TWO STAGES            */
*                             R5=RCBBUFE-R2;/* MOVE DATA FROM BEGINNG
*                                      OF RECORD             @YM08276*/
         L     R5,RCBBUFE(,R7)                                     0360
         SLR   R5,R2                                               0360
*                             R3=MOVELEN;/* THRU END OF BUFFER     0361
*                                                            @YM08276*/
         LR    R3,@14                                              0361
*                             GEN(MVCL R4,R2);/* MOVE THE DATA       */
         MVCL R4,R2
*                             R2=RCBBUFB;/* WRAP TO BEGINNING OF   0363
*                                      FIXED BUFFER                  */
         L     R2,RCBBUFB(,R7)                                     0363
*                             R5=R3;/* PICK UP REMANING LENGTH     0364
*                                                            @YM08276*/
         LR    R5,R3                                               0364
*                             GEN(MVCL R4,R2);/* MOVE THE DATA       */
         MVCL R4,R2
*                           END;                                   0366
*                         ELSE      /* IF NOT A WRAPPED RECORD       */
*                           DO;                                    0367
         B     @RC00358                                            0367
@RF00358 DS    0H                                                  0368
*                             R3=MOVELEN;/* MOVE THE ENTIRE RECORD   */
         L     @14,MOVELEN                                         0368
         LR    R3,@14                                              0368
*                             R5=MOVELEN;                          0369
         LR    R5,@14                                              0369
*                             GEN(MVCL R4,R2);/* MOVE THE DATA       */
