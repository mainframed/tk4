         TITLE 'IEAVTABI - ABDUMP INITIALIZATION                       *
                        '
IEAVTABI CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'IEAVTABI  77.006'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @11,0                                               0001
@PSTART  LA    @12,4095(,@11)                                      0001
         USING @PSTART,@11                                         0001
         USING @PSTART+4095,@12                                    0001
*
** REFS ARE GENERATED FOR NVT FIELDS THAT CONTAIN ADDRESSES OF
** NIP SERVICE ROUTINES, SO IEAPMNIP MACRO CALLS TO THESE ROUTINES
** WILL ASSEMBLE PROPERLY.
*   FLDRTCT(1)=ADDR(RTCTSAP);       /* SYSABEND PORTION OF RTCT.     */
         L     @10,CVTRTMCT(,CVTPTR)                               0130
         LA    @07,RTCTSAP(,@10)                                   0130
         ST    @07,FLDRTCT                                         0130
*   FLDRTCT(2)=ADDR(RTCTSUP);       /* SYSUDUMP PORTION OF RTCT.   0131
*                                                            @G33SPHW*/
         LA    @07,RTCTSUP(,@10)                                   0131
         ST    @07,FLDRTCT+4                                       0131
*   FLDRTCT(3)=ADDR(RTCTSYD);       /* SYSMDUMP PORTION OF RTCT.   0132
*                                                            @G33SPHW*/
         LA    @10,RTCTSYD(,@10)                                   0132
         ST    @10,FLDRTCT+8                                       0132
*   NVTPTRSV=RNVT;                  /* PRESERVE NVT ADDR.            */
         ST    RNVT,NVTPTRSV                                       0133
*   DO CNT=1 TO 3 BY 1;             /* PROCESS THREE PARMLIB MEMBERS
*                                                            @G33SPHW*/
*                                                                  0134
         LA    @10,1                                               0134
         ST    @10,CNT                                             0134
@DL00134 DS    0H                                                  0135
*     /***************************************************************/
*     /*                                                             */
*     /* INITIALIZE TO READ A MEMBER FROM PARMLIB.                   */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0135
*     GEN REFS(TRANTBL);                                           0135
      MVI   TRANTBL,X'06'    SET TABLE TO ALL INVALID
      MVC   TRANTBL+1(255),TRANTBL
*     TRANTBL('A'+1:'I'+1)=''B;     /* ALPHAS VALID.                 */
         XC    TRANTBL+193(9),TRANTBL+193                          0136
*     TRANTBL('J'+1:'R'+1)=''B;     /* ALPHAS VALID.                 */
         XC    TRANTBL+209(9),TRANTBL+209                          0137
*     TRANTBL('S'+1:'Z'+1)=''B;     /* ALPHAS VALID.                 */
         XC    TRANTBL+226(8),TRANTBL+226                          0138
*     TRANTBL('='+1)='01'X;         /* EQUAL ONLY CHAR VALID.        */
         MVI   TRANTBL+126,X'01'                                   0139
*     FLAG1='00'X;                  /* CLEAR CONTROL FLAGS.          */
         MVI   FLAG1,X'00'                                         0140
*     FLAG2='00'X;                  /* CLEAR CONTROL FLAGS.          */
         MVI   FLAG2,X'00'                                         0141
*     PARANCNT=0;                                                  0142
         MVI   PARANCNT,X'00'                                      0142
*     NEXTREAD='1'B;                /* FORCE FIRST READ.             */
*     NEXTCARD='1'B;                /* FIRST CARD EXPECTED.          */
         OI    NEXTREAD,B'11000000'                                0144
*     RNVT=NVTPTRSV;                /* ENSURE NVT ADDRESSABLE.       */
         L     RNVT,NVTPTRSV                                       0145
*     NVTPAREA->NIPPANAM=NAME(CNT); /* MEMBER TO BE READ.            */
         L     @10,NVTPAREA(,RNVT)                                 0146
         L     @07,CNT                                             0146
         LR    @15,@07                                             0146
         SLA   @15,3                                               0146
         LA    @14,NAME-8(@15)                                     0146
         MVC   NIPPANAM(8,@10),0(@14)                              0146
*     R6=FLDRTCT(CNT);              /* APPLICABLE PORTION OF RTCT.   */
         SLA   @07,2                                               0147
         L     R6,FLDRTCT-4(@07)                                   0147
*     DO WHILE NEXTREAD='1'B;       /* READ TILL TOLD TO STOP.       */
         B     @DE00148                                            0148
@DL00148 DS    0H                                                  0149
*       RNVT=NVTPTRSV;              /* SET NVT ADDR.                 */
*                                                                  0149
         L     RNVT,NVTPTRSV                                       0149
*       /*************************************************************/
*       /*                                                           */
*       /* REQUEST CARD FROM PARMLIB                                 */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0150
*       CALL GETCARD;               /* READ CARD VIA NIPPMMEM        */
*                                                                  0150
         BAL   @14,GETCARD                                         0150
*       /*************************************************************/
*       /*                                                           */
*       /* PROCESS CARD, IF PRESENT.                                 */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0151
*       IF RPARM>0 THEN             /* GOOD READ...                  */
         LTR   RPARM,RPARM                                         0151
         BNP   @RF00151                                            0151
*         DO;                       /* -YES.                         */
*           IF NEXTCARD='0'B THEN   /* SHOULD CARD BE PRESENT...     */
         TM    NEXTCARD,B'01000000'                                0153
         BNZ   @RF00153                                            0153
*             ERRSW='1'B;           /* -NO, ERROR.                   */
         OI    ERRSW,B'00010000'                                   0154
*           ELSE                                                   0155
*             CALL CARDSCAN;        /* CHECK ENTIRE CARD.            */
         B     @RC00153                                            0155
@RF00153 BAL   @14,CARDSCAN                                        0155
*         END;                                                     0156
*                                                                  0156
*       /*************************************************************/
*       /*                                                           */
*       /* CARD WAS NOT BROUGHT IN FOR SOME REASON.                  */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0157
*       ELSE                        /* BAD READ (RC=4,8,12,16)       */
*         DO;                                                      0157
         B     @RC00151                                            0157
@RF00151 DS    0H                                                  0158
*           NEXTREAD='0'B;          /* STOP FUTURE READS.            */
         NI    NEXTREAD,B'01111111'                                0158
*           IF NVTPAREA->NIPPARC=16 THEN/* EOF (RC=16) ...           */
         L     @10,NVTPAREA(,RNVT)                                 0159
         CLC   NIPPARC(4,@10),@CF00872                             0159
         BNE   @RF00159                                            0159
*             DO;                   /* -YES.                         */
*               IF NEXTCARD='1'B THEN/* WAS NEW CARD EXPECTED...     */
         TM    NEXTCARD,B'01000000'                                0161
         BNO   @RF00161                                            0161
*                 ERRSW='1'B;       /* -YES,ERROR-PREMATURE EOF      */
         OI    ERRSW,B'00010000'                                   0162
*             END;                                                 0163
*           ELSE                    /* RC = 4, 8, OR 12.             */
*             RTCTMULT=RTCTMULT&&RTCTMULT;/* NO OPTIONS ALLOWED      */
         B     @RC00159                                            0164
@RF00159 XC    RTCTMULT(4,R6),RTCTMULT(R6)                         0164
*         END;                                                     0165
*                                                                  0165
@RC00159 DS    0H                                                  0166
*       /*************************************************************/
*       /*                                                           */
*       /* IF ERROR WAS FOUND, PRINT ERROR MESSAGE(S) AND ABORT THIS */
*       /* PARMLIB MEMBER.                                           */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0166
*       IF ERRSW='1'B THEN          /* ERROR ENCOUNTERED...          */
@RC00151 TM    ERRSW,B'00010000'                                   0166
         BNO   @RF00166                                            0166
*         DO;                       /* -YES.                         */
*           NM884(1:8)=NAME(CNT);   /* SET NAME IN MSG               */
         L     @10,CNT                                             0168
         SLA   @10,3                                               0168
         LA    @07,NAME-8(@10)                                     0168
         MVC   NM884(8),0(@07)                                     0168
*           NM885(1:8)=NAME(CNT);                                  0169
         LA    @07,NAME-8(@10)                                     0169
         MVC   NM885(8),0(@07)                                     0169
*           CALL ERRMSG;            /* PUT OUT MSGS.                 */
         BAL   @14,ERRMSG                                          0170
*           NEXTREAD='0'B;          /* STOP READS.                   */
         NI    NEXTREAD,B'01111111'                                0171
*         END;                                                     0172
*     END;                          /* END OF DO-WHILE               */
@RF00166 DS    0H                                                  0173
@DE00148 TM    NEXTREAD,B'10000000'                                0173
         BO    @DL00148                                            0173
*   END;                            /* END OF DO-BY, ALL PROCESSED   */
         LA    @10,1                                               0174
         AL    @10,CNT                                             0174
         ST    @10,CNT                                             0174
         C     @10,@CF00090                                        0174
         BNH   @DL00134                                            0174
*   RTCTSAO=RTCTSAP;                /* SET SYSABEND EFFECTIVE OPTIONS
*                                                            @G33SPHW*/
         L     @10,CVTRTMCT(,CVTPTR)                               0175
         MVC   RTCTSAO(4,@10),RTCTSAP(@10)                         0175
*   RTCTSUO=RTCTSUP;                /* SET SYSUDUMP EFFECTIVE OPTIONS
*                                                            @G33SPHW*/
         MVC   RTCTSUO(4,@10),RTCTSUP(@10)                         0176
*   RTCTSYO=RTCTSYD;                /* SET SYSMDUMP EFFECTIVE OPTIONS
*                                                            @G33SPHW*/
         MVC   RTCTSYO(4,@10),RTCTSYD(@10)                         0177
*CARDSCAN:                                                         0178
*   PROC;                                                          0178
*                                                                  0178
@EL00001 DS    0H                                                  0178
@EF00001 DS    0H                                                  0178
@ER00001 LM    @14,@12,12(@13)                                     0178
         BR    @14                                                 0178
CARDSCAN STM   @14,@12,@SA00002                                    0178
*/* THIS ROUTINE WILL PROCESS THE ENTIRE CARD JUST READ IN.  IT      */
*/* WILL RETURN TO THE MAINLINE ONLY IF A NEW CARD IS NEEDED, OR     */
*/* IF AN ERROR IS DETECTED.                                         */
*                                                                  0179
*   IF RPARM->PARM(1:1)=' '&        /* BLANK CARD...                 */
*       RPARM->PARM(2:2)='FF'X THEN /* -YES.                         */
         CLI   PARM(RPARM),C' '                                    0179
         BNE   @RF00179                                            0179
         CLI   PARM+1(RPARM),X'FF'                                 0179
         BNE   @RF00179                                            0179
*     ERRSW='1'B;                   /* BLANK CARD NOT PERMITTED.     */
*                                                                  0180
         OI    ERRSW,B'00010000'                                   0180
*   /*****************************************************************/
*   /*                                                               */
*   /* CARD NOT BLANK SO SCAN IT.                                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0181
*   ELSE                                                           0181
*     DO;                                                          0181
         B     @RC00179                                            0181
@RF00179 DS    0H                                                  0182
*       START=RPARM;                /* START OF SCAN                 */
         LR    START,RPARM                                         0182
*       STOPSCAN='0'B;              /* DO-WHILE LOOP CONTROL         */
*                                                                  0183
         NI    STOPSCAN,B'11110111'                                0183
*       /*************************************************************/
*       /*                                                           */
*       /* SCAN ACROSS THE CARD UNTIL IT IS COMPLETE (NORMALLY OR    */
*       /* UNTIL AN ERROR IS FOUND).                                 */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0184
*       DO WHILE STOPSCAN='0'B;                                    0184
         B     @DE00184                                            0184
@DL00184 DS    0H                                                  0185
*         RFY                                                      0185
*          (R2,                                                    0185
*           R1) RSTD;                                              0185
*         R2=0;                     /* TRT INDEX VALUE.              */
         SLR   R2,R2                                               0186
*         TRT(PARM(1:9),TRANTBL);   /* LOOK FOR STOPPER.             */
         TRT   PARM(9,START),TRANTBL                               0187
*         IF R2=0 THEN              /* .GT. 8 CHARACTERS...          */
         LTR   R2,R2                                               0188
         BNZ   @RF00188                                            0188
*           ERRSW='1'B;             /* -YES, SHOW ERROR FOUND.       */
         OI    ERRSW,B'00010000'                                   0189
*         ELSE                      /* .LE. 8 CHARACTERS.            */
*           DO;                                                    0190
         B     @RC00188                                            0190
@RF00188 DS    0H                                                  0191
*             ERRSW='0'B;           /* NO ERROR YET.                 */
*             DOSCAN='1'B;          /* PRESUPPOSE SCAN NEEDED        */
         OI    DOSCAN,B'00100000'                                  0192
         NI    ERRSW,B'11101111'                                   0192
*             STOP=R1;              /* POINT WHERE SCAN STOPPED.     */
*                                                                  0193
         ST    R1,STOP                                             0193
*             /*******************************************************/
*             /*                                                     */
*             /* USE VALUE EXTRACTED FROM TRANSLATE TABLE TO GET TO  */
*             /* PROPER CHARACTER HANDLING ROUTINE.                  */
*             /*                                                     */
*             /*******************************************************/
*                                                                  0194
*             CALL BRTABLE(R2);     /* GO HANDLE STOPPER CHARACTER.  */
         LR    @10,R2                                              0194
         SLA   @10,2                                               0194
         L     @15,BRTABLE-4(@10)                                  0194
         BALR  @14,@15                                             0194
*             RFY                                                  0195
*              (R2,                                                0195
*               R1) UNRSTD;                                        0195
*                                                                  0195
*             /*******************************************************/
*             /*                                                     */
*             /* SYNTAX CHECK THE ISOLATED SUB-PARM IF THE STOPPER   */
*             /* CHARACTER WARRANTS IT.                              */
*             /*                                                     */
*             /*******************************************************/
*                                                                  0196
*             IF DOSCAN='1'B THEN   /* SCAN NEEDED...                */
         TM    DOSCAN,B'00100000'                                  0196
         BNO   @RF00196                                            0196
*               DO;                 /* -YES.                         */
*                 R5=STOP-START;    /* LENGTH OF SUBPARAMETER.       */
         L     R5,STOP                                             0198
         SLR   R5,START                                            0198
*                 IF R5<2 THEN      /* PARM TOO SHORT...             */
         C     R5,@CF00166                                         0199
         BNL   @RF00199                                            0199
*                   ERRSW='1'B;     /* -YES, SHOW ERROR.             */
         OI    ERRSW,B'00010000'                                   0200
*                 ELSE              /* PARM WITHIN LIMITS            */
*                   DO;                                            0201
         B     @RC00199                                            0201
@RF00199 DS    0H                                                  0202
*                     IF PDSDSW='1'B THEN/* KEYWORD TYPE...          */
         TM    PDSDSW,B'00000100'                                  0202
         BNO   @RF00202                                            0202
*                       CALL PDATPARM;/* PDATA = ...                 */
         BAL   @14,PDATPARM                                        0203
*                     ELSE                                         0204
*                       CALL SDATPARM;/* SDATA = ...                 */
         B     @RC00202                                            0204
@RF00202 BAL   @14,SDATPARM                                        0204
*                     IF STOP->PARM(1:1)=')' THEN/* SUBSTRING JUST 0205
*                                      END...                        */
@RC00202 L     @10,STOP                                            0205
         CLI   PARM(@10),C')'                                      0205
         BNE   @RF00205                                            0205
*                       OPNPARAN='0'B;/* -YES, DONT ALLOW MORE     0206
*                                      SDATA/PDATA SUBPARMS.         */
         NI    OPNPARAN,B'10111111'                                0206
*                   END;                                           0207
@RF00205 DS    0H                                                  0208
*               END;                                               0208
@RC00199 DS    0H                                                  0209
*           END;                                                   0209
@RF00196 DS    0H                                                  0210
*         IF ERRSW='1'B THEN        /* ANY ERRORS FOUND...           */
@RC00188 TM    ERRSW,B'00010000'                                   0210
         BNO   @RF00210                                            0210
*           STOPSCAN='1'B;          /* -YES, KILL SCAN.              */
         OI    STOPSCAN,B'00001000'                                0211
*         ELSE                      /* N0 ERRORS.                    */
*           START=STOP+1;           /* BUMP FOR NEXT TRT SCAN        */
         B     @RC00210                                            0212
@RF00210 LA    START,1                                             0212
         AL    START,STOP                                          0212
*       END;                        /* END STOPSCAN DO-WHILE.        */
*                                                                  0213
@RC00210 DS    0H                                                  0213
@DE00184 TM    STOPSCAN,B'00001000'                                0213
         BZ    @DL00184                                            0213
*       /*************************************************************/
*       /*                                                           */
*       /* CARD HAS EITHER BEEN COMPLETED OR ABORTED.                */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0214
*       IF ERRSW='1'B THEN          /* STOP BECAUSE OF ERROR...      */
         TM    ERRSW,B'00010000'                                   0214
         BNO   @RF00214                                            0214
*         NEXTREAD='0'B;            /* -YES, STOP THE READS.         */
         NI    NEXTREAD,B'01111111'                                0215
*     END;                          /* END OF NON-BLANK CARD.        */
*   RETURN;                         /* BACK TO MAINLINE.             */
@EL00002 DS    0H                                                  0217
@EF00002 DS    0H                                                  0217
@ER00002 LM    @14,@12,@SA00002                                    0217
         BR    @14                                                 0217
*   END CARDSCAN;                                                  0218
         B     @EL00002                                            0218
*LPARAN:                                                           0219
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0219
*                                                                  0219
LPARAN   DS    0H                                                  0220
*   /*****************************************************************/
*   /*                                                               */
*   /* A LEFT PARANTHESES WAS FOUND ON THE CARD. THE START-OF- SCAN  */
*   /* POINTER FOR THE TRT WILL BE UPDATED AND THE PARAN COUNT WILL  */
*   /* BE SET TO ONE. THE TRANSLATE TABLE WILL ALSO BE SET TO REFLECT*/
*   /* VALID AND INVALID CHARACTERS FOLLOWING THE LEFT PARAN.        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0220
*   RFY                                                            0220
*     R14 RSTD;                                                    0220
*   PARANCNT=1;                     /* SET PARAN COUNT.              */
         MVI   PARANCNT,X'01'                                      0221
*   OPNPARAN='1'B;                  /* SUBSTRINGING ALLOWED AFTER  0222
*                                      LEFT PARANTHESES.             */
*                                                                  0222
         OI    OPNPARAN,B'01000000'                                0222
*   /*****************************************************************/
*   /*                                                               */
*   /* ALLOW CHARACTERS.                                             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0223
*   TRANTBL(')'+1)='05'X;           /* RIGHT PARAN VALID             */
         MVI   TRANTBL+93,X'05'                                    0223
*   TRANTBL(','+1)='02'X;           /* COMMA VALID.                  */
*                                                                  0224
         MVI   TRANTBL+107,X'02'                                   0224
*   /*****************************************************************/
*   /*                                                               */
*   /* DISALLOW CHARACTERS                                           */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0225
*   TRANTBL('='+1)='06'X;           /* EQUAL SIGN INVALID            */
         MVI   TRANTBL+126,X'06'                                   0225
*   TRANTBL(' '+1)='06'X;           /* BLANK INVALID.                */
         MVI   TRANTBL+64,X'06'                                    0226
*   TRANTBL('('+1)='06'X;           /* LEFT PARAN INVALID            */
*                                                                  0227
         MVI   TRANTBL+77,X'06'                                    0227
*   /*****************************************************************/
*   /*                                                               */
*   /* SHOW NO SCAN NECCESSARY                                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0228
*   DOSCAN='0'B;                    /* NO SCAN NECCESSARY.           */
         NI    DOSCAN,B'11011111'                                  0228
*   RETURN;                                                        0229
@EL00003 DS    0H                                                  0229
@EF00003 DS    0H                                                  0229
@ER00003 BR    @14                                                 0229
*   RFY                                                            0230
*     R14 UNRSTD;                                                  0230
*   END LPARAN;                                                    0231
*INVALID:                                                          0232
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0232
*                                                                  0232
INVALID  DS    0H                                                  0233
*   /*****************************************************************/
*   /*                                                               */
*   /* AN INVALID CHARACTER WAS FOUND DURING THE SCAN, IE, IT WAS NOT*/
*   /* PERMITTED AFTER A PREVIOUSLY VALID STOPPER CHARACTER. THE     */
*   /* CURRENT PARMLIB MEMBER WILL BE FLUSHED.                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0233
*   RFY                                                            0233
*     R14 RSTD;                                                    0233
*   ERRSW='1'B;                     /* SHOW MEMBER FLUSH             */
*   DOSCAN='0'B;                    /* NO SCAN TO BE DONE            */
         OI    ERRSW,B'00010000'                                   0235
         NI    DOSCAN,B'11011111'                                  0235
*   RETURN;                         /* BACK TO MAINLINE              */
@EL00004 DS    0H                                                  0236
@EF00004 DS    0H                                                  0236
@ER00004 BR    @14                                                 0236
*   RFY                                                            0237
*     R14 UNRSTD;                                                  0237
*   END INVALID;                                                   0238
*COMMA:                                                            0239
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0239
*                                                                  0239
COMMA    DS    0H                                                  0240
*   /*****************************************************************/
*   /*                                                               */
*   /* A COMMA WAS FOUND ON THE CARD. THE SYNTAX SCAN SWITCH WILL BE */
*   /* SET. THE TRANSLATE TABLE WILL ALSO BE SET TO ALLOW VALID      */
*   /* CHARACTERS AFTER A COMMA.                                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0240
*   RFY                                                            0240
*     R14 RSTD;                                                    0240
*                                                                  0240
*   /*****************************************************************/
*   /*                                                               */
*   /* ALLOW...                                                      */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0241
*   TRANTBL(' '+1)='03'X;           /* BLANK NEXT VALID STOPPER.     */
         MVI   TRANTBL+64,X'03'                                    0241
*   TRANTBL(','+1)='02'X;           /* COMMA STILL VALID             */
         MVI   TRANTBL+107,X'02'                                   0242
*   TRANTBL(')'+1)='05'X;           /* RIGHT PARAN VALID             */
         MVI   TRANTBL+93,X'05'                                    0243
*   TRANTBL('='+1)='01'X;           /* EQUAL SIGN VALID              */
*                                                                  0244
         MVI   TRANTBL+126,X'01'                                   0244
*   /*****************************************************************/
*   /*                                                               */
*   /* DISALLOW                                                      */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0245
*   TRANTBL('('+1)='06'X;           /* LEFT PARAN NOT VALID.         */
         MVI   TRANTBL+77,X'06'                                    0245
*   IF(STOP-1)->PARM(1:1)=')' THEN  /* FOLLOW PARAN...               */
         L     @15,STOP                                            0246
         BCTR  @15,0                                               0246
         CLI   PARM(@15),C')'                                      0246
         BNE   @RF00246                                            0246
*     DOSCAN='0'B;                  /* -YES, NO SCAN NEEDED          */
         NI    DOSCAN,B'11011111'                                  0247
*   RETURN;                         /* BACK TO MAINLINE.             */
@EL00005 DS    0H                                                  0248
@EF00005 DS    0H                                                  0248
@ER00005 BR    @14                                                 0248
*   RFY                                                            0249
*     R14 UNRSTD;                                                  0249
*   END COMMA;                                                     0250
         B     @EL00005                                            0250
*BLANK:                                                            0251
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0251
*                                                                  0251
BLANK    DS    0H                                                  0252
*   /*****************************************************************/
*   /*                                                               */
*   /* A BLANK WAS READ. IF PRECEDED BY COMMA IT SIGNALS A           */
*   /* CONTINUATION CARD. IF PRECEDED BY ANYTHING ELSE IT SIGNALS END*/
*   /* OF MEMBER - NO NEXT CARD WILL BE ALLOWED                      */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0252
*   RFY                                                            0252
*     R14 RSTD;                                                    0252
*   NEXTREAD='1'B;                  /* TO TEST IF MORE CARDS.        */
*   STOPSCAN='1'B;                  /* STOP DO-WHILE.                */
         OI    NEXTREAD,B'10001000'                                0254
*   IF(STOP-1)->PARM(1:1)=',' THEN  /* IS IT CONTINUATION...         */
         L     @15,STOP                                            0255
         BCTR  @15,0                                               0255
         CLI   PARM(@15),C','                                      0255
         BNE   @RF00255                                            0255
*     DO;                           /* -YES.                         */
*       NEXTCARD='1'B;              /* SET TO EXPECT NEXT CARD.      */
*       DOSCAN='0'B;                /* NO SUBPARM SCAN.              */
         OI    NEXTCARD,B'01000000'                                0258
         NI    DOSCAN,B'11011111'                                  0258
