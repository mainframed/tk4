         L     @12,UCMBASPT(,@12)                                  0400
         CLI   CONSWCOD(@12),X'40'                                 0400
         BNE   @RF00400                                            0400
*                 CONSPARM=R2;      /* PLACE ECB VALUE IN WORKAREA   */
         L     @12,LCCACPUS(,R10)                                  0401
         L     @12,WSACACR(,@12)                                   0401
         ST    R2,CONSPARM(,@12)                                   0401
*               ELSE                                               0402
*                 CONSPARM=''B;     /* BUILD SINGLE MASK VALUE       */
*                                                                  0402
         B     @RC00400                                            0402
@RF00400 L     @12,LCCACPUS(,R10)                                  0402
         L     @12,WSACACR(,@12)                                   0402
         SLR   @08,@08                                             0402
         ST    @08,CONSPARM(,@12)                                  0402
*               /*****************************************************/
*               /*                                                   */
*               /* OR THE DEAD CPU MASK INTO THE ECB VALUE           */
*               /*                                                   */
*               /*****************************************************/
*                                                                  0403
*               SWMASK=SWMASK×R11->LCCACPUS->WSACACR->ACRCAFM;     0403
@RC00400 L     @12,LCCACPUS(,R10)                                  0403
         L     @12,WSACACR(,@12)                                   0403
         L     @08,LCCACPUS(,R11)                                  0403
         L     @08,WSACACR(,@08)                                   0403
         OC    SWMASK(2,@12),ACRCAFM(@08)                          0403
*               POSTCDE='40'X;      /* SET THE POST CODE ON          */
         MVI   POSTCDE(@12),X'40'                                  0404
*               R3=CONSPARM;        /* SET THE SWAP REGISTER         */
         L     R3,CONSPARM(,@12)                                   0405
*               CS(R2,R3,CONSWECB); /* POST THE UCMARECB             */
         L     @12,CVTPTR(,R4)                                     0406
         L     @12,UCMBASPT(,@12)                                  0406
         CS    R2,@03,CONSWECB(@12)                                0406
*               BC(7,CONSWAP);      /* NOT SUCESSFUL,ALREADY PSTD    */
         BC    7,CONSWAP                                           0407
*               RESPECIFY                                          0408
*                (R2,                                              0408
*                 R3) UNRSTD;                                      0408
*                                                                  0408
*/* ******************************************************************/
