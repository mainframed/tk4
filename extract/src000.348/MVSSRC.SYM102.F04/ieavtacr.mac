@RF00337 DS    0H                                                  0452
*/* ******************************************************************/
*/* ******************************************************************/
*/* ****************       ACR EXIT PROCESSING        ****************/
*/* ******************************************************************/
*/* ******************************************************************/
*                                                                  0452
*   DO;                                                            0452
*                                                                  0452
@RC00188 DS    0H                                                  0453
*/* ******************************************************************/
*/*                                                                  */
*/* 01* SEGMENT NAME  ACREXPR                                        */
*/*                                                                  */
*/* 02* FUNCTION    ACR EXIT PROCESSOR                               */
*/*                                                                  */
*/* 02* INPUT= PSAPTR (REG 4) - ADDR OF CURR PSA BASED ZERO          */
*/*            RRPLPTR (REG 6) - ADDR OF RECOVERY RTN PARAMETER LIST */
*/*            LCCAPTR (REG 10) - ADDR OF THE CURRENT LCCA           */
*/*            LCCAPTR2(REG 11)- ADDR OF OTHER LCCA                  */
*/*                                                                  */
*/* 02* OUTPUT= SEE EXIT SECTION OF MAIN PROLOGUE                    */
*/*                                                                  */
*/* ******************************************************************/
*                                                                  0453
*     RESPECIFY                                                    0453
*      (R1,                                                        0453
*       R2,                                                        0453
*       R14) RSTD;                                                 0453
*                                                                  0453
*     /***************************************************************/
*     /*                                                             */
*     /* EXIT ACR VIA THE SPECIFIC INTERFACE REQUIRED(SEE PROLOGUE)  */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0454
*     IF LCCACRLE='0'B              /* IS IT A LOCK MGR OR SPIN    0454
*                                      ROUTINE EXIT?                 */
*       THEN                                                       0454
         TM    LCCACRLE(R10),B'00100000'                           0454
         BNZ   @RF00454                                            0454
*       DO;                         /* NOT LOCK MGR-SPIN EXIT        */
*         IF LCCACRRM='0'B          /* IS IT A DISPATCHER EXIT?      */
*           THEN                                                   0456
         TM    LCCACRRM(R10),B'01000000'                           0456
         BNZ   @RF00456                                            0456
*           DO;                     /* NOT DISPATCHER EXIT           */
*             IF LCCACREF='0'B      /* IS IT AN EXT FLIH OR SLIH   0458
*                                      EXIT?                         */
*               THEN                                               0458
         TM    LCCACREF(R10),B'10000000'                           0458
         BNZ   @RF00458                                            0458
*               DO;                 /* NOT SLIH OR FLIH EXIT         */
*                 IF LCCACRRT='0'B  /* IS IT A RETURN TO RTM?        */
*                   THEN                                           0460
         TM    LCCACRRT(R10),B'00010000'                           0460
         BNZ   @RF00460                                            0460
*                   DO;             /* NO, RTM EXIT, IT MUST BE A  0461
*                                      REQUEST FOR SYSTERM           */
*                     R1=LCCACPUS->WSACACR->ACRENTR1;/* GET REAL   0462
*                                      ADDRESS OF SYSTEM TERMINATION
*                                      PARAMETER LIST                */
         L     @15,LCCACPUS(,R10)                                  0462
         L     @15,WSACACR(,@15)                                   0462
         L     R1,ACRENTR1(,@15)                                   0462
*                     PSAACR='0'B;  /* TURN OFF SUPER                */
         NI    PSAACR(R4),B'11111011'                              0463
*                     DO;           /* SYSTERM EXIT INDICATED        */
*/* ******************************************************************/
*/*                                                                  */
*/* 01* SEGMENT-NAME  ACRTRMEX                                       */
*/*                                                                  */
*/* 02* FUNCTION                                                     */
*/*     THIS SEGMENT PASSES CONTROL TO THE SYSTEM TERMINATION        */
*/*     FACILITY BY EXECUTING A LPSW WITH THE FOLLOWING ATTRIBUTES. -*/
*/*     DISABLED, KEY 0, DAT OFF, EC MODE, SUPERVISOR STATE.         */
*/*     REGISTER 1 CONTAINS THE REAL ADDRESS OF A 2 WORD PARAMETER   */
*/*     LIST WHICH CONTAINS THE REAL ADDRESS OF A MESSAGE BUFFER IN  */
*/*     THE FIRST WORD, AND THE REAL ADDRESS OF THE LOGREC BUFFER IN */
*/*     THE SECOND WORD.                                             */
*/*                                                                  */
*/* 02* INPUT= REGISTER 1 - ADDR OF SYSTERM PARAMETER LIST (REAL)    */
*/*                                                                  */
*/* 02* OUTPUT= REGISTER 1 - PASSED TO SYSTEM TERMINATION (IGFPTERM) */
*/*                                                                  */
*/* ******************************************************************/
*                                                                  0465
*                       GENERATE REFS(TRMPSW);                     0465
*                                                                  0465
        LPSW  TRMPSW                    GO TO SYSTEM TERMINATION
*/* ******************************************************************/
*/*   END OF SEGMENT ACRTERMEX                                       */
*/* ******************************************************************/
*                                                                  0466
*                     END;                                         0466
*                   END;                                           0467
*                 ELSE              /* RTM EXIT SET RETURN TO RTM    */
*                   DO;                                            0468
         B     @RC00460                                            0468
@RF00460 DS    0H                                                  0469
*                     R1=ACRENTR1;  /* GET ADDRESS OF RTM REENTRY PSW*/
         L     @15,LCCACPUS(,R10)                                  0469
         L     @15,WSACACR(,@15)                                   0469
         L     R1,ACRENTR1(,@15)                                   0469
*                     PSAPSWSV=R1->RTPSW;/* PLACE PASSED PSW IN PSA
*                                      FOR LPSW                      */
         MVC   PSAPSWSV(8,R4),RTPSW(R1)                            0470
*                     PSAACR='0'B;  /* TURN OFF SUPER BIT            */
         NI    PSAACR(R4),B'11111011'                              0471
*                     GENERATE REFS(R1,R0,R15,PSAPSWSV);           0472
                           LM R0,R15,0(R1)  LOAD PASSED REGISTERS
                           LPSW PSAPSWSV    RETURN TO RTM
*                   END;                                           0473
*               END;                                               0474
*             ELSE                                                 0475
*               DO;                                                0475
         B     @RC00458                                            0475
@RF00458 DS    0H                                                  0476
*                 R2=LCCACPUS->WSACACR->ACRRTNPT;                  0476
         L     @15,LCCACPUS(,R10)                                  0476
         L     @15,WSACACR(,@15)                                   0476
         L     R2,ACRRTNPT(,@15)                                   0476
*                 PSAACR='0'B;      /* TURN OFF SUPER BIT            */
         NI    PSAACR(R4),B'11111011'                              0477
*                 GO TO R2;         /* RETURN TO EXT FLIH OR SLIH    */
         BR    R2                                                  0478
*               END;                                               0479
*           END;                                                   0480
*         ELSE                      /* EXIT TO DISPATCHER          0481
*                                      (POST-PROCESSING PHASE      0481
*                                      COMPLETE)                     */
*           DO;                                                    0481
         B     @RC00456                                            0481
@RF00456 DS    0H                                                  0482
*             R14=CVT0DS;           /* YES,SET UP DISPATCHER RETRN   */
         L     @12,CVTPTR(,R4)                                     0482
         L     R14,CVT0DS(,@12)                                    0482
*             PSAANEW=0;            /* FORCE DISPATCHER TO NEW SEARCH*/
         SLR   @15,@15                                             0483
         ST    @15,PSAANEW(,R4)                                    0483
*             GEN(SETFRR D,WRKREGS=(R1,R2));/* DELETE FRR            */
         SETFRR D,WRKREGS=(R1,R2)
*             GOTO R14;             /* GO TO DISPATCHER              */
         BR    R14                                                 0485
*           END;                                                   0486
*       END;                                                       0487
*     ELSE                          /* YES, LOCK MGR-SPIN EXIT       */
*       DO;                                                        0488
         B     @RC00454                                            0488
@RF00454 DS    0H                                                  0489
*         R14=LCCACPUS->WSACACR->ACRRTNPT;/* PICK UP RESUME POINT  0489
*                                      FROM NOW CURRENT CPU'S ACRSA  */
         L     @12,LCCACPUS(,R10)                                  0489
         L     @12,WSACACR(,@12)                                   0489
         L     R14,ACRRTNPT(,@12)                                  0489
*         PSAACR='0'B;              /* TURN OFF SUPER BIT            */
         NI    PSAACR(R4),B'11111011'                              0490
*         RETURN;                   /* RETURN TO THE LOCK MGR OR   0491
*                                      GLOBAL SPIN ROUTINE, OR THE 0491
*                                      DISPATCHER                    */
@EL00001 DS    0H                                                  0491
@EF00001 DS    0H                                                  0491
@ER00001 BR    @14                                                 0491
*       END;                                                       0492
*     RESPECIFY                                                    0493
*      (R1,                                                        0493
*       R2,                                                        0493
*       R14) UNRESTRICTED;                                         0493
*                                                                  0493
@RC00454 DS    0H                                                  0494
*/* ******************************************************************/
*/*  END OF SEGMENT ACREXPR                                          */
*/* ******************************************************************/
