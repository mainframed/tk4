*               DO;                 /* DEAD CPU IN CONTROL           */
*                 GEN REFS(RRPLREGS,R6)(STM 0,14,RRPLREGS(R6));/*  0342
*                                      SAVE REGS FOR RECURSION       */
         STM 0,14,RRPLREGS(R6)
*                 RRPLFLGS='0'B;    /* CLEAR ERROR FLAGS             */
         MVI   RRPLFLGS(R6),B'00000000'                            0343
*RSTRT04:                                                          0344
*                 LOGPHID='04'X;    /* SET LOGICAL PHASE ID FOR    0344
*                                      RECOVERY RTN                  */
RSTRT04  MVI   LOGPHID(R6),X'04'                                   0344
*                 RESPECIFY                                        0345
*                  (R13) RSTD;                                     0345
*                 R13=WSACACR;      /* SET BASE TO ACR WORK SAVE AREA*/
         L     @08,LCCACPUS(,R10)                                  0346
         L     R13,WSACACR(,@08)                                   0346
*                 CALL ACRPROSW;    /* MAKE RECOVERY CPU CURRENT     */
         BAL   @14,ACRPROSW                                        0347
*                 RESPECIFY                                        0348
*                  (R13) UNRSTD;                                   0348
*               END;                                               0349
*           END;                                                   0350
@RF00340 DS    0H                                                  0351
*           DO;                                                    0351
*                                                                  0351
*/* ******************************************************************/
*/*                                                                  */
*/* 01* SEGMENT NAME  ACRPOSTP                                       */
*/*                                                                  */
*/* 02* FUNCTION                                                     */
*/*     THE ACR POST PROCESSOR IS ENTERED ONLY WHEN BOTH CPUS HAVE   */
*/*     ATTAINED THE LOGICALLY ENABLED STATE,(BOTH DISPATCHABLE, NO  */
*/*     GLOBAL LOCKS HELD,AND NON SRB MODE). THIS PHASE OF PROCESSING*/
*/*     WILL ALLOW THE RECOVERY CPU TO CLEAN UP THE SYSTEM SO IT CAN */
*/*     RESUME NORMAL DISPATCHING WITHOUT THE USE OF THE DEAD CPU.   */
*/*                                                                  */
*/* 02* INPUT= PSAPTR (REG 4) - ADDR OF CURR PSA BASED ZERO          */
*/*            RRPLPTR (REG 6) - ADDR OF RECOVERY RTN PARAMETER LIST */
*/*            LCCAPTR (REG 10) - ADDR OF THE CURRENT LCCA           */
*/*                                                                  */
*/* 02* OUTPUT= SAME AS INPUT                                        */
*/*                                                                  */
*/* ******************************************************************/
*                                                                  0352
*             RFY                                                  0352
*              (R3,                                                0352
*               R2,                                                0352
*               R1) RSTD;                                          0352
*                                                                  0352
*             /*******************************************************/
*             /*                                                     */
*             /* SET UP FRR PROTECTION FOR THE POST-PROCESSING PHASE */
*             /*                                                     */
