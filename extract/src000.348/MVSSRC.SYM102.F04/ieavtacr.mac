         TITLE 'IEAVTACR                                               *
                        '
IEAVTACR CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(15)                                             0001
         DC    C'IEAVTACR 77129 '                                  0001
         DROP  @15
@PROLOG  BALR  @05,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@05                                         0001
*   DO;                                                            0170
*                                                                  0170
*/* ******************************************************************/
*/*                                                                  */
*/* 01* SEGMENT NAME   ACRINIT1                                      */
*/*                                                                  */
*/* 02* FUNCTION                                                     */
*/*     ACR INITIALIZATION SEGMENT FOR ALL ENTRIES VIA CALLRTM       */
*/*                                                                  */
*/* 02* INPUT= ENTCDREG(REG 0) - ENTRY TYPE - IDENTIFIER CODE        */
*/*                            * TYPE=ACR           8                */
*/*                            * TYPE=ACRLM         4                */
*/*                            * TYPE=ACRDISP       2                */
*/*            LRBPTR (REG 1) - ENTRY TYPE  -    CONTENTS            */
*/*                           * TYPE=ACR       LOGREC BUFFER ADDRESS */
*/*                           * TYPE=ACRLM     UNKNOWN               */
*/*                           * TYPE=ACRDISP   UNKNOWN               */
*/*            IHRETPTR(REG 2) - ENTRY TYPE  -    CONTENTS           */
*/*                           * TYPE=ACR ONLY  RETURN PT             */
*/*            R14 (REG 14) -  ENTRY TYPE                            */
*/*                           * TYPE=ACRLM AND ACRDISP -RETURN PT    */
*/*                                                                  */
*/* 02* OUTPUT= PSAPTR (REG 4) - CURRENT PSA ADDRESS (BASED ZERO)    */
*/*             RRPLPTR (REG 6) - RECOVERY RTN PARM LIST ADDRESS     */
*/*             LCCAPTR (REG 10) - CURRENT LCCA ADDRESS              */
*/*                                                                  */
*/* ******************************************************************/
*                                                                  0171
*ACRSTART:                                                         0171
*     R4=0;                         /* SET BASE FOR PSA              */
ACRSTART SLR   R4,R4                                               0171
*     R10=PSALCCAV;                 /* INITIALIZE CURRENT LCCA BASE  */
         L     R10,PSALCCAV(,R4)                                   0172
*     LCCACREX=R0;                  /* SET THE ACR ENTRY FLAGS       */
         STC   R0,LCCACREX(,R10)                                   0173
*     RESPECIFY                                                    0174
*      (R6) RSTD;                                                  0174
*     R6=ADDR(ACRRRPL);             /* SET THE RECOVERY PARM ADDRESS */
         L     @15,LCCACPUS(,R10)                                  0175
         L     @15,WSACACR(,@15)                                   0175
         LA    R6,ACRRRPL(,@15)                                    0175
*     IF LCCACRIN='1'B THEN         /* IS THIS PRE-PROCESSING        */
         TM    LCCACRIN(R10),B'00001000'                           0176
         BNO   @RF00176                                            0176
*       DO;                         /* YES, PRE-PROCESSING           */
*         ACRRRPLP=R6;              /* YES, SET RRPL ADDR IN CURRENT
*                                      CPUS ACRSA                    */
         ST    R6,ACRRRPLP(,@15)                                   0178
*         ACRENTR1=R1;              /* TEMP SAVE AREA FOR LRB        */
         ST    R1,ACRENTR1(,@15)                                   0179
*       END;                                                       0180
*     ELSE                          /* NOT PRE-PROCESSING PHASE      */
*       LCCARCPU->LCCACPUS->WSACACR->ACRRRPLP=R6;/* SET RRPL ADDR IN
*                                      RECOVERY CPUS ACRSA           */
         B     @RC00176                                            0181
@RF00176 L     @15,LCCARCPU(,R10)                                  0181
         L     @15,LCCACPUS(,@15)                                  0181
         L     @15,WSACACR(,@15)                                   0181
         ST    R6,ACRRRPLP(,@15)                                   0181
*     RRPL=''B;                     /* CLEAR THE RRPL                */
@RC00176 XC    RRPL(64,R6),RRPL(R6)                                0182
*     GEN REFS(RRPLREGS)(STM 0,14,RRPLREGS(R6));/* SAVE REGS FOR   0183
*                                      RECURSION                     */
         STM 0,14,RRPLREGS(R6)
*     PSAACR='1'B;                  /* SET ACR SUPER BIT TO CAUSE RTM
*                                      TO DIRECT BRANCH TO ACR     0184
*                                      RECOVERY ROUTINE              */
         OI    PSAACR(R4),B'00000100'                              0184
*     RESPECIFY                                                    0185
*      (R0,                                                        0185
*       R1,                                                        0185
*       R2,                                                        0185
*       R14) UNRESTRICTED;                                         0185
*                                                                  0185
*/* ******************************************************************/
*/*  END OF  INITIALIZATION SEGMENT - ACRINIT1                       */
*/* ******************************************************************/
*                                                                  0186
*   END;                                                           0186
*RSTRT00:                           /* RETRY ENTRY FOR LOGICAL PHASE
*                                      IDS (00,01,02,03,04)          */
*   R10->LCCACPUS->WSACACR->ACRRTNPT=RRPLRRGS(14+1);/* SAVE LOCK MGR
*                                      AND SPIN RETURN POINT (REG 14
*                                      ON ENTRY)                     */
RSTRT00  L     @12,LCCACPUS(,R10)                                  0187
         L     @12,WSACACR(,@12)                                   0187
         L     @08,RRPLRRGS+56(,R6)                                0187
         ST    @08,ACRRTNPT(,@12)                                  0187
*   IF LCCACRIN='1'B THEN           /* IS ENTRY FROM THE EMS OR MFA
*                                      SLIH?                         */
         TM    LCCACRIN(R10),B'00001000'                           0188
         BNO   @RF00188                                            0188
*     DO;                                                          0189
*       R10->LCCACPUS->WSACACR->ACRRTNPT=RRPLRRGS(2+1);/* IN THE CASE
*                                      OF SLIH ENTRY SAVE REG 2 FOR
*                                      THE RETURN POINT              */
         L     @08,RRPLRRGS+8(,R6)                                 0190
         ST    @08,ACRRTNPT(,@12)                                  0190
*/* ******************************************************************/
*/* ******************************************************************/
*/* *********************    ACR PRE-PROCESSING     ******************/
*/* ******************************************************************/
*/* ******************************************************************/
*                                                                  0191
*       DO;                         /* EMS/MFA SLIH ENTRY            */
*/* ******************************************************************/
*/*                                                                  */
*/* 01* SEGMENT-NAME  ACRPREP                                        */
*/*                                                                  */
*/* 02* FUNCTION                                                     */
*/*     THE ACR PREPROCESSOR PHASE IS ENTERED VIA THE MFA OR EMS     */
*/*     EXTERNAL SLIH AS A RESULT OF A CPU MALFUNCTION.  REGISTER 1  */
*/*     CONTAINS THE LOGREC BUFFER ADDRESS ASSOCIATED WITH THE DEAD  */
*/*     CPU'S FAILURE.  THE ACR PREPROCESSOR IS AN EXTERNAL INTERRUPT*/
*/*     ROUTINE WHICH WILL MAKE THE ACR CONDITION KNOWN TO THE SYSTEM*/
*/*     AND PRESERVE THE NECESSARY DATA FOR ACR RECOVERY DURING LATER*/
*/*     PROCESSING.  THIS PHASE WILL RETURN CONTROL TO THE EMS SLIH, */
*/*     OR TO THE EXTERNAL FLIH IN CASES OF AN MFA.  WHEN THE        */
*/*     INTERRUPTED PROCESS RELINQUISHES CONTROL AGAIN, EITHER ON ITS*/
*/*     OWN ACCORD, OR DUE TO SOME CONTROL PROGRAM ACTION, ACR'S     */
*/*     INTERMEDIATE PROCESSING WILL GAIN CONTROL TO INITIATE SYSTEM */
*/*     RECOVERY.                                                    */
*/*                                                                  */
*/* 02* INPUT= PSAPTR (REG 4) - ADDR OF CURR PSA BASED ZERO          */
*/*            RRPLPTR (REG 6) - ADDR OF RECOVERY RTN PARAMETER LIST */
*/*            LCCAPTR (REG 10) - ADDR OF THE CURRENT LCCA           */
*/*                                                                  */
*/* 02* OUTPUT= PSAPTR (REG 4) - ADDR OF CURR PSA BASED ZERO         */
*/*            RRPLPTR (REG 6) - ADDR OF RECOVERY RTN PARAMETER LIST */
*/*            PSAPTR2 (REG 9) -  ADDR OF THE DEAD PSA               */
*/*            LCCAPTR (REG 10) - ADDR OF THE CURRENT LCCA           */
*/*            LCCAPTR2 (REG 11) - ADDR OF THE DEAD LCCA             */
*/*                                                                  */
*/* ******************************************************************/
*                                                                  0192
*         DO;                                                      0192
*                                                                  0192
*           /*********************************************************/
*           /*                                                       */
*           /* RECOVERY NOTE: POSSIBLE PHASE IDS AT THIS POINT ARE   */
*           /* (00,01). AN ERROR IN THIS PHASE IS RETRIED AT LABEL   */
*           /* RSTRT00.                                              */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0193
*           IF LOGPHID<'01'X THEN   /* IS ENTRY DUE TO A RETRY FOR AN
*                                      ERROR IN THIS PHASE           */
         CLI   LOGPHID(R6),X'01'                                   0193
         BNL   @RF00193                                            0193
*             RRPLFLGS='0'B;        /* NO,RESET ANY PREVIOUS PHASE 0194
*                                      ERROR INDICATIONS             */
         MVI   RRPLFLGS(R6),B'00000000'                            0194
*           LOGPHID='01'X;          /* SET LOGICAL PHASE ID 01       */
@RF00193 MVI   LOGPHID(R6),X'01'                                   0195
*         END;                                                     0196
*                                                                  0196
*         /***********************************************************/
*         /*                                                         */
*         /* SET THE DEAD CPU OFFLINE TO THE SYSTEM                  */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0197
*         RESPECIFY                                                0197
*          (CSD) BASED(R7);         /* TEMP BASE FOR CSD             */
*         RESPECIFY                                                0198
*          (R7) RSTD;                                              0198
*         R7=CVTCSD;                /* SET CSD ADDR IN CSDPTR        */
         L     @12,CVTPTR(,R4)                                     0199
         L     R7,CVTCSD(,@12)                                     0199
*         IF RRPLOL='0'B            /* HAS THE CPU ALREADY BEEN    0200
*                                      MARKED NOT ALIVE BY ACR?      */
*           THEN                                                   0200
         TM    RRPLOL(R6),B'01000000'                              0200
         BNZ   @RF00200                                            0200
*           DO;                     /* NO, MARK THE CPU DEAD         */
*             RESPECIFY                                            0202
*              (R2,                                                0202
*               R3) RSTD;                                          0202
*                                                                  0202
*             /*******************************************************/
*             /*                                                     */
*             /* PREVENT FURTHER JOB SCHEDULING TO THE DEAD CPU      */
*             /*                                                     */
*             /*******************************************************/
*                                                                  0203
*             R2=SKEDMSK;           /* PICK UP THE CURR JS MASK      */
         L     R2,SKEDMSK(,R7)                                     0203
*SWAPSKED:                          /* RETRY PT IF CS FAILS          */
*             SWAPWORD='FFFFFFFF'X; /* SET ALL BITS ON IN WORD       */
SWAPSKED L     @12,LCCACPUS(,R10)                                  0204
         L     @12,WSACACR(,@12)                                   0204
         MVC   SWAPWORD(4,@12),@CF03111                            0204
*             SWAPJS=SWAPJS&&BITS(PSASPAD+1);/* TURN OFF THE DEAD CPU
*                                      BIT                           */
         LH    @08,PSASPAD(,R4)                                    0205
         ALR   @08,@08                                             0205
         LA    @01,BITS(@08)                                       0205
         XC    SWAPJS(2,@12),0(@01)                                0205
*             R3=SWAPWORD&R2;       /* BLD NEW MSK LESS DEAD CPU     */
         L     R3,SWAPWORD(,@12)                                   0206
         NR    R3,R2                                               0206
*             CS(R2,R3,SKEDMSK);    /* CHANGE THE CSD JS MASK        */
         CS    R2,@03,SKEDMSK(R7)                                  0207
*             BC(7,SWAPSKED);       /* MASK CHANGED BEFORE SWAP      */
*                                                                  0208
         BC    7,SWAPSKED                                          0208
*             /*******************************************************/
*             /*                                                     */
*             /* PREVENT FURTHER SYSTEM REFERENCE TO THE DEAD CPU    */
*             /*                                                     */
*             /*******************************************************/
*                                                                  0209
*             R2=ALIVEMSK;          /* PICK UP CURR ALIVE MASK       */
         L     R2,ALIVEMSK(,R7)                                    0209
*SWAPALIV:                          /* RETRY PT IF CS FAILS          */
*             SWAPWORD='FFFFFFFF'X; /* SET ALL BITS ON IN SWAPWORD   */
SWAPALIV L     @12,LCCACPUS(,R10)                                  0210
         L     @12,WSACACR(,@12)                                   0210
         MVC   SWAPWORD(4,@12),@CF03111                            0210
*             SWAPAL=SWAPAL&&BITS(PSASPAD+1);/* RESET THE DEAD CPU'S
*                                      ALIVE MASK INDICATOR          */
         LH    @08,PSASPAD(,R4)                                    0211
         ALR   @08,@08                                             0211
         LA    @01,BITS(@08)                                       0211
         XC    SWAPAL(2,@12),0(@01)                                0211
*             SWAPWORD=SWAPWORD&R2; /* CHANGE MASK FOR COMP&SWAP     */
         L     @08,SWAPWORD(,@12)                                  0212
         NR    @08,R2                                              0212
         ST    @08,SWAPWORD(,@12)                                  0212
*             SWAPOL=R2-1;          /* DECREMENT THE ONLINE CPU CT   */
         LR    @08,R2                                              0213
         BCTR  @08,0                                               0213
         STH   @08,SWAPOL(,@12)                                    0213
*             R3=SWAPWORD;          /* LOAD SWAP REGISTER            */
         L     R3,SWAPWORD(,@12)                                   0214
*             CS(R2,R3,ALIVEMSK);   /* CHANGE THE CSD ALIVE MSK      */
         CS    R2,@03,ALIVEMSK(R7)                                 0215
*             BC(7,SWAPALIV);       /* MASK CHANGED BEFORE SWAP      */
         BC    7,SWAPALIV                                          0216
*             RRPLOL='1'B;          /* SET CT UPDATED BIT FOR FRR    */
         OI    RRPLOL(R6),B'01000000'                              0217
*             RESPECIFY                                            0218
*              (R2,                                                0218
*               R3) UNRSTD;                                        0218
*           END;                                                   0219
*                                                                  0219
*         /***********************************************************/
*         /*                                                         */
*         /* OBTAIN THE DEAD CPU'S SYSTEM CONTROL BLOCKS             */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0220
*         R9=PCCAT00P(PSASPAD+1)->PCCAPSAV;/* GET THE DEAD PSA ADDR  */
@RF00200 LH    @12,PSASPAD(,R4)                                    0220
         SLA   @12,2                                               0220
         L     @01,CVTPTR(,R4)                                     0220
         L     @01,CVTPCCAT(,@01)                                  0220
         L     @12,PCCAT00P(@12,@01)                               0220
         L     R9,PCCAPSAV(,@12)                                   0220
*         R11=R9->PSALCCAV;         /* GET THE DEAD LCCA ADDRESS     */
*                                                                  0221
         L     R11,PSALCCAV(,R9)                                   0221
*         /***********************************************************/
*         /*                                                         */
*         /* SET THE SYSTEM ACR IN PROGRESS INDICATOR IN THE CSD     */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0222
*         IF RRPLCSD='0'B THEN      /* HAS ACR ALREADY SET SYSTEM ACR
*                                      INDICATOR BYTE                */
         TM    RRPLCSD(R6),B'00100000'                             0222
         BNZ   @RF00222                                            0222
*           DO;                     /* NO                            */
*             R9->PSAPCCAV->PCCARMS='0'B;/* MARK RMS EMS RECEIVED    */
         L     @12,PSAPCCAV(,R9)                                   0224
         NI    PCCARMS(@12),B'11111110'                            0224
*             TS(CSDACR);           /* TEST AND SET THE SYSTEM ACR 0225
*                                      INDICATION                    */
         TS    CSDACR(R7)                                          0225
*             BC(7,STERM);          /* IF ACR IS ALREADY IN PROG-  0226
*                                      RESS WITHIN THE 'SYSTEM',   0226
*                                      TERMINATE THE 'SYSTEM'        */
         BC    7,STERM                                             0226
*             RRPLCSD='1'B;         /* SET THE ACR SET BIT FOR FRR   */
         OI    RRPLCSD(R6),B'00100000'                             0227
*           END;                                                   0228
*         RESPECIFY                                                0229
*          (CSD) BASED(CVTCSD);                                    0229
@RF00222 DS    0H                                                  0230
*         RESPECIFY                                                0230
*          (R7) UNRSTD;                                            0230
*                                                                  0230
*         /***********************************************************/
*         /*                                                         */
*         /* INITIALIZE THE LCCA ACR FIELDS OF THE RECOVERY CPU      */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0231
*         LCCARCPU=R10;             /* SET THE RECOVERING CPU FIELD
*                                      IN THE RECOVERY CPU'S LCCA    */
         ST    R10,LCCARCPU(,R10)                                  0231
*         LCCADCPU=R11;             /* SET THE 'DEAD' CPU FIELD IN 0232
*                                      THE RECOVER CPU'S LCCA        */
         ST    R11,LCCADCPU(,R10)                                  0232
*         LCCACRFL=LCCACRFL&'01'X;  /* CLEAR THE ACR FLAGS IN THE  0233
*                                      RECOVERY CPU LCCA             */
         NI    LCCACRFL(R10),X'01'                                 0233
*         LCCAACR='1'B;             /* SET THE ACR IN PROGRESS FLAG
*                                      IN THE RECOVERY LCCA          */
*                                                                  0234
         OI    LCCAACR(R10),B'10000000'                            0234
*         /***********************************************************/
*         /*                                                         */
*         /* INITIALIZE THE LCCA ACR FIELDS OF THE DEAD CPU'S LCCA   */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0235
*         R11->LCCARCPU=R10;        /* SET THE RECOVERING CPU FIELD
*                                      IN THE DEAD CPU'S LCCA        */
         ST    R10,LCCARCPU(,R11)                                  0235
*         R11->LCCADCPU=R11;        /* SET THE 'DEAD' CPU FIELD IN 0236
*                                      THE DEAD CPU'S LCCA           */
         ST    R11,LCCADCPU(,R11)                                  0236
*         R11->LCCACREX=LCCACREX;   /* PROPAGATE THE ENTRY FLAGS     */
         MVC   LCCACREX(1,R11),LCCACREX(R10)                       0237
*         R11->LCCACRFL=LCCACRFL;   /* CLEAR THE ACR FLAGS IN THE  0238
*                                      DEAD CPU'S LCCA               */
         MVC   LCCACRFL(1,R11),LCCACRFL(R10)                       0238
*         R11->LCCAACR='1'B;        /* SET ACR IN PROGRESS FLAG IN 0239
*                                      THE DEAD CPU'S LCCA           */
*                                                                  0239
         OI    LCCAACR(R11),B'10000000'                            0239
*         /***********************************************************/
*         /*                                                         */
*         /* SAVE THE MACHINE CHECK LOGREC ERROR BUFFER PASSED AS    */
*         /* INPUT                                                   */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0240
*         R11->LCCACPUS->WSACACR->ACRMCHEB=LCCACPUS->WSACACR->ACRENTR1;
*                                                                  0240
         L     @12,LCCACPUS(,R11)                                  0240
         L     @12,WSACACR(,@12)                                   0240
         L     @08,LCCACPUS(,R10)                                  0240
         L     @08,WSACACR(,@08)                                   0240
         L     @07,ACRENTR1(,@08)                                  0240
         ST    @07,ACRMCHEB(,@12)                                  0240
*         /***********************************************************/
*         /*                                                         */
*         /* OBTAIN THE DEAD CPU'S PSA ENVIRONMENT AT TIME OF ERROR  */
*         /* AND SAVE IT IN THE CPU RELATED ACR WORK SAVE AREA OF THE*/
*         /* DEAD CPU                                                */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0241
*         RESPECIFY                                                0241
*          (R13) RSTD;                                             0241
*         R13=WSACACR;              /* SET ACR SAVE AREA ADDRESS     */
         LR    R13,@08                                             0242
*         CALL ACRPROSW;                                           0243
         BAL   @14,ACRPROSW                                        0243
*         RESPECIFY                                                0244
*          (R13) UNRSTD;                                           0244
*                                                                  0244
*         /***********************************************************/
*         /*                                                         */
*         /* SET THE RETURN PATH TO THE EXTERNAL FLIH OR SLIH        */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0245
*         LCCACREF='1'B;            /* SET THE EXTERNAL EXIT FLAG    */
         OI    LCCACREF(R10),B'10000000'                           0245
*STERM:                                                            0246
*                                                                  0246
*         /***********************************************************/
*         /*                                                         */
*         /* THIS SECTION WILL ROUTE CONTROL TO THE SYSTEM           */
*         /* TERMINATION ROUTINE IF ACR HAS DETECTED A CONDITION     */
*         /* WHERE ANOTHER ACR PROCESS IS ALREADY IN PROGRESS WHEN AN*/
*         /* MFA OR EMS WAS RECEIVED TO INITIATE ACR RECOVERY FOR A  */
*         /* CPU.                                                    */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0246
*         IF LCCACREF='0'B THEN     /* IS THE EXTERNAL EXIT FLG SET  */
STERM    TM    LCCACREF(R10),B'10000000'                           0246
         BNZ   @RF00246                                            0246
*           DO;                     /* EXTERNAL EXIT FLAG NOT SET    */
*/* ******************************************************************/
*/*                                                                  */
*/* 01* SEGMENT-NAME  ACRSTERM                                       */
*/*                                                                  */
*/* 02*  FUNCTION:                                                   */
*/*     ACR SYSTEM TERMINATION INTERFACE ROUTINE - PREPARES THE      */
*/*     NECESSARY INTERFACE TO THE SYSTEM TERMINATION FACILITY.  IT  */
*/*     BUILDS THE WTO MESSAGE AND A TWO WORD PARAMETER LIST.  WORD 1*/
*/*     OF THE PARAMETER LIST CONTAINS THE REAL ADDRESS OF THE MSG   */
*/*     AREA, WORD 2 CONTAINS THE REAL ADDRESS OF THE LOGREC BUFFER  */
*/*     PASSED TO ACR BY RMS.  THIS ROUTINE WILL ALSO PLACE THE WAIT */
*/*     STATE CODE OF 050 INTO THE LOGREC BUFFER TO BE USED BY THE   */
*/*     SYSTEM TERMINATION FACILITY WAIT STATE.                      */
*/*                                                                  */
*/* 02* INPUT= RRPLPTR (REG 6) - RRPL PARAMETER LIST ADDRESS         */
*/*            PSAPTR  (REG 4) - RECOVERY CPU PSA PTR                */
*/*            PSAPTR2 (REG 9) - DEAD CPU PSA PTR                    */
*/*            LCCAPTR (REG 10) - RECOVERY CPU LCCA ADDRESS          */
*/*            LCCAPTR2 (REG 11) - DEAD CPU LCCA ADDRESS             */
*/*                                                                  */
*/* 02* USAGE= WPLPTR (REG 8),WRKPTR (REG 7)                         */
*/*                                                                  */
*/* 02* OUTPUT= SYSTEM TERMINATION PARAMETER LIST ADDRESS (REAL)HAS  */
*/*             BEEN PLACED IN THE RRPL FOR THE EXIT PROCESSOR       */
*/*                                                                  */
*/* ******************************************************************/
*                                                                  0248
*             RESPECIFY                                            0248
*              (R8,                                                0248
*               R7,                                                0248
*               R1,                                                0248
*               R2) RESTRICTED;                                    0248
*                                                                  0248
*             /*******************************************************/
*             /*                                                     */
*             /* SET WAIT STATE CODE 050 IN THE LOGREC BUFFER FOR    */
*             /* SYSTERM                                             */
*             /*                                                     */
*             /*******************************************************/
*                                                                  0249
*             R1=LCCACPUS->WSACACR->ACRENTR1;/* GET LOGREC BUFFER  0249
*                                      ADDR                          */
         L     @12,LCCACPUS(,R10)                                  0249
         L     @12,WSACACR(,@12)                                   0249
         L     R1,ACRENTR1(,@12)                                   0249
*             LRBMWSC=COD050;       /* PLACE WAIT CODE IN LRB        */
         MVC   LRBMWSC(4,R1),@CF02839                              0250
*             LRBHTYPE='81'X;       /* INDICATE SYSTEM TERMINATION 0251
*                                      RECORD TYPE           @YM04283*/
*                                                                  0251
         MVI   LRBHTYPE(R1),X'81'                                  0251
*             /*******************************************************/
*             /*                                                     */
*             /* BUILD THE OUTPUT MESSAGE -- IEA856W                 */
*             /*                                                     */
*             /*******************************************************/
*                                                                  0252
*             R8=R11->LCCACPUS->WSACACR;/* SET DEAD CPU ACRSA BASE   */
         L     @12,LCCACPUS(,R11)                                  0252
         L     R8,WSACACR(,@12)                                    0252
*             R7=ADDR(R8->ACRACRWA);/* POINT TO PARM LIST AREA       */
         LA    R7,ACRACRWA(,R8)                                    0253
*             R8->TXT856W(1:LENGTH(MSG856W))=MSG856W;/* MOVE MSG     */
         MVC   TXT856W(49,R8),MSG856W                              0254
*             R2=PSASPAD;           /* GET DEAD CPU ADDRESS          */
         LH    R2,PSASPAD(,R4)                                     0255
*             GENERATE REFS(R2,CVDDWD,R7);                         0256
    CVD R2,CVDDWD(R7)             CONVERT DEAD CPU ADDRESS
*             UNPK(R8->CPUADDRZ,CVDDWD(7:8));/* DEAD CPU ADDR TO MSG */
         UNPK  CPUADDRZ(2,R8),CVDDWD+6(2,R7)                       0257
*             R8->CPUADDRZ(2)=R8->CPUADDRZ(2)×'F0'X;/* MAKE PRINTABLE*/
*                                                                  0258
         OI    CPUADDRZ+1(R8),X'F0'                                0258
*             /*******************************************************/
*             /*                                                     */
*             /* BUILD SYSTERM PARAMETER LIST                        */
*             /*                                                     */
*             /*******************************************************/
*                                                                  0259
*             GENERATE REFS(R8,TXT856W,R1);                        0259
        LRA R8,TXT856W(R8)       OBTAIN REAL ADDRESS OF MSG
