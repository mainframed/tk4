*/*                                                                  */
*/* ******************************************************************/
*                                                                  0272
*         IF R10=LCCADCPU THEN      /* IF THE DEAD CPU IN CONTROL    */
         C     R10,LCCADCPU(,R10)                                  0272
         BNE   @RF00272                                            0272
*           R11=LCCARCPU;           /* SET OTHER LCCA BASE ADDRESS TO
*                                      THE RECOVERY LCCA ADDRESS     */
         L     R11,LCCARCPU(,R10)                                  0273
*         ELSE                      /* RECOVERY CPU IS IN CONTROL    */
*           R11=LCCADCPU;           /* SET OTHER LCCA BASE TO THE  0274
*                                      DEAD LCCA ADDRESS             */
         B     @RC00272                                            0274
@RF00272 L     R11,LCCADCPU(,R10)                                  0274
*         R11->LCCACREX=LCCACREX;   /* PROPOGATE ENTRY FLAGS TO LCCA2*/
@RC00272 MVC   LCCACREX(1,R11),LCCACREX(R10)                       0275
*         R9=                       /* OBTAIN OTHER CPU PHYSICAL ADDR*/
*             R11->LCCACPUS->WSACACR->ACRCPUAD;                    0276
         L     @12,LCCACPUS(,R11)                                  0276
         L     @12,WSACACR(,@12)                                   0276
         LH    R9,ACRCPUAD(,@12)                                   0276
         N     R9,@CF03159                                         0276
*         R9=                       /* OBTAIN OTHER PSA VIRTUAL ADDR */
*             PCCAT00P(R9+1)->PCCAPSAV;                            0277
