         LR    @07,R0                                              0683
         MVC   RUB4(4,@07),FRRSAPTR(@08)                           0683
*   DO;                             /* SETRP RC(4)RETADDR(RETRYPT)RET
*                                      REGS(YES)RUB(RUB)             */
*     RESPECIFY                                                    0685
*      (GPR00P,                                                    0685
*       GPR01P,                                                    0685
*       GPR14P,                                                    0685
*       GPR15P) RSTD;                                              0685
