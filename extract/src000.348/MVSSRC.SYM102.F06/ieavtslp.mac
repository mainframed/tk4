*   /*****************************************************************/
*   /*                                                               */
*   /* TO PREVENT ALTERATION OF THE CHAIN OF SCE INCREMENT THE USE   */
*   /* COUNTS FOR THE FORWARD POINTERS TO EACH STARTING AT THE SHDR. */
*   /* THIS MEANS THE USE COUNT IN THE SHDR AND ALL OF THE SCE EXCEPT*/
*   /* THE LAST ONE WILL BE INCREMENTED. IF THE SCE CHAIN IS IN USE  */
*   /* BY THIS MODULE IN ANOTHER ADDRESS SPACE THERE WILL BE NO      */
*   /* CONFLICT. IF IT IS IN USE BY IEECB905 (ADD OR DELETE AN SCE)  */
*   /* INCREMENTING THE USE COUNT WILL CAUSE IEECB905 TO ABORT ITS   */
*   /* PROCESSING                                                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0182
*   SCEPTR=ADDR(SHDR);              /* THE HEADER IS KNOWN TO EXIST
*                                      BECAUSE THAT IS A CONDITION 0182
*                                      FOR CALLING THIS MODULE. SHDR
*                                      ALSO SERVES AS A DUMMY SCE TO
*                                      BEGIN THE CHAIN               */
@RC00136 DS    0H                                                  0182
@RC00110 L     @01,CVTPTR                                          0182
         L     SCEPTR,CVTRTMS(,@01)                                0182
*   DO WHILE(SCEFWD^=0);            /* LOOP FORWARD THROUGH THE CHAIN*/
         B     @DE00183                                            0183
@DL00183 DS    0H                                                  0184
*     IF SCEFWD->SCECBID^=IDSCE×    /* VALIDATE NEXT SCE             */
*         SCEFWD->SCEBKWD^=SCEPTR THEN                             0184
         L     @15,SCEFWD(,SCEPTR)                                 0184
         CLC   SCECBID(4,@15),@CC03846                             0184
         BNE   @RT00184                                            0184
         C     SCEPTR,SCEBKWD(,@15)                                0184
         BE    @RF00184                                            0184
@RT00184 DS    0H                                                  0185
*       DO;                         /* IF SCE INVALID, REPLACE PTR TO
*                                      IT WITH PTR TO VALID SCE BY 0185
*                                      STARTING WITH THE SCE POINTED
*                                      TO VIA THE SHDRBKWD AND     0185
*                                      BACKING UP THE CHAIN UNTIL AN
*                                      INVALID SCE IS FOUND OR THE 0185
*                                      SCE SCEPTR IS FOUND           */
*         RFY                                                      0186
*          (BCKSCE,                                                0186
*           NXTBKSCE) RSTD;                                        0186
*         NXTBKSCE=SHDRBKWD;        /* START AT LAST SCE             */
         L     @07,CVTPTR                                          0187
         L     @07,CVTRTMS(,@07)                                   0187
         L     NXTBKSCE,SHDRBKWD(,@07)                             0187
*         BCKSCE=0;                                                0188
         SLR   BCKSCE,BCKSCE                                       0188
*         DO WHILE NXTBKSCE->SCECBID=IDSCE&/* FOLLOW BACKWORD CHAIN
*                                      UNTIL                         */
*               NXTBKSCE->SCEFWD=BCKSCE&/* INVALID SCE IS FOUND OR 0189
*                                      THE                           */
*               NXTBKSCE^=SCEPTR;   /* SCEPTR SCE IS FOUND           */
         B     @DE00189                                            0189
@DL00189 DS    0H                                                  0190
*           BCKSCE=NXTBKSCE;        /* UPDATE PTRS TO NEXT SCE       */
         LR    BCKSCE,NXTBKSCE                                     0190
*           NXTBKSCE=NXTBKSCE->SCEBKWD;                            0191
         L     NXTBKSCE,SCEBKWD(,NXTBKSCE)                         0191
*         END;                                                     0192
@DE00189 CLC   SCECBID(4,NXTBKSCE),@CC03846                        0192
         BNE   @DC00189                                            0192
         C     BCKSCE,SCEFWD(,NXTBKSCE)                            0192
         BNE   @DC00189                                            0192
         CR    NXTBKSCE,SCEPTR                                     0192
         BNE   @DL00189                                            0192
@DC00189 DS    0H                                                  0193
*         SCEFWD=BCKSCE;            /* CONNECT SCES                  */
         ST    BCKSCE,SCEFWD(,SCEPTR)                              0193
*         IF BCKSCE=0 THEN          /* IF AT END OF CHAIN            */
         LTR   BCKSCE,BCKSCE                                       0194
         BNZ   @RF00194                                            0194
*           SHDRBKWD=SCEPTR;        /* MOVE INTO SHDR                */
         L     @08,CVTPTR                                          0195
         L     @08,CVTRTMS(,@08)                                   0195
         ST    SCEPTR,SHDRBKWD(,@08)                               0195
*         ELSE                      /* IF NOT AT END                 */
*           BCKSCE->SCEBKWD=SCEPTR; /* MOVE ADDR INTO SCE            */
         B     @RC00194                                            0196
@RF00194 ST    SCEPTR,SCEBKWD(,BCKSCE)                             0196
*         RFY                                                      0197
*          (BCKSCE,                                                0197
*           NXTBKSCE) UNRSTD;                                      0197
@RC00194 DS    0H                                                  0198
*       END;                                                       0198
*     IF SCEFWD^=0 THEN             /* IF NOT AT END OF CHAIN        */
@RF00184 L     @15,SCEFWD(,SCEPTR)                                 0199
