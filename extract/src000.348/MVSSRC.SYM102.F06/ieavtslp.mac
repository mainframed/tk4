*     DO;                           /* FIRST RESET THE RTS ERROR   0650
*                                      RECOVERY                      */
*/* ******************************************************************/
*/* THIS SEGMENT IS DESIGNED TO RECOVER FROM                         */
*/* A POSSIBLE ERROR OCCURRING WHEN SLIP IS INVOKED FROM             */
*/*  IEAVTRTS.  IT PERFORMS THE FOLLOWING:                           */
*/* 1.REQUESTS RETRY IF THE ERROR TYPE IS NOT RESTART OR DATERR AND  */
*/*   THE CURRENT FRR STACK IS NOT EQUAL TO THE RECOVERY STACK USED  */
*/*   BY IEAVTRTS OR IS NOT EQUAL TO THE INTERRUPTED STACK IEAVTRTS  */
*/*  WAS PROCESSING PRIOR TO INVOKING SLIP                           */
*/* 2.REQUESTS PERCOLATION IF THE ERROR TYPE IS DATERR OR RESTART AND*/
*/*   THE STACK CONDITION DESCRIBED IN 1. EXISTS.                    */
*/* 3.ABORTS RECOVERY BY ACTIVATING(VIA SVC 13) RTM ABORT PROCESSING */
*/*   WHEN THE STACK CONDITION DESCRIBED IN 1. DOES NOT EXIST        */
*/* NOTES------                                                      */
*/*  - REGS(R0,R1,R14,R2) MUST BE RESTRICTED THROUGHOUT THIS FRR     */
*/*  - THE RSVT IN THE PSA IS RESET TO THE ORIGINAL CONDITIONS AT THE*/
*/*    TIME OF THE ORIGINAL ENTRY TO IEAVTRTS FOR THE ERROR          */
*/*    INDEPENDENT OF WHETHER THIS FRR REQUESTS PERCOLATION OR RETRY */
*/*  - RECORDING IS REQUESTED                                        */
*/*  - IF AN ERROR SHOULD OCCUR IN THIS FRR---RTM ABORT PROCESSING   */
*/*    WILL GET CONTROL                                              */
*/* ******************************************************************/
*                                                                  0651
*       RFY                                                        0651
*         FRRS BASED(FRRISTK);      /* ADDRESSABILITY TO STACK RTS 0651
*                                      WAS PROCESSING PRIOR TO SLIP
*                                      CHANGING THEM                 */
*                                                                  0651
*       /*************************************************************/
*       /*                                                           */
*       /* ABORT RECOVERY IF THE RTS RECOVERY STACK AND THE          */
*       /* INTERRUPTED STACK PRIOR TO THE SLIP FAILURE ARE EQUAL TO  */
*       /* THE CURRENT STACK--UNWINDING STACKS SO THAT RETRY OR      */
*       /* PERCOLATE CAN BE ACCOMPLISHED IS IMPOSSIBLE. RESTORATION  */
*       /* OF RTS PROCESSING FOR THE ORIGINAL ERROR CANNOT BE        */
*       /* ACCOMPLISHED                                              */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0652
*       IF PSACSTK=R2               /* IF THE RTS RECOVERY FRRS IS 0652
*                                      CURRENTLY THE SAME AS THE   0652
*                                      CURRENT FRRS                  */
*         THEN                      /* THEN THE FRR STACKS ARE     0652
*                                      CONFUSED                      */
         L     @07,PSACSTK                                         0652
         CR    @07,R2                                              0652
         BNE   @RF00652                                            0652
*         IF PSACSTK=FRRISTK        /* IF THE CURRENT FRRS IS THE  0653
*                                      SAME AS THE ONE INTERRUPTED BY
*                                      SLIP                          */
*           THEN                    /* THEN RECOVERY IS IMPOSSIBLE   */
         C     @07,FRRISTK(,@08)                                   0653
         BNE   @RF00653                                            0653
*           SVC(13);                /* ACTIVATE RTM ABORT PROCESSING */
         SVC   13                                                  0654
*         ELSE                      /* THE FRR STACKS CAN BE UNWOUND.
*                                      FIRST RESET THE RSVT TO THE 0655
*                                      STATE THAT EXISTED PRIOR TO 0655
*                                      THE SLIP FAILURE              */
*           IF PSACSTK=PSAPSTK      /* IF THE CURRENT FRRS IS FOR THE
*                                      PCFLIH                        */
*             THEN                                                 0655
         B     @RC00653                                            0655
@RF00653 CLC   PSACSTK(4),PSAPSTK                                  0655
         BNE   @RF00655                                            0655
*             PSAPSAV=FRRISTK;                                     0656
         L     @08,SDWAPARM(,R1)                                   0656
         L     @08,FRRISTK(,@08)                                   0656
         ST    @08,PSAPSAV                                         0656
*           ELSE                                                   0657
*             IF PSACSTK=PSARSTK    /* IF THE CURRENT FRRS IS FOR  0657
*                                      RESTART                       */
*               THEN                                               0657
         B     @RC00655                                            0657
@RF00655 CLC   PSACSTK(4),PSARSTK                                  0657
         BNE   @RF00657                                            0657
*               PSARSAV=FRRISTK;                                   0658
*                                                                  0658
         L     @08,SDWAPARM(,R1)                                   0658
         L     @08,FRRISTK(,@08)                                   0658
         ST    @08,PSARSAV                                         0658
*       /*************************************************************/
*       /*                                                           */
*       /* FOR CONDITIONS WHERE RECOVERY IS POSSIBLE CHECK TO SEE IF */
*       /* IT WAS A RESTART OR DAT ERROR                             */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0659
*       IF SDWATEXC=ON              /* IF THE ERROR IS A DATERR      */
*           ×SDWARKEY=ON THEN       /* OR A RESTART ERROR THE THIS 0659
*                                      FRR WILL SETUP TO PERCOLATE   */
@RF00657 DS    0H                                                  0659
@RC00655 DS    0H                                                  0659
@RC00653 DS    0H                                                  0659
@RF00652 TM    SDWATEXC(R1),B'00100010'                            0659
         BZ    @RF00659                                            0659
*         DO;                       /* SETUP TO PERCOLATE            */
*           DO;                     /* SETRP RC(0)                   */
*             RESPECIFY                                            0662
*              (GPR00P,                                            0662
*               GPR01P,                                            0662
*               GPR14P,                                            0662
*               GPR15P) RSTD;                                      0662
*             GPR01P->SDWARCDE=0;   /* SAVE RC VALUE                 */
         MVI   SDWARCDE(GPR01P),X'00'                              0663
*             RESPECIFY                                            0664
*              (GPR00P,                                            0664
*               GPR01P,                                            0664
*               GPR14P,                                            0664
*               GPR15P) UNRSTD;                                    0664
*           END;                    /* INDICATE PERCOLATION IN THE 0665
*                                      SDWA                  @ZA17761*/
*           IF FRRISTK^=PSANSTK THEN/* IF THE STACK BEING PROCESSED
*                                      PRIOR TO THE SLIP FAILURE IS A
*                                      SUPER STACK SET UP THIS STACK
*                                      SO PERCOLATION TO THE SUPER 0666
*                                      FRR WILL RESULT IN THE CURRENT
*                                      ERROR BEING GENERATED TO THE
*                                      LAST INTERRUPTED STK          */
         L     @08,SDWAPARM(,R1)                                   0666
         L     @08,FRRISTK(,@08)                                   0666
         C     @08,PSANSTK                                         0666
         BE    @RF00666                                            0666
*             DO;                                                  0667
*               FRRSRCUR=OFF;                                      0668
         L     @07,FRREPTR(,@08)                                   0668
         NI    FRRSRCUR(@07),B'01111111'                           0668
*               FRRSCURR=FRRSEMP;   /* INSURE THAT THE SUPER FRR IS
*                                      CURRENT                       */
         L     @07,FRRSEMP(,@08)                                   0669
         ST    @07,FRRSCURR(,@08)                                  0669
*               RT1TLPN=NONE;       /* PROPOGATE THE ERROR AS NEW    */
         MVI   RT1TLPN+16(@08),X'00'                               0670
*             END;                                                 0671
*           ELSE                    /* IF THE INTERRUPTED STACK PRIOR
*                                      TO THE SLIP FAILURE WAS THE 0672
*                                      NORMAL STACK THEN PROPOGATE 0672
*                                      THE ERROR AS A FRR FAILURE    */
*             RT1TLPN=VALIDREC;     /* INDICATE VALID RECURSION INTO
*                                      RTS                           */
         B     @RC00666                                            0672
@RF00666 L     @08,SDWAPARM(,R1)                                   0672
         L     @08,FRRISTK(,@08)                                   0672
         MVI   RT1TLPN+16(@08),X'FF'                               0672
*           ABFRR=OFF;                                             0673
@RC00666 L     @08,SDWAPARM(,R1)                                   0673
         NI    ABFRR(@08),B'11111110'                              0673
*           SHDRFRR=OFF;            /* IF THE FRR HAS SUCCESSFULLY 0674
*                                      DONE ITS WORK THE TURN OFF  0674
*                                      THIS FLAG TO ALLOW SLIP TO RUN
*                                      AGAIN                         */
         L     @08,CVTPTR                                          0674
         L     @08,CVTRTMS(,@08)                                   0674
         NI    SHDRFRR(@08),B'01111111'                            0674
*           RETURN CODE(0);         /* EXIT WITH PERCOLATION CODE FOR
*                                      DAT OR RESTART ERRORS         */
         SLR   @15,@15                                             0675
@EL00012 DS    0H                                                  0675
@EF00012 DS    0H                                                  0675
@ER00012 BR    @14                                                 0675
*         END;                                                     0676
*       ELSE                                                       0677
*         ;                         /* IF THE ERROR IS NEITHER DATERR
*                                      OR RESTART THEN A RETRY BACK
*                                      TO SLIP WILL BE SETUP         */
@RF00659 DS    0H                                                  0678
*     END;                                                         0678
*                                                                  0678
*   /*****************************************************************/
*   /*                                                               */
*   /* RETRY IN IEAVTSLP AT THE POINT WHERE THE ERROR RECOVERY IS    */
*   /* DELETED. REGISTERS 2,3,12,&13 ARE RESTORED FROM THE FRR       */
*   /* PARMLIST TO BE SURE THAT THE IEAVTSLP EXIT CODE WILL FUNCTION.*/
