         TITLE 'IEAVTSDT - SVC DUMP TASK                               *
                        '
IEAVTSDT CSECT ,                                                   0001
@MAINENT BALR  @15,0                                               0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'IEAVTSDT  77.285'                                 0001
         DROP  @15
@PROLOG  BALR  @09,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@09                                         0001
*   R10=R1;                         /* SAVE ADDR OF PASSED ECB, IF IN
*                                      MASTER ADDRESS SPACE          */
         LR    R10,R1                                              0076
*   IF ASCBASID=MASTASID THEN       /* IF IN THE MASTER ADDR SPACE   */
         L     @12,ASCBPTR                                         0077
         CLC   ASCBASID(2,@12),@CH00067                            0077
         BNE   @RF00077                                            0077
*     DO;                                                          0078
*       GEN(POST (R10),0);          /* POST ATTACHOR                 */
         POST (R10),0
*     END;                                                         0080
*   RFY                                                            0081
*     R7 RSTD;                      /*                       @ZM45196*/
@RF00077 DS    0H                                                  0082
*   RFY                                                            0082
*     SDWORK BASED(R7);             /* MAP SDUMP WORK AREA   @ZM45196*/
*   R7=RTCTSDWK;                    /* GET WORK AREA ADDRESS @ZM45196*/
         L     @12,CVTPTR                                          0083
         L     @12,CVTRTMCT(,@12)                                  0083
         L     R7,RTCTSDWK(,@12)                                   0083
*   RFY                                                            0084
*     R3 RSTD;                      /* RESTRICT ERROR WORK AREA    0084
*                                      POINTER               @ZM45196*/
*   R3=ADDR(ERRORWK);               /* GET ADDRESS ESTAE WORK AREA 0085
*                                                            @ZM45196*/
         LA    R3,ERRORWK(,R7)                                     0085
*   GEN(ESTAE DTESTAE1,PARAM=@PSTART,RECORD=YES,TERM=YES);/* @ZM45621*/
         ESTAE DTESTAE1,PARAM=@PSTART,RECORD=YES,TERM=YES
*   ERRRETRY=ADDR(RECRSME);         /* GET FIRST RETRY ADDRESS     0087
*                                                            @ZM45196*/
         LA    @12,RECRSME                                         0087
         ST    @12,ERRRETRY(,R3)                                   0087
*   ASCBDUMP=PSATOLD;               /* TCB ADDRESS CAN BE REFERENCED
*                                      FROM OTHER ADDRESS SPACES     */
         L     @12,PSATOLD                                         0088
         L     @08,ASCBPTR                                         0088
         ST    @12,ASCBDUMP(,@08)                                  0088
*   RFY                                                            0089
*    (R10,                                                         0089
*     R11) UNRSTD;                                                 0089
*   DO WHILE(ASCBDUMP^=NULL);       /* SERVICE SCHEDULE DUMP REQUESTS
*                                      AS LONG AS DUMP TASK ACTIVE   */
         B     @DE00090                                            0090
@DL00090 DS    0H                                                  0091
*RECRSME:                                                          0091
*     ;                             /* RETRY LOC FOR ERROR EXIT    0091
*                                                            @G33VPHD*/
*                                                                  0091
RECRSME  DS    0H                                                  0092
*     /***************************************************************/
*     /*                                                             */
*     /* OBTAIN LOCAL LOCK AND SUSPEND UNTIL NEXT RESUME     @G33VPHD*/
*     /*                                                             */
*     /***************************************************************/
*                                                                  0092
*     GEN REFS(PSALITA,FLC,FLCCVT,CVT,CVTSUSP);                    0092
*                                                /*GET LOCK  @G33VPHD*/
SETLOCK  SETLOCK   OBTAIN,TYPE=LOCAL,MODE=UNCOND,                      X
               RELATED=(SVCDUMP,IEAVTSDT(FREELOCK))          @G33VPHD
         SUSPEND   RB=CURRENT      SUSPEND DUMP TASK PROCESS @G33VPHD
FREELOCK SETLOCK   RELEASE,TYPE=LOCAL,                                 X
               RELATED=(SVCDUMP,IEAVTSDT(GETLOCK))           @G33VPHD
         CALLDISP  BRANCH=NO                                 @G33VPHD
*DUMPREQ:                                                          0093
*     ;                             /* ENTRY FOR A DUMP REQUEST    0093
*                                                            @G33VPHD*/
DUMPREQ  DS    0H                                                  0094
*/* ******************************************************************/
*/*    THIS SEGMENT WILL HANDLE A SCHEDULED DUMP REQUEST.  AN ESTAE  */
*/*    EXIT WILL BE ESTABLISHED TO PROTECT THE DUMP TASK WHILE IT    */
*/*    HANDLES THE REQUEST.  AN ENQ WILL BE PERFORMED ON THE SDUMP   */
*/*    RESOURCE.  WHEN CONTROL RETURNS, THE AREAS REQUESTED THAT     */
*/*    PERTAIN TO THIS ADDRESS SPACE WILL BE DUMPED USING THE SVC    */
*/*    DUMP MODULES.  AT THE COMPLETION OF THE DUMP THE CALLERS      */
*/*    ECB WILL BE POSTED IF ON IS SUPPLIED.  A DEQ IS PERFORMED     */
*/*    FROM THE SDUMP RESOURCE.                                      */
*/*                                                                  */
*/*    INPUT: RTCTSDPL CONTAINS ADDRESS OF CALLERS SDUMP PARAMETER   */
*/*    LIST.                                                         */
*/*                                                                  */
*/*    OUTPUT: SVC DUMP OF THE REQUESTED AREAS                       */
*/*                                                                  */
*/*    INCLUDED BY: TOP SEGMENT                                      */
*/* ******************************************************************/
*                                                                  0094
*     /***************************************************************/
*     /*                                                             */
*     /* ENQ ON SDUMP RESOURCE SO THAT ONLY ONE ADDRESS SPACE OF A   */
*     /* MULTIPLE ADDRESS SPACE DUMP WILL RUN AT ONE TIME    @G33VPHD*/
*     /*                                                             */
*     /***************************************************************/
*                                                                  0094
*     R1=ADDR(ENQLIST);             /* GET ADDRESS OF ENQ LIST FORM
*                                                            @G33VPHD*/
         LA    R1,ENQLIST                                          0094
*     GEN;                                                         0095
*                                  /*ENQ ON SDUMP RESOURCE   @G33VPHD*/
ENQ1     ENQ   MF=(E,(R1))                                   @G33VPHD
*     ERRRETRY=ADDR(RECEND);        /* GET NEXT RETRY LOCATION     0096
*                                      ADDRESS               @G33VPHD*/
         LA    @12,RECEND                                          0096
         ST    @12,ERRRETRY(,R3)                                   0096
*     ERRSDENQ='1'B;                /* INDICATE ENQD ON SDUMP      0097
*                                      RESOURCE              @G33VPHD*/
         OI    ERRSDENQ(R3),B'00100000'                            0097
*     PSATOLD->TCBEXSVC='1'B;       /* SVC DUMP ACTIVE THIS TCB    0098
*                                                            @ZM45196*/
         L     @12,PSATOLD                                         0098
         OI    TCBEXSVC(@12),B'00000001'                           0098
*     ERRCUDMP='1'B;                /* INDICATE A DUMP CAN BE TAKEN
*                                                            @ZM45196*/
         OI    ERRCUDMP(R3),B'00000100'                            0099
*     ERRSDBIT='1'B;                /* INDICATE SDUMP LOCKED @ZM45196*/
*     ERRBFBIT='1'B;                /* INDICATE SDUMP BUFFER LOCKED
*                                                            @ZM45196*/
*                                                                  0101
         OI    ERRSDBIT(R3),B'00110000'                            0101
*     /***************************************************************/
*     /*                                                             */
*     /* INITIALIZE POINTERS AND SET UP ESTAE EXIT           @G33VPHD*/
*     /*                                                             */
*     /***************************************************************/
*                                                                  0102
*     IF ERRTRMNT='0'B THEN         /* IF NO TERMINATING ERRORS SO 0102
*                                      FAR                   @ZM44940*/
         TM    ERRTRMNT(R3),B'00001000'                            0102
         BNZ   @RF00102                                            0102
*       DO;                         /* ATTEMPT DUMP OF THIS ADDRESS
*                                      SPACE                 @ZM44940*/
*         LSTESTAE=ESTAELST;        /* MOVE LST FORM ESTAE   @G33VPHD*/
         MVC   LSTESTAE(16,R7),ESTAELST                            0104
*         ERRBASE1=R9;              /* SAVE BASE REGISTER CONTENTS 0105
*                                                            @G33VPHD*/
         ST    R9,ERRBASE1(,R3)                                    0105
*         ERRWKADR=R7;              /* SAVE WORK AREA ADDRESS      0106
*                                                            @G33VPHD*/
         ST    R7,ERRWKADR(,R3)                                    0106
*         SVCDMPNM=CSECTNAM;        /* SAVE CSECT NAME FOR RECOVERY
*                                                            @G33VPHD*/
         MVC   SVCDMPNM(8,R7),@CC03098                             0107
*         RFY                                                      0108
*          (R10,                                                   0108
*           R11,                                                   0108
*           R4) RSTD;               /*                       @G33VPHD*/
*         R10=ADDR(ERRORWK);        /* ADDR ESTAE ERROR WORK AREA  0109
*                                                            @G33VPHD*/
         LA    R10,ERRORWK(,R7)                                    0109
*         R11=ADDR(SDWRGSV1);       /* ADDR SVC DUMP SAVE AREA     0110
*                                                            @ZM44940*/
         LA    R11,SDWRGSV1(,R7)                                   0110
*         R4=SDESTAAD;              /* GET SDUMP ESTAE ADDRESS FROM
*                                      VAD00                 @G33VPHD*/
         L     R4,SDESTAAD                                         0111
*         RTCTINDX=0;               /* INITIALIZE INDEX FOR RECOVERY
*                                      TEST                  @ZM45192*/
         L     @12,CVTPTR                                          0112
         L     @12,CVTRTMCT(,@12)                                  0112
         MVI   RTCTINDX(@12),X'00'                                 0112
*         R1=ADDR(LSTESTAE);        /* ADDR OF LIST FORM OF MACRO  0113
*                                                            @G33VPHD*/
         LA    R1,LSTESTAE(,R7)                                    0113
*         GEN REFS(CVTMAP,CVTSV60);                                0114
*                                             /*             @G33VPHD*/
         ESTAE (R4),TERM=YES,RECORD=YES,BRANCH=YES,                    X
               PARAM=(R10),SVEAREA=(R11),MF=(E,(R1))     @G33VPHD
*         ERRESTAE='1'B;            /* INDICATE ESTAE IS ACTIVE    0115
*                                                            @ZM44940*/
         OI    ERRESTAE(R3),B'01000000'                            0115
*         RFY                                                      0116
*          (R10,                                                   0116
*           R11,                                                   0116
*           R4) UNRSTD;             /*                       @G33VPHD*/
*                                                                  0116
*         /***********************************************************/
*         /*                                                         */
*         /* THIS LOOP SEARCHES THE RTCT ARRAY TO FIND THE   @G33VPHD*/
*         /* ENTRY FOR THIS ADDRESS SPACE                    @G33VPHD*/
*         /*                                                         */
*         /***********************************************************/
*                                                                  0117
*         IF RTCTSDMA='1'B&         /* IF THIS IS THE MASTER DUMP  0117
*                                      TASK                  @ZM44937*/
*             ASCBASID=MASTASID THEN/* AND FOR MULTI ASID DUMP     0117
*                                                            @ZM44937*/
         L     @12,CVTPTR                                          0117
         L     @12,CVTRTMCT(,@12)                                  0117
         TM    RTCTSDMA(@12),B'00010000'                           0117
         BNO   @RF00117                                            0117
         L     @08,ASCBPTR                                         0117
         CLC   ASCBASID(2,@08),@CH00067                            0117
         BNE   @RF00117                                            0117
*           RTCTINDX=MAXENTRY;      /* USE ENTRY 16 FOR THIS ASID  0118
*                                      ONLY                  @ZM44937*/
         MVI   RTCTINDX(@12),X'10'                                 0118
*         ELSE                                                     0119
*           DO;                     /* NOT GLOBAL AREA DUMP TASK FIND
*                                      CURRENT ENTRY         @ZM44937*/
         B     @RC00117                                            0119
@RF00117 DS    0H                                                  0120
*             DO RTCTINDX=ONE TO MAXENTRY WHILE(RTCTSDAS(RTCTINDX)^=
*                   ASCBASID);                                     0120
         LA    @12,1                                               0120
         L     @08,CVTPTR                                          0120
         L     @08,CVTRTMCT(,@08)                                  0120
         STC   @12,RTCTINDX(,@08)                                  0120
@DL00120 L     @12,CVTPTR                                          0120
         L     @12,CVTRTMCT(,@12)                                  0120
         SLR   @08,@08                                             0120
         IC    @08,RTCTINDX(,@12)                                  0120
         SLA   @08,2                                               0120
         ALR   @12,@08                                             0120
         L     @08,ASCBPTR                                         0120
         CLC   RTCTSDAS-4(2,@12),ASCBASID(@08)                     0120
         BE    @DC00120                                            0120
*             END;                  /* END OF SEARCH         @G33VPHD*/
         L     @08,CVTPTR                                          0121
         L     @08,CVTRTMCT(,@08)                                  0121
         LA    @12,1                                               0121
         SLR   @06,@06                                             0121
         IC    @06,RTCTINDX(,@08)                                  0121
         ALR   @12,@06                                             0121
         L     @08,CVTPTR                                          0121
         L     @08,CVTRTMCT(,@08)                                  0121
         STC   @12,RTCTINDX(,@08)                                  0121
         C     @12,@CF00127                                        0121
         BNH   @DL00120                                            0121
@DC00120 DS    0H                                                  0122
*             IF RTCTINDX>MAXENTRY THEN/* IF ASID ENTRY NOT FOUND  0122
*                                                            @G33VPHD*/
         L     @12,CVTPTR                                          0122
         L     @12,CVTRTMCT(,@12)                                  0122
         CLI   RTCTINDX(@12),16                                    0122
         BNH   @RF00122                                            0122
*               GEN(ABEND X'C0D',,,SYSTEM);/* BAD DUMP TASK RESUME 0123
*                                                            @G33VPHD*/
         ABEND X'C0D',,,SYSTEM
*           END;                    /* END NOT GLOBAL DUMP TASK FIND
*                                      CURRENT ENTRY LOOP    @ZM44937*/
@RF00122 DS    0H                                                  0125
*         ERRSAVE=RTCTINDX;         /* SAVE RTCT ARRAY INDEX @G33VPHD*/
@RC00117 L     @12,CVTPTR                                          0125
         L     @12,CVTRTMCT(,@12)                                  0125
         MVC   ERRSAVE(1,R3),RTCTINDX(@12)                         0125
*                                                                  0126
*         /***********************************************************/
*         /*                                                         */
*         /* THIS SECTION CHECKS TO SEE THAT THE RESUMPTION OF THE   */
*         /* DUMP TASK IS VALID AND THE DUMP OF THIS ADDRESS SPACE   */
*         /* CAN PROCEED. IF THE RESUMPTION IS VALID THE SVC DUMP    */
*         /* SERVICE ROUTINES ARE CALLED TO PERFORM THE DUMP @G33VPHD*/
*         /*                                                         */
*         /***********************************************************/
*                                                                  0126
*         IF RTCTSDPL^=NULL&        /* IF SVC DUMP IS LOCKED AND   0126
*                                                            @G33VPHD*/
*             RTCTSDSH='1'B&        /* THIS IS SCHEDULE DUMP AND   0126
*                                                            @G33VPHD*/
*             RTCTSDEP='0'B&        /* CALLERS ECB NOT POSTED      0126
*                                                            @G33VPHD*/
*             RTCTSDSS(RTCTINDX)='1'B&/* THE SRB WAS SCHEDULED     0126
*                                                            @G33VPHD*/
*             RTCTSDNC(RTCTINDX)='1'B&/* NON-DISP SRB GOT CONTROL  0126
*                                                            @G33VPHD*/
*             RTCTSDAN(RTCTINDX)='1'B&/* ADDR SPACE SET NONDISP    0126
*                                                            @G33VPHD*/
*             RTCTSDRM(RTCTINDX)='1'B&/* DUMP TASK WAS RESUMED     0126
*                                                            @G33VPHD*/
*             RTCTSDTR(RTCTINDX)='0'B&/* DUMP TASK NOT RUNNING     0126
*                                                            @G33VPHD*/
*             RTCTSDEN(RTCTINDX)='0'B&/* SVC DUMP NOT ENTERED      0126
*                                                            @G33VPHD*/
*             RTCTSDDO(RTCTINDX)='0'B THEN/* DUMP NOT COMPLETE ASID
*                                                            @G33VPHD*/
         SLR   @08,@08                                             0126
         L     @06,RTCTSDPL(,@12)                                  0126
         CR    @06,@08                                             0126
         BE    @RF00126                                            0126
         TM    RTCTSDSH(@12),B'00100000'                           0126
         BNO   @RF00126                                            0126
         TM    RTCTSDEP(@12),B'00001000'                           0126
         BNZ   @RF00126                                            0126
         SLR   @02,@02                                             0126
         IC    @02,RTCTINDX(,@12)                                  0126
         SLA   @02,2                                               0126
         ALR   @02,@12                                             0126
         TM    RTCTSDSS-4(@02),B'11110000'                         0126
         BNO   @RF00126                                            0126
         TM    RTCTSDTR-4(@02),B'00001011'                         0126
         BNZ   @RF00126                                            0126
*           DO;                     /* VALID DUMP TASK RESUME      0127
*                                                            @G33VPHD*/
*             IF ERRENDMP='0'B THEN /* IF NO PREVIOUS ERRORS OK    0128
*                                                            @ZM44943*/
         TM    ERRENDMP(R3),B'01000000'                            0128
         BNZ   @RF00128                                            0128
*               DO;                 /* NO ERRORS SO CONTINUE @ZM44943*/
*                 RTCTSDTR(RTCTINDX)='1'B;/* DUMP TASK RUNNING     0130
*                                                            @G33VPHD*/
*                 RTCTSDEQ(RTCTINDX)='1'B;/* DUMP TASK ENQUED      0131
*                                                            @G33VPHD*/
*                 RFY                                              0132
*                   SDUMP BASED(RTCTSDPL);/* MAP CALLERS PARAMETER 0132
*                                      LIST                  @G33VPHD*/
*                 RTCTSDEN(RTCTINDX)='1'B;/* INDICATE SVC DUMP     0133
*                                      ENTERED               @G33VPHD*/
         L     @02,CVTPTR                                          0133
         L     @02,CVTRTMCT(,@02)                                  0133
         SLR   @15,@15                                             0133
         IC    @15,RTCTINDX(,@02)                                  0133
         SLA   @15,2                                               0133
         ALR   @15,@12                                             0133
         OI    RTCTSDTR-4(@15),B'00001110'                         0133
*                 RTCTSDID=ASCBASID;/* GET CURRENT ASID FOR IEAVTSDR
*                                                            @G33VPHD*/
         L     @02,ASCBPTR                                         0134
         LH    @02,ASCBASID(,@02)                                  0134
         N     @02,@CF03250                                        0134
         STH   @02,RTCTSDID(,@12)                                  0134
*                 SVCDOPTS=RTCTASO; /* GET EFFECTIVE DUMP OPTIONS  0135
*                                                            @G33VPHD*/
         MVC   SVCDOPTS(2,R7),RTCTASO(@12)                         0135
*                 R13=ADDR(SDWRGSV1);/* GET SAVE AREA ADDRESS      0136
*                                                            @G33VPHD*/
         LA    R13,SDWRGSV1(,R7)                                   0136
*                 SDWSDPL=RTCTSDPL; /* GET PARAMETER LIST ADDRESS  0137
*                                                            @G33VPHD*/
*                                                                  0137
         ST    @06,SDWSDPL(,R7)                                    0137
*                 /***************************************************/
*                 /*                                                 */
*                 /* THIS SECTION INITIALIZES KEY WORK AREA FIELDS   */
*                 /* AND THE DUMP DATA SET IF THIS IS THE FIRST OR   */
*                 /* ONLY ADDRESS SPACE TO BE DUMPED         @G33VPHD*/
*                 /*                                                 */
*                 /***************************************************/
*                                                                  0138
*                 IF RTCTSDSL='0'B THEN/* IF DUMP DATA SET NOT     0138
*                                                            @G33VPHD*/
         TM    RTCTSDSL(@12),B'00000100'                           0138
         BNZ   @RF00138                                            0138
*                   DO;             /* SELECTED,SELECT & INITIALIZE
*                                                            @G33VPHD*/
*                     SDRETCOD=RTCTSDPR;/* GET CURRENT RETURN CODE 0140
*                                                            @ZM45074*/
         SLR   @02,@02                                             0140
         IC    @02,RTCTSDPR(,@12)                                  0140
         ST    @02,SDRETCOD(,R7)                                   0140
*                     FLAGSONE=NULLCHAR;/* CLEAR WORK AREA FLAG BYTE
*                                                            @G33VPHD*/
         MVI   FLAGSONE(R7),X'00'                                  0141
*                     FLAGSTWO=NULLCHAR;/* CLEAR WORK AREA FLAG BYTE
*                                                            @G33VPHD*/
         MVI   FLAGSTWO(R7),X'00'                                  0142
*                     SMWKPSSE=NULL;/* ZERO POINTER FIELD FOR SUMDUMP
*                                                            @ZA19201*/
         L     @12,RTCTSDSW(,@12)                                  0143
         ST    @08,SMWKPSSE(,@12)                                  0143
*                     ERRRETRY=ADDR(DTLOC01);/* NEXT RETRY LOCATION
*                                      ADDRESS               @ZM45053*/
         LA    @12,DTLOC01                                         0144
         ST    @12,ERRRETRY(,R3)                                   0144
*                     IF SVCDTRT='1'B×/* IF TRACE REQUESTED OR     0145
*                                      ENABLED               @ZA19201*/
*                         (SDUBRANH='0'B&SVCDNSMD='0'B) THEN/*     0145
*                                      SUMMARY DUMP IS REQUESTED   0145
*                                                            @ZA19210*/
         TM    SVCDTRT(R7),B'00000001'                             0145
         BO    @RT00145                                            0145
         TM    SDUBRANH(@06),B'00000001'                           0145
         BNZ   @RF00145                                            0145
         TM    SVCDNSMD(R7),B'00010000'                            0145
         BNZ   @RF00145                                            0145
@RT00145 DS    0H                                                  0146
*                       DO;         /* SUPERVISOR TRT OR GTF ACTIVE  */
*                         IF CVTTRACE(2)='FA'X THEN/* IF SUPERVISOR
*                                      TRT ACTIVE,           @ZA08114*/
         L     @12,CVTPTR                                          0147
         CLI   CVTTRACE+1(@12),X'FA'                               0147
         BNE   @RF00147                                            0147
*                           DO;     /* STOP IT                       */
*                             RFY                                  0149
*                              (R10,                               0149
*                               R11) RSTD;                         0149
*                             CVTSDTRC='1'B;/* INDICATE SVC DUMP   0150
*                                      STOPPED SUPERVISOR TRACE TABLE*/
         OI    CVTSDTRC(@12),B'00000001'                           0150
*                             R10=TRTACTVE;                        0151
         L     R10,@CF03117                                        0151
*                             R11=TRTSTOP;                         0152
         L     R11,@CF03119                                        0152
*                             CS(R10,R11,TRTCVTWD);/* IF TRACE TAB 0153
*                                      ACTIVE, STOP IT               */
         L     @12,CVTPTR                                          0153
         CS    R10,@11,TRTCVTWD(@12)                               0153
*                             RFY                                  0154
*                              (R10,                               0154
*                               R11) UNRSTD;                       0154
*                             CURRSTOP='1'B;/* INDICATE ON WHICH   0155
*                                      ENTRY THE TRT WAS STOPPED     */
         L     @12,FLCTRACE                                        0155
         L     @12,TRTCURR(,@12)                                   0155
         OI    CURRSTOP(@12),B'10000000'                           0155
*                           END;                                   0156
*                         ELSE                                     0157
*                           ;                                      0157
@RF00147 DS    0H                                                  0158
*                         IF CVTGTFAV='1'B&/* IF GTF ACTIVE, @ZA19201*/
*                             SVCDTRT='1'B THEN/* AND TRACE REQUESTED
*                                      RESERVE TRACE         @ZA19201*/
         L     @12,CVTPTR                                          0158
         TM    CVTGTFAV(@12),B'10000000'                           0158
         BNO   @RF00158                                            0158
         TM    SVCDTRT(R7),B'00000001'                             0158
         BNO   @RF00158                                            0158
*                           DO;     /* BUFFERS FOR DUMP              */
*                             GEN(HOOK  EID=IEASVCD);              0160
         HOOK  EID=IEASVCD
*                             ERRGTFGO='1'B;/* INDICATE WE WENT TO 0161
*                                      GTF                           */
         OI    ERRGTFGO(R3),B'00000100'                            0161
*                           END;                                   0162
*                         ELSE                                     0163
*                           ;                                      0163
@RF00158 DS    0H                                                  0164
*                       END;                                       0164
*                     ELSE                                         0165
*                       ;                                          0165
@RF00145 DS    0H                                                  0166
*                     ERRCSDT='1'B; /* INDICATE SDT CALLED SDH     0166
*                                                            @G33VPHD*/
         OI    ERRCSDT(R3),B'00010000'                             0166
*                     CALL IEAVTSDH;/* CHOOSE AND INITIALIZE DUMP  0167
*                                      DATA SET              @G33VPHD*/
         L     @15,@CV03126                                        0167
         BALR  @14,@15                                             0167
*                     ERRBASE1=R9;  /* RESTORE RECOVERY BASE REGISTER
*                                                            @G33VPHD*/
         ST    R9,ERRBASE1(,R3)                                    0168
*                     SVCDMPNM=CSECTNAM;/* GET CSECT NAME FOR      0169
*                                      RECOVERY              @G33VPHD*/
         MVC   SVCDMPNM(8,R7),@CC03098                             0169
*                     RTCTINDX=ERRSAVE;/* RESTORE SAVED INDEX      0170
*                                                            @G33VPHD*/
         L     @12,CVTPTR                                          0170
         L     @12,CVTRTMCT(,@12)                                  0170
         MVC   RTCTINDX(1,@12),ERRSAVE(R3)                         0170
*                   END;            /*                       @G33VPHD*/
*                 RFY                                              0172
*                   SDUMP BASED(RTCTSDPL);/* MAP SDUMP PARAMETER LIST
*                                                            @ZM45051*/
@RF00138 DS    0H                                                  0173
*                                                                  0173
*/* ******************************************************************/
*/*   THIS SEGMENT HANDLES DUMP TASK SVC DUMP PROCESSING             */
*/*   IN ALL ADDRESS SPACES. IF THERE ARE DISABLED SUMMARY           */
*/*   DUMP RECORDS TO WRITE MODULE IEAVTSDW IS CALLED TO WRITE       */
*/*   THE RECORDS AND FREE THE REAL STORAGE BUFFER. IF AN ENABLED    */
*/*   SUMMARY DUMP IS REQUESTED, MODULE IEAVTSSE IS CALLED TO        */
*/*   DUMP THE ENABLED SUMMARY DUMP ON THE DUMP DATA SET. IF GLOBAL  */
*/*   STORAGE AREAS ARE REQUESTED MODULE IEAVTSDG IS CALLED.         */
*/*   WHEN CONTROL RETURNS FORM IEAVTSDG THE EFFECTIVE               */
*/*   GLOBAL STORAGE OPTIONS ARE ZEROED SO THAT THE GLOBAL           */
*/*   AREAS ARE ONLY DUMPED ONCE. IF LOCAL STORAGE AREAS ARE         */
*/*   REQUESTED MODULE IEAVTSDL IS CALLED. IF NO LOCAL STORAGE       */
*/*   AREAS ARE REQUESTED AND THIS IS A MULTIPLE ADDRESS SPACE DUMP  */
*/*   THE DUMPS IN ALL OTHER REQUESTED ADDRESS SPACES ARE            */
*/*   MARKED COMPLETE.                                               */
*/*                                                                  */
*/*   INPUT: EFFECTIVE DUMP OPTION                                   */
*/*                                                                  */
*/*   OUTPUT: DUMP TAKEN OF REQUESTED AREAS                          */
*/*                                                                  */
*/*   INCLUDED BY: DTREQ                                     @G33VPHD*/
*/*                                                                  */
