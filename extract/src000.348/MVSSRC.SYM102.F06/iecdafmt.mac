*/********************************************************************/
*/* FUNCTION-                                                      0658
*       ROUTINE TO RELEASE THE LOCAL LOCK IF IT IS HELD AND TO TURN
*     OFF THE LOCKHELD BIT IN THE ESTAE PARM LIST.                 0658
*                                                                  0658
*                                                                  0658
*   INPUT -                                                        0658
*         REGISTER 1 POINTS TO THE PARMLIST PASSED BY THE CALLER.  0658
*                                                                  0658
*    CALLS-                                                        0658
*         MAKES NO OUTSIDE CALLS                                   0658
*                                                                  0658
*    CALLED BY -                                                   0658
*         IECDAFMT                                                 0658
*                                                                  0658
*    RETURNS TO -                                                  0658
*         CALLER WITHOUT SETTING A RETURN CODE                     0658
*                                                                  0658
*                                                                    */
*/********************************************************************/
*                                                                  0658
*   IF LOCKHELD=ON THEN             /* RELEASE LOCAL LOCK            */
         TM    LOCKHELD,B'01000000'                                0658
         BNO   @RF00658                                            0658
*     DO;                                                          0659
*       PARMSAVE=ABDPLPTR;          /* SAVE REGISTER 1               */
*                                                                  0660
         LR    PARMSAVE,ABDPLPTR                                   0660
*       /*************************************************************/
*       /*                                                           */
*       /* ONLY REG 1, WHICH GETS MODIFIED BY SETLOCK, NEEDS TO BE   */
*       /* SAVED                                                     */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0661
*       DO;                         /* SETLOCK (RELEASE) TYPE(LOCAL)
*                                      REGS(USE) RELATED(IECDAFMT)   */
*         RFY                                                      0662
*          (GPR11P,                                                0662
*           GPR12P,                                                0662
*           GPR13P,                                                0662
*           GPR14P) RSTD;                                          0662
*         RFY                                                      0663
*          (GPR15P,                                                0663
*           GPR00P,                                                0663
*           GPR01P) RSTD;                                          0663
*         GPR15P=GPR11P;            /* SAVE REG. 11 IN REG. 15.      */
         LR    GPR15P,GPR11P                                       0664
*         GPR00P=GPR12P;            /* SAVE REG. 12 IN REG. 0.       */
         LR    GPR00P,GPR12P                                       0665
*         GPR01P=GPR13P;            /* SAVE REG. 13 IN REG. 1.       */
         LR    GPR01P,GPR13P                                       0666
*         GPR13P=PSALITA;           /* ADDRESS OF LOCK INTERFACE   0667
*                                      TABLE.                        */
         L     GPR13P,PSALITA                                      0667
*         GEN(LM GPR11P,GPR13P,588(GPR13P));/* LOAD LOCKWORD, CLHT 0668
*                                      AND ENTRY ADRS.               */
         LM GPR11P,GPR13P,588(GPR13P)
*         GEN(BALR GPR14P,GPR13P);  /* BALR TO SETLOCK RTN.          */
         BALR GPR14P,GPR13P
*         GPR11P=GPR15P;            /* RESTORE REG. 11.              */
         LR    GPR11P,GPR15P                                       0670
*         GPR15P=GPR13P;            /* SAVE RETURN CODE.             */
         LR    GPR15P,GPR13P                                       0671
*         GPR12P=GPR00P;            /* RESTORE REG. 12               */
         LR    GPR12P,GPR00P                                       0672
*         GPR13P=GPR01P;            /* RESTORE REG. 13.              */
         LR    GPR13P,GPR01P                                       0673
*         RFY                                                      0674
*          (GPR15P,                                                0674
*           GPR00P,                                                0674
*           GPR01P) UNRSTD;                                        0674
*         RFY                                                      0675
*          (GPR11P,                                                0675
*           GPR12P,                                                0675
*           GPR13P,                                                0675
*           GPR14P) UNRSTD;                                        0675
*       END;                        /* SETLOCK (RELEASE) TYPE(LOCAL)
*                                      REGS(USE) RELATED(IECDAFMT)   */
*       LOCKHELD=OFF;               /* INDICATE LOCK NO LONGER HELD  */
         NI    LOCKHELD,B'10111111'                                0677
*       ABDPLPTR=PARMSAVE;          /* RESTORE REGISTER 1            */
         LR    ABDPLPTR,PARMSAVE                                   0678
*     END;                          /* END LOCAL LOCK RELEASED       */
*   ELSE                                                           0680
*     ;                             /* NO NEED TO RELEASE LOCK       */
@RF00658 DS    0H                                                  0681
*   END RELLOCK;                                                   0681
@EL00004 DS    0H                                                  0681
@EF00004 DS    0H                                                  0681
@ER00004 L     @14,@SA00004                                        0681
