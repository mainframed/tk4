         TITLE 'IECDAFMT-MAINLINE FOR DATA MGMT CONTROL BLOCK FORMATTER*
               R        '
IECDAFMT CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'IECDAFMT  77.299'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @11,0                                               0001
@PSTART  LA    @12,4095(,@11)                                      0001
         USING @PSTART,@11                                         0001
         USING @PSTART+4095,@12                                    0001
*                                                                  0153
*/********************************************************************/
*/* FUNCTION -                                                     0153
*      ROUTINE TO DETERMINE IF THE DATA MANAGEMENT CONTROL BLOCKS ARE
*      TO BE FORMATTED. IF THEY ARE, IT SCANS THE DEB CHAIN (ALWAYS
*      STARTING AT THE TCB) TO PROCESS EACH DEB.                   0153
*                                                                  0153
*   INPUT -                                                        0153
*      REGISTER 1 MUST POINT TO THE PARMLIST PASSED BY THE CALLER  0153
*                                                                  0153
*   CALLS -                                                        0153
*      ACCESRTN, FORMATIT, RELLOCK, AND PRINT                      0153
*                                                                  0153
*   CALLED BY -                                                    0153
*      AMDPRDMP OR SNAP/ABEND TO FORMAT THE DATA MGMT CONTROL BLOCKS
*                                                                  0153
*   RETURNS TO -                                                   0153
*      CALLER WITH RETURN CODE 0                                   0153
*                                                                    */
*/********************************************************************/
*                                                                  0153
*   TREG2=ABDPLPTR;                 /* SAVE PARM LIST POINTER /*   0153
*                                                            @ZA26256*/
         LR    TREG2,ABDPLPTR                                      0153
*   IF ADPLSNPR=OFF THEN            /* SNAP CALLER           @ZA26256*/
         TM    ADPLSNPR(ABDPLPTR),B'10000000'                      0154
         BNZ   @RF00154                                            0154
*     DO;                           /* YES.....              @ZA26256*/
*       PARMPTR=ADDR(ADPLCOM2);     /* ADDRESS OF GETMAIN LIST     0156
*                                                            @ZA26256*/
         LA    @14,ADPLCOM2(,ABDPLPTR)                             0156
         LR    PARMPTR,@14                                         0156
*       GENERATE REFS(DREG,ADPLCOM1);                              0157
*                                                         /* @ZA26256*/
    GETMAIN EC,LV=@ENDDATD-@DATD,SP=253,A=ADPLCOM1(TREG2),   @ZA26256  X
               MF=(E,(1))                                    @ZA26256
*     END;                          /* END... SNAP CALLER    @ZA26256*/
*   ELSE                                                           0159
*     DO;                           /*                       @ZA26256*/
         B     @RC00154                                            0159
@RF00154 DS    0H                                                  0160
*       TREG6=0;                    /*                       @ZA26256*/
         SLR   TREG6,TREG6                                         0160
*       GENERATE REFS(TREG6,ADPLSBPL);                             0161
*                                                         /* @ZA26256*/
    IC TREG6,ADPLSBPL(TREG2)                              /* @ZA26256*/
*       PARMPTR=ADDR(ADPLCOM2);     /* ADDRESS OF GETMAIN LIST     0162
*                                                            @ZA26256*/
         LA    @14,ADPLCOM2(,ABDPLPTR)                             0162
         LR    PARMPTR,@14                                         0162
*       GENERATE REFS(TREG6,ADPLSBPL,ADPLCOM1);                    0163
*                                                         /* @ZA26256*/
    GETMAIN EC,LV=@ENDDATD-@DATD,SP=(TREG6),                 @ZA26256  X
               A=ADPLCOM1(TREG2),MF=(E,(1))                  @ZA26256
*     END;                                                         0164
*   IF RCREG^=0 THEN                /* RETURN CODE FROM GM TEST /* 0165
*                                                            @ZA26256*/
@RC00154 LTR   RCREG,RCREG                                         0165
         BZ    @RF00165                                            0165
*     DO;                           /*                       @ZA26256*/
*       IF ADPLSNPR=OFF THEN        /* WHICH ERROR CODE TO USE?    0167
*                                                            @ZA26256*/
         TM    ADPLSNPR(ABDPLPTR),B'10000000'                      0167
         BNZ   @RF00167                                            0167
*         RETURN CODE(8);           /* RETURN WITH RC 8      @ZA26256*/
         LA    @15,8                                               0168
         L     @14,12(,@13)                                        0168
         LM    @00,@12,20(@13)                                     0168
         BR    @14                                                 0168
*       ELSE                        /*                       @ZA26256*/
*         RETURN CODE(4);           /* RETURN WITH RC 4      @ZA26256*/
@RF00167 LA    @15,4                                               0169
         L     @14,12(,@13)                                        0169
         LM    @00,@12,20(@13)                                     0169
         BR    @14                                                 0169
*     END;                          /*                       @ZA26256*/
*   ABDPLPTR=TREG2;                 /* RESTORE PARM POINTER  @ZA26256*/
@RF00165 LR    ABDPLPTR,TREG2                                      0171
*   DREG=ADPLCOM1;                  /* SAVE ADDRESS OF GETMAINED AREA
*                                                            @ZA26256*/
         L     DREG,ADPLCOM1(,ABDPLPTR)                            0172
*   GENERATE REFS(SAVEA1,REG13);                                   0173
*                                                                  0173
*                                                         /* @ZA26256*/
      USING @DATD,DREG           BASE FOR DSECT              @ZA26256
      ST REG13,SAVEA1+4          SAVE OLD SAVEAREA ADDRESS   @ZA26256
      LA R5,SAVEA1               GET NEW SAVEAREA ADDRESS    @ZA26256
      ST R5,8(REG13)             SAVE NEW SAVEAREA ADDRESS   @ZA26256
      LR REG13,R5                NEW SAVEAREA ADDRESS IN R13 @ZA26256
*   /*****************************************************************/
*   /*                                                               */
*   /* ESTABLISH THE ESTAE                                           */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0174
*   ESTAERR=ZERO;                   /* TURN OFF ESTAE BITS   @ZM45067*/
         SLR   @14,@14                                             0174
         ST    @14,ESTAERR                                         0174
*   STM(VADR,RCREG,RTYREGS);        /* SAVE REGS FOR THE ESTAE     0175
*                                                            @ZM45067*/
         STM   VADR,RCREG,RTYREGS                                  0175
*   LCALREGS=RTYREGS;               /* REGS FOR EXPECTED RETRY     0176
*                                                            @ZM45067*/
         MVC   LCALREGS(64),RTYREGS                                0176
*   SAVEREG1=ABDPLPTR;              /* SAVE REGISTER 1 ACCROSS ESTAE */
         LR    SAVEREG1,ABDPLPTR                                   0177
*   ESTAEXEC(1:ESTALEN)=ESTALIST(1:ESTALEN);/* MOVE LIST INTO      0178
*                                      AUTOMATIC AREA                */
         L     @14,ESTALEN                                         0178
         BCTR  @14,0                                               0178
         EX    @14,@SM02961                                        0178
*   GEN FLOWS(ESTAERTN)(ESTAE  ESTAERTN,PARAM=(DREG),MF=(E,ESTAEXEC));
         ESTAE  ESTAERTN,PARAM=(DREG),MF=(E,ESTAEXEC)
*                                   /* ESTAE WITH PARAM POINTING TO
*                                      ALL THE AUTOMATIC DATA FOR  0180
*                                      THIS MODULE                   */
*   IF RCREG=ZERO THEN              /* ESTAE RESOLVED FINE           */
         SLR   @14,@14                                             0180
         CR    RCREG,@14                                           0180
         BNE   @RF00180                                            0180
*     DO;                                                          0181
*       ABDPLPTR=SAVEREG1;          /* RESTORE REG 1                 */
*                                                                  0182
         LR    ABDPLPTR,SAVEREG1                                   0182
*       /*************************************************************/
*       /*                                                           */
*       /* FORMAT THE CONTROL BLOCKS. CHECK IF THE DUMP LEVEL IS     */
*       /* COMPATIBLE WITH THE FORMATTER AND THAT THE NEEDED INFO WAS*/
*       /* PASSED                                                    */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0183
*       IF ADPLTCB=ZERO×ADPLCVT=ZERO THEN/* CALLER DID NOT PASS TCB
*                                      OR CVT                        */
         C     @14,ADPLTCB(,ABDPLPTR)                              0183
         BE    @RT00183                                            0183
         C     @14,ADPLCVT(,ABDPLPTR)                              0183
         BNE   @RF00183                                            0183
@RT00183 DS    0H                                                  0184
*         BUFFER(1:LENGTH(NOINF))=NOINF;/* ERROR MESSAGE             */
         L     @14,ADPLBUF(,ABDPLPTR)                              0184
         MVC   BUFFER(99,@14),NOINF                                0184
*       ELSE                        /* NEEDED TCB AND CVT WERE PASSED*/
*         DO;                                                      0185
         B     @RC00183                                            0185
@RF00183 DS    0H                                                  0186
*           VADR=ADDR(CVTDCB);      /* OPERATING SYSTEM FLAGS        */
         L     @14,CVTPTR(,ABDPLPTR)                               0186
         LA    VADR,CVTDCB(,@14)                                   0186
*           CALL ACCESRTN;          /* ACCESS THEM                   */
         BAL   @14,ACCESRTN                                        0187
*           IF RCREG^=ZERO THEN     /* ACCESS ERROR                  */
         LTR   RCREG,RCREG                                         0188
         BZ    @RF00188                                            0188
*             BUFFER(1:LENGTH(INCOMP))=INCOMP;/* ERROR MESSAGE       */
         L     @14,ADPLBUF(,ABDPLPTR)                              0189
         MVC   BUFFER(74,@14),INCOMP                               0189
*           ELSE                    /* FOUND THEM                    */
*             DO;                                                  0190
         B     @RC00188                                            0190
@RF00188 DS    0H                                                  0191
*               IF ADPLSYTM=ON×(DATABYTE&LOWBIT)^=LOWBIT THEN/* DUMP
*                                      SYSTEM IS NOT MVS             */
         TM    ADPLSYTM(ABDPLPTR),B'01000000'                      0191
         BO    @RT00191                                            0191
         SLR   @14,@14                                             0191
         IC    @14,LOWBIT                                          0191
         SLR   @10,@10                                             0191
         IC    @10,DATABYTE(,DATAPTR)                              0191
         NR    @10,@14                                             0191
         CR    @10,@14                                             0191
         BE    @RF00191                                            0191
@RT00191 DS    0H                                                  0192
*                 BUFFER(1:LENGTH(INCOMP))=INCOMP;                 0192
         L     @14,ADPLBUF(,ABDPLPTR)                              0192
         MVC   BUFFER(74,@14),INCOMP                               0192
*               ELSE                /* WAS AN MVS SYSTEM             */
*                 DO;                                              0193
*                                                                  0193
         B     @RC00191                                            0193
@RF00191 DS    0H                                                  0194
*                   /*************************************************/
*                   /*                                               */
*                   /* DETERMINE IF THE DATA MGMT CONTROL BLOCKS ARE */
*                   /* WANTED. IF THE CALLER IS AMDPRDMP, THE TASK   */
*                   /* MUST BE AN ABENDING TASK OR HAVE NON-ZERO     */
*                   /* COMPLETION CODE.                              */
*                   /*                                               */
*                   /*************************************************/
*                                                                  0194
*                   VADR=ADDR(TCBCMP);/* TCB COMPLETION CODE         */
         L     @14,ADPLTCB(,ABDPLPTR)                              0194
         LA    VADR,TCBCMP(,@14)                                   0194
*                   CALL ACCESRTN;  /* ACCESS IT                     */
         BAL   @14,ACCESRTN                                        0195
*                   IF RCREG^=ZERO THEN/* COULD NOT ACCESS TCB       */
         LTR   RCREG,RCREG                                         0196
         BZ    @RF00196                                            0196
*                     BUFFER(1:LENGTH(TCBERR))=TCBERR;             0197
         L     @14,ADPLBUF(,ABDPLPTR)                              0197
         MVC   BUFFER(72,@14),TCBERR                               0197
*                   ELSE            /* FOUND TCB COMPLETION CODE     */
*                     DO;                                          0198
         B     @RC00196                                            0198
@RF00196 DS    0H                                                  0199
*                       COMPC=DATAWORD;/* SAVE COMPLETION CODE       */
         MVC   COMPC(4),DATAWORD(DATAPTR)                          0199
*                       VADR=ADDR(TCBFLGS1);/* ABEND CODE BYTES      */
         L     @14,ADPLTCB(,ABDPLPTR)                              0200
         LA    VADR,TCBFLGS1(,@14)                                 0200
*                       CALL ACCESRTN;/* ACCESS IT                   */
         BAL   @14,ACCESRTN                                        0201
*                       IF RCREG^=ZERO THEN/* ACCESS ERROR           */
         LTR   RCREG,RCREG                                         0202
         BZ    @RF00202                                            0202
*                         BUFFER(1:LENGTH(TCBERR))=TCBERR;         0203
         L     @14,ADPLBUF(,ABDPLPTR)                              0203
         MVC   BUFFER(72,@14),TCBERR                               0203
*                       ELSE        /* FOUND ABEND FLAGS             */
*                         DO;                                      0204
         B     @RC00202                                            0204
@RF00202 DS    0H                                                  0205
*                           IF COMPC^=ZERO×/* NON -ZERO COMPLETION 0205
*                                      CODE                          */
*                               (FLGS1&HIGHON)=HIGHON×/* ABEND IN  0205
*                                      PROGRESS FLAG ON              */
*                               ADPLSNPR=OFF THEN/* SNAP WAS INVOKER
*                                      - CONTROL BLOCKS WANTED       */
         ICM   @14,15,COMPC                                        0205
         BNZ   @RT00205                                            0205
         SLR   @14,@14                                             0205
         IC    @14,HIGHON                                          0205
         SLR   @10,@10                                             0205
         IC    @10,FLGS1(,DATAPTR)                                 0205
         NR    @10,@14                                             0205
         CR    @10,@14                                             0205
         BE    @RT00205                                            0205
         TM    ADPLSNPR(ABDPLPTR),B'10000000'                      0205
         BNZ   @RF00205                                            0205
@RT00205 DS    0H                                                  0206
*                             DO;                                  0206
*                                                                  0206
*                               /*************************************/
*                               /*                                   */
*                               /* LOOP THROUGH THE DEB CHAIN        */
*                               /* STARTING EACH TIME AT THE TCB AND */
*                               /* FINDING ONE DEB FURTHER. THIS     */
*                               /* ELIMINATES ERROR IF THE DEB CHAIN */
*                               /* CHANGES BETWEEN SCANS             */
*                               /*                                   */
*                               /*************************************/
*                                                                  0207
*                               DEBPTR=1;/* DUMMY VALUE TO ALLOW   0207
*                                      FIRST PASS THROUGH LOOP       */
         LA    DEBPTR,1                                            0207
*                               DO DEBCNTR=0 TO DEBMAX WHILE(DEBPTR^=
*                                     ZERO);/* LOOP THROUGH DEB CHAIN*/
         SLR   @14,@14                                             0208
         ST    @14,DEBCNTR                                         0208
@DL00208 LTR   DEBPTR,DEBPTR                                       0208
         BZ    @DC00208                                            0208
*                                 IF ADPLSNPR=OFF THEN/* SNAP WAS  0209
*                                      CALLER-LOCAL LOCK IS NEEDED   */
         TM    ADPLSNPR(ABDPLPTR),B'10000000'                      0209
         BNZ   @RF00209                                            0209
*                                   DO;                            0210
*                                                                  0210
*                                    /********************************/
*                                    /*                              */
*                                    /* ONLY REG 1 WHICH GETS        */
*                                    /* MODIFIED BY SETLOCK NEEDS TO */
*                                    /* BE SAVED                     */
*                                    /*                              */
*                                    /********************************/
*                                                                  0211
*                                    PARMSAVE=ABDPLPTR;/* SAVE PTR 0211
*                                      ACCROSS SETLOCK               */
         LR    PARMSAVE,ABDPLPTR                                   0211
*                                    DO;/* SETLOCK (OBTAIN)        0212
*                                      MODE(UNCOND) TYPE(LOCAL)    0212
*                                      REGS(USE) RELATED(IECDAFMT)   */
*                                     RFY                          0213
*                                      (GPR11P,                    0213
*                                       GPR12P,                    0213
*                                       GPR13P,                    0213
*                                       GPR14P) RSTD;              0213
*                                     RFY                          0214
*                                      (GPR15P,                    0214
*                                       GPR00P,                    0214
*                                       GPR01P) RSTD;              0214
*                                     GPR15P=GPR11P;/* SAVE REG. 11
*                                      IN REG. 15.                   */
         LR    GPR15P,GPR11P                                       0215
*                                     GPR00P=GPR12P;/* SAVE REG. 12
*                                      IN REG. 0.                    */
         LR    GPR00P,GPR12P                                       0216
*                                     GPR01P=GPR13P;/* SAVE REG. 13
*                                      IN REG. 1.                    */
         LR    GPR01P,GPR13P                                       0217
*                                     GPR13P=PSALITA;/* ADDRESS OF 0218
*                                      LOCK INTERFACE TABLE.         */
         L     GPR13P,PSALITA                                      0218
*                                     GEN(LM GPR11P,GPR13P,576(GPR13P))
*                                         ;/* LOAD LOCKWORD, CLHT AND
*                                      ENTRY ADRS.                   */
         LM GPR11P,GPR13P,576(GPR13P)
*                                     GEN(BALR GPR14P,GPR13P);/* BALR
*                                      TO SETLOCK RTN.               */
         BALR GPR14P,GPR13P
*                                     GPR11P=GPR15P;/* RESTORE REG.
*                                      11.                           */
         LR    GPR11P,GPR15P                                       0221
*                                     GPR15P=GPR13P;/* SAVE RETURN 0222
*                                      CODE.                         */
         LR    GPR15P,GPR13P                                       0222
*                                     GPR12P=GPR00P;/* RESTORE REG.
*                                      12                            */
         LR    GPR12P,GPR00P                                       0223
*                                     GPR13P=GPR01P;/* RESTORE REG.
*                                      13.                           */
         LR    GPR13P,GPR01P                                       0224
*                                     RFY                          0225
*                                      (GPR15P,                    0225
*                                       GPR00P,                    0225
*                                       GPR01P) UNRSTD;            0225
*                                     RFY                          0226
*                                      (GPR11P,                    0226
*                                       GPR12P,                    0226
*                                       GPR13P,                    0226
*                                       GPR14P) UNRSTD;            0226
*                                    END;/* SETLOCK (OBTAIN)       0227
*                                      MODE(UNCOND) TYPE(LOCAL)    0227
*                                      REGS(USE) RELATED(IECDAFMT)   */
*                                    ABDPLPTR=PARMSAVE;/* RESTORE  0228
*                                      REGISTER 1                    */
         LR    ABDPLPTR,PARMSAVE                                   0228
*                                    LOCKHELD=ON;/* INDICATE LOCK  0229
*                                      HELD TO ESTAE                 */
         OI    LOCKHELD,B'01000000'                                0229
*                                   END;/* END LOCAL LOCK NEEDED     */
*                                 ELSE                             0231
*                                   ;/* LOCK NOT NEEDED FOR AMDPRDMP */
@RF00209 DS    0H                                                  0232
*                                 VADR=ADDR(TCBDEB);/* PTR TO DEB  0232
*                                      CHAIN FROM TCB                */
         L     @14,ADPLTCB(,ABDPLPTR)                              0232
         LA    VADR,TCBDEB(,@14)                                   0232
*                                 CALL ACCESRTN;/* ACCESS IT         */
         BAL   @14,ACCESRTN                                        0233
*                                 IF RCREG^=ZERO THEN/* ERROR      0234
*                                      ACCESSING TCB                 */
         LTR   RCREG,RCREG                                         0234
         BZ    @RF00234                                            0234
*                                   DO;                            0235
*                                    BUFFER(1:LENGTH(TCBERR))=TCBERR;
         L     @14,ADPLBUF(,ABDPLPTR)                              0236
         MVC   BUFFER(72,@14),TCBERR                               0236
*                                    /* ERROR MSG                    */
*                                    CALL PRINT;/* PRINT IT          */
         L     @15,ADPLPRNT(,ABDPLPTR)                             0237
         BALR  @14,@15                                             0237
*                                    DEBPTR=ZERO;/* END THE LOOP     */
         SLR   DEBPTR,DEBPTR                                       0238
*                                   END;/* END ACCESS ERROR          */
*                                 ELSE/* FOUND DEB ADDRESS           */
*                                   DO;                            0240
         B     @RC00234                                            0240
@RF00234 DS    0H                                                  0241
*                                    DEBPTR=DATAWORD;/* SAVE PTR TO
*                                      DEB                           */
*                                                                  0241
         L     DEBPTR,DATAWORD(,DATAPTR)                           0241
*                                    /********************************/
*                                    /*                              */
*                                    /* INNER LOOP TO FIND THE NEXT  */
*                                    /* DEB ON THE CHAIN. IF THIS IS */
*                                    /* THE FIRST TIME DEBPTR ALREADY*/
*                                    /* CONTAINS THE DEB WANTED. THE */
*                                    /* FIRST TIME THROUGH THE       */
*                                    /* DEBCNTR IS ZERO - INDICATING */
*                                    /* THE PTR FROM THE TCB IS THE  */
*                                    /* ONE WANTED. THE LOOP IS NOT  */
*                                    /* ENTERED. THE NEXT TIME       */
*                                    /* THROUGH, DEBCNTR = 1,THE LOOP*/
*                                    /* IS ENTERED ONCE TO GET THE   */
*                                    /* POINTER TO THE NEXT DEB      */
*                                    /*                              */
*                                    /********************************/
*                                                                  0242
*                                    DO LOOPCNTR=DEBCNTR TO 1 BY-1 0242
*                                         WHILE(DEBPTR^=ZERO);     0242
         L     @14,DEBCNTR                                         0242
         ST    @14,LOOPCNTR                                        0242
         B     @DE00242                                            0242
@DL00242 LTR   DEBPTR,DEBPTR                                       0242
         BZ    @DC00242                                            0242
*                                     VADR=ADDR(DEBDEBAD);/* NEXT DEB
*                                      ADDR                          */
         LA    VADR,DEBDEBAD(,DEBPTR)                              0243
*                                     CALL ACCESRTN;/* ACCESS IT     */
         BAL   @14,ACCESRTN                                        0244
*                                     IF RCREG^=ZERO THEN/* ACCESS 0245
*                                      ERROR                         */
         LTR   RCREG,RCREG                                         0245
         BZ    @RF00245                                            0245
*                                      DO;                         0246
*                                       BUFFER(1:LENGTH(DEBERR))=DEBERR
*                                           ;/* ERROR MESSAGE        */
         L     @14,ADPLBUF(,ABDPLPTR)                              0247
         MVC   BUFFER(55,@14),DEBERR                               0247
*                                       CALL PRINT;/* PRINT IT       */
         L     @15,ADPLPRNT(,ABDPLPTR)                             0248
         BALR  @14,@15                                             0248
*                                       DEBPTR=ZERO;/* TERMINATES  0249
*                                      LOOP                          */
         SLR   DEBPTR,DEBPTR                                       0249
*                                      END;/* END ACCESS ERROR       */
*                                     ELSE/* FOUND NEXT DEB          */
*                                      DEBPTR=DATAADR;/* SET DEB   0251
*                                      ADDRESS                       */
         B     @RC00245                                            0251
@RF00245 SLR   DEBPTR,DEBPTR                                       0251
         ICM   DEBPTR,7,DATAADR(DATAPTR)                           0251
*                                    END;/* END LOOP                 */
@RC00245 SLR   @14,@14                                             0252
         BCTR  @14,0                                               0252
         AL    @14,LOOPCNTR                                        0252
         ST    @14,LOOPCNTR                                        0252
@DE00242 LTR   @14,@14                                             0252
         BP    @DL00242                                            0252
@DC00242 DS    0H                                                  0253
*                                    IF DEBPTR=ZERO THEN/* VALID DEB
*                                      WAS NOT FOUND                 */
         LTR   DEBPTR,DEBPTR                                       0253
         BNZ   @RF00253                                            0253
*                                     CALL RELLOCK;/* RELEASE LOCK IF
*                                      HELD                          */
         BAL   @14,RELLOCK                                         0254
*                                    ELSE/* VALID DEB FOUND          */
*                                     DO;                          0255
*                                                                  0255
         B     @RC00253                                            0255
@RF00253 DS    0H                                                  0256
*                                      /******************************/
*                                      /*                            */
*                                      /* A VALID DEB WAS FOUND TO BE*/
*                                      /* PROCESSED                  */
*                                      /*                            */
*                                      /******************************/
*                                                                  0256
*                                      VADR=ADDR(DEBDCBAD);/* PTR TO
*                                      ASSOCIATED DCB                */
         LA    VADR,DEBDCBAD(,DEBPTR)                              0256
*                                      CALL ACCESRTN;/* ACCESS IT    */
         BAL   @14,ACCESRTN                                        0257
*                                      IF RCREG^=ZERO THEN/* ACCESS
*                                      ERROR                         */
         LTR   RCREG,RCREG                                         0258
         BZ    @RF00258                                            0258
*                                       DCBPTR=NEGONE;/* NEGATIVE  0259
*                                      NUMBER TO INDICATE ERROR    0259
*                                      MESSAGE TO BE PRINTED LATER   */
         L     DCBPTR,NEGONE                                       0259
*                                      ELSE/* FOUND DCB ADDRESS      */
*                                       DCBPTR=DATAADR;/* SAVE DCB 0260
*                                      ADDRESS                       */
         B     @RC00258                                            0260
@RF00258 SLR   DCBPTR,DCBPTR                                       0260
         ICM   DCBPTR,7,DATAADR(DATAPTR)                           0260
*                                      CALL RELLOCK;/* RELEASE LOCAL
*                                      LOCK IF HELD                  */
@RC00258 BAL   @14,RELLOCK                                         0261
*                                      CALL FORMATIT;/* ROUTINE TO 0262
*                                      DETERMINE WHAT TO FORMAT      */
         BAL   @14,FORMATIT                                        0262
*                                     END;/* END VALID DEB           */
*                                   END;/* END FOUND DEB ADDR        */
@RC00253 DS    0H                                                  0265
*                               END;/* END LOOP THROUGH DEB CHAIN    */
@RC00234 LA    @14,1                                               0265
         AL    @14,DEBCNTR                                         0265
         ST    @14,DEBCNTR                                         0265
         CH    @14,@CH02732                                        0265
         BNH   @DL00208                                            0265
@DC00208 DS    0H                                                  0266
*                             END;  /* END CONTROL BLOCKS WANTED     */
*                           ELSE                                   0267
*                             ;     /* CONTROL BLOCKS NOT WANTED     */
@RF00205 DS    0H                                                  0268
*                         END;      /* END FOUND ABEND FLAGS         */
*                     END;          /* END FOUND TCB COMPLETION CODE */
