ACCESRTN ST    @14,@SA00005                                        0682
*/********************************************************************/
*/* FUNCTION-                                                      0683
*       ROUTINE WHICH CALLS THE ACCESS SERVICE ROUTINE AND MOVES THE
*     VALUE RETURNED IN R0 INTO DATAPTR.                           0683
*                                                                  0683
*                                                                  0683
*   INPUT -                                                        0683
*         VADR(REGISTER 0) CONTAINS THE VIRTUAL ADDRESS OF THE FIELD
*         TO BE ACCESSED.                                          0683
*                                                                  0683
*    CALLS-                                                        0683
*         ACCESS SERVICE ROUTINE PASSED IN PARMLIST                0683
*                                                                  0683
*    CALLED BY -                                                   0683
*         ALL ROUTINES IN THIS MODULE                              0683
*                                                                  0683
*    RETURNS TO -                                                  0683
*         CALLER WITH THE RETURN CODE SET BY THE ACCESS            0683
*         SERVICE ROUTINE.                                         0683
*                                                                  0683
*                                                                    */
*/********************************************************************/
*                                                                  0683
*   IF ADPLSNPR=OFF THEN            /* SNAP WAS THE CALLER           */
         TM    ADPLSNPR(ABDPLPTR),B'10000000'                      0683
         BNZ   @RF00683                                            0683
*     DO;                                                          0684
*       RTYADR=ADDR(RETRY);         /* RETRY ADDR FOR ESTAE IN CASE
*                                      OF EXPECTED ERROR-SIMULATES 0685
*                                      PRINT DUMP                    */
         LA    @14,RETRY                                           0685
         ST    @14,RTYADR                                          0685
*       DATAPTR=VADR;               /* PREPARE FOR A LOAD FROM THE 0686
*                                      DUMP INTO AN INTERNAL BUFFER  */
         LR    DATAPTR,VADR                                        0686
*       RCREG=ZERO;                 /* SUCCESS UNLESS OTHERWISE    0687
*                                      CHANGED                       */
         SLR   RCREG,RCREG                                         0687
*       ERREXP=ON;                  /* AN ERROR IS EXPECTED          */
         OI    ERREXP,B'10000000'                                  0688
*       VERIFY=DATADWRD;            /* MOVE A DOUBLE WORD INTO THE 0689
*                                      BUFFER                        */
*                                                                  0689
         MVC   VERIFY(8),DATADWRD(DATAPTR)                         0689
*       /*************************************************************/
*       /*                                                           */
*       /* THIS MOVE WILL CAUSE AN ABEND IF THE DUMP DOES NOT CONTAIN*/
*       /* THIS AREA OR IF IT IS PROTECTED. IF AN ABEND OCCURS THE   */
*       /* ESTAE WILL GET CONTROL AND RETRY AT THE POINT WHICH SETS A*/
*       /* BAD RETURN CODE AND CONTINUES IN THE MAIN LINE. THIS THEN */
*       /* SIMULATES THE PRINT DUMP ACCESS ROUTINE. IF THE AREA ASKED*/
*       /* FOR IS IN THE DUMP. A DOUBLE WORD IS MOVED INTO AN        */
*       /* INTERNAL BUFFER TO PREVENT AN UNEXPECTED ERROR WHEN       */
*       /* REFERENCING THE AREA AGAIN                                */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0690
*       ERREXP=OFF;                 /* ERROR NO LONGER EXPECTED      */
         NI    ERREXP,B'01111111'                                  0690
*       DATAPTR=ADDR(VERIFY);       /* BASE DATAWORD ON THE BUFFER   */
         LA    DATAPTR,VERIFY                                      0691
*     END;                          /* END SNAP WAS CALLER           */
*   ELSE                            /* CALLER HAS OWN ACCESS ROUTINE */
*     DO;                                                          0693
         B     @RC00683                                            0693
@RF00683 DS    0H                                                  0694
*       CALL ACCESS;                /* ACCESS SERVICE ROUTINE PASSED
*                                      BY CALLER                     */
         L     @15,ADPLMEMA(,ABDPLPTR)                             0694
         BALR  @14,@15                                             0694
*       IF RCREG=ZERO THEN                                         0695
         LTR   RCREG,RCREG                                         0695
         BNZ   @RF00695                                            0695
*         DATAPTR=VADR;             /* SAVE PTR RETURNED BY ACCESS 0696
*                                      SERVICE ROUTINE               */
         LR    DATAPTR,VADR                                        0696
*       ELSE                        /* BAD RETURN CODE               */
*RETRY:                             /* RETRY ADDR FOR THE ESTAE      */
*         RCREG=4;                  /* SET BAD RETURN FOR THE      0697
*                                      MAINLINE                      */
         B     @RC00695                                            0697
@RF00695 DS    0H                                                  0697
RETRY    LA    RCREG,4                                             0697
*     END;                          /* END CALLER NOT SNAP           */
@RC00695 DS    0H                                                  0699
*   RCCODE=RCREG;                   /* SAVE RETURN CODE              */
@RC00683 ST    RCREG,RCCODE                                        0699
*   RETURN CODE(RCCODE);                                           0700
         L     @14,RCCODE                                          0700
         LR    @15,@14                                             0700
@EL00005 DS    0H                                                  0700
@EF00005 DS    0H                                                  0700
@ER00005 L     @14,@SA00005                                        0700
         BR    @14                                                 0700
*   END ACCESRTN;                                                  0701
*ESTAERTN:                                                         0702
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0702
*                                                                  0702
ESTAERTN DS    0H                                                  0703
*/********************************************************************/
*/* FUNCTION-                                                      0703
*       ROUTINE TO DETERMINE IF THE ERROR WAS EXPECTED AND CONTINUE
*     PROCESSING OR TO CLEAN UP AND EXIT. IF THE ERROR WAS EXPECTED
*     THEN THE CONTROL IS RETURNED TO THE ADDRESS PASSED IN THE ESTAE
*     PARM LIST. THIS RETURN ACTS LIKE THE BAD RETURN FROM THE ACCESS
*     SERVICE ROUTINE. IF THE ERROR WAS NOT EXPECTED, REQUIRED CLEAN UP
*     OF RESOURCES IS DONE. A MESSAGE IS FILLED INTO THE BUFFER AND
*     CONTROL IS RETURNED AT THE END OF THE FORMATTER.             0703
*                                                                  0703
*    CALLS-                                                        0703
*         MAKES NO OUTSIDE CALLS                                   0703
*                                                                  0703
*    CALLED BY -                                                   0703
*         RTM IN AN ABEND SITUATION                                0703
*                                                                  0703
*    RETURNS TO -                                                  0703
*         CALLER                                                   0703
*                                                                    */
*/********************************************************************/
*                                                                  0703
*   /*****************************************************************/
*   /*                                                               */
*   /* NO RESTORE REGS SPECIFIED                                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0703
*   DO;                             /* ENTESTAE PARMREG(DREG)        */
*/*  ENTRY CODE FOR ESTAE ROUTINE.                                   */
*/*  IF RTCA ALLOCATED THEN RESTORE CODE AND                         */
*/*  DATA REGS FROM START OF USERS PARM LIST,                        */
*/*  AND ESTABLISH ADDR TO ESTAE SAVE AREA                           */
*/*  IF REQUIRED.                                                    */
*/*  IF NO RTCA IS AVAILABLE THEN JUST RETURN                        */
*/*  TO RTM TO CONTINUE TERMINATION                                  */
*                                                                  0704
*     GEN(USING *,GPR15P);                                         0704
         USING *,GPR15P
*     GEN(CH    GPR00F,*+14);       /* CHECK IF RTCA ALLOC FAILED    */
         CH    GPR00F,*+14
*     GEN(BNE   *+12);              /* NO,SAVE RTM REGS              */
         BNE   *+12
*     GEN(DROP  GPR15P);            /* DROP LOCAL ADDR               */
         DROP  GPR15P
*     GEN(LA    GPR15P,0);          /* YES, SET CONT TERM RC         */
         LA    GPR15P,0
*     GEN(BR    GPR14P);            /* RETURN TO RTM                 */
