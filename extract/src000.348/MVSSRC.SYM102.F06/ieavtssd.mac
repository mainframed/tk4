*   SMWKNSTB=SMWKNSTB+1;            /* INCREMENT STEAL BACK COUNTER
*                                                            @ZM45073*/
         LA    @02,1                                               0790
         AH    @02,SMWKNSTB(,R9)                                   0790
         STH   @02,SMWKNSTB(,R9)                                   0790
*   RFY                                                            0791
*    (R10,                                                         0791
*     R11,                                                         0791
*     R8) RSTD;                                                    0791
*   RFY                                                            0792
*     SUMFRR6W BASED(R8);           /* MAP THE 6 WORD FRR WORK AREA  */
*                                                                  0792
*   /*****************************************************************/
*   /*                                                               */
