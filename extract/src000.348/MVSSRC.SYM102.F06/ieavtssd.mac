*                                      CONTROL                       */
         TM    SUMFRRST(R8),B'00000010'                            0758
         BNO   @RF00758                                            0758
*     SDWACSCT=ENTNAME;             /* IEAVTSDS ENTRY POINT NAME FOR
*                                      RECORD                        */
         MVC   SDWACSCT(8,R1),@CC03213                             0759
*   ELSE                                                           0760
*     SDWACSCT=CSECTNAM;            /* IEAVTSSD CSECT NAME FOR RECORD*/
         B     @RC00758                                            0760
@RF00758 MVC   SDWACSCT(8,R1),@CC03206                             0760
*   SDWAREXN=FRRNAME2;              /* FRR NAME FOR RECORD           */
