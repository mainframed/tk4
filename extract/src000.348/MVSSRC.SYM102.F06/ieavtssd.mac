*         DO;                       /* NOT LAST-MOVE IS NECESSARY    */
*           SMWKPSW=SMWKPSW&&SMWKPSW;/* INIT PSW FOR LPSW USE        */
         XC    SMWKPSW(8,R9),SMWKPSW(R9)                           0805
*           RFY                                                    0806
*            (R4,                                                  0806
*             R5,                                                  0806
*             R6,                                                  0806
*             R7,                                                  0806
*             R2,                                                  0806
*             R10) RSTD;                                           0806
