         C     @11,@CF00126                                        0526
         BNH   @DL00513                                            0526
*   R14=SMWKSMRG;                   /* RESTORE CALLERS RETURN ADDRESS*/
         L     R14,SMWKSMRG(,R9)                                   0527
*   RFY                                                            0528
*     REGARRAY BASED;                                              0528
*   RFY                                                            0529
*     R10 UNRSTD;                                                  0529
*   END SUMREGS;                                                   0530
@EL00006 DS    0H                                                  0530
@EF00006 DS    0H                                                  0530
@ER00006 BR    @14                                                 0530
*                                                                  0531
*/*  *****************************************************************/
*/*      THIS SEGMENT BUILDS THE SUMMARY DUMP LOGICAL RECORD HEADER  */
*/*      AND CALLS THE SUMMARY DUMP SERVICE ROUTINES TO VALIDATE     */
*/*      THE AREAS TO BE MOVED AND TO ACTUALLY MOVE THE DATA TO BE   */
*/*      DUMPED TO THE REAL STORAGE BUFFER. IF THE LENGTH OF THE     */
*/*      AREA TO BE MOVED FIELD IS ZERO ON ENTRY (SMDLRLEN) AN ERROR */
*/*      CONDITION IS SIGNALED AND THE SUMMOVE SUBROUTINE IS CALLED. */
*/*      IF THE LENGTH IS NON-ZERO SUMVALID IS CALLED TO VALIDATE    */
*/*      EACH REFERENCED VIRTUAL PAGE AND SUMMOVE IS CALLED TO MOVE  */
*/*      THE REQUESTED AREAS OF THE VALID PAGES TO THE REAL STORAGE  */
*/*      BUFFER.                                                     */
*/*                                                                  */
*/*      INPUT: SMDLRADR = ADDRESS OF AREA TO BE MOVED               */
*/*             SMDLRLEN = TOTAL LENGTH OF AREA TO BE MOVED          */
*/*                                                                  */
*/*      OUTPUT: SMDLRADR= ADDRESS OF VALID AREA TO BE MOVED         */
*/*              SMDLRLEN= TOTAL LENGTH OR ZERO (CONTINUATION RECORDS*/
*/*              SMDLRPL= LENGTH OF VALID AREA TO BE MOVED           */
*/*                                                                  */
*/*      SUBROUTINES CALLED: SUMVALID, SUMMOVE                       */
*/*                                                                  */
*/*      CALLED BY: SUMDUMP, SUMLIST                                 */
*/*                                                                  */
*/*  *****************************************************************/
*                                                                  0531
*SUMDATA:                                                          0531
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0531
SUMDATA  DS    0H                                                  0532
*   SMWKSMDT=R14;                   /* SAVE CALLERS RETURN ADDRESS   */
         ST    R14,SMWKSMDT(,R9)                                   0532
*   IF SMDLRLEN=ZERO THEN           /* IF THIS IS AN ERROR RECORD    */
         SLR   @02,@02                                             0533
         LA    @11,656                                             0533
         ALR   @11,R9                                              0533
         C     @02,SMDLRLEN(,@11)                                  0533
         BNE   @RF00533                                            0533
*     DO;                           /* JUST WRITE HEADER ON BUFFER   */
*       SMDLRPL=ZERO;               /* MAKE SURE PARTIAL LENGTH IS 0535
*                                      ZERO                          */
         ST    @02,SMDLRPL(,@11)                                   0535
*       CALL SUMMOVE;               /* MORE ERROR HEADER TO BUFFER   */
         BAL   @14,SUMMOVE                                         0536
*     END;                          /* END OF WRITE ERROR HEADER ON
*                                      BUFFER                        */
*   ELSE                                                           0538
*     DO;                           /* MOVE HEADER AND DATA TO BUFFER*/
         B     @RC00533                                            0538
@RF00533 DS    0H                                                  0539
*       SMDLRLEN=(SMDLRLEN+THREE)&FULLWORD;/* ROUND TOTAL LENGTH TO A
*                                      FULL WORD BOUNDARY            */
         LA    @02,656                                             0539
         ALR   @02,R9                                              0539
         LA    @00,3                                               0539
         AL    @00,SMDLRLEN(,@02)                                  0539
         N     @00,@CF03197                                        0539
         ST    @00,SMDLRLEN(,@02)                                  0539
*       SMWKVADR=SMDLRADR;          /* INITIALIZE WORKING VIRTUAL  0540
*                                      ADDR                          */
         L     @11,SMDLRADR(,@02)                                  0540
         ST    @11,SMWKVADR(,R9)                                   0540
*       SMWKENDA=SMDLRADR+SMDLRLEN; /* CALCULATE TOTAL AREA END ADDR */
         ALR   @11,@00                                             0541
         ST    @11,SMWKENDA(,R9)                                   0541
*       SMDLRPL=ZERO;               /* INITIALIZE PARTIAL LENGTH TO
*                                      ZERO                          */
*                                                                  0542
         SLR   @00,@00                                             0542
         ST    @00,SMDLRPL(,@02)                                   0542
*       /*************************************************************/
*       /*                                                           */
*       /* THIS LOOP PROCESSES THE VIRTUAL STORAGE AREA BOUNDED BY   */
*       /* SMWKVADR AND SMWKENDA. ALL REFERENCED PAGES ARE VALIDATED */
*       /* AND THE SMWKRPLN FIELD IS SET TO THE LENGTH OF EACH VALID */
*       /* PART OF THE AREA TO BE MOVED. THE SUMMOVE SUBROUTINE IS   */
*       /* CALLED TO WRITE THE HEADER AND THE AREA SPECIFIED ON THE  */
*       /* REAL STORAGE BUFFER.                                      */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0543
*       DO WHILE(SMWKVADR<SMWKENDA)&(SMWKRBFL=OFF);/* PROCESS WHOLE
*                                      AREA                          */
         B     @DE00543                                            0543
@DL00543 DS    0H                                                  0544
*         CALL SUMVALID;            /* VALIDATE CURRENT AREA ADDRESS */
         BAL   @14,SUMVALID                                        0544
*         IF R15=NULL THEN          /* IF THIS AREA WAS PAGED IN AND
*                                      OK                            */
         LTR   R15,R15                                             0545
         BNZ   @RF00545                                            0545
*           DO;                     /* UPDATE PARTIAL LENGTH AND   0546
*                                      CONTINUE                      */
*             SMWKSAVA=SMWKVADR;    /* SAVE CURRENT ADDRESS          */
         L     @02,SMWKVADR(,R9)                                   0547
         ST    @02,SMWKSAVA(,R9)                                   0547
*             SMWKVADR=(SMWKVADR&FOURKBDY)+FOURK;/* REFERENCE NEXT 0548
*                                      VIRTUAL PAGE                  */
         LR    @00,@02                                             0548
         N     @00,@CF03185                                        0548
         AL    @00,@CF03187                                        0548
         ST    @00,SMWKVADR(,R9)                                   0548
*             IF SMWKVADR>=SMWKENDA THEN/* IF WE ARE FINISHED, LAST
*                                      PAGE WAS A VALID PAGE         */
         L     @11,SMWKENDA(,R9)                                   0549
         CR    @00,@11                                             0549
         BL    @RF00549                                            0549
*               DO;                 /* MOVE HEADER AND AREA TO THE 0550
*                                      BUFFER                        */
*                 SMDLRPL=SMDLRPL+(SMWKENDA-SMWKSAVA);/* UPDATE    0551
*                                      PARTIAL LENGTH WITH VALID AREA
*                                      LENGTH                        */
         LA    @10,656                                             0551
         ALR   @10,R9                                              0551
         SLR   @11,@02                                             0551
         AL    @11,SMDLRPL(,@10)                                   0551
         ST    @11,SMDLRPL(,@10)                                   0551
*                 CALL SUMMOVE;     /* MOVE HEADER AND DATA TO BUFFER*/
         BAL   @14,SUMMOVE                                         0552
*                 SMDLRADR=SMWKVADR;/* POINT HEADER TO NEXT AREA TO
*                                      BE REFERENCED         @ZA25122*/
         L     @02,SMWKVADR(,R9)                                   0553
         ST    @02,SMDLRADR+656(,R9)                               0553
*               END;                /* END MOVE HEADER AND AREA TO 0554
*                                      BUFFER                        */
*             ELSE                                                 0555
*               SMDLRPL=SMDLRPL+(SMWKVADR-SMWKSAVA);/* NOT DONE SO 0555
*                                      UPDATE PARTIAL LENGTH AND   0555
*                                      CONTINUE                      */
         B     @RC00549                                            0555
@RF00549 LA    @02,656                                             0555
         ALR   @02,R9                                              0555
         L     @00,SMWKVADR(,R9)                                   0555
         SL    @00,SMWKSAVA(,R9)                                   0555
         AL    @00,SMDLRPL(,@02)                                   0555
         ST    @00,SMDLRPL(,@02)                                   0555
*           END;                    /* END OF AREA WAS PAGED IN AND
*                                      OK                            */
*         ELSE                                                     0557
*           DO;                     /* AREA NOT PAGED IN MOVE VALID
*                                      AREA IF ANY OR PUT OUT AREA 0557
*                                      MISSING ADDRESS               */
         B     @RC00545                                            0557
@RF00545 DS    0H                                                  0558
*             SMWKVADR=(SMWKVADR&FOURKBDY)+FOURK;/* REFERENCE NEXT 0558
*                                      VIRTUAL PAGE                  */
         L     @02,SMWKVADR(,R9)                                   0558
         N     @02,@CF03185                                        0558
         AL    @02,@CF03187                                        0558
         ST    @02,SMWKVADR(,R9)                                   0558
*             IF(SMDLRLEN^=ZERO)(SMDLRPL^=ZERO) THEN/* IF ONE OF THE
*                                      LENGTHS IS NON-ZERO           */
         SLR   @02,@02                                             0559
         LA    @11,656                                             0559
         ALR   @11,R9                                              0559
         C     @02,SMDLRLEN(,@11)                                  0559
         BNE   @RT00559                                            0559
         C     @02,SMDLRPL(,@11)                                   0559
         BE    @RF00559                                            0559
@RT00559 DS    0H                                                  0560
*               DO;                 /* MOVE HEADER AND MAYBE DATA TO
*                                      BUFFER                        */
*                 CALL SUMMOVE;     /* MOVE HEADER/DATA TO BUFFER    */
         BAL   @14,SUMMOVE                                         0561
*                 SMDLRPL=ZERO;     /* INITIALIZE PARTIAL LENGTH     */
         SLR   @02,@02                                             0562
         LA    @11,656                                             0562
         ALR   @11,R9                                              0562
         ST    @02,SMDLRPL(,@11)                                   0562
*                 SMDLRLEN=ZERO;    /* INDICATE A CONTINUATION HEADER*/
         ST    @02,SMDLRLEN(,@11)                                  0563
*               END;                /* END OF MOVE HEADER/DATA TO  0564
*                                      BUFFER                        */
*             IF SMWKVADR>=SMWKENDA THEN/* IF CHECKED WHOLE AREA     */
@RF00559 L     @02,SMWKENDA(,R9)                                   0565
         C     @02,SMWKVADR(,R9)                                   0565
         BH    @RF00565                                            0565
*               DO;                 /* PUT OUT LAST PART MISSING   0566
*                                      HEADER                        */
*                 SMDLRADR=SMWKENDA;/* SET END ADDRESS FOR LAST PART
*                                      MISSING HEADER        @ZM45066*/
         ST    @02,SMDLRADR+656(,R9)                               0567
*                 CALL SUMMOVE;     /* MOVE LAST PART MISSING HEADER
*                                      TO BUFFER                     */
         BAL   @14,SUMMOVE                                         0568
*               END;                /* PUT OUT LAST PART MISSING   0569
*                                      HEADER                        */
*             SMDLRADR=SMWKVADR;    /* POINT HEADER TO NEXT AREA TO
*                                      BE REFERENCED         @ZA25122*/
@RF00565 L     @02,SMWKVADR(,R9)                                   0570
         ST    @02,SMDLRADR+656(,R9)                               0570
*           END;                    /* END OF AREA NOT PAGED IN      */
*       END;                        /* END OF LOOP TO PROCESS WHOLE
*                                      AREA                          */
@RC00545 DS    0H                                                  0572
@DE00543 CLC   SMWKVADR(4,R9),SMWKENDA(R9)                         0572
         BNL   @DC00543                                            0572
         TM    SMWKRBFL(R9),B'00100000'                            0572
         BZ    @DL00543                                            0572
@DC00543 DS    0H                                                  0573
*     END;                          /* END OF NOT AN ERROR HEADER    */
*   R14=SMWKSMDT;                   /* RESTORE CALLERS RETURN ADDRESS*/
@RC00533 L     R14,SMWKSMDT(,R9)                                   0574
*   END SUMDATA;                                                   0575
@EL00007 DS    0H                                                  0575
@EF00007 DS    0H                                                  0575
@ER00007 BR    @14                                                 0575
*                                                                  0576
*/*  *****************************************************************/
*/*    THIS SECTION OBTAINS THE REAL ADDRESS OF THE VIRTUAL STORAGE  */
*/*    AREA TO BE MOVED AND CALLS THE SUMBUFF ROUTINE TO ACTUALLY    */
*/*    MOVE THE AREA TO A REAL FRAME. SUMBUFF IS CALLED ONLY TO      */
*/*    MOVE THE DATA LOCATED ON 1 VIRTUAL PAGE. IF THE AREA LENGTH   */
*/*    CAUSES A PAGE BOUNDARY CROSS, SUMBUFF WILL BE CALLED TO       */
*/*    MOVE DATA FROM EACH REFERENCED VIRTUAL PAGE.                  */
*/*                                                                  */
*/*     INPUT: SMDLRADR = VIRTUAL ADDRESS OF AREA TO BE MOVED        */
*/*            SMDLRPL = LENGTH OF AREA TO BE MOVED                  */
*/*                                                                  */
*/*     OUTPUT: R7  CONTAINS REAL ADDRESS OF 1 REFERENCED VIRTUAL    */
*/*                 PAGE                                             */
*/*             SMWKALEN CONTAINS LENGTH OF AREA TO THE END OR TO    */
*/*                      THE NEXT PAGE BOUNDARY WHICH EVER IS LESS   */
*/*             SMDLRPL WILL BE ZERO WHEN ALL OF AREA HAS BEEN       */
*/*                      MOVED                                       */
*/*                                                                  */
*/*     CALLED BY: SUMDATA                                           */
*/*                                                                  */
*/*  *****************************************************************/
*                                                                  0576
*SUMMOVE:                                                          0576
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0576
SUMMOVE  DS    0H                                                  0577
*   SMWKSMMV=R14;                   /* SAVE CALLERS RETURN ADDRESS   */
*                                                                  0577
         ST    R14,SMWKSMMV(,R9)                                   0577
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS SECTION WILL MOVE THE HEADER TO THE BUFFER               */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0578
*   RFY                                                            0578
*    (R10,                                                         0578
*     R11,                                                         0578
*     R6) RSTD;                                                    0578
*   R10=ADDR(SMWKHEAD);             /* GET ADDRESS OF HEADER RECORD  */
         LA    R10,SMWKHEAD(,R9)                                   0579
*   SMWKALEN=LENGTH(SMDLRHDR);      /* OBTAIN LENGTH OF HEADER     0580
*                                                            @ZA25122*/
         MVC   SMWKALEN(2,R9),@CH01311                             0580
*   GEN(LRA   R6,0(R10));           /* OBTAIN REAL ADDRESS OF HEADER */
         LRA   R6,0(R10)
*   CALL SUMBUFF;                   /* MOVE HEADER TO THE BUFFER     */
         BAL   @14,SUMBUFF                                         0582
*   IF SMDLRPL>ZERO THEN            /* IF SOME DATA SHOULD BE MOVED  */
         SLR   @07,@07                                             0583
         LA    @02,656                                             0583
         ALR   @02,R9                                              0583
         L     @05,SMDLRPL(,@02)                                   0583
         CR    @05,@07                                             0583
         BNH   @RF00583                                            0583
*     DO;                           /* GET REAL ADDRESS AND MOVE TO
*                                      BUFFER                        */
*       SMWKPEND=SMDLRADR+SMDLRPL;  /* CALCULATE END ADDRESS OF THIS
*                                      VIRTUAL STORAGE AREA          */
         L     @02,SMDLRADR(,@02)                                  0585
         ALR   @05,@02                                             0585
         ST    @05,SMWKPEND(,R9)                                   0585
*       SMWKALEN=ZERO;              /* INITIALIZE LENGTH OF AREA TO
*                                      BE MOVED                      */
         STH   @07,SMWKALEN(,R9)                                   0586
*       R10=SMDLRADR;               /* OBTAIN BEGINNING VIRTUAL    0587
*                                      ADDRESS                       */
*                                                                  0587
         LR    R10,@02                                             0587
*       /*************************************************************/
*       /*                                                           */
*       /* THIS LOOP WILL CALL SUMBUFF AS MANY TIMES AS THRERE ARE   */
*       /* VIRTUAL PAGES REFERENCED FOR THE DATA AREA TO BE DUMPED   */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0588
*       DO WHILE(SMDLRPL>ZERO&SMWKRBFL=OFF);/* PROCESS ALL VIRTUAL 0588
*                                      PAGES WHILE FRAMES ARE      0588
*                                      AVAILABLE             @ZM45083*/
         B     @DE00588                                            0588
@DL00588 DS    0H                                                  0589
*         R10=R10+SMWKALEN;         /* MOVE TO NEXT VIRTUAL PAGE FOR
*                                      DATA TO BE MOVED              */
         AH    R10,SMWKALEN(,R9)                                   0589
*         R11=(R10&FOURKBDY)+FOURK; /* GET ADDRESS OF NEXT VIRTUAL 0590
*                                      PAGE                          */
         LR    R11,R10                                             0590
         N     R11,@CF03185                                        0590
         AL    R11,@CF03187                                        0590
*         IF R11<=SMWKPEND THEN     /* IF HAVE REACHED END OF THIS 0591
*                                      DATA AREA                     */
         C     R11,SMWKPEND(,R9)                                   0591
         BH    @RF00591                                            0591
*           SMWKALEN=R11-R10;       /* LENGTH IS DISTANCE BETWEEN  0592
*                                      PAGES                         */
         LR    @07,R11                                             0592
         SLR   @07,R10                                             0592
         STH   @07,SMWKALEN(,R9)                                   0592
*         ELSE                                                     0593
*           SMWKALEN=SMWKPEND-R10;  /* LENGTH TO END OF AREA         */
         B     @RC00591                                            0593
@RF00591 L     @07,SMWKPEND(,R9)                                   0593
         SLR   @07,R10                                             0593
         STH   @07,SMWKALEN(,R9)                                   0593
*         GEN(LRA   R6,0(R10));     /* GET DATA AREA REAL ADDRESS    */
@RC00591 DS    0H                                                  0594
         LRA   R6,0(R10)
*         CALL SUMBUFF;             /* MOVE THIS AREA TO REAL STORAGE
*                                      BUFFER                        */
         BAL   @14,SUMBUFF                                         0595
*         SMDLRPL=SMDLRPL-SMWKALEN; /* CALCULATE AMOUNT OF VIRTUAL 0596
*                                      AREA STILL LEFT TO MOVE       */
         LA    @07,656                                             0596
         ALR   @07,R9                                              0596
         L     @02,SMDLRPL(,@07)                                   0596
         SH    @02,SMWKALEN(,R9)                                   0596
         ST    @02,SMDLRPL(,@07)                                   0596
*       END;                        /* END OF PROCESS ALL PAGES LOOP */
@DE00588 L     @07,SMDLRPL+656(,R9)                                0597
         LTR   @07,@07                                             0597
         BNP   @DC00588                                            0597
         TM    SMWKRBFL(R9),B'00100000'                            0597
         BZ    @DL00588                                            0597
@DC00588 DS    0H                                                  0598
*     END;                          /* END OF SOME DATA SHOULD BE  0598
*                                      MOVED                         */
*   RFY                                                            0599
*    (R10,                                                         0599
*     R11,                                                         0599
*     R6) UNRSTD;                                                  0599
@RF00583 DS    0H                                                  0600
*   R14=SMWKSMMV;                   /* RESTORE CALLERS RETURN ADDRESS*/
         L     R14,SMWKSMMV(,R9)                                   0600
*   END SUMMOVE;                                                   0601
@EL00008 DS    0H                                                  0601
@EF00008 DS    0H                                                  0601
@ER00008 BR    @14                                                 0601
*                                                                  0602
*/*  *****************************************************************/
*/*     THIS SEGMENT VALIDATES THE DATA AREA TO BE MOVED TO THE      */
*/*     REAL STORAGE BUFFER.                                         */
*/*                                                                  */
*/*     INPUT: SMWADDR CONTAINS VIRTUAL ADDRESS TO BE VALIDATED      */
*/*                                                                  */
*/*     OUTPUT: RETURN CODE IN R15                                   */
*/*                                                                  */
*/*     CALLED BY: SUMDATA                                           */
*/*                                                                  */
*/*     INCLUDED BY: TOP SEGMENT                                     */
*/*                                                                  */
*/*  *****************************************************************/
*                                                                  0602
*SUMVALID:                                                         0602
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0602
SUMVALID DS    0H                                                  0603
*   RFY                                                            0603
*     R11 RSTD;                                                    0603
*   SMWKSMVL=R14;                   /* SAVE CALLERS RETURN ADDRESS   */
         ST    R14,SMWKSMVL(,R9)                                   0604
*   R11=SMWKVADR;                   /* PUT ADDRESS TO BE VALIDATED IN
*                                      A REGISTER                    */
         L     R11,SMWKVADR(,R9)                                   0605
*   GEN(LRA   R11,0(R11));          /* VALIDATE AREA VIRTUAL ADDRESS */
         LRA   R11,0(R11)
*   BC(7,BADCODE);                  /* PAGE NOT VALID GIVE BAD RETURN
*                                      CODE                          */
         BC    7,BADCODE                                           0607
*   R14=SMWKSMVL;                   /* RESTORE CALLERS RETURN ADDRESS*/
         L     R14,SMWKSMVL(,R9)                                   0608
*   RETURN CODE(0);                 /* A VALID VADDR WAS FOUND       */
         SLR   @15,@15                                             0609
@EL00009 DS    0H                                                  0609
@EF00009 DS    0H                                                  0609
@ER00009 BR    @14                                                 0609
*BADCODE:                           /* AN INVALID VADDR WAS FOUND    */
*   R14=SMWKSMVL;                   /* RESTORE CALLERS RETURN ADDRESS*/
BADCODE  L     R14,SMWKSMVL(,R9)                                   0610
*   RETURN CODE(8);                 /* AN INVALID ADDRESS WAS FOUND  */
         LA    @15,8                                               0611
         BR    @14                                                 0611
*   RFY                                                            0612
*     R11 UNRSTD;                                                  0612
*   END SUMVALID;                                                  0613
*                                                                  0614
*/*  *****************************************************************/
*/*    THIS SEGMENT MOVES THE DATA DEFINED BY R7 AND SMWKALEN TO A   */
*/*    REAL STORAGE FRAME. IF THE SPACE ON THE CURRENT REAL STORAGE  */
*/*    FRAME IS USED UP AN NEW FRAME IS OBTAINED AND THE FRAME SPACE */
*/*    VALUE IS UPDATED. LPSW LOGIC IS USED TO TURN DAT OFF, MOVE    */
*/*    THE DATA TO THE BUFFER AND TO TURN DAT ON.                    */
*/*                                                                  */
*/*    INPUT: SMWKALEN CONTAINS LENGTH OF VIRTUAL AREA TO BE MOVED   */
*/*           R7 CONTAINS REAL ADDRESS OF THE AREA TO BE MOVED       */
*/*           SMWKSCBF CONTAINS SPACE REMAINING ON CURRENT FRAME     */
*/*           SMWKRFAD CONTAINS REAL ADDRESS OF FREE AREA ON FRAME   */
*/*                                                                  */
*/*    OUTPUT: VIRTUAL STORAGE AREA MOVE TO REAL STORAGE BUFFER      */
*/*            INPUT AREAS UPDATED                                   */
*/*                                                                  */
*/*    CALLED BY: SUMMOVE                                            */
*/*                                                                  */
*/*  *****************************************************************/
*                                                                  0614
*SUMBUFF:                                                          0614
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0614
SUMBUFF  DS    0H                                                  0615
*   RFY                                                            0615
*    (R6,                                                          0615
*     R7,                                                          0615
*     R10) RSTD;                                                   0615
*   SMWKSMBF=R14;                   /* SAVE CALLERS RETURN ADDRESS   */
         ST    R14,SMWKSMBF(,R9)                                   0616
*   R7=SMWKALEN;                    /* OBTAIN LENGTH OF VIRTUAL PAGE
*                                      AREA                          */
*                                                                  0617
         LH    R7,SMWKALEN(,R9)                                    0617
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS LOOP WILL MOVE THE VIRTUAL STORAGE TO A REAL FRAME IF THE*/
*   /* SPACE ON THE CURRENT REAL FRAME IS TOO SMALL A NEW FRAME WILL */
*   /* BE OBTAINED AND THE MOVE COMPLETED                            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0618
*   DO WHILE(R7>ZERO);              /* PROCESS WHOLE VIRTUAL AREA    */
         B     @DE00618                                            0618
@DL00618 DS    0H                                                  0619
*     IF SMWKSCBF=ZERO THEN         /* IF THE CURRENT REAL FRAME IS
*                                      FULL                          */
         SLR   @11,@11                                             0619
         CH    @11,SMWKSCBF(,R9)                                   0619
         BNE   @RF00619                                            0619
*       DO;                         /* OBTAIN THE NEXT REAL FRAME    */
*         IF SMWKNFAV>ZERO THEN     /* IF A REAL FRAME IS AVAILABLE  */
         LH    @02,SMWKNFAV(,R9)                                   0621
         CR    @02,@11                                             0621
         BNH   @RF00621                                            0621
*           DO;                     /* USE THE NEXT REAL FRAME       */
*             SMWKNFAV=SMWKNFAV-ONE;/* ONE LESS FRAME AVAILABLE      */
         BCTR  @02,0                                               0623
         STH   @02,SMWKNFAV(,R9)                                   0623
*             SMWKFRUS=SMWKFRUS+ONE;/* ONE MORE FRAME USED           */
         LA    @11,1                                               0624
         LH    @02,SMWKFRUS(,R9)                                   0624
         ALR   @02,@11                                             0624
         STH   @02,SMWKFRUS(,R9)                                   0624
*             SMWKRSIX=SMWKRSIX+ONE;/* INDEX MOVES TO NEW FRAME      */
         AH    @11,SMWKRSIX(,R9)                                   0625
         STH   @11,SMWKRSIX(,R9)                                   0625
*             SMWKRFAD=SMWKFRMA(SMWKRSIX);/* GET REAL ADDRESS OF NEW
*                                      FRAME                         */
         SLA   @11,2                                               0626
         L     @11,SMWKFRMA+192(@11,R9)                            0626
         ST    @11,SMWKRFAD(,R9)                                   0626
*             SMWKSCBF=FRAMSIZE;    /* RE INITIALIZE SPACE REMAINING */
         MVC   SMWKSCBF(2,R9),@CH03187                             0627
*           END;                    /* END OF USE NEXT FRAME         */
*         ELSE                                                     0629
