         TITLE 'IEAVTSSD SVC DUMP SUMMARY DUMP PROCESSOR               *
                        '
IEAVTSSD CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL2(@EP00001-@MAINENT)
         DC    AL1(16)                                             0001
         DC    C'IEAVTSSD  78.135'                                 0001
IEAVTSDS DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL2(@EP00787-IEAVTSDS)
         ENTRY IEAVTSDS
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @12,0                                               0001
@PSTART  LA    @03,4095(,@12)                                      0001
         USING @PSTART,@12                                         0001
         USING @PSTART+4095,@03                                    0001
         AH    @15,4(,@15)                                         0001
         BR    @15                                                 0001
@EP00001 DS    0H                                                  0002
*   R9=RTCTSDSW;                    /* GET ADDRESS SUM DUMP WORK AREA*/
         L     @02,PSAPTR                                          0140
         L     @02,CVTPTR(,@02)                                    0140
         L     @02,CVTRTMCT(,@02)                                  0140
         L     R9,RTCTSDSW(,@02)                                   0140
*   RFY                                                            0141
*     SMWKRSCB BASED(ADDR(SMWKRSM));/* MAP RSM PAR OF WORK AREA      */
*   RFY                                                            0142
*     SMDLR BASED(ADDR(SMWKHEAD));  /* MAP SUM DUMP HEADER RECORD    */
*                                                                  0142
*   /*****************************************************************/
*   /*                                                               */
*   /* ESTABLISH AN FRR RECOVERY EXIT                                */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0143
*   GEN REFS(SUMFRRAD,PSA,PSACSTK,PSALSFCC,FRRS,FRRSCURR,FRRSFRRA, 0143
*       FRRSPARM,FRRSENTR);                                        0143
         SETFRR    A,FRRAD=SUMFRRAD,                                   X
               PARMAD=(R8),WRKREGS=(R10,R11)
*   RFY                                                            0144
*    (R10,                                                         0144
*     R11) UNRSTD;                                                 0144
*   RFY                                                            0145
*     SUMFRR6W BASED(R8);           /* MAP THE 6 WORD FRR WORK AREA  */
*   SUMFRRBR=R12;                   /* SAVE THE BASE REGISTER        */
         ST    R12,SUMFRRBR(,R8)                                   0146
*   SUMFRRB2=R3;                    /* SAVE SECOND BASE REGISTER     */
         ST    R3,SUMFRRB2(,R8)                                    0147
*   SUMFRRSA=R13;                   /* SAVE CALLERS SAVE AREA ADDR   */
         ST    R13,SUMFRRSA(,R8)                                   0148
*   RTCTSDSD=ON;                    /* INDICATE SUMMARY DUMP ENTERED */
         L     @02,PSAPTR                                          0149
         L     @02,CVTPTR(,@02)                                    0149
         L     @02,CVTRTMCT(,@02)                                  0149
         OI    RTCTSDSD(@02),B'00000100'                           0149
*   SUMFRRRT=ADDR(SUMUNLCK);        /* NEXT RETRY LOCATION ADDRESS   */
         LA    @02,SUMUNLCK                                        0150
         ST    @02,SUMFRRRT(,R8)                                   0150
*   CALL SUMLOCK;                   /* OBTAIN THE SALLOC LOCK        */
         BAL   @14,SUMLOCK                                         0151
*   SUMFRRRT=ADDR(SUMFRFRM);        /* GET NEXT RETRY LOCATION     0152
*                                      ADDRESS                       */
*                                                                  0152
         LA    @02,SUMFRFRM                                        0152
         ST    @02,SUMFRRRT(,R8)                                   0152
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE THE WORK AREA AND CALL RSM TO OBTAIN REAL STORAGE  */
*   /* FRAMES FOR THE BUFFER                                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0153
*   IF R15=NULL THEN                /* WAS LOCK OBTAINED OR ALREADY
*                                      OWNED                         */
         LTR   R15,R15                                             0153
         BNZ   @RF00153                                            0153
*     DO;                           /* YES CONTINUE DUMP PROCESSING  */
*       CALL SUMINIT;               /* INIT KEY SUMDUMP WORKAREA   0155
*                                      VALUES                        */
         BAL   @14,SUMINIT                                         0155
*       R13=ADDR(SMWKSAV1);         /* GET ADDRESS OF SAVE AREA FOR
*                                      RSM                           */
         LA    R13,SMWKSAV1(,R9)                                   0156
*       RFY                                                        0157
*         SDUMP BASED(RTCTSDPL);    /* MAP CALLERS PARAMTER LIST   0157
*                                                            @ZM44941*/
*       IF SDUSULST=ON THEN         /* IF SUMLIST IS SPECIFED GET  0158
*                                      LARGE NUMBER OF FRAMES      0158
*                                                            @ZM44941*/
         L     @02,PSAPTR                                          0158
         L     @02,CVTPTR(,@02)                                    0158
         L     @02,CVTRTMCT(,@02)                                  0158
         L     @02,RTCTSDPL(,@02)                                  0158
         TM    SDUSULST(@02),B'00001000'                           0158
         BNO   @RF00158                                            0158
*         SMWKFRHD=NUMFROPT;        /* GET NUMBER OF FRAMES TO OBTAIN*/
         MVC   SMWKFRHD+196(2,R9),@CH01387                         0159
*       ELSE                                                       0160
*         SMWKFRHD=DEFLTNUM;        /* NOT SUMLIST GET SMALL BUFFER
*                                                            @ZM44941*/
         B     @RC00158                                            0160
@RF00158 MVC   SMWKFRHD+196(2,R9),@CH01336                         0160
*       SMWKNFRQ=SMWKFRHD;          /* OBTAIN NUMBER FRAMES REQUESTED
*                                                            @ZM45073*/
@RC00158 LH    @02,SMWKFRHD+196(,R9)                               0161
         N     @02,@CF03420                                        0161
         STH   @02,SMWKNFRQ(,R9)                                   0161
*       R1=ADDR(SMWKRSM);           /* OBTAIN ADDRESS REAL STORAGE 0162
*                                      CONTROL BLOCK                 */
         LA    R1,SMWKRSM(,R9)                                     0162
*       CALL IEAVPRSB;              /* OBTAIN THE REAL STORAGE FROM
*                                      RSM                           */
         L     @02,PSAPTR                                          0163
         L     @02,CVTPTR(,@02)                                    0163
         L     @02,PVTPTR(,@02)                                    0163
         L     @15,IEAVPRSB(,@02)                                  0163
         BALR  @14,@15                                             0163
*       IF SMWKFRHD>ZERO THEN       /* IF SOME STORAGE WAS OBTAINED  */
         LH    @02,SMWKFRHD+196(,R9)                               0164
         N     @02,@CF03420                                        0164
         LTR   @02,@02                                             0164
         BNP   @RF00164                                            0164
*         DO;                       /* CONTINUE WITH THE SUMMMARY  0165
*                                      DUMP                          */
*           SUMFRRRF=ON;            /* INDICATE REAL STORAGE OBTAINED*/
         OI    SUMFRRRF(R8),B'01000000'                            0166
*           SMWKNFAV=SMWKFRHD;      /* NUMBER OBTAINED IS NUMBER   0167
*                                      AVAIL                         */
         STH   @02,SMWKNFAV(,R9)                                   0167
*           SMWKNFAQ=SMWKFRHD;      /* GET NUMBER OF FRAMES ACQUIRED
*                                                            @ZM45073*/
         STH   @02,SMWKNFAQ(,R9)                                   0168
*                                                                  0169
*           /*********************************************************/
*           /*                                                       */
*           /* THIS SECTION BUILDS THE MODEL PSW USED BY SUMBUFF TO  */
*           /* TURN DAT ON AND OFF. SUMBUFF USES LPSW LOGIC TO       */
*           /* PERFORM THIS FUNCTION                                 */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0169
*           GEN REFS(SMWKPSWK,SMWKPSWS);                           0169
         STOSM SMWKPSWS(R9),X'00'  PUT CURRENT SYSTEM MASK IN MODEL PSW
*           SMWKPSWK='0C'X;         /* SET EC ON AND ENABLE FOR    0170
*                                      MACHINE CHECK INTERRUPTS      */
*                                                                  0170
         MVI   SMWKPSWK(R9),X'0C'                                  0170
*           /*********************************************************/
*           /*                                                       */
*           /* MOVE STORAGE AREAS TO THE REAL STORAGE FRAMES         */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0171
*           CALL SUMDUMP;           /* MOVE REQUESTED AREAS TO BUFFER*/
*                                                                  0171
         BAL   @14,SUMDUMP                                         0171
*           /*********************************************************/
*           /*                                                       */
*           /* FREE ANY UNUSED FRAMES                                */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0172
*SUMFRFRM:                                                         0172
*           SUMFRRRT=ADDR(SUMUNLCK);/* NEXT RETRY LOCATION ADDRESS   */
SUMFRFRM LA    @02,SUMUNLCK                                        0172
         ST    @02,SUMFRRRT(,R8)                                   0172
*           SMWKNFUS=SMWKFRUS;      /* SAVE NUMBER FRAMES USED     0173
*                                                            @ZM45073*/
         LH    @02,SMWKFRUS(,R9)                                   0173
         STH   @02,SMWKNFUS(,R9)                                   0173
*           IF SMWKNFAV^=ZERO THEN  /* IF ALL FRAMES NOT USED        */
         LH    @02,SMWKNFAV(,R9)                                   0174
         LTR   @02,@02                                             0174
         BZ    @RF00174                                            0174
*             DO;                   /* RETURN ALL UNUSED FRAMES      */
*               SUMFRRRT=ADDR(SUMUNLCK);/* NEXT RETRY LOCATION ADDR  */
         LA    @02,SUMUNLCK                                        0176
         ST    @02,SUMFRRRT(,R8)                                   0176
*               SMWKRSIX=SMWKRSIX+ONE;/* GET INDEX 1ST FRAME FREED   */
         LA    @02,1                                               0177
         AH    @02,SMWKRSIX(,R9)                                   0177
         STH   @02,SMWKRSIX(,R9)                                   0177
*               DO WHILE(SMWKNFAV>ZERO);/* FREE ALL UNUSED FRAMES    */
         B     @DE00178                                            0178
@DL00178 DS    0H                                                  0179
*                 SMWKFRPT=SMWKFRMA(SMWKRSIX);/* GET FRAME ADDRESS   */
         LA    @02,196                                             0179
         ALR   @02,R9                                              0179
         LH    @11,SMWKRSIX(,R9)                                   0179
         SLA   @11,2                                               0179
         L     @00,SMWKFRMA-4(@11,@02)                             0179
         ST    @00,SMWKFRPT(,@02)                                  0179
*                 SMWKNFAV=SMWKNFAV-ONE;/* DECREMENT AVAIL COUNT     */
         LH    @00,SMWKNFAV(,R9)                                   0180
         BCTR  @00,0                                               0180
         STH   @00,SMWKNFAV(,R9)                                   0180
*                 SMWKFRHD=SMWKFRHD-ONE;/* DECREMENT NUMBER HELD     */
         LH    @00,SMWKFRHD(,@02)                                  0181
         N     @00,@CF03420                                        0181
         BCTR  @00,0                                               0181
         STH   @00,SMWKFRHD(,@02)                                  0181
*                 IF SMWKFRHD=ZERO THEN/* IF THIS IS LAST FRAME      */
         LTR   @00,@00                                             0182
         BNZ   @RF00182                                            0182
*                   SMWKEVRY=ON;    /* TURN ON EVERY BIT TO INSURE 0183
*                                      RSM CLEAN UP                  */
         OI    SMWKEVRY(@02),B'10000000'                           0183
*                 SMWKFRMA(SMWKRSIX)=NULL;/* ZERO FRAME TABLE ENTRY  */
@RF00182 LH    @02,SMWKRSIX(,R9)                                   0184
         LR    @11,@02                                             0184
         SLA   @11,2                                               0184
         LA    @10,196                                             0184
         ALR   @10,R9                                              0184
         SLR   @00,@00                                             0184
         ST    @00,SMWKFRMA-4(@11,@10)                             0184
*                 SMWKRSIX=SMWKRSIX+ONE;/* UPDATE FRAME INDEX NEXT   */
         AL    @02,@CF00067                                        0185
         STH   @02,SMWKRSIX(,R9)                                   0185
*                 R1=ADDR(SMWKRSM); /* GET PARM ADDRESS FOR RSM      */
         LA    R1,SMWKRSM(,R9)                                     0186
*                 R13=ADDR(SMWKSAV1);/* GET RSM SAVE AREA ADDRESS    */
         LA    R13,SMWKSAV1(,R9)                                   0187
*                 CALL IEAVPRSR;    /* RETURN UNUSED FRAME           */
         L     @15,IEAVPRSR(,@10)                                  0188
         BALR  @14,@15                                             0188
*               END;                /* END FREE ALL UNUSED FRAMES    */
@DE00178 LH    @02,SMWKNFAV(,R9)                                   0189
         LTR   @02,@02                                             0189
         BP    @DL00178                                            0189
*               SMWKRSIX=SMWKFRUS;  /* RESTORE CURRENT FRAME INDEX   */
         LH    @02,SMWKFRUS(,R9)                                   0190
         STH   @02,SMWKRSIX(,R9)                                   0190
*             END;                  /* END ALL REAL FRAMES NOT USED  */
*                                                                  0191
*           /*********************************************************/
*           /*                                                       */
*           /* SET UP PERMENANT RETURN CODE BASED ON AMOUNT OF       */
*           /* REQUESTED AREAS ACTUALLY DUMPED                       */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0192
*           IF SMWKRSNM=ON THEN     /* IF ALL REQUESTED AREAS NOT  0192
*                                      MOVED                         */
@RF00174 TM    SMWKRSNM(R9),B'00010000'                            0192
         BNO   @RF00192                                            0192
*             DO;                   /* INDICATED A PARTIAL DUMP      */
*               RTCTSDPR=PARTDUMP;  /* SHOW IN PERMENANT RETURN CODE */
         L     @02,PSAPTR                                          0194
         L     @02,CVTPTR(,@02)                                    0194
         L     @02,CVTRTMCT(,@02)                                  0194
         MVI   RTCTSDPR(@02),X'04'                                 0194
*             END;                  /* END OF INDICATE PARTIAL DUMP  */
*           ELSE                                                   0196
*             RTCTSDPR=NULL;        /* RETURN A ZERO RETURN CODE     */
         B     @RC00192                                            0196
@RF00192 L     @02,PSAPTR                                          0196
         L     @02,CVTPTR(,@02)                                    0196
         L     @02,CVTRTMCT(,@02)                                  0196
         MVI   RTCTSDPR(@02),X'00'                                 0196
*         END;                      /* END OF CONTINUE WITH THE    0197
*                                                             SUMDUMP*/
*                                                                  0197
@RC00192 DS    0H                                                  0198
*       /*************************************************************/
*       /*                                                           */
*       /* RELEASE SALLOC LOCK IF OBTAINED                           */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0198
*SUMUNLCK:                                                         0198
*       SUMFRRRT=NULL;              /* NO RETRY LOCATION ADDRESS     */
@RF00164 DS    0H                                                  0198
SUMUNLCK SLR   @02,@02                                             0198
         ST    @02,SUMFRRRT(,R8)                                   0198
*       IF SUMFRRSL=ON THEN         /* IF SDUMP OBTAINED SALLOC LOCK */
         TM    SUMFRRSL(R8),B'00100000'                            0199
         BNO   @RF00199                                            0199
*         DO;                       /* FREE SALLOC LOCK              */
*           GEN REFS(PSALITA);                                     0201
*                                     /*RELEASE SALLOC LOCK          */
FREELOCK SETLOCK   RELEASE,TYPE=SALLOC,REGS=USE,                       X
               RELATED=(SUMDUMP,IEAVTSSD(GETLOCK))
*           SUMFRRSL=OFF;           /* INDICATE SUM DUMP DOES NOT OWN
*                                      SALLOC                        */
         NI    SUMFRRSL(R8),B'11011111'                            0202
*         END;                      /* END OF FREE SALLOC LOCK       */
*     END;                          /* END LOCK WAS OBTAINED OR    0204
*                                      ALREADY OWNED                 */
*   ELSE                                                           0205
*     DO;                           /* SALLOC LOCK WAS NOT OBTAINED  */
         B     @RC00153                                            0205
@RF00153 DS    0H                                                  0206
*       RTCTSDPR=PARTDUMP;          /* THIS MEANS A PARTIAL SVC DUMP */
         L     @02,PSAPTR                                          0206
         L     @02,CVTPTR(,@02)                                    0206
         L     @02,CVTRTMCT(,@02)                                  0206
         MVI   RTCTSDPR(@02),X'04'                                 0206
*     END;                                                         0207
*   RTCTSDSC=ON;                    /* INDICATE SUMMARY DUMP COMPLETE*/
@RC00153 L     @02,PSAPTR                                          0208
         L     @02,CVTPTR(,@02)                                    0208
         L     @02,CVTRTMCT(,@02)                                  0208
         OI    RTCTSDSC(@02),B'00000001'                           0208
*   R13=SUMFRRSA;                   /* RESTORE CALLERS SAVE AREA ADDR*/
         L     R13,SUMFRRSA(,R8)                                   0209
*   RFY                                                            0210
*    (R10,                                                         0210
*     R11) RSTD;                                                   0210
*                                                                  0210
*   /*****************************************************************/
*   /*                                                               */
*   /* DELETE THE RECOVERY EXIT FRR                                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0211
*   GEN REFS(PSA,PSACSTK,FRRS,FRRSCURR,FRRSEMP,FRRSELEN,FRRSCURR); 0211
    SETFRR    D,WRKREGS=(R10,R11)
*   RFY                                                            0212
*    (R10,                                                         0212
*     R11) UNRSTD;                                                 0212
*   RETURN CODE(0);                 /* RETURN TO CALLER              */
         SLR   @15,@15                                             0213
         L     @14,12(,@13)                                        0213
         LM    @00,@12,20(@13)                                     0213
         BR    @14                                                 0213
*                                                                  0214
*/********************************************************************/
*/*   THIS SEGMENT INITIALIZES KEY FIELDS OF THE SUMMARY DUMP WORK   */
*/*   AREA TO ZERO.  THIS MAKES THE VALUE OF ALL OF THESE FIELDS     */
*/*   KNOWN PRIOR TO ANY SUMMARY DUMP PROCESSING                     */
*/*                                                                  */
*/*   INPUT: UNINITIALIZED SUMMARY DUMP WORK AREA                    */
*/*                                                                  */
*/*   OUTPUT: INITIALIZED SUMMARY DUMP WORK AREA                     */
*/*                                                                  */
*/*   INCLUDED BY: TOP SEGMENT                                       */
*/********************************************************************/
*                                                                  0214
*SUMINIT:                                                          0214
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0214
SUMINIT  DS    0H                                                  0215
*   SMWKPSW=SMWKPSW&&SMWKPSW;       /* ZERO MODULE PSW FOR LPSW      */
         XC    SMWKPSW(8,R9),SMWKPSW(R9)                           0215
*   SMWKRSM(1:256)=SMWKRSM(1:256)&&SMWKRSM(1:256);/* ZERO FIRST PART
*                                      OF RSM PART OF WORK AREA      */
         XC    SMWKRSM(256,R9),SMWKRSM(R9)                         0216
*   SMWKRSM(257:LENGTH(SMWKRSM))=SMWKRSM(257:LENGTH(SMWKRSM))&&SMWKRSM(
*       257:LENGTH(SMWKRSM));       /* ZERO REST OF RSM AREA         */
         XC    SMWKRSM+256(160,R9),SMWKRSM+256(R9)                 0217
*   SMWKFLGS=SMWKFLGS&&SMWKFLGS;    /* ZERO FLAG BYTE AREAS          */
         XC    SMWKFLGS(4,R9),SMWKFLGS(R9)                         0218
*   SMWKRSIX=ZERO;                  /* ZERO STORAGE INDEX FIELD      */
         SLR   @02,@02                                             0219
         STH   @02,SMWKRSIX(,R9)                                   0219
*   SMWKRFAD=ZERO;                  /* ZERO REAL FRAME ADDRESS FIELD */
         ST    @02,SMWKRFAD(,R9)                                   0220
*   SMWKNFAV=ZERO;                  /* ZERO NUMBER OF FRAMES AVAIL 0221
*                                      COUNT                         */
         STH   @02,SMWKNFAV(,R9)                                   0221
*   SMWKFRUS=ZERO;                  /* ZERO NUMBER FRAMES USED COUNT */
         STH   @02,SMWKFRUS(,R9)                                   0222
*   SMWKREGN=ZERO;                  /* ZERO NUMBER OF SAVED REGISTERS*/
         STH   @02,SMWKREGN(,R9)                                   0223
*   SMWKNFRI=ZERO;                  /* ZERO NUMBER FRAMES FREED    0224
*                                      INTERVAL                      */
         STH   @02,SMWKNFRI(,R9)                                   0224
*   SMWKNFRE=ZERO;                  /* ZERO NUMBER OF FRAMES TO BE 0225
*                                      FREED                         */
         STH   @02,SMWKNFRE(,R9)                                   0225
*   SMWKNDIE=ZERO;                  /* ZERO COUNTER TIMES ENTERED DIE*/
         STH   @02,SMWKNDIE(,R9)                                   0226
*   SMWKNFRQ=ZERO;                  /* ZERO NUMBER FRAMES REQUESTED
*                                                            @ZM45073*/
         STH   @02,SMWKNFRQ(,R9)                                   0227
*   SMWKNFAQ=ZERO;                  /* ZERO NUMBER FRAMES ACQUIRED 0228
*                                                            @ZM45073*/
         STH   @02,SMWKNFAQ(,R9)                                   0228
*   SMWKNFUS=ZERO;                  /* ZERO NUMBER FRAMES USED     0229
*                                                            @ZM45073*/
         STH   @02,SMWKNFUS(,R9)                                   0229
*   SMWKNTDI=ZERO;                  /* ZERO NUMBER TIMES DIE ENTERED
*                                                            @ZM45073*/
         STH   @02,SMWKNTDI(,R9)                                   0230
*   SMWKNTDQ=ZERO;                  /* ZERO NUMBER TIMES DIE REQUED
*                                                            @ZM45073*/
         STH   @02,SMWKNTDQ(,R9)                                   0231
*   SMWKNFFD=ZERO;                  /* ZERO NUMBER FRAMES FREED    0232
*                                                            @ZM45073*/
         STH   @02,SMWKNFFD(,R9)                                   0232
*   SMWKNSTB=ZERO;                  /* ZERO NUMBER OF STEAL BACKS  0233
*                                                            @ZM45073*/
         STH   @02,SMWKNSTB(,R9)                                   0233
*   SMWKHEAD=SMWKHEAD&&SMWKHEAD;    /* INITIALIZE RECORD HEADER AREA */
         XC    SMWKHEAD(20,R9),SMWKHEAD(R9)                        0234
*   SMWKSCBF=ZERO;                  /* INITIALIZE CURRENT BUFFER   0235
*                                      SPACE                         */
         STH   @02,SMWKSCBF(,R9)                                   0235
*   SMWKTQEA=ADDR(SMWKTQE);         /* PUT ADDR TQE IN WORK AREA FOR
*                                      SRB                           */
         LA    @02,SMWKTQE(,R9)                                    0236
         ST    @02,SMWKTQEA(,R9)                                   0236
*   SMWKTQE=SMWKTQE&&SMWKTQE;       /* ZERO TQE FOR USE BY DIE       */
         XC    SMWKTQE(128,R9),SMWKTQE(R9)                         0237
*   SMWKTSDS=ADDR(IEAVTSDS);        /* GET STEAL BACK ADDRESS FOR RSM*/
         L     @02,@CA03297                                        0238
         ST    @02,SMWKTSDS+196(,R9)                               0238
*   END SUMINIT;                                                   0239
@EL00002 DS    0H                                                  0239
@EF00002 DS    0H                                                  0239
@ER00002 BR    @14                                                 0239
*                                                                  0240
*/********************************************************************/
*/*   THIS SEGMENT ATTEMPTS TO OBTAIN THE SALLOC LOCK. IF THE LOCK   */
*/*   IS ALREADY OWNED ON THIS CPU, BIT SCHFRRSL IS SET TO ZERO      */
*/*   A RETURN CODE OF ZERO IS PASSED TO THE CALLER. IF THE LOCK IS  */
*/*   NOT ALREADY OWNED A TEST IS MADE TO SEE IF ANY HIGHER LEVEL    */
*/*   LOCKS ARE HELD. IF THERE ARE (IE DISPATCHER OR ASM HELD),      */
*/*   THE SALLOC LOCK IS OBTAINED CONDITIONALLY. IF NO HIGHER LEVEL  */
*/*   LOCKS ARE HELD, THE SALLOC IS OBTAINED UNCONDITIONALLY. IF THE */
*/*   SALLOC LOCK IS OBTAINED BIT SCHFRRSL IS SET TO 1 AND A RETURN  */
*/*   CODE OF ZERO IS PASSED TO THE CALLER. IF THE LOCK CANNOT BE    */
*/*   OBTAINED RETURN CODE 8 IS RETURNED TO THE CALLER.              */
*/*                                                                  */
*/*   MACROS USED=SETLOCK                                            */
*/*                                                                  */
*/*   CALLED BY: TOP SEGMENT                                         */
*/*                                                                  */
*/********************************************************************/
*                                                                  0240
*SUMLOCK:                                                          0240
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0240
SUMLOCK  DS    0H                                                  0241
*   SMWKSMLK=R14;                   /* SAVE CALLERS RETURN ADDRESS   */
         ST    R14,SMWKSMLK(,R9)                                   0241
*   R15=NULL;                       /* INITIALIZE RETURN CODE TO ZERO
*                                                            @ZM45054*/
         SLR   R15,R15                                             0242
*   GEN REFS(PSALITA);                                             0243
*                          /*TEST TO SEE WHAT LOCKS ARE HELD @ZM43803*/
         SETLOCK   TEST,TYPE=SALLOC,BRANCH=(HELD,SSDSLHLD)             X
               RELATED=(SVCDUMP,IEAVTSSD(SUMLOCK))
         SETLOCK   TEST,TYPE=DISP,BRANCH=(HELD,SSDCOND)                X
               RELATED=(SVCDUMP,IEAVTSSD(SUMLOCK))
         SETLOCK   TEST,TYPE=ASM,BRANCH=(HELD,SSDCOND)                 X
               RELATED=(SVCDUMP,IEAVTSSD(SUMLOCK))
GETLOCKU SETLOCK   OBTAIN,TYPE=SALLOC,MODE=UNCOND,REGS=USE,            X
               RELATED=(SVCDUMP,IEAVTSSD(FRELOCKU))
*   IF R15=NULL THEN                /* IF SALLOC LOCK WAS OBTAINED   */
         LTR   R15,R15                                             0244
         BNZ   @RF00244                                            0244
