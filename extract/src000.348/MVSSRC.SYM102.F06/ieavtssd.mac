*                                      RECORD                        */
         MVI   SDWAURAL(R1),X'18'                                  0763
*   SDWAVRA=SUMFRR6W;               /* RECORD FRR 6 WORD WORK AREA   */
         MVI   SDWAVRA+24(R1),C' '                                 0764
         MVC   SDWAVRA+25(83,R1),SDWAVRA+24(R1)                    0764
         MVC   SDWAVRA(24,R1),SUMFRR6W(R8)                         0764
