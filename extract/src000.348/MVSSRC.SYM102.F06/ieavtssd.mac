         LH    @10,SMWKRSIX(,R9)                                   0814
         SLA   @10,2                                               0814
         L     @00,SMWKFRMA-4(@10,@11)                             0814
         ST    @00,SMWKFRMA-4(@02,@11)                             0814
*         END;                      /* END OF A MOVE IS NECESSARY    */
*       SMWKFRMA(SMWKRSIX)=NULL;    /* ZERO BOTTOM LIST ENTRY        */
@RF00803 LH    @02,SMWKRSIX(,R9)                                   0816
         LR    @11,@02                                             0816
         SLA   @11,2                                               0816
         LA    @10,196                                             0816
         ALR   @10,R9                                              0816
         SLR   @00,@00                                             0816
         ST    @00,SMWKFRMA-4(@11,@10)                             0816
*       SMWKFRUS=SMWKFRUS-ONE;      /* ONE LESS FRAME IN USE         */
         LH    @00,SMWKFRUS(,R9)                                   0817
         BCTR  @00,0                                               0817
         STH   @00,SMWKFRUS(,R9)                                   0817
*       SMWKFRHD=SMWKFRHD-ONE;      /* ONE LESS FRAME HELD           */
         LH    @00,SMWKFRHD(,@10)                                  0818
         N     @00,@CF03420                                        0818
         BCTR  @00,0                                               0818
