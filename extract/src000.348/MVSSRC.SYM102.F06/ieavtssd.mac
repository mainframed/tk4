*   R8=SDWAPARM;                    /* RESTORE ADDRESS OF FRR 6 WORDS*/
         L     R8,SDWAPARM(,R1)                                    0753
*   R12=SUMFRRBR;                   /* RESTORE BASE REGISTER         */
         L     R12,SUMFRRBR(,R8)                                   0754
*   R3=SUMFRRB2;                    /* RESTORE SECOND BASE REGISTER  */
         L     R3,SUMFRRB2(,R8)                                    0755
