         TITLE 'IKJEBEAE - EDIT STAE EXIT ROUTINE                      *
                        '
IKJEBEAE CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'IKJEBEAE  78.045'                                 0001
         DROP  @15
@PROLOG  BALR  @11,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@11                                         0001
*   IF R0=TWELVE THEN               /* IS AN SDWA PROVIDED ?         */
         CH    R0,@CH00803                                         0041
         BNE   @RF00041                                            0041
*NOSDWA:                                                           0042
*     DO;                           /* NO - CLEANUP AND RETURN       */
NOSDWA   DS    0H                                                  0043
*       R3=R1;                      /* SAVE THE ABEND COMPLETION CODE*/
         LR    R3,R1                                               0043
*       COMMREG=R2;                 /* ADDRESS OF IKJEBECA           */
         LR    COMMREG,R2                                          0044
*     END NOSDWA;                                                  0045
*   ELSE                            /* A SDWA WAS PROVIDED           */
*YESSDWA:                                                          0046
*     DO;                                                          0046
         B     @RC00041                                            0046
@RF00041 DS    0H                                                  0046
YESSDWA  DS    0H                                                  0047
*       GEN(STM 14,12,12(13));      /* SAVE THE REGISTERS            */
         STM 14,12,12(13)
*       SDWAPTR=R1;                 /* SET UP ADDRESSABILITY TO SDWA */
         LR    SDWAPTR,R1                                          0048
*       COMMREG=SDWAPARM;           /* ADDRESS OF IKJEBECA           */
         L     COMMREG,SDWAPARM(,SDWAPTR)                          0049
*     END YESSDWA;                                                 0050
*   DATAREG=ADDR(CATEMPBF);         /* ADDR OF WORKAREA IN CA        */
@RC00041 LA    DATAREG,CATEMPBF(,COMMREG)                          0051
*   GENERATE REFS(DATAREG);                                        0052
           USING @DATD,DATAREG  ADDRESSABILITY TO WORK AREA
*   AERETPT=R14;                    /* SAVE RETURN ADDRESS           */
         ST    R14,AERETPT                                         0053
*   IF CARECURS='1'B THEN           /* IS RECURSIVE ABEND    @YA01951
*                                      BIT ON -              @YA01951*/
         TM    CARECURS(COMMREG),B'00000010'                       0054
         BO    @RT00054                                            0054
*     GOTO EXIT;                    /* YES, RET TO CALLER    @YA01951*/
*   ELSE                            /* OTHERWISE,            @YA01951*/
*     CARECURS='1'B;                /* TURN BIT ON           @YA01951
*                                      AND CONTINUE          @YA01951*/
         OI    CARECURS(COMMREG),B'00000010'                       0056
*   RESPECIFY                                                      0057
*    (R2) UNRSTD;                                                  0057
*   ZSAVE1=ZSAVE1&&ZSAVE1;          /* ZERO SAVE AREAS               */
         XC    ZSAVE1(256,COMMREG),ZSAVE1(COMMREG)                 0058
*   ZSAVE2=ZSAVE2&&ZSAVE2;          /* ZERO SAVE AREAS               */
         XC    ZSAVE2(256,COMMREG),ZSAVE2(COMMREG)                 0059
*   ZSAVE3=ZSAVE3&&ZSAVE3;          /* ZERO SAVE AREAS               */
         XC    ZSAVE3(136,COMMREG),ZSAVE3(COMMREG)                 0060
*   RTMSAREA=R13;                   /* ADDRESS OF LAST SAVE AREA - 0061
*                                      NOTE R13 MAY BE BAD IF NO SDWA
*                                      WAS PROVIDED                  */
         ST    R13,RTMSAREA(,COMMREG)                              0061
*   IF R0^=TWELVE THEN                                             0062
         CH    R0,@CH00803                                         0062
         BE    @RF00062                                            0062
*     RTMXAREA=ADDR(AESVAREA);      /* PLACE ADDR OF THIS SAVEAREA IN
*                                      THE PREVIOUS AREA TO        0063
*                                      CROSS-CHAIN THEM              */
         LA    @15,AESVAREA(,COMMREG)                              0063
         ST    @15,RTMXAREA(,R13)                                  0063
*   R13=ADDR(AESVAREA);             /* ADDR OF SAVE AREA FOR THIS  0064
*                                      MODULE                        */
@RF00062 LA    R13,AESVAREA(,COMMREG)                              0064
*   CANXTSVA=ADDR(CASVAREA)+2*72;   /* SAVE ADDR OF NEXT SAVEAREA    */
         LA    @15,CASVAREA(,COMMREG)                              0065
         AH    @15,@CH00867                                        0065
         ST    @15,CANXTSVA(,COMMREG)                              0065
*   AEWKAREA=AEWKAREA&&AEWKAREA;    /* ZERO WORK AREA                */
         XC    AEWKAREA(97),AEWKAREA                               0066
*   IF R0=TWELVE THEN               /* NO SDWA PROVIDED              */
         CH    R0,@CH00803                                         0067
         BNE   @RF00067                                            0067
*     DO;                                                          0068
*       AESDWA='1'B;                /* TURN ON SW SAYING NO SDWA     */
*       AEERRMSG='1'B;              /* ISSUE ERROR MESSAGE           */
         OI    AESDWA,B'11000000'                                  0070
*     END;                                                         0071
*   ELSE                                                           0072
*     IF SDWACLUP='0'B THEN         /* AE NOT ENTERED FOR CLEANUP  0072
*                                      ONLY, I.E., A CANCEL OR DETACH*/
         B     @RC00067                                            0072
@RF00067 TM    SDWACLUP(SDWAPTR),B'10000000'                       0072
         BNZ   @RF00072                                            0072
*       AEERRMSG='1'B;              /* ISSUE ERROR MSG               */
         OI    AEERRMSG,B'01000000'                                0073
*     ELSE                                                         0074
*       IF ECTATRM='0'B THEN        /* USER WAS CANCELED             */
         B     @RC00072                                            0074
@RF00072 L     @15,ECTPTR(,COMMREG)                                0074
         TM    ECTATRM(@15),B'00100000'                            0074
         BNZ   @RF00074                                            0074
*         DO;                       /*                       @ZA25432*/
*           AECANCEL='1'B;          /* ISSUE 'EDIT CANCELED' MSG     */
         OI    AECANCEL,B'00100000'                                0076
*           CAX22ABN='1'B;          /* SET USER CANCELED     @ZA25432*/
         OI    CAX22ABN(COMMREG),B'00000010'                       0077
*         END;                      /*                       @ZA25432*/
*       ELSE                                                       0079
*         CADSMODS='0'B;            /* ISSUE NO MSG & TURN OFF THE 0079
*                                      DATASET MODIFIED SWITCH - EDIT
*                                      DETACHED BY TMP               */
         B     @RC00074                                            0079
@RF00074 NI    CADSMODS(COMMREG),B'01111111'                       0079
*   CATPUTVF='0'B;                  /* TELL MS NOT VERIFY    @ZA14254*/
@RC00074 DS    0H                                                  0080
@RC00072 DS    0H                                                  0080
@RC00067 DS    0H                                                  0081
*   CAABEND='1'B;                   /* INDICATE ABEND IN PROGRESS    */
         OI    CAABEND(COMMREG),B'00010000'                        0081
         NI    CATPUTVF(COMMREG),B'11011111'                       0081
*                                                                  0082
*/********************************************************************/
*/*                                                                  */
*/*      MESSAGE ROUTINES                                            */
*/*                                                                  */
*/********************************************************************/
*                                                                  0082
*   IF AEERRMSG='1'B THEN                                          0082
         TM    AEERRMSG,B'01000000'                                0082
         BNO   @RF00082                                            0082
*ERRTN:                                                            0083
*     DO;                           /* ISSUE ERROR MESSAGE           */
ERRTN    DS    0H                                                  0084
*       AECMDLEN=CAMODELN-1;        /* STORE LENGTH OF INSERT        */
         LH    @15,CAMODELN(,COMMREG)                              0084
         BCTR  @15,0                                               0084
         STH   @15,AECMDLEN                                        0084
*       AECMDOFF=10;                /* INSERT OFFSET                 */
         MVC   AECMDOFF(2),@CH00842                                0085
*       AECMDNAM(1:AECMDLEN)=CAMODETX(2:AECMDLEN+1);/* MOVE NAME     */
         BCTR  @15,0                                               0086
         EX    @15,@SM00875                                        0086
*       RESPECIFY                                                  0087
*        (CCTEST,                                                  0087
*         LEVEL2) RSTD;                                            0087
*       IF AESDWA='1'B THEN                                        0088
         TM    AESDWA,B'10000000'                                  0088
         BNO   @RF00088                                            0088
*         CCTEST=R3;                /* CODE SAVED IN REGISTER IF NO
*                                      SDWA                          */
         LR    CCTEST,R3                                           0089
*       ELSE                        /* PICK UP CODE FROM SDWA WHEN 0090
*                                      THERE                         */
*         DO;                                                      0090
         B     @RC00088                                            0090
@RF00088 DS    0H                                                  0091
*           R3=SDWACMPC;            /* PUT CODE IN R3                */
         SLR   R3,R3                                               0091
         ICM   R3,7,SDWACMPC(SDWAPTR)                              0091
*           CCTEST=R3;              /* LOAD IN CCTEST--TO CHECK FOR
*                                      SYSTEM OR USER CODE           */
         LR    CCTEST,R3                                           0092
*         END;                                                     0093
*       CCTEST=CCTEST&'00FFF000'X;  /* TEST FOR SYSTEM OR USER     0094
*                                      COMPLETION CODE               */
@RC00088 N     CCTEST,@CF00843                                     0094
*/********************************************************************/
*/*                                                                  */
*/*      CHECK FOR ABEND DURING SAVE SHICH INDICATES THAT THERE IS   */
*/*      INSUFFICIENT SPACE AVAILABLE TO SAVE INTO THE DATASET       */
*/*      SPECIFIED. IF SO ISSUE MSGS IKJ52427I AND IKJ52428I.        */
*/*  CHECK FOR B37,D37,E37 ABEND. IF SO RECURSION THRU IKJEBEAE IS OK*/
*/*                                                                  */
*/********************************************************************/
*                                                                  0095
*       IF CCTEST='00B37000'X×CCTEST='00D37000'X×CCTEST='00E37000'X
*         THEN                      /* CHECK FOR SPECIFIC ABENDS   0095
*                                                            @OZ01558*/
         C     CCTEST,@CF00844                                     0095
         BE    @RT00095                                            0095
         C     CCTEST,@CF00845                                     0095
         BE    @RT00095                                            0095
         C     CCTEST,@CF00846                                     0095
         BNE   @RF00095                                            0095
@RT00095 DS    0H                                                  0096
*         DO;                                                      0096
*           CARECURS='0'B;          /* INDICATE RECURSION IS OK    0097
*                                                            @OZ01558*/
         NI    CARECURS(COMMREG),B'11111101'                       0097
*           CADSMODS='1'B;          /* FORCE RESAVE OPPORTUNITY    0098
*                                                            @ZA06619*/
         OI    CADSMODS(COMMREG),B'10000000'                       0098
*           IF CAESDSPL=0 THEN      /* WAS SAVE IN CONTROL AT      0099
*                                                            @ZA26020*/
*                                                                  0099
         ICM   @15,15,CAESDSPL(COMMREG)                            0099
         BZ    @RT00099                                            0099
*             /*******************************************************/
*             /*                                                     */
*             /* THE TIME OF THE X37 ABEND@ZA26020                   */
*             /*                                                     */
*             /*******************************************************/
*                                                                  0100
*             GOTO ENDOFMSG;        /* ZERO INDICATES X37 IN @ZA26020*/
*                                                                  0100
*           /*********************************************************/
*           /*                                                       */
*           /* EDIT WORKFILE                                 @ZA26020*/
*           /*                                                       */
*           /*********************************************************/
*                                                                  0101
*           CAX37ABN='1'B;          /* X37 ABEND IN PROCESS  @ZA09473*/
         OI    CAX37ABN(COMMREG),B'00000100'                       0101
*           GENERATE REFS(CAPTMS,AEMSPLST);                        0102
*                                        /* PUT OUT INFO MSG @OZ01558*/
         IKJEBEML 1547,,,,MF=(E,AEMSPLST)
*           RESPECIFY                                              0103
*            (R15) RSTD;            /*                       @OZ01558*/
*           IF R15^=0&R15^=20 THEN  /* AN ERROR OCCURRED     @OZ01558*/
         LTR   R15,R15                                             0104
         BZ    @RF00104                                            0104
         CH    R15,@CH00125                                        0104
         BE    @RF00104                                            0104
*             AERETRY='1'B;         /* INDICATE NO RETRY     @OZ01558*/
         OI    AERETRY,B'00010000'                                 0105
*           RESPECIFY                                              0106
*            (R15) UNRSTD;          /*                       @OZ01558*/
@RF00104 DS    0H                                                  0107
*           GENERATE REFS(CAPTMS,AEMSPLST);                        0107
*                                       /* PUT NEXT INFO MSG @OZ01558*/
         IKJEBEML 1548,,,,MF=(E,AEMSPLST)
*           RESPECIFY                                              0108
*            (R15) RSTD;            /*                       @OZ01558*/
*           IF R15^=0&R15^=20 THEN  /* AN ERROR OCCURRED@OZ01558     */
         LTR   R15,R15                                             0109
         BZ    @RF00109                                            0109
         CH    R15,@CH00125                                        0109
         BE    @RF00109                                            0109
*             AERETRY='1'B;         /* INDICATE NO RETRY     @OZ01558*/
         OI    AERETRY,B'00010000'                                 0110
*         END;                      /*                       @OZ01558*/
@RF00109 DS    0H                                                  0112
*ENDOFMSG:                          /*                       @ZA26020*/
*       IF CCTEST^=0 THEN                                          0112
@RF00095 DS    0H                                                  0112
ENDOFMSG LTR   CCTEST,CCTEST                                       0112
         BZ    @RF00112                                            0112
*SYSCODE:                                                          0113
*         DO;                       /* IT WAS A SYSTEM COMPLETION  0113
*                                      CODE                          */
SYSCODE  DS    0H                                                  0114
*           LEVEL2=1;               /* SECOND LEVEL MESSAGE NUMBER   */
         LA    LEVEL2,1                                            0114
*           AECDEOFF=36;            /* OFFSET OF INSERT              */
         MVC   AECDEOFF(2),@CH00631                                0115
*           AECDELEN=SEVEN;         /* OFFSET LENGTH                 */
         LA    @10,7                                               0116
         STH   @10,AECDELEN                                        0116
*           R5=R3/256;              /* SHIFT RIGHT EIGHT BITS        */
         LR    R5,R3                                               0117
         SRL   R5,8                                                0117
*           R5=R5×AEZONES;          /* ADD ZONE                      */
         O     R5,@CF00787                                         0118
*           AECDETXT(5:8)=R5;       /* STORE VALUE FOR CONVERSION    */
         ST    R5,AECDETXT+4                                       0119
*           UNPK(AECDETXT(1:3),AECDETXT(5:8));/* UNPACK THE CODE     */
         UNPK  AECDETXT(3),AECDETXT+4(4)                           0120
*           AETABADR=ADDR(AETRNSTB)-CHARZERO;/* START TRANSLATION  0121
*                                      TABLE HERE                    */
         LA    @10,AETRNSTB                                        0121
         SH    @10,CHARZERO                                        0121
         ST    @10,AETABADR                                        0121
*           TR(AECDETXT(1:3),AETRNSLT);/* TRANSLATE TO EBCDIC        */
         TR    AECDETXT(3),AETRNSLT(@10)                           0122
*         END SYSCODE;                                             0123
*       ELSE                                                       0124
*USERCODE:                                                         0124
*         DO;                       /* A USER COMPLETION CODE WAS  0124
*                                      PRESENT                       */
         B     @RC00112                                            0124
@RF00112 DS    0H                                                  0124
USERCODE DS    0H                                                  0125
*           LEVEL2=2;               /* SECOND LEVEL MSG NUMBER       */
         LA    LEVEL2,2                                            0125
*           AECDEOFF=34;            /* OFFSET OF INSERT              */
         MVC   AECDEOFF(2),@CH00855                                0126
*           AECDELEN=EIGHT;         /* INSERT LENGTH                 */
         LA    @10,8                                               0127
         STH   @10,AECDELEN                                        0127
*           CVD(R3,AECDETXT(5:8));  /* CONVERT TO EBCDIC             */
         CVD   R3,@TS00001                                         0128
         MVC   AECDETXT+4(4),@TS00001+4                            0128
*           UNPK(AECDETXT(1:4),AECDETXT(5:8));/* PRINTABLE CHARS     */
         UNPK  AECDETXT(4),AECDETXT+4(4)                           0129
*           AECDETXT=AECDETXT×AEZONES;/* ADD ZONE            @ZA26294*/
         OC    AECDETXT(4),@CB00787                                0130
         XC    AECDETXT+4(4),AECDETXT+4                            0130
*         END USERCODE;                                            0131
*                                                                  0131
*       /*************************************************************/
*       /*                                                           */
*       /* PUT OUT THE MESSAGE CONSTRUCTED BY CALLING IKJEBEMS       */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0132
*       GENERATE REFS(CAPTMS,LEVEL2,AECMDINS,AECDEINS,AEMSPLST);   0132
@RC00112 DS    0H                                                  0132
         IKJEBEML 1285,(LEVEL2),AECMDINS,AECDEINS,MF=(E,AEMSPLST)
*       RESPECIFY                                                  0133
*        (CCTEST,                                                  0133
*         LEVEL2) UNRSTD;                                          0133
*       RESPECIFY                                                  0134
*        (R15) RSTD;                                               0134
*       IF R15^=0&R15^=20 THEN      /* AN ERROR WAS ENCOUNTERED,   0135
*                                      DON'T RETRY-CLEANUP & EXIT    */
         LTR   R15,R15                                             0135
         BZ    @RF00135                                            0135
         CH    R15,@CH00125                                        0135
         BE    @RF00135                                            0135
*         AERETRY='1'B;             /* INDICATE NO RETRY             */
         OI    AERETRY,B'00010000'                                 0136
*       RESPECIFY                                                  0137
*        (R15) UNRSTD;                                             0137
@RF00135 DS    0H                                                  0138
*     END ERRTN;                                                   0138
*   IF AECANCEL='1'B THEN           /* SESSION CANCELLED             */
@RF00082 TM    AECANCEL,B'00100000'                                0139
         BNO   @RF00139                                            0139
*CANMSG:                                                           0140
*     DO;                           /* ISSUE CANCEL MESSAGE          */
CANMSG   DS    0H                                                  0141
*       GENERATE REFS(CAPTMS,AEMSPLST);                            0141
          IKJEBEML 1543,,,,MF=(E,AEMSPLST)
*                                   /* PUT OUT MSG TELLING USER EDIT
*                                      SESSION CANCELED              */
*       RESPECIFY                                                  0142
*        (R15) RSTD;                                               0142
*       IF R15^=0&R15^=20 THEN                                     0143
         LTR   R15,R15                                             0143
         BZ    @RF00143                                            0143
         CH    R15,@CH00125                                        0143
         BE    @RF00143                                            0143
*         AERETRY='1'B;             /* DONT RETRY IF AN ERROR      0144
*                                      OCCURRED                      */
         OI    AERETRY,B'00010000'                                 0144
*       RESPECIFY                                                  0145
*        (R15) UNRSTD;                                             0145
@RF00143 DS    0H                                                  0146
*     END CANMSG;                                                  0146
*                                                                  0147
*/********************************************************************/
*/*                                                                  */
*/*   CLEAN-UP PROCESSING.                                           */
*/*                                                                  */
*/* 1. STOP AUTOMATIC LINE NUMBER PROMPTING - ISSUE THE SPAUTOPT     */
*/*    MACRO INSTRUCTION TO INSURE THAT LINE NUMBER PROMPTING IS     */
*/*    STOPPED.                                                      */
*/*                                                                  */
*/* 2. DELETE THE INPUT STACK - INSURE THAT INPUT FOR PROMPT         */
*/*    MESSAGE IN THE RETRY ROUTINE IS FROM THE TERMINAL.            */
*/*                                                                  */
*/* 3. PLACE THE COMPLETION CODE IN THE ECT AND INDICATE THAT AN     */
*/*    ABEND IS IN PROGRESS.                                         */
*/*                                                                  */
*/* 4. FREE ANY GOTTEN CORE - DETERMINE IF A IKJPARS PDL EXISTS, IF  */
*/*    A SUBCOMMAND BUFFER EXISTS, IF AN INCORE DATA SET EXISTS      */
*/*    OR IF A GETMAINED BUFFER EXISTS.                              */
*/*    ISSUE A FREEMAIN MACRO INSTRUCTION FOR ANY OF THE ABOVE THAT  */
*/*    EXISTS.                                                       */
*/*                                                                  */
*/********************************************************************/
*                                                                  0147
*   GEN( SPAUTOPT );                /* STOP AUTOMATIC LINE # PROMPT  */
@RF00139 DS    0H                                                  0147
          SPAUTOPT
*   RESPECIFY                                                      0148
*    (R15) RSTD;                                                   0148
*   IF R15^=0&R15^=8 THEN           /* ERROR OCCURRED, RETRY       0149
*                                      IMPOSSIBLE                    */
         LTR   R15,R15                                             0149
         BZ    @RF00149                                            0149
         CH    R15,@CH00046                                        0149
         BE    @RF00149                                            0149
*     AERETRY='1'B;                 /* INDICATE NO RETRY             */
         OI    AERETRY,B'00010000'                                 0150
*   RESPECIFY                                                      0151
*    (R15) UNRSTD;                                                 0151
@RF00149 DS    0H                                                  0152
*   CALL IKJEBMA2(AESTPB);          /* CALL THE CSECT OF IKJEBEMA TO
*                                      HANDLE THE QUEUE CLEAR & STACK
*                                      DELETE PUT IN NO-OP AND A 1 0152
*                                      FOR THE ID=1 KEYWORD ON THE 0152
*                                      ASSEMBLER CALL MACRO          */
         LA    @15,AESTPB                                          0152
         ST    @15,@AL00001                                        0152
         L     @15,@CV00758                                        0152
         LA    @01,@AL00001                                        0152
         BALR  @14,@15                                             0152
*   GENERATE;                                                      0153
          DC    X'4700'
          DC    AL2(1)
*   RESPECIFY                                                      0154
*    (R15) RSTD;                                                   0154
*   IF R15^=0 THEN                  /* AN ERROR OCCURRED             */
         LTR   R15,R15                                             0155
         BZ    @RF00155                                            0155
*     AERETRY='1'B;                 /* DO NOT RETRY                  */
         OI    AERETRY,B'00010000'                                 0156
*   RESPECIFY                                                      0157
*    (R15) UNRSTD;                                                 0157
@RF00155 DS    0H                                                  0158
*   ECTRTCD=R3;                     /* STORE RETURN CODE IN ECT      */
         L     @15,ECTPTR(,COMMREG)                                0158
         STCM  R3,7,ECTRTCD(@15)                                   0158
*   ECTRCDF='1'B;                   /* INDICATE THAT EDIT IS ABENDING*/
         MVI   ECTRCDF(@15),B'10000000'                            0159
*   RESPECIFY                                                      0160
*    (R2) RSTD;                                                    0160
*   IF CAFREEDL='0'B THEN           /* DOES A PDL EXIST WHICH      0161
*                                      REQUIRES FREEING ?            */
         TM    CAFREEDL(COMMREG),B'10000000'                       0161
         BNZ   @RF00161                                            0161
*FREEPDL:                                                          0162
*     DO;                           /* IF YES RELEASE IT             */
FREEPDL  DS    0H                                                  0163
*       R2=CAPTPRSD;                /* LOAD PDL ADDRESS              */
         L     R2,CAPTPRSD(,COMMREG)                               0163
*       GEN( IKJRLSA (R2) );        /* FREEMAIN IKJPARS PDL          */
          IKJRLSA (R2)
*       CAFREEDL='1'B;              /* TURN OFF PDL INDICATION       */
         OI    CAFREEDL(COMMREG),B'10000000'                       0165
*     END FREEPDL;                                                 0166
*   RESPECIFY                                                      0167
*    (R2) UNRSTD;                                                  0167
@RF00161 DS    0H                                                  0168
*   IF CAPTCORE^=0 THEN                                            0168
         L     @15,CAPTCORE(,COMMREG)                              0168
         LTR   @15,@15                                             0168
         BZ    @RF00168                                            0168
*     DO;                           /* FREE CORE FOR SYNTAX CHECKERS
*                                      OR RUN BUFFER                 */
*       R3=CAPTCORE;                /* PTR TO CORE           @YM04732*/
         LR    R3,@15                                              0170
*       R5=CACORELN;                /* LENGTH OF CORE        @YM04732*/
         L     R5,CACORELN(,COMMREG)                               0171
*       GENERATE REFS(AEFREMNL);                                   0172
*                                     /*                     @YM04732*/
          FREEMAIN E,LV=(R5),A=(R3),SP=1,MF=(E,AEFREMNL)     @YM04732
*       CAPTCORE=0;                 /* INDICATE CORE FREED           */
         SLR   @15,@15                                             0173
         ST    @15,CAPTCORE(,COMMREG)                              0173
*     END;                                                         0174
*   CALL IKJEBMA2(AEFREMNL);        /* BRANCH TO FREE CURRENT INPUT
*                                      BUFFER DUPLICATE THE ID=0 PARM
*                                      OF ASSEMBLER/CALL             */
@RF00168 LA    @15,AEFREMNL                                        0175
         ST    @15,@AL00001                                        0175
         L     @15,@CV00758                                        0175
         LA    @01,@AL00001                                        0175
         BALR  @14,@15                                             0175
*   GENERATE;                                                      0176
          DC    X'4700'
          DC    AL2(0)
*                                                                  0177
*/********************************************************************/
*/*                                                                  */
*/*   IF SDWA WAS PASSED AND NO ERRORS WERE ENCOUNTERED IN AE        */
*/*   PROCESSING, THEN RETRY--PASS THE RETRY ADDRESS (IN IKJEBEMA),  */
*/*   A RETURN CODE OF 4 INDICATING RETRY, AND TELL RTM TO FREE      */
*/*   THE SDWA. IF ERRORS OCCURRED IN PROCESSING, A RETURN CODE OF   */
*/*   ZERO IS SET TO INDICATE TERMINATION IS TO CONTINUE.            */
*/*                                                                  */
*/*   IF NO SDWA WAS PASSED, LINK TO IKJEBEEN (END) TO CLEAN UP      */
*/*   BEFORE RETURNING.                                              */
*/*                                                                  */
*/********************************************************************/
*                                                                  0177
*   R1=SDWAPTR;                     /* SET UP POINTER TO SDWA        */
         LR    R1,SDWAPTR                                          0177
*   IF AESDWA='0'B&                 /* SDWA WAS PRESENT              */
*       AERETRY='0'B                /* NO ERRORS IN CLEANUP          */
*       &SDWACLUP='0'B              /* NOT ENTERED FOR CLEANUP ONLY  */
*       &CAINPROC='0'B              /* NOT AN IN-CORE PROCEDURE      */
*     THEN                                                         0178
         TM    AESDWA,B'10010000'                                  0178
         BNZ   @RF00178                                            0178
         TM    SDWACLUP(SDWAPTR),B'10000000'                       0178
         BNZ   @RF00178                                            0178
         TM    CAINPROC(COMMREG),B'00000100'                       0178
         BNZ   @RF00178                                            0178
*RETRY:                                                            0179
*     DO;                           /* GO TO THE RETRY ROUTINE       */
RETRY    DS    0H                                                  0180
*       R13=RTMSAREA;               /* RESTORE SAVE AREA ADDRESS     */
         L     R13,RTMSAREA(,COMMREG)                              0180
*       DO;                         /* SETRP RETADDR(AERTRY)RC(4)REGS
*                                      (14,12)FRESDWA(YES)           */
*         RESPECIFY                                                0182
*          (GPR00P,                                                0182
*           GPR01P,                                                0182
*           GPR14P,                                                0182
*           GPR15P) RSTD;                                          0182
*         GPR01P->SDWARCDE=4;       /* STORE RC INTO SDWA            */
         MVI   SDWARCDE(GPR01P),X'04'                              0183
*         GPR01P->SDWARTYA=ADDR(AERTRY);/* SAVE RETRY ADDRESS        */
         MVC   SDWARTYA(4,GPR01P),CAPTRTRY(COMMREG)                0184
*         GPR01P->SDWAFREE='1'B;    /* SET FLAG TO FREE SDWA         */
         OI    SDWAFREE(GPR01P),B'00000100'                        0185
*         GEN(RETURN (14,12));                                     0186
         RETURN (14,12)
*         RESPECIFY                                                0187
*          (GPR00P,                                                0187
*           GPR01P,                                                0187
*           GPR14P,                                                0187
*           GPR15P) UNRSTD;                                        0187
*       END;                                                       0188
*     END RETRY;                                                   0189
*   ELSE                                                           0190
*NORETRY:                                                          0190
*     DO;                           /* NO RETRY-CALL END TO FINISH 0190
*                                      CLEANUP                       */
         B     @RC00178                                            0190
@RF00178 DS    0H                                                  0190
NORETRY  DS    0H                                                  0191
*       IF AERETRY='0'B THEN                                       0191
         TM    AERETRY,B'00010000'                                 0191
         BNZ   @RF00191                                            0191
*         DO;                       /* GO TO END IF ALL STILL OK     */
*           PARMREG1=COMMREG;       /* GET COMM AREA PTR     @ZA09473*/
         LR    PARMREG1,COMMREG                                    0193
*           CALL IKJEBEEN;          /* GO TO END             @ZA09473*/
         L     @15,@CV00865                                        0194
         BALR  @14,@15                                             0194
*         END;                                                     0195
*EXIT:                                                             0196
*       IF AESDWA='0'B THEN                                        0196
@RF00191 DS    0H                                                  0196
EXIT     TM    AESDWA,B'10000000'                                  0196
         BNZ   @RF00196                                            0196
*         DO;                                                      0197
*           R13=RTMSAREA;           /* SAVE AREA ADDRESS             */
         L     R13,RTMSAREA(,COMMREG)                              0198
*           DO;                     /* SETRP RC(0)REGS(14,12)        */
*             RESPECIFY                                            0200
*              (GPR00P,                                            0200
*               GPR01P,                                            0200
*               GPR14P,                                            0200
*               GPR15P) RSTD;                                      0200
*             GPR01P->SDWARCDE=0;   /* SAVE RC VALUE                 */
         MVI   SDWARCDE(GPR01P),X'00'                              0201
*             GEN(RETURN (14,12));                                 0202
         RETURN (14,12)
*             RESPECIFY                                            0203
*              (GPR00P,                                            0203
*               GPR01P,                                            0203
*               GPR14P,                                            0203
*               GPR15P) UNRSTD;                                    0203
*           END;                    /* CONTINUE TERMINATION: ERRORS
*                                      WERE ENCOUNTERED OR IT WAS AN
*                                      INCORE PROC                   */
*         END;                                                     0205
*       ELSE                                                       0206
*         R15=0;                    /* NO SDWA WAS PROVIDED          */
         B     @RC00196                                            0206
@RF00196 SLR   R15,R15                                             0206
*     END NORETRY;                                                 0207
@RC00196 DS    0H                                                  0208
*   R14=AERETPT;                    /* LOAD RETURN ADDRESS           */
@RC00178 L     R14,AERETPT                                         0208
*   RETURN;                                                        0209
@EL00001 DS    0H                                                  0209
@EF00001 DS    0H                                                  0209
@ER00001 BR    @14                                                 0209
