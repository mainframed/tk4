         TITLE 'IKJEBELI-EDIT COMMAND, INSERT/REPLACE/DELETE SUBCMD    *
                        '
IKJEBELI CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'IKJEBELI  74.206'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @11,0                                               0001
@PSTART  LA    @06,4095(,@11)                                      0001
         USING @PSTART,@11                                         0001
         USING @PSTART+4095,@06                                    0001
         TITLE 'IKJEBELI - INSERT/REPLACE/DEL SUBCMD - EDIT CP'
*           COMMREG = PARMREG1;      /* GET ADDR OF IKJEBECA         */
*                                                                  0072
         LR    COMMREG,PARMREG1                                    0072
*           RESPECIFY (GPR13P,GPR15P) RSTD;                        0073
*           GPR15P = CANXTSVA;       /* SAVE AREA FOR THIS MODULE    */
         L     GPR15P,CANXTSVA(,COMMREG)                           0074
*           EVAL(GPR15P + 4) -> CASAVPTR = GPR13P;   /* SET THIS     */
         LA    @14,4                                               0075
         ALR   @14,GPR15P                                          0075
         ST    GPR13P,CASAVPTR(,@14)                               0075
*                                    /* SAVEAREA BKWD PTR            */
*           EVAL(GPR13P + 8) -> CASAVPTR = GPR15P;   /* SET CALLER   */
         LA    @14,8                                               0076
         ALR   @14,GPR13P                                          0076
         ST    GPR15P,CASAVPTR(,@14)                               0076
*                                    /* FWD PTR                      */
*           GPR13P = GPR15P;         /* NEW SAVEAREA ADDR            */
         LR    GPR13P,GPR15P                                       0077
*           CANXTSVA = GPR13P + 72;   /* UPDATE NEX-SAVE-AREA PTR    */
         LA    @14,72                                              0078
         ALR   @14,GPR13P                                          0078
         ST    @14,CANXTSVA(,COMMREG)                              0078
*           RESPECIFY (GPR13P,GPR15P) UNRSTD;                      0079
*                                                                  0079
*           DATAREG = ADDR(CASCWKA);   /* SET BASE REG FOR WORK AREA */
         LA    DATAREG,CASCWKA(,COMMREG)                           0080
*           GEN( USING @DATD,DATAREG);   /* SET ADDRESSABILITY TO    */
          USING @DATD,DATAREG
*                                    /* CASCWKA                      */
*        /* THESE REGISTERS ARE ALWAYS RESTRICTED                     *
*        RESPECIFY (COMMREG, BASEREG, DATAREG) RSTD;               0082
*                                                                  0082
*        CCODE = 0;                    /* INITIALIZE RETURN CODE      *
*                                                                  0083
         SLR   @07,@07                                             0083
         LR    CCODE,@07                                           0083
*        FLAGS = FLAGS&&FLAGS;         /* INITIALIZE SWITCHES TO ZERO *
*                                                                  0084
         XC    FLAGS(1),FLAGS                                      0084
*        /* BACK UP BUFFER SCAN POINTER FOR PARSE                     *
*        CMDBFR (3:4) = CMDBFR (3:4) && CMDBFR (3:4);              0085
         L     @03,CAPTIBFR(,COMMREG)                              0085
         XC    CMDBFR+2(2,@03),CMDBFR+2(@03)                       0085
*        PPLPTR = ADDR (CATMPLST);     /* SET PTR TO PARSE PARM LIST  *
         LA    PPLPTR,CATMPLST(,COMMREG)                           0086
*        PCLPTR = ADDR (IKJEBLI1);     /* POINT TO PARSE PDL          *
         L     @03,@CV01097                                        0087
         ST    @03,PCLPTR(,PPLPTR)                                 0087
*        PPLANS = ADDR ( PDLPTR);      /* POINT TO TABLE RETURNED BY
*                                         PARSE                       *
         LA    @03,PDLPTR(,COMMREG)                                0088
         ST    @03,PPLANS(,PPLPTR)                                 0088
*        PPLCBUF = CAPTIBFR;           /* POINT TO COMMAND BUFFER     *
         L     @03,CAPTIBFR(,COMMREG)                              0089
         ST    @03,PPLCBUF(,PPLPTR)                                0089
*        PPLUWA = 0;                   /* INDICATE NO USER WORK AREA  *
         ST    @07,PPLUWA(,PPLPTR)                                 0090
*          DO; /*CALLTSSR EP(IKJPARS)MF(E,PPL)*/                   0091
*            RESPECIFY GPR01P RSTD;                                0092
*            GPR01P = ADDR(PPL);                                   0093
         LR    GPR01P,PPLPTR                                       0093
*            IF CVTPARS = 0                                        0094
*              THEN                                                0094
         L     @15,CVTPTR                                          0094
         C     @07,CVTPARS(,@15)                                   0094
         BNE   @RF00094                                            0094
*                GEN(LINK EP=IKJPARS);                             0095
         LINK EP=IKJPARS
*              ELSE                                                0096
*                CALL CVTPARS;                                     0096
         B     @RC00094                                            0096
@RF00094 L     @07,CVTPTR                                          0096
         L     @15,CVTPARS(,@07)                                   0096
         BALR  @14,@15                                             0096
*            RESPECIFY GPR01P UNRSTD;                              0097
@RC00094 DS    0H                                                  0098
*          END; /* INVOKE IKJPARS                                  0098
*                  SERVICE ROUTINE         */                      0098
*                                                                  0098
*        /* CHECK RETURN CODE FROM PARSE                              *
*        IF RCODEREG = 0                                           0099
*          THEN;                       /* IF RETURN CODE = 0 FALL  0099
*                                         THROUGH                     *
         LTR   RCODEREG,RCODEREG                                   0099
         BZ    @RT00099                                            0099
*          ELSE                        /* IDENTIFY RETURN CODE        *
*          /* DIAGNOSE ERROR CONDITION                                *
*            DO;                                                   0101
*              WREG1 = RCODEREG;       /* SAVE RETURN CODE            *
*                                                                  0102
         LR    WREG1,RCODEREG                                      0102
*              /* CHECK IF NO PROMPT USER                             *
*              IF WREG1 = 4                                        0103
*                THEN                  /* RETURN CODE IS 4            *
         CH    WREG1,@CH00376                                      0103
         BNE   @RF00103                                            0103
*                  DO;                 /* PROCESS RET CODE OF 4       *
*FLUSH:              CCODE=8;          /* INDICATE FLUSH NEEDED       *
FLUSH    LA    CCODE,8                                             0105
*                    GOTO EXIT;        /* RETURN TO COMMAND MODE      *
         B     EXIT                                                0106
*                  END;                                            0107
*                                                                  0107
*              /* CHECK IF ATTN ISSUED                                *
*              IF WREG1 = 8                                        0108
*                THEN                  /* RETURN CODE IS 8            *
@RF00103 CH    WREG1,@CH00049                                      0108
         BNE   @RF00108                                            0108
*                  DO;                 /* PROC&SS R&TURN CODE OF 8    *
*                    GOTO EXIT;        /* RETURN TO COMMAND MODE      *
         B     EXIT                                                0110
*                  END;                                            0111
*                                                                  0111
*              /* CHECK IF NOT ENOUGH CORE AVAILABLE                  *
*              IF WREG1 = 16                                       0112
*                THEN                  /* RETURN CODE IS 16           *
@RF00108 CH    WREG1,@CH00224                                      0112
         BNE   @RF00112                                            0112
*                  DO;                 /* PROCESS RETURN CODE OF 16   *
*NOCOREMG: ;                           /* SEND NO CORE MSG TO USER    *
NOCOREMG DS    0H                                                  0115
*                    RELMSG1 = 270;    /* LOAD MSG NUMBER             *
         MVC   RELMSG1(2),@CH01128                                 0115
*                    RELMSG2 = 0;      /* LOAD REL LEVEL-2 NUMBER     *
         SLR   @12,@12                                             0116
         STH   @12,RELMSG2                                         0116
*                    LEV1INS = ADDR (INSM25); /* LOAD INSERTION PTR   *
         LA    LEV1INS,INSM25                                      0117
*                    LEV2INS = 0;        /* INDICATE NO LEVEL-2 INS   *
         LR    LEV2INS,@12                                         0118
*                    CALL MSGRTN;      /* PUT OUT MESSAGE             *
         BAL   @14,MSGRTN                                          0119
*                    GOTO FLUSH;       /* BRANCH TO INDICATE FLUSH    *
         B     FLUSH                                               0120
*                  END;                                            0121
*                ELSE                  /* RETURN CODE IS 12           *
*                  DO;                                             0122
*                                                                  0122
@RF00112 DS    0H                                                  0123
*                    /* ISSUE 'COMMAND SYSTEM ERROR' MESSAGE          *
*                    CMDER1 = M313INS1;/* MOVE MODEL TO WORK AREA     *
         MVC   CMDER1(16),M313INS1                                 0123
*                    CMDER2 = M313INS2;/* MOVE MODEL TO WORK AREA     *
*                                                                  0124
         MVC   CMDER2(16),M313INS2                                 0124
*SYSERR: ;                             /* BRANCH ADDRESS              *
SYSERR   DS    0H                                                  0126
*                    /* ENTER HERE FOR INSERTIONS OTHER THAN 'PARSE'  *
*                                                                  0126
*                    /* CONVERT RETURN CODE TO PRINTABLE CHARACTERS   *
*                    RFY CVDREG RSTD;                              0126
*                    CVDREG = WREG1;                               0127
         LR    CVDREG,WREG1                                        0127
*                    RFY CVDREG RSTD;                              0128
*                    CVDREG = WREG1;                               0129
         LR    CVDREG,WREG1                                        0129
*                    GEN;                                          0130
PKLEN    EQU   8                       LENGTH OF PACK OR UNPACK
PKOFF    EQU   7                       OFFSET FOR MOVE
MLEN     EQU   1                       LENGTH OF MOVE ZONE
PKOFF2   EQU   6                       OFFSET FOR MOVE
         CVD   CVDREG,PACKTEMP         CONVERT TO DECIMAL
         UNPK  PACKAREA(PKLEN),PACKTEMP(PKLEN) UNPACK NUMBER
         MVZ   PACKAREA+PKOFF(MLEN),PACKAREA+PKOFF2 SET SIGN
*                    RFY CVDREG UNRSTD;                            0131
*                    RFY CVDREG UNRSTD;                            0132
*                    CER2PTR = ADDR (CMDER2); /* PTR TO SECOND     0133
*                                                INSERTION            *
*                                                                  0133
         LA    @12,CMDER2                                          0133
         ST    @12,CER2PTR                                         0133
*                    /* MOVE COND CODE TO MODEL INSERTION             *
*                    CER2TXT (1:2) = PACKAREA (7:8);               0134
         MVC   CER2TXT(2),PACKAREA+6                               0134
*                    RELMSG1 = 271;    /* LOAD MESSAGE NUMBER         *
         MVC   RELMSG1(2),@CH01131                                 0135
*                    RELMSG2 = 1;      /* LOAD LEV-2 NUMBER           *
         MVC   RELMSG2(2),@CH00051                                 0136
*                    LEV1INS = 0;        /* INDICATE NO LEV-1 INSERT  *
         SLR   LEV1INS,LEV1INS                                     0137
*                    LEV2INS = ADDR (CMDER1); /* LEVEL-2 INSERT       *
         LA    LEV2INS,CMDER1                                      0138
*                    CALL MSGRTN;      /* GO PUT OUT MESSAGE          *
         BAL   @14,MSGRTN                                          0139
*                    GOTO FLUSH;       /* BRANCH TO INDICATE FLUSH    *
         B     FLUSH                                               0140
*                  END;                                            0141
*                  END;                                            0142
*                                                                  0142
*              /* CHECK FOR PRESENCE OF SECOND OPERAND                *
*              IF TXTLEN ^= 0                                      0143
*                THEN;                 /* YES, SECOND OPERAND PRESENT
*                                         - INSERT OR REPLACE         *
@RT00099 L     @07,PDLPTR(,COMMREG)                                0143
         LH    @07,TXTLEN(,@07)                                    0143
         LTR   @07,@07                                             0143
         BZ    @RF00143                                            0143
*                ELSE                  /* NO, SECOND OPERAND NULL     *
*                  GOTO DELETE;        /* GO DELETE RECORD            *
*                                                                  0145
*              /* CHECK FOR STRING LENGTH GREATER THAN 255            *
*              IF TXTLEN <= 255                                    0146
*                THEN;                 /* OK, LESS THAN 255           *
         CH    @07,@CH01132                                        0146
         BNH   @RT00146                                            0146
*                ELSE                  /* NOT OK, GREATER THAN 255    *
*                  DO;                                             0148
*                    TXTLEN = 255;     /* RESET LENGTH TO MAX         *
         L     @07,PDLPTR(,COMMREG)                                0149
         MVC   TXTLEN(2,@07),@CH01132                              0149
*                    MSGSW1 = '1'B;    /* INDICATE LINE OVERFLOW      *
         OI    MSGSW1,B'10000000'                                  0150
*                  END;                                            0151
*              OUTSTRNG (1:TXTLEN) = TXTFLD (1:TXTLEN); /* MOVE RECORD
*                                                   TO WORK AREA      *
*                                                                  0152
@RT00146 L     @07,PDLPTR(,COMMREG)                                0152
         LH    @15,TXTLEN(,@07)                                    0152
         BCTR  @15,0                                               0152
         L     @01,TXTPTR(,@07)                                    0152
         EX    @15,@SM01195                                        0152
*              /* CHECK FOR POSITIONING BY *                          *
*              IF NUMFLD (1) = '*'                                 0153
*                THEN                  /* YES, * USED, CONTINUE       *
         L     @07,NUMPTR(,@07)                                    0153
         CLI   NUMFLD(@07),C'*'                                    0153
         BNE   @RF00153                                            0153
*                  OUTKEY = CACURNUM;  /* SET REC KEY TO CURR NUMBER  *
         MVC   OUTKEY(4),CACURNUM(COMMREG)                         0154
*                ELSE                  /* NO OPERATION BY LINE NUM    *
*                /* FUNCTION BASED ON LINE NUMBER ENTERED             *
*                  DO;                                             0155
*                                                                  0155
         B     @RC00153                                            0155
@RF00153 DS    0H                                                  0156
*                    /* CHECK FOR LINE-NUMBERED DATA SET              *
*                    IF CANONUM = '0'B                             0156
*                      THEN            /* IF DATA SET IS LINE NUMBERED*
         TM    CANONUM(COMMREG),B'00000001'                        0156
         BNZ   @RF00156                                            0156
*                        DO;           /* UPDATE CURRENT LINE POINTER.*
*                          WREG1 = NUMLEN; /* PUT NUMBER LENGTH IN REG*
         L     @07,PDLPTR(,COMMREG)                                0158
         LH    WREG1,NUMLEN(,@07)                                  0158
*                          WREG2 = ADDR (NUMFLD); /* POINT TO NUMBER  *
         L     WREG2,NUMPTR(,@07)                                  0159
*                          CALL CONVERT; /* CONVERT LINE NUMBER TO BIN*
         BAL   @14,CONVERT                                         0160
*                          OUTKEY = WREG1; /* UPDATE CURRENT LINE PTR *
         ST    WREG1,OUTKEY                                        0161
*                        END;                                      0162
*                      ELSE            /* DATA SET IS NOT NUMBERED    *
*                        DO;                                       0163
         B     @RC00156                                            0163
@RF00156 DS    0H                                                  0164
*UNNUMDS:                  /* DATA SET 'NONUM', LINE NUMBER ENTERED   *
*                            RELMSG1 = 1288; /* PUT OUT ERR MSG LOAD
*                                           MSG NUMBER                *
UNNUMDS  MVC   RELMSG1(2),@CH01137                                 0164
*                            RELMSG2=0;     /* NO SECOND-LEVEL MSG    *
         SLR   @12,@12                                             0165
         STH   @12,RELMSG2                                         0165
*                            LEV1INS=0;        /* NO LEVEL-1 INSERT   *
         LR    LEV1INS,@12                                         0166
*                            LEV2INS=0;        /* NO LEVEL-2 INSERT   *
         LR    LEV2INS,@12                                         0167
*                          CALL MSGRTN;/* PUT OUT MESSAGE             *
         BAL   @14,MSGRTN                                          0168
*                          GOTO FLUSH; /* BRANCH TO INDICATE FLUSH    *
         B     FLUSH                                               0169
*                        END;                                      0170
*                  END;                                            0171
@RC00156 DS    0H                                                  0172
*              LETXTPTR = ADDR (LESTRNG); /* POINT TO TEXT TO BE   0172
*                                            EDITED                   *
@RC00153 LA    @07,LESTRNG                                         0172
         ST    @07,LETXTPTR                                        0172
*              LEFLG = '00'X;          /* INIT FLAGS FOR IKJEBELE     *
         MVI   LEFLG,X'00'                                         0173
*              LECAREA = COMMREG;      /* STORE COMM AREA FOR LE      *
         ST    COMMREG,LECAREA                                     0174
*              LESTRLEN = TXTLEN + 4;  /* SET STRING LENGTH FOR LE    *
         L     @07,PDLPTR(,COMMREG)                                0175
         LA    @15,4                                               0175
         AH    @15,TXTLEN(,@07)                                    0175
         STH   @15,LESTRLEN                                        0175
*              LETXT (1:LESTRLEN) = TXTFLD; /* MOVE LINE TO BE EDITED
*                                              TO WORK AREA           *
*                                                                  0176
         BCTR  @15,0                                               0176
         L     @07,TXTPTR(,@07)                                    0176
         EX    @15,@SM01197                                        0176
*              /* CALL IKJEBELE TO EDIT THE INPUT LINE FOR TABS AND   *
*              /* UPPER CASE                                          *
*              GEN ;                                               0177
*                                                                  0177
         IKJEBESH (COMMREG),IKJEBELE,PARAM=((COMMREG),LEPARM),         X
               MF=(E,SRWORK)
*              /* CHECK FOR LINE OVERFLOW INDICATION                  *
*              IF RCODEREG = 0 & MSGSW1 = '0'B                     0178
*                THEN;                 /* CODE=0 , CONTINUE           *
         LTR   RCODEREG,RCODEREG                                   0178
         BNZ   @RF00178                                            0178
         TM    MSGSW1,B'10000000'                                  0178
         BZ    @RT00178                                            0178
*                ELSE                  /* CODE NOT 0 - ISSUE MESSAGE  *
*                  DO;                                             0180
@RF00178 DS    0H                                                  0181
*                    OUTLEN = CALRECL; /* SET LENGTH OF NEW RECORD TO
*                                         LRECL                       *
         LH    @07,CALRECL(,COMMREG)                               0181
         STH   @07,OUTLEN                                          0181
*                    WREG2 = CALRECL - 4; /* GET LINE LENGTH FOR   0182
*                                            CONVERSION               *
*                                                                  0182
         SH    @07,@CH00376                                        0182
         LR    WREG2,@07                                           0182
*                    /* CONVERT LINE SIZE TO PRINTABLE CHARACTERS     *
*                    RFY CVDREG RSTD;                              0183
*                    CVDREG = WREG2;                               0184
         LR    CVDREG,WREG2                                        0184
*                    GEN;                                          0185
ZONESIGN EQU   X'F0'                   EBCDIC NUMERIC SIGN
         CVD   CVDREG,PACKTEMP         CONVERT LENGTH TO DECIMAL
         UNPK  PACKAREA(PKLEN),PACKTEMP(PKLEN)
         OI    PACKAREA+PKOFF,ZONESIGN SET SIGN BITS
*                    RFY CVDREG UNRSTD;                            0186
*                    M44INLN = 7;      /* STORE LENGTH OF INSERTION   *
         MVC   M44INLN(2),@CH00115                                 0187
*                    M44INX = 25;      /* SET OFFSET                  *
*                                                                  0188
         MVC   M44INX(2),@CH01139                                  0188
*                    /* MOVE LINE LENGTH TO INSERTION AREA            *
*                    M44INTXT (1:3) = PACKAREA (6:8);              0189
         MVC   M44INTXT(3),PACKAREA+5                              0189
*                    M44IN2PT = 0;        /* NO LEVEL 2 INSERTIONS    *
*                                                                  0190
         SLR   @07,@07                                             0190
         ST    @07,M44IN2PT                                        0190
*                    /* SEE IF DATA SET IS LINE NUMBERED              *
*                    IF CANONUM = '1'B                             0191
*                      THEN            /* DATA SET NOT NUMBERED       *
         TM    CANONUM(COMMREG),B'00000001'                        0191
         BNO   @RF00191                                            0191
*                        DO;                                       0192
*                          LEV1INS = 0;        /* NO LEVEL-1 INSERT   *
         LR    LEV1INS,@07                                         0193
*                          GOTO NOLEV2;/* CONTINUE PROCESSING         *
         B     NOLEV2                                              0194
*                        END;                                      0195
*                      ELSE;           /* DATA SET IS NUMBERED        *
@RF00191 DS    0H                                                  0197
*                    WREG2 = OUTKEY;   /* GET CURRENT LINE NUMBER     *
         L     WREG2,OUTKEY                                        0197
*                    RFY CVDREG RSTD;                              0198
*                    CVDREG = WREG2;                               0199
         LR    CVDREG,WREG2                                        0199
*                    GEN (CVD   CVDREG,PACKTEMP); /*CONVERT TO DECIMAL*
         CVD   CVDREG,PACKTEMP
*                    RFY CVDREG UNRSTD;                            0201
*                                                                  0201
*                    /* CONVERT LINE NUMBER TO PRINTABLE CHARACTERS   *
*                    GEN (UNPK  PACKAREA(PKLEN),PACKTEMP(PKLEN));  0202
         UNPK  PACKAREA(PKLEN),PACKTEMP(PKLEN)
*                    /* SET SIGN BITS                                 *
*                    GEN (OI    PACKAREA+PKOFF,ZONESIGN);          0203
*                                                                  0203
         OI    PACKAREA+PKOFF,ZONESIGN
*                    /* THIS DO LOOP LOCATES THE FIRST NON-ZERO       *
*                    /* BYTE OF THE NUMBER TO BE SENT TO THE          *
*                    /* TERMINAL. IT SCANS FROM LEFT TO RIGHT         *
*                    DO WREG2 = 1 TO LENGTH (PACKAREA) BY 1;       0204
*                                                                  0204
         LA    WREG2,1                                             0204
@DL00204 DS    0H                                                  0205
*                      /* CHECK CURRENT CHARACTER FOR DECIMAL ZERO    *
*                      IF PACKAREA (WREG2) = '0'                   0205
*                        THEN;         /* ZERO - CONTINUE LOOP        *
         LA    @07,PACKAREA-1(WREG2)                               0205
         CLI   0(@07),C'0'                                         0205
         BNE   @RF00205                                            0205
*                        ELSE          /* NOT ZERO - EXIT             *
*                          GOTO LPOUT; /* EXIT FROM LOOP              *
*                    END;              /* WREG2 CONTAINS THE OFFSET   *
         AH    WREG2,@CH00051                                      0208
         CH    WREG2,@CH00049                                      0208
         BNH   @DL00204                                            0208
*                    WREG2 = LENGTH (PACKAREA); /* RESET TO MAX VALUE *
*                                                                  0209
         LA    WREG2,8                                             0209
*LPOUT:              /* CONTINUE FIND LENGTH OF NUM                   *
*                    WREG1 = LENGTH (PACKAREA) + 1 - WREG2;        0210
*                                                                  0210
LPOUT    LA    WREG1,9                                             0210
         SLR   WREG1,WREG2                                         0210
*                    /* MOVE NONZERO PORTION OF NUMBER TO INSERTION   *
*                    /* RECORD                                        *
*                    SYNERRTX(1:WREG1)=PACKAREA(WREG2:WREG2+WREG1-1);
         LR    @07,WREG1                                           0211
         BCTR  @07,0                                               0211
         LA    @01,PACKAREA-1(WREG2)                               0211
         EX    @07,@SM01199                                        0211
*                    SYNERRLN = WREG1 + 4;      /* STORE LENGTH OF 0212
*                                                  INSERTION          *
         LA    @07,4                                               0212
         ALR   @07,WREG1                                           0212
         STH   @07,SYNERRLN                                        0212
*                    SYNERRM2 = 0;        /* END OF INSERTION CHAIN   *
         SLR   @07,@07                                             0213
         ST    @07,SYNERRM2                                        0213
*                    SYNERROF = 15;      /* SET OFFSET OF INSERTION   *
         MVC   SYNERROF(2),@CH00056                                0214
*                    LEV1INS = ADDR (SYNERMG1); /* POINT TO INSERTION *
         LA    LEV1INS,SYNERMG1                                    0215
*NOLEV2:             /* COMPLETE MESSAGE PARAMETERS                   *
*                    RELMSG1 = 1293;   /* LOAD MSG NUMBER             *
NOLEV2   MVC   RELMSG1(2),@CH01146                                 0216
*                    RELMSG2 = 1;      /* INDICATE REL LEVEL-2 NUM    *
         MVC   RELMSG2(2),@CH00051                                 0217
*                    LEV2INS = ADDR (M44IN1); /* POINT TO LEVEL-2  0218
*                                              INSERTION              *
         LA    LEV2INS,M44IN1                                      0218
*                    CALL MSGRTN;      /* PUT OUT MESSAGE             *
         BAL   @14,MSGRTN                                          0219
*                  END;                                            0220
*              DCBPT = CAPTCDCB;       /* POINT TO UTIL DCB           *
@RT00178 MVC   DCBPT(4),CAPTCDCB(COMMREG)                          0221
*              UTOPTCD = '20'X;        /* SET WRITE OP CODE           *
         MVI   UTOPTCD,X'20'                                       0222
*              UTRECPT = ADDR (OUTLINE); /* PTR TO OUTPUT RECORD      *
         LA    @07,OUTLINE                                         0223
         ST    @07,UTRECPT                                         0223
*              LBIT2 = '1'B;           /* SET END-OF-LIST INDICATOR   *
*                                                                  0224
         OI    LBIT2,B'10000000'                                   0224
*                                      /* REPLACE OR INSERT LINE      *
*              /* CALL IKJEBEUT TO WRITE RECORD INTO EDIT DATA SET    *
*              GEN;                                                0225
*                                                                  0225
         IKJEBESH (COMMREG),IKJEBEUT,PARAM=((COMMREG),UTLST),          X
               MF=(E,SRWORK)
*              /* CHECK RETURN CODES FROM IKJEBEUT                    *
*              IF RCODEREG = 0                                     0226
*                THEN;                 /* YES, RET CODE=0             *
         LTR   RCODEREG,RCODEREG                                   0226
         BZ    @RT00226                                            0226
*                ELSE                  /* NO, RET CODE NOT 0          *
*                  DO;                                             0228
*                    CODESAVE = RCODEREG; /* SAVE RETURN CODE         *
*                                                                  0229
         LR    CODESAVE,RCODEREG                                   0229
*                    /* CHECK FOR RETURN CODE OF 12                   *
*                    IF CODESAVE = 12                              0230
*                      THEN            /* YES, CODE WAS 12            *
         CH    CODESAVE,@CH00134                                   0230
         BNE   @RF00230                                            0230
*                        DO;                                       0231
*                          GOTO EXIT;  /* RETURN TO COMMAND MODE      *
         B     EXIT                                                0232
*                        END;                                      0233
