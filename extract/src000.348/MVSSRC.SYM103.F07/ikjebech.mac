@RF00210 TM    CANONUM(COMMREG),B'00000001'                        0212
         BNO   @RF00212                                            0212
*     DO;                           /* SEND DATA SET NOT NUMBERED    */
*       RELMSG1=1288;               /* MSG IKJ52502I                 */
         MVC   RELMSG1(2),@CH01335                                 0214
