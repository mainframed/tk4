*                                      PROMPTING REQUIRED            */
         TM    LNENDF(@12),B'10000000'                             0202
         BNZ   @RF00202                                            0202
         TM    LNCNTF(@12),B'10000000'                             0202
         BNZ   @RF00202                                            0202
         TM    STR1F(@12),B'10000000'                              0202
         BNZ   @RF00202                                            0202
         TM    STR2F(@12),B'10000000'                              0202
         BNZ   @RF00202                                            0202
*       DO;                         /* USER ENTERED ONLY ASTERISK    */
