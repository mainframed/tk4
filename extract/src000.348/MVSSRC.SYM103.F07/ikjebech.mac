@RF00201 L     @12,TABPTR1                                         0210
         TM    LNENDF(@12),B'10000000'                             0210
         BNZ   @RF00210                                            0210
         TM    STR1F(@12),B'10000000'                              0210
         BZ    @RT00210                                            0210
