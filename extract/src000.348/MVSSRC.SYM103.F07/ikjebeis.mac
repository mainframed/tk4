         TITLE 'IKJEBEIS - EDIT COMMAND, INSERT SUBCOMMAND             *
                        '
IKJEBEIS CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'IKJEBEIS  74.206'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @11,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@11                                         0001
         TITLE 'IKJEBEIS  -  EDIT COMMAND, INSERT SUBCOMMAND'
*           COMMREG = COMMREG;       /* GET ADDR OF IKJEBECA         */
*                                                                  0069
*           RESPECIFY (GPR13P,GPR15P) RSTD;                        0070
*           GPR15P = CANXTSVA;       /* SAVE AREA FOR THIS MODULE    */
         L     GPR15P,CANXTSVA(,COMMREG)                           0071
*           EVAL(GPR15P + 4) -> CASAVPTR = GPR13P;   /* SET THIS     */
         LA    @14,4                                               0072
         ALR   @14,GPR15P                                          0072
         ST    GPR13P,CASAVPTR(,@14)                               0072
*                                    /* SAVEAREA BKWD PTR            */
*           EVAL(GPR13P + 8) -> CASAVPTR = GPR15P;   /* SET CALLER   */
         LA    @14,8                                               0073
         ALR   @14,GPR13P                                          0073
         ST    GPR15P,CASAVPTR(,@14)                               0073
*                                    /* FWD PTR                      */
*           GPR13P = GPR15P;         /* NEW SAVEAREA ADDR            */
         LR    GPR13P,GPR15P                                       0074
*           CANXTSVA = GPR13P + 72;   /* UPDATE NEX-SAVE-AREA PTR    */
         LA    @14,72                                              0075
         ALR   @14,GPR13P                                          0075
         ST    @14,CANXTSVA(,COMMREG)                              0075
*           RESPECIFY (GPR13P,GPR15P) UNRSTD;                      0076
*                                                                  0076
*           DATAREG = ADDR(CASCWKA);   /* SET BASE REG FOR WORK AREA */
         LA    DATAREG,CASCWKA(,COMMREG)                           0077
*           GEN( USING @DATD,DATAREG);   /* SET ADDRESSABILITY TO    */
          USING @DATD,DATAREG
*                                    /* CASCWKA                      */
*        /* THESE REGS ARE RESTRICTED FOR THE DURATION OF INSERT      *
*        RESPECIFY(COMMREG, BASEREG, DATAREG) RSTD;                0079
*        CCODE = 0;                    /* INIT RETURN CODE            *
*                                                                  0080
         SLR   CCODE,CCODE                                         0080
*        /* CHECK FOR OPERANDS ON INSERT SUBCOMMAND                   *
*        IF CAOPERND = '0'B                                        0081
*          THEN                        /* NO, OPERAND NOT PRESENT, 0081
*                                         INPUT TO BE CALLED          *
         TM    CAOPERND(COMMREG),B'10000000'                       0081
         BNZ   @RF00081                                            0081
*        /* CAUSE INPUT PROCESSOR TO BE CALLED BY IKJEBEMA            *
*            DO;                                                   0082
*              CCODE = 4;              /* TELL IKJEBEMA TO GIVE CTL TO
*                                         IKJEBEIP (INPUT MODE        *
         LA    @12,4                                               0083
         LR    CCODE,@12                                           0083
*              CAIMINS = '1'B;         /* SET FLAG FOR INPUT          *
         OI    CAIMINS(COMMREG),B'01000000'                        0084
*              CASYNIS = '0'B;         /* TURN OFF INTERFACE BIT      *
         NI    CASYNIS(COMMREG),B'11101111'                        0085
*              RCODEREG = CCODE;       /* LOAD RETURN CODE            *
*                                                                  0086
         LR    RCODEREG,CCODE                                      0086
*               RESPECIFY (GPR12P,GPR13P) RSTD;                    0087
*               CANXTSVA = GPR13P;   /* UPDATE NEXT-SAVE-AREA PTR    */
         ST    GPR13P,CANXTSVA(,COMMREG)                           0088
*               GPR12P = GPR13P;     /* SAVE PRESENT SAVE-AREA ADDR  */
         LR    GPR12P,GPR13P                                       0089
*               GPR13P = (GPR13P + 4) -> CASAVPTR; /* GET BACKWARD   */
         LA    @08,4                                               0090
         ALR   @08,GPR13P                                          0090
         L     @08,CASAVPTR(,@08)                                  0090
         LR    GPR13P,@08                                          0090
*                                    /* SAVE-AREA PTR                */
*               RESPECIFY CASAVE BASED(GPR12P); /* CHANGE BASE FOR   */
*                                    /* SAVE-AREA                    */
*               CASAVE = CASAVE && CASAVE; /* ZERO OUT THIS          */
         XC    CASAVE(72,GPR12P),CASAVE(GPR12P)                    0092
*                                    /* SAVE-AREA                    */
*               RESPECIFY CASAVE BASED;                            0093
*               RETURN CODE(RCODEREG); /* RETURN WITH RETURN CODE    */
         L     @14,12(,@13)                                        0094
         LM    @00,@12,20(@13)                                     0094
         BR    @14                                                 0094
*               RESPECIFY (GPR12P,GPR13P) UNRSTD;                  0095
*                                     /*       RETURN                */
*            END;                                                  0096
*          ELSE;                       /* YES OPERANDS PRESENT        *
@RF00081 DS    0H                                                  0098
*        FLAGS = FLAGS && FLAGS;       /* CLEAR FLAG FIELD            *
*                                                                  0098
         XC    FLAGS(1),FLAGS                                      0098
*        /* RESET LENGTH IN PARSE BFR                                 *
*        CMDSCNLN = CAPTSCMD + CASCMDLN - CAPTIBFR - 4;            0099
         L     @12,CAPTIBFR(,COMMREG)                              0099
         LH    @05,CASCMDLN(,COMMREG)                              0099
         AL    @05,CAPTSCMD(,COMMREG)                              0099
         SLR   @05,@12                                             0099
         SH    @05,@CH00407                                        0099
         STH   @05,CMDSCNLN(,@12)                                  0099
*        PPLPTR = ADDR (CATMPLST);     /* SET PRM ADDR FOR PARSE      *
         LA    PPLPTR,CATMPLST(,COMMREG)                           0100
*        PPLPCL = ADDR (IKJEBIS1);     /* SET PCL ADDR FOR PARSE      *
         L     @05,@CV00365                                        0101
         ST    @05,PPLPCL(,PPLPTR)                                 0101
*        PPLANS = ADDR (PDLPTR);       /* INIT ANSWER PLACE PTR       *
         LA    @05,PDLPTR(,COMMREG)                                0102
         ST    @05,PPLANS(,PPLPTR)                                 0102
*        PPLCBUF = CAPTIBFR;           /* SET PTR TO CMD BUFFER       *
         ST    @12,PPLCBUF(,PPLPTR)                                0103
*        PPLUWA = 0;                   /* INDICATE NO WORK AREA       *
         SLR   @12,@12                                             0104
         ST    @12,PPLUWA(,PPLPTR)                                 0104
*         DO; /*CALLTSSR EP(IKJPARS)MF(E,PPL)*/                    0105
*           RESPECIFY GPR01P RSTD;                                 0106
*           GPR01P = ADDR(PPL);                                    0107
         LR    GPR01P,PPLPTR                                       0107
*           IF CVTPARS = 0                                         0108
*             THEN                                                 0108
         L     @15,CVTPTR                                          0108
         C     @12,CVTPARS(,@15)                                   0108
         BNE   @RF00108                                            0108
*               GEN(LINK EP=IKJPARS);                              0109
         LINK EP=IKJPARS
*             ELSE                                                 0110
*               CALL CVTPARS;                                      0110
         B     @RC00108                                            0110
@RF00108 L     @12,CVTPTR                                          0110
         L     @15,CVTPARS(,@12)                                   0110
         BALR  @14,@15                                             0110
*           RESPECIFY GPR01P UNRSTD;                               0111
@RC00108 DS    0H                                                  0112
*         END;  /* CALL IKJPARS TO SCAN                            0112
*                  THE OPERAND             */                      0112
*                                                                  0112
*        /* CHECK RETURN CODE FROM IKJPARS                            *
*        IF RCODEREG = 0                                           0113
*          THEN;                       /* RET CODE=0, CONTINUE        *
         LTR   RCODEREG,RCODEREG                                   0113
         BZ    @RT00113                                            0113
*          ELSE                        /* RET CODE NOT ZERO           *
*        /* IKJPARS WAS NOT SUCCESSFUL                                *
*            DO;                                                   0115
*              CODESAVE = RCODEREG;    /* SAVE RETURN CODE            *
*                                                                  0116
         LR    CODESAVE,RCODEREG                                   0116
*              /* CHECK FOR GETMAIN ERROR                             *
*              IF CODESAVE = 16                                    0117
*                THEN                  /* YES, GETMAIN ERROR          *
         CH    CODESAVE,@CH00223                                   0117
         BNE   @RF00117                                            0117
*        /* NOT ENOUGH MAIN STORAGE AVAILABLE - SEND MESSAGE          *
*                  DO;                                             0118
*NOCOREMG: ;                           /* SEND NO CORE AVAILABLE MSG  *
NOCOREMG DS    0H                                                  0120
*                    RELMSG1 = 270;    /* NOCORE MSG                  *
         MVC   RELMSG1(2),@CH01129                                 0120
*                    RELMSG2 = 0;      /* INDICATE SECOND-LEVEL       *
         SLR   @12,@12                                             0121
         STH   @12,RELMSG2                                         0121
*                    LEV1INS = ADDR (M25IN1); /* FIRST LINE INSERTION *
         LA    LEV1INS,M25IN1                                      0122
*                    LEV2INS= 0;        /* INDICATE NO LEVEL-2 INSERTS*
         LR    LEV2INS,@12                                         0123
*                    CALL MSGRTN;      /* PUT OUT NO CORE MSG         *
         BAL   @14,MSGRTN                                          0124
*FLUSH:              CCODE=8;          /* INDICATE FLUSH NEEDED       *
FLUSH    LA    CCODE,8                                             0125
*EXIT: ;                               /* EXIT CODE                   *
EXIT     DS    0H                                                  0127
*PRSEXIT:  ;                           /* EXIT CODE                   *
PRSEXIT  DS    0H                                                  0128
*                    PARMREG1 = ADDR(PDLPTR); /* POINT TO PARSE PDL
*                                                ANS PLACE            *
         LA    PARMREG1,PDLPTR(,COMMREG)                           0128
*                    GEN (IKJRLSA (1)); /* FREE PDL CORE              *
         IKJRLSA (1)
*                    CAFREEDL= '1'B;   /* INDICATE NO PLD TO FREE     *
         OI    CAFREEDL(COMMREG),B'10000000'                       0130
*                    CASYNIS= '0'B;    /* RESET INTERFACE BIT         *
         NI    CASYNIS(COMMREG),B'11101111'                        0131
*                    RCODEREG = CCODE; /* LOAD RETURN CODE            *
*                                                                  0132
*                                                                  0132
         LR    RCODEREG,CCODE                                      0132
*                     RESPECIFY (GPR12P,GPR13P) RSTD;              0133
*                     CANXTSVA = GPR13P; /* UPDATE NEXT-SAVE-AREA    */
         ST    GPR13P,CANXTSVA(,COMMREG)                           0134
*                                    /* PTR                          */
*                     GPR12P = GPR13P; /* SAVE PRESENT SAVE-AREA     */
         LR    GPR12P,GPR13P                                       0135
*                                    /* ADDR                         */
*                     GPR13P = (GPR13P + 4) -> CASAVPTR; /* GET      */
         L     @08,CASAVPTR+4(,GPR13P)                             0136
         LR    GPR13P,@08                                          0136
*                                    /* BACKWARD SAVE-AREA PTR       */
*                     RESPECIFY CASAVE BASED(GPR12P); /* CHANGE      */
*                                    /* BASE FOR SAVE-AREA           */
*                     CASAVE = CASAVE && CASAVE; /* ZERO OUT THIS    */
         XC    CASAVE(72,GPR12P),CASAVE(GPR12P)                    0138
*                                    /* SAVE-AREA                    */
*                     RESPECIFY CASAVE BASED;                      0139
*                     RETURN CODE(RCODEREG); /* RETURN WITH RETURN   */
         L     @14,12(,@13)                                        0140
         LM    @00,@12,20(@13)                                     0140
         BR    @14                                                 0140
*                                    /* CODE                         */
*                     RESPECIFY (GPR12P,GPR13P) UNRSTD;            0141
*                                          /* RETURN           */  0142
*                  END;                                            0142
*                ELSE                  /* NO, NOT GETMAIN ERROR       *
*                  DO;                                             0143
*                                                                  0143
@RF00117 DS    0H                                                  0144
*                    /* CHECK IF PROMPT REQUIRED BUT USER NOPROMPT    *
*                    IF CODESAVE=4                                 0144
*                      THEN            /* USER IS NOPROMPT, PROMPTING
*                                         SITUATION OCCURRED          *
         CH    CODESAVE,@CH00407                                   0144
         BE    @RT00144                                            0144
*                        GOTO FLUSH;   /* INDICATE FLUSH NEEDED       *
*                                                                  0145
*                    /* CHECK IF ATTENTION ISSUED                     *
*                    IF CODESAVE=8                                 0146
*                      THEN            /* ATTN ECB HAS BEEN POSTED    *
         CH    CODESAVE,@CH00048                                   0146
         BE    @RT00146                                            0146
*                        GOTO EXIT;    /* RETURN TO EDIT MODE         *
*                      ELSE;           /* CODE GREATER THEN 8         *
*                                                                  0148
*                    /* ISSUE 'COMMAND SYSTEM ERROR' MESSAGE          *
*                    CMDER1 = M313INS1;/* MOVE MODEL TO WORK AREA     *
         MVC   CMDER1(16),M313INS1                                 0149
*                    CMDER2 = M313INS2;/* MOVE MODEL TO WORK AREA     *
         MVC   CMDER2(16),M313INS2                                 0150
*SYSERR: ;                             /* BRANCH ADDRESS              *
SYSERR   DS    0H                                                  0152
*                    /* ENTER HERE FOR INSERTIONS OTHER THAN 'PARSE'  *
*                                                                  0152
*                    WREG1 = CODESAVE; /* GET LINE NUMBER             *
         ST    CODESAVE,WREG1                                      0152
*                    RFY CVDREG RSTD;                              0153
*                    CVDREG = WREG1;                               0154
*                                                                  0154
         L     @12,WREG1                                           0154
         LR    CVDREG,@12                                          0154
*                    /* CONVERT INSTRUCTIONS                          *
*                    GEN;                                          0155
PKLEN    EQU   8                       LENGTH OF PACK OR UNPACK
PKOFF    EQU   7                       OFFSET FOR MOVE
MLEN     EQU   1                       LENGTH OF MOVE
PKOFF2   EQU   6                       OFFSET FOR MOVE
ZONESIGN EQU   X'F0'                   EBCDIC NUMERIC ZONE SIGN
         CVD   CVDREG,PACKTMP          CONVERT TO DECIMAL
         UNPK  PACKAREA(PKLEN),PACKTMP(PKLEN)  UNPACK NUMBER
         MVZ   PACKAREA+PKOFF(MLEN),PACKAREA+PKOFF2 SET SIGN
*                    RFY CVDREG UNRSTD;                            0156
*                    CER2PTR = ADDR (CMDER2); /* PTR TO SECOND     0157
*                                                INSERTION            *
*                                                                  0157
         LA    @12,CMDER2                                          0157
         ST    @12,CER2PTR                                         0157
*                    /* MOVE COND CODE TO MODEL INSERTION             *
*                    CER2TXT (1:2) = PACKAREA (7:8);               0158
         MVC   CER2TXT(2),PACKAREA+6                               0158
*                    RELMSG1=271;      /* LOAD MESSAGE NUMBER         *
         MVC   RELMSG1(2),@CH01140                                 0159
*                    RELMSG2=1;        /* LOAD LEV-2 NUMBER           *
         MVC   RELMSG2(2),@CH00050                                 0160
*                    LEV1INS=0;        /* INDICATE NO LEV-1 INSERTION *
         SLR   LEV1INS,LEV1INS                                     0161
*                    LEV2INS=ADDR (CMDER1); /* POINT TO LEVEL-2 INSERT*
         LA    LEV2INS,CMDER1                                      0162
*                    CALL MSGRTN;      /* GO PUT OUT MESSAGE          *
         BAL   @14,MSGRTN                                          0163
*                    GOTO FLUSH;       /* INDICATE FLUSH NEEDED       *
         B     FLUSH                                               0164
*                  END;                                            0165
*            END;                                                  0166
*                                                                  0166
*        /* CHECK FOR STRING LENGTH GREATER THAN 255                  *
*        IF INLEN <= 255                                           0167
*          THEN;                       /* OK, LESS THAN 255           *
@RT00113 L     @12,PDLPTR(,COMMREG)                                0167
         LH    @12,INLEN(,@12)                                     0167
         CH    @12,@CH01141                                        0167
         BNH   @RT00167                                            0167
*          ELSE                        /* NOT OK, GREATER THAN 255    *
*            DO;                                                   0169
*              INLEN = 255;            /* RESET LENGTH TO MAX         *
         L     @12,PDLPTR(,COMMREG)                                0170
         MVC   INLEN(2,@12),@CH01141                               0170
*                MSGSW1  = '1'B;       /* RESET LINE OVERFLOW         *
         OI    MSGSW1,B'01000000'                                  0171
*            END;                                                  0172
*        OUTDATA(1:INLEN)=INSTRING(1:INLEN); /* MOVE STRING TO OUTPUT
*                                             REC                     *
@RT00167 L     @12,PDLPTR(,COMMREG)                                0173
         LH    @15,INLEN(,@12)                                     0173
         LR    @14,@15                                             0173
         BCTR  @14,0                                               0173
         L     @12,INPTR(,@12)                                     0173
         EX    @14,@SM01196                                        0173
*        OUTLEN = INLEN + 4;           /* SET LENGTH OF RECORD        *
         AH    @15,@CH00407                                        0174
         STH   @15,OUTLEN                                          0174
*        OUTXXX = 0;                   /* CLEAR UNUSED FIELD          *
         SLR   @12,@12                                             0175
         STH   @12,OUTXXX                                          0175
*        OUTKEY = CACURNUM + 1;        /* SET KEY OF NEW RECORD       *
         LA    @12,1                                               0176
         AL    @12,CACURNUM(,COMMREG)                              0176
         ST    @12,OUTKEY                                          0176
*        DCBPT = CAPTCDCB;             /* PUT DCB PTR IN UT PARM LST  *
         MVC   DCBPT(4),CAPTCDCB(COMMREG)                          0177
*        UTOPTCD = '00'X;              /* SET OP CODE FOR READ CURR   *
         MVI   UTOPTCD,X'00'                                       0178
*        UTRECPT = ADDR (OUTKEY);      /* POINT TO KEY OF CURRENT  0179
*                                         RECORD                      *
         LA    @12,OUTKEY                                          0179
         ST    @12,UTRECPT                                         0179
*        UTNEWREC = ADDR (CLNBFR (1)); /* POINT TO INPUT AREA         *
         LA    @12,CLNBFR(,COMMREG)                                0180
         ST    @12,UTNEWREC                                        0180
*        LBIT2 = '0'B;                 /* TURN OFF END-OF-PARM IND    *
         NI    LBIT2,B'01111111'                                   0181
*        LBIT3 = '1'B;                 /* TURN ON END-OF-PARM IND     *
         OI    LBIT3,B'10000000'                                   0182
*        CALL IORTN;                   /* READ CURRENT RECORD         *
*                                                                  0183
         BAL   @14,IORTN                                           0183
*        /* CHECK RETURN CODE FROM IKJEBEUT                           *
*        IF RCODEREG = 0                                           0184
*          THEN                        /* RECORD ALREADY EXISTS       *
         LTR   RCODEREG,RCODEREG                                   0184
         BNZ   @RF00184                                            0184
*        /* IF DATA SET IS 'NONUM' PUSH RECORD KEYS DOWN TO MAKE      *
*        /* ROOM FOR RECORD TO BE INSERTED.  IF DATA SET IS 'NUM'     *
*        /* NOTIFY USER THAT NO INSERTION IS POSSIBLE AND TERMINATE.  *
*            DO;                                                   0185
*                                                                  0185
*              /* CHECK FOR LINE NUMBERED DATA SET                    *
*              IF CANONUM = '0'B                                   0186
*                THEN                  /* DATA SET IS NUMBERED        *
         TM    CANONUM(COMMREG),B'00000001'                        0186
         BNZ   @RF00186                                            0186
*                /* SEND ERROR MESSAGE AND TERMINATE                  *
*                  DO;                                             0187
*                    RFY CVDREG RSTD;                              0188
*                    CVDREG = OUTKEY;  /* LOAD NUMBER TO CONVERT      *
         L     CVDREG,OUTKEY                                       0189
*                    CALL CVDRTN;      /* CONVERT NUMBER TO DECIMAL   *
         BAL   @14,CVDRTN                                          0190
*                    RFY CVDREG UNRSTD;                            0191
*                    INSREC = INSM561; /* MOVE MODEL TO WORKAREA      *
*                                                                  0192
         MVC   INSREC(16),INSM561                                  0192
*                    /* THIS DO LOOP LOCATES THE FIRST NON-ZERO       *
*                    /* BYTE OF THE NUMBER TO BE SENT TO THE          *
*                    /* TERMINAL. IT SCANS FROM LEFT TO RIGHT         *
*                    DO WREG2 = 1 TO LENGTH (NUMTEMP) BY 1;        0193
*                                                                  0193
         LA    @12,1                                               0193
         ST    @12,WREG2                                           0193
@DL00193 DS    0H                                                  0194
*                      /* CHECK CURRENT CHARACTER FOR DECIMAL ZERO    *
*                      IF NUMTEMP (WREG2) = '0'                    0194
*                        THEN;         /* ZERO - CONTINUE LOOP        *
         LA    @08,NUMTEMP-1(@12)                                  0194
         CLI   0(@08),C'0'                                         0194
         BNE   @RF00194                                            0194
*                        ELSE          /* NOT ZERO - EXIT             *
*                          GOTO LPOUT1;/* EXIT FROM LOOP              *
*                    END;              /* WREG2 CONTAINS THE OFFSET   *
         AH    @12,@CH00050                                        0197
         ST    @12,WREG2                                           0197
         CH    @12,@CH00048                                        0197
         BNH   @DL00193                                            0197
*                    WREG2 = LENGTH (NUMTEMP); /* RESET TO MAX LENGTH *
         MVC   WREG2(4),@CF00048                                   0198
*LPOUT1: ;                             /* CONTINUE                    *
*                                                                  0199
LPOUT1   DS    0H                                                  0200
*                    /* FIND LENGTH OF NUMBER                         *
*                    WREG1 = LENGTH (INSNUM) + 1 - WREG2;          0200
*                                                                  0200
         L     @12,WREG2                                           0200
         LA    @08,9                                               0200
         SLR   @08,@12                                             0200
         ST    @08,WREG1                                           0200
*                    /* MOVE NON-ZERO PORTION OF NUM TO INSERT        *
*                    /* RECORD                                        *
*                    INSNUM (1:WREG1) =                            0201
*                        NUMTEMP (WREG2:LENGTH (NUMTEMP));         0201
         LR    @15,@08                                             0201
         BCTR  @15,0                                               0201
         LA    @01,NUMTEMP-1(@12)                                  0201
         EX    @15,@SM01199                                        0201
*                    INSLEN = WREG1 + 4;      /* FIND LENGTH OF    0202
*                                                INSERTION            *
         AH    @08,@CH00407                                        0202
         STH   @08,INSLEN                                          0202
*                    RELMSG1 = 1035;   /* LOAD MESSAGE NUMBER         *
         MVC   RELMSG1(2),@CH01146                                 0203
*                    RELMSG2 = 0;      /* INDICATE NO LEV-2 MSG       *
         SLR   @12,@12                                             0204
         STH   @12,RELMSG2                                         0204
*                    LEV1INS = ADDR (INSREC); /* POINT TO INSERTION   *
         LA    LEV1INS,INSREC                                      0205
*                    LEV2INS = 0;        /* SPECIFY NO LEVEL-2 INSERTS*
         LR    LEV2INS,@12                                         0206
*                    CALL MSGRTN;      /* PUT OUT MESSAGE             *
         BAL   @14,MSGRTN                                          0207
*                    GOTO FLUSH;       /* INDICATE FLUSH NEEDED       *
         B     FLUSH                                               0208
*                  END;                                            0209
*                ELSE                  /* DATA SET IS NOT NUMBERED    *
*                  DO;                                             0210
@RF00186 DS    0H                                                  0211
*                    CALL CLNSAVE;     /* GO MAKE ROOM IN DATA SET    *
         BAL   @14,CLNSAVE                                         0211
*LABL1:            END;                                            0212
LABL1    DS    0H                                                  0213
*            END;                                                  0213
*        /* ********************************************************* *
*        /* THE RECORD CAN NOW BE FORMATTED AND INSERTED              *
*        /* ********************************************************* *
*        LETXTPTR = ADDR (OUTLEN);     /* POINT TO STRING FOR LE      *
@RF00184 LA    @12,OUTLEN                                          0214
         ST    @12,LETXTPTR                                        0214
*        LEFLAG = '00'X;               /* SET FLAGS FOR IKJEBELE      *
         MVI   LEFLAG,X'00'                                        0215
*        LECAREA = COMMREG;            /* STORE COMM AREA PTR FOR LE  *
*                                                                  0216
         ST    COMMREG,LECAREA                                     0216
*        /* CALL IKJEBELE TO EDIT THE INPUT LINE FOR TABS AND UPPER   *
*        /* CASE                                                      *
*        GEN;                                                      0217
*                                                                  0217
         IKJEBESH (COMMREG),IKJEBELE,PARAM=((COMMREG),LEPARM),         X
               MF=(E,SRWORK)
*        /* CHECK FOR LINE OVERFLOW INDICATION                        *
*        IF RCODEREG = 0 & MSGSW1 = '0'B                           0218
*          THEN;                       /* NO ERRORS                   *
         LTR   RCODEREG,RCODEREG                                   0218
         BNZ   @RF00218                                            0218
         TM    MSGSW1,B'01000000'                                  0218
         BZ    @RT00218                                            0218
*          ELSE                        /* ERROR LINE TOO LONG         *
*          /* PREPARE TO NOTIFY USER THAT LINE HAS BEEN TRUNCATED     *
*            DO;                                                   0220
@RF00218 DS    0H                                                  0221
*              INSREC1 = MSGNUM2;      /* MOVE MODEL TO WORKAREA      *
*                                                                  0221
         MVC   INSREC1(11),MSGNUM2                                 0221
*              /* CHECK IF DATA SET IS LINE NUMBERED                  *
*              IF CANONUM = '0'B                                   0222
*                THEN                  /* DATA SET IS LINE NUMBERED   *
         TM    CANONUM(COMMREG),B'00000001'                        0222
         BNZ   @RF00222                                            0222
*                /* LINE NUMBER INSERTION IS REQUIRED IN MESSAGE      *
*                  DO;                                             0223
*                    RFY CVDREG RSTD;                              0224
*                    CVDREG = OUTKEY;  /* LOAD NUMBER TO BE CONVERT   *
         L     CVDREG,OUTKEY                                       0225
