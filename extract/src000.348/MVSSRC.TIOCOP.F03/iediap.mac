@EP00379 DS    0H                                                  0379
@EC00379 DS    0H                                                  0380
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE AVT/SAVT ADDRESSABILITY                            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0380
*   RFY                                                            0380
*    (RSIB,                                                        0380
*     INTTCIN) RSTD;                /* RESTRICT REGS         @YM06106*/
*   RAVT=NAVT;                      /* PTR TO TCAM AVT ADDR          */
         LA    @15,240                                             0381
         AL    @15,NCVTADR                                         0381
         L     RAVT,NAVT(,@15)                                     0381
*   RAVT=TCXAVT;                    /* ESTABLISH TCAM AVT ADDRESS    */
         L     RAVT,TCXAVT(,RAVT)                                  0382
*   RSAVT=AVTSAVTP;                 /* SAVT ADDRESS                  */
         L     RSAVT,AVTSAVTP(,RAVT)                               0383
*   RTRM=RPARM-LENGTH(IEDNTRM);     /* BACK TO NEG PREFIX    @YM06106*/
*                                                                  0384
         LR    RTRM,RPARM                                          0384
         SL    RTRM,@CF01763                                       0384
