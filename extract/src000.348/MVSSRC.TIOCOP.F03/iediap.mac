         B     @PB00002                                            0319
IANSI    DS    0H                                                  0320
*   RFY                                                            0320
*    (REG6,                                                        0320
*     RQCB,                                                        0320
*     RLCB,                                                        0320
*     RDISP) RSTD;                  /* RESTRICT REGS         @YM06106*/
*                                                                  0320
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE                                                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0321
*   RAVT=NAVT;                      /* PTR TO TCAM AVT ADDRESS       */
         LA    @15,240                                             0321
         AL    @15,NCVTADR                                         0321
         L     RAVT,NAVT(,@15)                                     0321
*   RAVT=TCXAVT;                    /* ESTABLISH TCAM AVT ADDRESS    */
         L     RAVT,TCXAVT(,RAVT)                                  0322
*   RSAVT=AVTSAVTP;                 /* SAVT ADDRESS                  */
*                                                                  0323
         L     RSAVT,AVTSAVTP(,RAVT)                               0323
*   /*****************************************************************/
*   /*                                                               */
*   /* DEALLOCATE THE PLCB SUBROUTINE                                */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0324
*   REG6=RRETURN;                   /* PRESERVE RETURN POINT         */
         LR    REG6,RRETURN                                        0324
*   RLCB=RPARM;                     /* COPY PLCB ADDRESS     @YM04668*/
         LR    RLCB,RPARM                                          0325
*   RPARM=LCBTTCIN;                 /* GET TTE INDEX                 */
         LH    RPARM,LCBTTCIN(,RLCB)                               0326
         N     RPARM,@CF02050                                      0326
*   CALL IEDQTNT;                   /* CONVERT IT TO TTE ADDRESS     */
         L     @15,AVTRNMPT(,RAVT)                                 0327
         BALR  @14,@15                                             0327
*   RTRM=RPARM-LENGTH(IEDNTRM);     /* BACK UP TO NEG PREFIX @YM06106*/
         LR    RTRM,RPARM                                          0328
         SL    RTRM,@CF01763                                       0328
*   RQCB=TRMDESTQ;                  /* GET QCB ADDRESS               */
         SLR   RQCB,RQCB                                           0329
         ICM   RQCB,7,TRMDESTQ(RTRM)                               0329
*   QCBPLCBA=ZERO;                  /* CLEAR PLCB ADDR IN QCB        */
         LR    @15,RQCB                                            0330
         SL    @15,@CF00046                                        0330
         SLR   @05,@05                                             0330
         STCM  @05,7,QCBPLCBA(@15)                                 0330
*   QCBTTCIN=LCBTTCIN;              /* PUT TTCIN BACK INTO THE QCB   */
         LH    @05,LCBTTCIN(,RLCB)                                 0331
         N     @05,@CF02050                                        0331
         STH   @05,QCBTTCIN(,@15)                                  0331
*   QCBPLCBN=OFF;                   /* RESET PLCB ASSIGNED IN QCB    */
         NI    QCBPLCBN(@15),B'01111111'                           0332
*   IF LCBSTCBA->STCBVTO=DSPSEND THEN                              0333
         L     @05,LCBSTCBA-1(,RLCB)                               0333
         CLI   STCBVTO(@05),X'22'                                  0333
         BNE   @RF00333                                            0333
*     DO;                           /* IF QCB STILL IN LCB   @YM09090*/
*       QCBSNDRQ=ON;                /* REINSERT SEND SCHEDULER NEXT
*                                      TIME                  @YM09090*/
         OI    QCBSNDRQ(@15),B'00100000'                           0335
*       REG3=RQCB;                  /* SET WHERE TO INSERT SCHEDULER
*                                                            @YM09090*/
         LR    REG3,RQCB                                           0336
*       RQCB=RLCB;                  /* SET WHERE TO REMOVE SCHEDULER
*                                                            @YM09090*/
         LR    RQCB,RLCB                                           0337
*       RDISP=AVTEA;                /* GET DISPATCHER BASE   @YM09090*/
         L     RDISP,AVTEA(,RAVT)                                  0338
*       CALL DSPUNAVR;              /* REMOVE SEND SCHEDULER @YM09090*/
         LA    @15,DSPUNAVR(,RDISP)                                0339
         BALR  @14,@15                                             0339
*     END;                          /*                       @YM09090*/
*   SAVTPUSE=SAVTPUSE-1;            /* DECREMENT USE COUNT           */
*                                                                  0341
@RF00333 LH    @15,SAVTPUSE(,RSAVT)                                0341
         BCTR  @15,0                                               0341
         STH   @15,SAVTPUSE(,RSAVT)                                0341
*   /*****************************************************************/
*   /*                                                               */
*   /* PUT PLCB BACK ON AVAILABLE QUE                                */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0342
*   IF SAVTPLCB=AVTFZERO THEN                                      0342
         L     @15,AVTFZERO(,RAVT)                                 0342
         C     @15,SAVTPLCB(,RSAVT)                                0342
         BNE   @RF00342                                            0342
*     DO;                           /* NO PLCB'S             @OY19488*/
*       SAVTPLCB=RLCB;              /* THEN RETURN THIS      @OY19488*/
         ST    RLCB,SAVTPLCB(,RSAVT)                               0344
*       SAVTLLCB=RLCB;              /* PLCB                  @OY19488*/
         ST    RLCB,SAVTLLCB(,RSAVT)                               0345
*       LCBLINK=AVTFZERO;           /* CLEAR LINK FIELD      @OY19488*/
         STCM  @15,7,LCBLINK(RLCB)                                 0346
*     END;                          /*                       @OY19488*/
*   ELSE                                                           0348
*     DO;                           /*                       @OY19488*/
         B     @RC00342                                            0348
@RF00342 DS    0H                                                  0349
*       SAVTLLCB->LCBLINK=RLCB;     /* SET NEW               @OY19488*/
         L     @15,SAVTLLCB(,RSAVT)                                0349
         STCM  RLCB,7,LCBLINK(@15)                                 0349
*       SAVTLLCB=RLCB;              /* LAST PLCB             @OY19488*/
         ST    RLCB,SAVTLLCB(,RSAVT)                               0350
*       LCBLINK=AVTFZERO;           /* CLEAR LINK FIELD      @OY19488*/
         MVC   LCBLINK(3,RLCB),AVTFZERO+1(RAVT)                    0351
*       RLCB=SAVTPLCB;              /* SET PLCB ADDRESS      @OY19488*/
         L     RLCB,SAVTPLCB(,RSAVT)                               0352
*     END;                          /*                       @OY19488*/
*                                                                  0353
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK FOR WAITING REQUESTS AND SATISFY THEM IF POSSIBLE       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0354
*   RFY                                                            0354
*    (RSCB,                                                        0354
*     REG2) RSTD;                                                  0354
@RC00342 DS    0H                                                  0355
*   IF SAVTPRTQ^=ZERO THEN                                         0355
         L     @15,SAVTPRTQ(,RSAVT)                                0355
         LTR   @15,@15                                             0355
         BZ    @RF00355                                            0355
*     DO;                           /*                       @YM02937*/
*       RSCB=LCBSCBA;               /* ESTABLISH SCB BASE            */
         L     RSCB,LCBSCBA-1(,RLCB)                               0357
         LA    RSCB,0(,RSCB)                                       0357
*       SRSAVE=RSAVE;               /* PRESERVE CALLERS SAVE AREA    */
         ST    RSAVE,SRSAVE(,RSCB)                                 0358
*       SREG10=REG6;                /* PRESERVE MY RETURN POINT      */
         ST    REG6,SREG10(,RSCB)                                  0359
*       SAVTPWT=SAVTPWT-1;          /* REDUCE WAIT COUNT             */
         LH    @03,SAVTPWT(,RSAVT)                                 0360
         BCTR  @03,0                                               0360
         STH   @03,SAVTPWT(,RSAVT)                                 0360
*       RQCB=SAVTPRTQ;              /* REMOVE REQUEST ELEMENT FROM 0361
*                                      REQUEST QCB                   */
         LR    RQCB,@15                                            0361
*       SCINDEX=CINDEX;             /* PRESERVE QCB INDEX            */
         IC    @15,CINDEX(,RSAVT)                                  0362
         STC   @15,SCINDEX(,RSCB)                                  0362
*       SAVTPRTQ=QCBCHAIN;          /* PUT NEXT REQST ELEMENT AT TOP
*                                      OF Q                          */
         LR    @15,RQCB                                            0363
         AL    @15,@CF02053                                        0363
         L     @15,QCBCHAIN(,@15)                                  0363
         ST    @15,SAVTPRTQ(,RSAVT)                                0363
*       CALL ALLOCATE;              /* GO ALLOCATE THE PLCB          */
*                                                                  0364
         BAL   @14,ALLOCATE                                        0364
*       /*************************************************************/
*       /*                                                           */
*       /* PREPARE FOR DSPPOSTR TO POST PLCB TO ITSELF               */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0365
*       RDISP=AVTEA;                /* SET DISPATCHER BASE           */
         L     RDISP,AVTEA(,RAVT)                                  0365
*       REG2=RSAVE;                 /* PRESERVE SAVE AREA            */
         LR    REG2,RSAVE                                          0366
*       RSAVE=ADDR(AVTSAVE2);       /* SET DISPATCHER SAVE AREA      */
         LA    RSAVE,AVTSAVE2(,RAVT)                               0367
*       IF QCBNDXRQ=ON THEN                                        0368
         LR    @15,RQCB                                            0368
         AL    @15,@CF02054                                        0368
         TM    QCBNDXRQ-4(@15),B'00000100'                         0368
         BNO   @RF00368                                            0368
*         LCBQCBA=TOQCB(RINDEX);    /* INDEX QCB                     */
         LR    @15,RINDEX                                          0369
         SLA   @15,2                                               0369
         L     @15,TOQCB-4(@15)                                    0369
         STCM  @15,7,LCBQCBA(RLCB)                                 0369
*       ELSE                                                       0370
*         ;                         /* NORMAL REQUEST                */
@RF00368 DS    0H                                                  0371
*       RPARM=RLCB;                 /* SET ELEMENT TO BE POSTED      */
         LR    RPARM,RLCB                                          0371
*       CALL DSPPOSTR;              /* POST PLCB TO ITSELF           */
         LA    @15,DSPPOSTR(,RDISP)                                0372
         BALR  @14,@15                                             0372
*       RSAVE=REG2;                 /* RESTORE CALLERS SAVE AREA     */
         LR    RSAVE,REG2                                          0373
*     END;                          /* OF ALLOCATE WAITING QCB       */
*   ELSE                                                           0375
*     ;                             /* NO QCBS WAITING FOR THIS PLCB */
@RF00355 DS    0H                                                  0376
*   RRETURN=REG6;                   /* RESTORE MY RETURN POINT       */
         LR    RRETURN,REG6                                        0376
*   END;                            /* END OF IANSI                  */
*                                                                  0377
@EL00002 DS    0H                                                  0377
@EF00002 DS    0H                                                  0377
@ER00002 BR    @14                                                 0377
