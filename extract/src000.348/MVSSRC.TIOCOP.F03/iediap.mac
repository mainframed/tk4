*   /*                                                               */
*   /* IF SIB AVAILABLE ALLOCATE IT                                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0385
*   RSIB=SAVTSIB;                   /* GET SIB AT TOP OF AVAILABLE 0385
*                                      QUE                           */
         L     RSIB,SAVTSIB(,RSAVT)                                0385
*   IF RSIB^=ZERO THEN                                             0386
         LTR   RSIB,RSIB                                           0386
         BZ    @RF00386                                            0386
*     DO;                           /* IF ONE AVAILABLE EXECUTE DO 0387
*                                      GROUP                         */
*       SAVTSIB=SIBCHAIN;           /* NEXT SIB TO TOP OF AVAILABLE Q*/
         L     @15,SIBCHAIN(,RSIB)                                 0388
         ST    @15,SAVTSIB(,RSAVT)                                 0388
*       SAVTSUSE=SAVTSUSE+1;        /* INCREMENT USE COUNT           */
         LA    @15,1                                               0389
         AH    @15,SAVTSUSE(,RSAVT)                                0389
         STH   @15,SAVTSUSE(,RSAVT)                                0389
*       IF SAVTSUSE>SAVTSH20 THEN   /* INCR HI WATER IF NEEDED       */
         CH    @15,SAVTSH20(,RSAVT)                                0390
         BNH   @RF00390                                            0390
*         SAVTSH20=SAVTSUSE;                                       0391
         STH   @15,SAVTSH20(,RSAVT)                                0391
*       ELSE                                                       0392
*         ;                         /* DONT INCREMENT                */
*                                                                  0392
@RF00390 DS    0H                                                  0393
*       /*************************************************************/
*       /*                                                           */
*       /* CLEAR THE SIB                                             */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0393
*       IEDSIBD(1:LENGTH(IEDSIBD))=IEDSIBD(1:LENGTH(IEDSIBD))&&IEDSIBD(
*           1:LENGTH(IEDSIBD));                                    0393
*                                                                  0393
         XC    IEDSIBD(24,RSIB),IEDSIBD(RSIB)                      0393
*       /*************************************************************/
*       /*                                                           */
*       /* INITIALIZE THE SIB                                        */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0394
*       SIBINDEX=INTTCIN;           /* INITIALIZE SIB INDEX          */
         STH   INTTCIN,SIBINDEX(,RSIB)                             0394
*       IF INTTCIN=SAVTSCPT         /* IF SIB FOR AN SSCP SESSION    */
*         THEN                                                     0395
         LH    @15,SAVTSCPT(,RSAVT)                                0395
         N     @15,@CF02050                                        0395
         CLR   INTTCIN,@15                                         0395
         BNE   @RF00395                                            0395
*         DO;                                                      0396
*           RFY                                                    0397
*             DCTPTR RSTD;                                         0397
*           DCTPTR=AVTCSTCS+(AVTDCTLN*(TRMCHCIN-1));/* GET DCT PTR 0398
*                                                            @YM03683*/
         SLR   DCTPTR,DCTPTR                                       0398
         IC    DCTPTR,TRMCHCIN(,RTRM)                              0398
         BCTR  DCTPTR,0                                            0398
         MH    DCTPTR,AVTDCTLN(,RAVT)                              0398
         AL    DCTPTR,AVTCSTCS(,RAVT)                              0398
*           SIBTSPRO=DCTBYTE4&TSAND;/* SET TS PROFILE FROM DCT       */
         SLR   @15,@15                                             0399
