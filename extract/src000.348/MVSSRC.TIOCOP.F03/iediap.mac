         L     RSAVT,AVTSAVTP(,RAVT)                               0287
*   /*****************************************************************/
*   /*                                                               */
*   /* IF PLCB AVAILABLE GO ALLOCATE IT                              */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0288
*   RFY                                                            0288
*    (RLCB,                                                        0288
*     RSCB,                                                        0288
*     RQCB) RSTD;                                                  0288
*   RQCB=RPARM;                     /* COPY QCB ADDRESS              */
         LR    RQCB,RPARM                                          0289
*   RLCB=SAVTPLCB;                  /* TOP PLCB ON AVAILABLE QUE     */
         L     RLCB,SAVTPLCB(,RSAVT)                               0290
*   IF RLCB^=ZERO THEN                                             0291
         LTR   RLCB,RLCB                                           0291
         BZ    @RF00291                                            0291
*     DO;                           /* SEE IF PLCB EXISTS            */
*       RSCB=LCBSCBA;               /* SCB ADDRESSABILITY            */
         L     RSCB,LCBSCBA-1(,RLCB)                               0293
         LA    RSCB,0(,RSCB)                                       0293
*       SRSAVE=RSAVE;               /* PRESERVE SAVE AREA            */
         ST    RSAVE,SRSAVE(,RSCB)                                 0294
*       CALL ALLOCATE;              /* GO ALLOCATE THE PLCB          */
         BAL   @14,ALLOCATE                                        0295
*     END;                          /* END OF AVAILABLE PLCB DO GROUP*/
*                                                                  0296
*   /*****************************************************************/
*   /*                                                               */
*   /* IF NOT AVAILABLE THEN ENQUE THE REQUEST                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0297
*   ELSE                                                           0297
*     DO;                                                          0297
         B     @RC00291                                            0297
@RF00291 DS    0H                                                  0298
*       SAVTPWT=SAVTPWT+1;          /* INCREMENT WAIT COUNT          */
         LA    @15,1                                               0298
         AH    @15,SAVTPWT(,RSAVT)                                 0298
         STH   @15,SAVTPWT(,RSAVT)                                 0298
*       IF SAVTPWT>SAVTWH20 THEN    /* IS UPDTE OF HI WATER CNT    0299
*                                      NEEDED                        */
         CH    @15,SAVTWH20(,RSAVT)                                0299
         BNH   @RF00299                                            0299
*         SAVTWH20=SAVTPWT;         /* YES DO IT                     */
         STH   @15,SAVTWH20(,RSAVT)                                0300
*       ELSE                                                       0301
*         ;                         /* UPDATE NOT NEEDED             */
*                                                                  0301
@RF00299 DS    0H                                                  0302
*       /*************************************************************/
*       /*                                                           */
*       /* ENQUE THE REQUEST ON SAVTPRTQ                             */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0302
*       QCBCHAIN=SAVTPRTQ;          /* PUT EXISTING CHAIN INTO QCB   */
         LR    @15,RQCB                                            0302
         SL    @15,@CF00046                                        0302
         L     @06,SAVTPRTQ(,RSAVT)                                0302
         ST    @06,QCBCHAIN(,@15)                                  0302
*       SAVTPRTQ=RQCB;              /* PUT QCB AT TOP OF CHAIN       */
         ST    RQCB,SAVTPRTQ(,RSAVT)                               0303
*       IF QCBNDXRQ=ON THEN                                        0304
         TM    QCBNDXRQ(@15),B'00000100'                           0304
         BNO   @RF00304                                            0304
*         CINDEX=RINDEX;            /* INDEXED REQUEST               */
         STC   RINDEX,CINDEX(,RSAVT)                               0305
*       ELSE                                                       0306
*         CINDEX=ZERO;              /* NORMAL REQUEST                */
         B     @RC00304                                            0306
@RF00304 MVI   CINDEX(RSAVT),X'00'                                 0306
*       RFY                                                        0307
*         REG15 RSTD;                                              0307
@RC00304 DS    0H                                                  0308
*       REG15=REG15&&REG15;         /* SET RETURN CODE               */
         XR    REG15,REG15                                         0308
*     END;                                                         0309
*   RETURN CODE(REG15);             /* RETURN TO CALLER              */
@RC00291 L     @14,12(,@13)                                        0310
         LM    @00,@12,20(@13)                                     0310
         BR    @14                                                 0310
*   RFY                                                            0311
*    (RQCB,                                                        0311
