         TITLE 'IEDIAB - PLCB,SIB,SAT,RVT,NAT INITIATION ROUTINE       *
                        '
IEDIAB   CSECT ,                                                   0001
@PROLOG  DS    0H                                                  0002
*                                   /*
         USING IEDIAB,RBASE
         STM   14,12,AVTSAVE3+12(13)   SAVE REGS
         LR    RBASE,R15               SET BASE
IEDIAB   IEDHJN AFTERHJN,HJN
*                                                                  0160
*   /*****************************************************************/
*   /*                                                               */
*   /* LEFT ADJUST HOST SUBAREA, DEVELOP BIT MASK TO AND OFF ELEMENT */
*   /* PORTION OF NETWORK ADDRESS, DEVELOP SHIFT COUNT TO SHIFT OFF  */
*   /* SUBAREA PORTION NETWORK ADDRESS.                              */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0160
*   R2TO8TH='80'X                   /* COMPARAND TO DETERMINE      0160
*                                      HI-ORDER BIT OF MAX SUBAREA.  */
*       ;                                                          0160
         LA    R2TO8TH,128                                         0160
*   RNUMBITS=8;                     /* # BITS REQUIRED FOR MAX     0161
*                                      SUBAREA. START AT 8 AND WORK
*                                      DOWN.                         */
         LA    RNUMBITS,8                                          0161
*   RMASK='FF'X;                    /* MASK TO AND OFF ELEMENT. START
*                                      AT X'FF00'                    */
         LA    RMASK,255                                           0162
*   RHOST=SAVTSUBV;                 /* WILL CONTAIN HOST SUBAREA LEFT
*                                      ADJUSTED.                     */
         L     @09,AVTSAVTP(,IEDQAPTR)                             0163
         SLR   @15,@15                                             0163
         IC    @15,SAVTSUBV(,@09)                                  0163
         LR    RHOST,@15                                           0163
*   SUBVSAVE=SAVTSUBV;              /* SAVE RIGHT ADJUSTED HOST    0164
*                                      SUBAREA FOR LATER             */
         STC   @15,SUBVSAVE                                        0164
*   DO WHILE(RNUMBITS>=2)           /* DO WHILE # BITS >= 2(MINIMUM
*                                      ALLOWED)                      */
*         ;                                                        0165
         B     @DE00165                                            0165
@DL00165 DS    0H                                                  0166
*     IF SAVTSUBA<R2TO8TH           /* IF MAX SUBAREA ^ >= CURRENT 0166
*                                      HI-BIT                        */
*       THEN                                                       0166
         L     @09,AVTSAVTP(,IEDQAPTR)                             0166
         SLR   @15,@15                                             0166
         IC    @15,SAVTSUBA(,@09)                                  0166
         CLR   R2TO8TH,@15                                         0166
         BNH   @RF00166                                            0166
*       DO;                         /* THEN                          */
*         R2TO8TH=R2TO8TH/2;        /* SHIFT HI-BIT RIGHT ONE PLACE. */
         SRL   R2TO8TH,1                                           0168
*         RNUMBITS=RNUMBITS-1;      /* # SUBAREA BITS - 1.           */
         BCTR  RNUMBITS,0                                          0169
*         RMASK=RMASK*2;            /* SHIFT MASK TO LEFT 1.         */
         ALR   RMASK,RMASK                                         0170
*         RHOST=RHOST*2;            /* SHIFT HOST SUBAREA LEFT ONE 0171
*                                      BIT                           */
         ALR   RHOST,RHOST                                         0171
*       END;                                                       0172
*     ELSE                                                         0173
*       DO;                         /* ELSE                          */
         B     @RC00166                                            0173
@RF00166 DS    0H                                                  0174
*         SAVTSUBL=RNUMBITS;        /* SAVE SUBAREA SHIFT COUNT      */
         L     @09,AVTSAVTP(,IEDQAPTR)                             0174
         STC   RNUMBITS,SAVTSUBL(,@09)                             0174
*         SAVTSUBM=RMASK;           /* SAVE ELEMENT AND MASK.        */
         STC   RMASK,SAVTSUBM(,@09)                                0175
*         SAVTSUBV=RHOST;           /* HOST SUBAREA LEFT ADJUSTED    */
         STC   RHOST,SAVTSUBV(,@09)                                0176
*         RNUMBITS=1;               /* BREAK DO LOOP                 */
         LA    RNUMBITS,1                                          0177
*       END;                                                       0178
*   END;                                                           0179
*                                                                  0179
@RC00166 DS    0H                                                  0179
@DE00165 CL    RNUMBITS,@CF00067                                   0179
         BNL   @DL00165                                            0179
*   /*****************************************************************/
*   /*                                                               */
*   /* SET INDEX OF FIRST MH TABLE ENTRY INTO SAVT                   */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0180
*   RTNT=AVTRNMPT;                  /* TNT BASE                      */
         L     RTNT,AVTRNMPT(,IEDQAPTR)                            0180
*   RFY                                                            0181
*     TCXPTR RSTD;                                                 0181
*   RFY                                                            0182
*     IEDQTCXD BASED(TCXPTR);                                      0182
*   TCXPTR=CVTAQAVT;                /* TCX BASE                      */
         L     @09,CVTPTR                                          0183
         L     TCXPTR,CVTAQAVT(,@09)                               0183
*   SAVTMHDX=TNTLEN+TCXTOTE+1;      /* TOTAL # TNT + # TOTE + 1      */
*                                                                  0184
         L     @11,AVTSAVTP(,IEDQAPTR)                             0184
         L     @09,IEDQNPTR(,IEDQAPTR)                             0184
         LH    @09,TNTLEN(,@09)                                    0184
         N     @09,@CF02253                                        0184
         SLR   @15,@15                                             0184
         IC    @15,TCXTOTE(,TCXPTR)                                0184
         ALR   @09,@15                                             0184
         AL    @09,@CF00063                                        0184
         STH   @09,SAVTMHDX(,@11)                                  0184
*   /*****************************************************************/
*   /*                                                               */
*   /* SCAN TNT TO OBTAIN: 1ST NAT ENTRY INDEX(1ST TNT SNA/3705      */
*   /* DIRECT ENTRY), SSCP TNT INDEX, LAST SNA/3705 DIRECT TNT INDEX,*/
*   /* # INBOARD RVT ENTRIES FROM TNT.                               */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0185
*   CALL SCANTNT;                                                  0185
         BAL   @14,SCANTNT                                         0185
*   TCXPTR=CVTAQAVT;                /* RELOAD THE TCX BASE   @OY20269*/
*                                                                  0186
         L     @09,CVTPTR                                          0186
         L     TCXPTR,CVTAQAVT(,@09)                               0186
*   /*****************************************************************/
*   /*                                                               */
*   /* # TNT ENTRIES IN NAT = # TNT ENTRIES + # TOTE TNT ENTRIES -   */
*   /* TNT INDEX 1ST NAT ENTRY + 1.                                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0187
*   NNAT=TNTLEN+TCXTOTE-SAVTNAT1+1  /* # TNT NAT ENTRIES.            */
*       ;                                                          0187
*                                                                  0187
         L     @11,IEDQNPTR(,IEDQAPTR)                             0187
         SLR   @09,@09                                             0187
         IC    @09,TCXTOTE(,TCXPTR)                                0187
         LH    @11,TNTLEN(,@11)                                    0187
         N     @11,@CF02253                                        0187
         ALR   @11,@09                                             0187
         L     @15,AVTSAVTP(,IEDQAPTR)                             0187
         LR    @14,@11                                             0187
         SH    @14,SAVTNAT1(,@15)                                  0187
         LA    @14,1(,@14)                                         0187
         STH   @14,NNAT                                            0187
*   /*****************************************************************/
*   /*                                                               */
*   /* TNT INDEX LAST NAT ENTRY FROM TNT = # TNT ENTRIES + # TOTE TNT*/
*   /* ENTRIES.                                                      */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0188
*   LASTTNT=TNTLEN+TCXTOTE;         /* TNT INDEX LAST IN NAT         */
*                                                                  0188
         STH   @11,LASTTNT                                         0188
*   /*****************************************************************/
*   /*                                                               */
*   /* #RVT ENTRIES FROM TNT = ACCUMULATION FROM SCAN + # TOTE TNT   */
*   /* ENTRIES                                                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0189
*   NRVT=NRVT+TCXTOTE;              /* ACCUMULATION+TOTE=#RVT      0189
*                                                            @YM07388*/
         LH    @11,NRVT                                            0189
         N     @11,@CF02253                                        0189
         ALR   @09,@11                                             0189
         STH   @09,NRVT                                            0189
*   RFY                                                            0190
*     TCXPTR UNRSTD;                                               0190
*                                                                  0190
*   /*****************************************************************/
*   /*                                                               */
*   /* SET UP MH TABLE ENTRY COUNT.                                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0191
*   RNMH=0;                         /* INITIATE CTR TO 0             */
         SLR   RNMH,RNMH                                           0191
*   RMH=SAVTMHTB;                   /* 1ST MH ENTRY ADDRESS          */
*                                                                  0192
         L     RMH,SAVTMHTB(,@15)                                  0192
*   /*****************************************************************/
*   /*                                                               */
*   /* IF THERE IS AN MH TABLE THEN                                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0193
*   IF RMH^=0                       /* IS THERE AN MH TABLE?         */
*     THEN                                                         0193
         LTR   RMH,RMH                                             0193
         BZ    @RF00193                                            0193
*     DO;                           /* YES                           */
*                                                                  0194
*       /*************************************************************/
*       /*                                                           */
*       /* COUNT # MH TABLE ENTRIES AND ADD TO # RVT ENTRIES.        */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0195
*       CALL MHCOUNT;                                              0195
         BAL   @14,MHCOUNT                                         0195
*     END;                                                         0196
*                                                                  0196
*   /*****************************************************************/
*   /*                                                               */
*   /* IF # MH TABLE ENTRIES = 0 THEN                                */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0197
*   IF RNMH=0                       /* # MH = 0 ?                    */
*     THEN                                                         0197
@RF00193 LTR   RNMH,RNMH                                           0197
         BNZ   @RF00197                                            0197
*     DO;                           /* YES                           */
*                                                                  0198
*       /*************************************************************/
*       /*                                                           */
*       /* INDEX LAST NAT ENTRY = INDEX LAST TNT NAT ENTRY.          */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0199
*       SAVTNATL=LASTTNT;                                          0199
         L     @09,AVTSAVTP(,IEDQAPTR)                             0199
         LH    @15,LASTTNT                                         0199
         N     @15,@CF02253                                        0199
         STH   @15,SAVTNATL(,@09)                                  0199
*     END;                                                         0200
*                                                                  0200
*   /*****************************************************************/
*   /*                                                               */
*   /* ELSE INDEX LAST NAT ENTRY = 1ST MH INDEX + # MH ENTRIES - 1 . */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0201
*   ELSE                                                           0201
*     SAVTNATL=SAVTMHDX+RNMH-1;                                    0201
*                                                                  0201
         B     @RC00197                                            0201
@RF00197 L     @09,AVTSAVTP(,IEDQAPTR)                             0201
         LH    @15,SAVTMHDX(,@09)                                  0201
         N     @15,@CF02253                                        0201
         ALR   @15,RNMH                                            0201
         BCTR  @15,0                                               0201
         STH   @15,SAVTNATL(,@09)                                  0201
*   /*****************************************************************/
*   /*                                                               */
*   /* DEVELOP TOTAL NAT ENTRIES.                                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0202
*   NNAT=NNAT+RNMH;                 /* #TNT NAT ENTRIES + # MH NAT.  */
*                                                                  0202
@RC00197 LH    @09,NNAT                                            0202
         N     @09,@CF02253                                        0202
         ALR   @09,RNMH                                            0202
         STH   @09,NNAT                                            0202
*   /*****************************************************************/
*   /*                                                               */
*   /* CALCULATE NAT SIZE.                                           */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0203
*   NATSIZE=NNAT*(LENGTH(NATODAF)/8);/* # NAT ENTRIES TIMES ENTRY  0203
*                                                            @YM08010*/
*                                                                  0203
         ALR   @09,@09                                             0203
         ST    @09,NATSIZE                                         0203
*   /*****************************************************************/
*   /*                                                               */
*   /* CALCULATE SATSIZE.                                            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0204
*   SATSIZE=(SAVTSUBA)*LENGTH(IEDSATD);                            0204
*                                                                  0204
         L     @15,AVTSAVTP(,IEDQAPTR)                             0204
         SLR   @14,@14                                             0204
         IC    @14,SAVTSUBA(,@15)                                  0204
         SLA   @14,3                                               0204
         ST    @14,SATSIZE                                         0204
*   /*****************************************************************/
*   /*                                                               */
*   /* CALCULATE RVT LENGTH.                                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0205
*   RFY                                                            0205
*     IEDRVTD BASED(ADDRRVT);                                      0205
*   RVTSIZE=NRVT*LENGTH(RVTINDEX)+ADDR(RVTINDEX)-ADDR(IEDRVTD);/*  0206
*                                      #ENTRIES*ENT LEN + COUNT FIELD
*                                      LENGTH                @YM07388*/
*                                                                  0206
         L     @11,ADDRRVT                                         0206
         LH    @10,NRVT                                            0206
         N     @10,@CF02253                                        0206
         ALR   @10,@10                                             0206
         LA    @01,RVTINDEX(,@11)                                  0206
         ALR   @10,@01                                             0206
         SLR   @10,@11                                             0206
         ST    @10,RVTSIZE                                         0206
*   /*****************************************************************/
*   /*                                                               */
*   /* CALCULATE PLCB POOL SIZE.                                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0207
*   PLCBSIZE=AVTPLCBN*(LENGTH(IEDQLCB)/* LENGTH OF PLCB      @YM08062*/
*       -LENGTH(LCBCPA)+48+AVTSCBSZ);/*                      @YM07425*/
         LA    @11,192                                             0207
         SLR   @01,@01                                             0207
         IC    @01,AVTSCBSZ(,IEDQAPTR)                             0207
         ALR   @11,@01                                             0207
         MH    @11,AVTPLCBN(,IEDQAPTR)                             0207
         ST    @11,PLCBSIZE                                        0207
*/* PLCB LEN + SCB LEN TIMES # ENTRIES                               */
*                                                                  0208
*   /*****************************************************************/
*   /*                                                               */
*   /* CALCULATE SIB POOL SIZE.                                      */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0208
*   SIBSIZE=SAVTSIBC*((LENGTH(IEDSIBD)+FOUR)&CLR3);/* ENTRIES X ENTRY
*                                      LENGTH                @YM04606*/
*                                                                  0208
         LH    @01,SAVTSIBC(,@15)                                  0208
         MH    @01,@CH02246                                        0208
         ST    @01,SIBSIZE                                         0208
*   /*****************************************************************/
*   /*                                                               */
*   /* TOTAL GETMAIN SIZE = PLCB POOL + SIB POOL + NAT + SAT + RVT   */
*   /* ROUNDED UP TO NEXT DBL WORD                           @YM02919*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0209
*   AVTGETMN=((PLCBSIZE+SIBSIZE+NATSIZE+SATSIZE+RVTSIZE)+EIGHT)&   0209
*       CLRLOBTS;                                                  0209
*                                                                  0209
         LA    @00,8                                               0209
         ALR   @11,@01                                             0209
         ALR   @11,@09                                             0209
         ALR   @11,@14                                             0209
         ALR   @11,@10                                             0209
         ALR   @11,@00                                             0209
         N     @11,@CF02198                                        0209
         ST    @11,AVTGETMN(,IEDQAPTR)                             0209
*   /*****************************************************************/
*   /*                                                               */
*   /* PUT GETMAIN SIZE DIVIDED BY EIGHT INTO SEC AVT        @YM02919*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0210
*   SAVTPLSZ=AVTGETMN/EIGHT;                                       0210
*                                                                  0210
         SLR   @10,@10                                             0210
         DR    @10,@00                                             0210
         STH   @11,SAVTPLSZ(,@15)                                  0210
*   /*****************************************************************/
*   /*                                                               */
*   /* GETMAIN TABLES AND POOLS.                                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0211
*   CALL GETMAIN;                                                  0211
*                                                                  0211
         BAL   @14,GETMAIN                                         0211
*   /*****************************************************************/
*   /*                                                               */
*   /* STORAGE OBTAINED NOW CLEAR IT FOR SUBSEQUENT USE      @YM04618*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0212
*   RCURR=SAVTPOOL;                 /* POINT AT RECEIVING FIELD    0212
*                                                            @YM04665*/
         L     @09,AVTSAVTP(,IEDQAPTR)                             0212
         L     RCURR,SAVTPOOL(,@09)                                0212
*   RNEXT=(SAVTPLSZ*8);             /* LENGTH OF RCVING FIELD      0213
*                                                            @YM04665*/
         LH    RNEXT,SAVTPLSZ(,@09)                                0213
         SLA   RNEXT,3                                             0213
*   R14=RCURR;                      /* POINT AT SENDING FIELD      0214
*                                                            @YM04665*/
         LR    R14,RCURR                                           0214
*   R15=0;                          /* SET 0 PAD AND LENGTH  @YM04665*/
         SLR   R15,R15                                             0215
*   MVCL(RCURR,R14);                /* CLEAR GETMAINED AREA  @YM04618*/
*                                                                  0216
         MVCL  RCURR,R14                                           0216
*   /*****************************************************************/
*   /*                                                               */
*   /* PLACE PLCB POOL ADDRESS INTO SAVT.                            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0217
*   SAVTPLCB=SAVTPOOL;                                             0217
         L     @09,AVTSAVTP(,IEDQAPTR)                             0217
         L     @15,SAVTPOOL(,@09)                                  0217
         ST    @15,SAVTPLCB(,@09)                                  0217
*   SAVTLLCB=SAVTPLCB+PLCBSIZE-(LENGTH(IEDQLCB)-LENGTH(LCBCPA)+48+ 0218
*       AVTSCBSZ);                  /* SET ADDRESS LAST PLCB @OY19488*/
*                                                                  0218
         AL    @15,PLCBSIZE                                        0218
         LA    @14,192                                             0218
         SLR   @11,@11                                             0218
         IC    @11,AVTSCBSZ(,IEDQAPTR)                             0218
         ALR   @14,@11                                             0218
         LCR   @14,@14                                             0218
         ALR   @14,@15                                             0218
         ST    @14,SAVTLLCB(,@09)                                  0218
*   /*****************************************************************/
*   /*                                                               */
*   /* PLACE SIB POOL ADDRESS INTO SAVT.                             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0219
*   SAVTSIB=SAVTPLCB+PLCBSIZE;      /* PLCB POOL + LEN = SIB POOL ADR*/
*                                                                  0219
         ST    @15,SAVTSIB(,@09)                                   0219
*   /*****************************************************************/
*   /*                                                               */
*   /* PLACE SAT ADDRESS INTO SAVT.                                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0220
*   SAVTSAT=SAVTSIB+SIBSIZE;        /* SAT ADDR = SIB + SIB LEN      */
*                                                                  0220
         AL    @15,SIBSIZE                                         0220
         ST    @15,SAVTSAT(,@09)                                   0220
*   /*****************************************************************/
*   /*                                                               */
*   /* CALCULATE RVT ADDRESS.                                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0221
*   ADDRRVT=SAVTSAT+SATSIZE;        /* RVT ADDR = SAT + SAT LEN      */
*                                                                  0221
         LR    @14,@15                                             0221
         AL    @14,SATSIZE                                         0221
         ST    @14,ADDRRVT                                         0221
*   /*****************************************************************/
*   /*                                                               */
*   /* PLACE NAT ADDRESS INTO SAVT.                                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0222
*   SAVTNAT=ADDRRVT+RVTSIZE-((SAVTNAT1-1)*LENGTH(IEDNATD))/* NAT ADDR
*                                      = RVT ADDR + RVT LEN ADDR IS
*                                      ADJUSTED BY THE DISPLACEMENT
*                                      OF THE FIST INDEX TIMES AN  0222
*                                      ENTRY SIZE. THERE- FORE, TNT
*                                      INDEX-1 TIMES ENTRY MAY BE  0222
*                                      ADDED DIRECTLY TO NAT ADDRESS.*/
*       ;                                                          0222
*                                                                  0222
         LR    @11,@14                                             0222
         AL    @11,RVTSIZE                                         0222
         LH    @10,SAVTNAT1(,@09)                                  0222
         LR    @01,@10                                             0222
         BCTR  @01,0                                               0222
         LR    @00,@01                                             0222
         ALR   @00,@00                                             0222
         SLR   @11,@00                                             0222
         ST    @11,SAVTNAT(,@09)                                   0222
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIATE HOST ENTRY WITH THE RVT ADDRESS AND AN LCB ADDRESS OF*/
*   /* ZERO.                                                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0223
*   RFY                                                            0223
*     IEDSATD BASED(RSAT);                                         0223
*   RFY                                                            0224
*     RSAT RSTD;                                                   0224
*   RSAT=(SAVTSAT+((SUBVSAVE-1)*LENGTH(IEDSATD)));/* HOST SAT ENTRY
*                                      ADDRESS                       */
         SLR   RSAT,RSAT                                           0225
         IC    RSAT,SUBVSAVE                                       0225
         BCTR  RSAT,0                                              0225
         SLA   RSAT,3                                              0225
         ALR   @15,RSAT                                            0225
         LR    RSAT,@15                                            0225
*   RSAT->SATRNRVT=ADDRRVT;         /* RVT                           */
         ST    @14,SATRNRVT(,RSAT)                                 0226
*   RFY                                                            0227
*     RSAT UNRSTD;                                                 0227
*                                                                  0227
*   /*****************************************************************/
*   /*                                                               */
*   /* PLACE SSCP TNT INDEX INTO 1ST 2 RVT SLOTS. ADD # ELEMENTS TO  */
*   /* RVT.                                                          */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0228
*   RRVT=(ADDRRVT);                 /* RVT ADDRESS                   */
         LR    RRVT,@14                                            0228
*   RRVT->RVTINDEX=SAVTSCPT;        /* SET SSCP INDEX INTO PU RVT  0229
*                                      ENTRY                         */
         LH    @09,SAVTSCPT(,@09)                                  0229
         N     @09,@CF02253                                        0229
         STH   @09,RVTINDEX(,RRVT)                                 0229
*   RRVT=RRVT+LENGTH(RVTINDEX);     /* 2ND ENTRY ADDRESS(SSCP)       */
         LA    @15,2                                               0230
         ALR   RRVT,@15                                            0230
*   RRVT->RVTINDEX=SAVTSCPT;        /* SET SSCP INDEX INTO SSCP'S  0231
*                                      SLOT                          */
         STH   @09,RVTINDEX(,RRVT)                                 0231
*   ADDRRVT->RVTNUM=NRVT;           /* SET # ELEMENTS IN RVT @YM07388*/
*                                                                  0232
         LH    @09,NRVT                                            0232
         N     @09,@CF02253                                        0232
         STH   @09,RVTNUM(,@14)                                    0232
*   /*****************************************************************/
*   /*                                                               */
*   /* SET UP TO INITIATE RVT AND NAT. RVT PTR, RVT ELEMENT ADDRESS, */
*   /* TNT PTR, NAT PTR ARE SET ONE ENTRY BACK OF FIRST ENTRY TO BE  */
*   /* PROCESSED.                                                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0233
*   RRVT=RRVT+LENGTH(RVTINDEX);     /* 1ST RVT ENTRY AFTER SSCP/PU   */
         ALR   RRVT,@15                                            0233
*   NRVT=2;                         /* START ELE ADDRESS AT 2      0234
*                                                            @YM07388*/
         STH   @15,NRVT                                            0234
*   RTNT=ADDR(TNTFIRST)+((SAVTNAT1-1)*(TNTENLEN+3)-3);/* ADDRESS-3 OF
*                                      1ST TNT ENTRY HAVING AN     0235
*                                      ASSOCIATED NAT ENTRY          */
         L     @09,IEDQNPTR(,IEDQAPTR)                             0235
         LA    RTNT,82                                             0235
         ALR   RTNT,@09                                            0235
         LA    @14,3                                               0235
         SLR   @00,@00                                             0235
         IC    @00,TNTENLEN(,@09)                                  0235
         ALR   @00,@14                                             0235
         ST    @11,@TF00001                                        0235
         LR    @11,@00                                             0235
         ST    @10,@TF00002                                        0235
         MR    @10,@01                                             0235
         SLR   @11,@14                                             0235
         ALR   RTNT,@11                                            0235
*   RTTCIN=SAVTNAT1-1;              /* INDEX 1ST TNT NAT ENTRY-1     */
