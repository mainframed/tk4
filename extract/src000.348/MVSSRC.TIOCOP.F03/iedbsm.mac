*  RLCB=PLBMLCB;                   /* LCB ADDRESSABILITY             */
         L     RLCB,PLBMLCB(,PLBMPTR)                              0198
*  RSIB=PLBMSIB;                   /* SIB ADDRESSABILITY             */
         L     RSIB,PLBMSIB(,PLBMPTR)                              0199
*/*                                                                  */
*  CALL DOICOORD;                  /* CALCULATE INPUT COORDINATE     */
         BAL   @14,DOICOORD                                        0200
*/*                                                                  */
*  IF ICOORD^=0 THEN               /* VALID INPUT COORDINATE         */
         LTR   ICOORD,ICOORD                                       0201
         BZ    @RF00201                                            0201
*    DO;                           /* YES                            */
*      CALL DOSCOORD;              /* CALCULATE STATE COORDINATE     */
