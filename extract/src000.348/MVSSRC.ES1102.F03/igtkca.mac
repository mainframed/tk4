         TM    APIORXLT(@09),B'00010000'                        02S1535
         BNZ   @RF01535                                         02S1535
*     R14P->VSLAST=BON;             /* LIST. RE-TRANS ADDS MORE.     */
         OI    VSLAST(R14P),B'10000000'                         02S1536
*   DVSLEND=R14P+R1F;               /* NEW END OF VSL POINTER        */
@RF01535 LR    @09,R14P                                         02S1537
         ALR   @09,R1F                                          02S1537
         ST    @09,DVSLEND                                      02S1537
*   RFY                                                         02S1538
*    (R0P,                                                      02S1538
*     R1F,                                                      02S1538
*     R14P,                                                     02S1538
*     R15P) UNRSTD;                 /* FINISHED WITH REGISTERS       */
*                                                               02S1538
*   /*****************************************************************/
*   /*                                                               */
*   /* SET DVSLSZ = REQUIRED VSL SIZE                                */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S1539
*   DVSLSZ=DVSLEND-DVSLBEG;         /* VSL SIZE                      */
         SL    @09,DVSLBEG                                      02S1539
         ST    @09,DVSLSZ                                       02S1539
*   RETURN;                         /* RETURN TO CALLER              */
@EL00004 DS    0H                                               02S1540
@EF00004 DS    0H                                               02S1540
@ER00004 LM    @14,@12,12(@13)                                  02S1540
         BR    @14                                              02S1540
*   END SORTVSL;                    /* REAL VSL BUILT                */
