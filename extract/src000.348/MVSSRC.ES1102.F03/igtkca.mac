*   /*                                                               */
*   /* INITIALIZE THE RECOVERY AREA AND ISSUE AN ESTAE TO COVER THIS */
*   /* PROGRAM'S PROCESSING.                                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0233
*   DO;                             /* START OF CODE SEGMENT         */
*                                                               02S0233
*     /***************************************************************/
*     /*                                                             */
*     /* ESTABLISH RECOVERY ENVIRONMENT. THIS EXIT WILL GAIN CONTROL */
*     /* IF AN ABEND OCCURS. A DUMP IS TAKEN, THE APPROPRIATE ERROR  */
*     /* FLAGS SET, AND CONTROL PASSED TO THE CLEANUP ROUTINE TO     */
*     /* RELEASE RESOURCES BEFORE RETURNING TO THE CALLER.           */
*     /*                                                             */
*     /***************************************************************/
*                                                               02S0234
*                                                               02S0234
*     /***************************************************************/
*     /*                                                             */
*     /* BUILD ESTAE/FRR EXIT PARAMETER LIST                         */
*     /*                                                             */
*     /***************************************************************/
*                                                               02S0234
*     DRCSWS=DRCSWS&&DRCSWS;        /* CLEAR RECOVERY SWITCHES       */
         XC    DRCSWS(2),DRCSWS                                 02S0234
*     GEN(MVC   DXSTAE,CXSTAE)                                  02S0235
*     REFS(CXSTAE) SETS(DXSTAE);    /* INITIALIZE ESTAE PARM LIST    */
         MVC   DXSTAE,CXSTAE
*     RFY                                                       02S0236
*      (R0P,                                                    02S0236
*       R1F,                                                    02S0236
*       R14P,                                                   02S0236
*       R15F) RSTD;                 /* INITIALIZE APES BLOCK         */
*     R0P=ADDR(APES);               /* TO ZEROS                      */
         LA    R0P,APES                                         02S0237
*     R1F=LENGTH(APES);             /* USE MVCL TO                   */
         LA    R1F,648                                          02S0238
*     R15F=0;                       /* CLEAR, SINCE FIELD            */
         SLR   R15F,R15F                                        02S0239
*     MVCL(R0P,R14P);               /* MORE THAN 256 BYTES LONG      */
         MVCL  R0P,R14P                                         02S0240
*     RFY                                                       02S0241
*      (R0P,                                                    02S0241
*       R1F,                                                    02S0241
*       R14P,                                                   02S0241
*       R15F) UNRSTD;               /* FINISHED WITH MVCL REGS       */
*     APESID=APESIDC;               /* IN-CORE ID                    */
         MVC   APESID(4),@CC03810                               02S0242
*     APESADDR=ADDR(APES);          /* ESTAE PARM VALUE              */
         LA    @04,APES                                         02S0243
         ST    @04,APESADDR                                     02S0243
*     APESAPMV=ADDR(CMBRXZZZ);      /* MESSAGE INDEX TABLE           */
         LA    @04,CMBRXZZZ                                     02S0244
         ST    @04,APESAPMV                                     02S0244
*     APESMXCD=CERRN99;             /* MAXIMUM APESCODE VALUE        */
         MVI   APESMXCD,X'21'                                   02S0245
*     APESFDMP=BON;                 /* DUMP ON ERROR                 */
*     APESFREC=BON;                 /* RECORD ON LOGREC              */
*     APESFSDP=BON;                 /* SDUMP ON ERROR                */
         OI    APESFDMP,B'11100000'                             02S0248
*     APESMOD='IGTKCA  ';           /* MODULE IN CONTROL             */
         MVC   APESMOD(8),@CC03972                              02S0249
*     APESPROC='IGTKCA  ';          /* CSECT IN CONTROL              */
         MVC   APESPROC(8),@CC03972                             02S0250
*     APESFRR='        ';           /* NO FRR ID                     */
         MVC   APESFRR(8),@CC03973                              02S0251
*     APESREGP=ADDR(APESREGZ);      /* DEFAULT REGISTER SAVEAREA     */
         LA    @04,APESREGZ                                     02S0252
         ST    @04,APESREGP                                     02S0252
*     RFY                                                       02S0253
*      (R15P) RSTD;                 /* SET REGISTER 15 TO            */
*     R15P=ADDR(CANESTAE);          /* BACKOUT ROUTINE               */
         LA    R15P,CANESTAE                                    02S0254
*     STM(R0F,R15F,APESREGZ);       /* SAVE REGISTERS FOR RETRY      */
         STM   R0F,R15F,APESREGZ                                02S0255
*     RFY                                                       02S0256
*       R15P UNRSTD;                /* FINISHED WITH 15              */
*     APESCODE=CERRN99;             /* 'UNKNOWN' ERROR               */
         MVI   APESCODE,X'21'                                   02S0257
*     RFY                                                       02S0258
*       RIGTUKA RSTD;               /* POINTS TO EXIT ROUTINE        */
*     RIGTUKA=ADDR(APCVZUKA);       /* (IGTUKA).                     */
         L     RIGTUKA,APCTZUKA(,APCTPTR)                       02S0259
*     GEN REFS(APESADDR,APSVPTR,DXSTAE,RIGTUKA,FLCCVT,PSA,CVTSV60,
*         CVTMAP) SETS(DXSTAE);                                 02S0260
*                                   /*
         IGTYSTAE (RIGTUKA),       IGTUKA IS EXIT ROUTINE              *
               PARAM=APESADDR,     ADDR OF APES IS PARAMETER LIST      *
               BRANCH=YES,         BRANCH ENTRY                        *
               SVEAREA=(APSVPTR),  USE REGISTER 13 SAVEAREA            *
               KEY=5,              CURRENTLY IN KEY-5 - RESET          *
               MF=(E,DXSTAE)       EXECUTE FORM OF THE MACRO
*     RFY                                                       02S0261
*      (R15F) RSTD;                 /* ESTAE RETURN CODE             */
*     RFY                                                       02S0262
*       RIGTUKA UNRSTD;             /* DONE WITH EXIT POINTER        */
*     IF R15F^=0 THEN               /* IF ESTAE FAILED,              */
         LTR   R15F,R15F                                        02S0263
         BZ    @RF00263                                         02S0263
*       DO;                         /* THEN                          */
*                                                               02S0264
*         /***********************************************************/
*         /*                                                         */
*         /* CONVERT THE ERROR CODE (APESCOD1) TO APRL ERROR CODES   */
*         /*                                                         */
*         /***********************************************************/
*                                                               02S0265
*         APESCOD1=CERRN01;         /* SET ERROR CODE AND            */
         MVI   APESCOD1,X'01'                                   02S0265
*         CALL SETAPRL;             /* CONVERT APESCOD1 TO APRL CODES*/
         BAL   @14,SETAPRL                                      02S0266
*         SPKA(DUSERKEY);           /* SWITCH TO USER'S KEY          */
         SLR   @14,@14                                          02S0267
         IC    @14,DUSERKEY                                     02S0267
         SPKA  0(@14)                                           02S0267
*         DAPRLPTR->APRLRCT=ADDR(DAPRL)->APRLRCT;/* MOVE VALUES      */
         L     @14,DAPRLPTR                                     02S0268
         LA    @09,DAPRL                                        02S0268
         IC    @04,APRLRCT(,@09)                                02S0268
         STC   @04,APRLRCT(,@14)                                02S0268
*         DAPRLPTR->APRLRCC=ADDR(DAPRL)->APRLRCC;/* FROM PROTECTED   */
         IC    @04,APRLRCC(,@09)                                02S0269
         STC   @04,APRLRCC(,@14)                                02S0269
*         DAPRLPTR->APRLRCN=ADDR(DAPRL)->APRLRCN;/* APRL (DAPRL)     */
         IC    @04,APRLRCN(,@09)                                02S0270
         STC   @04,APRLRCN(,@14)                                02S0270
*         DAPRLPTR->APRLMSGN=ADDR(DAPRL)->APRLMSGN;/* TO USER APRL   */
         LH    @04,APRLMSGN(,@09)                               02S0271
         STH   @04,APRLMSGN(,@14)                               02S0271
*         DAPRLPTR->APRLVALN=ADDR(DAPRL)->APRLVALN;/* MUST RUN IN    */
         LH    @09,APRLVALN(,@09)                               02S0272
         STH   @09,APRLVALN(,@14)                               02S0272
*         GEN(MODESET EXTKEY=DATAMGT)                           02S0273
*         NOREFS NOSETS;            /* USER KEY.                     */
         MODESET EXTKEY=DATAMGT
*         GOTO EXIT;                /* GO TO PROGRAM EXIT LOGIC      */
         B     EXIT                                             02S0274
*       END;                        /* ERROR PROCESSED               */
*     RFY                                                       02S0276
*       R15F UNRSTD;                /* ESTAE RETURN CODE PROCESSED   */
@RF00263 DS    0H                                               02S0277
*   END;                            /* ESTAE ENVIRONMENT SET         */
         EJECT
*                                                               01S0278
*   /*****************************************************************/
*   /*                                                               */
*   /* COPY THE REQUIRED PORTIONS OF THE USER'S APRL TO A PROTECTED  */
*   /* AREA.                                                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0278
*   DO;                                                         02S0278
*                                                               02S0278
*     /***************************************************************/
*     /*                                                             */
*     /* COPY THE USER'S APRL TO A PROTECTED COPY. THIS ALLOWS       */
*     /* REFERENCE TO A STABLE COPY AND ELIMINATES HAVING TO SWITCH  */
*     /* PROTECT KEYS TO REFERENCE THE APRL. ONLY THOSE FIELDS       */
*     /* REQUIRED FOR PROCESSING ARE COPIED. IF AN ERROR OCCURS      */
*     /* REFERENCING THE APRL, THE ESTAE WILL INTERCEPT THE FAILURE. */
*     /* AN ERROR CODE WILL BE RETURNED TO THE CALLER INDICATING THE */
*     /* FAILURE. THEN MERGE THE VALUES SPECIFIED IN THE APDS INTO   */
*     /* THE APRL.                                                   */
*     /*                                                             */
*     /***************************************************************/
*                                                               02S0279
*     DAPRL=DAPRL&&DAPRL;           /* CLEAR INTERNAL APRL AREA      */
         XC    DAPRL(80),DAPRL                                  02S0279
*     APESCODE=CERRN02;             /* ERROR CODE - BAD APRL         */
         MVI   APESCODE,X'02'                                   02S0280
*     APESFSDP=BOFF;                /* NO SDUMP ON ERROR             */
         NI    APESFSDP,B'11011111'                             02S0281
*     SPKA(DUSERKEY);               /* SWITCH TO USER'S KEY          */
         SLR   @04,@04                                          02S0282
         IC    @04,DUSERKEY                                     02S0282
         SPKA  0(@04)                                           02S0282
*     RFY                                                       02S0283
*      (R0F,                                                    02S0283
*       R1F,                                                    02S0283
*       R14F,                                                   02S0283
*       R15F) RSTD;                 /* COPY REGISTERS                */
*     R0F=DAPRLPTR->APRLOPTS;       /* HOLD OPTION SWITCHES          */
         L     @04,DAPRLPTR                                     02S0284
         L     R0F,APRLOPTS(,@04)                               02S0284
*     R1F=DAPRLPTR->APRLOPTZ;       /* HOLD OPTION SWITCHES          */
         L     R1F,APRLOPTZ(,@04)                               02S0285
*     R14F=DAPRLPTR->APRLCCW;       /* ADDR USER'S CHANNEL PGM       */
         L     R14F,APRLCCW(,@04)                               02S0286
*     R15F=DAPRLPTR->APRLUSER;      /* INSTALLATION FIELD            */
         L     R15F,APRLUSER(,@04)                              02S0287
*     GEN(MODESET EXTKEY=DATAMGT)                               02S0288
*     NOREFS NOSETS;                /* RESUME NORMAL KEY             */
         MODESET EXTKEY=DATAMGT
*     APESCODE=CERRN99;             /* IS VPSS FAULT.                */
         MVI   APESCODE,X'21'                                   02S0289
*     APESFSDP=BON;                 /* SDUMP WANTED AGAIN            */
         OI    APESFSDP,B'00100000'                             02S0290
*     DAPRLXPT=ADDR(DAPRL);         /* INITIALIZE POINTER TO COPY    */
         LA    @04,DAPRL                                        02S0291
         ST    @04,DAPRLXPT                                     02S0291
*     RFY                                                       02S0292
*       APRL BASED(DAPRLXPT);       /* OF APRL.              @ZA26916*/
*     APRLOPTS=R0F;                 /* SAVE OPTION SWITCHES          */
         ST    R0F,APRLOPTS(,@04)                               02S0293
*     APRLOPTZ=R1F;                 /* SAVE OPTION SWITCHES          */
         ST    R1F,APRLOPTZ(,@04)                               02S0294
*     APRLCCW=R14F;                 /* SAVE USER'S CHANNEL PGM ADDR  */
         ST    R14F,APRLCCW(,@04)                               02S0295
*     APRLUSER=R15F;                /* INSTALLATION FIELD            */
         ST    R15F,APRLUSER(,@04)                              02S0296
*     RFY                                                       02S0297
*      (R0F,                                                    02S0297
*       R1F,                                                    02S0297
*       R14F,                                                   02S0297
*       R15F) UNRSTD;               /* FINISHED WITH REGISTERS       */
*     APRLID=APRLIDC;               /* IN-CORE ID FOR DUMP           */
         MVC   APRLID(4,@04),@CC01912                           02S0298
*     APESRTRY=ADDR(RESTAPRL);      /* ON ERROR, RESTORE USER'S APRL */
         LA    @15,RESTAPRL                                     02S0299
         L     @14,APESREGP                                     02S0299
         ST    @15,APESRTRY(,@14)                               02S0299
*     IF APRLXIO=BON THEN           /* IF APRL INDICATES THAT        */
         TM    APRLXIO(@04),B'00000100'                         02S0300
         BNO   @RF00300                                         02S0300
*       DO;                         /* APIO ALREADY EXISTS, IS ERROR */
*         APESCODE=CERRN26;         /* SET ERROR CODE                */
         MVI   APESCODE,X'1A'                                   02S0302
*         APESFSDP=BOFF;            /* SDUMP NOT WANTED              */
         NI    APESFSDP,B'11011111'                             02S0303
*         DO;                       /* FORCE ABEND TO LOG ERROR &
*                                      PASS CONTROL TO THE (E)STAE OR
*                                      FRR EXIT ROUTINE.             */
*           DO;                     /* ABEND(APCTABND)               */
*             RESPECIFY                                         02S0306
*              (GPR01F) RESTRICTED;                             02S0306
*             GPR01F=APCTABND;      /* COMP CODE IN REG 1            */
         L     GPR01F,APCTABND(,APCTPTR)                        02S0307
*             SVC(13);              /* ISSUE ABEND SVC               */
