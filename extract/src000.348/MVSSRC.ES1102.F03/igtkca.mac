*FIXAPIO:                           /* REPAIR THE APIO CHAIN         */
*   PROC;                           /* ENTERED IN KEY-0              */
*                                                               02S1764
FIXAPIO  STM   @14,@12,@SA00013                                 02S1764
*   /*****************************************************************/
*   /*                                                               */
*   /* ENTERED WHEN AN ERROR OCCURS SCANNING OR UPDATING THE APIO    */
*   /* CHAIN. THIS ROUTINE WILL ATTEMPT TO REPAIR THE CHAIN. IF      */
*   /* REPAIR IS IMPOSSIBLE, THE QUEUE HEADERS AND RESET TO ZERO,    */
*   /* THUS ELIMINATING THE ENTIRE APIO QUEUE. CONTROL THEN RETURNS  */
*   /* TO CALL QUEUE USER (RETURN ADDRESS CONTAINED IN REGISTER 14). */
*   /* CONTROL MUST BE RETURNED IN KEY-5, WITH THE RECURSION FLAG    */
*   /* (APESPREV) SET ON. THIS PREVENTS RECURSION LOOPING IF THE     */
*   /* CALLER HAS A PROBLEM (RATHER THAN AN INVALID APIO QUEUE). ON  */
*   /* ENTRY, THE LOCAL AND CMS LOCKS MUST BE OWNED (THIS IS A       */
*   /* REQUIREMENT FOR TASKS UPDATING THE APIO QUEUE).               */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S1765
*                                                               02S1765
*   /*****************************************************************/
*   /*                                                               */
*   /* SAVE CURRENT RECOVERY ENVIRONMENT. SET RETRY FOR 'FIXAPIO1',  */
*   /* WHICH ELIMINATES THE APIO CHAIN.                              */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S1765
*   GEN(MODESET EXTKEY=DATAMGT)                                 02S1765
*   NOREFS NOSETS;                  /* NORMAL KEY                    */
         MODESET EXTKEY=DATAMGT
*   DRETRY(10)=APESRTRY;            /* SAVE CURRENT RETRY POINT      */
         L     @09,APESREGP                                     02S1766
         L     @04,APESRTRY(,@09)                               02S1766
         ST    @04,DRETRY+36                                    02S1766
*   APESRTRY=ADDR(FIXAPIO1);        /* NEW RETRY -- ELIMINATE APIO   */
         LA    @04,FIXAPIO1                                     02S1767
         ST    @04,APESRTRY(,@09)                               02S1767
*   GEN NOREFS NOSETS SEQFLOW FLOWS(FIXAPIO1);                  02S1768
*                                   /* NO CODE -- SHOW FIXAPIO1 USED
*   DO;                             /* ERROR RECOVERY COMPLETE       */
*     APESFPRV=BOFF;                /* RESET RECURSION FLAG.         */
         NI    APESFPRV,B'10111111'                             02S1770
*     APESCODE=CERRN99;             /* RESET ERROR CODE              */
         MVI   APESCODE,X'21'                                   02S1771
*     APESFDMP=BON;                 /* RESET DUMP/LOG FLAGS          */
*     APESFREC=BON;                 /* TO THEIR NORMAL VALUES        */
*     APESFSDP=BON;                 /* (ALL ON)                      */
         OI    APESFDMP,B'11100000'                             02S1774
*   END;                            /* ALL INDICATORS RESET          */
*                                                               02S1775
*   /*****************************************************************/
*   /*                                                               */
*   /* BUILD Q-VERIFICATION BLOCK                                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S1776
*   QVPL=QVPL&&QVPL;                /* RESET ALL FIELDS              */
         XC    QVPL(40),QVPL                                    02S1776
*   QVPLEVR=ADDR(FIXAPIOV);         /* APIO VERIFICATION ROUTINE     */
