*SORTVSL:                           /* BUILD REAL VSL TABLE          */
*   PROC                            /* FOR PGFIX/PGFREE PROCESSING   */
*       OPTIONS(NOSAVEAREA,         /* CALL NO ONE                   */
*       NOPARMREG);                 /* REGS 0&1 NOT USED             */
*                                                               02S1499
SORTVSL  STM   @14,@12,12(@13)                                  02S1499
*   /*****************************************************************/
*   /*                                                               */
*   /* TAKE THE PGFIX LIST ALREADY CONSTRUCTED AND BUILD AN EFFICIENT*/
*   /* LIST. REMOVE DUPLICATE ENTRIES AND COMBINE ADJACENT ENTRIES.  */
*   /* SET THE END-OF-LIST INDICATOR IN THE LAST ENTRY. ON INPUT,    */
*   /* DVSLBEG POINTS TO THE FIRST ENTRY, AND DVSLEND THE FIRST      */
*   /* AVAILABLE SLOT. ON EXIT, SET DVSLSZ TO THE SIZE OF THE PGFIX  */
*   /* (VSL) LIST.                                                   */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S1500
*                                                               02S1500
*   /*****************************************************************/
*   /*                                                               */
*   /* SORT ENTRIES IN ASCENDING SEQUENCE (VSLSTRT FIELD)            */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S1500
*   RFY                             /* SORT REGISTERS                */
*    (R0F,                          /* INTERCHANGE SWITCH            */
*     R1P,                          /* NEXT ENTRY                    */
*     R14P,                         /* SCANNER                       */
*     R15P) RSTD;                   /* LAST ENTRY                    */
*   R15P=DVSLEND-LENGTH(VSL);       /* LAST ENTRY                    */
         L     R15P,DVSLEND                                     02S1501
         SL    R15P,@CF00037                                    02S1501
*   DO                              /* CONTINUE SORT UNTIL           */
*         UNTIL(R0F=0              /* NO INTERCHANGES LAST PASS     */
*         R15P=DVSLBEG);            /* OR ONLY 1 ENTRY LEFT.         */
@DL01502 DS    0H                                               02S1503
*     R14P=DVSLBEG;                 /* FIRST ENTRY                   */
         L     R14P,DVSLBEG                                     02S1503
*     R0F=0;                        /* NO INTERCHANGES SO FAR        */
         SLR   R0F,R0F                                          02S1504
*     DO UNTIL(R14P=R15P);          /* SCAN UNTIL LAST ENTRY         */
@DL01505 DS    0H                                               02S1506
*       R1P=R14P+LENGTH(VSL);       /* NEXT VSL ENTRY                */
         LA    R1P,8(,R14P)                                     02S1506
*       IF R14P->VSLSTRT>           /* IF CURRENT START > THAN       */
*           R1P->VSLSTRT THEN       /* NEXT START,                   */
         CLC   VSLSTRT(4,R14P),VSLSTRT(R1P)                     02S1507
         BNH   @RF01507                                         02S1507
*         DO;                       /* THEN MUST INTERCHANGE.        */
*           R14P->VSL=R14P->VSL&&   /* USE THREE                     */
*               R1P->VSL;           /* EXCLUSIVE OR'S                */
         XC    VSL(8,R14P),VSL(R1P)                             02S1509
*           R1P->VSL=R1P->VSL&&     /* (XC) TO INTERCHANGE           */
*               R14P->VSL;          /* THE CONTENTS OF THE           */
         XC    VSL(8,R1P),VSL(R14P)                             02S1510
*           R14P->VSL=R14P->VSL&&   /* TWO VSL ENTRIES               */
*               R1P->VSL;           /* THEN SHOW THAT                */
         XC    VSL(8,R14P),VSL(R1P)                             02S1511
*           R0F=R0F-1;              /* INTERCHANGE MADE              */
         BCTR  R0F,0                                            02S1512
*         END;                      /* I+1_ENTRY >= I_ENTRY          */
*       R14P=R1P;                   /* R14P = ADDR(NEXT_ENTRY)       */
@RF01507 LR    R14P,R1P                                         02S1514
*     END;                          /* R15P-> VSL HIGHEST ADDRESS    */
@DE01505 CR    R14P,R15P                                        02S1515
         BNE   @DL01505                                         02S1515
*     R15P=R15P-LENGTH(VSL);        /* ONLY SORT PREVIOUS ENTRIES    */
         SL    R15P,@CF00037                                    02S1516
*   END;                            /* ENTRIES SORTED.               */
@DE01502 LTR   R0F,R0F                                          02S1517
         BZ    @DC01502                                         02S1517
         C     R15P,DVSLBEG                                     02S1517
         BNE   @DL01502                                         02S1517
@DC01502 DS    0H                                               02S1518
*   RFY                                                         02S1518
*    (R0F,                                                      02S1518
*     R1P,                                                      02S1518
*     R14P,                                                     02S1518
*     R15P) UNRSTD;                 /* FINISHED WITH REGISTERS       */
*                                                               02S1519
*   /*****************************************************************/
*   /*                                                               */
*   /* SCAN THE VSL ENTRIES CHECKING FOR THE STARTING ADDRESS IN     */
*   /* ENTRY N+1 <= ENDING ADDRESS IN ENTRY N. IF FOUND, SET THE     */
*   /* ENDING ADDRESS IN ENTRY N EQUAL THE MAXIMUM OF THE ENDING     */
*   /* ADDRESSES IN ENTRIES N AND N+1. THEN ELIMINATE ENTRY N+1.     */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S1519
*   RFY                                                         02S1519
*    (R1F,                          /* VSL SIZE                      */
*     R14P,                         /* CURRENT VSL ENTRY             */
*     R15P,                         /* 'FROM' VSL ENTRY              */
*     R0P) RSTD;                    /* END OF VSL ENTRIES ADDRESS    */
*   R14P=DVSLBEG;                   /* INITIAL VSL ENTRY             */
         L     R14P,DVSLBEG                                     02S1520
*   R1F=LENGTH(VSL);                /* VSL ENTRY LENGTH              */
         LA    R1F,8                                            02S1521
*   R0P=DVSLEND;                    /* END-OF-VSL ENTRIES POINTER    */
         L     R0P,DVSLEND                                      02S1522
*   R15P=R14P+R1F;                  /* 'FROM' IS SECOND VSL ENTRY    */
         LR    R15P,R14P                                        02S1523
         ALR   R15P,R1F                                         02S1523
*   DO UNTIL(R15P=R0P);             /* SCAN ALL VSL ENTRIES          */
@DL01524 DS    0H                                               02S1525
*     IF R15P->VSLSTRT<=            /* IF START N+1 <= END OF N,     */
*         R14P->VSLEND THEN         /* THEN SET                      */
         L     @09,VSLEND(,R14P)                                02S1525
         C     @09,VSLSTRT(,R15P)                               02S1525
         BL    @RF01525                                         02S1525
*       IF R15P->VSLEND>            /* END OF ENTRY N =              */
*           R14P->VSLEND THEN       /* MAXIMUM OF END IN ENTRIES     */
         L     @04,VSLEND(,R15P)                                02S1526
         CR    @04,@09                                          02S1526
         BNH   @RF01526                                         02S1526
*         R14P->VSLEND=R15P->VSLEND;/* N AND N+1.                    */
         ST    @04,VSLEND(,R14P)                                02S1527
*       ELSE                                                    02S1528
*         ;                         /* IF START OF N+1 > END OF      */
@RF01526 DS    0H                                               02S1529
*     ELSE                          /* ENTRY N, THEN FINISHED        */
*       DO;                         /* WITH ENTRY N. COPY ENTRY      */
         B     @RC01525                                         02S1529
@RF01525 DS    0H                                               02S1530
*         R14P=R14P+R1F;            /* N+1 TO SLOT AFTER ENTRY       */
         ALR   R14P,R1F                                         02S1530
*         R14P->VSL=R15P->VSL;      /* N. WILL PGRFORM SAME PROCESS  */
         MVC   VSL(8,R14P),VSL(R15P)                            02S1531
*       END;                        /* AS ABOVE.                     */
*     R15P=R15P+R1F;                /* N+1 MERGED OR COPIED          */
@RC01525 ALR   R15P,R1F                                         02S1533
*   END;                            /* CONTINUE THRU ALL VSL SLOTS.  */
*                                                               02S1534
@DE01524 CR    R15P,R0P                                         02S1534
         BNE   @DL01524                                         02S1534
*   /*****************************************************************/
*   /*                                                               */
*   /* COMPRESSED LIST BUILT. REGISTER R14P POINTS TO LAST ENTRY.    */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S1535
*   IF APIORXLT=BOFF THEN           /* IF NOT RE-TRANS, SET END-OF-  */
