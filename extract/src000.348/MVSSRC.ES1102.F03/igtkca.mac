*   QVPLWKA=ADDR(DFXAPIOW);         /* Q-VERIFICATION WORKAREA       */
         LA    @09,DFXAPIOW                                     02S1778
         ST    @09,QVPLWKA                                      02S1778
*   QVPLHDR=ADDR(APCTAPIO);         /* Q-HEADER (FORWARD)            */
         LA    @09,APCTAPIO(,APCTPTR)                           02S1779
         ST    @09,QVPLHDR                                      02S1779
*   QVPLFPTR=ADDR(APIOAPIO)-ADDR(APIO);/* FORWARD POINTER OFFSET     */
         MVC   QVPLFPTR(2),@CH04216                             02S1780
*   QVPLTRLR=ADDR(APCTIOAP);        /* Q-HEADER (BACKWARD)           */
