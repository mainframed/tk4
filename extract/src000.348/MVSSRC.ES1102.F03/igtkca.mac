TKCA     TITLE 'IGTKCA -- TRANSLATE VPSS CHANNEL PROGRAM               *
                        '
IGTKCA   CSECT ,                                                01S0001
@MAINENT DS    0H                                               01S0001
         USING *,@15                                            01S0001
         B     @PROLOG                                          01S0001
         DC    AL1(16)                                          01S0001
         DC    C'IGTKCA    77.286'                              01S0001
         DROP  @15
@PROLOG  BALR  @07,0                                            01S0001
@PSTART  LA    @08,4095(,@07)                                   01S0001
         USING @PSTART,@07                                      01S0001
         USING @PSTART+4095,@08                                 01S0001
*   RAPRLPTR=R1P;                   /* SAVE APRL ADDRESS             */
         LR    RAPRLPTR,R1P                                     01S0179
*   RFY                                                         01S0180
*     APCT BASED(APCTPTR);          /* TELL                          */
*   RFY                                                         01S0181
*     APDX BASED(RAPDXPTR);         /* COMPILER                      */
*   RFY                                                         01S0182
*     APRL BASED(RAPRLPTR);         /* OF POINTERS.                  */
*   RFY                                                         01S0183
*     DEBBASIC BASED(RDEBPTR);      /* DEB,APDX,APCT OK BY DEBCHK    */
*   RFY                                                         01S0184
*     R1P UNRSTD;                   /* FINISHED WITH INPUT REG       */
*   GEN(MODESET EXTKEY=DATAMGT)                                 01S0185
*   NOREFS NOSETS;                  /* RUN IN DATA MANAGEMENT KEY    */
         MODESET EXTKEY=DATAMGT
         EJECT
*                                                               01S0186
*   /*****************************************************************/
*   /*                                                               */
*   /* ACQUIRE SAVEAREA/WORKAREA. INITIALIZE PL/S WORKAREA.          */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0186
*   DO;                             /* START OF CODE SEGMENT         */
*                                                               02S0186
*     /***************************************************************/
*     /*                                                             */
*     /* ISSUE CONDITIONAL GETMAIN FOR SA-WA. ALLOW FOR CONSTRUCTED  */
*     /* CONTROL BLOCKS (ACQUIRED BY 'IGTYGSTK'). ALLOW FOR FINAL    */
*     /* COPY AT END OF AREA BY FREEING HALF THE EXTRA SPACE. THIS   */
*     /* MINIMIZES STORAGE FRAGMENTATION SINCE STORAGE IS OBTAINED   */
*     /* FROM THE HIGH END.                                          */
*     /*                                                             */
*     /***************************************************************/
*                                                               02S0187
*     RFY                                                       02S0187
*      (RLV,                        /* GETMAIN LENGTH                */
*       RADDR,                      /* ACQUIRED STORAGE ADDRESS      */
*       R1P,                        /* ACQUIRED STORAGE ADDRESS      */
*       R15F) RSTD;                 /* GETMAIN RETURN CODE           */
*     RLV=(CPGSIZE+                 /* PL/S WORKAREA PLUS            */
*         2*CTOTAL);                /* TWICE CONTROL BLOCK SIZE      */
         L     RLV,@CF04211                                     02S0188
*     GEN REFS(RLV);                                            02S0189
*                                   /*
         GETMAIN RC,               CONDITIONAL REQUEST                 *
               LV=(RLV),           REQUIRED LENGTH                     *
               SP=CSPWORK,         SUBPOOL                             *
               BNDRY=PAGE          PAGE BOUNDARY
*     IF R15F^=0 THEN               /* IF STORAGE NOT ACQUIRED,      */
         LTR   R15F,R15F                                        02S0190
         BZ    @RF00190                                         02S0190
*       DO;                         /* THEN SET ERROR FLAGS & EXIT.  */
*         SPKA(DEBPROTG);           /* USER (APRL) KEY               */
         SLR   @14,@14                                          02S0192
         IC    @14,DEBPROTG(,RDEBPTR)                           02S0192
         SPKA  0(@14)                                           02S0192
*         APRLRCT=APCCTLOG;         /* LOGICAL ERROR                 */
         MVI   APRLRCT(RAPRLPTR),X'08'                          02S0193
*         APRLRCC=APCCI;            /* DETECTED BY I/O PROCESSOR     */
         MVI   APRLRCC(RAPRLPTR),X'05'                          02S0194
*         APRLRCN=APCCI002;         /* INDIVIDUAL NUMBER             */
         MVI   APRLRCN(RAPRLPTR),X'02'                          02S0195
*         APRLMSGN=APCMI002;        /* NO STORAGE                    */
         MVC   APRLMSGN(2,RAPRLPTR),@CH01949                    02S0196
*         APRLVALN=APCMK049;        /* SUBPOOL NUMBER                */
         MVC   APRLVALN(2,RAPRLPTR),@CH02060                    02S0197
*         R15F=APCCRERR;            /* REGISTER 15 ERROR VALUE       */
         LA    R15F,8                                           02S0198
*         SVC(3);                   /* EXIT BACK TO CALLER           */
         SVC   3                                                02S0199
*         GEN NOREFS NOSETS EXIT;                               02S0200
*                                   /*
*                                  NO CODE -- TELL PL/S THAT SVC 3
*                                  * DOES NOT RETURN CONTROL.
*       END;                        /* ERROR PROCESSED               */
*                                                               02S0202
*     /***************************************************************/
*     /*                                                             */
*     /* FREEMAIN HIGH END OF ACQUIRED STORAGE. MAY BE RE-USED FOR   */
*     /* PERMANENT COPIES (MINIMIZE STORAGE FRAGMENTATION).          */
*     /*                                                             */
*     /***************************************************************/
*                                                               02S0202
*     RFY                                                       02S0202
*       R0F RSTD;                   /* REQUIRED REGISTER             */
@RF00190 DS    0H                                               02S0203
*     RADDR=R1P;                    /* SAVE ADDR ACQUIRED STORAGE    */
         LR    RADDR,R1P                                        02S0203
*     RLV=RLV-CTOTAL;               /* WILL FREE 'CTOTAL' BYTES      */
         L     @14,@CF03933                                     02S0204
         SLR   RLV,@14                                          02S0204
*     R1P=R1P+RLV;                  /* POINT TO AREA TO FREE         */
         ALR   R1P,RLV                                          02S0205
*     R0F=CTOTAL;                   /* LENGTH TO FREE                */
         LR    R0F,@14                                          02S0206
*     GEN;                                                      02S0207
*                                   /*
         FREEMAIN RU,              FREE EXCESS STORAGE                 *
               LV=(0),             * LENGTH = CTOTAL                   *
               A=(1),              * FREE HI-END OF AREA               *
               SP=CSPWORK          * SUBPOOL
*     RFY                                                       02S0208
*      (R0F,                                                    02S0208
*       R1P,                                                    02S0208
*       R15F) UNRSTD;               /* FINISHED WITH REGISTERS       */
*                                                               02S0208
*     /***************************************************************/
*     /*                                                             */
*     /* CALL IGTUAF TO BUILD A SAVEAREA-WORKAREA. INPUT IS: BYTE-0  */
*     /* OF R0 = SUBPOOL, BYTES 1-3 OF R0 = LENGTH, R1 HAS DYNAMIC   */
*     /* STORAGE ADDRESS. ON RETURN, R1 HAS ADDRESS OF APSC.         */
*     /*                                                             */
*     /***************************************************************/
*                                                               02S0209
*     RFY                                                       02S0209
*      (R0F,                                                    02S0209
*       R1P) RSTD;                  /* REQUIRED REGISTERS            */
*     R0F=CSPWORK*'01000000'X+RLV;  /* SUBPOOL + LENGTH IF SUBPOOL
*                                      NOT CONSTANT, USE 'SLL' TO
*                                      KEEP HI-BIT SET RIGHT         */
         LR    R0F,RLV                                          02S0210
         AL    R0F,@CF04212                                     02S0210
*     R1P=RADDR;                    /* ADDRESS OF DYNAMIC AREA       */
         LR    R1P,RADDR                                        02S0211
*     CALL APCVZUAF;                /* BUILD VPSS STORAGE BLOCKS     */
         L     @15,APCTZUAF(,APCTPTR)                           02S0212
         BALR  @14,@15                                          02S0212
*     RFY                                                       02S0213
*      (RLV,                                                    02S0213
*       RADDR) UNRSTD;              /* FINISHED WITH THESE           */
*     R0F=(APSVLN+APSCLN+@SIZDATD+7)/* PL/S WORKAREA + SAVEAREA      */
*         &'0000FFF8'X;             /* ROUNDED TO DOUBLE-WORD        */
         LA    R0F,95                                           02S0214
         AL    R0F,@SIZDATD                                     02S0214
         N     R0F,@CF03963                                     02S0214
*     R1P->APSCCRSZ=R0F;            /* SAVE THE SIZE                 */
         STH   R0F,APSCCRSZ(,R1P)                               02S0215
*     R1P=R1P+APSCLN;               /* POINT TO APSV                 */
         LA    R1P,8(,R1P)                                      02S0216
*     RFY                                                       02S0217
*       R0F UNRSTD;                 /* FINISHED WITH REGISTER        */
*     R1P->APSVPREV=0;              /* NO PREVIOUS APSV              */
         SLR   @10,@10                                          02S0218
         ST    @10,APSVPREV(,R1P)                               02S0218
*     APSVPTR=R1P;                  /* POINT TO APSV                 */
         LR    APSVPTR,R1P                                      02S0219
*     RFY                                                       02S0220
*       R1P UNRSTD;                 /* FINISHED WITH REGISTER        */
*     RFY                                                       02S0221
*       APDATA RSTD;                /* ESTABLISH DYNAMIC AREA        */
*     APDATA=ADDR(APSV)+APSVLN;     /* BASE. TELL ASSEMBLER TOO.     */
         LA    APDATA,80(,APSVPTR)                              02S0222
*     GEN(USING @DATD,APDATA)                                   02S0223
*     NOSETS REFS(APDATA);          /* IS AFTER SAVEAREA             */
*                                                               02S0223
         USING @DATD,APDATA
*     /***************************************************************/
*     /*                                                             */
*     /* SAVE REGISTER POINTERS IN WORKAREA. THIS ALLOWS FOR PL/S    */
*     /* REGISTER OPTIMIZATION.                                      */
*     /*                                                             */
*     /***************************************************************/
*                                                               02S0224
*     DAPRLPTR=RAPRLPTR;            /* ADDRESS OF INPUT APRL         */
         ST    RAPRLPTR,DAPRLPTR                                02S0224
*     DDEBPTR=RDEBPTR;              /* ADDRESS OF ASSOCIATED DEB     */
         LR    DDEBPTR,RDEBPTR                                  02S0225
*     DAPDXPTR=RAPDXPTR;            /* ADDRESS OF APDX FOR DEB       */
         ST    RAPDXPTR,DAPDXPTR                                02S0226
*     RFY                                                       02S0227
*       APRL BASED(DAPRLPTR);       /* TELL COMPILER OF              */
*     RFY                                                       02S0228
*       DEBBASIC BASED(DDEBPTR);    /* THESE POINTERS                */
*     RFY                                                       02S0229
*       APDX BASED(DAPDXPTR);       /* FOR LATER USE.                */
*     DUSERKEY=DEBPROTG;            /* SAVE USER PROTECT KEY (THIS IS
*                                      KEY OF OPEN REQUESTOR).       */
         MVC   DUSERKEY(1),DEBPROTG(DDEBPTR)                    02S0230
*     RFY                                                       02S0231
*      (RDEBPTR,                                                02S0231
*       RAPDXPTR,                                               02S0231
*       RAPRLPTR) UNRSTD;           /* VALUES NOW SAVED              */
*   END;                            /* END OF CODE SEGMENT           */
         EJECT
