         L     GPR01F,APCTABND(,APCTPTR)                        02S1559
*           SVC(13);                /* ISSUE ABEND SVC               */
         SVC   13                                               02S1560
*           RESPECIFY                                           02S1561
*            (GPR01F) UNRESTRICTED;                             02S1561
*         END;                      /* ABEND(APCTABND) FORCE ABEND   */
*         GEN NOREFS NOSETS NOSEQFLOW FLOWS(APCVZUKA);          02S1563
*                                                  /* NO CODE
*       END;                        /* ABEND ISSUED                  */
*     END;                          /* ERROR PROCESSED               */
*                                                               02S1565
*   /*****************************************************************/
*   /*                                                               */
*   /* ADD APXL ENTRIES FOR EACH IDAL SLOT.                  @ZA26916*/
*   /* IF NOT FAKE (RE-TRANSLATE) CCW, BUILD THE ACTUAL ENTRIES. IF A*/
*   /* FAKE CCW, JUST INCREASE THE EXTRA APXL SPACE REQUIRED.        */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S1566
*   IF DSW0FCCW=BON THEN            /* IF FAKE CCW, JUST CALCULATE   */
@RF01553 TM    DSW0FCCW,B'10000000'                             02S1566
         BNO   @RF01566                                         02S1566
*     DEXTAPXL=DEXTAPXL+DIDALCT0*APXLLN1;/* REQUIRED SPACE.          */
         LR    @09,DIDALCT0                                     02S1567
         SLA   @09,3                                            02S1567
         ALR   DEXTAPXL,@09                                     02S1567
*   ELSE                                                        02S1568
*     DO;                           /* BUILD ACTUAL ENTRIES. @ZA26916*/
         B     @RC01566                                         02S1568
@RF01566 DS    0H                                               02S1569
*       RFY                                                     02S1569
*        (R1P,                                                  02S1569
*         R0P) RSTD;                /* INTERFACE REGISTERS           */
*       R0P=CCWDATA;                /* CCW BUFFER (DATA) ADDRESS     */
         L     R0P,CCWDATA-1(,DCCWCUR)                          02S1570
         ICM   R0P,8,@CB01727                                   02S1570
*       R1P=DIDALCUR;               /* FIRST IDAL SLOT               */
         L     R1P,DIDALCUR                                     02S1571
*       DO DIDALCT2=DIDALCT0 TO 1 BY-1;/* ENOUGH IDAL ENTRIES FOR    */
         LR    DIDALCT2,DIDALCT0                                02S1572
         B     @DE01572                                         02S1572
@DL01572 DS    0H                                               02S1573
*         R1P->I001C='00'X;         /* ENTIRE I/O BUFFER.            */
         MVI   I001C(R1P),X'00'                                 02S1573
*         CALL ADDAPXL;             /* BUILD APXL ENTRY              */
         BAL   @14,ADDAPXL                                      02S1574
*         R1P=R1P+LENGTH(IDAL);     /* NEXT IDAL ENTRY SLOT          */
         LA    R1P,4(,R1P)                                      02S1575
*         R0P=(R0P+CIOXSZ)&CRNDIXDN;/* IDAL ENTRY POINTS TO NEXT PAGE*/
         LA    @09,2048                                         02S1576
         ALR   @09,R0P                                          02S1576
         N     @09,@CF03927                                     02S1576
         LR    R0P,@09                                          02S1576
*       END;                        /* APXL BUILT FOR IDAL.          */
         BCTR  DIDALCT2,0                                       02S1577
@DE01572 LTR   DIDALCT2,DIDALCT2                                02S1577
         BP    @DL01572                                         02S1577
*     END;                          /* APXL IDAL ENTRIES PROCESSED   */
*                                                               02S1578
*   /*****************************************************************/
*   /*                                                               */
*   /* ADD APXL ENTRY TO POINT CCW AT IDAL                           */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S1579
*   R0P=DIDALCUR;                   /* POINT CCW AT 1ST IDAL SLOT    */
@RC01566 L     R0P,DIDALCUR                                     02S1579
*   R1P=ADDR(CCWDATA);              /* STORE IN CCW DATA FIELD       */
         LA    R1P,CCWDATA(,DCCWCUR)                            02S1580
*   CALL ADDAPXL;                   /* ADD IT                        */
         BAL   @14,ADDAPXL                                      02S1581
*   RFY                                                         02S1582
*    (R1P,                                                      02S1582
*     R0P) UNRSTD;                  /* FINISHED WITH REGISTERS       */
*                                                               02S1582
*   /*****************************************************************/
*   /*                                                               */
*   /* UPDATE DIDALCT AND DIDALCUR TO REFLECT USED SLOTS             */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S1583
*   DIDALCT=DIDALCT+DIDALCT0;       /* NUMBER SLOTS USED THIS PAGE   */
         LR    @09,DIDALCT0                                     02S1583
         AL    @09,DIDALCT                                      02S1583
         ST    @09,DIDALCT                                      02S1583
*   DIDALCUR=DIDALCUR+DIDALCT0*LENGTH(IDAL);/* NEXT FREE SLOT        */
         LR    @09,DIDALCT0                                     02S1584
         SLA   @09,2                                            02S1584
         AL    @09,DIDALCUR                                     02S1584
         ST    @09,DIDALCUR                                     02S1584
*   END ADDIDAL;                    /* IDAL BUILT. RETURN.           */
@EL00005 DS    0H                                               02S1585
@EF00005 DS    0H                                               02S1585
@ER00005 LM    @14,@03,@SA00005                                 02S1585
         LM    @05,@12,@SA00005+24                              02S1585
         BR    @14                                              02S1585
         EJECT
*ADDAPXL:                           /* ADD ENTRY TO APXL TABLE       */
*   PROC                            /* INTERNAL SUBROUTINE           */
*       OPTIONS(PARMREG,            /* REGISTERS 0/1 HAVE INPUT      */
*       NOSAVEAREA);                /* CALL NO ONE                   */
*                                                               02S1586
ADDAPXL  STM   @14,@12,12(@13)                                  02S1586
*   /*****************************************************************/
*   /*                                                               */
*   /* ADD AN ENTRY TO THE APXL TABLE. ON ENTRY, REGISTER 0 CONTAINS */
*   /* THE ADDRESS TO TRANSLATE, AND REGISTER 1 HAS THE ADDRESS WHERE*/
*   /* THE TRANSLATED ADDRESS IS TO BE STORED (FULLWORD). SAVE THE   */
*   /* HI-ORDER BYTE OF THE MODIFIED FIELD IN THE HI-ORDER BYTE OF   */
*   /* REGISTER 1. IF WILL BE USED TO RESET THE HI-BYTE WHEN THE REAL*/
*   /* ADDRESS IS PLACED IN THE REQUESTED FIELD.                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S1587
*   RFY                                                         02S1587
*    (R0P,                                                      02S1587
*     R1P) RSTD;                    /* REQUIRED REGISTERS            */
*   IF DAPXLCUR>=DAPXLEND THEN      /* IF APXL WORKAREA FULL         */
         CLC   DAPXLCUR(4),DAPXLEND                             02S1588
         BL    @RF01588                                         02S1588
*     DO;                           /* (SHOULD NOT HAPPEN),          */
*       APESCODE=CERRN12;           /* THEN                          */
         MVI   APESCODE,X'0C'                                   02S1590
*       DO;                         /* FORCE ABEND TO LOG ERROR &
*                                      PASS CONTROL TO THE (E)STAE OR
*                                      FRR EXIT ROUTINE.             */
*         DO;                       /* ABEND(APCTABND)               */
*           RESPECIFY                                           02S1593
*            (GPR01F) RESTRICTED;                               02S1593
*           GPR01F=APCTABND;        /* COMP CODE IN REG 1            */
         L     GPR01F,APCTABND(,APCTPTR)                        02S1594
*           SVC(13);                /* ISSUE ABEND SVC               */
         SVC   13                                               02S1595
*           RESPECIFY                                           02S1596
*            (GPR01F) UNRESTRICTED;                             02S1596
*         END;                      /* ABEND(APCTABND) FORCE ABEND   */
*         GEN NOREFS NOSETS NOSEQFLOW FLOWS(APCVZUKA);          02S1598
*                                                  /* NO CODE
*       END;                        /* ABEND ISSUED                  */
*     END;                          /* QUIT.                         */
*   RFY                                                         02S1601
*     APXL BASED(DAPXLCUR);         /* APXL POINTER                  */
@RF01588 DS    0H                                               02S1602
*   R1P=R1P&'FFFFFFFC'X;            /* ROUND DOWN TO FULLWORD        */
         N     R1P,@CF04115                                     02S1602
*   GEN(ICM   R1P,B'1000',0(R1P))   /* SAVE HI-BYTE FOR LATER        */
*   REFS(R1P) SETS(R1P);            /* RESTORE AFTER LRA             */
         ICM   R1P,B'1000',0(R1P)
*   APXLADDV=R0P;                   /* ADDR TO ISSUE LRA FOR         */
         L     @09,DAPXLCUR                                     02S1604
         ST    R0P,APXLADDV(,@09)                               02S1604
*   APXLADDL=R1P;                   /* WHERE TO SAVE REAL ADDR       */
         ST    R1P,APXLADDL(,@09)                               02S1605
*   DAPXLCUR=ADDR(APXL)+APXLLN1;    /* NEXT APXL SLOT                */
         AL    @09,@CF00037                                     02S1606
         ST    @09,DAPXLCUR                                     02S1606
*   RFY                                                         02S1607
*     APXL;                         /* FINISHED WITH APXL            */
*   RFY                                                         02S1608
*    (R0P,                                                      02S1608
*     R1P) UNRSTD;                  /* FINISHED WITH REGS            */
*   END ADDAPXL;                    /* RETURN TO CALLER              */
@EL00006 DS    0H                                               02S1609
@EF00006 DS    0H                                               02S1609
@ER00006 LM    @14,@12,12(@13)                                  02S1609
         BR    @14                                              02S1609
         EJECT
*CHKOLAP:                           /* FIND FIRST 'READ' CCW WHICH   */
*   PROC;                           /* OVERLAYS 'WRITE' CCW DATA.    */
*                                                               02S1610
CHKOLAP  STM   @14,@12,12(@13)                                  02S1610
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK FOR CHANNEL PROGRAM I/O OVERLAY. FIND THE FIRST 'READ'  */
*   /* CCW WHOSE I/O AREA OVERLAPS A PREVIOUS 'WRITE' CCW I/O AREA.  */
*   /* IF FOUND, THE CHANNEL PROGRAM MAY NOT BE RETRIED IF IT FAILS  */
*   /* HERE (OR FUTHER ON). THE CHANNEL PROGRAM MAY BE RESTARTED     */
*   /* HOWEVER. SET APIOOLAP TO THE OFFSET INTO THE CHANNEL PROGRAM  */
*   /* OF THE 1ST CCW WHICH PREVENTS THE CHANNEL PROGRAM FROM BEING  */
*   /* RETRIED (SHARED PORT ONLY).                                   */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S1611
*   IF CCWOP=CREADOP&               /* IF 'READ' CCW AND             */
*       APIOOLAP=0 THEN             /* OVERLAP NOT YET FOUND,        */
         CLI   CCWOP(DCCWCUR),2                                 02S1611
         BNE   @RF01611                                         02S1611
         LA    @09,DAPIO                                        02S1611
         LH    @04,APIOOLAP(,@09)                               02S1611
         LTR   @04,@04                                          02S1611
         BNZ   @RF01611                                         02S1611
*     IF DSW0FCCW=BON THEN          /* IF FAKE CCW, CONSIDER IT      */
         TM    DSW0FCCW,B'10000000'                             02S1612
         BNO   @RF01612                                         02S1612
*       APIOOLAP=ADDR(CCW)-DCCWBEG; /* OVERLAPPING SINCE MODIFIABLE  */
         LR    @04,DCCWCUR                                      02S1613
         SL    @04,DCCWBEG                                      02S1613
         STH   @04,APIOOLAP(,@09)                               02S1613
*     ELSE                          /* OTHERWISE --          @ZA26916*/
*       DO;                         /* SCAN FOR OVERLAY NOW.         */
         B     @RC01612                                         02S1614
@RF01612 DS    0H                                               02S1615
*         RFY                                                   02S1615
*          (R14P,                                               02S1615
*           R15P,                                               02S1615
*           R1P) RSTD;              /* SCAN REGISTERS                */
*         R14P=CCWDATA;             /* START OF I/O BUFFER           */
         L     @09,CCWDATA-1(,DCCWCUR)                          02S1616
         LA    @09,0(,@09)                                      02S1616
         LR    R14P,@09                                         02S1616
*         R15P=CCWDATA+CCWBCNT-1;   /* ADDRESS OF LAST BYTE          */
         LH    R15P,CCWBCNT(,DCCWCUR)                           02S1617
         N     R15P,@CF04247                                    02S1617
         ALR   @09,R15P                                         02S1617
         BCTR  @09,0                                            02S1617
         LR    R15P,@09                                         02S1617
*         DO R1P=DCCWBEG BY LENGTH(CCW)/* SCAN PREVIOUS              */
*               WHILE(R1P<DCCWCUR); /* CCWS FOR SAME I/O AREA.       */
         L     R1P,DCCWBEG                                      02S1618
         B     @DE01618                                         02S1618
@DL01618 DS    0H                                               02S1619
*           IF(R1P->CCWOP=CWRITEOP)&/* IF WRITE CCW AND              */
*               ((R1P->CCWDATA>=R14P&/* EITHER START OF BUFFER       */
*               R1P->CCWDATA<=R15P)/* OVERLAPS NEW CCW I/O AREA     */
*               ((R1P->CCWDATA)+(R1P->CCWBCNT)-1>=R14P&/* OR END     */
*               (R1P->CCWDATA)+(R1P->CCWBCNT)-1<=R15P)) THEN/* OF    */
         CLI   CCWOP(R1P),1                                     02S1619
         BNE   @RF01619                                         02S1619
         L     @09,CCWDATA-1(,R1P)                              02S1619
         LA    @09,0(,@09)                                      02S1619
         CR    @09,R14P                                         02S1619
         BL    @GL00021                                         02S1619
         CR    @09,R15P                                         02S1619
         BNH   @RT01619                                         02S1619
@GL00021 L     @09,CCWDATA-1(,R1P)                              02S1619
         LA    @09,0(,@09)                                      02S1619
         LH    @04,CCWBCNT(,R1P)                                02S1619
         N     @04,@CF04247                                     02S1619
         ALR   @09,@04                                          02S1619
         BCTR  @09,0                                            02S1619
         CR    @09,R14P                                         02S1619
         BL    @RF01619                                         02S1619
         CR    @09,R15P                                         02S1619
         BH    @RF01619                                         02S1619
@RT01619 DS    0H                                               02S1620
*             DO;                   /* BUFFER OVERLAPS, IS 1ST CCW   */
*               APIOOLAP=ADDR(CCW)-DCCWBEG;/* CAUSING I/O DATA       */
         LA    @09,DAPIO                                        02S1621
         LR    @04,DCCWCUR                                      02S1621
         SL    @04,DCCWBEG                                      02S1621
         STH   @04,APIOOLAP(,@09)                               02S1621
*               RETURN;             /* OVERLAY. SAVE OFFSET & RETURN.*/
@EL00007 DS    0H                                               02S1622
@EF00007 DS    0H                                               02S1622
@ER00007 LM    @14,@12,12(@13)                                  02S1622
         BR    @14                                              02S1622
*             END;                  /* OVERLAP DETECTED AND NOTED.   */
*         END;                      /* CONTINUE OVERLAY SCAN         */
@RF01619 AL    R1P,@CF00037                                     02S1624
@DE01618 CR    R1P,DCCWCUR                                      02S1624
         BL    @DL01618                                         02S1624
*         RFY                                                   02S1625
*          (R14P,                                               02S1625
*           R15P,                                               02S1625
*           R1P) UNRSTD;            /* SCAN REGISTERS                */
*       END;                        /* NO OVERLAP SO FAR             */
*   END CHKOLAP;                    /* OVERLAP CHECKED               */
         B     @EL00007                                         02S1627
         EJECT
*SETAPRL:                           /* CONVERT APESCOD1 INTO APRL    */
*   PROC;                           /* ERROR INDICATORS              */
*                                                               02S1628
SETAPRL  STM   @14,@12,12(@13)                                  02S1628
*   /*****************************************************************/
*   /*                                                               */
*   /* TAKE THE ERROR CODE (APESCOD1) AND CONVERT IT INTO ERRORS     */
*   /* CODES WHICH ARE MOVED INTO THE APRL (PROTECTED COPY).         */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S1629
*   RFY                                                         02S1629
*     APRL BASED(DAPRLXPT);         /* USE PROTECTED COPY    @ZA26916*/
*   IF APESCOD1^=0 THEN             /* IF NON-ZERO CODE, THEN        */
         CLI   APESCOD1,0                                       02S1630
         BE    @RF01630                                         02S1630
*     DO;                           /* BUILD APRL ERROR VALUES.      */
*       IF APESCOD1>CERRN99 THEN    /* IF INVALID VALUE, USE         */
         CLI   APESCOD1,33                                      02S1632
         BNH   @RF01632                                         02S1632
*         APESCOD1=CERRN99;         /* 'INDETERMINATE' ERROR VALUE.  */
         MVI   APESCOD1,X'21'                                   02S1633
*       APRLRCT=CMBXRCT(APESCOD1);  /* ERROR TYPE                    */
@RF01632 L     @09,DAPRLXPT                                     02S1634
         SLR   @04,@04                                          02S1634
         IC    @04,APESCOD1                                     02S1634
         SLA   @04,3                                            02S1634
         SLR   @15,@15                                          02S1634
         IC    @15,CMBXRCT-8(@04)                               02S1634
         STC   @15,APRLRCT(,@09)                                02S1634
*       APRLRCC=APCCI;              /* ERROR COMPONENT (IO)          */
         MVI   APRLRCC(@09),X'05'                               02S1635
*       APRLRCN=CMBXRCN(APESCOD1);  /* UNIQUE ERROR NUMBER           */
         LH    @15,CMBXRCN-8(@04)                               02S1636
         STC   @15,APRLRCN(,@09)                                02S1636
*       APRLMSGN=CMBRMT(APESCOD1);  /* IGTMBR MESSAGE NUMBER         */
         LH    @15,CMBRMT-8(@04)                                02S1637
         STH   @15,APRLMSGN(,@09)                               02S1637
*       APRLVALN=CMBRKT(APESCOD1);  /* IGTMBR KEYWORD NUMBER         */
         LH    @04,CMBRKT-8(@04)                                02S1638
         STH   @04,APRLVALN(,@09)                               02S1638
*     END;                          /* ERROR CODES SET               */
*   RETURN;                         /* RETURN TO CALLER              */
@EL00008 DS    0H                                               02S1640
@EF00008 DS    0H                                               02S1640
@ER00008 LM    @14,@12,12(@13)                                  02S1640
         BR    @14                                              02S1640
*   RFY                                                         02S1641
*     APRL BASED(DAPRLXPT);         /* USE PROTECTED COPY    @ZA26916*/
*   END SETAPRL;                    /* END OF SUBROUTINE             */
         B     @EL00008                                         02S1642
         EJECT
*GETLLCK:                           /* GET THE LOCAL LOCK AND        */
*   PROC OPTIONS(NOSAVEAREA);       /* ESTABLISH FRR ENVIRONMENT     */
*                                                               02S1643
GETLLCK  STM   @14,@12,12(@13)                                  02S1643
*   /*****************************************************************/
*   /*                                                               */
*   /* GET THE LOCAL LOCK. THEN ISSUE A SETFRR TO PROVIDE RECOVERY   */
*   /* SUPPORT. THE SAME PARAMETERS AS AVAILABLE TO AN ESTAE (IF ANY)*/
*   /* ARE PASSED ('APES' BLOCK)                                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S1644
*                                                               02S1644
*   /*****************************************************************/
*   /*                                                               */
*   /* GET THE LOCAL LOCK                                            */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S1644
*   STM(R11F,R13F,DSUPVSVE);        /* SAVE DESTROYED REGISTERS      */
         STM   R11F,R13F,DSUPVSVE                               02S1644
*   APESCODE=CERRN22;               /* SET ERROR CODE                */
         MVI   APESCODE,X'16'                                   02S1645
*   GEN REFS(PSALITA,FLC) SETS(R11F,R12F,R13F,R14F);            02S1646
*                                   /*
         MODESET EXTKEY=ZERO       SETLOCK REQUIRES KEY-0
         SETLOCK OBTAIN,           ACQUIRE THE                         *
               TYPE=LOCAL,         * LOCAL LOCK                        *
               MODE=UNCOND,        * UNCONDITIONALLY.                  *
               RELATED=(FREELLCK,APESFLLK) * R13 HAS RETURN CODE
         MODESET EXTKEY=DATAMGT    RESUME STANDARD KEY
*   RFY                                                         02S1647
*     R15F RSTD;                    /* HOLDS SETLOCK RETURN CODE     */
*   R15F=R13F;                      /* SAVE SETLOCK RETURN CODE      */
         LR    R15F,R13F                                        02S1648
*   APESCODE=CERRN99;               /* RESET ERROR CODE              */
         MVI   APESCODE,X'21'                                   02S1649
*   LM(R11F,R13F,DSUPVSVE);         /* RELOAD DESTROYED REGISTERS    */
*                                                               02S1650
         LM    R11F,R13F,DSUPVSVE                               02S1650
*   /*****************************************************************/
*   /*                                                               */
*   /* INSURE LOCK ACQUIRED. ABEND IF NOT                            */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S1651
*   IF R15F^=0 THEN                 /* IF SETLOCK FAILED,            */
         LTR   R15F,R15F                                        02S1651
         BZ    @RF01651                                         02S1651
*     DO;                           /* THEN TERMINATE PROCESSING     */
*       APESCODE=CERRN22;           /* SET ERROR CODE                */
         MVI   APESCODE,X'16'                                   02S1653
*       DO;                         /* FORCE ABEND TO LOG ERROR &
*                                      PASS CONTROL TO THE (E)STAE OR
*                                      FRR EXIT ROUTINE.             */
*         DO;                       /* ABEND(APCTABND)               */
*           RESPECIFY                                           02S1656
*            (GPR01F) RESTRICTED;                               02S1656
*           GPR01F=APCTABND;        /* COMP CODE IN REG 1            */
         L     GPR01F,APCTABND(,APCTPTR)                        02S1657
*           SVC(13);                /* ISSUE ABEND SVC               */
         SVC   13                                               02S1658
*           RESPECIFY                                           02S1659
*            (GPR01F) UNRESTRICTED;                             02S1659
*         END;                      /* ABEND(APCTABND) FORCE ABEND   */
*         GEN NOREFS NOSETS NOSEQFLOW FLOWS(APCVZUKA);          02S1661
*                                                  /* NO CODE
*       END;                        /* ABEND ISSUED                  */
*     END;                          /* ERROR PROCESSED               */
*   RFY                                                         02S1664
*     R15F UNRSTD;                  /* FINISHED WITH RETURN CODE     */
@RF01651 DS    0H                                               02S1665
*   APESFLLK=BON;                   /* SHOW LOCAL LOCK OWNED         */
*                                                               02S1665
         OI    APESFLLK,B'00100000'                             02S1665
*   /*****************************************************************/
*   /*                                                               */
*   /* ISSUE SETFRR TO COVER THE PROCESSING. PARAMETER LIST IS 'APES'*/
*   /* BLOCK. THE EXIT ROUTINE CAN PROCESS BOTH (E)STAE AND FRR      */
*   /* ENTRIES.                                                      */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S1666
*   APESCODE=CERRN30;               /* SETFRR ERROR CODE             */
         MVI   APESCODE,X'1E'                                   02S1666
*   RFY                                                         02S1667
*     R15P RSTD;                    /* SETFRR SETS TO PARM LIST      */
*   GEN REFS(PSA,PSACSTK,PSALSFCC,APCTZUKA,APCT,APCTPTR,FRRSFRRA,
*       FRRSENTR,FRRSPARM,FRRSCURR,FRRSLAST,FRRS) SETS(R14F,R15F,R15P);
*                                   /*
         MODESET EXTKEY=ZERO       SETFRR REQUIRES KEY-0
         SETFRR A,FRRAD=APCTZUKA-APCT(,APCTPTR), IGTUKA IS EXIT ROUTINE*
               PARMAD=(R15P),      * SAVE @ 6-WORD PARM FIELD HERE     *
               CLEAR=NO,           * DO NOT CLEAR PARM FIELD           *
               WRKREGS=(R14F,R15F), * WORK REGISTERS                   *
               RELATED=(FREELLCK,APES) * APES IS INPUT
*   R15P->I031P=ADDR(APES);         /* 1ST WORD OF PARM LIST IS 02S1669
*                                      ADDRESS OF 'APES'             */
         LA    @14,APES                                         02S1669
         ST    @14,I031P(,R15P)                                 02S1669
*   RFY                                                         02S1670
*     R15P UNRSTD;                  /* SETFRR PARM FIELD SET         */
*   GEN(MODESET EXTKEY=DATAMGT)                                 02S1671
*   NOREFS NOSETS;                  /* RESUME STANDARD KEY           */
         MODESET EXTKEY=DATAMGT
*   APESCODE=CERRN99;               /* RESET ERROR CODE              */
         MVI   APESCODE,X'21'                                   02S1672
*   APESFFRR=BON;                   /* SHOW SETFRR ISSUED            */
*                                                               02S1673
         OI    APESFFRR,B'10000000'                             02S1673
*   /*****************************************************************/
*   /*                                                               */
*   /* PROCESSING COMPLETE. RETURN TO CALLER                         */
*   /*                                                               */
