*   /*                                                               */
*   /* SET POINTERS FOR CONTROL BLOCKS NEEDED                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0043
*   DO;                                                         01S0043
*     RFY                                                       01S0044
*       APCWPTR RSTD;               /* NEW POINTER FOR APCW          */
*     APCWPTR=ADDR(DAPCW);                                      01S0045
         L     APCWPTR,@PC00001                                 01S0045
*     RFY                                                       01S0046
*       APCW BASED(APCWPTR);                                    01S0046
*     RFY                                                       01S0047
*       APEM BASED(ADDR(APCWEMSG)); /* GET APEM                      */
*     RFY                                                       01S0048
*       IFGACB BASED(APCWACB);      /* GET ACB                       */
*     RFY                                                       01S0049
*       APIN BASED(APCWAPIN);       /* GET APIN                      */
*     RFY                                                       01S0050
*       APCX BASED(ACBAMAX);        /* GET APCX                      */
*     RFY                                                       01S0051
*       APAX BASED(ACBAMWAP);       /* GET APAX                      */
