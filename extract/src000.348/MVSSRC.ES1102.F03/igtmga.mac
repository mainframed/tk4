*                                                               01S0118
*   DO;                             /* IGTYIO APRL(APRL)FUNC(XLTE)   */
*     RFY                                                       01S0119
*      (GPR00F,                                                 01S0119
*       GPR01P) RSTD;               /* RESTRICT REQUIRED REGISTERS   */
*     GPR00F=1;                     /* SET R0 = FUNCTION REQUEST ID  */
         LA    GPR00F,1                                         01S0120
*     GPR01P=ADDR(APRL);            /* POINT TO APRL TO PROCESS      */
         L     GPR01P,APCWAPRL(,APCWPTR)                        01S0121
*     SVC(111);                     /* ISSUE VPSS SVC REQUEST        */
         SVC   111                                              01S0122
*     RFY                                                       01S0123
*      (GPR00F,                                                 01S0123
*       GPR01P) UNRSTD;             /* RELEASE REGISTERS             */
*   END;                            /* IGTYIO APRL(APRL)FUNC(XLTE)   */
*                                                               01S0124
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK RETURN CODE                                             */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0125
*   DO;                                                         01S0125
*     RFY                                                       01S0126
*       APRCODE RSTD;                                           01S0126
*     IF APRCODE^=0 THEN            /* IF R15 NONZERO - ERROR        */
         SLR   @14,@14                                          01S0127
         CR    APRCODE,@14                                      01S0127
         BE    @RF00127                                         01S0127
*       DO;                                                     01S0128
*         APCWZAM=ON;               /* FLAG ERROR TO CALL ROUTER     */
         OI    APCWZAM(APCWPTR),B'00000001'                     01S0129
*         APCWR15C=APRCODE;         /* MOVE R15 RETURN CODE          */
         ST    APRCODE,APCWR15C(,APCWPTR)                       01S0130
*         APCWRCDE=APRLFDBK;        /* MOVE RETURN CODE              */
         L     @09,APCWAPRL(,APCWPTR)                           01S0131
         L     @11,APRLFDBK(,@09)                               01S0131
         ST    @11,APCWRCDE(,APCWPTR)                           01S0131
*         APEMMSGN=APRLMSGN;        /* MOVE MESSAGE NUMBER           */
         LA    @11,112                                          01S0132
         ALR   @11,APCWPTR                                      01S0132
         LH    @10,APRLMSGN(,@09)                               01S0132
         ST    @10,APEMMSGN(,@11)                               01S0132
*         APEMVALN=APRLVALN;        /* MOVE KEYWORD VALUE            */
         LH    @10,APRLVALN(,@09)                               01S0133
         ST    @10,APEMVALN(,@11)                               01S0133
*         APRLAPRL=APAXAPRL;        /* RETURN NEXT APRL TO QUEUE     */
         L     @01,APCWACB(,APCWPTR)                            01S0134
         L     @11,ACBAMWAP(,@01)                               01S0134
         L     @10,APAXAPRL(,@11)                               01S0134
         ST    @10,APRLAPRL(,@09)                               01S0134
*         APAXAPRL=APCWAPRL;        /* RETURN THIS APRL TO QUEUE     */
         ST    @09,APAXAPRL(,@11)                               01S0135
*         APINAPRL=0;               /* ZERO USER APRL POINTER        */
         L     @09,APCWAPIN(,APCWPTR)                           01S0136
         L     @09,APINAAR(,@09)                                01S0136
         ST    @14,APINAPRL(,@09)                               01S0136
*       END;                                                    01S0137
*     ELSE                                                      01S0138
*       ;                                                       01S0138
@RF00127 DS    0H                                               01S0139
*     RFY                                                       01S0139
*       APRCODE UNRSTD;                                         01S0139
*   END;                                                        01S0140
*   RETURN;                                                     01S0141
         B     @EL00001                                         01S0141
