*                                                               01S0070
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK FOR MORE APRLS IN QUEUE. IF NO MORE, SET ERROR          */
*   /* INDICATORS AND RETURN                                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0071
*   DO;                                                         01S0071
*     IF APAXAPRL=0 THEN            /* TEST FOR MORE APRLS           */
         L     @09,APCWACB(,APCWPTR)                            01S0072
         L     @09,ACBAMWAP(,@09)                               01S0072
         L     @09,APAXAPRL(,@09)                               01S0072
         LTR   @09,@09                                          01S0072
         BNZ   @RF00072                                         01S0072
*       DO;                         /* NO - INDICATE ERROR           */
*         APCWZAM=ON;               /* FLAG ERROR TO CALL ROUTER     */
         OI    APCWZAM(APCWPTR),B'00000001'                     01S0074
*         APCWR15C=APCCRERR;        /* R15 RETURN CODE               */
         MVC   APCWR15C(4,APCWPTR),@CF00043                     01S0075
*         APCWRCT=APCCTLOG;         /* ERROR TYPE - LOGICAL          */
         MVI   APCWRCT(APCWPTR),X'08'                           01S0076
*         APCWRCC=APCCB;            /* COMPONENT ID                  */
         MVI   APCWRCC(APCWPTR),X'04'                           01S0077
*         APCWRCN=APCCB005;         /* ERROR NUMBER                  */
         MVI   APCWRCN(APCWPTR),X'05'                           01S0078
*         APEMMSGN=APCMB005;        /* MSG. NO. - NO FREE APRLS      */
         MVC   APEMMSGN+112(4,APCWPTR),@CF00040                 01S0079
*         RETURN;                   /* RETURN TO CALL ROUTER         */
         B     @EL00001                                         01S0080
*       END;                        /* END CODE FOR NO APRLS         */
*     ELSE                                                      01S0082
*       ;                           /* HAVE MORE APRLS - GO ON       */
@RF00072 DS    0H                                               01S0083
*   END;                                                        01S0083
*                                                               01S0083
*   /*****************************************************************/
*   /*                                                               */
*   /* REMOVE AN APRL FROM QUEUE                                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0084
*   DO;                                                         01S0084
*     APINAPRL=APAXAPRL;            /* GIVE APRL ADDRESS TO USER     */
         L     @09,APCWAPIN(,APCWPTR)                           01S0085
         L     @09,APINAAR(,@09)                                01S0085
         L     @15,APCWACB(,APCWPTR)                            01S0085
         L     @14,ACBAMWAP(,@15)                               01S0085
         L     @11,APAXAPRL(,@14)                               01S0085
         ST    @11,APINAPRL(,@09)                               01S0085
*     APCWAPRL=APAXAPRL;            /* TAKE THE APRL                 */
         ST    @11,APCWAPRL(,APCWPTR)                           01S0086
*     APAXARBL=APAXAPRL;                                        01S0087
         ST    @11,APAXARBL(,@14)                               01S0087
*     RFY                                                       01S0088
*       APRL BASED(APCWAPRL);                                   01S0088
*     APAXAPRL=APRLAPRL;            /* SET NEXT APRL IN CHAIN        */
         L     @09,APRLAPRL(,@11)                               01S0089
         ST    @09,APAXAPRL(,@14)                               01S0089
*   END;                                                        01S0090
*                                                               01S0090
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE THE APRL                                           */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0091
*   DO;                                                         01S0091
*     APRLCCW=APAXCCWS;             /* CCW TABLE ADDRESS IN APRL     */
         L     @10,APAXCCWS(,@14)                               01S0092
         ST    @10,APRLCCW(,@11)                                01S0092
*     APRLECB=APRLECB&&APRLECB;     /* ZERO ECB TO POST              */
         L     @05,APRLECB(,@11)                                01S0093
         LR    @04,@05                                          01S0093
         XR    @04,@05                                          01S0093
         ST    @04,APRLECB(,@11)                                01S0093
*     APRLFDBK=APRLFDBK&&APRLFDBK;  /* ZERO FEEDBACK WORD            */
         L     @05,APRLFDBK(,@11)                               01S0094
         LR    @04,@05                                          01S0094
         XR    @04,@05                                          01S0094
         ST    @04,APRLFDBK(,@11)                               01S0094
*     APRLAPIO=APRLAPIO&&APRLAPIO;  /* ZERO APIO POINTER             */
         L     @05,APRLAPIO(,@11)                               01S0095
         LR    @04,@05                                          01S0095
         XR    @04,@05                                          01S0095
         ST    @04,APRLAPIO(,@11)                               01S0095
*     APRLOPTS=APRLOPTS&&APRLOPTS;  /* ZERO OPTION FLAGS             */
         XC    APRLOPTS(4,@11),APRLOPTS(@11)                    01S0096
*     APRLACTV=APRLACTV&&APRLACTV;  /* ZERO ACTIVE APRL SWITCH       */
         XC    APRLACTV(1,@11),APRLACTV(@11)                    01S0097
*     APRLEXTS=APRLEXTS&&APRLEXTS;                              01S0098
         XC    APRLEXTS(1,@11),APRLEXTS(@11)                    01S0098
*     APRLAPRL=APRLAPRL&&APRLAPRL;  /* ZERO NEXT APRL POINTER        */
         LR    @05,@09                                          01S0099
         XR    @05,@09                                          01S0099
         ST    @05,APRLAPRL(,@11)                               01S0099
*     APRLURCP=APCXARCD;            /* INIT RETURN CODE WORD         */
         L     @09,ACBAMAX(,@15)                                01S0100
         L     @09,APCXARCD(,@09)                               01S0100
         ST    @09,APRLURCP(,@11)                               01S0100
*     APRLEMSP=ACBMSGAR;            /* INIT MSG AREA POINTER         */
         L     @09,ACBMSGAR(,@15)                               01S0101
         ST    @09,APRLEMSP(,@11)                               01S0101
*     APRLEMSL=ACBMSGLN;            /* INIT MSG LENGTH WORD          */
         LH    @09,ACBMSGLN(,@15)                               01S0102
         STH   @09,APRLEMSL(,@11)                               01S0102
*     APRLOPTZ=APAXSTAT;            /* MOVE IN ISTATE FROM BLD       */
         MVC   APRLOPTZ(4,@11),APAXSTAT(@14)                    01S0103
*   END;                                                        01S0104
