*   /*****************************************************************/
*   /*                                                               */
*   /* SET CIT LENGTH IN WRITE CIT CCW                               */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0105
*   DO;                                                         01S0105
*     CITCCWPT=APAXCCWS+8;          /* POINT TO WRITE CIT CCW        */
         AL    @10,@CF00043                                     01S0106
         LR    CITCCWPT,@10                                     01S0106
*     RFY                                                       01S0107
*       CCWCOUNT BASED(CITCCWPT);                               01S0107
*     BYTCOUNT=APAXCIN-APAXCIS;     /* PUT CIT LENGTH IN CCW         */
         L     @09,APAXCIN(,@14)                                01S0108
         SL    @09,APAXCIS(,@14)                                01S0108
         STH   @09,BYTCOUNT(,CITCCWPT)                          01S0108
