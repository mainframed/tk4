*   DO;                                                         01S0110
*     CCWENTRY=CCWENTRY&&CCWENTRY;  /* ZERO WORK AREA                */
         XC    CCWENTRY(8),CCWENTRY                             01S0111
*     CMDCODE=NOPCCW;               /* INSERT NOP COMMAND CODE       */
         MVI   CMDCODE,X'03'                                    01S0112
*     CALL APCVZMIB(CCWENTRY,ERRORBIT);/* MOVE CCW TO CCW TABLE      */
         LA    @09,CCWENTRY                                     01S0113
         ST    @09,@AL00001                                     01S0113
         LA    @09,ERRORBIT                                     01S0113
         ST    @09,@AL00001+4                                   01S0113
         L     @15,APCTZMIB(,APCTPTR)                           01S0113
         LA    @01,@AL00001                                     01S0113
         BALR  @14,@15                                          01S0113
