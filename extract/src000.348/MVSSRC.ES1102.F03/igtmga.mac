*   /*****************************************************************/
*   /*                                                               */
*   /* SCAN THE LABEL TABLE FOR UNRESOLVED BRANCH LABELS             */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0054
*   DO;                                                         01S0054
*     DO I=1 TO APLBNUMB;           /* SCAN UNTIL ERROR FOUND        */
         LA    I,1                                              01S0055
         B     @DE00055                                         01S0055
@DL00055 DS    0H                                               01S0056
*       IF(APLBGOOD(I)=ON)&(APLBDEF(I)=OFF) THEN/* LOOK FOR A   01S0056
*                                      REFERENCE ENTRY WHICH IS STILL
*                                      VALID                         */
         LR    @09,I                                            01S0056
         SLA   @09,3                                            01S0056
         L     @03,APCWACB(,APCWPTR)                            01S0056
         L     @03,ACBAMWAP(,@03)                               01S0056
         L     @03,APAXAPLB(,@03)                               01S0056
         ST    @09,@TF00001                                     01S0056
         ALR   @09,@03                                          01S0056
         TM    APLBGOOD-8(@09),B'10000000'                      01S0056
         BNO   @RF00056                                         01S0056
         TM    APLBDEF-8(@09),B'01000000'                       01S0056
         BNZ   @RF00056                                         01S0056
*         DO;                       /* SET ERROR INDICATORS          */
*           APCWZAM=ON;             /* SET ERROR FLAG                */
         OI    APCWZAM(APCWPTR),B'00000001'                     01S0058
*           APCWR15C=APCCRERR;      /* SET RETURN CODE               */
         MVC   APCWR15C(4,APCWPTR),@CF00043                     01S0059
*           APCWRCT=APCCTLOG;       /* INDICATE ERROR TYPE AS LOGICAL*/
         MVI   APCWRCT(APCWPTR),X'08'                           01S0060
*           APCWRCC=APCCB;          /* SET COMPONENT ID              */
         MVI   APCWRCC(APCWPTR),X'04'                           01S0061
*           APCWRCN=APCCB004;       /* SET ERROR NUMBER              */
         MVI   APCWRCN(APCWPTR),X'04'                           01S0062
*           APEMMSGN=APCMB004;      /* SET MESSAGE NUMBER FOR   01S0063
*                                      UNRESOLVED LABEL              */
         LA    @09,112                                          01S0063
         ALR   @09,APCWPTR                                      01S0063
         MVC   APEMMSGN(4,@09),@CF00641                         01S0063
*           APEMVALN=-1;            /* INDICATE USE OF VALP          */
         MVC   APEMVALN(4,@09),@CF01602                         01S0064
*           APEMVALP=APLBLBL(I);    /* PASS BACK THE LABEL           */
         AL    @03,@TF00001                                     01S0065
         MVC   APEMVALP(4,@09),APLBLBL-8(@03)                   01S0065
*           RETURN;                 /* RETURN TO CALLER              */
@EL00001 L     @13,4(,@13)                                      01S0066
@EF00001 L     @00,@SIZDATD                                     01S0066
         LR    @01,@06                                          01S0066
         IGTYFSV  R,LV=(0),A=(1)
@ER00001 LM    @14,@12,12(@13)                                  01S0066
         BR    @14                                              01S0066
*         END;                                                  01S0067
*       ELSE                                                    01S0068
*         ;                                                     01S0068
@RF00056 DS    0H                                               01S0069
*     END;                                                      01S0069
         AL    I,@CF00055                                       01S0069
@DE00055 L     @09,APCWACB(,APCWPTR)                            01S0069
         L     @09,ACBAMWAP(,@09)                               01S0069
         L     @09,APAXAPLB(,@09)                               01S0069
         C     I,APLBNUMB(,@09)                                 01S0069
         BNH   @DL00055                                         01S0069
