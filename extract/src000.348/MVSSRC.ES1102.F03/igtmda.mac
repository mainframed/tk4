@RC00255 DS    0H                                               02S0258
@RC00244 DS    0H                                               02S0258
@RC00235 L     @04,APCWAPAL(,APCWPTR)                           02S0258
         TM    APALYDRQ(@04),B'10000000'                        02S0258
