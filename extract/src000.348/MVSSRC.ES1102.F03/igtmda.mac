         L     @04,APCWAPAL(,APCWPTR)                           02S0608
         TM    APALYDOP(@04),B'11000000'                        02S0608
         BZ    @RF00608                                         02S0608
*       IF(IDLY<-32768)×            /* IDLY MUST BE IN RANGE @ZA25370*/
*           (IDLY>65536) THEN       /* -32768 <-> +65536.    @ZA25370*/
         L     @04,IDLY(,DESCRPTR)                              02S0609
         C     @04,@CF01788                                     02S0609
         BL    @RT00609                                         02S0609
         C     @04,@CF01789                                     02S0609
         BNH   @RF00609                                         02S0609
@RT00609 DS    0H                                               02S0610
