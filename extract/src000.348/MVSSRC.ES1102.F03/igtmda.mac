         TM    ERRORBIT,B'10000000'                             01S0083
         BNO   @RF00083                                         01S0083
*     DO;                           /* YES - SET ERROR INDICATORS    */
*       APCWZAM=ON;                 /* INDICATE ERROR OCCURRED       */
         OI    APCWZAM(APCWPTR),B'00000001'                     01S0085
*       APCWR15C=APCCRERR;          /* SET R15 RET CODE              */
         MVC   APCWR15C(4,APCWPTR),@CF00145                     01S0086
