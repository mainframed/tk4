*                   ELSE                                        01S0121
*                     ;             /* NULL - NO MIF PROBLEMS        */
@RF00114 DS    0H                                               01S0122
*             END;                                              01S0122
@RF00106 DS    0H                                               01S0123
*           RFY                                                 01S0123
*             PARMSIGN BASED(ADDR(APINAYST));                   01S0123
