         B     OUTERR                                           01S0112
*                   END;            /* END OF SEGMENT                */
*                 ELSE              /* TEST FOR RANGE ERROR          */
*                   IF RSOLVERR=RNGERROR THEN/* RANGE ERROR?         */
@RF00107 CLC   RSOLVERR(4),@CF00130                             01S0114
         BNE   @RF00114                                         01S0114
