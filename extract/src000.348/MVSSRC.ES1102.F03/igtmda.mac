         L     @04,RSOLVERR                                     02S0213
         LTR   @04,@04                                          02S0213
         BZ    @RF00213                                         02S0213
*             IF RSOLVERR=BDYERROR THEN/* IF ERROR WAS WORD BDY 02S0214
*                                      PROBLEM                       */
         C     @04,@CF00045                                     02S0214
         BNE   @RF00214                                         02S0214
*               DO;                 /* SET ERROR INDICATORS FOR YARRV
*                                      NOT A WORD BOUNDARY           */
*                 APCWRCN=APCCA015; /* ERROR NUMBER FOR YARRV NOT A
