         B     @RC00230                                         02S0232
@RF00230 AL    INPUTPTR,@CF00145                                02S0232
*         END;                                                  02S0233
*       ELSE                        /* ERROR - IFMY,YARRV REQD ALWAYS*/
*         GOTO OUTERR1;             /* GO SET ERR INDICATORS         */
*       IF APALYCRQ=ON THEN         /* TEST IF ICTY REQD             */
@RC00208 L     @04,APCWAPAL(,APCWPTR)                           02S0235
         TM    APALYCRQ(@04),B'10000000'                        02S0235
         BNO   @RF00235                                         02S0235
*         IF LASTPARM=OFF THEN      /* IF MORE PARAMETERS EXIST      */
         TM    LASTPARM,B'10000000'                             02S0236
         BNZ   @RF00236                                         02S0236
