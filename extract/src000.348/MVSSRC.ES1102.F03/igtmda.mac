         SLR   @15,@15                                          02S0256
         IC    @15,APALYCT(,@04)                                02S0256
         ST    @15,ICTY(,DESCRPTR)                              02S0256
*           ELSE                                                02S0257
*             ICTY=1;               /* OTHERWISE SUPPLY VALUE OF 1   */
         B     @RC00255                                         02S0257
@RF00255 MVC   ICTY(4,DESCRPTR),@CF00045                        02S0257
