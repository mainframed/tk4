         B     OUTERR                                           02S0219
*               END;                /* END OF SEGMENT                */
*             ELSE                  /* CHECK FOR RANGE ERROR         */
*               IF RSOLVERR=RNGERROR THEN/* IF RANGE ERROR           */
@RF00214 CLC   RSOLVERR(4),@CF00130                             02S0221
         BNE   @RF00221                                         02S0221
*                 DO;               /* SET ERROR INDICATORS FOR YARRV
*                                      OUT OF RANGE                  */
*                   APCWRCN=APCCA020;/* ERROR NUMBER FOR YARRV OUT OF
*                                      RANGE                         */
         MVI   APCWRCN(APCWPTR),X'14'                           02S0223
*                   APEMMSGN=APCMA020;/* MSG NUMBER FOR OUT OF RANGE */
         LA    @09,112                                          02S0224
         ALR   @09,APCWPTR                                      02S0224
         MVC   APEMMSGN(4,@09),@CF00156                         02S0224
*                   APEMVALN=APCMK017;/* KEYWORD NUMBER FOR YARRV    */
         MVC   APEMVALN(4,@09),@CF00180                         02S0225
*                   GOTO OUTERR;    /* GO SET COMMON FLAGS & RETURN  */
         B     OUTERR                                           02S0226
*                 END;              /* END OF SEGMENT                */
*               ELSE                                            02S0228
*                 ;                 /* NULL - NO MIF PROBLEMS        */
@RF00221 DS    0H                                               02S0229
*           ELSE                                                02S0229
*             ;                     /* YARRV OK - NO ACTION          */
@RF00213 DS    0H                                               02S0230
*           IF APINAP2E=ON THEN     /* IF YARRV IS LAST PARM -       */
         TM    APINAP2E(INPUTPTR),B'10000000'                   02S0230
         BNO   @RF00230                                         02S0230
*             LASTPARM=ON;          /* SET LAST PARM FLAG ON         */
         OI    LASTPARM,B'10000000'                             02S0231
*           ELSE                    /* YARRV NOT LAST                */
