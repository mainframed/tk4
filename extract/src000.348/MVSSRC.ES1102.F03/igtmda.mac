         LA    @04,CITENTRY                                     01S0077
         OI    APCIASSK(@04),B'10000000'                        01S0077
*       ELSE                        /* NO - ERROR                    */
*         ERRORBIT=ON;              /* FLAG IT                       */
         B     @RC00076                                         01S0078
@RF00076 OI    ERRORBIT,B'10000000'                             01S0078
*     ELSE                                                      01S0079
*       IF APALSTKR=ON THEN         /* IS STACKING REQUIRED?         */
         B     @RC00075                                         01S0079
@RF00075 L     @04,APCWAPAL(,APCWPTR)                           01S0079
         TM    APALSTKR(@04),B'10000000'                        01S0079
         BNO   @RF00079                                         01S0079
