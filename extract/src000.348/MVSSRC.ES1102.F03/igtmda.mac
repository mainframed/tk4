*       ELSE                        /* IDLY IS OK                    */
*         APCIADL1=IDLY;            /* MOVE IDLY TO WORKING CIT ENTRY*/
@RF00609 LA    @04,CITENTRY                                     02S0616
         L     @03,IDLY(,DESCRPTR)                              02S0616
         STH   @03,APCIADL1(,@04)                               02S0616
*     ELSE                                                      02S0617
