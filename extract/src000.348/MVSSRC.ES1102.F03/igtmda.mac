         L     @04,RSOLVERR                                     01S0106
         LTR   @04,@04                                          01S0106
         BZ    @RF00106                                         01S0106
*                 IF RSOLVERR=BDYERROR THEN/* BOUNDARY ERROR?        */
         C     @04,@CF00045                                     01S0107
         BNE   @RF00107                                         01S0107
*                   DO;             /* YES - SET ERROR INDICATORS FOR
