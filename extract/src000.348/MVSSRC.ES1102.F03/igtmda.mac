         ALR   @09,APCWPTR                                      01S0117
         MVC   APEMMSGN(4,@09),@CF00156                         01S0117
*                       APEMVALN=APCMK017;/* YARRV KWD NUMBER        */
         MVC   APEMVALN(4,@09),@CF00180                         01S0118
*                       GOTO OUTERR;/* GO SET COMMON ERROR INDICATORS
