TMJI     TITLE 'IGTMJI --- PROVIDE OPEN CLOSE && DYNALLOC RETURN CODES *
                         '
IGTMJI   CSECT ,                                                01S0001
@MAINENT DS    0H                                               01S0001
         USING *,@15                                            01S0001
         B     @PROLOG                                          01S0001
         DC    AL1(16)                                          01S0001
         DC    C'IGTMJI    77.093'                              01S0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                  01S0001
         BALR  @07,0                                            01S0001
@PSTART  DS    0H                                               01S0001
         USING @PSTART,@07                                      01S0001
         L     @00,@SIZDATD                                     01S0001
         IGTYGSV  R,LV=(0)
         LR    @06,@01                                          01S0001
         USING @DATD,@06                                        01S0001
         ST    @13,@SA00001+4                                   01S0001
         LM    @00,@01,20(@13)                                  01S0001
         ST    @06,8(,@13)                                      01S0001
         LR    @13,@06                                          01S0001
         MVC   @PC00001(8),0(@01)                               01S0001
*/*                                                                  */
*/* END OF APSV                                                      */
*/*                                                                  */
*                                                               01S0021
*                                                               01S0021
*   /*****************************************************************/
*   /*                                                               */
*   /* PROVIDE ERROR RETURN CODES FOR ERRORS DETECTED BY OPEN, CLOSE,*/
*   /* OR DYNALLOC MACROS.                                           */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0021
*   DO;                                                         01S0021
*     RFY                                                       01S0022
*       APCWPTR RSTD;                                           01S0022
*     APCWPTR=ADDR(DAPCW);          /* NEW POINTER FOR APCW          */
         L     APCWPTR,@PC00001                                 01S0023
*     RFY                                                       01S0024
*       APCW BASED(APCWPTR);                                    01S0024
*     RFY                                                       01S0025
*       APEM BASED(ADDR(APCWEMSG)); /* NEW POINTER FOR THE APEM      */
*     APCWSVAL=BITZERO;             /* INIT BIT TO IDENTIFY THE MACRO*/
*                                                               01S0026
         NI    APCWSVAL(APCWPTR),B'10111111'                    01S0026
*     /***************************************************************/
*     /*                                                             */
*     /* CONDUCT A SEARCH OF THE APPROPRIATE TABLE, THEN SET ALL THE */
*     /* RETURN CODES AND VALUES. IF NO MATCH IS FOUND BY THE SEARCH,*/
*     /* THEN THE RETURN VALUES ARE TAKEN FROM THE LAST ENTRY IN THE */
*     /* RESPECTIVE TABLE.                                           */
*     /*                                                             */
*     /***************************************************************/
*                                                               01S0027
*     IF MACRONAM='OPEN' THEN                                   01S0027
         L     @09,@PC00001+4                                   01S0027
         CLC   MACRONAM(4,@09),@CC01249                         01S0027
         BNE   @RF00027                                         01S0027
*       CASEVRBL=1;                 /* PREPARE TO BRANCH FOR OPEN    */
         LA    CASEVRBL,1                                       01S0028
*     ELSE                                                      01S0029
*       IF MACRONAM='CLSE' THEN                                 01S0029
         B     @RC00027                                         01S0029
@RF00027 L     @09,@PC00001+4                                   01S0029
         CLC   MACRONAM(4,@09),@CC01250                         01S0029
         BNE   @RF00029                                         01S0029
*         CASEVRBL=2;               /* PREPARE TO BRANCH FOR CLOSE   */
         LA    CASEVRBL,2                                       01S0030
*       ELSE                                                    01S0031
*         CASEVRBL=3;               /* PREPARE TO BRANCH FOR DYNA.   */
         B     @RC00029                                         01S0031
@RF00029 LA    CASEVRBL,3                                       01S0031
*     ANSRINDX=ZERO;                /* INITIALIZE MATCH ENTRY INDEX  */
@RC00029 DS    0H                                               01S0032
@RC00027 SLR   ANSRINDX,ANSRINDX                                01S0032
*TABLES:                                                        01S0033
*     DO;                           /* TABLES:CASE(CASEVRBL)NAMES(OPE
*                                      N,CLSE,DYNA)                  */
TABLES   DS    0H                                               01S0034
*       DCL                                                     01S0034
*         CL0001@ LABEL BASED(CP0001@(CASEVRBL)) VALUERANGE(OPEN,CLSE,
*             DYNA);                /* BRANCH TARGET                 */
*       DCL                                                     01S0035
*         CP0001@(3) PTR(31) INIT(ADDR(OPEN),ADDR(CLSE),ADDR(DYNA));
*       GOTO CL0001@;               /* BRANCH TO SELECTED CASE BRANCH
*                                      TO APPROPRIATE TABLE          */
         LR    @09,CASEVRBL                                     01S0036
         SLA   @09,2                                            01S0036
         L     @09,CP0001@-4(@09)                               01S0036
         BR    @09                                              01S0036
*OPEN:                                                          01S0037
*       DO;                         /* SUBCASE FOLLOWING CASE IS FOR
*                                      OPEN                          */
*                                                               01S0037
OPEN     DS    0H                                               01S0038
*         /***********************************************************/
*         /*                                                         */
*         /* SEARCH OPEN TABLE AND SET VALUES.                       */
*         /*                                                         */
*         /***********************************************************/
*                                                               01S0038
*         DO;                                                   01S0038
*           DO INDEX=1 TO OPENTBSZ BY 1 WHILE(ANSRINDX=ZERO);   01S0039
*                                                               01S0039
         LA    INDEX,1                                          01S0039
@DL00039 LTR   ANSRINDX,ANSRINDX                                01S0039
         BNZ   @DC00039                                         01S0039
*             /*******************************************************/
*             /*                                                     */
*             /* COMMENCE SEQUENTIAL SEARCH                          */
*             /*                                                     */
*             /*******************************************************/
*                                                               01S0040
*             IF FBYTE1=OPENCODE(INDEX) THEN/* IF MATCH FOUND        */
         L     @09,@PC00001+4                                   01S0040
         LR    @05,INDEX                                        01S0040
         MH    @05,@CH00140                                     01S0040
         SLR   @03,@03                                          01S0040
         IC    @03,FBYTE1(,@09)                                 01S0040
         SLR   @09,@09                                          01S0040
         IC    @09,OPENCODE-6(@05)                              01S0040
         CR    @03,@09                                          01S0040
         BNE   @RF00040                                         01S0040
*               ANSRINDX=INDEX;     /* DENOTE END OF SEARCH          */
         LR    ANSRINDX,INDEX                                   01S0041
*             ELSE                                              01S0042
*               ;                                               01S0042
@RF00040 DS    0H                                               01S0043
*           END;                                                01S0043
         AL    INDEX,@CF00058                                   01S0043
         C     INDEX,@CF00063                                   01S0043
         BNH   @DL00039                                         01S0043
@DC00039 DS    0H                                               01S0044
*           IF ANSRINDX=ZERO THEN   /* IF NO MATCH FOUND             */
         LTR   ANSRINDX,ANSRINDX                                01S0044
         BNZ   @RF00044                                         01S0044
*             ANSRINDX=OPENTBSZ;    /* THEN SELECT LAST TABLE ENTRY  */
         LA    ANSRINDX,14                                      01S0045
*           ELSE                                                01S0046
*             ;                                                 01S0046
*                                                               01S0046
@RF00044 DS    0H                                               01S0047
*           /*********************************************************/
*           /*                                                       */
*           /* NOW ASSIGN RETURN CODES & VALUES                      */
*           /*                                                       */
*           /*********************************************************/
*                                                               01S0047
*           APCWRCT=OPENRCT(ANSRINDX);/* INDICATE TYPE OF ERROR      */
         LR    @09,ANSRINDX                                     01S0047
         MH    @09,@CH00140                                     01S0047
         SLR   @03,@03                                          01S0047
         IC    @03,OPENRCT-6(@09)                               01S0047
         STC   @03,APCWRCT(,APCWPTR)                            01S0047
*           APCWRCN=OPENRCN(ANSRINDX);/* ASSIGN AN ERROR CODE        */
         SLR   @03,@03                                          01S0048
         IC    @03,OPENRCN-6(@09)                               01S0048
         STC   @03,APCWRCN(,APCWPTR)                            01S0048
*           APEMMSGN=OPENMSGN(ANSRINDX);/* ASSIGN A MESSAGE INDEX    */
         LA    @03,112                                          01S0049
         ALR   @03,APCWPTR                                      01S0049
         LH    @09,OPENMSGN-6(@09)                              01S0049
         ST    @09,APEMMSGN(,@03)                               01S0049
*           IF ANSRINDX=ZERO THEN   /* IF NO TABLE MATCH WAS FOUND   */
         LTR   ANSRINDX,ANSRINDX                                01S0050
         BNZ   @RF00050                                         01S0050
*             DO;                   /* RETURN ERRONEOUS ERROR CODE   */
*               CVD(FBYTE1,FULLWORD);/* CONVERT BINARY CODE TO  01S0052
*                                      DECIMAL                       */
         L     @09,@PC00001+4                                   01S0052
         SLR   @02,@02                                          01S0052
         IC    @02,FBYTE1(,@09)                                 01S0052
         CVD   @02,@TS00001                                     01S0052
         MVC   FULLWORD(4),@TS00001+4                           01S0052
*               FULLWORD=FULLWORD×'0000000F'X;/* TURN SIGN TO EBCD   */
         OC    FULLWORD(4),@CB01260                             01S0053
*               UNPK(APEMVALP,FULLWORD);/* UNPACK DEC CODE TO EBCDIC */
         UNPK  APEMVALP(4,@03),FULLWORD(4)                      01S0054
*               APEMVALN=-1;        /* SIGNAL AN ERRONEOUS ERROR CODE*/
         MVC   APEMVALN(4,@03),@CF01262                         01S0055
*             END;                                              01S0056
*           ELSE                    /* OTHERWISE, TABLE MATCH FOUND  */
*             APEMVALN=OPENVALN(ANSRINDX);/* RETURN KEYWORD INDEX    */
         B     @RC00050                                         01S0057
@RF00050 LR    @09,ANSRINDX                                     01S0057
         MH    @09,@CH00140                                     01S0057
         SLR   @03,@03                                          01S0057
         IC    @03,OPENVALN-6(@09)                              01S0057
         ST    @03,APEMVALN+112(,APCWPTR)                       01S0057
*         END;                                                  01S0058
*         GOTO CE0001@;             /* BRANCH TO END OF CASE         */
         B     CE0001@                                          01S0059
*       END OPEN;                   /* END OF SUBCASE                */
*CLSE:                                                          01S0061
*       DO;                         /* SUBCASE FOLLOWING CASE IS FOR
*                                      CLSE                          */
*                                                               01S0061
CLSE     DS    0H                                               01S0062
*         /***********************************************************/
*         /*                                                         */
*         /* SEARCH CLSE TABLE AND SET VALUES.                       */
*         /*                                                         */
*         /***********************************************************/
*                                                               01S0062
*         DO;                                                   01S0062
*           DO INDEX=1 TO CLSETBSZ BY 1 WHILE(ANSRINDX=ZERO);   01S0063
*                                                               01S0063
         LA    INDEX,1                                          01S0063
@DL00063 LTR   ANSRINDX,ANSRINDX                                01S0063
         BNZ   @DC00063                                         01S0063
*             /*******************************************************/
*             /*                                                     */
*             /* COMMENCE SEQUENTIAL SEARCH                          */
*             /*                                                     */
*             /*******************************************************/
*                                                               01S0064
*             IF FBYTE1=CLSECODE(INDEX) THEN/* IF MATCH FOUND        */
         L     @09,@PC00001+4                                   01S0064
         LR    @05,INDEX                                        01S0064
         MH    @05,@CH00140                                     01S0064
         SLR   @03,@03                                          01S0064
         IC    @03,FBYTE1(,@09)                                 01S0064
         SLR   @09,@09                                          01S0064
         IC    @09,CLSECODE-6(@05)                              01S0064
         CR    @03,@09                                          01S0064
         BNE   @RF00064                                         01S0064
*               ANSRINDX=INDEX;     /* DENOTE END OF SEARCH          */
         LR    ANSRINDX,INDEX                                   01S0065
*             ELSE                                              01S0066
*               ;                                               01S0066
@RF00064 DS    0H                                               01S0067
*           END;                                                01S0067
         AL    INDEX,@CF00058                                   01S0067
         C     INDEX,@CF00050                                   01S0067
         BNH   @DL00063                                         01S0067
@DC00063 DS    0H                                               01S0068
*           IF ANSRINDX=ZERO THEN   /* IF NO MATCH FOUND             */
         LTR   ANSRINDX,ANSRINDX                                01S0068
         BNZ   @RF00068                                         01S0068
*             ANSRINDX=CLSETBSZ;    /* THEN SELECT LAST TABLE ENTRY  */
         LA    ANSRINDX,2                                       01S0069
*           ELSE                                                01S0070
*             ;                                                 01S0070
*                                                               01S0070
@RF00068 DS    0H                                               01S0071
*           /*********************************************************/
*           /*                                                       */
*           /* NOW ASSIGN RETURN CODES & VALUES                      */
*           /*                                                       */
*           /*********************************************************/
*                                                               01S0071
*           APCWRCT=CLSERCT(ANSRINDX);/* INDICATE TYPE OF ERROR      */
         LR    @09,ANSRINDX                                     01S0071
         MH    @09,@CH00140                                     01S0071
         SLR   @03,@03                                          01S0071
         IC    @03,CLSERCT-6(@09)                               01S0071
         STC   @03,APCWRCT(,APCWPTR)                            01S0071
*           APCWRCN=CLSERCN(ANSRINDX);/* ASSIGN AN ERROR CODE        */
         SLR   @03,@03                                          01S0072
         IC    @03,CLSERCN-6(@09)                               01S0072
         STC   @03,APCWRCN(,APCWPTR)                            01S0072
*           APEMMSGN=CLSEMSGN(ANSRINDX);/* ASSIGN A MESSAGE INDEX    */
         LA    @03,112                                          01S0073
         ALR   @03,APCWPTR                                      01S0073
         LH    @09,CLSEMSGN-6(@09)                              01S0073
         ST    @09,APEMMSGN(,@03)                               01S0073
*           IF ANSRINDX=ZERO THEN   /* IF NO TABLE MATCH WAS FOUND   */
         LTR   ANSRINDX,ANSRINDX                                01S0074
         BNZ   @RF00074                                         01S0074
*             DO;                   /* RETURN ERRONEOUS ERROR CODE   */
*               CVD(FBYTE1,FULLWORD);/* CONVERT BINARY CODE TO  01S0076
*                                      DECIMAL                       */
         L     @09,@PC00001+4                                   01S0076
         SLR   @02,@02                                          01S0076
         IC    @02,FBYTE1(,@09)                                 01S0076
         CVD   @02,@TS00001                                     01S0076
         MVC   FULLWORD(4),@TS00001+4                           01S0076
*               FULLWORD=FULLWORD×'0000000F'X;/* TURN SIGN TO EBCD   */
         OC    FULLWORD(4),@CB01260                             01S0077
*               UNPK(APEMVALP,FULLWORD);/* UNPACK DEC CODE TO EBCDIC */
         UNPK  APEMVALP(4,@03),FULLWORD(4)                      01S0078
*               APEMVALN=-1;        /* SIGNAL AN ERRONEOUS ERROR CODE*/
         MVC   APEMVALN(4,@03),@CF01262                         01S0079
*             END;                                              01S0080
*           ELSE                    /* OTHERWISE, TABLE MATCH FOUND  */
*             APEMVALN=CLSEVALN(ANSRINDX);/* RETURN KEYWORD INDEX    */
         B     @RC00074                                         01S0081
@RF00074 LR    @09,ANSRINDX                                     01S0081
         MH    @09,@CH00140                                     01S0081
         SLR   @03,@03                                          01S0081
         IC    @03,CLSEVALN-6(@09)                              01S0081
         ST    @03,APEMVALN+112(,APCWPTR)                       01S0081
*         END;                                                  01S0082
*         GOTO CE0001@;             /* BRANCH TO END OF CASE         */
         B     CE0001@                                          01S0083
*       END CLSE;                   /* END OF SUBCASE                */
*DYNA:                                                          01S0085
*       DO;                         /* SUBCASE FOLLOWING CASE IS FOR
*                                      DYNA                          */
*                                                               01S0085
DYNA     DS    0H                                               01S0086
*         /***********************************************************/
*         /*                                                         */
*         /* NO TABLE SEARCH FOR DYNALLOC. MERELY RETURN THE ERROR & */
*         /* INFO. REASON CODES                                      */
*         /*                                                         */
*         /***********************************************************/
*                                                               01S0086
*         DO;                                                   01S0086
*           IF DYNAR15C=4 THEN      /* IF 4 WAS RETURNED TO IGTMJC   */
         L     @09,@PC00001+4                                   01S0087
         CLC   DYNAR15C(4,@09),@CF00045                         01S0087
         BNE   @RF00087                                         01S0087
*             DO;                                               01S0088
*               APCWRCN=APCCS100;   /* SELECT RETURN CODE FOR USER   */
         MVI   APCWRCN(APCWPTR),X'64'                           01S0089
*               APCWRCT=APCCTLOG;   /* DESIGNATE AS LOGIC ERROR      */
         MVI   APCWRCT(APCWPTR),X'08'                           01S0090
*               APEMMSGN=APCMS100;  /* SELECT MESSAGE INDEX          */
         MVC   APEMMSGN+112(4,APCWPTR),@CF00168                 01S0091
*             END;                                              01S0092
*           ELSE                                                01S0093
*             IF DYNAR15C=8 THEN    /* IF 8 WAS RETURNED TO IGTMJC   */
         B     @RC00087                                         01S0093
@RF00087 L     @09,@PC00001+4                                   01S0093
         CLC   DYNAR15C(4,@09),@CF00052                         01S0093
         BNE   @RF00093                                         01S0093
*               DO;                                             01S0094
*                 APCWRCN=APCCS101; /* SELECT RETURN CODE FOR USER   */
         MVI   APCWRCN(APCWPTR),X'65'                           01S0095
*                 APCWRCT=APCCTLOG; /* DESIGNATE AS LOGIC ERROR      */
         MVI   APCWRCT(APCWPTR),X'08'                           01S0096
*                 APEMMSGN=APCMS101;/* SELECT MESSAGE INDEX          */
         MVC   APEMMSGN+112(4,APCWPTR),@CF00601                 01S0097
*               END;                                            01S0098
*             ELSE                  /* RTRN CODE TO IGTMJC MUST BE 12*/
*               DO;                                             01S0099
         B     @RC00093                                         01S0099
@RF00093 DS    0H                                               01S0100
*                 APCWRCN=APCCS102; /* SELECT RETURN CODE FOR USER   */
         MVI   APCWRCN(APCWPTR),X'66'                           01S0100
*                 APCWRCT=APCCTLOG; /* DESIGNATE AS LOGIC ERROR      */
         MVI   APCWRCT(APCWPTR),X'08'                           01S0101
*                 APEMMSGN=APCMS102;/* SELECT MESSAGE INDEX          */
         MVC   APEMMSGN+112(4,APCWPTR),@CF00168                 01S0102
*               END;                                            01S0103
*           RFY                                                 01S0104
*             R3F RSTD;             /* BITWHACKING WILL OCCUR IN R3F */
@RC00093 DS    0H                                               01S0104
@RC00087 DS    0H                                               01S0105
*           IF DYNAR15C^=8 THEN     /* IF ERROR WAS NOT INSTALLATION */
         L     @09,@PC00001+4                                   01S0105
         CLC   DYNAR15C(4,@09),@CF00052                         01S0105
         BE    @RF00105                                         01S0105
*             DO INDEX=1 TO 4;      /* THEN PASS ON ERROR & INFO CODE*/
         LA    INDEX,1                                          01S0106
@DL00106 DS    0H                                               01S0107
*               R3F=ZERO;           /* CLEAR REG BEFORE INSERT CHAR  */
         SLR   R3F,R3F                                          01S0107
*               R3F=CODES(INDEX:INDEX);/* SELECT NEXT CODES CHAR.    */
         L     @09,@PC00001+4                                   01S0108
         SLR   R3F,R3F                                          01S0108
         IC    R3F,CODES-1(INDEX,@09)                           01S0108
*               SRA(R3F,4);         /* SHIFT OUT 4 LOW ORDER BITS    */
         SRA   R3F,4                                            01S0109
*               IF R3F>9 THEN       /* IF 4 HIGH ORDER BITS ARE > 9  */
         LA    @09,9                                            01S0110
         CR    R3F,@09                                          01S0110
         BNH   @RF00110                                         01S0110
*                 R3F=(R3F-9)×'000000C0'X;/* MAKE DIGIT PRINTABLE    */
         LCR   @09,@09                                          01S0111
         ALR   @09,R3F                                          01S0111
         O     @09,@CF01268                                     01S0111
         LR    R3F,@09                                          01S0111
*               ELSE                /* ELSE BITS IN DECIMAL RANGE 0-9*/
*                 R3F=R3F×'000000F0'X;/* MAKE DEC DIGIT PRINTABLE    */
         B     @RC00110                                         01S0112
@RF00110 O     R3F,@CF01269                                     01S0112
*               APEMVALU(2*INDEX-1:2*INDEX-1)=R3F;/* READY TO PRINT  */
@RC00110 LR    @09,INDEX                                        01S0113
         ALR   @09,@09                                          01S0113
         STC   R3F,APEMVALU+110(@09,APCWPTR)                    01S0113
*               R3F=CODES(INDEX:INDEX);/* LOAD IN THE CHAR AGAIN     */
         L     @09,@PC00001+4                                   01S0114
         SLR   R3F,R3F                                          01S0114
         IC    R3F,CODES-1(INDEX,@09)                           01S0114
*               R3F=R3F&'0000000F'X;/* AND OUT ALL THE HIGH BITS     */
         N     R3F,@CF01260                                     01S0115
*               IF R3F>9 THEN       /* IF 4 HIGH ORDER BITS ARE > 9  */
         LA    @09,9                                            01S0116
         CR    R3F,@09                                          01S0116
         BNH   @RF00116                                         01S0116
*                 R3F=(R3F-9)×'000000C0'X;/* MAKE DIGIT PRINTABLE    */
         LCR   @09,@09                                          01S0117
         ALR   @09,R3F                                          01S0117
         O     @09,@CF01268                                     01S0117
         LR    R3F,@09                                          01S0117
*               ELSE                /* ELSE BITS IN DECIMAL RANGE 0-9*/
*                 R3F=R3F×'000000F0'X;/* MAKE DEC DIGIT PRINTABLE    */
         B     @RC00116                                         01S0118
@RF00116 O     R3F,@CF01269                                     01S0118
*               APEMVALU(2*INDEX:2*INDEX)=R3F;/* READY TO PRINT      */
@RC00116 LR    @09,INDEX                                        01S0119
         ALR   @09,@09                                          01S0119
         STC   R3F,APEMVALU+111(@09,APCWPTR)                    01S0119
*             END;                                              01S0120
         AL    INDEX,@CF00058                                   01S0120
         C     INDEX,@CF00045                                   01S0120
         BNH   @DL00106                                         01S0120
*           ELSE                                                01S0121
*             ;                     /* INSTALLATION ERROR CODES UNK. */
@RF00105 DS    0H                                               01S0122
*           RFY                                                 01S0122
*             R3F UNRSTD;           /* GIVE BACK REG 3               */
*           APCWSVAL=BITONE;        /* SIGNAL DYNALLOC ERROR         */
         OI    APCWSVAL(APCWPTR),B'01000000'                    01S0123
*         END;                                                  01S0124
*         GOTO CE0001@;             /* BRANCH TO END OF CASE         */
         B     CE0001@                                          01S0125
*       END DYNA;                   /* END OF SUBCASE                */
*CE0001@:                                                       01S0127
*     END TABLES;                   /* END OF CASE                   */
CE0001@  DS    0H                                               01S0128
*     APCWRC0=ZERO;                 /* HIGH BYTE OF APCWRCDE = ZERO  */
         MVI   APCWRC0(APCWPTR),X'00'                           01S0128
*     APCWRCC=APCCS;                /* IN ALL CASES MACRO FOUND ERROR*/
         MVI   APCWRCC(APCWPTR),X'06'                           01S0129
*     APCWR15C=APCCRERR;            /* SET REG 15 CODE TO SHOW ERROR */
         MVC   APCWR15C(4,APCWPTR),@CF00052                     01S0130
*     RETURN;                                                   01S0131
@EL00001 L     @13,4(,@13)                                      01S0131
@EF00001 L     @00,@SIZDATD                                     01S0131
         LR    @01,@06                                          01S0131
         IGTYFSV  R,LV=(0),A=(1)
@ER00001 LM    @14,@12,12(@13)                                  01S0131
         BR    @14                                              01S0131
*   END;                            /* END OF APCVZMJI PROCESSING    */
