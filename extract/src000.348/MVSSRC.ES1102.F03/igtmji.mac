*   /*                                                               */
*   /* SYSTEM DECLARES                                               */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0133
*   DECLARE                         /* COMMON VARIABLES              */
*     I256C CHAR(256) BASED,                                    01S0133
*     I031F FIXED(31) BASED,                                    01S0133
*     I031P PTR(31) BASED,                                      01S0133
*     I015F FIXED(15) BASED,                                    01S0133
*     I015P PTR(15) BASED,                                      01S0133
*     I008P PTR(8) BASED,                                       01S0133
*     I001C CHAR(1) BASED;                                      01S0133
*                                                               01S0134
*   /*****************************************************************/
*   /*                                                               */
*   /* MODULE PATCH AREA                                             */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0134
*   DCL                                                         01S0134
*     APPATCH CHAR(*) INT STATIC LOCAL GEND;                    01S0134
*   GENERATE DATA DEFS(APPATCH);                                01S0135
