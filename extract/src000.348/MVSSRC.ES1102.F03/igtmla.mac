*   /*****************************************************************/
*   /*                                                               */
*   /* ZERO CCW AND CIT HEADER WORK AREAS                            */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0048
*   DO;                                                         01S0048
*     CCWENTRY=CCWENTRY&&CCWENTRY;  /* ZERO CCW WORK AREA            */
         XC    CCWENTRY(8),CCWENTRY                             01S0049
*     CITHDR=CITHDR&&CITHDR;        /* ZERO CIT HEADER               */
         XC    CITHDR(4),CITHDR                                 01S0050
*   END;                                                        01S0051
*                                                               01S0051
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE CIT AND CCW POINTERS - THE USER CCW TABLE START    */
*   /* ADDRESS(ACCW) IS SAVED IN APAX. THE ADDRESS IS THEN CHECKED TO*/
*   /* ASSURE THAT IT IS ON A DOUBLEWORD BOUNDARY. IF NOT, AN ERROR  */
*   /* MESSAGE IS SET UP (NOT DWORD BDY - ACCW) AND A RETURN IS MADE */
*   /* TO THE CALLING PROGRAM. OTHERWISE, THE CCW NEXT ENTRY ADDRESS */
*   /* IS SET TO THE START ADDRESS. THE CCW END ADDRESS IS THEN      */
*   /* COMPUTED USING THE CCW TABLE LENGTH (ICCW). NEXT, THE CIT     */
*   /* START, NEXT AND END ADDRESSES ARE SIMILARLY SAVED IN APAX. THE*/
*   /* ADDRESS OF THE LAST CIT ENTRY AND NUMBER OF WORDS IN THE      */
*   /* CURRENT CCW DATA CHAIN ARE INITIALIZED TO ZERO.               */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0052
*   DO;                                                         01S0052
*     APAXCCWS=APINAACW;            /* SET CCW START ADDRESS         */
         L     @09,APCWACB(,APCWPTR)                            01S0053
         L     @09,ACBAMWAP(,@09)                               01S0053
         L     @01,APCWAPIN(,APCWPTR)                           01S0053
         L     @15,APINAACW(,@01)                               01S0053
         ST    @15,APAXCCWS(,@09)                               01S0053
*     IF(APAXCCWS&EVENBITS)^=0 THEN /* TEST IF CCW TABLE IS ON A
*                                      DWORD BOUNDARY                */
         N     @15,@CF00065                                     01S0054
         LTR   @15,@15                                          01S0054
         BZ    @RF00054                                         01S0054
*       DO;                         /* NO - SET UP ERROR INDICATORS  */
*         APCWR15C=APCCRERR;        /* PROVIDE R15 RETURN CODE       */
         MVC   APCWR15C(4,APCWPTR),@CF00040                     01S0056
*         APCWRCT=APCCTLOG;         /* ERROR TYPE - LOGICAL          */
         MVI   APCWRCT(APCWPTR),X'08'                           01S0057
*         APCWRCC=APCCB;            /* COMPONENT ID                  */
         MVI   APCWRCC(APCWPTR),X'04'                           01S0058
*         APCWRCN=APCCB003;         /* ERROR NUMBER                  */
         MVI   APCWRCN(APCWPTR),X'03'                           01S0059
*         APCWZAM=ON;               /* SET FLAG TO INDICATE ERROR
*                                      DETECTED                      */
         OI    APCWZAM(APCWPTR),B'00000001'                     01S0060
*         APEMMSGN=APCMB003;        /* MESSAGE NUMBER FOR NOT DWORD
*                                      BDY                           */
         LA    @09,112                                          01S0061
         ALR   @09,APCWPTR                                      01S0061
         MVC   APEMMSGN(4,@09),@CF00583                         01S0061
*         APEMVALN=APCMK043;        /* KEYWORD NUMBER FOR ACCW       */
         MVC   APEMVALN(4,@09),@CF00338                         01S0062
*         RETURN;                   /* RETURN TO CALLER              */
@EL00001 L     @13,4(,@13)                                      01S0063
@EF00001 L     @00,@SIZDATD                                     01S0063
         LR    @01,@06                                          01S0063
         IGTYFSV  R,LV=(0),A=(1)
@ER00001 LM    @14,@12,12(@13)                                  01S0063
         BR    @14                                              01S0063
*       END;                                                    01S0064
*     ELSE                                                      01S0065
*       ;                           /* NO ERROR                      */
@RF00054 DS    0H                                               01S0066
*     APAXCCWN=APAXCCWS;            /* SET CCW NEXT ENTRY ADDR       */
         L     @09,APCWACB(,APCWPTR)                            01S0066
         L     @09,ACBAMWAP(,@09)                               01S0066
         L     @15,APAXCCWS(,@09)                               01S0066
         ST    @15,APAXCCWN(,@09)                               01S0066
*     APAXCCWE=APAXCCWS+(4*APINICW)-1;/* SET CCW END ADDRESS         */
         L     @14,APCWAPIN(,APCWPTR)                           01S0067
         L     @01,APINAICW(,@14)                               01S0067
         L     @11,APINICW(,@01)                                01S0067
         SLA   @11,2                                            01S0067
         ALR   @15,@11                                          01S0067
         BCTR  @15,0                                            01S0067
         ST    @15,APAXCCWE(,@09)                               01S0067
*     APAXCIS=APINAACI;             /* SET CIT START ADDRESS         */
         L     @15,APINAACI(,@14)                               01S0068
         ST    @15,APAXCIS(,@09)                                01S0068
*     APAXCIN=APAXCIS;              /* SET CIT NEXT ENTRY ADDR       */
         ST    @15,APAXCIN(,@09)                                01S0069
*     APAXCIE=APAXCIS+(4*APINICI)-1;/* SET CIT END ADDRESS           */
         L     @01,APINAICI(,@14)                               01S0070
         L     @14,APINICI(,@01)                                01S0070
         SLA   @14,2                                            01S0070
         ALR   @15,@14                                          01S0070
         BCTR  @15,0                                            01S0070
         ST    @15,APAXCIE(,@09)                                01S0070
*     APAXLCIT=APAXLCIT&&APAXLCIT;  /* SET ADDRESS OF LAST CIT  01S0071
*                                      INITIALLY TO ZERO             */
         L     @15,APAXLCIT(,@09)                               01S0071
         LR    @14,@15                                          01S0071
         XR    @14,@15                                          01S0071
         ST    @14,APAXLCIT(,@09)                               01S0071
*     APAXIOCT=APAXIOCT&&APAXIOCT;  /* INITIALIZE NUMBER OF WORDS IN
*                                      CURRENT CCW CHAIN TO ZERO     */
         L     @15,APAXIOCT(,@09)                               01S0072
         LR    @14,@15                                          01S0072
         XR    @14,@15                                          01S0072
         ST    @14,APAXIOCT(,@09)                               01S0072
*   END;                                                        01S0073
*                                                               01S0073
*   /*****************************************************************/
*   /*                                                               */
*   /* CONSTRUCT PROCESS CCW IN WORK AREA - THE PROCESS CCW ALERTS   */
*   /* THE 3838 TO GET READY. MISSING FIELDS IN THE CCW WILL BE      */
*   /* FILLED IN AT EXC OR EXCW TIME. IT IS CONSTRUCTED HERE SINCE   */
*   /* SPACE MUST BE AVAILABLE AT THE HEAD OF THE CCW CHAIN.         */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0074
*   DO;                                                         01S0074
*     CMDCODE=PROCCCW;              /* SET CCW CODE TO PROCESS       */
         MVI   CMDCODE,X'0F'                                    01S0075
*     CCFLAG=ON;                    /* SET COMMAND CHAINING          */
         OI    CCFLAG,B'01000000'                               01S0076
*     BYTCOUNT=8;                   /* SET BYTE TRANSFER COUNT       */
         MVC   BYTCOUNT(2),@CH00040                             01S0077
*   END;                                                        01S0078
*                                                               01S0078
*   /*****************************************************************/
*   /*                                                               */
*   /* MOVE PROCESS CCW TO CCW TABLE                                 */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0079
*   DO;                                                         01S0079
*     CALL APCVZMIB(CCWENTRY,ERRORBIT);/* CALL MOVE CCW              */
         LA    @09,CCWENTRY                                     01S0080
         ST    @09,@AL00001                                     01S0080
         LA    @09,ERRORBIT                                     01S0080
         ST    @09,@AL00001+4                                   01S0080
         L     @15,APCTZMIB(,APCTPTR)                           01S0080
         LA    @01,@AL00001                                     01S0080
         BALR  @14,@15                                          01S0080
*     IF ERRORBIT=ON THEN           /* CCW TABLE FULL?               */
         TM    ERRORBIT,B'10000000'                             01S0081
         BO    @RT00081                                         01S0081
*       RETURN;                     /* YES - RETURN                  */
*     ELSE                                                      01S0083
*       ;                                                       01S0083
*     CCWENTRY=CCWENTRY&&CCWENTRY;  /* ZERO WORK AREA                */
         XC    CCWENTRY(8),CCWENTRY                             01S0084
*   END;                                                        01S0085
*                                                               01S0085
*   /*****************************************************************/
*   /*                                                               */
*   /* CONSTRUCT WRITE CCW IN WORK AREA - THIS CCW WRITES THE CIT TO */
*   /* THE 3838. THE LENGTH FIELD WILL BE FILLED IN WITH THE ACTUAL  */
*   /* LENGTH OF THE CIT AT THE TIME OF THE XLTE CALL.               */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0086
*   DO;                                                         01S0086
*     CMDCODE=WRITECCW;             /* WRITE CCW                     */
         MVI   CMDCODE,X'01'                                    01S0087
*     DATADDR=APAXCIS;              /* START ADDR OF CIT             */
         L     @09,APCWACB(,APCWPTR)                            01S0088
         L     @09,ACBAMWAP(,@09)                               01S0088
         MVC   DATADDR(3),APAXCIS+1(@09)                        01S0088
*     CCFLAG=ON;                    /* SPECIFY COMMAND CHAINING      */
         OI    CCFLAG,B'01000000'                               01S0089
*   END;                                                        01S0090
*                                                               01S0090
*   /*****************************************************************/
*   /*                                                               */
*   /* MOVE CCW ENTRY TO CCW TABLE                                   */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0091
*   CALL APCVZMIB(CCWENTRY,ERRORBIT);/* CALL MOVE CCW                */
         LA    @09,CCWENTRY                                     01S0091
         ST    @09,@AL00001                                     01S0091
         LA    @09,ERRORBIT                                     01S0091
         ST    @09,@AL00001+4                                   01S0091
         L     @15,APCTZMIB(,APCTPTR)                           01S0091
         LA    @01,@AL00001                                     01S0091
         BALR  @14,@15                                          01S0091
*   IF ERRORBIT=ON THEN             /* CCW TABLE FULL?               */
         TM    ERRORBIT,B'10000000'                             01S0092
         BO    @RT00092                                         01S0092
*     RETURN;                       /* YES - RETURN                  */
*   ELSE                                                        01S0094
*     ;                                                         01S0094
*                                                               01S0094
*   /*****************************************************************/
*   /*                                                               */
*   /* MOVE CIT HEADER TO CIT                                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0095
*   DO;                                                         01S0095
*     CITLNGTH=1;                   /* HEADER LENGTH IN WORDS        */
         MVC   CITLNGTH(4),@CF00047                             01S0096
*     CALL APCVZMIC(CITHDR,CITLNGTH,ERRORBIT);/* CALL MOVE CIT       */
         LA    @09,CITHDR                                       01S0097
         ST    @09,@AL00001                                     01S0097
         LA    @09,CITLNGTH                                     01S0097
         ST    @09,@AL00001+4                                   01S0097
         LA    @09,ERRORBIT                                     01S0097
         ST    @09,@AL00001+8                                   01S0097
         L     @15,APCTZMIC(,APCTPTR)                           01S0097
         LA    @01,@AL00001                                     01S0097
         BALR  @14,@15                                          01S0097
*     IF ERRORBIT=ON THEN           /* CIT FULL?                     */
         TM    ERRORBIT,B'10000000'                             01S0098
         BO    @RT00098                                         01S0098
*       RETURN;                     /* YES - RETURN                  */
*     ELSE                                                      01S0100
*       ;                                                       01S0100
*   END;                                                        01S0101
*                                                               01S0101
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE THE LABEL TABLE TO ALL ZEROES.                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0102
*   DO I=APLBNUMB TO 1 BY-1;        /* APLBNUMB CONTAINS THE NUMBER
*                                      OF ENTRIES IN APLB            */
         L     @09,APCWACB(,APCWPTR)                            01S0102
         L     @09,ACBAMWAP(,@09)                               01S0102
         L     @09,APAXAPLB(,@09)                               01S0102
         L     I,APLBNUMB(,@09)                                 01S0102
         B     @DE00102                                         01S0102
@DL00102 DS    0H                                               01S0103
*     APLBENTY(I)=APLBENTY(I)&&APLBENTY(I);                     01S0103
         LR    @09,I                                            01S0103
         SLA   @09,3                                            01S0103
         L     @03,APCWACB(,APCWPTR)                            01S0103
         L     @03,ACBAMWAP(,@03)                               01S0103
         L     @03,APAXAPLB(,@03)                               01S0103
         ALR   @03,@09                                          01S0103
         XC    APLBENTY-8(8,@03),APLBENTY-8(@03)                01S0103
*   END;                                                        01S0104
*                                                               01S0104
         BCTR  I,0                                              01S0104
@DE00102 LTR   I,I                                              01S0104
         BP    @DL00102                                         01S0104
*   /*****************************************************************/
*   /*                                                               */
*   /* SAVE THE ISTATE FOR LATER USE BY APRL                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0105
*   DO;                                                         01S0105
*     APAXSTAT=APINSTB;                                         01S0106
         L     @09,APCWACB(,APCWPTR)                            01S0106
         L     @09,ACBAMWAP(,@09)                               01S0106
         L     @01,APCWAPIN(,APCWPTR)                           01S0106
         L     @01,APINASTB(,@01)                               01S0106
         MVC   APAXSTAT(4,@09),APINSTB(@01)                     01S0106
*   END;                                                        01S0107
*                                                               01S0107
*   /*****************************************************************/
*   /*                                                               */
*   /* SET ACTIVE BUILD SWITCH                                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0108
*   DO;                                                         01S0108
*     APAXBDSW=ON;                                              01S0109
         OI    APAXBDSW(@09),B'10000000'                        01S0109
*   END;                                                        01S0110
