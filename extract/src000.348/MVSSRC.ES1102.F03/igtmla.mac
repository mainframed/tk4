*   /*****************************************************************/
*   /*                                                               */
*   /* SET POINTERS FOR CONTROL BLOCKS NEEDED                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0038
*   DO;                                                         01S0038
*     RFY                                                       01S0039
*       APCWPTR RSTD;               /* NEW POINTER FOR APCW          */
*     APCWPTR=ADDR(DAPCW);                                      01S0040
         L     APCWPTR,@PC00001                                 01S0040
*     RFY                                                       01S0041
*       APCW BASED(APCWPTR);                                    01S0041
*     RFY                                                       01S0042
*       APEM BASED(ADDR(APCWEMSG)); /* GET APEM                      */
*     RFY                                                       01S0043
*       APIN BASED(APCWAPIN);       /* GET APIN                      */
*     RFY                                                       01S0044
*       IFGACB BASED(APCWACB);      /* GET ACB                       */
*     RFY                                                       01S0045
*       APAX BASED(ACBAMWAP);       /* GET APAX                      */
*     RFY                                                       01S0046
*       APLB BASED(APAXAPLB);       /* GET APLB                      */
*   END;                                                        01S0047
