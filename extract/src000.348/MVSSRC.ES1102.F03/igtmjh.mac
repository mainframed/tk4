TMJH     TITLE 'IGTMJH --- PROCESS USER INPUTS FOR GENACB AND MODACB   *
                        '
IGTMJH   CSECT ,                                                01S0001
@MAINENT DS    0H                                               01S0001
         USING *,@15                                            01S0001
         B     @PROLOG                                          01S0001
         DC    AL1(16)                                          01S0001
         DC    C'IGTMJH    77.231'                              01S0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                  01S0001
         BALR  @07,0                                            01S0001
@PSTART  DS    0H                                               01S0001
         USING @PSTART,@07                                      01S0001
         L     @00,@SIZDATD                                     01S0001
         IGTYGSV  R,LV=(0)
         LR    @06,@01                                          01S0001
         USING @DATD,@06                                        01S0001
         ST    @13,@SA00001+4                                   01S0001
         LM    @00,@01,20(@13)                                  01S0001
         ST    @06,8(,@13)                                      01S0001
         LR    @13,@06                                          01S0001
         MVC   @PC00001(4),0(@01)                               01S0001
*/*                                                                  */
*/* END OF APSV                                                      */
*/*                                                                  */
*                                                               01S0040
*                                                               01S0040
*   /*****************************************************************/
*   /*                                                               */
*   /* PROCESS USER'S ACB INPUT PARAMETERS                           */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0040
*   DO;                                                         01S0040
*     RFY                                                       01S0041
*       APCWPTR RSTD;                                           01S0041
*     APCWPTR=ADDR(DAPCW);          /* NEW POINTER FOR APCW          */
         L     APCWPTR,@PC00001                                 01S0042
*     RFY                                                       01S0043
*       APCW BASED(APCWPTR);                                    01S0043
*     RFY                                                       01S0044
*       APCT BASED(APCTPTR);        /* GRAB HOLD OF APCT             */
*     RFY                                                       01S0045
*       IFGACB BASED(APCWACB);      /* GRAB HOLD OF ACB              */
*     RFY                                                       01S0046
*       APCX BASED(ACBAMAX);        /* GRAB HOLD OF APCX             */
*     RFY                                                       01S0047
*       IFGEXLST BASED(ACBEXLST);   /* GRAB HOLD OF EXLST            */
*     RFY                                                       01S0048
*       APEM BASED(ADDR(APCWEMSG)); /* GRAB HOLD OF APEM             */
*     PARAMPTR=APCWAPIN;            /* INITIALIZE MOVING APIN PTR    */
         L     PARAMPTR,APCWAPIN(,APCWPTR)                      01S0049
*     RFY                                                       01S0050
*       APIN BASED(PARAMPTR);       /* GRAB HOLD OF APIN             */
*     IF APINAP3E=BITZERO THEN      /* SEE IF IERROR IS LAST PARAM.  */
         TM    APINAP3E(PARAMPTR),B'10000000'                   01S0051
         BNZ   @RF00051                                         01S0051
*       DO;                         /* IF NOT, LOOK FOR MORE PARAM.S */
*         ENDATKYW=FALSE;           /* INDICATES LIST END AT KEYWORD */
         NI    ENDATKYW,B'01111111'                             01S0053
*         ENDATKYV=FALSE;           /* INDICATES LIST END AT KEYVALUE*/
         NI    ENDATKYV,B'01111111'                             01S0054
*         DO WHILE(ENDATKYW=FALSE&ENDATKYV=FALSE);              01S0055
*                                                               01S0055
         B     @DE00055                                         01S0055
@DL00055 DS    0H                                               01S0056
*           /*********************************************************/
*           /*                                                       */
*           /* EXAMINE PAIRS OF PARAMETERS, UNTIL LIST END IS        */
*           /* ENCOUNTERED                                           */
*           /*                                                       */
*           /*********************************************************/
*                                                               01S0056
*           KYWMATCH=FALSE;         /* INITIALIZE MATCH INDICATOR    */
         NI    KYWMATCH,B'01111111'                             01S0056
*           DO INDEX=1 TO KYWCOUNT BY 1 WHILE(KYWMATCH=FALSE);/* SCAN
*                                      ALL POSSIBLE KEYWORDS FOR A
*                                      KEYWORD MATCH                 */
         LA    INDEX,1                                          01S0057
@DL00057 TM    KYWMATCH,B'10000000'                             01S0057
         BNZ   @DC00057                                         01S0057
*             IF APINKYWD=KYWDCHAR(INDEX) THEN                  01S0058
         L     @09,APINAKYW(,PARAMPTR)                          01S0058
         LR    @05,INDEX                                        01S0058
         SLA   @05,3                                            01S0058
         LA    @01,KYWDCHAR-8(@05)                              01S0058
         CLC   APINKYWD(4,@09),0(@01)                           01S0058
         BNE   @RF00058                                         01S0058
*               DO;                 /* A KEYWORD MATCH IS OBTAINED   */
*                 KYWMATCH=TRUE;    /* SET MATCH INDICATOR           */
         OI    KYWMATCH,B'10000000'                             01S0060
*                 KYWINDEX=INDEX;   /* PRESERVE INDEX FOR CASES BELOW*/
         LR    KYWINDEX,INDEX                                   01S0061
*               END;                                            01S0062
*             ELSE                                              01S0063
*               ;                                               01S0063
@RF00058 DS    0H                                               01S0064
*           END;                                                01S0064
         AL    INDEX,@CF00040                                   01S0064
         C     INDEX,@CF00057                                   01S0064
         BNH   @DL00057                                         01S0064
@DC00057 DS    0H                                               01S0065
*           IF KYWMATCH=TRUE THEN                               01S0065
         TM    KYWMATCH,B'10000000'                             01S0065
         BNO   @RF00065                                         01S0065
*             DO;                                               01S0066
*                                                               01S0066
*               /*****************************************************/
*               /*                                                   */
*               /* A KEYWORD MATCH HAVING BEEN OBTAINED, PLACE INPUT */
*               /* VALUE (OR NULL VALUE IF NONE WERE INPUT) INTO     */
*               /* APPROPRIATE CONTROL BLOCK.                        */
*               /*                                                   */
*               /*****************************************************/
*                                                               01S0067
*               IF WSIGNBIT=BITONE THEN                         01S0067
         TM    WSIGNBIT(PARAMPTR),B'10000000'                   01S0067
         BNO   @RF00067                                         01S0067
*                 ENDATKYW=TRUE;    /* KEYWORD IS FINAL PARAMETER,
*                                      AND NULL VALUE IS USED.       */
         OI    ENDATKYW,B'10000000'                             01S0068
*               ELSE                                            01S0069
*                 IF VSIGNBIT=BITONE THEN                       01S0069
         B     @RC00067                                         01S0069
@RF00067 TM    VSIGNBIT(PARAMPTR),B'10000000'                   01S0069
         BNO   @RF00069                                         01S0069
*                   ENDATKYV=TRUE;  /* VALUE IS FINAL PARAMETER      */
         OI    ENDATKYV,B'10000000'                             01S0070
*                 ELSE                                          01S0071
*                   ;               /* PARAMETER STRING CONTINUES    */
@RF00069 DS    0H                                               01S0072
*               ERRORNUM=ZERO;      /* INTERNAL ERROR INDICATOR      */
@RC00067 SLR   ERRORNUM,ERRORNUM                                02S0072
*KEYWORD:                                                       02S0073
*               DO;                 /* KEYWORD:CASE(KYWINDEX)NAMES(RE
*                                      G,STRN,OWN,LERR,PERR,INTV,EMSG
*                                      ,RBA,RCDE,BSAD,NULL)          */
KEYWORD  DS    0H                                               02S0074
*                 DCL                                           02S0074
*                   CL0001@ LABEL BASED(CP0001@(KYWINDEX)) VALUERANGE(
*                       REG,STRN,OWN,LERR,PERR,INTV,EMSG,RBA,RCDE,BSAD,
*                       NULL);      /* BRANCH TARGET                 */
*                 DCL                                           02S0075
*                   CP0001@(11) PTR(31) INIT(ADDR(REG),ADDR(STRN),ADDR(
*                       OWN),ADDR(LERR),ADDR(PERR),ADDR(INTV),ADDR(EMSG
*                       ),ADDR(RBA),ADDR(RCDE),ADDR(BSAD),ADDR(NULL));
*                 GOTO CL0001@;     /* BRANCH TO SELECTED CASE NTH
*                                      NAME SHOULD = KYWDCHAR(N)     */
         LR    @09,KYWINDEX                                     02S0076
         SLA   @09,2                                            02S0076
         L     @09,CP0001@-4(@09)                               02S0076
         BR    @09                                              02S0076
*REG:                                                           02S0077
*                 DO;               /* SUBCASE                       */
REG      DS    0H                                               02S0078
*                   IF ACBOPEN=TRUE THEN/* NO REG MODIFY ON OPEN ACB */
         L     @09,APCWACB(,APCWPTR)                            02S0078
         TM    ACBOPEN(@09),B'00010000'                         02S0078
         BNO   @RF00078                                         02S0078
*                     DO;                                       02S0079
*                       APEMMSGN=APCMG007;/* SPECIFY MESSAGE NO.
*                                      INDEX                         */
         MVC   APEMMSGN+112(4,APCWPTR),@CF00105                 02S0080
*                       APCWRCN=APCCG007;/* PROVIDE ERROR RETURN
*                                      NUMBER                        */
         MVI   APCWRCN(APCWPTR),X'07'                           02S0081
*                       ERRORNUM=1; /* TO DENOTE ERROR ON REG PARM.  */
         LA    ERRORNUM,1                                       02S0082
*                     END;                                      02S0083
*                   ELSE                                        02S0084
*                     IF ENDATKYW=TRUE THEN/* RESET PARAMETER TO NULL
*                                      VALUE                         */
         B     @RC00078                                         02S0084
@RF00078 TM    ENDATKYW,B'10000000'                             02S0084
         BNO   @RF00084                                         02S0084
*                       APCXREG=ZERO;                           02S0085
         L     @09,APCWACB(,APCWPTR)                            02S0085
         L     @09,ACBAMAX(,@09)                                02S0085
         SLR   @05,@05                                          02S0085
         ST    @05,APCXREG(,@09)                                02S0085
*                     ELSE                                      02S0086
*                       APCXREG=APINKYVF;/* USE INPUT VALUE          */
         B     @RC00084                                         02S0086
@RF00084 L     @09,APCWACB(,APCWPTR)                            02S0086
         L     @09,ACBAMAX(,@09)                                02S0086
         L     @05,APINAKYV(,PARAMPTR)                          02S0086
         L     @05,APINKYVF(,@05)                               02S0086
         ST    @05,APCXREG(,@09)                                02S0086
*                   GOTO CE0001@;   /* BRANCH TO END OF CASE         */
         B     CE0001@                                          02S0087
*                 END REG;          /* END OF SUBCASE                */
*STRN:                                                          02S0089
*                 DO;               /* SUBCASE                       */
STRN     DS    0H                                               02S0090
*                   IF ACBOPEN=TRUE THEN/* NO STRN MODIFY ON AN OPEN
*                                      ACB                           */
         L     @09,APCWACB(,APCWPTR)                            02S0090
         TM    ACBOPEN(@09),B'00010000'                         02S0090
         BNO   @RF00090                                         02S0090
*                     DO;                                       02S0091
*                       APEMMSGN=APCMG008;/* SPECIFY MESSAGE NO.
*                                      INDEX                         */
         MVC   APEMMSGN+112(4,APCWPTR),@CF00105                 02S0092
*                       APCWRCN=APCCG008;/* PROVIDE ERROR RETURN
*                                      NUMBER                        */
         MVI   APCWRCN(APCWPTR),X'08'                           02S0093
*                       ERRORNUM=2; /* TO DENOTE ERROR ON STRN PARM. */
         LA    ERRORNUM,2                                       02S0094
*                     END;                                      02S0095
*                   ELSE                                        02S0096
*                     IF ENDATKYW=TRUE THEN/* RESET PARAMETER TO NULL
*                                      VALUE                         */
         B     @RC00090                                         02S0096
@RF00090 TM    ENDATKYW,B'10000000'                             02S0096
         BNO   @RF00096                                         02S0096
*                       ACBSTRNO=APCTSTDF;                      02S0097
         L     @09,APCWACB(,APCWPTR)                            02S0097
         L     @05,APCTSTDF(,APCTPTR)                           02S0097
         STC   @05,ACBSTRNO(,@09)                               02S0097
*                     ELSE                                      02S0098
*                       IF(APINKYVF<1×APINKYVF>255) THEN        02S0098
         B     @RC00096                                         02S0098
@RF00096 L     @09,APINAKYV(,PARAMPTR)                          02S0098
         L     @09,APINKYVF(,@09)                               02S0098
         LTR   @09,@09                                          02S0098
         BNP   @RT00098                                         02S0098
         C     @09,@CF01344                                     02S0098
         BNH   @RF00098                                         02S0098
@RT00098 DS    0H                                               02S0099
*                         DO;       /* STRN VALUE IS OUT OF RANGE    */
*                           APEMMSGN=APCMG003;/* SPECIFY ERROR  02S0100
*                                      MESSAGE INDEX                 */
         MVC   APEMMSGN+112(4,APCWPTR),@CF00092                 02S0100
*                           APCWRCN=APCCG003;/* SPECIFY ERROR RETURN
*                                      CODE                          */
         MVI   APCWRCN(APCWPTR),X'03'                           02S0101
*                           ERRORNUM=2;/* TO DENOTE ERROR ON STRN
*                                      PARM.                         */
         LA    ERRORNUM,2                                       02S0102
*                         END;                                  02S0103
*                       ELSE                                    02S0104
*                         ACBSTRNO=APINKYVF;/* USE INPUT VALUE       */
         B     @RC00098                                         02S0104
@RF00098 L     @09,APCWACB(,APCWPTR)                            02S0104
         L     @05,APINAKYV(,PARAMPTR)                          02S0104
         L     @05,APINKYVF(,@05)                               02S0104
         STC   @05,ACBSTRNO(,@09)                               02S0104
*                   GOTO CE0001@;   /* BRANCH TO END OF CASE         */
         B     CE0001@                                          02S0105
*                 END STRN;         /* END OF SUBCASE                */
*OWN:                                                           02S0107
*                 DO;               /* SUBCASE                       */
OWN      DS    0H                                               02S0108
*                   IF ACBOPEN=TRUE THEN/* NO OWN MODIFY ON AN OPEN
*                                      ACB                           */
         L     @09,APCWACB(,APCWPTR)                            02S0108
         TM    ACBOPEN(@09),B'00010000'                         02S0108
         BNO   @RF00108                                         02S0108
*                     DO;                                       02S0109
*                       APEMMSGN=APCMG002;/* SPECIFY MESSAGE NO.
*                                      INDEX                         */
         MVC   APEMMSGN+112(4,APCWPTR),@CF00105                 02S0110
*                       APCWRCN=APCCG002;/* PROVIDE ERROR RETURN
*                                      NUMBER                        */
         MVI   APCWRCN(APCWPTR),X'02'                           02S0111
*                       ERRORNUM=3; /* TO DENOTE ERROR ON OWN PARM.  */
         LA    ERRORNUM,3                                       02S0112
*                     END;                                      02S0113
*                   ELSE                                        02S0114
*                     IF ENDATKYW=TRUE THEN/* RESET PARAMETER TO NULL
*                                      VALUE                         */
         B     @RC00108                                         02S0114
@RF00108 TM    ENDATKYW,B'10000000'                             02S0114
         BNO   @RF00114                                         02S0114
*                       APCXOWN=BITZERO;/* '0'B REPRESENTS OWN='SHR' */
         L     @09,APCWACB(,APCWPTR)                            02S0115
         L     @09,ACBAMAX(,@09)                                02S0115
         NI    APCXOWN(@09),B'01111111'                         02S0115
*                     ELSE                                      02S0116
*                       IF APINKYVL='SHR ' THEN/* SET PARAMETER TO
*                                      INPUT VALUE                   */
         B     @RC00114                                         02S0116
@RF00114 L     @09,APINAKYV(,PARAMPTR)                          02S0116
         CLC   APINKYVL(4,@09),@CC01600                         02S0116
         BNE   @RF00116                                         02S0116
*                         APCXOWN=BITZERO;                      02S0117
         L     @09,APCWACB(,APCWPTR)                            02S0117
         L     @09,ACBAMAX(,@09)                                02S0117
         NI    APCXOWN(@09),B'01111111'                         02S0117
*                       ELSE                                    02S0118
*                         IF APINKYVL='EXC ' THEN/* ALSO SET TO INPUT
*                                      VALUE                         */
         B     @RC00116                                         02S0118
@RF00116 L     @09,APINAKYV(,PARAMPTR)                          02S0118
         CLC   APINKYVL(4,@09),@CC01601                         02S0118
         BNE   @RF00118                                         02S0118
*                           APCXOWN=BITONE;                     02S0119
         L     @09,APCWACB(,APCWPTR)                            02S0119
         L     @09,ACBAMAX(,@09)                                02S0119
         OI    APCXOWN(@09),B'10000000'                         02S0119
*                         ELSE                                  02S0120
*                           DO;     /* OWN PARAMETER IS INVALID      */
         B     @RC00118                                         02S0120
@RF00118 DS    0H                                               02S0121
*                             APEMMSGN=APCMG004;/* SPECIFY ERROR
*                                      MESSAGE INDEX                 */
         MVC   APEMMSGN+112(4,APCWPTR),@CF00057                 02S0121
*                             APCWRCN=APCCG004;/* SPECIFY ERROR 02S0122
*                                      RETURN CODE                   */
         MVI   APCWRCN(APCWPTR),X'04'                           02S0122
*                             ERRORNUM=3;/* DENOTE ERROR AS FOR OWN
*                                      PARM.                         */
         LA    ERRORNUM,3                                       02S0123
*                           END;                                02S0124
*                   GOTO CE0001@;   /* BRANCH TO END OF CASE         */
         B     CE0001@                                          02S0125
*                 END OWN;          /* END OF SUBCASE                */
*LERR:                                                          02S0127
*                 DO;               /* SUBCASE                       */
LERR     DS    0H                                               02S0128
*                   IF ENDATKYW=TRUE THEN                       02S0128
         TM    ENDATKYW,B'10000000'                             02S0128
         BNO   @RF00128                                         02S0128
*                     DO;           /* RESET PARAMETER TO NULL       */
*                       EXLLERF=BITZERO;                        02S0130
         L     @09,APCWACB(,APCWPTR)                            02S0130
         L     @09,ACBEXLST(,@09)                               02S0130
         MVI   EXLLERF(@09),B'00000000'                         02S0130
*                       EXLLERP=ZERO;                           02S0131
         SLR   @05,@05                                          02S0131
         STCM  @05,15,EXLLERP(@09)                              02S0131
*                     END;                                      02S0132
*                   ELSE                                        02S0133
*                     DO;                                       02S0133
         B     @RC00128                                         02S0133
@RF00128 DS    0H                                               02S0134
*                       EXLLERP=APINKYVP;/* SET PARAMETER TO INPUT
*                                      VALUE                         */
         L     @09,APCWACB(,APCWPTR)                            02S0134
         L     @09,ACBEXLST(,@09)                               02S0134
         L     @05,APINAKYV(,PARAMPTR)                          02S0134
         MVC   EXLLERP(4,@09),APINKYVP(@05)                     02S0134
*                       EXLLERS=BITONE;/* INDICATE INPUT VALUE GIVEN */
*                       EXLLERA=BITONE;/* INDICATE EXIT ENTRY ACTIVE */
*                       EXLLERK=BITZERO;/* INDICATE ENTRY NAME EMPTY */
         OI    EXLLERS(@09),B'11000000'                         02S0137
         NI    EXLLERK(@09),B'11011111'                         02S0137
*                       RFY                                     02S0138
*                         PARMSIGN BASED(ADDR(EXLLERP));        02S0138
*                       PSIGNBIT=BITZERO;/* ENSURE THAT SIGNBIT IS
*                                      OFF                           */
         NI    PSIGNBIT+16(@09),B'01111111'                     02S0139
*                     END;                                      02S0140
*                   GOTO CE0001@;   /* BRANCH TO END OF CASE         */
         B     CE0001@                                          02S0141
*                 END LERR;         /* END OF SUBCASE                */
*PERR:                                                          02S0143
*                 DO;               /* SUBCASE                       */
PERR     DS    0H                                               02S0144
*                   IF ENDATKYW=TRUE THEN                       02S0144
         TM    ENDATKYW,B'10000000'                             02S0144
         BNO   @RF00144                                         02S0144
*                     DO;           /* RESET PARAMETER TO NULL       */
*                       EXLSYNF=BITZERO;                        02S0146
         L     @09,APCWACB(,APCWPTR)                            02S0146
         L     @09,ACBEXLST(,@09)                               02S0146
         MVI   EXLSYNF(@09),B'00000000'                         02S0146
*                       EXLSYNP=ZERO;                           02S0147
         SLR   @05,@05                                          02S0147
         STCM  @05,15,EXLSYNP(@09)                              02S0147
*                     END;                                      02S0148
*                   ELSE                                        02S0149
*                     DO;                                       02S0149
         B     @RC00144                                         02S0149
@RF00144 DS    0H                                               02S0150
*                       EXLSYNP=APINKYVP;/* SET PARAMETER TO INPUT
*                                      VALUE                         */
         L     @09,APCWACB(,APCWPTR)                            02S0150
         L     @09,ACBEXLST(,@09)                               02S0150
         L     @05,APINAKYV(,PARAMPTR)                          02S0150
         MVC   EXLSYNP(4,@09),APINKYVP(@05)                     02S0150
*                       EXLSYNS=BITONE;/* INDICATE INPUT VALUE GIVEN */
*                       EXLSYNA=BITONE;/* INDICATE EXIT ENTRY ACTIVE */
*                       EXLSYNK=BITZERO;/* INDICATE ENTRY NAME EMPTY */
         OI    EXLSYNS(@09),B'11000000'                         02S0153
         NI    EXLSYNK(@09),B'11011111'                         02S0153
*                       RFY                                     02S0154
*                         PARMSIGN BASED(ADDR(EXLSYNP));        02S0154
*                       PSIGNBIT=BITZERO;/* ENSURE THAT SIGNBIT IS
*                                      OFF                           */
         NI    PSIGNBIT+11(@09),B'01111111'                     02S0155
*                     END;                                      02S0156
*                   GOTO CE0001@;   /* BRANCH TO END OF CASE         */
         B     CE0001@                                          02S0157
*                 END PERR;         /* END OF SUBCASE                */
*INTV:                                                          02S0159
*                 DO;               /* SUBCASE                       */
INTV     DS    0H                                               02S0160
*                   IF ENDATKYW=TRUE THEN                       02S0160
         TM    ENDATKYW,B'10000000'                             02S0160
         BNO   @RF00160                                         02S0160
*                     APCXINTF=ZERO;/* RESET PARAMETER TO NULL       */
         L     @09,APCWACB(,APCWPTR)                            02S0161
         L     @09,ACBAMAX(,@09)                                02S0161
         SLR   @05,@05                                          02S0161
         ST    @05,APCXINTF(,@09)                               02S0161
*                   ELSE                                        02S0162
*                     APCXINTF=APINKYVF;/* SET PARAMETER TO INPUT
*                                      VALUE                         */
         B     @RC00160                                         02S0162
@RF00160 L     @09,APCWACB(,APCWPTR)                            02S0162
         L     @09,ACBAMAX(,@09)                                02S0162
         L     @05,APINAKYV(,PARAMPTR)                          02S0162
         L     @05,APINKYVF(,@05)                               02S0162
         ST    @05,APCXINTF(,@09)                               02S0162
*                   GOTO CE0001@;   /* BRANCH TO END OF CASE         */
         B     CE0001@                                          02S0163
*                 END INTV;         /* END OF SUBCASE                */
*EMSG:                                                          02S0165
*                 DO;               /* SUBCASE                       */
EMSG     DS    0H                                               02S0166
*                   RFY                                         02S0166
*                     PARMSIGN BASED(ADDR(ACBMSGAR));           02S0166
*                   IF ENDATKYW=TRUE THEN                       02S0167
         TM    ENDATKYW,B'10000000'                             02S0167
         BNO   @RF00167                                         02S0167
*                     ACBMSGAR=ZERO;/* RESET PARM ADDR TO NULL       */
         L     @09,APCWACB(,APCWPTR)                            02S0168
         SLR   @05,@05                                          02S0168
         ST    @05,ACBMSGAR(,@09)                               02S0168
*                   ELSE                                        02S0169
*                     DO;                                       02S0169
         B     @RC00167                                         02S0169
@RF00167 DS    0H                                               02S0170
*                       ACBMSGAR=APINAKYV;/* SECURE ADDRESS OF  02S0170
*                                      MESSAGE AREA                  */
         L     @09,APCWACB(,APCWPTR)                            02S0170
         L     @05,APINAKYV(,PARAMPTR)                          02S0170
         ST    @05,ACBMSGAR(,@09)                               02S0170
*                       PSIGNBIT=BITZERO;/* ENSURE THAT SIGNBIT IS
*                                      OFF                           */
         NI    PSIGNBIT+28(@09),B'01111111'                     02S0171
*                     END;                                      02S0172
*                   GOTO CE0001@;   /* BRANCH TO END OF CASE         */
         B     CE0001@                                          02S0173
*                 END EMSG;         /* END OF SUBCASE                */
*RBA:                                                           02S0175
*                 DO;               /* SUBCASE                       */
RBA      DS    0H                                               02S0176
*                   IF ENDATKYW=TRUE THEN                       02S0176
         TM    ENDATKYW,B'10000000'                             02S0176
         BNO   @RF00176                                         02S0176
*                     DO;           /* RESET PARAMETER ADDR'S TO NULL*/
*                       APCXACCW=ZERO;                          02S0178
         SLR   @09,@09                                          02S0178
