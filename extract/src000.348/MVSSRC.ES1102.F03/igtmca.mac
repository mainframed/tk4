*                                                               02S0252
@RF00162 DS    0H                                               02S0253
*     /***************************************************************/
*     /*                                                             */
*     /* BASED UPON 'DINFINDX' SET THE APRL RETURN CODE AND MESSAGE  */
*     /* NUMBER. 'DINFINDX' HAS BEEN SET BY CONTINUAL REFINEMENT OF  */
*     /* THE ERROR CONDITION. EXTENSIVE ANALYSIS IS PERFORMED FOR    */
*     /* UNIT-CHECK ERRORS WITH FORMAT 2 SENSE DATA (USER ERROR). IF */
*     /* THE ERROR WAS DETECTED BY THE VPSS STARTIO DRIVER (IGTKBA), */
*     /* THE APRL HAS ALREADY BEEN UPDATED BY IGTKBA. THE ECB        */
*     /* COMPLETION CODE IS X'40' IN THIS CASE.                      */
*     /*                                                             */
*     /***************************************************************/
*                                                               02S0253
*     IF APRLECBC^='40'X THEN       /* IF APRL NOT UPDATED           */
         L     @10,APCWAPRL(,APCWPTR)                           02S0253
         CLI   APRLECBC(@10),X'40'                              02S0253
         BE    @RF00253                                         02S0253
*       DO;                         /* BY IGTKBA, THEN DO NOW.       */
*         APRLRCC=APCCI;            /* ERROR COMPONENT IS ALWAYS I/O.*/
         MVI   APRLRCC(@10),X'05'                               02S0255
*         APRLRCT=CINFRCT(DINFINDX);/* RETURN CODE TYPE FROM ARRAY.
*                                      (LOGICAL/INVALID_CNTRL_BLK)   */
         LR    @09,DINFINDX                                     02S0256
         SLA   @09,3                                            02S0256
         SLR   @02,@02                                          02S0256
         IC    @02,CINFRCT-8(@09)                               02S0256
         STC   @02,APRLRCT(,@10)                                02S0256
*         APRLRCN=CINFRCN(DINFINDX);/* UNIQUE ERROR ID WITHIN   02S0257
*                                      COMPONENT (APCCIXXX).         */
         SLR   @02,@02                                          02S0257
         IC    @02,CINFRCN-8(@09)                               02S0257
         STC   @02,APRLRCN(,@10)                                02S0257
*         APRLMSGN=CINFMSGN(DINFINDX);/* IGTMBR INDEX VALUE          */
         LH    @09,CINFMSGN-8(@09)                              02S0258
         STH   @09,APRLMSGN(,@10)                               02S0258
*         APRLVALN=APCMK000;        /* NO KEYWORD SPECIFIED          */
         SLR   @09,@09                                          02S0259
         STH   @09,APRLVALN(,@10)                               02S0259
*       END;                        /* APRL UPDATED NON-IGTKBA ERR   */
*   END;                            /* APRL ERROR CODES UPDATED      */
@RF00253 DS    0H                                               02S0262
