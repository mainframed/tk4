*   DO;                             /* START OF SEGMENT              */
*                                                               02S0262
*     /***************************************************************/
*     /*                                                             */
*     /* MOVE APRL ERROR VALUES TO THE APCW. ALSO SET RETURN CODE,   */
*     /* AND TYPE OF ERROR USING 'DINFINDX' TO INDEX INTO ERROR      */
*     /* TABLE.                                                      */
*     /*                                                             */
*     /***************************************************************/
*                                                               02S0263
*     APCWR15B=APCCRERR;            /* REGISTER-15 ERROR CODE (ALWAYS
*                                      8).                           */
         MVI   APCWR15B(APCWPTR),X'08'                          02S0263
*     APCWRCC=APRLRCC;              /* ERROR COMPONENT               */
         L     @10,APCWAPRL(,APCWPTR)                           02S0264
         IC    @09,APRLRCC(,@10)                                02S0264
         STC   @09,APCWRCC(,APCWPTR)                            02S0264
*     APCWRCT=APRLRCT;              /* RETURN CODE TYPE         02S0265
*                                      (LOGICAL/INVALID_CNTRL_BLK)   */
         IC    @09,APRLRCT(,@10)                                02S0265
         STC   @09,APCWRCT(,APCWPTR)                            02S0265
*     APCWRCN=APRLRCN;              /* UNIQUE ERROR ID WITHIN   02S0266
*                                      COMPONENT (APCCIXXX).         */
         IC    @09,APRLRCN(,@10)                                02S0266
         STC   @09,APCWRCN(,APCWPTR)                            02S0266
*     APCWINTV=CINFINTV(DINFINDX);  /* TYPE OF ERROR (OVERFLOW, ETC).
*                                      DETERMINES ERROR ROUTINE TO
*                                      CALL.                         */
         LR    @09,DINFINDX                                     02S0267
         SLA   @09,3                                            02S0267
         LA    @02,CINFINTV-8(@09)                              02S0267
         MVC   APCWINTV(4,APCWPTR),0(@02)                       02S0267
*     APEMMSGN=APRLMSGN;            /* IGTMBR MESSAGE INDEX          */
         LH    @09,APRLMSGN(,@10)                               02S0268
         ST    @09,APEMMSGN(,RAPEMPTR)                          02S0268
*     APEMVALN=APRLVALN;            /* IGTMBR KEYWORD INDEX          */
         LH    @09,APRLVALN(,@10)                               02S0269
         ST    @09,APEMVALN(,RAPEMPTR)                          02S0269
