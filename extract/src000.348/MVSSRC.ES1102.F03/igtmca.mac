TMCA     TITLE ' IGTMCA -- INTERPRET I/O ERROR                         *
                        '
IGTMCA   CSECT ,                                                01S0001
@MAINENT DS    0H                                               01S0001
         USING *,@15                                            01S0001
         B     @PROLOG                                          01S0001
         DC    AL1(16)                                          01S0001
         DC    C'IGTMCA    77.231'                              01S0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                  01S0001
         BALR  @07,0                                            01S0001
@PSTART  DS    0H                                               01S0001
         USING @PSTART,@07                                      01S0001
         L     @00,@SIZDATD                                     01S0001
         IGTYGSV  R,LV=(0)
         LR    @06,@01                                          01S0001
         USING @DATD,@06                                        01S0001
         ST    @13,@SA00001+4                                   01S0001
         LM    @00,@01,20(@13)                                  01S0001
         ST    @06,8(,@13)                                      01S0001
         LR    @13,@06                                          01S0001
         EJECT
*                                                               01S0126
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE POINTERS TO MAJOR CONTROL BLOCKS                   */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0126
*   DO;                             /* START OF SEGMENT              */
*                                                               02S0126
*     /***************************************************************/
*     /*                                                             */
*     /* FIND APIO AND INSURE THAT AN ERROR HAS OCCURRED             */
*     /* (APRLECBC^='7F'X). THEN POINT TO OTHER MAJOR CONTROL BLOCKS */
*     /* TO IMPROVE PL/S GENERATED CODE.                             */
*     /*                                                             */
*     /***************************************************************/
*                                                               02S0127
*     IF APRLECBC='7F'X THEN        /* IF COMPLETION CODE = '7F'X    */
         L     @09,APCWAPRL(,APCWPTR)                           02S0127
         CLI   APRLECBC(@09),X'7F'                              02S0127
         BE    @RT00127                                         02S0127
*       RETURN;                     /* THEN NO ERROR -- SO EXIT      */
*     RFY                                                       02S0129
*       RAPIOPTR RSTD;              /* USE REGISTER POINTER          */
*     RAPIOPTR=APCWAPIO;            /* FOR APIO/IOSB TO IMPROVE      */
         L     RAPIOPTR,APCWAPIO(,APCWPTR)                      02S0130
*     RFY                                                       02S0131
*       APIO BASED(RAPIOPTR);       /* PL/S CODE.                    */
*     RFY                                                       02S0132
*       RAPEMPTR RSTD;              /* POINT TO VPSS ERROR MESSAGE   */
*     RAPEMPTR=ADDR(APEM);          /* IN THE APCW. USE REGISTER TO  */
         LA    RAPEMPTR,112                                     02S0133
         ALR   RAPEMPTR,APCWPTR                                 02S0133
*     RFY                                                       02S0134
*       APEM BASED(RAPEMPTR);       /* IMPROVE PL/S CODE.            */
*     RFY                                                       02S0135
*       RAPSNPTR RSTD;              /* POINT TO 3838 SENSE DATA      */
*     RAPSNPTR=ADDR(APSN);          /* IN THE APIO. USE REGISTER TO  */
         LA    RAPSNPTR,256                                     02S0136
         ALR   RAPSNPTR,RAPIOPTR                                02S0136
*     RFY                                                       02S0137
*       APSN BASED(RAPSNPTR);       /* IMPROVE PL/S CODE.            */
*   END;                            /* MAJOR CONTROL BLOCKS LOCATED  */
*                                                               01S0139
*   /*****************************************************************/
*   /*                                                               */
*   /* PLACE ADDRESS OF ASSOCIATED APRL IN THE PARAMETER LIST        */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0139
*   DO;                                                         02S0139
*                                                               02S0139
*     /***************************************************************/
*     /*                                                             */
*     /* PLACE THE ADDRESS OF THE ASSOCIATED APRL IN THE ERROR       */
*     /* MESSAGE.                                                    */
*     /*                                                             */
*     /***************************************************************/
*                                                               02S0140
*     IF APCWAPRL>0 THEN            /* IF APRL EXISTS, THEN          */
         LTR   @09,@09                                          02S0140
         BNP   @RF00140                                         02S0140
*       DO;                         /* MOVE TO MESSAGE.              */
*         DFIXED4=ADDR(APRL);       /* MAKE ADDRESS FIELD            */
         ST    @09,DFIXED4                                      02S0142
*         UNPK(XAPEMRL,DWORK8(1:5));/* (6 CHARACTERS)                */
         UNPK  XAPEMRL(7,RAPEMPTR),DWORK8(5)                    02S0143
*         TR(APEMAPRL,CCNVTHEX);    /* PRINTABLE HEX.                */
         TR    APEMAPRL(6,RAPEMPTR),CCNVTHEX                    02S0144
*         APEMCM17=',';             /* RESET FIELD SEPARATOR.        */
         MVI   APEMCM17(RAPEMPTR),C','                          02S0145
*       END;                        /* APRL IDENTIFIED               */
*   END;                            /* APRL ADDR IN MSG IF AVAILABLE */
@RF00140 DS    0H                                               02S0148
*                                                               01S0148
*   /*****************************************************************/
*   /*                                                               */
*   /* PLACE THE CHANNEL/UNIT NUMBER OF THE ASSOCIATED 3838 PORT IN  */
*   /* THE ERROR MESSAGE (APEM).                                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0148
*   DO;                                                         02S0148
*                                                               02S0148
*     /***************************************************************/
*     /*                                                             */
*     /* PLACE THE UNIT NUMBER (CUU) OF THE 3838 SUBCHANNEL WHICH    */
*     /* DETECTED THE ERROR IN THE ERROR MESSAGE                     */
*     /*                                                             */
*     /***************************************************************/
*                                                               02S0149
*     IF APIOAPUB->APUBID=APUBIDC THEN/* IF APIO POINTS TO APUB,     */
         L     @09,APIOAPUB(,RAPIOPTR)                          02S0149
         CLC   APUBID(4,@09),@CC02740                           02S0149
         BNE   @RF00149                                         02S0149
*       APEMUNIT=APIOAPUB->APUBUCB->UCBNAME;/* MOVE CUU FROM UCB     */
         L     @09,APUBUCB(,@09)                                02S0150
         MVC   APEMUNIT(3,RAPEMPTR),UCBNAME(@09)                02S0150
*   END;                            /* 3838 SUBCHANNEL IDENTIFIED    */
@RF00149 DS    0H                                               02S0152
*                                                               01S0152
*   /*****************************************************************/
*   /*                                                               */
*   /* BASED ON THE I/O COMPLETION CODE (APRLECBC), SET AN INITIAL   */
*   /* DESCRIPTION OF THE ERROR.                                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0152
*   DO;                             /* START OF SEGMENT              */
*                                                               02S0152
*     /***************************************************************/
*     /*                                                             */
*     /* FIND THE GENERAL CATEGORY OF ERROR BASED ON THE I/O ECB     */
*     /* COMPLETION CODE (APRLECBC). SET DINFINDX TO THE VALUE WHICH */
*     /* DESCRIBES THE ERROR, AND DFLAGS TO FURTHER PROCESSING       */
*     /* OPTIONS. IF NO MATCH IS FOUND, TREAT AS UNKNOWN. THE COMPARE*/
*     /* VALUES ARE IN THE ARRAY 'CECBTAB'.                          */
*     /*                                                             */
*     /***************************************************************/
*                                                               02S0153
*     RFY                                                       02S0153
*      (R1P,                                                    02S0153
*       R15F) RSTD;                 /* USE REGISTER POINTER TO       */
*     R1P=ADDR(APRL);               /* IMPROVE PL/S CODE             */
*                                                               02S0154
         L     R1P,APCWAPRL(,APCWPTR)                           02S0154
*     /***************************************************************/
*     /*                                                             */
*     /* SCAN 'CECBTAB'                                              */
*     /*                                                             */
*     /***************************************************************/
*                                                               02S0155
*     DO R15F=DIM(CECBTAB) TO 2 BY-1/* LOOK FOR MATCH. POINT         */
*           WHILE(R1P->APRLECBC^=CECBTECB(R15F));/* TO ENTRY-1       */
         LA    R15F,5                                           02S0155
@DL00155 LR    @14,R15F                                         02S0155
         SLA   @14,2                                            02S0155
         SLR   @09,@09                                          02S0155
         IC    @09,APRLECBC(,R1P)                               02S0155
         SLR   @02,@02                                          02S0155
         IC    @02,CECBTECB-4(@14)                              02S0155
         CR    @09,@02                                          02S0155
         BE    @DC00155                                         02S0155
*     END;                          /* IF NO MATCH (UNKNOWN COND).   */
         BCTR  R15F,0                                           02S0156
         C     R15F,@CF00099                                    02S0156
         BNL   @DL00155                                         02S0156
@DC00155 DS    0H                                               02S0157
*     RFY                                                       02S0157
*       R1P UNRSTD;                 /* FINISHED WITH APRL POINTER    */
*                                                               02S0157
*     /***************************************************************/
*     /*                                                             */
*     /* MATCH FOUND OR POINTING AT CECBTAB(1)                       */
*     /*                                                             */
*     /***************************************************************/
*                                                               02S0158
*     DFLAGS=CECBTFLG(R15F);        /* FURTHER PROCESSING FLAGS      */
         LR    @14,R15F                                         02S0158
         SLA   @14,2                                            02S0158
         LH    @10,CECBTFLG-4(@14)                              02S0158
         N     @10,@CF03412                                     02S0158
         STH   @10,DFLAGS                                       02S0158
*     DINFINDX=CECBTINF(R15F);      /* CINFTAB INDEX                 */
         SLR   DINFINDX,DINFINDX                                02S0159
         IC    DINFINDX,CECBTINF-4(@14)                         02S0159
*     RFY                                                       02S0160
*       R15F UNRSTD;                /* FINISHED WITH LOOP COUNTER    */
*   END;                            /* ECB INTERROGATED              */
*                                                               01S0162
*   /*****************************************************************/
*   /*                                                               */
*   /* FURTHER REFINEMENT OF ERROR POSSIBLE IF VALID CSW             */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0162
*   IF DFECSW=BON THEN              /* IF CSW EXISTS                 */
         TM    DFECSW,B'10000000'                               01S0162
         BNO   @RF00162                                         01S0162
*     DO;                           /* CONTINUE REFINEMENT.          */
*                                                               01S0163
*       /*************************************************************/
*       /*                                                           */
*       /* CHECK THE CSW ERROR FLAGS (CSW BYTES 4-5)                 */
*       /*                                                           */
*       /*************************************************************/
*                                                               01S0164
*       DO;                         /* START OF SEGMENT              */
*                                                               02S0164
*         /***********************************************************/
*         /*                                                         */
*         /* BASED OF THE CSW BITS SET, DEVELOP AN INFTAB INDEX AND  */
*         /* SET FURTHER PROCESSING SWITCHES. THE CSW BITS AND       */
*         /* CHECKED IN THE SAME ORDER AS THE ERP AND ONLY THE MOST  */
*         /* SEVERE ERROR REPORTED.                                  */
*         /*                                                         */
*         /***********************************************************/
*                                                               02S0165
*         DMATCH(1:2)=DMATCH(1:2)&&DMATCH(1:2);/* 0-1 NOT USED       */
         XC    DMATCH(2),DMATCH                                 02S0165
*         DMATCH(3:4)=CSWFLAGS;     /* UNIT/CHANNEL ERRORS           */
         L     @10,APIOVCSW(,RAPIOPTR)                          02S0166
         MVC   DMATCH+2(2),CSWFLAGS(@10)                        02S0166
*         DLOOKPTR=ADDR(CCSWTAB);   /* LOOKUP TABLE FOR CSW          */
         LA    DLOOKPTR,CCSWTAB                                 02S0167
*         CALL FNDMATCH;            /* LOOK FOR BIT-SET MATCH        */
         BAL   @14,FNDMATCH                                     02S0168
*       END;                        /* CSW INTERROGATED              */
*                                                               01S0170
*       /*************************************************************/
*       /*                                                           */
*       /* IF CCW ADDRESS IN THE CSW VALID, FORMAT THE CCW COMMAND   */
*       /* CODE (BYTE-0 OF THE CCW).                                 */
*       /*                                                           */
*       /*************************************************************/
*                                                               01S0170
*       DO;                         /* START OF SEGMENT              */
*                                                               02S0170
*         /***********************************************************/
*         /*                                                         */
*         /* IF THE CCW ADDRESS IN THE CSW IS VALID, SAVE THE OP-CODE*/
*         /* IN THE ERROR MESSAGE (APEM). MAKE THE COMMAND CODE      */
*         /* PRINTABLE HEX AND FOLLOW BY A 1-3 CHARACTER DESCRIPTION */
*         /* OF THE OPERATION (EG RD,WR).                            */
*         /*                                                         */
*         /***********************************************************/
*                                                               02S0171
*         IF DFCCCW=BON&            /* IF CSW CAN HAVE VALID CCW AND */
*             CSWCCW^=0 THEN        /* CCW ADDRESS NOT ZERO, THEN    */
         TM    DFCCCW,B'10000000'                               02S0171
         BNO   @RF00171                                         02S0171
         L     @10,APIOVCSW(,RAPIOPTR)                          02S0171
         L     @10,CSWCCW-1(,@10)                               02S0171
         LA    @10,0(,@10)                                      02S0171
         LTR   @10,@10                                          02S0171
         BZ    @RF00171                                         02S0171
*           DO;                     /* PRINT IT                      */
*             RFY                                               02S0173
*               R1P RSTD;           /* POINT TO LAST CCW.            */
*             R1P=CSWCCW-           /* CSW HAS NEXT CCW ADDRESS      */
*                 LENGTH(CCW);      /* SO BACK OFF BY 1 CCW          */
         SL    @10,@CF00040                                     02S0174
         LR    R1P,@10                                          02S0174
*             RFY                                               02S0175
*               CCW BASED(R1P);     /* TELL PL/S                     */
*                                                               02S0175
*             /*******************************************************/
*             /*                                                     */
*             /* OP CODE PRINTABLE HEX                               */
*             /*                                                     */
*             /*******************************************************/
*                                                               02S0176
*             IF ADDR(CCW)<APIOGMAD×/* IF FAILING CCW NOT IN         */
*                 ADDR(CCW)>=APIOGMAD+APIOGMS1/* ORIGINAL CHANNEL PGM*/
*               THEN                /* (ERP FAILED, ETC.)            */
         L     @10,APIOGMAD(,RAPIOPTR)                          02S0176
         CR    R1P,@10                                          02S0176
         BL    @RT00176                                         02S0176
         AH    @10,APIOGMS1(,RAPIOPTR)                          02S0176
         CR    R1P,@10                                          02S0176
         BL    @RF00176                                         02S0176
@RT00176 DS    0H                                               02S0177
*               APEMCCWC(1:2)='**'; /* THEN CANNOT LOCATE.           */
         MVC   APEMCCWC(2,RAPEMPTR),@CC03313                    02S0177
*             ELSE                  /* IF FAILING CCW IN THE         */
*               DO;                 /* USER'S PGM, THEN FORMAT       */
         B     @RC00176                                         02S0178
@RF00176 DS    0H                                               02S0179
*                 UNPK(APEMCCWC(1:3),CCW(1:2));/* MAKE THE 1-BYTE CMD*/
         UNPK  APEMCCWC(3,RAPEMPTR),CCW(2,R1P)                  02S0179
*                 TR(APEMCCWC(1:2),CCNVTHEX);/* CODE INTO 2     02S0180
*                                      CHARACTERS.                   */
         TR    APEMCCWC(2,RAPEMPTR),CCNVTHEX                    02S0180
*               END;                /* CMD CODE FORMATTED            */
*                                                               02S0181
*             /*******************************************************/
*             /*                                                     */
*             /* ENGLISH DESCRIPTION OP CCW OPCODE                   */
*             /*                                                     */
*             /*******************************************************/
*                                                               02S0182
*             APEMCCWC(3:3)='-';    /* SEPERATOR                     */
@RC00176 MVI   APEMCCWC+2(RAPEMPTR),C'-'                        02S0182
*             IF CCWOP=CWRITEOP THEN/* DESCRIBE OP CODE. FOLLOWING   */
         CLI   CCWOP(R1P),1                                     02S0183
         BNE   @RF00183                                         02S0183
*               APEMCCWC(4:6)='WR ';/* VALUES ALLOWED --             */
         MVC   APEMCCWC+3(3,RAPEMPTR),@CC03315                  02S0184
*             ELSE                  /* 01-WR WRITE                   */
*               IF CCWOP=CREADOP THEN/* 02-RD READ                   */
         B     @RC00183                                         02S0185
@RF00183 CLI   CCWOP(R1P),2                                     02S0185
         BNE   @RF00185                                         02S0185
*                 APEMCCWC(4:6)='RD ';/* 03-NOP CONTROL NO OPERATION */
         MVC   APEMCCWC+3(3,RAPEMPTR),@CC03316                  02S0186
*               ELSE                /* OF-PRC PROCESS (CONTROL)      */
*                 IF CCWOP=CNOPOP THEN/* ALL OTHER OPCODES ARE       */
         B     @RC00185                                         02S0187
@RF00185 CLI   CCWOP(R1P),3                                     02S0187
         BNE   @RF00187                                         02S0187
*                   APEMCCWC(4:6)='NOP';/* INVALID, AND CAN NOT      */
         MVC   APEMCCWC+3(3,RAPEMPTR),@CC03317                  02S0188
*                 ELSE              /* OCCUR. IF FOUND, USE          */
*                   IF CCWOP=CPROCOP THEN/* 'UNK' FOR UNKNOWN.       */
         B     @RC00187                                         02S0189
@RF00187 CLI   CCWOP(R1P),15                                    02S0189
         BNE   @RF00189                                         02S0189
*                     APEMCCWC(4:6)='PRC';                      02S0190
         MVC   APEMCCWC+3(3,RAPEMPTR),@CC03318                  02S0190
*                   ELSE                                        02S0191
*                     APEMCCWC(4:6)='UNK';                      02S0191
         B     @RC00189                                         02S0191
@RF00189 MVC   APEMCCWC+3(3,RAPEMPTR),@CC03319                  02S0191
*             RFY                                               02S0192
*               CCW;                /* OP CODE PROCESSED             */
@RC00189 DS    0H                                               02S0192
@RC00187 DS    0H                                               02S0192
@RC00185 DS    0H                                               02S0192
@RC00183 DS    0H                                               02S0193
*             RFY                                               02S0193
*               R1P UNRSTD;         /* RELEASE PL/S RESOURCES        */
*           END;                    /* VALID CCW COMMAND CODE PRINTED*/
*       END;                        /* CCW PROCESSED                 */
@RF00171 DS    0H                                               02S0196
*                                                               01S0196
*       /*************************************************************/
*       /*                                                           */
*       /* IF UNIT CHECK AND VALID SENSE (STANDARD USER-ERROR CASE), */
*       /* CONTINUE THE ANALYSIS BY INSPECTING BYTES 0-1 OF THE SENSE*/
*       /* DATA.                                                     */
*       /*                                                           */
*       /*************************************************************/
*                                                               01S0196
*       IF DFCUCHK=BON&             /* IF UNIT-CHECK AND             */
*           APIOVSNS=BON THEN       /* THE 3838 GAVE VALID SENSE,    */
         TM    DFCUCHK,B'01000000'                              01S0196
         BNO   @RF00196                                         01S0196
         TM    APIOVSNS(RAPIOPTR),B'01000000'                   01S0196
         BNO   @RF00196                                         01S0196
*         DO;                       /* CONTINUE PROCESSING.          */
*           DO;                     /* START OF SEGMENT              */
*                                                               02S0198
*             /*******************************************************/
*             /*                                                     */
*             /* CHECK BYTES 0-1 OF SENSE. DEVELOP AN INFTAB INDEX   */
*             /* AND SET FURTHER PROCESSING SWITCHES. THE SENSE DATA */
*             /* IS CHECKED IN THE SAME ORDER AS THE ERP AND ONLY THE*/
*             /* MOST SEVERE ERROR IS REPORTED.                      */
*             /*                                                     */
*             /*******************************************************/
*                                                               02S0199
*             DMATCH(1:2)=DMATCH(1:2)&&DMATCH(1:2);/* 0-1 NOT USED   */
         XC    DMATCH(2),DMATCH                                 02S0199
*             DMATCH(3:4)=APSN(1:2);/* CHECK STANDARD SENSE          */
         MVC   DMATCH+2(2),APSN(RAPSNPTR)                       02S0200
*             DLOOKPTR=ADDR(CSN0TAB);/* LOOKUP TABLE FOR BYTES 0-1   */
         LA    DLOOKPTR,CSN0TAB                                 02S0201
*             CALL FNDMATCH;        /* FIND BIT-MATCH ENTRY          */
         BAL   @14,FNDMATCH                                     02S0202
*           END;                    /* SENSE BYTES 0-1 CHECKED       */
*                                                               01S0204
*           /*********************************************************/
*           /*                                                       */
*           /* IF DATA DEPENDANT (USER) ERROR (FORMAT-2 SENSE),      */
*           /* CONTINUE THE REFINEMENT.                              */
*           /*                                                       */
*           /*********************************************************/
*                                                               01S0204
*           IF DFSUSER=BON&         /* IF BYTES 0-1 OF SENSE SHOW
*                                      DATA                          */
*               APSNFMT=APSNFUSR THEN/* & FORMAT-2 SENSE GIVEN,      */
         TM    DFSUSER,B'10000000'                              01S0204
         BNO   @RF00204                                         01S0204
         CLI   APSNFMT(RAPSNPTR),2                              01S0204
         BNE   @RF00204                                         01S0204
*             DO;                   /* THEN CHECK SENSE BYTES 8-11.  */
*               DO;                 /* START OF SEGMENT              */
*                                                               02S0206
*                 /***************************************************/
*                 /*                                                 */
*                 /* CHECK FORMAT-2 (DATA ERRORS) BYTES 8-11 OF THE  */
*                 /* SENSE. DEVELOP AN INFTAB INDEX AND SET FURTHER  */
*                 /* PROCESSING SWITCHES. IF MORE THAN 1 ERROR IS    */
*                 /* DETECTED, ONLY ONE IS REPORTED.                 */
*                 /*                                                 */
*                 /***************************************************/
*                                                               02S0207
*                 MVC(DMATCH,APSNFMT2);/* SENSE BYTES 8-11           */
         MVC   DMATCH(4),APSNFMT2(RAPSNPTR)                     02S0207
*                 DLOOKPTR=ADDR(CSNUTAB);/* LOOKUP TABLE USER SENSE
*                                      ERROR                         */
         LA    DLOOKPTR,CSNUTAB                                 02S0208
*                 CALL FNDMATCH;    /* CHECK BITS                    */
         BAL   @14,FNDMATCH                                     02S0209
*               END;                /* SENSE BYTES 8-11 CHECKED      */
*             END;                  /* FORMAT-2 SENSE DATA CHECKED   */
*         END;                      /* SENSE CHECKED FOR ERROR INDIC */
*                                                               01S0212
@RF00204 DS    0H                                               01S0213
*       /*************************************************************/
*       /*                                                           */
*       /* DESCRIBE THE APCI ENTRY THAT FAILED. (REQUIRES VALID      */
*       /* SENSE). AVAILABILITY OF SENSE IN APIO IF CSW VALID.       */
*       /*                                                           */
*       /*************************************************************/
*                                                               01S0213
*       DO;                         /* START OF SEGMENT              */
*                                                               02S0213
@RF00196 DS    0H                                               02S0214
*         /***********************************************************/
*         /*                                                         */
*         /* IF THE SENSE DATA DESCRIBES THE FAILING OPERATION       */
*         /* (APSNCIDS ^= 0), DETERMINE THE NUMBER OF THE ENTRY AND  */
*         /* THE USER'S MNEMONIC WHICH CREATED THE ENTRY.            */
*         /*                                                         */
*         /***********************************************************/
*                                                               02S0214
*         IF APIOVSNS=BON&          /* INSURE SENSE IS VALID         */
*             APSNCIDS>0&           /* INSURE APCI ENTRY IN SENSE    */
*             APSNCIDS<APIOCISZ THEN/* IS VALID                      */
         TM    APIOVSNS(RAPIOPTR),B'01000000'                   02S0214
         BNO   @RF00214                                         02S0214
         LH    @10,APSNCIDS(,RAPSNPTR)                          02S0214
         N     @10,@CF03412                                     02S0214
         LTR   @10,@10                                          02S0214
         BNP   @RF00214                                         02S0214
         CH    @10,APIOCISZ(,RAPIOPTR)                          02S0214
         BNL   @RF00214                                         02S0214
*           DO;                     /* IF WITHIN RANGE, THEN PROCESS */
*             RFY                                               02S0216
*              (R0P,                                            02S0216
*               R1P,                                            02S0216
*               R15F) RSTD;         /* PL/S CODE OPTIMIZATION        */
*             R1P=ADDR(APIOAPCI->APCI);/* POINT TO APCI (HEADER)     */
         L     R1P,APIOAPCI(,RAPIOPTR)                          02S0217
*             RFY                                               02S0218
*               APCI BASED(R1P);    /* TELL PL/S.                    */
*             R0P=ADDR(APCI)+APSNCIDS;/* POINT TO FAILING ENTRY      */
         ALR   @10,R1P                                          02S0219
         LR    R0P,@10                                          02S0219
*             R1P=ADDR(APCI)+APCIHLN;/* POINT TO FIRST ENTRY         */
*                                                               02S0220
         LA    R1P,4(,R1P)                                      02S0220
*             /*******************************************************/
*             /*                                                     */
*             /* COUNT NUMBER OF ENTRIES TO FAILING ONE. WHEN        */
*             /* COMPLETE, R15P SHOWS WHICH ENTRY FAILED (NTH        */
*             /* OCCURANCE OF THE OP CODE). FOR EXAMPLE IF R15=3 AND */
*             /* THE FAILING OPERATION WAS AN SVE, THIS MEANS THE 3RD*/
*             /* SVE IN THE USER'S REQUEST FAILED.                   */
*             /*                                                     */
*             /*******************************************************/
*                                                               02S0221
*             R15P=1;               /* ALSO INCLUDE FAILING ENTRY    */
         LA    R15P,1                                           02S0221
*             DO WHILE((R1P<R0P)&   /* LOOP THRU APCI ENTRIES        */
*                   (APCIOPSZ>0));  /* (INSURE NO LOOP).             */
         B     @DE00222                                         02S0222
@DL00222 DS    0H                                               02S0223
