*   /*****************************************************************/
*   /*                                                               */
*   /* SCAN THE 'DLOOKUP' TABLE TO FIND A 'DLOOKFND' WITH A BIT SET  */
*   /* THAT IS ALSO SET IS 'DMATCH'. IF A MATCH IS FOUND, SAVE THE   */
*   /* CORRESPONDING 'DLOOKINX' AND 'DLOOKFLG' VALUES IN 'DINFINDX'  */
*   /* AND 'DFLAGS'. IF A MATCH IS NOT FOUND, SET 'DFLAGS' TO ZERO,  */
*   /* BUT DO NOT CHANGE 'DINFINDX'. THE 'DLOOKUP' TABLE IS POINTED  */
*   /* TO BY 'DLOOKPTR'. THE END OF THE TABLE IS DENOTED BY A        */
*   /* 'DLOOKINX' VALUE OF ALL ZEROS.                                */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0273
*   RFY                                                         01S0273
*    (R0F,                          /* HOLDS DMATCH                  */
*     R1P) RSTD;                    /* DLOOKUP SCANNER               */
*   R0F=DMATCH;                                                 01S0274
         L     R0F,DMATCH                                       01S0274
*   RFY                                                         01S0275
*     DLOOKUP BASED(R1P);           /* SCAN TABLE BASE               */
*   DO R1P=DLOOKPTR BY LENGTH(DLOOKUP)/* SCAN DLOOKUP UNTIL          */
*         WHILE(((DMATCH&DLOOKFND)=0)&/* BIT-SET FOUND               */
*         (DLOOKFND^=0));           /* OR END OF TABLE               */
         LR    R1P,DLOOKPTR                                     01S0276
         B     @DE00276                                         01S0276
@DL00276 DS    0H                                               01S0277
*   END;                            /* CONTINUE LOOP                 */
         AL    R1P,@CF00040                                     01S0277
@DE00276 SLR   @10,@10                                          01S0277
         L     @02,DLOOKFND(,R1P)                               01S0277
         L     @15,DMATCH                                       01S0277
         NR    @15,@02                                          01S0277
         CLR   @15,@10                                          01S0277
         BNE   @DC00276                                         01S0277
         CLR   @02,@10                                          01S0277
         BNE   @DL00276                                         01S0277
@DC00276 DS    0H                                               01S0278
*   IF DLOOKFND=0 THEN              /* IF NO MATCH FOUND,            */
         SLR   @10,@10                                          01S0278
         CL    @10,DLOOKFND(,R1P)                               01S0278
         BNE   @RF00278                                         01S0278
*     DFLAGS=0;                     /* SET FLAGS TO ZERO.            */
         STH   @10,DFLAGS                                       01S0279
*   ELSE                            /* IF MATCH FOUND,               */
*     DO;                                                       01S0280
         B     @RC00278                                         01S0280
@RF00278 DS    0H                                               01S0281
*       DFLAGS=DLOOKFLG;            /* SAVE NEW FLAGS AND            */
         MVC   DFLAGS(2),DLOOKFLG(R1P)                          01S0281
*       DINFINDX=DLOOKINX;          /* IMPROVE MESSAGE RESOLUTION.   */
         SLR   DINFINDX,DINFINDX                                01S0282
         IC    DINFINDX,DLOOKINX(,R1P)                          01S0282
*     END;                                                      01S0283
*   RETURN;                                                     01S0284
@EL00002 DS    0H                                               01S0284
@EF00002 DS    0H                                               01S0284
