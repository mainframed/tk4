TMAB     TITLE 'IGTMAB -- USER/VPSS CONNECTION                         *
                        '
IGTMAB   CSECT ,                                                01S0001
@MAINENT DS    0H                                               01S0001
         USING *,@15                                            01S0001
         B     @PROLOG                                          01S0001
         DC    AL1(16)                                          01S0001
         DC    C'IGTMAB    77.093'                              01S0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                  01S0001
         BALR  @07,0                                            01S0001
@PSTART  DS    0H                                               01S0001
         USING @PSTART,@07                                      01S0001
         L     @00,@SIZDATD                                     01S0001
         GETMAIN  R,LV=(0)
         LR    @06,@01                                          01S0001
         USING @DATD,@06                                        01S0001
         ST    @13,@SA00001+4                                   01S0001
         LM    @00,@01,20(@13)                                  01S0001
         ST    @06,8(,@13)                                      01S0001
         LR    @13,@06                                          01S0001
         MVC   @PC00001(16),0(@01)                              01S0001
         MVC LINKMFLD,LINKMFL
*   R14P=CVTLINK;                   /* R14 -> DCB FOR SYS1.LINKLIB   */
         L     @15,CVTPTR                                       01S0040
         L     R14P,CVTLINK(,@15)                               01S0040
*   R1P=ADDR(SSNAME);               /* R1 -> SUBSYSTEM NAME          */
         L     R1P,@PC00001+4                                   01S0041
*   GEN(LINK DCB=(14),SF=(E,LINKMFLD))/* FIND THE SSCVT FOR          */
*   REFS(R14P,R1P,SSNAME);          /* SUBSYSTEM 'SSNAME'.           */
         LINK DCB=(14),SF=(E,LINKMFLD)
*   RFY                                                         01S0043
*    (R14P,                                                     01S0043
*     R1P) UNRSTD;                  /* FINISHED WITH R14,R1          */
*   RFY                                                         01S0044
*    (SSCTPTR) RSTD;                /* R15 -> SSCVT (OR 0)           */
*                                                               01S0044
*   /*****************************************************************/
*   /*                                                               */
*   /* SSCTPTR = ADDRESS OF SSCVT FOR SUBSYSTEM 'SSNAME'. IF NOT     */
*   /* FOUND IT IS SET TO 0. USE SSCVT TO LOCATE APCT.               */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0045
*   IF SSCTPTR=0 THEN               /* IF SSCVT NOT FOUND, THEN      */
         LTR   SSCTPTR,SSCTPTR                                  01S0045
         BNZ   @RF00045                                         01S0045
*     ERRCODE=APCCRCRT;             /* GIVE ERROR CODE               */
         LA    ERRCODE,12                                       01S0046
*   ELSE                            /* OTHERWISE R15 -> SSCVT        */
*     DO;                           /* THEREFORE, CONTINUE           */
         B     @RC00045                                         01S0047
@RF00045 DS    0H                                               01S0048
*       APCTPTR=SSCTSUSE;           /* POINT TO APCT FOR THIS SYSTEM */
         L     APCTPTR,SSCTSUSE(,SSCTPTR)                       01S0048
*       RFY                                                     01S0049
*        (SSCTPTR) UNRSTD;          /* DONE WITH REG 15              */
*       IF APCTID^=APCTIDC THEN     /* IF NOT POINTING TO APCT,      */
         CLC   APCTID(4,APCTPTR),@CC01609                       01S0050
         BE    @RF00050                                         01S0050
*         ERRCODE=APCCRCRT;         /* IS NOT VPSS SUBSYSTEM. ERROR. */
         LA    ERRCODE,12                                       01S0051
*       ELSE                        /* IF VPSS SUBSYSTEM SO CONTINUE */
*         DO;                       /* INITIALIZATION LOGIC.         */
*                                                               01S0052
         B     @RC00050                                         01S0052
@RF00050 DS    0H                                               01S0053
*           /*********************************************************/
*           /*                                                       */
*           /* APCT FOUND. INITIALIZE APUT/APAT.                     */
*           /*                                                       */
*           /*********************************************************/
*                                                               01S0053
*           DO;                     /* START OF CODE                 */
*                                                               02S0053
*             /*******************************************************/
*             /*                                                     */
*             /* LOAD APUT (MODULE IGT1VPSS) & INITIALIZE. THIS      */
*             /* MODULE ALSO CONTAINS THE APAT. SERIALIZE ACROSS THIS*/
*             /* PROCESSING TO PREVENT CONCURRENTLY EXECUTING        */
*             /* SUBTASKS FROM CREATING MULTIPLE APUT/APAT CONTROL   */
*             /* BLOCKS.                                             */
*             /*                                                     */
*             /*******************************************************/
*                                                               02S0054
*                                                               02S0054
*             /*******************************************************/
*             /*                                                     */
*             /* ISSUE EXCLUSIVE ENQ ON USERVPSS/APUT TO PREVENT     */
*             /* MULTIPLE BUILDS OF THE APUT                         */
*             /*                                                     */
*             /*******************************************************/
*                                                               02S0054
*             GEN(MVC ENQMFLD,ENQMFL)                           02S0054
*             REFS(ENQMFL) SETS(ENQMFLD);/* MOVE ENQ LIST TO DYNAMIC
*                                      AREA                          */
         MVC ENQMFLD,ENQMFL
*             ENQMJ1=ENQ1;          /* BUILD ENQ MAJOR NAME OF       */
         MVC   ENQMJ1(4),ENQ1                                   02S0055
*             ENQMJ2=APCTSSID;      /* 'USER' ×× SSNAME -- USERVPSS  */
         MVC   ENQMJ2(4),APCTSSID(APCTPTR)                      02S0056
*             GEN(ENQ (ENQMJ),MF=(E,ENQMFLD))                   02S0057
*             REFS(ENQMJ) SETS(ENQMFLD,R0F,R1F,R14F,R15F);/*    02S0057
*                                      EXCLUSIVE ENQ                 */
*                                                               02S0057
         ENQ (ENQMJ),MF=(E,ENQMFLD)
*             /*******************************************************/
*             /*                                                     */
*             /* DETERMINE IF APUT PLACED IN USER'S PARM LIST. IF SO,*/
*             /* THEN SIMULTANEOUS CALL (SUBTASKING ENVIRONMENT) HAS */
*             /* BUILD APAT/APUT. THEREFORE JUST RETURN. OTHERWISE   */
*             /* LOAD THE APUT (MODULE IGT1VPSS) AND RETURN ITS      */
*             /* ADDRESS TO THE CALLER.                              */
*             /*                                                     */
*             /*******************************************************/
*                                                               02S0058
*             IF ADRAPUT>0 THEN     /* IF USER NOW HAS APUT ADDRESS, */
         L     @15,@PC00001                                     02S0058
         L     @15,ADRAPUT(,@15)                                02S0058
         LTR   @15,@15                                          02S0058
         BNP   @RF00058                                         02S0058
*               ERRCODE=APCCROK;    /* RETURN SINCE CONTROL BLOCKS   */
         SLR   ERRCODE,ERRCODE                                  02S0059
*             ELSE                  /* BUILT ALREADY. OTHERWISE      */
*               DO;                 /* CONSTRUCT THE APUT.           */
         B     @RC00058                                         02S0060
@RF00058 DS    0H                                               02S0061
*                 DO;               /* START OF CODE                 */
*                                                               03S0061
*                   /*************************************************/
*                   /*                                               */
*                   /* LOAD & INITIALIZE THE APUT-APAT (IF NOT       */
*                   /* ALREADY BUILT). THEN CALL AN INSTALLATION EXIT*/
*                   /* TO PERFORM ANY REQUIRED INSTALLATION-DEPENDENT*/
*                   /* PROCESSING.                                   */
*                   /*                                               */
*                   /*************************************************/
*                                                               03S0062
*                   MODNAME1='IGT1';/* BUILD APUT/APAT MODULE NAME:  */
         MVC   MODNAME1(4),@CC01832                             03S0062
*                   MODNAME2=APCTSSID;/* 'IGT1'××SSNAME = 'IGT1VPSS' */
         MVC   MODNAME2(4),APCTSSID(APCTPTR)                    03S0063
*                   RFY                                         03S0064
*                    (R0P,                                      03S0064
*                     R1P) RSTD;    /* USING REG 0 & 1 FOR LOAD      */
*                   R1P=CVTLINK;    /* R1 -> DCB FOR SYS1.LINKLIB    */
         L     @15,CVTPTR                                       03S0065
         L     R1P,CVTLINK(,@15)                                03S0065
*                   GEN(LOAD EPLOC=MODNAME,DCB=(1))/* LOAD      03S0066
*                                      APUT-IGT1VPSS                 */
*                   REFS(R1P,MODNAME);/* ON RETURN, R0 -> IGT1VPSS.  */
         LOAD EPLOC=MODNAME,DCB=(1)
*                   RFY                                         03S0067
*                    (R1P) UNRSTD;  /* FINISHED WITH REGISTER 1      */
*                   RFY                                         03S0068
*                    (APUTPTR) RSTD;/* USING APUTPTR THROUGHOUT      */
*                   APUTPTR=R0P;    /* SAVE APUT ADDRESS             */
         LR    APUTPTR,R0P                                      03S0069
*                   RFY                                         03S0070
*                    (APUT) BASED(APUTPTR);/* TELL COMPILER OF  03S0070
*                                      POINTER                       */
*                   RFY                                         03S0071
*                    (R0P) UNRSTD;  /* FINISHED WITH REG 0           */
*                   APUTAPCT=ADDR(APCT);/* SAVE CONTROL TABLE ADDRESS*/
         ST    APCTPTR,APUTAPCT(,APUTPTR)                       03S0072
*                   RFY                                         03S0073
*                    (APAT) BASED(APUTAPAT);/* APUT POINTS TO APAT   */
*                   ADRAPUT=ADDR(APUT);/* RETURN APUT ADDR TO USER   */
*                                                               03S0074
         L     @10,@PC00001                                     03S0074
         ST    APUTPTR,ADRAPUT(,@10)                            03S0074
*                   /*************************************************/
*                   /*                                               */
*                   /* CALL INSTALLATION EXIT. PASS APUT & USER PARM */
*                   /* ADDRESSES.                                    */
*                   /*                                               */
*                   /*************************************************/
*                                                               03S0075
*                   CALL APCV1MAB(APUT,USER);/* CALL INSTALLATION
*                                      EXIT                          */
         ST    APUTPTR,@AL00001                                 03S0075
         L     @10,@PC00001+12                                  03S0075
         ST    @10,@AL00001+4                                   03S0075
         MVI   @AL00001+4,X'80'                                 03S0075
         L     @15,APCT1MAB(,APCTPTR)                           03S0075
         LA    @01,@AL00001                                     03S0075
         BALR  @14,@15                                          03S0075
*                   IF R15F>0 THEN  /* IF RETURN CODE 0,             */
         LTR   R15F,R15F                                        03S0076
         BNP   @RF00076                                         03S0076
*                     ERRCODE=APCCRCRT;/* THEN SHOW ERROR CONDITION. */
         LA    ERRCODE,12                                       03S0077
*                   ELSE            /* OTHERWISE SHOW THAT           */
*                     ERRCODE=APCCROK;/* INITIALIZATION OK.          */
*                                                               03S0078
         B     @RC00076                                         03S0078
@RF00076 SLR   ERRCODE,ERRCODE                                  03S0078
*                   /*************************************************/
*                   /*                                               */
*                   /* SHOW APUT/APAT NOW CONSTRUCTED.               */
*                   /*                                               */
*                   /*************************************************/
*                                                               03S0079
*                   APUTACT=BON;    /* SET ACTIVE SWITCH ON          */
@RC00076 OI    APUTACT(APUTPTR),B'10000000'                     03S0079
*                 END;              /* END OF CODE                   */
*               END;                /* INITIALIZATION COMPLETE.      */
*                                                               02S0081
*             /*******************************************************/
*             /*                                                     */
*             /* INITIALIZATION COMPLETE. RETURN ADDRESS OF FAST-PATH*/
*             /* ROUTINE (MODULE IGTMBA) TO USER. THIS MUST BE THE   */
*             /* LAST ACTION TAKEN SINCE FUTURE CALLS TO VPSS        */
*             /* (POSSIBLY BEFORE THIS ROUTINE TERMINATES) GO        */
*             /* DIRECTLY TO THE FAST-PATH ROUTINE WHICH ASSUMES THAT*/
*             /* ALL CONTROL BLOCKS HAVE BEEN BUILT.                 */
*             /*                                                     */
*             /*******************************************************/
*                                                               02S0082
*             ADRRTN=APCTZMBA;      /* ADDRESS OF INTERFACE ROUTINE  */
@RC00058 L     @10,@PC00001+8                                   02S0082
         L     @03,APCTZMBA(,APCTPTR)                           02S0082
         ST    @03,ADRRTN(,@10)                                 02S0082
*             GEN(DEQ MF=(E,ENQMFLD))/* RELEASE SERIALIZATION SINCE  */
*             REFS(ENQMFLD);        /* APUT/APAT INITIALIZED         */
         DEQ MF=(E,ENQMFLD)
*           END;                    /* APUT/APAT INITIALIZED         */
*         END;                      /* APUT/APAT BUILT               */
*     END;                          /* APUT/APAT BUILT               */
*                                                               01S0086
@RC00050 DS    0H                                               01S0087
*   /*****************************************************************/
*   /*                                                               */
*   /* APUT/APAT BUILT. ADDRESS OF VPSS FAST-PATH PROCESSOR RETURNED */
*   /* TO THE USER. FUTURE CALLS GO DIRECTLY TO THE FAST-PATH ROUTINE*/
*   /* (IGTMBA). THEREFORE RETURN TO CALLER. REGISTER 15 HAS THE     */
*   /* RETURN CODE: =0 IF NO ERROR, ^=0 IF ERROR.                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0087
*   RETURN CODE(ERRCODE);           /* RETURN TO CALLER (R15 SET)    */
@RC00045 L     @13,4(,@13)                                      01S0087
         L     @00,@SIZDATD                                     01S0087
         LR    @01,@06                                          01S0087
         FREEMAIN R,LV=(0),A=(1)
         LR    @15,@02                                          01S0087
         L     @14,12(,@13)                                     01S0087
         LM    @00,@12,20(@13)                                  01S0087
         BR    @14                                              01S0087
         EJECT
*                                   /* IGTXEPLG                      */
*                                                               01S0088
*   /*****************************************************************/
*   /*                                                               */
*   /* SYSTEM DECLARES                                               */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0088
*   DECLARE                         /* COMMON VARIABLES              */
*     I256C CHAR(256) BASED,                                    01S0088
*     I031F FIXED(31) BASED,                                    01S0088
*     I031P PTR(31) BASED,                                      01S0088
*     I015F FIXED(15) BASED,                                    01S0088
*     I015P PTR(15) BASED,                                      01S0088
*     I008P PTR(8) BASED,                                       01S0088
*     I001C CHAR(1) BASED;                                      01S0088
*                                                               01S0089
*   /*****************************************************************/
*   /*                                                               */
*   /* MODULE PATCH AREA                                             */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0089
*   DCL                                                         01S0089
*     APPATCH CHAR(*) INT STATIC LOCAL GEND;                    01S0089
*   GENERATE DATA DEFS(APPATCH);                                01S0090
*                                                               01S0091
*   /*****************************************************************/
*   /*                                                               */
*   /* GENERAL REGISTER DECLARES                                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0091
*   DCL                                                         01S0091
*     R0F FIXED(31) REG(0),                                     01S0091
*     R0P PTR(24) REG(0),                                       01S0091
*     R0RSTD PTR(31) REG(0),                                    01S0091
*     R1F FIXED(31) REG(1),                                     01S0091
*     R1P PTR(24) REG(1),                                       01S0091
*     R1RSTD PTR(31) REG(1),                                    01S0091
*     R2F FIXED(31) REG(2),                                     01S0091
*     R2P PTR(24) REG(2),                                       01S0091
*     R2RSTD PTR(31) REG(2),                                    01S0091
*     R3F FIXED(31) REG(3),                                     01S0091
*     R3P PTR(24) REG(3),                                       01S0091
*     R3RSTD PTR(31) REG(3),                                    01S0091
*     R4F FIXED(31) REG(4),                                     01S0091
*     R4P PTR(24) REG(4),                                       01S0091
*     R4RSTD PTR(31) REG(4),                                    01S0091
*     R5F FIXED(31) REG(5),                                     01S0091
*     R5P PTR(24) REG(5),                                       01S0091
*     R5RSTD PTR(31) REG(5),                                    01S0091
*     R6F FIXED(31) REG(6),                                     01S0091
*     R6P PTR(24) REG(6),                                       01S0091
*     R6RSTD PTR(31) REG(6),                                    01S0091
*     R7F FIXED(31) REG(7),                                     01S0091
*     R7P PTR(24) REG(7),                                       01S0091
*     R7RSTD PTR(31) REG(7),                                    01S0091
*     R8F FIXED(31) REG(8),                                     01S0091
*     R8P PTR(24) REG(8),                                       01S0091
*     R8RSTD PTR(31) REG(8),                                    01S0091
*     R9F FIXED(31) REG(9),                                     01S0091
*     R9P PTR(24) REG(9),                                       01S0091
*     R9RSTD PTR(31) REG(9),                                    01S0091
*     R10F FIXED(31) REG(10),                                   01S0091
*     R10P PTR(24) REG(10),                                     01S0091
*     R10RSTD PTR(31) REG(10),                                  01S0091
*     R11F FIXED(31) REG(11),                                   01S0091
*     R11P PTR(24) REG(11),                                     01S0091
*     R11RSTD PTR(31) REG(11),                                  01S0091
*     R12F FIXED(31) REG(12),                                   01S0091
*     R12P PTR(24) REG(12),                                     01S0091
*     R12RSTD PTR(31) REG(12),                                  01S0091
*     R13F FIXED(31) REG(13),                                   01S0091
*     R13P PTR(24) REG(13),                                     01S0091
*     R13RSTD PTR(31) REG(13),                                  01S0091
*     R14F FIXED(31) REG(14),                                   01S0091
*     R14P PTR(24) REG(14),                                     01S0091
*     R14RSTD PTR(31) REG(14),                                  01S0091
*     R15F FIXED(31) REG(15),                                   01S0091
*     R15P PTR(24) REG(15),                                     01S0091
*     R15RSTD PTR(31) REG(15),                                  01S0091
*                                                               01S0091
*     /***************************************************************/
*     /*                                                             */
*     /* VPSS CONTROL BLOCK POINTER DECLARES                         */
*     /*                                                             */
*     /***************************************************************/
*                                                               01S0091
*     APSMPTR PTR(24) REG(01),      /* STORAGE MANAGEMENT BLOCK 01S0091
*                                      (SUGGESTED POINTER)           */
*     APSCPTR PTR(24) REG(01),      /* STORAGE MANAGEMENT CONTROL
*                                      WORD (SUGGESTED POINTER)      */
*     APINPTR PTR(24) REG(01),      /* USER'S INPUT PARAMETER LIST
*                                      (SUGGESTED POINTER)           */
*/*                                                                  */
*                                                               01S0091
*     APDATA PTR(24) REG(06),       /* ADDRESS OF DYNAMIC DATA  01S0091
*                                      WORKAREA (DATAREG)            */
*     APCODE PTR(24) REG(07),       /* PROGRAM CODE BASE REGISTER
*                                      (CODEREG)                     */
*     APDEBPTR PTR(24) REG(08),     /* VPSS DEB (SUGGESTED POINTER)  */
*     APNPPTR PTR(24) REG(08),      /* SUBSYSTEM INITIALIZATION 01S0091
*                                      WORKAREA BLOCK (ONLY USED BY
*                                      SUBSYSTEM INITIALIZATION)     */
*     APRLPTR PTR(24) REG(08),      /* VPSS ACB REQUEST LIST    01S0091
*                                      (SUGGESTED POINTER)           */
*     APCIPTR PTR(24) REG(08),      /* 3838 CONTROL TABLE (SUGGESTED
*                                      POINTER)                      */
*     APCWPTR PTR(24) REG(08),      /* CALL INTERFACE WORKAREA  01S0091
*                                      (SUGGESTED POINTER)           */
*     APERPTR PTR(24) REG(08),      /* ERP/IO APPENDAGE WORKAREA
*                                      (SUGGESTED POINTER)           */
*     APACBPTR PTR(24) REG(09),     /* VPSS ACB (SUGGESTED POINTER)  */
*     APCQPTR PTR(24) REG(09),      /* COMMAND QUEUE ELEMENT    01S0091
*                                      (SUGGESTED POINTER)           */
*     APOTPTR PTR(24) REG(09),      /* OPTION TABLE (SUGGESTED  01S0091
*                                      POINTER)                      */
*     APIOPTR PTR(24) REG(10),      /* I/O ELEMENT (SUGGESTED   01S0091
*                                      POINTER)                      */
*     APEXLPTR PTR(24) REG(10),     /* VPSS EXLST (SUGGESTED POINTER)*/
*     APUBPTR PTR(24) REG(10),      /* UNIT BLOCK (SUGGESTED POINTER)*/
*     APAXPTR PTR(24) REG(10),      /* VPSS ACB EXTENSION (SUGGESTED
*                                      POINTER)                      */
*     APCPPTR PTR(24) REG(10),      /* VPSS CALL PROCESSING     01S0091
*                                      INDICATORS (SUGGESTED POINTER)*/
*     APOCPTR PTR(24) REG(10),      /* VPSS OPEN/CLOSE SUBSYSTEM
*                                      WORKAREA (REQUIRED POINTER)   */
*     APATPTR PTR(24) REG(11),      /* VPSS ACB TABLE (SUGGESTED
*                                      POINTER)                      */
*     APLBPTR PTR(24) REG(11),      /* LABEL RESOLUTION TABLE   01S0091
*                                      (SUGGESTED POINTER)           */
*     APUTPTR PTR(24) REG(11),      /* USER TABLE (SUGGESTED POINTER)*/
*     APDSPTR PTR(24) REG(11),      /* VPSS DATASET ENTRY (SUGGESTED
*                                      POINTER)                      */
*     APXLPTR PTR(24) REG(11),      /* TRANSLATION TABLE (SUGGESTED
*                                      POINTER)                      */
*     APDXPTR PTR(24) REG(11),      /* VPSS DEB EXTENSION (SUGGESTED
*                                      POINTER)                      */
*     APCTPTR PTR(24) REG(12) RSTD, /* CONTROL TABLE (REQUIRED  01S0091
*                                      POINTER)                      */
*     APSVPTR PTR(24) REG(13) RSTD, /* OS/VPSS SAVEAREA/WORKAREA
*                                      (REQUIRED POINTER)            */
*     APRETURN PTR(24) REG(14),     /* RETURN ADDRESS FOR SUBROUTINE
*                                      LINKAGE                       */
*     APENTRY PTR(24) REG(15),      /* SUBROUTINE ENTRY POINT LINKAGE*/
