*               MACRONAM='OPEN';    /* IDENTIFY OPEN AS MACRO RETURN */
         MVC   MACRONAM(4),@CC01829                             02S0185
*               FBYTE0=BITZERO;     /* LEAD BYTE IS ZERO             */
         MVI   FBYTE0,X'00'                                     02S0186
*               FBYTE1=ACBERFLG;    /* ACBERFLG IS MACRO RTRN CODE   */
         IC    @15,ACBERFLG(,@09)                               02S0187
         STC   @15,FBYTE1                                       02S0187
*               CALL APCVZMJI(DAPCW,ERRFLAGS);/* SET CORRECT ERROR
*                                      RTRN CODES                    */
         L     @09,@PC00001                                     02S0188
         ST    @09,@AL00001                                     02S0188
         LA    @09,ERRFLAGS                                     02S0188
         ST    @09,@AL00001+4                                   02S0188
         L     @15,APCTZMJI(,APCTPTR)                           02S0188
         LA    @01,@AL00001                                     02S0188
         BALR  @14,@15                                          02S0188
*             END;                  /* END OF PROCESSING AT OPEN FAIL*/
*           ELSE                    /* USER HAS SURVIVED; ACB IS OPEN*/
*             DO;                                               02S0190
*                                                               02S0190
         B     @RC00182                                         02S0190
@RF00182 DS    0H                                               02S0191
*               /*****************************************************/
*               /*                                                   */
*               /* ACB WAS SUCCESSFULLY OPENED                       */
*               /*                                                   */
*               /*****************************************************/
*                                                               02S0191
*               APCWR15C=APCCROK;   /* R15 CODE DENOTES SUCCESS      */
         SLR   @09,@09                                          02S0191
         ST    @09,APCWR15C(,APCWPTR)                           02S0191
*               APCWRCT=APCCTOK;    /* USER RETURN DENOTES SUCCESS   */
         MVI   APCWRCT(APCWPTR),X'00'                           02S0192
*             END;                                              02S0193
*         END;                                                  01S0194
*     RETURN;                                                   01S0195
