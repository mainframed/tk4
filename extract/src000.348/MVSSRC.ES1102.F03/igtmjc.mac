TMJC     TITLE 'IGTMJC --- OPEN AN ACB                                 *
                        '
IGTMJC   CSECT ,                                                01S0001
@MAINENT DS    0H                                               01S0001
         USING *,@15                                            01S0001
         B     @PROLOG                                          01S0001
         DC    AL1(16)                                          01S0001
         DC    C'IGTMJC    77.093'                              01S0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                  01S0001
         BALR  @07,0                                            01S0001
@PSTART  DS    0H                                               01S0001
         USING @PSTART,@07                                      01S0001
         L     @00,@SIZDATD                                     01S0001
         IGTYGSV  R,LV=(0)
         LR    @06,@01                                          01S0001
         USING @DATD,@06                                        01S0001
         ST    @13,@SA00001+4                                   01S0001
         LM    @00,@01,20(@13)                                  01S0001
         ST    @06,8(,@13)                                      01S0001
         LR    @13,@06                                          01S0001
         MVC   @PC00001(4),0(@01)                               01S0001
*/*                                                                  */
*/* END OF APSV                                                      */
*/*                                                                  */
*                                                               01S0057
*                                                               01S0057
*   /*****************************************************************/
*   /*                                                               */
*   /* OPEN A GENERATED ACB                                          */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0057
*   DO;                                                         01S0057
*                                                               01S0057
*     /***************************************************************/
*     /*                                                             */
*     /* SEE IF A DD CARD EXISTS FOR UNIT SPECIFIED                  */
*     /*                                                             */
*     /***************************************************************/
*                                                               01S0058
*     RFY                                                       01S0058
*       APCWPTR RSTD;                                           01S0058
*     APCWPTR=ADDR(DAPCW);          /* NEW POINTER FOR APCW          */
         L     APCWPTR,@PC00001                                 01S0059
*     RFY                                                       01S0060
*       APCW BASED(APCWPTR);                                    01S0060
*     RFY                                                       01S0061
*       APEM BASED(ADDR(APCWEMSG)); /* TAKE HOLD OF THE APEM         */
*     RFY                                                       01S0062
*       IFGACB BASED(APCWACB);      /* TAKE HOLD OF THE ACB          */
*     RFY                                                       01S0063
*       APCX BASED(ACBAMAX);        /* TAKE HOLD OF THE APCX         */
*     APCWZAM=BITZERO;              /* INITIALLY, NO ERROR PREVAILS  */
         NI    APCWZAM(APCWPTR),B'11111110'                     01S0064
*     DEVWORD0='00000000'X;         /* INIT. 1ST WORD OF OUTPUT AREA */
         MVC   DEVWORD0(4),@CB01799                             01S0065
*     DEVWORD1='00000000'X;         /* INIT. 2ND WORD OF OUTPUT AREA */
         MVC   DEVWORD1(4),@CB01799                             01S0066
*     RFY                                                       01S0067
*       R2P RSTD;                   /* SECURE REG 2 FOR DEVTYPE CALL */
*     R2P=ADDR(ACBDDNM);            /* OBTAIN ADDRESS OF DESIRED DDNM*/
         L     R2P,APCWACB(,APCWPTR)                            01S0068
         LA    R2P,ACBDDNM(,R2P)                                01S0068
*     GENERATE(DEVTYPE (2),DEVAREA)                             01S0069
*     SETS(R15P);                   /* CALL ON DEVTYPE               */
         DEVTYPE (2),DEVAREA
*     RFY                                                       01S0070
*       R2P UNRSTD;                 /* RELEASE REG 2 AGAIN           */
*     RFY                                                       01S0071
*       R15F RSTD;                                              01S0071
*     IF R15F^=ZERO THEN                                        01S0072
         SLR   @14,@14                                          01S0072
         CR    R15F,@14                                         01S0072
         BE    @RF00072                                         01S0072
*       DO;                         /* BEGIN PROCESS FOR ABSENT DD   */
*         RFY                                                   01S0074
*           R15F UNRSTD;                                        01S0074
*                                                               01S0074
*         /***********************************************************/
*         /*                                                         */
*         /* THERE BEING NO DD CARD, TRY DYNAMIC ALLOCATION          */
*         /*                                                         */
*         /***********************************************************/
*                                                               01S0075
*         RFY                                                   01S0075
*           APCT BASED(APCTPTR);                                01S0075
*                                                               02S0076
*         /***********************************************************/
*         /*                                                         */
*         /* ATTEMPT DYNAMIC ALLOCATION                              */
*         /*                                                         */
*         /***********************************************************/
*                                                               02S0076
*         RFY                                                   02S0076
*           S99RBPTR BASED(ADDR(RQBLKPTR));/* SET PTR AT FRONT OF
*                                      WRKAREA                       */
*         RFY                                                   02S0077
*           S99RB BASED(ADDR(RQBLK));/* SET S99RB IMMEDIATELY BEHIND */
*         S99RBPTR=ADDR(S99RB);     /* INITIALIZE REQ.BLOCK POINTER  */
         LA    @09,RQBLKPTR                                     02S0078
         LA    @03,RQBLK                                        02S0078
         ST    @03,S99RBPTR(,@09)                               02S0078
*         S99RBPND=BITONE;          /* SET BIT FOR LAST PTR TO RB'S  */
         OI    S99RBPND(@09),B'10000000'                        02S0079
*         RFY                                                   02S0080
*           S99TUPL BASED(ADDR(TUPTRS));/* SET TEXT UNIT PTRS AFTER
*                                      BLOCK                         */
*                                                               02S0080
*         /***********************************************************/
*         /*                                                         */
*         /* INTIALIZE SVC99 REQUEST BLOCK                           */
*         /*                                                         */
*         /***********************************************************/
*                                                               02S0081
*         S99RBLN=20;               /* REQ.BLOCK LENGTH IS 20 BYTES  */
         MVI   S99RBLN(@03),X'14'                               02S0081
*         S99VERB=S99VRBAL;         /* VERB CODE IS FOR DSNAME ALLOC.*/
         MVI   S99VERB(@03),X'01'                               02S0082
*         S99FLAG1=RQBFLAG1;        /* FLAGS1 DEFINED IN RQBFLAG1    */
         MVC   S99FLAG1(2,@03),RQBFLAG1                         02S0083
*         S99ERROR=ZERO;            /* ERROR REASON INITIALLY ZERO   */
         STH   @14,S99ERROR(,@03)                               02S0084
*         S99INFO=ZERO;             /* INFO. REASON INITIALLY ZERO   */
         STH   @14,S99INFO(,@03)                                02S0085
*         S99TXTPP=ADDR(TUPTRS);    /* SECURE ADDR OF TEXT POINTERS  */
         LA    @09,TUPTRS                                       02S0086
         ST    @09,S99TXTPP(,@03)                               02S0086
*         S99RSV01=ZERO;            /* PROVIDE A FULLWORD OF ZEROES  */
         ST    @14,S99RSV01(,@03)                               02S0087
*         S99FLAG2=RQBFLAG2;        /* FLAGS2 DEFINED IN RQBFLAG2
*                                      PREPARE FOR BUILDING POINTER
*                                      LIST AND TEXT UNITS.          */
*                                                               02S0088
         MVC   S99FLAG2(4,@03),RQBFLAG2                         02S0088
*         /***********************************************************/
*         /*                                                         */
*         /* 1ST TEXT UNIT IS FOR DDNAME                             */
*         /*                                                         */
*         /***********************************************************/
*                                                               02S0089
*         S99TUPTR(1)=ADDR(TXTUNITS);/* SET FIRST PTR IN PTR LIST    */
         LA    @03,TXTUNITS                                     02S0089
         ST    @03,S99TUPTR(,@09)                               02S0089
*         S99TUPLN(1)=BITZERO;      /* THIS PTR NOT LAST IN LIST     */
         NI    S99TUPLN(@09),B'01111111'                        02S0090
*         RFY                                                   02S0091
*           S99TUNIT BASED(S99TUPTR(1));                        02S0091
*         S99TUKEY=DALDDNAM;        /* KEY FOR DDNAME PARAMETER      */
         L     @03,S99TUPTR(,@09)                               02S0092
         MVC   S99TUKEY(2,@03),@CB00165                         02S0092
*         S99TUNUM=1;               /* ONLY ONE PARM FOR THIS TXTUNIT*/
         LA    @15,1                                            02S0093
         STCM  @15,3,S99TUNUM(@03)                              02S0093
*         S99TULNG=LENGTH(ACBDDNM); /* LENGTH OF DDNAME              */
         MVC   S99TULNG(2,@03),@CH00057                         02S0094
*         S99TUPAR(1:LENGTH(ACBDDNM))=ACBDDNM;/* MOVE IN THE DDNAME  */
*                                                               02S0095
         L     @14,APCWACB(,APCWPTR)                            02S0095
         MVC   S99TUPAR(8,@03),ACBDDNM(@14)                     02S0095
*         /***********************************************************/
*         /*                                                         */
*         /* 2ND TEXT UNIT IS FOR SUBSYS                             */
*         /*                                                         */
*         /***********************************************************/
*                                                               02S0096
*         S99TUPTR(2)=S99TUPTR(1)+LENGTH(S99TUKEY)+LENGTH(S99TUNUM)+
*             LENGTH(S99TULNG)+LENGTH(ACBDDNM);/* SET PTR FOR 2ND
*                                      PARM                          */
         AL    @03,@CF00521                                     02S0096
         ST    @03,S99TUPTR+4(,@09)                             02S0096
*         S99TUPLN(2)=BITZERO;      /* THIS PTR NOT LAST IN LIST     */
         NI    S99TUPLN+4(@09),B'01111111'                      02S0097
*         RFY                                                   02S0098
*           S99TUNIT BASED(S99TUPTR(2));                        02S0098
*         S99TUKEY=DALSSNM;         /* KEY FOR SUBSYS PARAMETER      */
         L     @03,S99TUPTR+4(,@09)                             02S0099
         MVC   S99TUKEY(2,@03),@CB00353                         02S0099
*         S99TUNUM=1;               /* ONLY ONE PARM FOR THIS TXTUNIT*/
         STCM  @15,3,S99TUNUM(@03)                              02S0100
*         S99TULNG=LENGTH(APCTSSID);/* LENGTH OF SUBSYSTEM ID        */
         MVC   S99TULNG(2,@03),@CH00043                         02S0101
*         S99TUPAR(1:LENGTH(APCTSSID))=APCTSSID;/* MOVE IN SUBSYS ID */
*                                                               02S0102
         MVC   S99TUPAR(4,@03),APCTSSID(APCTPTR)                02S0102
*         /***********************************************************/
*         /*                                                         */
*         /* 3RD TEXT UNIT IS FOR USER PARMS                         */
*         /*                                                         */
*         /***********************************************************/
*                                                               02S0103
*         S99TUPTR(3)=S99TUPTR(2)+LENGTH(S99TUKEY)+LENGTH(S99TUNUM)+
*             LENGTH(S99TULNG)+LENGTH(APCTSSID);/* SET PTR FOR 3RD
*                                      PARM                          */
         AL    @03,@CF00448                                     02S0103
         ST    @03,S99TUPTR+8(,@09)                             02S0103
*         S99TUPLN(3)=BITZERO;      /* THIS PTR NOT LAST IN LIST     */
         NI    S99TUPLN+8(@09),B'01111111'                      02S0104
*         RFY                                                   02S0105
*           S99TUNIT BASED(S99TUPTR(3));                        02S0105
*         S99TUKEY=DALSSPRM;        /* KEY FOR USER PARMS            */
         L     @09,S99TUPTR+8(,@09)                             02S0106
         MVC   S99TUKEY(2,@09),@CB00355                         02S0106
*         S99TUNUM=2;               /* TWO PARMS FOR THIS TEXT UNIT  */
         MVC   S99TUNUM(2,@09),@CH00067                         02S0107
*         S99TULNG=LENGTH(REGPARM); /* SHOW BYTES FOR REG PARM       */
         MVC   S99TULNG(2,@09),@CH00051                         02S0108
*         REGPARM1='REG=';          /* INSERT BEGINNING OF PARAMETER */
         MVC   REGPARM1(4),@CC01817                             02S0109
*         CVD(APCXREG,DUBLWORD);    /* CONVERT 'REG' TO DECIMAL      */
         L     @03,ACBAMAX(,@14)                                02S0110
         L     @15,APCXREG(,@03)                                02S0110
         CVD   @15,DUBLWORD                                     02S0110
*         LOWWORD=LOWWORD×'0000000F'X;/* ENSURE SIGNBYTE IS EBCDIC
*                                      ZONE                          */
         OC    LOWWORD(4),@CB01819                              02S0111
*         UNPK(REGPARM2,LOWWORD);   /* PUT EBCDIC VALUE IN REGPARM   */
         UNPK  REGPARM2(4),LOWWORD(4)                           02S0112
*         REGPARM3='K';             /* DENOTES REG AS KILOBYTES      */
         MVI   REGPARM3,C'K'                                    02S0113
*         S99TUPAR(1:LENGTH(REGPARM))=REGPARM;/* REG=XXXXK IS 1ST
*                                      PARM                          */
         MVC   S99TUPAR(9,@09),REGPARM                          02S0114
*         S99TUFP=S99TUPTR(3)+LENGTH(S99TUKEY)+LENGTH(S99TUNUM)+LENGTH(
*             S99TULNG)+LENGTH(REGPARM);/* SET PTR FOR 2ND PARM      */
         AL    @09,@CF00156                                     02S0115
         LR    S99TUFP,@09                                      02S0115
*         IF APCXOWN=BITZERO THEN   /* PARAMETER TREATMENT IS FOR SHR*/
         TM    APCXOWN(@03),B'10000000'                         02S0116
         BNZ   @RF00116                                         02S0116
*           DO;                                                 02S0117
*             S99TULEN=LENGTH(SHRPARM);/* PARM LENGTH IS OF OWN=SHR  */
         MVC   S99TULEN(2,S99TUFP),@CH00085                     02S0118
*             S99TUPRM(1:LENGTH(SHRPARM))=SHRPARM;/* '0'B IS SHARED
*                                      PORT                          */
         MVC   S99TUPRM(7,S99TUFP),@CC00088                     02S0119
*           END;                                                02S0120
*         ELSE                      /* PARAMETER TREATMENT IS FOR EXC*/
*           DO;                                                 02S0121
         B     @RC00116                                         02S0121
@RF00116 DS    0H                                               02S0122
*             S99TULEN=LENGTH(EXCPARM);/* PARM LENGTH IS OF OWN=EXC  */
         MVC   S99TULEN(2,S99TUFP),@CH00085                     02S0122
*             S99TUPRM(1:LENGTH(EXCPARM))=EXCPARM;/* '1'B IS EXCLUSV
*                                      PORT                          */
         MVC   S99TUPRM(7,S99TUFP),@CC00086                     02S0123
*           END;                                                02S0124
*                                                               02S0124
*         /***********************************************************/
*         /*                                                         */
*         /* 4TH TEXT UNIT IS FOR FREE@CLOSE                         */
*         /*                                                         */
*         /***********************************************************/
*                                                               02S0125
*         S99TUPTR(4)=S99TUPTR(3)+LENGTH(S99TUKEY)+LENGTH(S99TUNUM)+
*             LENGTH(S99TULNG)+LENGTH(REGPARM)+LENGTH(S99TULEN)+LENGTH(
*             SHRPARM);             /* SET 4TH PTR IN PTR LIST       */
@RC00116 LA    @09,TUPTRS                                       02S0125
         LA    @15,24                                           02S0125
         AL    @15,S99TUPTR+8(,@09)                             02S0125
         ST    @15,S99TUPTR+12(,@09)                            02S0125
*         IF APCXOWN=BITONE THEN    /* LENGTH OF EXCPARM NOT SHRPARM */
         L     @01,APCWACB(,APCWPTR)                            02S0126
         L     @01,ACBAMAX(,@01)                                02S0126
         TM    APCXOWN(@01),B'10000000'                         02S0126
         BNO   @RF00126                                         02S0126
*           S99TUPTR(4)=S99TUPTR(4)-LENGTH(SHRPARM)+LENGTH(EXCPARM);
         ST    @15,S99TUPTR+12(,@09)                            02S0127
*         S99TUPLN(4)=BITONE;       /* THIS PTR IS LAST IN LIST      */
@RF00126 LA    @09,TUPTRS                                       02S0128
         OI    S99TUPLN+12(@09),B'10000000'                     02S0128
*         RFY                                                   02S0129
*           S99TUNIT BASED(S99TUPTR(4));                        02S0129
*         S99TUKEY=DALCLOSE;        /* KEY FOR UNALLOCATION AT CLOSE */
         L     @09,S99TUPTR+12(,@09)                            02S0130
         MVC   S99TUKEY(2,@09),@CB00219                         02S0130
*         S99TUNUM=ZERO;            /* NO PARMS TO FOLLOW            */
*                                                               02S0131
         SLR   @15,@15                                          02S0131
         STCM  @15,3,S99TUNUM(@09)                              02S0131
*         /***********************************************************/
*         /*                                                         */
*         /* WITH DATA AREAS PREPARED, SET REG1 AND INVOKE DYNALLOC  */
*         /*                                                         */
*         /***********************************************************/
*                                                               02S0132
*         RFY                                                   02S0132
*           R1F RSTD;                                           02S0132
*         R1P=ADDR(S99RBPTR);       /* SETUP POINTER TO REQUEST BLOCK*/
         LA    R1P,RQBLKPTR                                     02S0133
*         GENERATE(DYNALLOC)                                    02S0134
*         REFS(R1P) SETS(R15F);     /* ATTEMPT ALLOCATION            */
         DYNALLOC
*         RFY                                                   02S0135
*           R1F UNRSTD;                                         02S0135
*         RFY                                                   02S0136
*           R15F RSTD;              /* SECURE DYNALLOC RETURN CODE   */
*         IF R15F^=ZERO THEN                                    02S0137
         LTR   R15F,R15F                                        02S0137
         BZ    @RF00137                                         02S0137
*           DO;                                                 02S0138
*                                                               02S0138
*             /*******************************************************/
*             /*                                                     */
*             /* ERROR CONDITION DETECTED BY DYNALLOC                */
*             /*                                                     */
*             /*******************************************************/
*                                                               02S0139
*             DYNAR15C=R15F;        /* SAVE RETURN CODE FOR IGTMJI   */
         ST    R15F,DYNAR15C                                    02S0139
*             RFY                                               02S0140
*               R15F UNRSTD;                                    02S0140
*             APCWZAM=BITONE;       /* INDICATE ERROR                */
         OI    APCWZAM(APCWPTR),B'00000001'                     02S0141
*             MACRONAM='DYNA';      /* IDENTIFY DYNALLOC AS RETURNER */
         MVC   MACRONAM(4),@CC01826                             02S0142
*             ERRRCODE=S99ERROR;    /* SECURE ERROR REASON CODE      */
         LA    @09,RQBLK                                        02S0143
         MVC   ERRRCODE(2),S99ERROR(@09)                        02S0143
*             INFOCODE=S99INFO;     /* SECURE ERROR INFO CODE        */
         MVC   INFOCODE(2),S99INFO(@09)                         02S0144
*             CALL APCVZMJI(DAPCW,ERRFLAGS);/* SET CORRECT ERROR RTRN
*                                      CODES                         */
         L     @09,@PC00001                                     02S0145
         ST    @09,@AL00001                                     02S0145
         LA    @09,ERRFLAGS                                     02S0145
         ST    @09,@AL00001+4                                   02S0145
         L     @15,APCTZMJI(,APCTPTR)                           02S0145
         LA    @01,@AL00001                                     02S0145
         BALR  @14,@15                                          02S0145
*           END;                    /* END OF THE ROPE - NO DYNALLOC */
*         ELSE                      /* NO DYNALLOC ERROR; TRY AN OPEN*/
*           DO;                                                 02S0147
*                                                               02S0147
         B     @RC00137                                         02S0147
@RF00137 DS    0H                                               02S0148
*             /*******************************************************/
*             /*                                                     */
*             /* HAVING ALLOCATED, TRY AN OPEN OF THE ACB.           */
*             /*                                                     */
*             /*******************************************************/
*                                                               02S0148
*                                                               03S0148
*             /*******************************************************/
*             /*                                                     */
*             /* ATTEMPT AN OPEN OF THE ACB                          */
*             /*                                                     */
*             /*******************************************************/
*                                                               03S0148
*             ACBERFLG=BITZERO;     /* INITIALIZE ERROR CODE TO ZERO */
         L     @09,APCWACB(,APCWPTR)                            03S0148
         MVI   ACBERFLG(@09),B'00000000'                        03S0148
*             RFY                                               03S0149
*               R11P RSTD;          /* SECURE R11 FOR OPEN MACRO     */
*             R11P=APCWACB;         /* PROVIDE ADDRESS OF ACB        */
         LR    R11P,@09                                         03S0150
*             GENERATE              /* EXECUTE AN OPEN OF THE ACB    */
*                 REFS(R11P,OPENERS) SETS(OPENERA,R0F,R1F,R11P);
*                                      /*
     MVC   OPENERA(OPENEND),OPENERS
     OPEN ((R11P)),MF=(E,OPENERA)
*             RFY                                               03S0152
*               R11P UNRSTD;        /* GIVE BACK REGISTER 11         */
*             IF ACBOPEN=BITZERO THEN                           03S0153
         L     @09,APCWACB(,APCWPTR)                            03S0153
         TM    ACBOPEN(@09),B'00010000'                         03S0153
         BNZ   @RF00153                                         03S0153
*               DO;                                             03S0154
*                                                               03S0154
*                 /***************************************************/
*                 /*                                                 */
*                 /* ACB WAS NOT OPENED                              */
*                 /*                                                 */
*                 /***************************************************/
*                                                               03S0155
*                 APCWZAM=BITONE;   /* INDICATE ERROR                */
         OI    APCWZAM(APCWPTR),B'00000001'                     03S0155
*                 MACRONAM='OPEN';  /* IDENTIFY OPEN AS MACRO RETURN */
         MVC   MACRONAM(4),@CC01829                             03S0156
*                 FBYTE0=BITZERO;   /* LEAD BYTE IS ZERO             */
         MVI   FBYTE0,X'00'                                     03S0157
*                 FBYTE1=ACBERFLG;  /* ACBERFLG IS MACRO RTRN CODE   */
         IC    @15,ACBERFLG(,@09)                               03S0158
         STC   @15,FBYTE1                                       03S0158
*                 CALL APCVZMJI(DAPCW,ERRFLAGS);/* SET CORRECT ERROR
*                                      RTRN CODES                    */
         L     @09,@PC00001                                     03S0159
         ST    @09,@AL00001                                     03S0159
         LA    @09,ERRFLAGS                                     03S0159
         ST    @09,@AL00001+4                                   03S0159
         L     @15,APCTZMJI(,APCTPTR)                           03S0159
         LA    @01,@AL00001                                     03S0159
         BALR  @14,@15                                          03S0159
*               END;                /* END OF PROCESSING AT OPEN FAIL*/
*             ELSE                  /* USER HAS SURVIVED; ACB IS OPEN*/
*               DO;                                             03S0161
*                                                               03S0161
         B     @RC00153                                         03S0161
@RF00153 DS    0H                                               03S0162
*                 /***************************************************/
*                 /*                                                 */
*                 /* ACB WAS SUCCESSFULLY OPENED                     */
*                 /*                                                 */
*                 /***************************************************/
*                                                               03S0162
*                 APCWR15C=APCCROK; /* R15 CODE DENOTES SUCCESS      */
         SLR   @09,@09                                          03S0162
         ST    @09,APCWR15C(,APCWPTR)                           03S0162
*                 APCWRCT=APCCTOK;  /* USER RETURN DENOTES SUCCESS   */
         MVI   APCWRCT(APCWPTR),X'00'                           03S0163
*               END;                                            03S0164
*           END;                    /* END OF CODE FOR GOOD DYNALLOC */
*       END;                        /* END PROCESS FOR MISSING DD    */
*     ELSE                                                      01S0167
*                                                               01S0167
*       /*************************************************************/
*       /*                                                           */
*       /* DD CARD NOT MISSING                                       */
*       /*                                                           */
*       /*************************************************************/
*                                                               01S0167
*       IF DEVWORD0^='00000102'X THEN                           01S0167
         B     @RC00072                                         01S0167
@RF00072 CLC   DEVWORD0(4),@CB01830                             01S0167
         BE    @RF00167                                         01S0167
*         DO;                                                   01S0168
*                                                               01S0168
*           /*********************************************************/
*           /*                                                       */
*           /* DD CARD LACKS CORRECT PARM'S                          */
*           /*                                                       */
*           /*********************************************************/
*                                                               01S0169
*           APCWZAM=BITONE;         /* INDICATE ERROR                */
         OI    APCWZAM(APCWPTR),B'00000001'                     01S0169
*           APCWRCT=APCCTLOG;       /* DENOTE ERROR WAS LOGICAL      */
         MVI   APCWRCT(APCWPTR),X'08'                           01S0170
*           APCWRCC=APCCG;          /* ERROR FOUND BY SUBSYSTEM      */
         MVI   APCWRCC(APCWPTR),X'03'                           01S0171
*           APCWR15C=APCCRERR;      /* PROVIDE R15 ERROR CODE        */
         MVC   APCWR15C(4,APCWPTR),@CF00057                     01S0172
*           APCWRCN=APCCG006;       /* SELECT PROPER USER RETURN CODE*/
         MVI   APCWRCN(APCWPTR),X'06'                           01S0173
*           APEMMSGN=APCMG006;      /* INDICATE APPROPRIATE MESSAGE  */
         MVC   APEMMSGN+112(4,APCWPTR),@CF00519                 01S0174
*         END;                                                  01S0175
*       ELSE                                                    01S0176
*         DO;                                                   01S0176
*                                                               01S0176
         B     @RC00167                                         01S0176
@RF00167 DS    0H                                               01S0177
*           /*********************************************************/
*           /*                                                       */
*           /* HAVING GOOD DD CARD, TRY OPEN                         */
*           /*                                                       */
