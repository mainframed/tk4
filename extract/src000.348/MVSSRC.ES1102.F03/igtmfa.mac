*CDBRANCH:                                                      02S0162
*       DO;                         /* SUBCASE                       */
CDBRANCH DS    0H                                               02S0163
*/************************************************************       */
*/*                                                                  */
*/* THIS CASE BUILDS A CIT ENTRY FOR CONDITIONAL BRANCH              */
*/* LOGIC OPERATIONS. THE CALL FORMAT FOR A CONDITIONAL              */
*/* BRANCH IS:                                                       */
*/*                                                                  */
*/*     CALL VPSS(IUNIT,IFUNC,IVAL1,IVAL2,LABEL,COND)                */
*/*                                                                  */
*/*     WHERE:                                                       */
*/*                                                                  */
*/*       IUNIT - UNIT REFERENCE NUMBER                              */
*/*       IFUNC - FUNCTION MNEMONIC AS FOLLOWS -                     */
*/*                 'XC  ' - COMPARE INDEX : INDEX                   */
*/*                 'XCI ' - COMPARE INDEX : IMMEDIATE DATA          */
*/*                 'XDC ' - DECREMENT AND COMPARE                   */
*/*                          INDEX : INDEX                           */
*/*                 'XDCI' - DECREMENT AND COMPARE                   */
*/*                          INDEX : IMMEDIATE                       */
*/*                 'XCF ' - FLOATING-POINT COMPARE                  */
*/*                          INDEX : INDEX                           */
*/*                 'XCFI' - FLOATING-POINT COMPARE                  */
*/*                          INDEX : IMMEDIATE                       */
*/*       IVAL1 - INDEX ID                                           */
*/*       IVAL2 - INDEX ID IF 'XC  ','XDC ' OR 'XCF '                */
*/*               IMMEDIATE DATA IF 'XCI ','XDCI' OR 'XCFI'          */
*/*       LABEL - BRANCH TO LOCATION - DEFINED BY 'XID '             */
*/*       COND  - CONDITION FOR WHICH BRANCH IS TO BE TAKEN          */
*/*               IF TRUE. COND MAY BE ONE OF THE FOLLOWING -        */
*/*                 'EQ  ' - EQUAL                                   */
*/*                 'GT  ' - GREATER THAN                            */
*/*                 'GE  ' - GREATER THAN OR EQUAL                   */
*/*                 'NE  ' - NOT EQUAL                               */
*/*                 'LT  ' - LESS THAN                               */
*/*                 'LE  ' - LESS THAN OR EQUAL                      */
*/*                                                                  */
*/* THE CIT FORMAT FOR CONDITIONAL BRANCH OPERATIONS IS:             */
*/*                                                                  */
*/*      <--------------- 4 BYTES ---------------->                  */
*/*      ------------------------------------------                  */
*/*      ×   OPERATION ID    ×ENTRY LENGTH(HWORDS)×                  */
*/*      ------------------------------------------                  */
*/*      ×   FIRST INDEX ID  ×2ND INDEX ID/UNUSED ×                  */
*/*      ------------------------------------------                  */
*/*      ×    IMMEDIATE DATA OR UNUSED            ×                  */
*/*      ------------------------------------------                  */
*/*      ×  CONDITION MASK   ×REL CIT OFFSET-HWORD×                  */
*/*      ------------------------------------------                  */
*/*                                                                  */
*/*   CONDITION MASK MAY BE:                                         */
*/*     X'0008' - EQUAL                                              */
*/*     X'FFF7' - NOT EQUAL                                          */
*/*     X'0002' - (FIRST) INDEX LOW                                  */
*/*     X'FFFD' - (FIRST) INDEX NOT LOW                              */
*/*     X'FFF5' - (FIRST) INDEX HIGH                                 */
*/*     X'000A' - (FIRST) INDEX NOT HIGH                             */
*/*     X'0000' - NEVER BRANCH                                       */
*/*     X'FFFF' - UNCONDITIONAL BRANCH                               */
*/*                                                                  */
*/************************************************************       */
*                                                               02S0163
*         DO;                                                   02S0163
*           IF(APINXI1>=1)&(APINXI1<=15) THEN/* TEST IF IVAL1 IS A
*                                      VALID INDEX ID                */
         LA    @04,1                                            02S0164
         L     @15,APCWAPIN(,APCWPTR)                           02S0164
         L     @01,APINAXI1(,@15)                               02S0164
         L     @14,APINXI1(,@01)                                02S0164
         CR    @14,@04                                          02S0164
         BL    @RF00164                                         02S0164
         LA    @11,15                                           02S0164
         CR    @14,@11                                          02S0164
         BH    @RF00164                                         02S0164
*             DO;                                               02S0165
*               APCIXID1=APINXI1;   /* YES - STORE IN CIT            */
         LA    @10,CITENTRY                                     02S0166
         STH   @14,APCIXID1(,@10)                               02S0166
*               IF APALLI2X=ON THEN /* TEST IF IVAL2 SHOULD BE AN
*                                      INDEX ID                      */
         L     @01,APCWAPAL(,APCWPTR)                           02S0167
         TM    APALLI2X(@01),B'10000000'                        02S0167
         BNO   @RF00167                                         02S0167
*                 DO;               /* YES                           */
*                   IF(APINXI2>=1)&(APINXI2<=15) THEN/* TEST IF IVAL2
*                                      IS A VALID INDEX ID           */
         L     @01,APINAXI2(,@15)                               02S0169
         L     @15,APINXI2(,@01)                                02S0169
         CR    @15,@04                                          02S0169
         BL    @RF00169                                         02S0169
         CR    @15,@11                                          02S0169
         BH    @RF00169                                         02S0169
*                     APCIXID2=APINXI2;/* YES - STORE IN CIT         */
         STH   @15,APCIXID2(,@10)                               02S0170
*                   ELSE                                        02S0171
*                     DO;           /* NO - SET ERROR INDICATORS     */
         B     @RC00169                                         02S0171
@RF00169 DS    0H                                               02S0172
*                       ERRFLAG=ON; /* SET ERROR FLAG                */
         LA    ERRFLAG,1                                        02S0172
*                       APCWRCN=APCCX002;/* SET ERROR NUMBER         */
         MVI   APCWRCN(APCWPTR),X'02'                           02S0173
*                       APEMMSGN=APCMX002;/* SET MESSAGE NUMBER FOR
*                                      OUT OF RANGE                  */
         LA    @04,112                                          02S0174
         ALR   @04,APCWPTR                                      02S0174
         MVC   APEMMSGN(4,@04),@CF00628                         02S0174
*                       APEMVALN=APCMK039;/* SET KEYWORD NUMBER FOR
*                                      IVAL2                         */
         MVC   APEMVALN(4,@04),@CF01150                         02S0175
*                     END;                                      02S0176
*                 END;                                          02S0177
*               ELSE                /* IVAL2 IS IMMEDIATE DATA       */
*                 APCIXIMM=APINXI2; /* STORE INTO CIT                */
*                                                               02S0178
         B     @RC00167                                         02S0178
@RF00167 LA    @04,CITENTRY                                     02S0178
         L     @01,APCWAPIN(,APCWPTR)                           02S0178
         L     @01,APINAXI2(,@01)                               02S0178
         L     @15,APINXI2(,@01)                                02S0178
         ST    @15,APCIXIMM(,@04)                               02S0178
*               /*****************************************************/
*               /*                                                   */
*               /* DETERMINE THE BRANCH CONDITION REQUESTED BY THE   */
*               /* COND PARAMETER AND SET THE CORRECT CONDITION MASK */
*               /* IN THE CIT ENTRY                                  */
*               /*                                                   */
*               /*****************************************************/
*                                                               02S0179
*               IF APINXCD='EQ  ' THEN/* EQUAL?                      */
@RC00167 L     @04,APCWAPIN(,APCWPTR)                           02S0179
         L     @04,APINAXCD(,@04)                               02S0179
         CLC   APINXCD(4,@04),@CC01867                          02S0179
         BNE   @RF00179                                         02S0179
*                 APCIXCC=APCIXCEQ; /* STORE EQ CONDITION MASK       */
         LA    @04,CITENTRY                                     02S0180
         MVC   APCIXCC(2,@04),@CB00456                          02S0180
*               IF APINXCD='NE  ' THEN/* NOT EQUAL?                  */
@RF00179 L     @04,APCWAPIN(,APCWPTR)                           02S0181
         L     @04,APINAXCD(,@04)                               02S0181
         CLC   APINXCD(4,@04),@CC01868                          02S0181
         BNE   @RF00181                                         02S0181
*                 APCIXCC=APCIXCNE; /* STORE NE CONDITION MASK       */
         LA    @04,CITENTRY                                     02S0182
         MVC   APCIXCC(2,@04),@CB00458                          02S0182
*               IF APINXCD='LT  ' THEN/* LESS THAN?                  */
@RF00181 L     @04,APCWAPIN(,APCWPTR)                           02S0183
         L     @04,APINAXCD(,@04)                               02S0183
         CLC   APINXCD(4,@04),@CC01869                          02S0183
         BNE   @RF00183                                         02S0183
*                 APCIXCC=APCIXCLT; /* STORE LT CONDITION MASK       */
         LA    @04,CITENTRY                                     02S0184
         MVC   APCIXCC(2,@04),@CB00460                          02S0184
*               IF APINXCD='LE  ' THEN/* LESS THAN OR EQUAL?         */
@RF00183 L     @04,APCWAPIN(,APCWPTR)                           02S0185
         L     @04,APINAXCD(,@04)                               02S0185
         CLC   APINXCD(4,@04),@CC01870                          02S0185
         BNE   @RF00185                                         02S0185
*                 APCIXCC=APCIXCLE; /* STORE LE CONDITION MASK       */
         LA    @04,CITENTRY                                     02S0186
         MVC   APCIXCC(2,@04),@CB00464                          02S0186
*               IF APINXCD='GT  ' THEN/* GREATER THAN?               */
@RF00185 L     @04,APCWAPIN(,APCWPTR)                           02S0187
         L     @04,APINAXCD(,@04)                               02S0187
         CLC   APINXCD(4,@04),@CC01871                          02S0187
         BNE   @RF00187                                         02S0187
*                 APCIXCC=APCIXCGT; /* STORE GT CONDITION MASK       */
         LA    @04,CITENTRY                                     02S0188
         MVC   APCIXCC(2,@04),@CB00466                          02S0188
*               IF APINXCD='GE  ' THEN/* GREATER THAN OR EQUAL?      */
@RF00187 L     @04,APCWAPIN(,APCWPTR)                           02S0189
         L     @04,APINAXCD(,@04)                               02S0189
         CLC   APINXCD(4,@04),@CC01872                          02S0189
         BNE   @RF00189                                         02S0189
*                 APCIXCC=APCIXCGE; /* STORE GE CONDITION MASK       */
*                                                               02S0190
         LA    @04,CITENTRY                                     02S0190
         MVC   APCIXCC(2,@04),@CB00462                          02S0190
*               /*****************************************************/
*               /*                                                   */
*               /* TEST IF THE COND PARAMETER WAS VALID. IF NOT SET  */
*               /* UP ERROR INDICATORS.                              */
*               /*                                                   */
*               /*****************************************************/
*                                                               02S0191
*               IF APCIXCC=0 THEN   /* TEST IF COND VALID            */
@RF00189 LA    @04,CITENTRY                                     02S0191
         LH    @04,APCIXCC(,@04)                                02S0191
         N     @04,@CF01966                                     02S0191
         LTR   @04,@04                                          02S0191
         BNZ   @RF00191                                         02S0191
*                 DO;               /* NO - SET ERROR INDICATORS     */
*                   ERRFLAG=ON;     /* INDICATE ERROR                */
         LA    ERRFLAG,1                                        02S0193
*                   APCWRCN=APCCX006;/* SET ERROR NUMBER             */
         MVI   APCWRCN(APCWPTR),X'06'                           02S0194
*                   APEMMSGN=APCMX006;/* SET MESSAGE NUMBER FOR 02S0195
*                                      INVALID VALUE                 */
         LA    @04,112                                          02S0195
         ALR   @04,APCWPTR                                      02S0195
         MVC   APEMMSGN(4,@04),@CF01059                         02S0195
*                   APEMVALN=APCMK037;/* SET KEYWORD NUMBER FOR COND */
         MVC   APEMVALN(4,@04),@CF01146                         02S0196
*                 END;                                          02S0197
*               ELSE                /* COND OKAY - CONTINUE          */
*                                                               02S0198
*                 /***************************************************/
*                 /*                                                 */
*                 /* SEARCH THE LABEL TABLE TO SEE IF THIS LABEL HAS */
*                 /* BEEN DEFINED. IF SO, GET THE BRANCH TO OFFSET   */
*                 /* AND INSERT IT INTO THE CIT ENTRY.               */
*                 /*                                                 */
*                 /***************************************************/
*                                                               02S0198
*                 DO;                                           02S0198
         B     @RC00191                                         02S0198
@RF00191 DS    0H                                               02S0199
*                   LOOPCTRL=OFF;   /* INITIALIZE LOOP CONTROL FLAG
*                                      TO OFF                        */
         SLR   @11,@11                                          02S0199
         LR    LOOPCTRL,@11                                     02S0199
*                   GOODONE=OFF;    /* INITIALIZE FLAG               */
         LR    GOODONE,@11                                      02S0200
*                   DO I=1 TO APLBNUMB WHILE LOOPCTRL=OFF;/* LOOP
*                                      THRU APLB LOOKING FOR A  02S0201
*                                      DEFINITION OF THIS LABEL      */
         LA    I,1                                              02S0201
         B     @DE00201                                         02S0201
@DL00201 LTR   LOOPCTRL,LOOPCTRL                                02S0201
         BNZ   @DC00201                                         02S0201
*                     IF(APLBGOOD(I)=ON)&(APLBDEF(I)=ON)&(APLBLBL(I)=
*                         APINXLB) THEN/* TEST IF LABEL DEFINED      */
         LR    @11,I                                            02S0202
         SLA   @11,3                                            02S0202
         L     @04,APCWACB(,APCWPTR)                            02S0202
         L     @04,ACBAMWAP(,@04)                               02S0202
         L     @04,APAXAPLB(,@04)                               02S0202
         ST    @11,@TF00001                                     02S0202
         ALR   @11,@04                                          02S0202
         TM    APLBGOOD-8(@11),B'11000000'                      02S0202
         BNO   @RF00202                                         02S0202
         L     @11,APCWAPIN(,APCWPTR)                           02S0202
         L     @11,APINAXLB(,@11)                               02S0202
         LR    @01,@04                                          02S0202
         AL    @01,@TF00001                                     02S0202
         CLC   APLBLBL-8(4,@01),APINXLB(@11)                    02S0202
         BNE   @RF00202                                         02S0202
*                       DO;         /* YES - GET CIT OFFSET          */
*                         APCIXBRO=APLBLOC(I);/* INSERT INTO CIT     */
         LA    @11,CITENTRY                                     02S0204
         L     @01,@TF00001                                     02S0204
         LH    @04,APLBLOC-8(@01,@04)                           02S0204
         STH   @04,APCIXBRO(,@11)                               02S0204
*                         LOOPCTRL=ON;/* END SCAN                    */
         LA    @11,1                                            02S0205
         LR    LOOPCTRL,@11                                     02S0205
*                         GOODONE=ON;/* MISSION ACCOMPLISHED         */
         LR    GOODONE,@11                                      02S0206
*                       END;                                    02S0207
*                     ELSE                                      02S0208
*                       ;                                       02S0208
@RF00202 DS    0H                                               02S0209
*                   END;                                        02S0209
*                                                               02S0209
         AL    I,@CF00061                                       02S0209
@DE00201 L     @11,APCWACB(,APCWPTR)                            02S0209
         L     @11,ACBAMWAP(,@11)                               02S0209
         L     @11,APAXAPLB(,@11)                               02S0209
         C     I,APLBNUMB(,@11)                                 02S0209
         BNH   @DL00201                                         02S0209
@DC00201 DS    0H                                               02S0210
*                   /*************************************************/
*                   /*                                               */
*                   /* IF THE LABEL WAS NOT DEFINED, INSERT A        */
*                   /* REFERENCE ENTRY INTO THE LABEL TABLE SO THE   */
*                   /* BRANCH CAN BE RESOLVED WHEN DEFINED VIA XID.  */
*                   /*                                               */
*                   /*************************************************/
*                                                               02S0210
*                   IF GOODONE=OFF THEN/* TEST IF LABEL WAS DEFINED  */
         SLR   @11,@11                                          02S0210
         CR    GOODONE,@11                                      02S0210
         BNE   @RF00210                                         02S0210
*                     DO;           /* NO                            */
*                       LOOPCTRL=OFF;/* INITIALIZE FLAG              */
         LR    LOOPCTRL,@11                                     02S0212
*                       DO I=1 TO APLBNUMB WHILE LOOPCTRL=OFF;/* FIND
*                                      AN UNUSED ENTRY AND USE IT    */
         LA    I,1                                              02S0213
         B     @DE00213                                         02S0213
@DL00213 LTR   LOOPCTRL,LOOPCTRL                                02S0213
         BNZ   @DC00213                                         02S0213
*                         IF APLBGOOD(I)=OFF THEN/* TEST IF ENTRY IN
*                                      USE                           */
         LR    @11,I                                            02S0214
         SLA   @11,3                                            02S0214
         L     @04,APCWACB(,APCWPTR)                            02S0214
         L     @04,ACBAMWAP(,@04)                               02S0214
         L     @15,APAXAPLB(,@04)                               02S0214
         ST    @11,@TF00001                                     02S0214
         ALR   @11,@15                                          02S0214
         TM    APLBGOOD-8(@11),B'10000000'                      02S0214
         BNZ   @RF00214                                         02S0214
*                           DO;     /* NO                            */
*                             APLBGOOD(I)=ON;/* SHOW ENTRY AS VALID  */
         LR    @11,@15                                          02S0216
         AL    @11,@TF00001                                     02S0216
         OI    APLBGOOD-8(@11),B'10000000'                      02S0216
*                             APLBLBL(I)=APINXLB;/* INSERT LABEL     */
         LR    @11,@15                                          02S0217
         AL    @11,@TF00001                                     02S0217
         L     @01,APCWAPIN(,APCWPTR)                           02S0217
         L     @01,APINAXLB(,@01)                               02S0217
         MVC   APLBLBL-8(4,@11),APINXLB(@01)                    02S0217
*                             APLBDEF(I)=OFF;/* MARK AS REFERENCE    */
         LR    @11,@15                                          02S0218
         AL    @11,@TF00001                                     02S0218
         NI    APLBDEF-8(@11),B'10111111'                       02S0218
*                             APLBLOC(I)=(APAXCIN-APAXCIS)/2;/* 02S0219
*                                      INSERT HALFWORD OFFSET OF THIS
*                                      CIT ENTRY                     */
         L     @00,APAXCIN(,@04)                                02S0219
         SL    @00,APAXCIS(,@04)                                02S0219
         SRDA  @00,32                                           02S0219
         D     @00,@CF00064                                     02S0219
         L     @11,@TF00001                                     02S0219
         STH   @01,APLBLOC-8(@11,@15)                           02S0219
*                             LOOPCTRL=ON;/* GET OUT OF LOOP         */
         LA    @11,1                                            02S0220
         LR    LOOPCTRL,@11                                     02S0220
*                             GOODONE=ON;/* INDICATE SUCCESS         */
         LR    GOODONE,@11                                      02S0221
*                           END;                                02S0222
*                         ELSE                                  02S0223
*                           ;                                   02S0223
@RF00214 DS    0H                                               02S0224
*                       END;                                    02S0224
         AL    I,@CF00061                                       02S0224
@DE00213 L     @11,APCWACB(,APCWPTR)                            02S0224
         L     @11,ACBAMWAP(,@11)                               02S0224
         L     @11,APAXAPLB(,@11)                               02S0224
         C     I,APLBNUMB(,@11)                                 02S0224
         BNH   @DL00213                                         02S0224
@DC00213 DS    0H                                               02S0225
*                     END;                                      02S0225
*                   ELSE                                        02S0226
*                     ;                                         02S0226
*                                                               02S0226
@RF00210 DS    0H                                               02S0227
*                   /*************************************************/
*                   /*                                               */
*                   /* IF UNABLE TO MAKE ENTRY INTO THE LABEL TABLE, */
*                   /* IT IS FULL AND AN ERROR EXISTS.               */
*                   /*                                               */
*                   /*************************************************/
