         LA    @04,112                                          02S0231
         ALR   @04,APCWPTR                                      02S0231
         MVC   APEMMSGN(4,@04),@CF01150                         02S0231
*                       APEMVALN=APCMK051;/* SET KEYWORD NUMBER FOR
*                                      APLB                          */
         MVC   APEMVALN(4,@04),@CF01173                         02S0232
*                     END;                                      02S0233
*                   ELSE                                        02S0234
