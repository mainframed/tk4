@RF00227 DS    0H                                               02S0235
*                 END;                                          02S0235
*             END;                                              02S0236
*           ELSE                    /* IVAL1 INVALID                 */
*             DO;                                               02S0237
         B     @RC00164                                         02S0237
@RF00164 DS    0H                                               02S0238
*               ERRFLAG=ON;         /* SET ERROR FLAG                */
         LA    ERRFLAG,1                                        02S0238
*               APCWRCN=APCCX001;   /* SET ERROR NUMBER              */
         MVI   APCWRCN(APCWPTR),X'01'                           02S0239
*               APEMMSGN=APCMX001;  /* SET MESSAGE NUMBER FOR OUT OF
*                                      RANGE                         */
         LA    @04,112                                          02S0240
         ALR   @04,APCWPTR                                      02S0240
         MVC   APEMMSGN(4,@04),@CF00628                         02S0240
*               APEMVALN=APCMK038;  /* SET KEYWORD VALUE FOR IVAL2   */
         MVC   APEMVALN(4,@04),@CF01148                         02S0241
*             END;                                              02S0242
*         END;                                                  02S0243
*         GOTO CE0001@;             /* BRANCH TO END OF CASE         */
         B     CE0001@                                          02S0244
*       END CDBRANCH;               /* END OF SUBCASE                */
*XGO:                                                           02S0246
*       DO;                         /* SUBCASE                       */
XGO      DS    0H                                               02S0247
*/* THIS CASE BUILDS A CIT ENTRY FOR THE UNCONDITIONAL               */
*/* BRANCH OPERATION. THE CALL FORMAT FOR AN UNCONDITIONAL           */
*/* BRANCH IS:                                                       */
*/*                                                                  */
*/*       CALL VPSS(IUNIT,'XGO ',LABEL)                              */
*/*                                                                  */
*/*       WHERE:                                                     */
*/*                                                                  */
*/*         IUNIT - UNIT REFERENCE NUMBER                            */
*/*         XGO   - FUNCTION MNEMONIC                                */
*/*         LABEL - BRANCH TO LOCATION - DEFINED BY 'XID '           */
*/*                                                                  */
*/* THE CIT FORMAT FOR AN UNCONDITIONAL BRANCH IS:                   */
*/*                                                                  */
*/*       <-------------- 4 BYTES ----------------->                 */
*/*       ------------------------------------------                 */
*/*       ×   OPERATION ID    ×ENTRY LENGTH(HWORDS)×                 */
*/*       ------------------------------------------                 */
*/*       ×  ANY INDEX ID     ×     UNUSED         ×                 */
*/*       ------------------------------------------                 */
*/*       ×                UNUSED                  ×                 */
*/*       ------------------------------------------                 */
*/*       ×  CONDITION MASK   ×REL CIT OFFSET-HWORD×                 */
*/*       ------------------------------------------                 */
*/*                                                                  */
*/*   CONDITION MASK IS:                                             */
*/*     X'FFFF' - UNCONDITIONAL BRANCH                               */
*/*                                                                  */
*/************************************************************       */
*                                                               02S0247
*         DO;                                                   02S0247
*           APCIXCC=APCIXCGO;       /* INSERT CONDITION MASK INTO THE
*                                      CIT ENTRY                     */
         LA    @11,CITENTRY                                     02S0248
         MVC   APCIXCC(2,@11),@CB00470                          02S0248
*           APCIXID1=1;             /* SUPPLY AN INDEX TO KEEP CP
*                                      HAPPY                         */
*                                                               02S0249
         LA    @04,1                                            02S0249
         STH   @04,APCIXID1(,@11)                               02S0249
*           /*********************************************************/
*           /*                                                       */
