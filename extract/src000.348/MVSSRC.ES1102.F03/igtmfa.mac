*                   IF GOODONE=OFF THEN/* TEST SUCCESS INDICATOR     */
         LTR   GOODONE,GOODONE                                  02S0227
         BNZ   @RF00227                                         02S0227
*                     DO;           /* UNSUCCESSFUL - SET ERROR 02S0228
*                                      INDICATORS                    */
*                       ERRFLAG=ON; /* SET ERROR FLAG                */
         LA    ERRFLAG,1                                        02S0229
*                       APCWRCN=APCCX004;/* SET ERROR NUMBER         */
         MVI   APCWRCN(APCWPTR),X'04'                           02S0230
*                       APEMMSGN=APCMX004;/* SET MESSAGE NUMBER FOR
