TMFA     TITLE 'IGTMFA --- PROCESS LOGICAL OPERATION REQUESTS          *
                        '
IGTMFA   CSECT ,                                                01S0001
@MAINENT DS    0H                                               01S0001
         USING *,@15                                            01S0001
         B     @PROLOG                                          01S0001
         DC    AL1(16)                                          01S0001
         DC    C'IGTMFA    77.093'                              01S0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                  01S0001
         BALR  @07,0                                            01S0001
@PSTART  LA    @05,4095(,@07)                                   01S0001
         USING @PSTART,@07                                      01S0001
         USING @PSTART+4095,@05                                 01S0001
         L     @00,@SIZDATD                                     01S0001
         IGTYGSV  R,LV=(0)
         LR    @06,@01                                          01S0001
         USING @DATD,@06                                        01S0001
         ST    @13,@SA00001+4                                   01S0001
         LM    @00,@01,20(@13)                                  01S0001
         ST    @06,8(,@13)                                      01S0001
         LR    @13,@06                                          01S0001
         MVC   @PC00001(4),0(@01)                               01S0001
*/*                                                                  */
*/* END OF APAX                                                      */
*/*                                                                  */
*                                                               01S0048
*                                                               01S0048
*   /*****************************************************************/
*   /*                                                               */
*   /* SET POINTERS FOR CONTROL BLOCKS NEEDED                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0048
*   DO;                                                         01S0048
*     RFY                                                       01S0049
*       APCWPTR RSTD;                                           01S0049
*     APCWPTR=ADDR(DAPCW);                                      01S0050
         L     APCWPTR,@PC00001                                 01S0050
*     RFY                                                       01S0051
*       APCW BASED(APCWPTR);        /* GET APCW                      */
*     RFY                                                       01S0052
*       APCI BASED(ADDR(CITENTRY(1)));/* GET APCI                    */
*     RFY                                                       01S0053
*       APAL BASED(APCWAPAL);       /* GET APAL                      */
*     RFY                                                       01S0054
*       APIN BASED(APCWAPIN);       /* GET APIN                      */
*     RFY                                                       01S0055
*       APEM BASED(ADDR(APCWEMSG)); /* GET APEM                      */
*     RFY                                                       01S0056
*       IFGACB BASED(APCWACB);      /* GET ACB                       */
*     RFY                                                       01S0057
*       APAX BASED(ACBAMWAP);       /* GET APAX                      */
*     RFY                                                       01S0058
*       APLB BASED(APAXAPLB);       /* GET APLB                      */
*   END;                                                        01S0059
*                                                               01S0059
*   /*****************************************************************/
*   /*                                                               */
*   /* ZERO OUT CIT WORK AREA                                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0060
*   DO;                                                         01S0060
*     APCIXDTA=APCIXDTA&&APCIXDTA;  /* CLEAR WORK AREA               */
         LA    @09,CITENTRY                                     01S0061
         XC    APCIXDTA(12,@09),APCIXDTA(@09)                   01S0061
*   END;                                                        01S0062
*                                                               01S0062
*   /*****************************************************************/
*   /*                                                               */
*   /* BUILD CIT ENTRY IN AUTOMATIC WORK AREA                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0063
*   DO;                                                         01S0063
         ST    @09,@TF00001                                     01S0063
*     APCIOPID=APCWAPOP;            /* PUT OPERATION ID INTO CIT
*                                      ENTRY                         */
         L     @04,@TF00001                                     01S0064
         MVC   APCIOPID(2,@04),APCWAPOP(APCWPTR)                01S0064
*     APCIOPSZ=APALCISZ;            /* PUT ENTRY LENGTH(HALFWORDS)
*                                      INTO CIT ENTRY                */
         L     @04,APCWAPAL(,APCWPTR)                           01S0065
         LH    @03,APALCISZ(,@04)                               01S0065
         L     @01,@TF00001                                     01S0065
         STH   @03,APCIOPSZ(,@01)                               01S0065
*     NDEXTYPE=APALLTYP;            /* SET SELECTOR VALUE FOR   01S0066
*                                      LOGCASES                      */
         LH    NDEXTYPE,APALLTYP(,@04)                          01S0066
*     ERRFLAG=OFF;                  /* ERROR INDICATOR INITIALLY OFF */
*                                                               02S0067
         SLR   ERRFLAG,ERRFLAG                                  02S0067
*     /***************************************************************/
*     /*                                                             */
*     /* DETERMINE TYPE OF INDEX OPERATION AND BUILD APPROPRIATE CIT */
*     /* ENTRY                                                       */
*     /*                                                             */
*     /***************************************************************/
*                                                               02S0068
*LOGTYPE:                                                       02S0068
*     DO;                           /* LOGTYPE:CASE(NDEXTYPE)NAMES(ND
*                                      EXBULK,NDEXNDEX,NDEXIMMD,CDBRA
*                                      NCH,XGO,XBCT,XID,DUMP,BREAK)  */
LOGTYPE  DS    0H                                               02S0069
*       DCL                                                     02S0069
*         CL0001@ LABEL BASED(CP0001@(NDEXTYPE)) VALUERANGE(NDEXBULK,
*             NDEXNDEX,NDEXIMMD,CDBRANCH,XGO,XBCT,XID,DUMP,BREAK);/*
*                                      BRANCH TARGET                 */
*       DCL                                                     02S0070
*         CP0001@(9) PTR(31) INIT(ADDR(NDEXBULK),ADDR(NDEXNDEX),ADDR(
*             NDEXIMMD),ADDR(CDBRANCH),ADDR(XGO),ADDR(XBCT),ADDR(XID),
*             ADDR(DUMP),ADDR(BREAK));                          02S0070
*       GOTO CL0001@;               /* BRANCH TO SELECTED CASE       */
         LR    @04,NDEXTYPE                                     02S0071
         SLA   @04,2                                            02S0071
         L     @04,CP0001@-4(@04)                               02S0071
         BR    @04                                              02S0071
*NDEXBULK:                                                      02S0072
*       DO;                         /* SUBCASE INDEX - BULK STORE
*                                      TRANSFER                      */
NDEXBULK DS    0H                                               02S0073
*/*********************************************************          */
*/*                                                                  */
*/* THIS CASE BUILDS A CIT ENTRY FOR INDEX - BULK STORE              */
*/* TRANSFERS. USER CALL FORMAT FOR INDEX - BULK STORE               */
*/* TRANSFERS IS:                                                    */
*/*                                                                  */
*/*         CALL VPSS(IUNIT,IFUNC,YARRV,ICTY,YARRVX,IVAL1)           */
*/*                                                                  */
*/*         WHERE:                                                   */
*/*                                                                  */
*/*         IUNIT - UNIT REFERENCE NUMBER                            */
*/*         IFUNC - FUNCTION MNEMONIC -                              */
*/*                   'XMVX' - MOVE INDEX TO BULK STORE              */
*/*                   'XMVS' - MOVE BULK STORE TO INDEX              */
*/*        YARRV - BULK STORE ADDRESS                                */
*/*        ICTY  - NUMBER OF INDEXES TO LOAD/STORE                   */
*/*        YARRVX- BULK STORE ADDRESS INDEX                          */
*/*        IVAL1 - FIRST INDEX TO LOAD/STORE                         */
*/*                                                                  */
*/* THE CIT FORMAT FOR INDEX - BULK STORE TRANSFER IS:               */
*/*                                                                  */
*/*      <-------------- 4 BYTES ----------------->                  */
*/*      ------------------------------------------                  */
*/*      ×  OPERATION ID     ×ENTRY LENGTH(HWORDS)×                  */
*/*      ------------------------------------------                  */
*/*      ×  FIRST INDEX ID   × LAST INDEX ID        ×                */
*/*      ------------------------------------------                  */
*/*      ×            BULK STORE ADDRESS          ×                  */
*/*      ------------------------------------------                  */
*/*      ×  BULK STORE INDEX ×      UNUSED          ×                */
*/*      ____________________________________________                */
*/*                                                                  */
*/*********************************************************          */
*                                                               02S0073
*         DO;                       /* BUILD CIT ENTRY FOR INDEX/BULK
*                                      STORE TRANSFER OPERATIONS     */
*           IF(APINXID>0)&(APINXID<=15) THEN/* IVAL1 VALIDITY TEST   */
         L     @04,APCWAPIN(,APCWPTR)                           02S0074
         L     @01,APINAXID(,@04)                               02S0074
         L     @15,APINXID(,@01)                                02S0074
         LTR   @15,@15                                          02S0074
         BNP   @RF00074                                         02S0074
         LA    @14,15                                           02S0074
         CR    @15,@14                                          02S0074
         BH    @RF00074                                         02S0074
*             DO;                   /* IVAL1 OK - CONTINUE           */
*               APCIXID1=APINXID;   /* MOVE IVAL1 FROM USER PARM LIST
*                                      TO CIT WORK AREA              */
         LA    @11,CITENTRY                                     02S0076
         STH   @15,APCIXID1(,@11)                               02S0076
*               APCIXID2=APCIXID1+APINXCT-1;/* CALCULATE LAST INDEX
*                                      TO TRANSFER USING IVAL1 AND
*                                      ICTY                          */
         L     @01,APINAXCT(,@04)                               02S0077
         LR    @10,@15                                          02S0077
         AL    @10,APINXCT(,@01)                                02S0077
         BCTR  @10,0                                            02S0077
         STH   @10,APCIXID2(,@11)                               02S0077
*               IF(APCIXID2>=1)&    /* ICTY VALIDITY CHECK - MUST    */
*                   (APCIXID2<=15)& /* RESULT IN LAST INDEX RANGE    */
*                   (APCIXID2>=APCIXID1)/* OF 1-15 AND BE GE TO IVAL1*/
*                 THEN                                          02S0078
         C     @10,@CF00061                                     02S0078
         BL    @RF00078                                         02S0078
         CR    @10,@14                                          02S0078
         BH    @RF00078                                         02S0078
         CR    @10,@15                                          02S0078
         BL    @RF00078                                         02S0078
*                 DO;               /* ICTY VALID - CONTINUE         */
*                   APCIXBSA=APINAXBS;/* MOVE POINTER TO BULK STORE
*                                      ADDRESS TO CIT WORKAREA       */
         L     @04,APINAXBS(,@04)                               02S0080
         ST    @04,APCIXBSA(,@11)                               02S0080
*                   CALL APCVZMIF(APCIXBSA,RSOLVERR);/* RESOLVE IT   */
         LA    @04,APCIXBSA(,@11)                               02S0081
         ST    @04,@AL00001                                     02S0081
         LA    @04,RSOLVERR                                     02S0081
         ST    @04,@AL00001+4                                   02S0081
         L     @15,APCTZMIF(,APCTPTR)                           02S0081
         LA    @01,@AL00001                                     02S0081
         BALR  @14,@15                                          02S0081
*                   IF RSOLVERR=0 THEN/* TEST FOR ERROR ON CALL      */
         SLR   @04,@04                                          02S0082
         C     @04,RSOLVERR                                     02S0082
         BNE   @RF00082                                         02S0082
*                     DO;           /* NO ERROR - CONTINUE           */
*                       IF(APINXBX>=0)&(APINXBX<=15) THEN/* YARRVX
*                                      VALIDITY TEST                 */
         L     @01,APCWAPIN(,APCWPTR)                           02S0084
         L     @01,APINAXBX(,@01)                               02S0084
         L     @15,APINXBX(,@01)                                02S0084
         CR    @15,@04                                          02S0084
         BL    @RF00084                                         02S0084
         C     @15,@CF00084                                     02S0084
         BH    @RF00084                                         02S0084
*                         APCIXBSX=APINXBX;/* YARRVX OK - MOVE TO CIT
*                                      WORKAREA                      */
         LA    @04,CITENTRY                                     02S0085
         STH   @15,APCIXBSX(,@04)                               02S0085
*                       ELSE        /* YARRVX INVALID - TELL USER    */
*                         DO;       /* YARRVX ERROR PROCESSING       */
         B     @RC00084                                         02S0086
@RF00084 DS    0H                                               02S0087
*                           ERRFLAG=ON;/* FLAG ERROR TO MAIN SEGMENT */
         LA    ERRFLAG,1                                        02S0087
*                           APCWRCN=APCCX008;/* ERROR NUMBER FOR OUT
*                                      OF RANGE - YARRVX             */
         MVI   APCWRCN(APCWPTR),X'08'                           02S0088
*                           APEMMSGN=APCMX008;/* MESSAGE NUMBER FOR
*                                      OUT OF RANGE                  */
         LA    @04,112                                          02S0089
         ALR   @04,APCWPTR                                      02S0089
         MVC   APEMMSGN(4,@04),@CF00628                         02S0089
*                           APEMVALN=APCMK020;/* KEYWORD NUMBER FOR
*                                      YARRVX                        */
         MVC   APEMVALN(4,@04),@CF00961                         02S0090
*                         END;      /* END OF YARRVX ERROR PROCESSING*/
*                     END;          /* END OF SEGMENT                */
*                   ELSE            /* MIF RETURNED ERROR FROM  02S0093
*                                      ATTEMPT TO RESOLVE THE BULK
*                                      STORE ADDRESS                 */
*                     DO;           /* YARRV ERROR PROCESSING        */
         B     @RC00082                                         02S0093
@RF00082 DS    0H                                               02S0094
*                       ERRFLAG=ON; /* FLAG ERROR TO MAIN            */
         LA    ERRFLAG,1                                        02S0094
*                       APEMVALN=APCMK017;/* KEYWORD NUMBER FOR 02S0095
*                                      YARRVX                        */
         LA    @04,112                                          02S0095
         ALR   @04,APCWPTR                                      02S0095
         MVC   APEMVALN(4,@04),@CF01068                         02S0095
*                       IF RSOLVERR=BDYERROR THEN/* TEST IF ERROR
*                                      BECAUSE YARRV WAS NOT ALIGNED
*                                      ON A FULLWORD BULKSTORE  02S0096
*                                      BOUNDARY                      */
         CLC   RSOLVERR(4),@CF00061                             02S0096
         BNE   @RF00096                                         02S0096
*                         DO;       /* BOUNDARY PROBLEM              */
*                           APCWRCN=APCCX011;/* ERROR NUMBER FOR NOT
*                                      A WORD BOUNDARY - YARRV       */
         MVI   APCWRCN(APCWPTR),X'0B'                           02S0098
*                           APEMMSGN=APCMX011;/* MESSAGE NUMBER FOR
*                                      NOT A WORD BOUNDARY           */
         MVC   APEMMSGN(4,@04),@CF01358                         02S0099
*                         END;      /* END OF YARRV BOUNDARY ERROR
*                                      SEGMENT                       */
*                       ELSE        /* YARRV ERROR WAS RANGE (YARRV
*                                      POINTER LESS THAN BSAD)       */
*                         DO;       /* RANGE ERROR                   */
         B     @RC00096                                         02S0101
@RF00096 DS    0H                                               02S0102
*                           APCWRCN=APCCX007;/* ERROR NUMBER FOR OUT
*                                      OF RANGE - YARRV              */
         MVI   APCWRCN(APCWPTR),X'07'                           02S0102
*                           APEMMSGN=APCMX007;/* MESSAGE NUMBER FOR
*                                      OUT OF RANGE                  */
         MVC   APEMMSGN+112(4,APCWPTR),@CF00628                 02S0103
*                         END;      /* END OF YARRV RANGE ERROR 02S0104
*                                      SEGMENT                       */
*                     END;          /* END OF SEGMENT                */
*                 END;              /* END OF SEGMENT                */
*               ELSE                /* ICTY INVALID                  */
*                 DO;               /* ICTY ERROR PROCESSING         */
         B     @RC00078                                         02S0107
@RF00078 DS    0H                                               02S0108
*                   ERRFLAG=ON;     /* FLAG ERROR TO MAIN SEGMENT    */
         LA    ERRFLAG,1                                        02S0108
*                   APCWRCN=APCCX012;/* ERROR NUMBER FOR OUT OF RANGE
*                                      - ICTY                        */
         MVI   APCWRCN(APCWPTR),X'0C'                           02S0109
*                   APEMMSGN=APCMX012;/* MESSAGE NUMBER FOR OUT OF
*                                      RANGE                         */
         LA    @04,112                                          02S0110
         ALR   @04,APCWPTR                                      02S0110
         MVC   APEMMSGN(4,@04),@CF00628                         02S0110
*                   APEMVALN=APCMK018;/* KEYWORD NUMBER FOR ICTY     */
         MVC   APEMVALN(4,@04),@CF01070                         02S0111
*                 END;              /* END OF ICTY ERROR PROCESSING  */
*             END;                  /* END OF SEGMENT                */
*           ELSE                    /* IVAL1 INVALID                 */
*             DO;                   /* IVAL1 ERROR PROCESSING        */
         B     @RC00074                                         02S0114
@RF00074 DS    0H                                               02S0115
*               ERRFLAG=ON;         /* FLAG ERROR TO MAIN SEGMENT    */
         LA    ERRFLAG,1                                        02S0115
*               APCWRCN=APCCX001;   /* ERROR NUMBER FOR OUT OF RANGE
*                                      - IVAL1                       */
         MVI   APCWRCN(APCWPTR),X'01'                           02S0116
*               APEMMSGN=APCMX001;  /* MESSAGE NUMBER FOR OUT OF
*                                      RANGE                         */
         LA    @04,112                                          02S0117
         ALR   @04,APCWPTR                                      02S0117
         MVC   APEMMSGN(4,@04),@CF00628                         02S0117
*               APEMVALN=APCMK038;  /* KEYWORD NUMBER - IVAL1        */
         MVC   APEMVALN(4,@04),@CF01148                         02S0118
*             END;                  /* END OF IVAL1 ERROR PROCESSING */
*         END;                      /* END OF SEGMENT - CIT BUILD FOR
*                                      XMVS/XMVX                     */
*         GOTO CE0001@;             /* BRANCH TO END OF CASE         */
         B     CE0001@                                          02S0121
*       END NDEXBULK;               /* END OF SUBCASE                */
*NDEXNDEX:                                                      02S0123
*       DO;                         /* SUBCASE INDEX - INDEX    02S0123
*                                      OPERATION                     */
NDEXNDEX DS    0H                                               02S0124
*/**************************************************************     */
*/*                                                                  */
*/* THIS CASE BUILDS A CIT ENTRY FOR INDEX - INDEX OPERATIONS        */
*/* OF THE FOLLOWING FORM:                                           */
*/*                                                                  */
*/*      CALL VPSS(IUNIT,IFUNC,IVAL1,IVAL2)                          */
*/*                                                                  */
*/*      WHERE:                                                      */
*/*                                                                  */
*/*        IUNIT - UNIT REFERENCE NUMBER                             */
*/*        IFUNC - FUNCTION MNEMONIC AS FOLLOWS-                     */
*/*                  'XMV ' - MOVE IVAL2 TO IVAL1                    */
*/*                  'XAD ' - ADD IVAL2 TO IVAL1                     */
*/*                  'XSB ' - SUBTRACT IVAL2 FROM IVAL1              */
*/*                  'XML ' - MULTIPLY IVAL2 TIMES IVAL1             */
*/*                  'XDV ' - DIVIDE IVAL2 INTO IVAL1                */
*/*        IVAL1 - INDEX ID                                          */
*/*        IVAL2 - INDEX ID                                          */
*/*    NOTE : RESULT ALWAYS PLACED IN IVAL1.                         */
*/*                                                                  */
*/* THE CIT FORMAT FOR INDEX - INDEX OPERATIONS IS:                  */
*/*                                                                  */
*/*        <-------------- 4 BYTES ----------------->                */
*/*        ------------------------------------------                */
*/*        ×   OPERATION ID    ×ENTRY LENGTH(HWORDS)×                */
*/*        ------------------------------------------                */
*/*        ×   FIRST INDEX ID  × SECOND INDEX ID    ×                */
*/*        ------------------------------------------                */
*/*                                                                  */
*/**************************************************************     */
*                                                               02S0124
*         DO;                                                   02S0124
*           IF(APINXI1>=1)&(APINXI1<=15) THEN/* TEST IF IVAL1 IS A
*                                      VALID INDEX ID                */
         LA    @04,1                                            02S0125
         L     @15,APCWAPIN(,APCWPTR)                           02S0125
         L     @01,APINAXI1(,@15)                               02S0125
         L     @14,APINXI1(,@01)                                02S0125
         CR    @14,@04                                          02S0125
         BL    @RF00125                                         02S0125
         LA    @11,15                                           02S0125
         CR    @14,@11                                          02S0125
         BH    @RF00125                                         02S0125
*             IF(APINXI2>=1)&(APINXI2<=15) THEN/* YES - TEST IF IVAL2
*                                      IS A VALID INDEX ID           */
         L     @01,APINAXI2(,@15)                               02S0126
         L     @15,APINXI2(,@01)                                02S0126
         CR    @15,@04                                          02S0126
         BL    @RF00126                                         02S0126
         CR    @15,@11                                          02S0126
         BH    @RF00126                                         02S0126
*               DO;                 /* YES - PUT INDEX IDS INTO THE
*                                      CIT ENTRY                     */
*                 APCIXID1=APINXI1; /* FIRST INDEX ID IS IVAL1       */
         LA    @04,CITENTRY                                     02S0128
         STH   @14,APCIXID1(,@04)                               02S0128
*                 APCIXID2=APINXI2; /* SECOND INDEX ID IS IVAL2      */
         STH   @15,APCIXID2(,@04)                               02S0129
*               END;                                            02S0130
*             ELSE                  /* IVAL2 INVALID                 */
*               DO;                 /* SET ERROR INDICATORS FOR OUT
*                                      OF RANGE - IVAL2 MSG.         */
         B     @RC00126                                         02S0131
@RF00126 DS    0H                                               02S0132
*                 ERRFLAG=ON;       /* SET ERROR FLAG                */
         LA    ERRFLAG,1                                        02S0132
*                 APCWRCN=APCCX002; /* SET ERROR NUMBER              */
         MVI   APCWRCN(APCWPTR),X'02'                           02S0133
*                 APEMMSGN=APCMX002;/* SET MESSAGE NUMBER            */
         LA    @04,112                                          02S0134
         ALR   @04,APCWPTR                                      02S0134
         MVC   APEMMSGN(4,@04),@CF00628                         02S0134
*                 APEMVALN=APCMK039;/* SET KEYWORD NUMBER FOR IVAL2  */
         MVC   APEMVALN(4,@04),@CF01150                         02S0135
*               END;                                            02S0136
*           ELSE                    /* IVAL1 INVALID                 */
*             DO;                   /* SET ERROR INDICATORS FOR OUT
*                                      OF RANGE - IVAL1              */
         B     @RC00125                                         02S0137
@RF00125 DS    0H                                               02S0138
*               ERRFLAG=ON;         /* FLAG THE ERROR                */
         LA    ERRFLAG,1                                        02S0138
*               APCWRCN=APCCX001;   /* SET ERROR NUMBER              */
         MVI   APCWRCN(APCWPTR),X'01'                           02S0139
*               APEMMSGN=APCMX001;  /* SET MESSAGE NUMBER            */
         LA    @04,112                                          02S0140
         ALR   @04,APCWPTR                                      02S0140
         MVC   APEMMSGN(4,@04),@CF00628                         02S0140
*               APEMVALN=APCMK038;  /* SET KEYWORD VALUE FOR IVAL1   */
         MVC   APEMVALN(4,@04),@CF01148                         02S0141
*             END;                                              02S0142
*         END;                                                  02S0143
*         GOTO CE0001@;             /* BRANCH TO END OF CASE         */
         B     CE0001@                                          02S0144
*       END NDEXNDEX;               /* END OF SUBCASE                */
*NDEXIMMD:                                                      02S0146
*       DO;                         /* SUBCASE INDEX-IMMEDIATE  02S0146
*                                      OPERATION                     */
NDEXIMMD DS    0H                                               02S0147
*/************************************************************       */
*/*                                                                  */
*/* THIS CASE BUILDS A CIT ENTRY FOR INDEX - IMMEDIATE               */
*/* LOGIC OPERATIONS OF THE FOLLOWING FORM:                          */
*/*                                                                  */
*/*      CALL VPSS(IUNIT,IFUNC,IVAL1,IVAL2)                          */
*/*                                                                  */
*/*      WHERE:                                                      */
*/*                                                                  */
*/*        IUNIT - UNIT REFERENCE NUMBER                             */
*/*        IFUNC - FUNCTION MNEMONIC AS FOLLOWS -                    */
*/*                  'XMVI' - MOVE IVAL2 TO IVAL1                    */
*/*                  'XADI' - ADD IVAL2 TO IVAL1                     */
*/*                  'XSBI' - SUBTRACT IVAL2 FROM IVAL1              */
*/*                  'XMLI' - MULTIPLY IVAL2 TIMES IVAL1             */
*/*                  'XDVI' - DIVIDE IVAL2 INTO IVAL1                */
*/*                  'XDIR' - DIVIDE IVAL1 INTO IVAL2                */
*/*        IVAL1 - INDEX ID                                          */
*/*        IVAL2 - IMMEDIATE DATA                                    */
*/*     NOTE: RESULT ALWAYS PLACED IN IVAL1.                         */
*/*                                                                  */
*/* THE CIT FORMAT FOR INDEX - IMMEDIATE OPERATIONS IS:              */
*/*                                                                  */
*/*        <------------- 4 BYTES ------------------>                */
*/*        ------------------------------------------                */
*/*        ×   OPERATION ID    ×ENTRY LENGTH(HWORDS)×                */
*/*        ------------------------------------------                */
*/*        ×   FIRST INDEX ID  ×     UNUSED         ×                */
*/*        ------------------------------------------                */
*/*        ×             IMMEDIATE DATA             ×                */
*/*        ------------------------------------------                */
*/*                                                                  */
*/************************************************************       */
*                                                               02S0147
*         DO;                                                   02S0147
*           IF(APINXI1>=1)&(APINXI1<=15) THEN/* TEST IF IVAL1 IS A
*                                      VALID INDEX ID                */
         L     @04,APCWAPIN(,APCWPTR)                           02S0148
         L     @01,APINAXI1(,@04)                               02S0148
         L     @15,APINXI1(,@01)                                02S0148
         C     @15,@CF00061                                     02S0148
         BL    @RF00148                                         02S0148
         C     @15,@CF00084                                     02S0148
         BH    @RF00148                                         02S0148
*             DO;                   /* YES - PUT INDEX ID INTO THE
*                                      CIT ENTRY                     */
*               APCIXID1=APINXI1;   /* FIRST INDEX ID IS IVAL1       */
         LA    @14,CITENTRY                                     02S0150
         STH   @15,APCIXID1(,@14)                               02S0150
*               APCIXIMM=APINXI2;   /* IMMEDIATE DATA IS IVAL2       */
         L     @04,APINAXI2(,@04)                               02S0151
         L     @04,APINXI2(,@04)                                02S0151
         ST    @04,APCIXIMM(,@14)                               02S0151
*             END;                                              02S0152
*           ELSE                    /* IVAL1 INVALID                 */
*             DO;                   /* SET ERROR INDICATORS FOR OUT
*                                      OF RANGE - IVAL1              */
         B     @RC00148                                         02S0153
@RF00148 DS    0H                                               02S0154
*               ERRFLAG=ON;         /* FLAG THE ERROR                */
         LA    ERRFLAG,1                                        02S0154
*               APCWRCN=APCCX001;   /* SET ERROR NUMBER              */
         MVI   APCWRCN(APCWPTR),X'01'                           02S0155
*               APEMMSGN=APCMX001;  /* SET MESSAGE NUMBER            */
         LA    @04,112                                          02S0156
         ALR   @04,APCWPTR                                      02S0156
         MVC   APEMMSGN(4,@04),@CF00628                         02S0156
*               APEMVALN=APCMK038;  /* SET KEYWORD VALUE FOR IVAL1   */
         MVC   APEMVALN(4,@04),@CF01148                         02S0157
*             END;                                              02S0158
