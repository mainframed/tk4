*    STARTED ON THE AFFECTED SUBCHANNEL.  IT USES THE IOSGEN MACRO
*    FOR BOTH PURPOSES. IT ALSO CANCELS THE ESTAE AND FREES THE 01S0742
*    DYNAMIC WORK AREA.                                              */
*                                                               01S0742
*RESETQB:                                                       01S0742
*   PROC OPTIONS(DONTSAVE,NOSAVEAREA);/* ENTRY TO THE ROUTINE.       */
*                                                               01S0742
RESETQB  DS    0H                                               01S0743
*   /*****************************************************************/
*   /*                                                               */
*   /* LOCAL DCLS.                                                   */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0743
*   DCL                                                         01S0743
*     RSQBSAVE PTR(31);             /* SAVE RETURN.                  */
*   RSQBSAVE=R14P;                  /* WHERE TO GO BACK TO.          */
         LR    RSQBSAVE,R14P                                    01S0744
*/*   RESET THE QUIESCE BIT IN THE UCB.                              */
*                                                               01S0745
*   RFY                                                         01S0745
*    (R1P,                                                      01S0745
*     R13P,                                                     01S0745
*     R14P) RSTD;                                               01S0745
*   R13P=ADDR(SAVE16(1));           /* 64 BYTE SAVE AREA             */
         LA    R13P,SAVE16                                      01S0746
*   R1P=IOSUCB;                                                 01S0747
         L     R1P,IOSUCB(,APIOPTR)                             01S0747
*   GEN REFS(UCBFLA,UCBFLB,UCBFLC,CVTPTR,CVTIXAVL,IOCGENA,UCBOB,
