*                                                               01S0716
*WRT300I:                                                       01S0716
*   PROC(INCHAR) OPTIONS(DONTSAVE,NOSAVEAREA);                  01S0716
WRT300I  MVC   @PC00003(4),0(@01)                               01S0716
*   DCL                                                         01S0717
*     MSG300S CHAR(*) GEND STATIC LOCAL;/* MSG SKELETON.             */
*   GEN DATA DEFS(MSG300S);                                     01S0718
*   DCL                                                         01S0719
*     1 MSG300 BDY(WORD),           /* IGT300I BUILD AREA.           */
*      2 MSGLEN PTR(15),            /* LENGTH                        */
*      2 MSGFLS BIT(16),            /* FLAGS                         */
*      2 MSGNUM CHAR(9),            /* MSG ID                        */
*      2 MSGSS CHAR(4),             /* SYBSYSTEM                     */
*      2 MSGSP1 CHAR(2),            /* SPACING                       */
*      2 MSGUNT CHAR(3),            /* UNIT                          */
*      2 MSGSP2 CHAR(1),            /* SPACING                       */
*      2 MSGERR CHAR(2),            /* COMPONENT                     */
*      2 MSGSP3 CHAR(1),            /* SPACEING                      */
*      2 MSGTYP CHAR(1),            /* PERM/TEMP.                    */
*      2 MSGSP4 CHAR(1),            /* SPACING                       */
*      2 MSGCNT CHAR(5),            /* COUNT                         */
*      2 MSGRT CHAR(4);             /* ROUTECODES.                   */
*   DCL                                                         01S0720
*     MSGRET PTR(31);               /* RETURN SAVE.                  */
*   DCL                                                         01S0721
*     I PTR(31);                    /* COUNTER                       */
*   DCL                                                         01S0722
*     INCHAR CHAR(1);               /* INDICATES TEMP OR PERM.       */
*   DCL                                                         01S0723
*     TCOUNT CHAR(5);               /* UNPACK AREA.                  */
*   DCL                                                         01S0724
*     PACKED CHAR(3);               /* CONVERT AREA.                 */
*   MSGRET=R14P;                    /* SAVE RETURN ADDRESS.          */
         LR    MSGRET,R14P                                      01S0725
*   MVC(MSG300,MSG300S(1:LENGTH(MSG300)));/* MOVE IN THE SKELETON.   */
         MVC   MSG300(37),MSG300S                               01S0726
*   MSGSS=APCTSSID;                 /* SUBSYS ID.                    */
         MVC   MSGSS(4),APCTSSID(APCTPTR)                       01S0727
*   MSGTYP=INCHAR;                  /* TYPE OF ERROR (TEMP/PERM).    */
         L     @15,@PC00003                                     01S0728
         MVC   MSGTYP(1),INCHAR(@15)                            01S0728
*   MSGUNT=UCBNAME;                 /* FAILING UNIT.                 */
         L     @01,UCBPTR(,APIOPTR)                             01S0729
         MVC   MSGUNT(3),UCBNAME(@01)                           01S0729
*   MSGERR=THRSPTR->I256C(1:2);     /* FAILING COMPONENT.            */
         MVC   MSGERR(2),I256C(THRSPTR)                         01S0730
*   CVD(CTRPTR->I015F,PACKED);      /* CONVERT COUNT TO DECIMAL.     */
         LH    @09,I015F(,CTRPTR)                               01S0731
         CVD   @09,@TS00001                                     01S0731
         MVC   PACKED(3),@TS00001+5                             01S0731
*   UNPK(TCOUNT,PACKED);            /* UNPACK RESULT.                */
         UNPK  TCOUNT(5),PACKED(3)                              01S0732
*   TCOUNT(5)=TCOUNT(5)×'F0'X;      /* GET RID OF SIGN.              */
         OI    TCOUNT+4,X'F0'                                   01S0733
*   DO I=1 TO 5                     /* FIND FIRST NON-ZERO NUMBER.   */
*         WHILE TCOUNT(I)='0';                                  01S0734
         LA    I,1                                              01S0734
@DL00734 LA    @09,TCOUNT-1(I)                                  01S0734
         CLI   0(@09),C'0'                                      01S0734
         BNE   @DC00734                                         01S0734
*   END;                                                        01S0735
         AL    I,@CF00033                                       01S0735
         C     I,@CF01950                                       01S0735
         BNH   @DL00734                                         01S0735
@DC00734 DS    0H                                               01S0736
*   MSGCNT(1:6-I)=TCOUNT(I:5);      /* ELIMINATE LEADING ZEROS.      */
         LA    @09,6                                            01S0736
         SLR   @09,I                                            01S0736
         BCTR  @09,0                                            01S0736
         LA    @01,TCOUNT-1(I)                                  01S0736
         EX    @09,@SM03285                                     01S0736
*   GEN REFS(MSGCNT);                                           01S0737
*                                 /* WRITE THE MESSAGE.              */
         SR    R0P,R0P
         WTO   MF=(E,MSG300)
*   R14P=MSGRET;                    /* RESTORE THE RETURN ADDRESS.   */
         LR    R14P,MSGRET                                      01S0738
*   RETURN;                         /* RETURN TO CALLER.             */
@EL00003 DS    0H                                               01S0739
@EF00003 DS    0H                                               01S0739
@ER00003 BR    @14                                              01S0739
*   END;                            /* END OF WRT300 PROC.           */
