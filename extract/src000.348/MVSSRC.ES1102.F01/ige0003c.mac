003C     TITLE 'IGE0003C - 3838 ERROR RECOVERY PROCEDURE.              *
                        '
*                                 /* DO OWN PROLOG.                  */
IGE0003C CSECT
         USING IGE0003C,CODEBASE     ADDRESSIBILITY FOR CODE.
*   CODEBASE=R15P;                  /* ADDRESS OF PROGRAM.           */
         LR    CODEBASE,R15P                                    01S0175
*   RFY                                                         01S0176
*     R15P UNRSTD;                                              01S0176
*   APIOPTR=R1P;                    /* ADDRESS OF APIO AND IOSB.     */
         LR    APIOPTR,R1P                                      01S0177
*   RFY                                                         01S0178
*     IOSB BASED(APIOPTR);                                      01S0178
*   EWAPTR=IOSERP;                  /* ADDRESS OF ERP WORKAREA.      */
         L     EWAPTR,IOSERP(,APIOPTR)                          01S0179
*   RFY                                                         01S0180
*     APSN BASED(ADDR(EWAIERP));                                01S0180
*   IOSERR=ON;                      /* ERP IN CONTROL.               */
*   IOSEX=ON;                       /* EXCEPTIONAL CONDITION.        */
         OI    IOSERR(APIOPTR),B'00100100'                      01S0182
*   IOSSDR=OFF;                     /* RESET INDICATORS IN THE IOSB  */
*   IOSMSG=OFF;                     /* WHICH MIGHT BE LEFT ON FROM   */
*   IOSBDCST=OFF;                   /* PREVIOUS ENTRIES.             */
*   IOSLOG=OFF;                     /* MORE OF SAME.                 */
         NI    IOSSDR(APIOPTR),B'10111000'                      01S0186
*   RFY                                                         01S0187
*    (R0P,                                                      01S0187
*     R2P,                                                      01S0187
*     R15P) RSTD;                   /* NEED THESE REGS.              */
*   IF KEYSAVED=ON THEN             /* IF THE KEY WAS CHANGED IN THE
*                                      IOSB THEN IT NEEDS TO BE 01S0188
*                                      RESTORD                       */
         TM    KEYSAVED(EWAPTR),B'00010000'                     01S0188
         BNO   @RF00188                                         01S0188
*     DO;                           /* RESTORE THE KEY.              */
*       KEYSAVED=OFF;               /* INDICATE THAT THE KEY IS 01S0190
*                                      RESTORD                       */
         NI    KEYSAVED(EWAPTR),B'11101111'                     01S0190
*       IOSCKEY=SAVEKEY;            /* RESTORE THE KEY.              */
         MVC   IOSCKEY(1,APIOPTR),SAVEKEY(EWAPTR)               01S0191
*     END;                          /* END OF KEY RESTORE.           */
*   GEN SETS(R0P)(L     R0P,@SIZDATD);/* SIZE REQUIRED FOR DYNAMIC
*                                      AREA                          */
@RF00188 DS    0H                                               01S0193
         L     R0P,@SIZDATD
*   SPSIZE=R0P;                     /* SET SIZE FOR POSTERITY-  01S0194
*                                      FREEMAIN                      */
         ST    R0P,SPSIZE(,EWAPTR)                              01S0194
*   DSP=230;                        /* TRY THIS SUBPOOL FIRST.       */
         MVI   DSP(EWAPTR),X'E6'                                01S0195
*   R2P=DSIZE;                      /* SET R2 FOR THE GETMAIN.       */
         L     R2P,DSIZE-1(,EWAPTR)                             01S0196
         LA    R2P,0(,R2P)                                      01S0196
*   GEN SETS(R0P,R1P,R15P)          /* CONTITIONAL GETMAIN.          */
*       (GETMAIN RC,LV=(2),SP=230);                             01S0197
         GETMAIN RC,LV=(2),SP=230
*   IF R15P^=0 THEN                 /* IF STORAGE NOT AVAILABLE 01S0198
*                                      THERE, TRY AGAIN IN CSA.      */
         LTR   R15P,R15P                                        01S0198
         BZ    @RF00198                                         01S0198
*     DO;                                                       01S0199
*       DSP=241;                    /* SUBPOOL FOR CSA.              */
         MVI   DSP(EWAPTR),X'F1'                                01S0200
*       R0P=SPSIZE;                 /* SET R0 FOR THE GETMAIN.       */
         L     R0P,SPSIZE(,EWAPTR)                              01S0201
*       GEN SETS(R1P)(GETMAIN R,LV=(0));/* IF CAN'T GET IT HERE THEN
*                                      ERP WILL ABEND. TOO BAD.      */
         GETMAIN R,LV=(0)
*     END;                                                      01S0203
*   RFY                                                         01S0204
*    (R0P,                                                      01S0204
*     R2P,                                                      01S0204
*     R15P) UNRSTD;                                             01S0204
@RF00198 DS    0H                                               01S0205
*   DYNPTR=R1P;                     /* IF GOT HERE R1 HAS ADDRESS OF
*                                      STORAGE.                      */
         ST    R1P,DYNPTR(,EWAPTR)                              01S0205
*   DATABASE=R1P;                   /* SET THE DYNAMIC AREA PTR.     */
         LR    DATABASE,R1P                                     01S0206
*   GEN(USING @DATD,DATABASE        ADDRESSIBILITY FOR DATA.);  01S0207
         USING @DATD,DATABASE        ADDRESSIBILITY FOR DATA.
*   RFY                                                         01S0208
*     R1P UNRSTD;                                               01S0208
*   STM(R3P,R12P,SAVE312);          /* SAVE REGS FOR RETRY.          */
         STM   R3P,R12P,SAVE312                                 01S0209
*   ESIND=0;                        /* CLEAR THE ESATE INDICATOR
*                                      FIELD.                        */
         SLR   @11,@11                                          01S0210
         ST    @11,ESIND                                        01S0210
*   PARMPTR=ADDR(ESIND);            /* REGS FOR THE ESTAE RETRY 01S0211
*                                      ROUTINE                       */
         LA    @11,ESIND                                        01S0211
         ST    @11,PARMPTR(,EWAPTR)                             01S0211
*   RFY                                                         01S0212
*     R2P RSTD;                                                 01S0212
*   R2P=ADDR(PARMPTR);              /* SET ADDRESS OF THE PARMLIST.  */
         LA    R2P,PARMPTR(,EWAPTR)                             01S0213
*   GEN REFS(DESTAE,XESTAE,PARMPTR) FLOWS(RECOVER);             01S0214
         MVC    DESTAE,XESTAE        MOVE IN THE SKELETON.
         ESTAE  RECOVER,CT,PARAM=(R2P),MF=(E,DESTAE)
*   RFY                                                         01S0215
*     R2P UNRSTD;                   /* COMMENT FOR THE STANDARDS.    */
*   DO;                                                         02S0216
*     IF IOSCOD=IOSFINTC THEN       /* TEST FOR INTERCEPT CONDITION. */
         CLI   IOSCOD(APIOPTR),X'7E'                            02S0217
         BNE   @RF00217                                         02S0217
*       DO;                         /* IT'S AN INTERCEPT ERROR       */
*         IOSCOD='7F'X;             /* WE WANT TO TELL IOS TO RETRY. */
         MVI   IOSCOD(APIOPTR),X'7F'                            02S0219
*         GO TO LOGRTRY;            /* LET THE CALLER FAIL IN HIS OWN
*                                      RIGHT.                        */
         B     LOGRTRY                                          02S0220
*       END;                                                    02S0221
*   END;                                                        02S0222
@RF00217 DS    0H                                               02S0223
*   RETRY=NO;                       /* ASSUME THAT RETRY CAN'T BE
*                                      DONE,                         */
*   RESTART=YES;                    /* * BUT THAT RESTART CAN.       */
         OI    RESTART(EWAPTR),B'00001000'                      01S0224
         NI    RETRY(EWAPTR),B'11111101'                        01S0224
*   IF IOSDVRID=IOSVPSID            /* FOR VPSS SAVE THE CSW FOR THE */
*       &PREV=OFF THEN              /* USER UNLESS ALREADY DONE.     */
         CLI   IOSDVRID(APIOPTR),X'0B'                          01S0225
         BNE   @RF00225                                         01S0225
         TM    PREV(EWAPTR),B'10000000'                         01S0225
         BNZ   @RF00225                                         01S0225
*     DO;                                                       01S0226
*       MVC(SAVEVCSW(1:8),MYCSW);   /* INCLUDE THE SIO CODE.         */
         MVC   SAVEVCSW(8,EWAPTR),MYCSW(APIOPTR)                01S0227
*       APIOVCSW=ADDR(IOSCC);       /* ADDRESS OF THE VIRTUAL CSW.   */
         LA    @11,IOSCC(,APIOPTR)                              01S0228
         ST    @11,APIOVCSW(,APIOPTR)                           01S0228
*     END;                                                      01S0229
*   DO;                                                         02S0230
*                                                               02S0230
@RF00225 DS    0H                                               02S0231
*     /***************************************************************/
*     /*                                                             */
*     /* THIS CODE EXAMINES THE CHAINING INDICATORS IN THE IOSB TO   */
*     /* DETERMINE IF THE CHANNEL PROGRAM MAY BE RESTARTED OR        */
*     /* RETRIED. RESTART MEANS THAT OPERATION BEGINS WITH THE FAILED*/
*     /* CCW. RETRY MEANS THAT CHANNEL PROGRAM IS STARTED AT THE     */
*     /* BEGINNING. IN GENERAL, ALL CHANNEL PROGRAMS ARE RESTARTABLE */
*     /* UNLESS DATA CHAINING IS INVOLVED, WHICH SHOULD OCCUR        */
*     /* INFREQUENTLY. RETRY IS POSSIBLE ONLY IF THE INPUT AND OUTPUT*/
*     /* AREAS DON'T OVERLAP.                                @ZA25369*/
*     /* FOR EXCP CALLERS, NEITHER RESTART NOR RETRY IS      @ZA25369*/
*     /* ATTEMPTED. THE ONLY EXPECTED USER OF EXCP IS THE VPSS       */
*     /* MONITOR AND IT DOES NOT WANT ANY RECOVERY PERFORMED.        */
*     /*                                                     @ZA25369*/
*     /*                                                     @ZA25369*/
*     /*                                                             */
*     /***************************************************************/
*                                                               02S0231
*     IF IOSCSWCA-8=IOSVST THEN     /* IF FAILURE WAS ON FIRST CCW,  */
         L     @11,IOSCSWCA-1(,APIOPTR)                         02S0231
         LA    @11,0(,@11)                                      02S0231
         SL    @11,@CF00059                                     02S0231
         C     @11,IOSVST(,APIOPTR)                             02S0231
         BNE   @RF00231                                         02S0231
*       DO;                         /* ASSUME THAT THE CHANNEL  02S0232
*                                      PROGRAM                       */
*         RETRY=YES;                /* MAY BE RETRIED,               */
*         RESTART=NO;               /* BUT NOT RESTARTED (WHICH 02S0234
*                                      INVOLVES THE RESTART CCW,
*                                      ETC).                         */
         OI    RETRY(EWAPTR),B'00000010'                        02S0234
         NI    RESTART(EWAPTR),B'11110111'                      02S0234
*       END;                        /* END OF FISRT CCW CHECK.       */
*     ELSE                          /* SEE IF OTHER CONDITIONS AFFECT*/
*       DO;                         /* RETRY AND RESTART.            */
         B     @RC00231                                         02S0236
@RF00231 DS    0H                                               02S0237
*         IF IOSDVRID=IOSVPSID      /* IF THE DRIVER IS VPSS,        */
*             &APIOEXCL=OFF THEN    /* * AND THE PORT IS SHARED.     */
         CLI   IOSDVRID(APIOPTR),X'0B'                          02S0237
         BNE   @RF00237                                         02S0237
         TM    APIOEXCL(APIOPTR),B'10000000'                    02S0237
         BNZ   @RF00237                                         02S0237
*           IF IOSACHN='00'B THEN   /* IF NO CHAINING, THEN MAY 02S0238
*                                      RETRY.                        */
         TM    IOSACHN(APIOPTR),B'11000000'                     02S0238
         BNZ   @RF00238                                         02S0238
*             RETRY=YES;                                        02S0239
         OI    RETRY(EWAPTR),B'00000010'                        02S0239
*           ELSE                                                02S0240
*             IF IOSDCHN=ON&        /* IF ONLY DATA CHAINING,        */
*                 IOSCCHN=OFF THEN  /* * MAY RETRY, BUT NOT RESTART. */
         B     @RC00238                                         02S0240
@RF00238 TM    IOSDCHN(APIOPTR),B'10000000'                     02S0240
         BNO   @RF00240                                         02S0240
         TM    IOSCCHN(APIOPTR),B'01000000'                     02S0240
         BNZ   @RF00240                                         02S0240
*               DO;                                             02S0241
*                 RETRY=YES;                                    02S0242
*                 RESTART=NO;                                   02S0243
         OI    RETRY(EWAPTR),B'00000010'                        02S0243
         NI    RESTART(EWAPTR),B'11110111'                      02S0243
*               END;                                            02S0244
*             ELSE                                              02S0245
*               DO;                                             03S0245
*                                                               03S0245
         B     @RC00240                                         03S0245
@RF00240 DS    0H                                               03S0246
*                 /***************************************************/
*                 /*                                                 */
*                 /* DATA CHAINING PRECLUDES A RESTART OF THE CHANNEL*/
*                 /* PROGRAM, BUT A RETRY MIGHT BE IN ORDER. RESTART */
*                 /* IS OUT BECAUSE WE MIGHT BEGIN OVER IN THE       */
*                 /* 'MIDDLE' OF AN I/O THAT HAS BEEN SPLIT INTO TWO */
*                 /* PHYSICAL I/O OPERATIONS. BUT IF NO DATA HAS BEEN*/
*                 /* OVERLAID BY ANY BY ANY READ OPERATIONS, WE CAN  */
*                 /* RETRY THE WHOLE OPERATION.                      */
*                 /*                                                 */
*                 /***************************************************/
*                                                               03S0246
*                 IF IOSCSWCA<ADDR(CCW2)×/* INSURE THAT THE FAILING
*                                      CCW IS NOT IN CCWS THAT WE
*                                      HAVE BUILT AND ARE THEREFORE
*                                      NOT CONTIGUOUS WITH THE CHAIN.*/
*                     IOSCSWCA>ADDR(CCW3) THEN/* IF NOT ONE OF OURS
*                                      THEN CHECK FOR OVERLAP.       */
         L     @11,IOSCSWCA-1(,APIOPTR)                         03S0246
         LA    @11,0(,@11)                                      03S0246
         LA    @09,CCW2(,EWAPTR)                                03S0246
         CR    @11,@09                                          03S0246
         BL    @RT00246                                         03S0246
         LA    @09,CCW3(,EWAPTR)                                03S0246
         CR    @11,@09                                          03S0246
         BNH   @RF00246                                         03S0246
@RT00246 DS    0H                                               03S0247
*                   IF APIOOLAP=0   /* THIS FIELD CONTAINS THE OFFSET
*                                      TO THE FIRST CCW THAT OVERLAYS
*                                      DATA THAT ANOTHER CCW WILL
*                                      USE. IF IT IS ZERO THERE IS NO
*                                      OVERLAP SO MAY RETRY.         */
*                       ×APIOOLAP>= /* ELSE MUST SEE IF OVERLAP 03S0247
*                                      OCCURRED.                     */
*                       (IOSCSWCA-IOSVST)/* THIS VALUE IS THE OFFSET
*                                      TO THE CCW THAT FAILED.       */
*                     THEN          /* IF CCW IN SAFE AREA.          */
         LH    @11,APIOOLAP(,APIOPTR)                           03S0247
         LTR   @11,@11                                          03S0247
         BZ    @RT00247                                         03S0247
         L     @09,IOSCSWCA-1(,APIOPTR)                         03S0247
         LA    @09,0(,@09)                                      03S0247
         SL    @09,IOSVST(,APIOPTR)                             03S0247
         CR    @11,@09                                          03S0247
         BL    @RF00247                                         03S0247
@RT00247 DS    0H                                               03S0248
*                     RETRY=YES;    /* SET RETRY = OK.               */
         OI    RETRY(EWAPTR),B'00000010'                        03S0248
*                 IF IOSDCHN=ON     /* IF DATA CHAINING IS ON, THEN
*                                      IT MIGHT STILL BE POSSIBLE TO
*                                      RESTART BUT ONLY IF THE CCW
*                                      THAT FAILED WAS NOT DATA 03S0249
*                                      CHAINED TO.                   */
*                     &(IOSCSWCA<ADDR(CCW2)×/* ENSURE THAT FAILURE
*                                      WAS NOT IN                    */
*                     IOSCSWCA>ADDR(CCW3))/* THE RESTART CCWS   03S0249
*                                      THEMSELVES.                   */
*                     &IOSCSWCA-16>=IOSVST/* ENSURE THAT WE DON'T GO
*                                      TOO FAR.                      */
*                     &(IOSCSWCA-16)->CCWDC=ON/* FINALLY, IS THE CCW
*                                      DATA                          */
*                   THEN            /* CHAINED TO?                   */
@RF00247 DS    0H                                               03S0249
@RF00246 TM    IOSDCHN(APIOPTR),B'10000000'                     03S0249
         BNO   @RF00249                                         03S0249
         L     @11,IOSCSWCA-1(,APIOPTR)                         03S0249
         LA    @11,0(,@11)                                      03S0249
         LA    @09,CCW2(,EWAPTR)                                03S0249
         CR    @11,@09                                          03S0249
         BL    @GL00006                                         03S0249
         LA    @09,CCW3(,EWAPTR)                                03S0249
         CR    @11,@09                                          03S0249
         BNH   @RF00249                                         03S0249
@GL00006 L     @11,IOSCSWCA-1(,APIOPTR)                         03S0249
         LA    @11,0(,@11)                                      03S0249
         SL    @11,@CF00040                                     03S0249
         C     @11,IOSVST(,APIOPTR)                             03S0249
         BL    @RF00249                                         03S0249
         TM    CCWDC(@11),B'10000000'                           03S0249
         BNO   @RF00249                                         03S0249
*                   RESTART=NO;     /* RESTART IS JUST NOT POSSIBLE. */
         NI    RESTART(EWAPTR),B'11110111'                      03S0250
*               END;                                            03S0251
*         ELSE                                                  02S0252
*           IF IOSDCHN=YES THEN     /* DOESNT MAKE ANY DIFFERENCE WHO*/
         B     @RC00237                                         02S0252
@RF00237 TM    IOSDCHN(APIOPTR),B'10000000'                     02S0252
         BNO   @RF00252                                         02S0252
*             RESTART=NO;           /* YOU ARE, IF DATA CHN, NO 02S0253
*                                      RESTRT.                       */
         NI    RESTART(EWAPTR),B'11110111'                      02S0253
*       END;                        /* END OF RETRY/RESTART CHECK.   */
@RF00252 DS    0H                                               02S0254
@RC00237 DS    0H                                               02S0255
*     IF IOSDVRID^=IOSVPSID THEN    /* CHECK FOR EXCP USER.  @ZA25369*/
@RC00231 CLI   IOSDVRID(APIOPTR),X'0B'                          02S0255
         BE    @RF00255                                         02S0255
*       DO;                         /* RESET FLAGS.          @ZA25369*/
*         RETRY=NO;                 /* NO RETRY IS ALLOWED.  @ZA25369*/
*         RESTART=NO;               /* NO RESTART IS ALLOWED.   02S0258
*                                                            @ZA25369*/
         NI    RETRY(EWAPTR),B'11110101'                        02S0258
*       END;                        /* END OF PROCESS EXCP USER.
*                                                            @ZA25369*/
*   END;                                                        02S0260
@RF00255 DS    0H                                               02S0261
*   RFY                                                         01S0261
*    (R0P,                                                      01S0261
*     R1P,                                                      01S0261
*     R9P,                                                      01S0261
*     R11P,                                                     01S0261
*     R12P,                                                     01S0261
*     R14P,                                                     01S0261
*     R15P) RSTD;                   /* USED BY INTERPRETER.          */
*   R1P=ADDR(IOSB);                 /* ADDRESS OF IOSB.              */
         LR    R1P,APIOPTR                                      01S0262
*   R15P=CVTXITP;                   /* ADDR OF INTERPRETER ROUTINE.  */
         L     @03,CVTPTR                                       01S0263
         L     R15P,CVTXITP(,@03)                               01S0263
*   GEN REFS(R15P) SETS(R14P) FLOWS(CHANERR,LOGMSG,UNITCK,ALLCLEAR)
*       NOSEQFLOW;                                              01S0264
*  THIS CODE INVOKES THE ERP BIT ANALYZER TO DETERMINE WHAT TO DO.
T1       BALR  R14P,R15P          CALL ERROR INTERPRETER ROUTINE.
         DC    X'1D',AL1(TCCC-T1-2)     CHAN CONTROL CK
         DC    X'1E',AL1(TCCC-T1-4)     INTERFACE CONTROL CK
         DC    X'1C',AL1(TCCC-T1-6)     CHAN DATA CK
         DC    X'10',AL1(TUNUSED-T1-8)  ATTN (UNUSED)
         DC    X'17',AL1(TUNUSED-T1-10) UNIT EXC (UNUSED)
         DC    X'16',AL1(TUNIT-T1-12)   UNIT CK
         DC    X'1F',AL1(TUSER-T1-14)   CHAIN CK
         DC    X'1A',AL1(TUSER-T1-16)   PROG CK
         DC    X'1B',AL1(TUSER-T1-18)   PROTECT CK
         DC    X'19',AL1(TILEN-T1-20)   INCORRECT LENGTH
         DC    X'2F',AL1(TGOOD-T1-22)   NO ERROR THIS ENTRY TO ERP.
TCCC     B     CHANERR
TUNUSED  B     LOGMSG
TUNIT    B     UNITCK
TUSER    B     PROGERR
TGOOD    B     ALLCLEAR
TILEN    B     ILLEN              GO TO ILLEGAL LENGTH ROUTINE.
*   RFY                                                         01S0265
*    (R0P,                                                      01S0265
*     R1P,                                                      01S0265
*     R9P,                                                      01S0265
*     R11P,                                                     01S0265
*     R12P,                                                     01S0265
*     R14P,                                                     01S0265
*     R15P) UNRSTD;                                             01S0265
*CHANERR:                                                       01S0266
*   ;                               /* ERROR WAS CCC, CDC, OR ICC.   */
CHANERR  DS    0H                                               01S0267
*   DO;                                                         02S0267
*                                                               02S0267
*     /***************************************************************/
*     /*                                                             */
*     /* PROCESS CCC, CDC AND ICC.                                   */
*     /*                                                             */
*     /***************************************************************/
*                                                               02S0268
*     RESTART=NO;                   /* RESTART IS NOT POSSIBLE EXCEPT
*                                      * FOR UNIT CHECKS.            */
         NI    RESTART(EWAPTR),B'11110111'                      02S0268
*     IF IOSCOD=IOSMIHC THEN        /* IF MISSING INTERRUPT THE CSW *
*                                      IS MADE UP BY MIH AND SO IS *
*                                      NOT VALID.                    */
         CLI   IOSCOD(APIOPTR),X'74'                            02S0269
         BNE   @RF00269                                         02S0269
*       IF RETRY=YES                /* CHECK TO SEE IF RETRY OK.     */
*           &PREV=OFF THEN          /* BUT ONLY IF NOT TRIED BEFORE. */
         TM    RETRY(EWAPTR),B'00000010'                        02S0270
         BNO   @RF00270                                         02S0270
         TM    PREV(EWAPTR),B'10000000'                         02S0270
         BNZ   @RF00270                                         02S0270
*         DO;                       /* PROCESS RETRY REQUEST.        */
*           PREV=YES;               /* INDICATE THAT IT TRIED ONCE.  */
         OI    PREV(EWAPTR),B'10000000'                         02S0272
*           GO TO DOITOVER;         /* GO TRY AGAIN.                 */
         B     DOITOVER                                         02S0273
*         END;                      /* END OF RETRY PROCESS.         */
*       ELSE                        /* IT CAN'T BE RETRIED.          */
*         GO TO LOGMSG;             /* SO IT IS A PERM ERROR.        */
*     IF EWANORTY=ON THEN           /* IF CCH SAYS NO, THEN FORGET
*                                      IT.                           */
@RF00269 TM    EWANORTY(EWAPTR),B'00000001'                     02S0276
         BO    @RT00276                                         02S0276
*       GO TO LOGMSG;                                           02S0277
*     IF PREV=YES THEN              /* IF BEEN HERE BEFORE, GIVE UP. */
         TM    PREV(EWAPTR),B'10000000'                         02S0278
         BO    @RT00278                                         02S0278
*       GOTO LOGMSG;                                            02S0279
*     ELSE                                                      02S0280
*       DO;                                                     02S0280
*         PREV=YES;                 /* SET HERE NOW.                 */
         OI    PREV(EWAPTR),B'10000000'                         02S0281
*         GOTO DOITOVER;                                        02S0282
         B     DOITOVER                                         02S0282
*       END;                                                    02S0283
*   END;                                                        02S0284
*UNITCK:                                                        01S0285
*   ;                               /* ERROR WAS UNIT CHECK.         */
UNITCK   DS    0H                                               01S0286
*   DO;                                                         02S0286
*     IF((IOSCSWCA-8)->CCWOPCD      /* CHECH THE CCW COMMAND @ZA25442*/
*         ^='02'X) THEN             /* CODE FOR A WRITE.     @ZA25442*/
         L     @11,IOSCSWCA-1(,APIOPTR)                         02S0287
         AL    @11,@CF03261                                     02S0287
         CLI   CCWOPCD(@11),X'02'                               02S0287
         BE    @RF00287                                         02S0287
*       EWAWTEMP=ON;                /* IF NOT READ,THEN INDICATE TO
*                                      STATISTICS UPDATE THAT IT IS A
*                                      WRITE.                @ZA25442*/
         OI    EWAWTEMP(EWAPTR),B'01000000'                     02S0288
*     IF EWABDSNS=OFF THEN          /* IF SENSE OK,                  */
@RF00287 TM    EWABDSNS(EWAPTR),B'00000001'                     02S0289
         BNZ   @RF00289                                         02S0289
*       DO;                                                     02S0290
*         EWASTUP=EWASTUP×APSN(1:2);/* OR IN SENSE FOR STATISTICS.   */
         LA    @11,32                                           02S0291
         ALR   @11,EWAPTR                                       02S0291
         OC    EWASTUP(2,EWAPTR),APSN(@11)                      02S0291
*         EWASTUP=EWASTUP&'7F00'X;  /* TURN OFF NON ESSENSIAL BITS.  */
         NC    EWASTUP(2,EWAPTR),@CB03043                       02S0292
*         IF APSNRQUR=ON THEN       /* IF 3838 SAYS THAT THE ERROR IS
*                                      UNRELATED TO THE CURRENT 02S0293
*                                      REQUEST THEN WE CAN ALWAYS
*                                      RETRY THE CHANNEL PROGRAM.    */
         TM    APSNRQUR(@11),B'01000000'                        02S0293
         BO    @RT00293                                         02S0293
