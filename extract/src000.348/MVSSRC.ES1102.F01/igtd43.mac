TD43     TITLE 'IGTD43 - ENABLE COMMAND PROCESSOR.                     *
                        '
IGTD43   CSECT ,                                                01S0001
@MAINENT DS    0H                                               01S0001
         USING *,@15                                            01S0001
         B     @PROLOG                                          01S0001
         DC    AL1(16)                                          01S0001
         DC    C'IGTD43    77.092'                              01S0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                  01S0001
         BALR  @07,0                                            01S0001
@PSTART  DS    0H                                               01S0001
         USING @PSTART,@07                                      01S0001
         L     @00,@SIZDATD                                     01S0001
         IGTYGSV  R,LV=(0)
         LR    @06,@01                                          01S0001
         USING @DATD,@06                                        01S0001
         ST    @13,@SA00001+4                                   01S0001
         LM    @00,@01,20(@13)                                  01S0001
         ST    @06,8(,@13)                                      01S0001
         LR    @13,@06                                          01S0001
*   IF APCTHALT='0'B THEN           /* IF SYSTEM IS NOT HALTING.     */
         TM    APCTHALT(APCTPTR),B'00010000'                    01S0104
         BNZ   @RF00104                                         01S0104
*     DO;                                                       01S0105
*       CALL APCVZDEQ;              /* GET APUBS.                    */
         L     @15,APCTZDEQ(,APCTPTR)                           01S0106
         BALR  @14,@15                                          01S0106
*       PARMLEN=APCQPLEN;           /* SET UP FOR IGTDAP = LENGTH.   */
         LH    @11,APCQPLEN(,APCQPTR)                           01S0107
         STH   @11,PARMLEN                                      01S0107
*       PARMADDR=ADDR(APCQCMND)     /* START ADDRESS.                */
*           +APCQPSTR-1;                                        01S0108
         LA    @08,APCQCMND(,APCQPTR)                           01S0108
         LH    @15,APCQPSTR(,APCQPTR)                           01S0108
         ALR   @08,@15                                          01S0108
         BCTR  @08,0                                            01S0108
         ST    @08,PARMADDR                                     01S0108
*       WORKAREA=''X;               /* CLEAR WORKAREA.               */
         XC    WORKAREA(8),WORKAREA                             01S0109
*       IF APCQPLEN>=3 THEN         /* MOVE IN FIRST PARM IF PRESENT.*/
         C     @11,@CF00384                                     01S0110
         BL    @RF00110                                         01S0110
*         WORKAREA(2:4)=APCQCMND(APCQPSTR:APCQPSTR+2);          01S0111
         ALR   @15,APCQPTR                                      01S0111
         MVC   WORKAREA+1(3),APCQCMND-1(@15)                    01S0111
*       DO UNTIL RC=4               /* DO ALL UNITS,                 */
*             ×RC=8;                /* OR UNTIL ERROR.               */
@RF00110 DS    0H                                               01S0112
@DL00112 DS    0H                                               01S0113
*         CALL APCVZDAP(PARMADDR,PARMLEN,WORKAREA);/* GET NEXT UNIT. */
         LA    @11,PARMADDR                                     01S0113
         ST    @11,@AL00001                                     01S0113
         LA    @11,PARMLEN                                      01S0113
         ST    @11,@AL00001+4                                   01S0113
         LA    @11,WORKAREA                                     01S0113
         ST    @11,@AL00001+8                                   01S0113
         L     @15,APCTZDAP(,APCTPTR)                           01S0113
         LA    @01,@AL00001                                     01S0113
         BALR  @14,@15                                          01S0113
*         RC=R15P;                  /* SAVE RETURN CODE.             */
         LR    RC,R15P                                          01S0114
*         IF RC=8 THEN              /* IF ERROR.                     */
         C     RC,@CF00044                                      01S0115
         BNE   @RF00115                                         01S0115
*           DO;                                                 01S0116
*             CODE='08';            /* OUTPUT AN ERRORMESSAGE=       */
         MVC   CODE(2),@CC01131                                 01S0117
*             DO;                                               02S0118
*               IF CODE='08' THEN   /* IF DEVICE NOT FOUND, OUTPUT   */
         CLC   CODE(2),@CC01131                                 02S0119
         BNE   @RF00119                                         02S0119
*                 CALL APCVZDWT(IGT235I,/* INVALID DEVICE MESSAGE.   */
*                     14,APCTSSID,4,20,WORKAREA(2:4),3);        02S0120
*                                                               02S0120
         MVC   @AL00001(28),@AL00120                            02S0120
         LA    @11,APCTSSID(,APCTPTR)                           02S0120
         ST    @11,@AL00001+8                                   02S0120
         LA    @11,WORKAREA+1                                   02S0120
         ST    @11,@AL00001+20                                  02S0120
         L     @15,APCTZDWT(,APCTPTR)                           02S0120
         LA    @01,@AL00001                                     02S0120
         BALR  @14,@15                                          02S0120
*               /*****************************************************/
*               /*                                                   */
*               /* OTHERWISE, WRITE CODED MSG.                       */
*               /*                                                   */
*               /*****************************************************/
*                                                               02S0121
*               ELSE                                            02S0121
*                 CALL APCVZDWT(IGT236I,/* WRITE THE ERROR MESSAGE.  */
*                     14,APCTSSID,4,20,UCBNAME,3,42,CODE,2);    02S0121
         B     @RC00119                                         02S0121
@RF00119 MVC   @AL00001(40),@AL00121                            02S0121
         LA    @11,APCTSSID(,APCTPTR)                           02S0121
         ST    @11,@AL00001+8                                   02S0121
         L     @11,UCBPTR(,APUBPTR)                             02S0121
         LA    @11,UCBNAME(,@11)                                02S0121
         ST    @11,@AL00001+20                                  02S0121
         LA    @11,CODE                                         02S0121
         ST    @11,@AL00001+32                                  02S0121
         L     @15,APCTZDWT(,APCTPTR)                           02S0121
         LA    @01,@AL00001                                     02S0121
         BALR  @14,@15                                          02S0121
*             END;                                              02S0122
@RC00119 DS    0H                                               02S0123
*           END;                                                01S0123
*         ELSE                                                  01S0124
*           IF APUBUNLD='1'B THEN   /* IF DEVICE IS UNLOADED,        */
         B     @RC00115                                         02S0124
@RF00115 TM    APUBUNLD(APUBPTR),B'00100000'                    02S0124
         BNO   @RF00124                                         02S0124
*             DO;                                               02S0125
*               CODE='16';          /* GENERATE AN APPROPRIATE MSG.  */
         MVC   CODE(2),@CC01134                                 02S0126
*               DO;                                             03S0127
*                 IF CODE='08' THEN /* IF DEVICE NOT FOUND, OUTPUT   */
         CLC   CODE(2),@CC01131                                 03S0128
         BNE   @RF00128                                         03S0128
*                   CALL APCVZDWT(IGT235I,/* INVALID DEVICE MESSAGE. */
*                       14,APCTSSID,4,20,WORKAREA(2:4),3);      03S0129
*                                                               03S0129
         MVC   @AL00001(28),@AL00129                            03S0129
         LA    @11,APCTSSID(,APCTPTR)                           03S0129
         ST    @11,@AL00001+8                                   03S0129
         LA    @11,WORKAREA+1                                   03S0129
         ST    @11,@AL00001+20                                  03S0129
         L     @15,APCTZDWT(,APCTPTR)                           03S0129
         LA    @01,@AL00001                                     03S0129
         BALR  @14,@15                                          03S0129
*                 /***************************************************/
*                 /*                                                 */
*                 /* OTHERWISE, WRITE CODED MSG.                     */
*                 /*                                                 */
*                 /***************************************************/
*                                                               03S0130
*                 ELSE                                          03S0130
*                   CALL APCVZDWT(IGT236I,/* WRITE THE ERROR MESSAGE.*/
*                       14,APCTSSID,4,20,UCBNAME,3,42,CODE,2);  03S0130
         B     @RC00128                                         03S0130
@RF00128 MVC   @AL00001(40),@AL00130                            03S0130
         LA    @11,APCTSSID(,APCTPTR)                           03S0130
         ST    @11,@AL00001+8                                   03S0130
         L     @11,UCBPTR(,APUBPTR)                             03S0130
         LA    @11,UCBNAME(,@11)                                03S0130
         ST    @11,@AL00001+20                                  03S0130
         LA    @11,CODE                                         03S0130
         ST    @11,@AL00001+32                                  03S0130
         L     @15,APCTZDWT(,APCTPTR)                           03S0130
         LA    @01,@AL00001                                     03S0130
         BALR  @14,@15                                          03S0130
*               END;                                            03S0131
@RC00128 DS    0H                                               03S0132
*             END;                                              02S0132
*           ELSE                                                02S0133
*             IF APUBZRGN='1'B THEN /* IF DEVICE HAS NO REGION       */
         B     @RC00124                                         02S0133
@RF00124 TM    APUBZRGN(APUBPTR),B'10000000'                    02S0133
         BNO   @RF00133                                         02S0133
*               DO;                                             02S0134
*                 CODE='12';        /* GENERATE ERROR MESSAGE.       */
         MVC   CODE(2),@CC01135                                 02S0135
*                 DO;                                           03S0136
*                   IF CODE='08' THEN/* IF DEVICE NOT FOUND, OUTPUT  */
         CLC   CODE(2),@CC01131                                 03S0137
         BNE   @RF00137                                         03S0137
*                     CALL APCVZDWT(IGT235I,/* INVALID DEVICE   03S0138
*                                      MESSAGE.                      */
*                         14,APCTSSID,4,20,WORKAREA(2:4),3);    03S0138
*                                                               03S0138
         MVC   @AL00001(28),@AL00138                            03S0138
         LA    @11,APCTSSID(,APCTPTR)                           03S0138
         ST    @11,@AL00001+8                                   03S0138
         LA    @11,WORKAREA+1                                   03S0138
         ST    @11,@AL00001+20                                  03S0138
         L     @15,APCTZDWT(,APCTPTR)                           03S0138
         LA    @01,@AL00001                                     03S0138
         BALR  @14,@15                                          03S0138
*                   /*************************************************/
*                   /*                                               */
*                   /* OTHERWISE, WRITE CODED MSG.                   */
*                   /*                                               */
*                   /*************************************************/
*                                                               03S0139
*                   ELSE                                        03S0139
*                     CALL APCVZDWT(IGT236I,/* WRITE THE ERROR  03S0139
*                                      MESSAGE.                      */
*                         14,APCTSSID,4,20,UCBNAME,3,42,CODE,2);
         B     @RC00137                                         03S0139
@RF00137 MVC   @AL00001(40),@AL00139                            03S0139
         LA    @11,APCTSSID(,APCTPTR)                           03S0139
         ST    @11,@AL00001+8                                   03S0139
         L     @11,UCBPTR(,APUBPTR)                             03S0139
         LA    @11,UCBNAME(,@11)                                03S0139
         ST    @11,@AL00001+20                                  03S0139
         LA    @11,CODE                                         03S0139
         ST    @11,@AL00001+32                                  03S0139
         L     @15,APCTZDWT(,APCTPTR)                           03S0139
         LA    @01,@AL00001                                     03S0139
         BALR  @14,@15                                          03S0139
*                 END;                                          03S0140
@RC00137 DS    0H                                               03S0141
*               END;                                            02S0141
*             ELSE                  /* OTHERWISE ENABLE IT.          */
*               DO;                                             02S0142
         B     @RC00133                                         02S0142
@RF00133 DS    0H                                               02S0143
*                 IF APUBSBCH='0'B THEN/* IF IT IS A MASTER PORT THEN
*                                      ONLY THE MESSAGE IS REQUIRED. */
         TM    APUBSBCH(APUBPTR),B'10000000'                    02S0143
         BNZ   @RF00143                                         02S0143
*                   DO;             /* PROCESS A USER PORT.          */
*                     APUBDSBL='0'B;/* RESET DISABLED.               */
         L     @11,APUBDSBL-1(,APUBPTR)                         02S0145
@GL00001 LR    @15,@11                                          02S0145
         N     @15,@CF01231                                     02S0145
         CS    @11,@15,APUBDSBL-1(APUBPTR)                      02S0145
         BNE   @GL00001                                         02S0145
*                     APUBIDAB='0'B;/* RESET I/O DISABLED.           */
         L     @11,APUBIDAB(,APUBPTR)                           02S0146
@GL00002 LR    @15,@11                                          02S0146
         N     @15,@CF01234                                     02S0146
         CS    @11,@15,APUBIDAB(APUBPTR)                        02S0146
         BNE   @GL00002                                         02S0146
*                     APUBBERR='0'B;/* RESET ERROR DISABLED.         */
         L     @11,APUBBERR-1(,APUBPTR)                         02S0147
@GL00003 LR    @15,@11                                          02S0147
         N     @15,@CF01237                                     02S0147
         CS    @11,@15,APUBBERR-1(APUBPTR)                      02S0147
         BNE   @GL00003                                         02S0147
*                     APUBIERR='0'B;/* RESET I/O ERROR.              */
         L     @11,APUBIERR-1(,APUBPTR)                         02S0148
@GL00004 LR    @15,@11                                          02S0148
         N     @15,@CF01240                                     02S0148
         CS    @11,@15,APUBIERR-1(APUBPTR)                      02S0148
         BNE   @GL00004                                         02S0148
*                     APUBDSBP='0'B;/* RESET DISABLE PENDING.        */
         L     @11,APUBDSBP-1(,APUBPTR)                         02S0149
@GL00005 LR    @15,@11                                          02S0149
         N     @15,@CF01243                                     02S0149
         CS    @11,@15,APUBDSBP-1(APUBPTR)                      02S0149
         BNE   @GL00005                                         02S0149
*                     APUBERRS=''X; /* ZERO ERROR CTRS.              */
         XC    APUBERRS(22,APUBPTR),APUBERRS(APUBPTR)           02S0150
*                     DO;           /* IGTYPOST ECB(APCTEULD)        */
*                       RFY                                     02S0152
*                        (GPR00P,                               02S0152
*                         GPR01P,                               02S0152
*                         GPR14F,                               02S0152
*                         GPR15F) RSTD;/* REGISTERS USED             */
*                       GPR15F=APCTEULD;/* CURRENT ECB CONTENTS      */
         L     GPR15F,APCTEULD(,APCTPTR)                        02S0153
*LTR0009X:                                                      02S0154
*                       IF GPR15F<0 THEN/* IF ECB ALREADY            */
LTR0009X LTR   GPR15F,GPR15F                                    02S0154
         BNM   @RF00154                                         02S0154
*                         DO;       /* WAITED UPON, MUST ISSUE POST  */
*                           GPR01P=ADDR(APCTEULD);/* ADDRESS OF ECB
*                                      TO POST                       */
         LA    GPR01P,APCTEULD(,APCTPTR)                        02S0156
*                           GPR00P=0;/* POST CODE                    */
         SLR   GPR00P,GPR00P                                    02S0157
*                           SVC(2); /* ISSUE POST                    */
         SVC   2                                                02S0158
*                         END;      /* ECB POSTED                    */
*                       ELSE        /* ECB NOT YET POSTED            */
*                         DO;       /* SO TRY 'COMPARE-AND-SWAP' POST*/
         B     @RC00154                                         02S0160
@RF00154 DS    0H                                               02S0161
*                           GPR14F='40000000'X;/* POST CODE          */
         L     GPR14F,@CF01143                                  02S0161
*                           CS(GPR15F,GPR14F,APCTEULD);/* POST THE
*                                      ECB                           */
         CS    GPR15F,@14,APCTEULD(APCTPTR)                     02S0162
*                           BC(7,LTR0009X);/* RETRY IF 'CS' FAILED   */
         BC    7,LTR0009X                                       02S0163
*                         END;      /* ECB POSTED VIA 'CS'           */
*                       RFY                                     02S0165
*                        (GPR00P,                               02S0165
*                         GPR01P,                               02S0165
*                         GPR14F,                               02S0165
*                         GPR15F) UNRSTD;/* DONE WITH REGS           */
@RC00154 DS    0H                                               02S0166
*                     END;          /* POST LOGIC COMPLETE CAUSE
*                                      DISABLE/UNLOAD SCAN.          */
*                     APCTACTR=APCTACTR+1;/* INCREMENT ALLOCATION
*                                      COUNTER.                      */
         LA    @11,1                                            02S0167
         AL    @11,APCTACTR(,APCTPTR)                           02S0167
         ST    @11,APCTACTR(,APCTPTR)                           02S0167
*                     DO;           /* IGTYPOST                 02S0168
*                                      ECB(APCTEALP)ASCBLOC(APCTAALP)*/
*                       RFY                                     02S0169
*                        (GPR00P,                               02S0169
*                         GPR01P,                               02S0169
*                         GPR14F,                               02S0169
*                         GPR15F) RSTD;/* REGISTERS USED             */
*                       GPR15F=APCTEALP;/* CURRENT ECB CONTENTS      */
         L     GPR15F,APCTEALP(,APCTPTR)                        02S0170
*LTR0011X:                                                      02S0171
*                       IF GPR15F<0 THEN/* IF ECB ALREADY            */
LTR0011X LTR   GPR15F,GPR15F                                    02S0171
         BNM   @RF00171                                         02S0171
*                         DO;       /* WAITED UPON, MUST ISSUE POST  */
*                           GPR01P=ADDR(APCTEALP);/* ADDRESS OF ECB
*                                      TO POST                       */
         LA    GPR01P,APCTEALP(,APCTPTR)                        02S0173
*                           GPR00P=APCTAALP;/* ADDR OF ASCB FOR 02S0174
*                                      MEMORY TO BE POSTED           */
         L     GPR00P,APCTAALP(,APCTPTR)                        02S0174
*                           CALL APCVZUBB;/* ISSUE CROSS-MEMORY POST */
         L     @15,APCTZUBB(,APCTPTR)                           02S0175
         BALR  @14,@15                                          02S0175
*                         END;      /* ECB POSTED                    */
*                       ELSE        /* ECB NOT YET POSTED            */
*                         DO;       /* SO TRY 'COMPARE-AND-SWAP' POST*/
         B     @RC00171                                         02S0177
@RF00171 DS    0H                                               02S0178
*                           GPR14F='40000000'X;/* POST CODE          */
         L     GPR14F,@CF01143                                  02S0178
*                           CS(GPR15F,GPR14F,APCTEALP);/* POST THE
*                                      ECB                           */
         CS    GPR15F,@14,APCTEALP(APCTPTR)                     02S0179
*                           BC(7,LTR0011X);/* RETRY IF 'CS' FAILED   */
         BC    7,LTR0011X                                       02S0180
*                         END;      /* ECB POSTED VIA 'CS'           */
*                       RFY                                     02S0182
*                        (GPR00P,                               02S0182
*                         GPR01P,                               02S0182
*                         GPR14F,                               02S0182
*                         GPR15F) UNRSTD;/* DONE WITH REGS           */
@RC00171 DS    0H                                               02S0183
*                     END;          /* POST LOGIC COMPLETE POST 02S0183
*                                      ALLOC.                        */
*                     CALL APCVZUHA(APUB);/* ADD IT TO THE SHARED
*                                      QUEUE.                        */
         ST    APUBPTR,@AL00001                                 02S0184
         L     @15,APCTZUHA(,APCTPTR)                           02S0184
         LA    @01,@AL00001                                     02S0184
         BALR  @14,@15                                          02S0184
*                   END;                                        02S0185
*                                                               02S0185
*                 /***************************************************/
*                 /*                                                 */
*                 /* OUTPUT GOOD MSG.                                */
*                 /*                                                 */
*                 /***************************************************/
*                                                               02S0186
*                 CALL APCVZDWT(IGT237I,14,APCTSSID,4,20,UCBNAME,3);
@RF00143 MVC   @AL00001(28),@AL00186                            02S0186
         LA    @11,APCTSSID(,APCTPTR)                           02S0186
         ST    @11,@AL00001+8                                   02S0186
         L     @11,UCBPTR(,APUBPTR)                             02S0186
         LA    @11,UCBNAME(,@11)                                02S0186
         ST    @11,@AL00001+20                                  02S0186
         L     @15,APCTZDWT(,APCTPTR)                           02S0186
         LA    @01,@AL00001                                     02S0186
         BALR  @14,@15                                          02S0186
*               END;                                            02S0187
*       END;                                                    01S0188
@RC00133 DS    0H                                               01S0188
@RC00124 DS    0H                                               01S0188
@RC00115 DS    0H                                               01S0188
@DE00112 C     RC,@CF00052                                      01S0188
         BE    @DC00112                                         01S0188
         C     RC,@CF00044                                      01S0188
         BNE   @DL00112                                         01S0188
@DC00112 DS    0H                                               01S0189
*       CALL APCVZDDQ;              /* RELEASE APUBS.                */
         L     @15,APCTZDDQ(,APCTPTR)                           01S0189
         BALR  @14,@15                                          01S0189
*     END;                                                      01S0190
*   ELSE                                                        01S0191
*     DO;                                                       01S0191
         B     @RC00104                                         01S0191
@RF00104 DS    0H                                               01S0192
*       CODE='04';                  /* SYSTEM IS QUITTING SO         */
         MVC   CODE(2),@CC01148                                 01S0192
*       DO;                                                     02S0193
*         IF CODE='08' THEN         /* IF DEVICE NOT FOUND, OUTPUT   */
         CLC   CODE(2),@CC01131                                 02S0194
         BNE   @RF00194                                         02S0194
*           CALL APCVZDWT(IGT235I,  /* INVALID DEVICE MESSAGE.       */
*               14,APCTSSID,4,20,WORKAREA(2:4),3);              02S0195
*                                                               02S0195
         MVC   @AL00001(28),@AL00195                            02S0195
         LA    @11,APCTSSID(,APCTPTR)                           02S0195
         ST    @11,@AL00001+8                                   02S0195
         LA    @11,WORKAREA+1                                   02S0195
         ST    @11,@AL00001+20                                  02S0195
         L     @15,APCTZDWT(,APCTPTR)                           02S0195
         LA    @01,@AL00001                                     02S0195
         BALR  @14,@15                                          02S0195
*         /***********************************************************/
*         /*                                                         */
*         /* OTHERWISE, WRITE CODED MSG.                             */
*         /*                                                         */
*         /***********************************************************/
*                                                               02S0196
