*           CALL APCVZDWT(IGT236I,  /* WRITE THE ERROR MESSAGE.      */
*               14,APCTSSID,4,20,UCBNAME,3,42,CODE,2);          02S0196
         B     @RC00194                                         02S0196
@RF00194 MVC   @AL00001(40),@AL00196                            02S0196
         LA    @11,APCTSSID(,APCTPTR)                           02S0196
         ST    @11,@AL00001+8                                   02S0196
         L     @11,UCBPTR(,APUBPTR)                             02S0196
         LA    @11,UCBNAME(,@11)                                02S0196
         ST    @11,@AL00001+20                                  02S0196
         LA    @11,CODE                                         02S0196
         ST    @11,@AL00001+32                                  02S0196
         L     @15,APCTZDWT(,APCTPTR)                           02S0196
         LA    @01,@AL00001                                     02S0196
         BALR  @14,@15                                          02S0196
