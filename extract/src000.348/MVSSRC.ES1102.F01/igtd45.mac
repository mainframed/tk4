TD45     TITLE 'IGTD45 - IPL COMMAND PROCESSOR.                        *
                        '
IGTD45   CSECT ,                                                01S0001
@MAINENT DS    0H                                               01S0001
         USING *,@15                                            01S0001
         B     @PROLOG                                          01S0001
         DC    AL1(16)                                          01S0001
         DC    C'IGTD45    77.092'                              01S0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                  01S0001
         BALR  @07,0                                            01S0001
@PSTART  DS    0H                                               01S0001
         USING @PSTART,@07                                      01S0001
         L     @00,@SIZDATD                                     01S0001
         IGTYGSV  R,LV=(0)
         LR    @06,@01                                          01S0001
         USING @DATD,@06                                        01S0001
         ST    @13,@SA00001+4                                   01S0001
         LM    @00,@01,20(@13)                                  01S0001
         ST    @06,8(,@13)                                      01S0001
         LR    @13,@06                                          01S0001
*   IF APCTHALT='0'B THEN           /* SEE MONITOR IS QUITTING.      */
         TM    APCTHALT(APCTPTR),B'00010000'                    01S0106
         BNZ   @RF00106                                         01S0106
*     DO;                           /* IF NOT, THEN                  */
*       CALL APCVZDEQ;              /* GET CONTROL OF THE APUBS.     */
         L     @15,APCTZDEQ(,APCTPTR)                           01S0108
         BALR  @14,@15                                          01S0108
*       PARMPTR=ADDR(APCQCMND)+APCQPSTR-1;/* ADDR OF PARM FIELD.     */
         LA    PARMPTR,APCQCMND(,APCQPTR)                       01S0109
         AH    PARMPTR,APCQPSTR(,APCQPTR)                       01S0109
         BCTR  PARMPTR,0                                        01S0109
*       RESPECIFY                                               01S0110
*         APUBPTR RESTRICTED;                                   01S0110
*       RESPECIFY                                               01S0111
*         APUB BASED(APUBPTR);                                  01S0111
*       APUBPTR=APCTAPUB;                                       01S0112
         L     APUBPTR,APCTAPUB(,APCTPTR)                       01S0112
*       DO WHILE APUBPTR^=0         /* THIS SIGNIFIES NO SUCH UNIT.  */
*             &UCBNAME^=PARM1;      /* THIS SIGNIFIES GOT IT         */
         B     @DE00113                                         01S0113
@DL00113 DS    0H                                               01S0114
*         APUBPTR=APUBMSTR;         /* PTR TO NEXT MASTER APUB.      */
         L     APUBPTR,APUBMSTR(,APUBPTR)                       01S0114
*       END;                                                    01S0115
@DE00113 LTR   APUBPTR,APUBPTR                                  01S0115
         BZ    @DC00113                                         01S0115
         L     @11,UCBPTR(,APUBPTR)                             01S0115
         CLC   UCBNAME(3,@11),PARM1(PARMPTR)                    01S0115
         BNE   @DL00113                                         01S0115
@DC00113 DS    0H                                               01S0116
*       IF APUBPTR^=0 THEN          /* VERIFY THE OPERATOR SUPPLIED
*                                      PARMS UNLESS THE COMMAND IS
*                                      ALREADY REJECTED.             */
         LTR   APUBPTR,APUBPTR                                  01S0116
         BZ    @RF00116                                         01S0116
*         IF APCQPLEN>3 THEN        /* SEE IF OPERATOR SUPPLIED ANY. */
         LH    @11,APCQPLEN(,APCQPTR)                           01S0117
         C     @11,@CF00041                                     01S0117
         BNH   @RF00117                                         01S0117
*           IF COMMA1^=',' THEN     /* IF ANY, COMMA SEPARATES THEM. */
         CLI   COMMA1(PARMPTR),C','                             01S0118
         BE    @RF00118                                         01S0118
*             APUBPTR=0;            /* INDICATE AN ERROR IN THE 01S0119
*                                      COMMAND.                      */
         SLR   APUBPTR,APUBPTR                                  01S0119
*           ELSE                    /* IF OK SO FAR CONTINUE    01S0120
*                                      CHECKING.                     */
*             IF PARM2(1)=',' THEN  /* SEE IF XX FIELD IS SUPPLIED.  */
         B     @RC00118                                         01S0120
@RF00118 CLI   PARM2(PARMPTR),C','                              01S0120
         BNE   @RF00120                                         01S0120
*               DO;                 /* IF NOT, MUST SUPPLY DEFAULT.  */
*                 PFLD2=PFLD1;      /* SAVE THE YY VALUE.            */
         MVC   @TS00001(3),PFLD1(PARMPTR)                       01S0122
         MVC   PFLD2(3,PARMPTR),@TS00001                        01S0122
*                 PARM2='00';       /* SUBSTITUTE THE DEFAULT.       */
         MVC   PARM2(2,PARMPTR),@CC01140                        01S0123
*                 APCQPLEN=APCQPLEN+2;/* INCREMENT THE LENGTH.       */
         LA    @11,2                                            01S0124
         AH    @11,APCQPLEN(,APCQPTR)                           01S0124
         STH   @11,APCQPLEN(,APCQPTR)                           01S0124
*               END;                /* DONE WITH SUPPLYIN THE   01S0125
*                                      DEFAULT.                      */
*             ELSE                  /* CHECK THE SECOND SEPARATOR.   */
*               IF APCQPLEN>6 THEN  /* BUT ONLY IF THERE             */
         B     @RC00120                                         01S0126
@RF00120 LH    @11,APCQPLEN(,APCQPTR)                           01S0126
         C     @11,@CF00135                                     01S0126
         BNH   @RF00126                                         01S0126
*                 IF COMMA2^=',' THEN/* IT MUST BE A COMMA.          */
         CLI   COMMA2(PARMPTR),C','                             01S0127
         BE    @RF00127                                         01S0127
*                   APUBPTR=0;      /* INDICATE AN ERROR IF IT'S NOT.*/
         SLR   APUBPTR,APUBPTR                                  01S0128
*       IF APUBPTR=0 THEN                                       01S0129
*                                                               01S0129
@RF00127 DS    0H                                               01S0129
@RF00126 DS    0H                                               01S0129
@RC00120 DS    0H                                               01S0129
@RC00118 DS    0H                                               01S0129
@RF00117 DS    0H                                               01S0129
@RF00116 LTR   APUBPTR,APUBPTR                                  01S0129
         BNZ   @RF00129                                         01S0129
*         /***********************************************************/
*         /*                                                         */
*         /* OUTPUT ERROR MESSAGE.                                   */
*         /*                                                         */
*         /***********************************************************/
*                                                               01S0130
*         DO;                                                   01S0130
*           IF APCQTXLN>40 THEN     /* IF MORE THAN 40, ONLY WRITE 40*/
         LA    @11,40                                           01S0131
         CH    @11,APCQTXLN(,APCQPTR)                           01S0131
         BNL   @RF00131                                         01S0131
*             APCQTXLN=40;          /* DUMMY UP RIGHT LENGTH.        */
         STH   @11,APCQTXLN(,APCQPTR)                           01S0132
*           TXLEN=APCQTXLN+15;      /* COMPUTE LENGTH OF SECOND LINE.*/
@RF00131 LH    @11,APCQTXLN(,APCQPTR)                           01S0133
         LA    @11,15(,@11)                                     01S0133
         STH   @11,TXLEN                                        01S0133
*           CALL APCVZDWT(IGT213I,14,APCTSSID,4,41,'04',2,66,APCQCMND,
*               EVAL(APCQTXLN),52,TXLEN,2);                     01S0134
         MVC   @AL00001(52),@AL00134                            01S0134
         LA    @11,APCTSSID(,APCTPTR)                           01S0134
         ST    @11,@AL00001+8                                   01S0134
         LA    @11,APCQCMND(,APCQPTR)                           01S0134
         ST    @11,@AL00001+32                                  01S0134
         LH    @11,APCQTXLN(,APCQPTR)                           01S0134
         ST    @11,@AFTEMPS                                     01S0134
         LA    @11,@AFTEMPS                                     01S0134
         ST    @11,@AL00001+36                                  01S0134
         LA    @11,TXLEN                                        01S0134
         ST    @11,@AL00001+44                                  01S0134
         L     @15,APCTZDWT(,APCTPTR)                           01S0134
         LA    @01,@AL00001                                     01S0134
         BALR  @14,@15                                          01S0134
*         END;                      /* END OF ERROR PROCESSING.      */
*       ELSE                                                    01S0136
*         DO;                                                   01S0136
         B     @RC00129                                         01S0136
@RF00129 DS    0H                                               01S0137
*           IF APUBIDWN='0'B THEN   /* TELL OPERATOR IPL NOT ALLOWED */
         TM    APUBIDWN(APUBPTR),B'10000000'                    01S0137
         BNZ   @RF00137                                         01S0137
*             CALL APCVZDWT(IGT216I,14,APCTSSID,4,27,PARM1,3);/*     */
         MVC   @AL00001(28),@AL00138                            01S0138
         LA    @11,APCTSSID(,APCTPTR)                           01S0138
         ST    @11,@AL00001+8                                   01S0138
         ST    PARMPTR,@AL00001+20                              01S0138
         L     @15,APCTZDWT(,APCTPTR)                           01S0138
         LA    @01,@AL00001                                     01S0138
         BALR  @14,@15                                          01S0138
*           ELSE                                                01S0139
*             DO;                                               01S0139
         B     @RC00137                                         01S0139
@RF00137 DS    0H                                               01S0140
*               IF APCQPLEN>=9 THEN                             01S0140
         LH    @11,APCQPLEN(,APCQPTR)                           01S0140
         C     @11,@CF00716                                     01S0140
         BL    @RF00140                                         01S0140
*                 APUBIPL=PARM3;    /* SAVE THE ID.                  */
         MVC   APUBIPL(2,APUBPTR),PARM3(PARMPTR)                01S0141
*               ELSE                                            01S0142
*                 APUBIPL='00';     /* DEFAULT ID                    */
*                                                               01S0142
         B     @RC00140                                         01S0142
@RF00140 MVC   APUBIPL(2,APUBPTR),@CC01140                      01S0142
*               /*****************************************************/
*               /*                                                   */
*               /* SAVE INITIALIZATION MEMBER NAME.                  */
*               /*                                                   */
*               /*****************************************************/
*                                                               01S0143
*               IF APCQPLEN>=6      /* IF THE PARM IS PRESENT        */
*                   &PARM2^='  ' THEN                           01S0143
@RC00140 LH    @11,APCQPLEN(,APCQPTR)                           01S0143
         C     @11,@CF00135                                     01S0143
         BL    @RF00143                                         01S0143
         CLC   PARM2(2,PARMPTR),@CC01146                        01S0143
         BE    @RF00143                                         01S0143
*                 APUBINIT=PARM2;   /* USE IT.                       */
         MVC   APUBINIT(2,APUBPTR),PARM2(PARMPTR)               01S0144
*               ELSE                                            01S0145
*                 APUBINIT='00';    /* ELSE USE DEFAULT.             */
         B     @RC00143                                         01S0145
@RF00143 MVC   APUBINIT(2,APUBPTR),@CC01140                     01S0145
*               CALL APCVZDAL;      /* ALLOCATE THE 3838'S PORTS.    */
@RC00143 L     @15,APCTZDAL(,APCTPTR)                           01S0146
         BALR  @14,@15                                          01S0146
*               RC=R15P;            /* SAVE RETURN CODE.             */
         LR    RC,R15P                                          01S0147
*               IF RC=0 THEN        /* IF NOT ERROR ,THEN            */
         LTR   RC,RC                                            01S0148
         BNZ   @RF00148                                         01S0148
*                 DO;               /* TRY TO BUILD CONTROL BLKS.    */
*                   CALL APCVZDBK;  /* BUILD EXCP CONTROL BLOCKS.    */
         L     @15,APCTZDBK(,APCTPTR)                           01S0150
         BALR  @14,@15                                          01S0150
*                   RC=R15P;        /* SAVE THE RETURN CODE.         */
         LR    RC,R15P                                          01S0151
*                   IF RC=0 THEN    /* IF NO ERROR THEN              */
         LTR   RC,RC                                            01S0152
         BNZ   @RF00152                                         01S0152
*                     CALL APCVZDIP;/* IPL THE 3838.                 */
         L     @15,APCTZDIP(,APCTPTR)                           01S0153
         BALR  @14,@15                                          01S0153
*                 END;                                          01S0154
@RF00152 DS    0H                                               01S0155
*               IF RC^=0 THEN       /* OUT PUT ERROR MESSAGE         */
@RF00148 LTR   RC,RC                                            01S0155
         BZ    @RF00155                                         01S0155
*                 DO;                                           02S0156
*                   CVD(RC,DIGITS); /* CONVERT RETURN CODE TO   02S0157
*                                      DECIMAL.                      */
         CVD   RC,@TS00001                                      02S0157
         MVC   DIGITS(2),@TS00001+6                             02S0157
*                   UNPK(CODE,DIGITS);/* UNPACK ANSWER.              */
         UNPK  CODE(2),DIGITS(2)                                02S0158
*                   CODE(2)=CODE(2)×'F0'X;/* MAKE IT AN UNSIGNED
*                                      NUMBER.                       */
*                                                               02S0159
         OI    CODE+1,X'F0'                                     02S0159
*                   /*************************************************/
*                   /*                                               */
*                   /* ADDRESS OF THE APCT.                          */
*                   /*                                               */
*                   /*************************************************/
*                                                               02S0160
*                   IF APCQIINF=APCQRIPL THEN/* IF THIS IS A RE-IPL. */
         CLC   APCQIINF(4,APCQPTR),@CF00725                     02S0160
         BNE   @RF00160                                         02S0160
*                     TYPE='RE-';   /* SAY SO.                       */
         MVC   TYPE(3),@CC01151                                 02S0161
*                   ELSE                                        02S0162
*                     TYPE='';      /* OTHERWISE, BLANK IT OUT.      */
         B     @RC00160                                         02S0162
@RF00160 MVI   TYPE,C' '                                        02S0162
         MVC   TYPE+1(2),TYPE                                   02S0162
*                   CALL APCVZDWT(IGT240I,14,APCTSSID,4,20,TYPE,3,30,
*                       UCBNAME,3,44,CODE,2);                   02S0163
@RC00160 MVC   @AL00001(52),@AL00163                            02S0163
         LA    @11,APCTSSID(,APCTPTR)                           02S0163
         ST    @11,@AL00001+8                                   02S0163
         LA    @11,TYPE                                         02S0163
         ST    @11,@AL00001+20                                  02S0163
         L     @11,UCBPTR(,APUBPTR)                             02S0163
         LA    @11,UCBNAME(,@11)                                02S0163
         ST    @11,@AL00001+32                                  02S0163
         LA    @11,CODE                                         02S0163
         ST    @11,@AL00001+44                                  02S0163
         L     @15,APCTZDWT(,APCTPTR)                           02S0163
         LA    @01,@AL00001                                     02S0163
         BALR  @14,@15                                          02S0163
*                 END;                                          02S0164
*             END;                                              01S0165
@RF00155 DS    0H                                               01S0166
*         END;                                                  01S0166
@RC00137 DS    0H                                               01S0167
*       CALL APCVZDDQ;              /* FREE THE APUBS.               */
@RC00129 L     @15,APCTZDDQ(,APCTPTR)                           01S0167
         BALR  @14,@15                                          01S0167
*     END;                                                      01S0168
*   ELSE                                                        01S0169
*     DO;                                                       01S0169
         B     @RC00106                                         01S0169
@RF00106 DS    0H                                               01S0170
*       IF APCQTXLN>40 THEN                                     01S0170
         LA    @11,40                                           01S0170
         CH    @11,APCQTXLN(,APCQPTR)                           01S0170
         BNL   @RF00170                                         01S0170
*         APCQTXLN=40;                                          01S0171
         STH   @11,APCQTXLN(,APCQPTR)                           01S0171
*       TXLEN=APCQTXLN+15;          /* LENGTH OF SECOND LINE.        */
@RF00170 LH    @11,APCQTXLN(,APCQPTR)                           01S0172
         LA    @08,15(,@11)                                     01S0172
         STH   @08,TXLEN                                        01S0172
*       CALL APCVZDWT(IGT210I,      /* WRITE ERROR MESSAGE           */
*           14,APCTSSID,4,70,APCQCMND,EVAL(APCQTXLN),70+APCQTXLN,'''',1
*           ,                       /* ADD CLOSEING QUOTE.           */
*           57,TXLEN,2);            /* LENGTH OF SECOND LINE.        */
         MVC   @AL00001(52),@AL00173                            01S0173
         LA    @08,APCTSSID(,APCTPTR)                           01S0173
         ST    @08,@AL00001+8                                   01S0173
         LA    @08,APCQCMND(,APCQPTR)                           01S0173
         ST    @08,@AL00001+20                                  01S0173
         LH    @08,APCQTXLN(,APCQPTR)                           01S0173
         ST    @08,@AFTEMPS                                     01S0173
         LA    @08,@AFTEMPS                                     01S0173
         ST    @08,@AL00001+24                                  01S0173
         AL    @11,@CF01154                                     01S0173
         ST    @11,@AFTEMPS+4                                   01S0173
         LA    @11,@AFTEMPS+4                                   01S0173
         ST    @11,@AL00001+28                                  01S0173
         LA    @11,TXLEN                                        01S0173
         ST    @11,@AL00001+44                                  01S0173
         L     @15,APCTZDWT(,APCTPTR)                           01S0173
         LA    @01,@AL00001                                     01S0173
         BALR  @14,@15                                          01S0173
*     END;                                                      01S0174
*   RETURN;                         /* IGTXEPLG                      */
@EL00001 L     @13,4(,@13)                                      01S0175
@EF00001 L     @00,@SIZDATD                                     01S0175
         LR    @01,@06                                          01S0175
         IGTYFSV  R,LV=(0),A=(1)
@ER00001 LM    @14,@12,12(@13)                                  01S0175
         BR    @14                                              01S0175
*                                                               01S0176
*   /*****************************************************************/
*   /*                                                               */
*   /* SYSTEM DECLARES                                               */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0176
*   DECLARE                         /* COMMON VARIABLES              */
*     I256C CHAR(256) BASED,                                    01S0176
*     I031F FIXED(31) BASED,                                    01S0176
*     I031P PTR(31) BASED,                                      01S0176
*     I015F FIXED(15) BASED,                                    01S0176
*     I015P PTR(15) BASED,                                      01S0176
*     I008P PTR(8) BASED,                                       01S0176
*     I001C CHAR(1) BASED;                                      01S0176
*                                                               01S0177
*   /*****************************************************************/
*   /*                                                               */
*   /* MODULE PATCH AREA                                             */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0177
*   DCL                                                         01S0177
*     APPATCH CHAR(*) INT STATIC LOCAL GEND;                    01S0177
*   GENERATE DATA DEFS(APPATCH);                                01S0178
*                                                               01S0179
*   /*****************************************************************/
*   /*                                                               */
*   /* GENERAL REGISTER DECLARES                                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0179
*   DCL                                                         01S0179
*     R0F FIXED(31) REG(0),                                     01S0179
*     R0P PTR(24) REG(0),                                       01S0179
*     R0RSTD PTR(31) REG(0),                                    01S0179
*     R1F FIXED(31) REG(1),                                     01S0179
*     R1P PTR(24) REG(1),                                       01S0179
*     R1RSTD PTR(31) REG(1),                                    01S0179
*     R2F FIXED(31) REG(2),                                     01S0179
*     R2P PTR(24) REG(2),                                       01S0179
*     R2RSTD PTR(31) REG(2),                                    01S0179
*     R3F FIXED(31) REG(3),                                     01S0179
*     R3P PTR(24) REG(3),                                       01S0179
*     R3RSTD PTR(31) REG(3),                                    01S0179
*     R4F FIXED(31) REG(4),                                     01S0179
*     R4P PTR(24) REG(4),                                       01S0179
*     R4RSTD PTR(31) REG(4),                                    01S0179
*     R5F FIXED(31) REG(5),                                     01S0179
*     R5P PTR(24) REG(5),                                       01S0179
*     R5RSTD PTR(31) REG(5),                                    01S0179
*     R6F FIXED(31) REG(6),                                     01S0179
*     R6P PTR(24) REG(6),                                       01S0179
*     R6RSTD PTR(31) REG(6),                                    01S0179
*     R7F FIXED(31) REG(7),                                     01S0179
*     R7P PTR(24) REG(7),                                       01S0179
*     R7RSTD PTR(31) REG(7),                                    01S0179
*     R8F FIXED(31) REG(8),                                     01S0179
*     R8P PTR(24) REG(8),                                       01S0179
*     R8RSTD PTR(31) REG(8),                                    01S0179
*     R9F FIXED(31) REG(9),                                     01S0179
*     R9P PTR(24) REG(9),                                       01S0179
*     R9RSTD PTR(31) REG(9),                                    01S0179
*     R10F FIXED(31) REG(10),                                   01S0179
*     R10P PTR(24) REG(10),                                     01S0179
*     R10RSTD PTR(31) REG(10),                                  01S0179
*     R11F FIXED(31) REG(11),                                   01S0179
*     R11P PTR(24) REG(11),                                     01S0179
*     R11RSTD PTR(31) REG(11),                                  01S0179
*     R12F FIXED(31) REG(12),                                   01S0179
*     R12P PTR(24) REG(12),                                     01S0179
*     R12RSTD PTR(31) REG(12),                                  01S0179
*     R13F FIXED(31) REG(13),                                   01S0179
*     R13P PTR(24) REG(13),                                     01S0179
*     R13RSTD PTR(31) REG(13),                                  01S0179
*     R14F FIXED(31) REG(14),                                   01S0179
*     R14P PTR(24) REG(14),                                     01S0179
*     R14RSTD PTR(31) REG(14),                                  01S0179
*     R15F FIXED(31) REG(15),                                   01S0179
*     R15P PTR(24) REG(15),                                     01S0179
*     R15RSTD PTR(31) REG(15),                                  01S0179
*                                                               01S0179
*     /***************************************************************/
*     /*                                                             */
*     /* VPSS CONTROL BLOCK POINTER DECLARES                         */
*     /*                                                             */
*     /***************************************************************/
*                                                               01S0179
*     APSMPTR PTR(24) REG(01),      /* STORAGE MANAGEMENT BLOCK 01S0179
*                                      (SUGGESTED POINTER)           */
*     APSCPTR PTR(24) REG(01),      /* STORAGE MANAGEMENT CONTROL
*                                      WORD (SUGGESTED POINTER)      */
*     APINPTR PTR(24) REG(01),      /* USER'S INPUT PARAMETER LIST
*                                      (SUGGESTED POINTER)           */
*/*                                                                  */
*                                                               01S0179
*     APDATA PTR(24) REG(06),       /* ADDRESS OF DYNAMIC DATA  01S0179
*                                      WORKAREA (DATAREG)            */
*     APCODE PTR(24) REG(07),       /* PROGRAM CODE BASE REGISTER
*                                      (CODEREG)                     */
*     APDEBPTR PTR(24) REG(08),     /* VPSS DEB (SUGGESTED POINTER)  */
*     APNPPTR PTR(24) REG(08),      /* SUBSYSTEM INITIALIZATION 01S0179
*                                      WORKAREA BLOCK (ONLY USED BY
*                                      SUBSYSTEM INITIALIZATION)     */
*     APRLPTR PTR(24) REG(08),      /* VPSS ACB REQUEST LIST    01S0179
*                                      (SUGGESTED POINTER)           */
*     APCIPTR PTR(24) REG(08),      /* 3838 CONTROL TABLE (SUGGESTED
*                                      POINTER)                      */
*     APCWPTR PTR(24) REG(08),      /* CALL INTERFACE WORKAREA  01S0179
*                                      (SUGGESTED POINTER)           */
*     APERPTR PTR(24) REG(08),      /* ERP/IO APPENDAGE WORKAREA
*                                      (SUGGESTED POINTER)           */
*     APACBPTR PTR(24) REG(09),     /* VPSS ACB (SUGGESTED POINTER)  */
*     APCQPTR PTR(24) REG(09),      /* COMMAND QUEUE ELEMENT    01S0179
*                                      (SUGGESTED POINTER)           */
*     APOTPTR PTR(24) REG(09),      /* OPTION TABLE (SUGGESTED  01S0179
*                                      POINTER)                      */
*     APIOPTR PTR(24) REG(10),      /* I/O ELEMENT (SUGGESTED   01S0179
*                                      POINTER)                      */
*     APEXLPTR PTR(24) REG(10),     /* VPSS EXLST (SUGGESTED POINTER)*/
*     APUBPTR PTR(24) REG(10),      /* UNIT BLOCK (SUGGESTED POINTER)*/
