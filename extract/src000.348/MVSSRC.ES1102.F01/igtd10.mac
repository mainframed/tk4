TD10     TITLE 'IGTD10 - MONITOR MAIN ROUTINE.                         *
                        '
*                          /* DEFINE INITIAL GETMAIN MACRO.          */
        MACRO
&LABEL  IGTDGSV &R,&LV=,&SP=,&INITIAL=YES
&LABEL  IGTYGSV &R,INITIAL=&INITIAL,SP=&SP,LV=&LV CALL GETMAIN RT.
        MEND
IGTD10   CSECT ,                                                01S0002
@MAINENT DS    0H                                               01S0002
         USING *,@15                                            01S0002
         B     @PROLOG                                          01S0002
         DC    AL1(16)                                          01S0002
         DC    C'IGTD10    77.231'                              01S0002
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                  01S0002
         BALR  @07,0                                            01S0002
@PSTART  DS    0H                                               01S0002
         USING @PSTART,@07                                      01S0002
         L     @00,@SIZDATD                                     01S0002
         IGTDGSV  R,LV=(0)
         LR    @06,@01                                          01S0002
         USING @DATD,@06                                        01S0002
         ST    @13,@SA00001+4                                   01S0002
         LM    @00,@01,20(@13)                                  01S0002
         ST    @06,8(,@13)                                      01S0002
         LR    @13,@06                                          01S0002
         MVC   @PC00001(4),0(@01)                               01S0002
*   MEMBID=INPARM1(1:8);            /* COMMAND SET MEMBER NAME.      */
         L     @15,@PC00001                                     01S0225
         MVC   MEMBID(8),INPARM1(@15)                           01S0225
*   RESPECIFY                                                   01S0226
*     R1P UNRESTRICTED;             /* RELEASE UNNEEDED REGISTER.    */
*                                                               02S0227
*/********************************************************************/
*/*  ESTABLISH THE ERROR RECOVERY ENVIRONMENT.                       */
*/********************************************************************/
*                                                               02S0227
*   DO;                             /* CREATE THE ESTAE.             */
*     RFY                                                       02S0228
*      (R2P,                                                    02S0228
*       R15P) RSTD;                 /* NEEDED BY ESTAE.              */
*     D10APCT=APCTPTR;              /* SET ADDRESS OF THE APCT.      */
         ST    APCTPTR,D10APCT                                  02S0229
*     D10CODE=10;                   /* INDICATE THIS IS D10 PARM
*                                      LIST.                         */
         MVC   D10CODE(2),@CH02928                              02S0230
*     D10FLAGS=''B;                 /* INIT FLAGS FIELD.             */
         XC    D10FLAGS(2),D10FLAGS                             02S0231
*     R2P=APCTZD4E;                 /* ADDRESS OF THE STAE EXIT RTN. */
         L     R2P,APCTZD4E(,APCTPTR)                           02S0232
*     GEN REFS(DSTAE,SSTAE,R2P);                                02S0233
*                                 /* ISSUE THE STAE OR ESTAE.        */
          MVC   DSTAE,SSTAE          MOVE IN THE SKELETON.
          IGTYSTAE (R2P),MF=(E,DSTAE),PARAM=D10STAE ISSUE THE ESTAE.
*     RFY                                                       02S0234
*      (R2P,                                                    02S0234
*       R15P) UNRSTD;               /* RELEASE THE REGS.             */
*   END;                            /* END OF ESTAE PROCESSING.      */
*   IF R15P=0 THEN                  /* IF IT IS OK, PROCEED.         */
         LTR   R15P,R15P                                        01S0236
         BNZ   @RF00236                                         01S0236
*     DO;                                                       01S0237
*       APOTNAME(1:4)='IGT0';       /* BUILD THE OPTION TABLE NAME.  */
         MVC   APOTNAME(4),@CC03884                             01S0238
*       APOTNAME(5:8)=APCTENQM(5:8);/* BUILD THE OPTION TABLE NAME.  */
         MVC   APOTNAME+4(4),APCTENQM+4(APCTPTR)                01S0239
*                                                               02S0240
*       /*************************************************************/
*       /*                                                           */
*       /* OPEN THE DCB FOR THE LOG DATA SET.                        */
*       /*                                                           */
*       /*************************************************************/
*                                                               02S0240
*       GEN REFS(LOGDCBS,LOGOPENS) SETS(LOGDCBD,OPENIT);        02S0240
         MVC   LOGDCBD,LOGDCBS     MOVE IN DCB SKELETON.
         MVC   OPENIT(4),LOGOPENS  MOVE IN OPEN SKELETON.
         OPEN  (LOGDCBD),MF=(E,OPENIT) OPEN THE LOG DCB.
*       APCTLOGD=ADDR(LOGDCBD);     /* MAKE DCB ADDRESS AVAILABLE TO
*                                      OTHER USERS.                  */
         LA    @15,LOGDCBD                                      02S0241
         ST    @15,APCTLOGD(,APCTPTR)                           02S0241
*       APCTLGLN=255;               /* INSURE THAT A TITLE IS PRINTE.*/
         MVC   APCTLGLN(2,APCTPTR),@CH00947                     02S0242
*       APCTLGPG=0;                 /* INITIALIZE THE PAGE COUNT.    */
         SLR   @15,@15                                          02S0243
         STH   @15,APCTLGPG(,APCTPTR)                           02S0243
*                                                               02S0244
*       /*************************************************************/
*       /*                                                           */
*       /* OPEN THE DCB FOR THE IPL LOAD MODULE LIBRARY.             */
*       /*                                                           */
*       /*************************************************************/
*                                                               02S0244
*       GEN REFS(STATLDCB,OPENLDCB) SETS(DYNLDCB,OPENIT);       02S0244
      MVC DYNLDCB,STATLDCB        GET COPY OF DCB
      MVC OPENIT(4),OPENLDCB      GET COPY OF OPEN
      OPEN (DYNLDCB),MF=(E,OPENIT) OPEN SYS1.LOAD3838
*       APCTDCLD=ADDR(DYNLDCB);     /* SET DCB ADDRESS.              */
         LA    @15,DYNLDCB                                      02S0245
         ST    @15,APCTDCLD(,APCTPTR)                           02S0245
*                                                               02S0246
*/*   THIS SECTION OF CODE REINITALIZES THE APCT SO THAT THE         */
*/*   MONITOR CAN RESTART AFTER A PREVIOUS TERMINATION.              */
*/*      THE THINGS THAT NEED CLEAN UP MAY BE DIVIDED INTO           */
*/*   THREE CATEGORIES:                                              */
*/*   1.  THOSE THAT MUST BE INITIALIZED FOR BOTH WARM AND           */
*/*       COLD START.                                                */
*/*   2.  THOSE INITILIZED ONLY IF A WARM START IS IN                */
*/*       PROGRESS.                                                  */
*/*   3.  THOSE INITIALIZED ONLY IF A COLD START IS IN               */
*/*       PROGRESS.                                                  */
*                                                               02S0246
*                                                               02S0246
*/*     CATEGORY 1.  INITIALIZE FOR BOTH COLD AND WARM START.        */
*                                                               02S0246
*       APCTCQLK=0;                 /* RESET APCQ LOCK.              */
         SLR   @15,@15                                          02S0246
         ST    @15,APCTCQLK(,APCTPTR)                           02S0246
*       APCTCQF=0;                  /* RESET ACTIVE APCQ FIRST PTR.  */
         ST    @15,APCTCQF(,APCTPTR)                            02S0247
*       APCTCQL=0;                  /* RESET ACTIVE APCQ LAST PTR.   */
         ST    @15,APCTCQL(,APCTPTR)                            02S0248
*       APCTCQAQ=0;                 /* RESET AVAILABLE APCQS PTR.    */
         ST    @15,APCTCQAQ(,APCTPTR)                           02S0249
*       APCTECIT=0;                 /* RESET COMMAND INTFACE TASK
*                                      ECB.                          */
         ST    @15,APCTECIT(,APCTPTR)                           02S0250
*       APCTEALC=0;                 /* RESET ALLOCATE RESOURCE ECB.  */
         ST    @15,APCTEALC(,APCTPTR)                           02S0251
*       APCTECPT=0;                 /* RESET COMMAND PROCESSOR ECB.  */
         ST    @15,APCTECPT(,APCTPTR)                           02S0252
*       APCTLOGL=0;                 /* RESET MESSAGE LOG LOCK.       */
         ST    @15,APCTLOGL(,APCTPTR)                           02S0253
*       APCTTCB=CURTCB;             /* GET THE ADDRESS OF THE   02S0254
*                                      MONITOR'S TCB.                */
         L     @01,CVTPTR                                       02S0254
         L     @15,CVTTCBP(,@01)                                02S0254
         L     @14,CURTCB(,@15)                                 02S0254
         ST    @14,APCTTCB(,APCTPTR)                            02S0254
*       APCTASCB=CURASCB;           /* GET THE ADDRESS OF THE   02S0255
*                                      MONITOR'S ASCB.               */
         L     @15,CURASCB(,@15)                                02S0255
         ST    @15,APCTASCB(,APCTPTR)                           02S0255
*       APCTASID=APCTASCB->ASCBASID;/* THE MONITORS ASID.            */
         LH    @15,ASCBASID(,@15)                               02S0256
         N     @15,@CF04017                                     02S0256
         STH   @15,APCTASID(,APCTPTR)                           02S0256
*       IF APCTWARM='1'B THEN       /* TEST FOR WARM START.          */
*                                                               02S0257
         TM    APCTWARM(APCTPTR),B'00000100'                    02S0257
         BNO   @RF00257                                         02S0257
*         /***********************************************************/
*         /*                                                         */
*         /* CATEGORY 2: INITIALIZE FOR WARM START.                  */
*         /*                                                         */
*         /***********************************************************/
*                                                               02S0258
*         DO;                                                   02S0258
*           APCTEULD='40000000'X;   /* FAKE A POST FOR DISABLE/UNLOAD
*                                      SCAN JUST IN CASE SOMETHING
*                                      HAPPENED THAT WOULD ALLOW A
*                                      DISABLE OR UNLOAD TO OCCUR
*                                      NOW.                          */
         L     @15,@CF03886                                     02S0259
         ST    @15,APCTEULD(,APCTPTR)                           02S0259
*           APCTECPW='40000000'X;   /* FAKE A POST SO THAT THE  02S0260
*                                      COMMAND PROCESSOR WILL PROCESS
*                                      ANY COMMANDS STILL HANGING
*                                      AROUND.                       */
         ST    @15,APCTECPW(,APCTPTR)                           02S0260
*           APCTEIPL='40000000'X;   /* FAKE A POST SO THAT ANY DUMPS
*                                      AND/OR RE-IPLS RQUESTED WHILE
*                                      WE WERE GONE CAN HAPPEN NOW.  */
         ST    @15,APCTEIPL(,APCTPTR)                           02S0261
*           DO;                                                 03S0262
*                                                               03S0262
*             /*******************************************************/
*             /*                                                     */
*             /* DYNAMICALLY ALLOCATE THE PORTS TO THE MONITOR. SINCE*/
*             /* THIS IS A WARM START.                               */
*             /*                                                     */
*             /*******************************************************/
*                                                               03S0263
*             RESPECIFY                                         03S0263
*               APUBPTR RESTRICTED;                             03S0263
*             RESPECIFY                                         03S0264
*               APUB BASED(APUBPTR);                            03S0264
*             APUBPTR=APCTAPUB;     /* SET PTR TO FIRST APUB.        */
         L     APUBPTR,APCTAPUB(,APCTPTR)                       03S0265
*             CANWARM='0'B;         /* RESET ABORT BIT.              */
         NI    CANWARM,B'10111111'                              03S0266
*             DO                    /* EXAMINE ALL MASTER APUBS      */
*                   WHILE APUBPTR^=0;/* LOOKING FOR IPLED 3838'S.    */
         B     @DE00267                                         03S0267
@DL00267 DS    0H                                               03S0268
*               IF APUBIDWN='0'B THEN/* IF DEVICE WAS IPLED          */
         TM    APUBIDWN(APUBPTR),B'10000000'                    03S0268
         BNZ   @RF00268                                         03S0268
*                 DO;                                           03S0269
*                   CALL APCVZDAL;  /* DYN ALLOC THE 3838, PORT 0-7. */
         L     @15,APCTZDAL(,APCTPTR)                           03S0270
         BALR  @14,@15                                          03S0270
*                   IF R15P^=0 THEN /* IF ALLOCATION OR OPEN FAILED. */
         LTR   R15P,R15P                                        03S0271
         BZ    @RF00271                                         03S0271
*                     DO;           /* MUST ABORT THE WARM START.    */
*                       RFY                                     03S0273
*                         UCBOB BASED(APUBUCB);/* PTR TO THE UCB.    */
*                       CALL APCVZDWT(IGT253I,/* OUTPUT MESSAGE.     */
*                           14,APCTSSID,4,20,UCBNAME,3);        03S0274
         MVC   @AL00001(28),@AL00274                            03S0274
         LA    @11,APCTSSID(,APCTPTR)                           03S0274
         ST    @11,@AL00001+8                                   03S0274
         L     @11,APUBUCB(,APUBPTR)                            03S0274
         LA    @11,UCBNAME(,@11)                                03S0274
         ST    @11,@AL00001+20                                  03S0274
         L     @15,APCTZDWT(,APCTPTR)                           03S0274
         LA    @01,@AL00001                                     03S0274
         BALR  @14,@15                                          03S0274
*                       RFY                                     03S0275
*                         UCBOB BASED(UCBPTR);                  03S0275
*                       CANWARM='1'B;/* INDICATE WARM START ABORTED. */
         OI    CANWARM,B'01000000'                              03S0276
*                     END;                                      03S0277
*                 END;                                          03S0278
@RF00271 DS    0H                                               03S0279
*               APUBPTR=APUBMSTR;   /* GO TO NEXT MASTER APUB.       */
@RF00268 L     APUBPTR,APUBMSTR(,APUBPTR)                       03S0279
*             END;                                              03S0280
@DE00267 LTR   APUBPTR,APUBPTR                                  03S0280
         BNZ   @DL00267                                         03S0280
*             IF CANWARM='1'B THEN  /* ENVIRONMENT OK FOR WARM START?*/
         TM    CANWARM,B'01000000'                              03S0281
         BNO   @RF00281                                         03S0281
*               DO;                 /* IF ON THEN NOT OK.            */
*                 CALL APCVZDWT(IGT254I,/* OUTPUT ABORT MESSAGE.     */
*                     14,APCTSSID,4);                           03S0283
         MVC   @AL00001(16),@AL00283                            03S0283
         LA    @11,APCTSSID(,APCTPTR)                           03S0283
         ST    @11,@AL00001+8                                   03S0283
         L     @15,APCTZDWT(,APCTPTR)                           03S0283
         LA    @01,@AL00001                                     03S0283
         BALR  @14,@15                                          03S0283
*                 GEN(IGTYSTAE 0);  /* CANCEL THE ESTAE.             */
*                                                               03S0284
         IGTYSTAE 0
*                 /***************************************************/
*                 /*                                                 */
*                 /* CLOSE THE DCB FOR THE 3838 LOAD MODULES.        */
*                 /*                                                 */
*                 /***************************************************/
*                                                               03S0285
*                 GEN SETS(CLOSEIT,DYNLDCB);                    03S0285
*                                                               03S0285
 MVI CLOSEIT,X'80'                    MOVE IN THE VL INDICATOR.
 CLOSE DYNLDCB,MF=(E,CLOSEIT)         CLOSE THE MOTHER.
*                 /***************************************************/
*                 /*                                                 */
*                 /* CLOSE THE DCB FOR THE MESSAGE LOG.              */
*                 /*                                                 */
*                 /***************************************************/
*                                                               03S0286
*                 GEN SETS(CLOSEIT,LOGDCBD);                    03S0286
 MVI CLOSEIT,X'80'                    MOVE IN THE VL INDICATOR.
 CLOSE LOGDCBD,MF=(E,CLOSEIT)         CLOSE THE MOTHER.
*                 APCTLOGD=0;       /* ZERO THE LOG DCB ADDRESS SO NO
*                                      FURTHER WRITES WILL BE   03S0287
*                                      ATTEMPTED.                    */
         SLR   @11,@11                                          03S0287
         ST    @11,APCTLOGD(,APCTPTR)                           03S0287
*                 RETURN CODE(4);   /* SET RETURN FOR IGTD00         */
         L     @13,4(,@13)                                      03S0288
         L     @00,@SIZDATD                                     03S0288
         LR    @01,@06                                          03S0288
         IGTYFSV  R,LV=(0),A=(1)
         LA    @15,4                                            03S0288
         L     @14,12(,@13)                                     03S0288
         LM    @00,@12,20(@13)                                  03S0288
         BR    @14                                              03S0288
*               END;                /* END OF WARMSTART ABORT.       */
*             ELSE                  /* EVERYTHING OK SO GEN WARMINIT.*/
*               DO;                 /* GENERATE WARM INIT COMMAND.   */
@RF00281 DS    0H                                               03S0291
*                 RESPECIFY                                     03S0291
*                   APCQPTR RESTRICTED;/* WILL HAVE PTR TO APCQ.     */
*                 RESPECIFY                                     03S0292
*                   APCQ BASED(APCQPTR);/* WILL HAVE PTR TO APCQ.    */
*                 CALL APCVZDX1;    /* GET AN APCQ TO PUT THE COMMAND
*                                      IN.                           */
         L     @15,APCTZDX1(,APCTPTR)                           03S0293
         BALR  @14,@15                                          03S0293
*                 APCQINT='1'B;     /* INDICATE AN INTERNAL COMMAND. */
         OI    APCQINT(APCQPTR),B'10000000'                     03S0294
*                 APCQIINF=APCQWI;  /* INDICATE COMMAND VERB.        */
         MVC   APCQIINF(4,APCQPTR),@CF02937                     03S0295
*                 CALL APCVZDX2;    /* QUEUE THE WARM INIT COMMAND
*                                      FOR IGTD40.                   */
         L     @15,APCTZDX2(,APCTPTR)                           03S0296
         BALR  @14,@15                                          03S0296
*               END;                /* END OF WARM INIT.             */
*             RESPECIFY                                         03S0298
*               APUB;               /* NO LONGER VALID BASE.         */
*             RESPECIFY                                         03S0299
*               APUBPTR UNRESTRICTED;                           03S0299
*           END;                                                03S0300
*         END;                                                  02S0301
*       ELSE                                                    02S0302
*                                                               02S0302
*         /***********************************************************/
*         /*                                                         */
*         /* CATEGORY 3: INITAILIZE FOR COLD START.                  */
*         /*                                                         */
*         /***********************************************************/
*                                                               02S0302
*         DO;                                                   02S0302
*                                                               02S0302
         B     @RC00257                                         02S0302
@RF00257 DS    0H                                               02S0303
*           /*********************************************************/
*           /*                                                       */
*           /* THESE NEXT FEW LINES REMOVE ALL TRACES OF APUBS THAT  */
*           /* MIGHT STILL BE HANGING AROUND. DON'T TRY TO USE THEM. */
*           /* IF THEY WERE STILL GOOD, EITHER STAE WOULD HAVE       */
*           /* RECOVERED OR A WARM START WOULD HAVE BEEN ATTEMPTED.  */
*           /*                                                       */
*           /*********************************************************/
*                                                               02S0303
*           APCTAPUB=0;                                         02S0303
         SLR   @08,@08                                          02S0303
         ST    @08,APCTAPUB(,APCTPTR)                           02S0303
*           APCTUBIS=0;                                         02S0304
         ST    @08,APCTUBIS(,APCTPTR)                           02S0304
*           APCTUBIC=0;                                         02S0305
         ST    @08,APCTUBIC(,APCTPTR)                           02S0305
*           APCTISUB=0;                                         02S0306
         ST    @08,APCTISUB(,APCTPTR)                           02S0306
*           APCTUBAP=0;                                         02S0307
         ST    @08,APCTUBAP(,APCTPTR)                           02S0307
*           APCTAPDS=0;             /* CLEAR APDS PTRS.              */
         ST    @08,APCTAPDS(,APCTPTR)                           02S0308
*           APCTDSAP=0;                                         02S0309
*                                                               02S0309
         ST    @08,APCTDSAP(,APCTPTR)                           02S0309
*           /*********************************************************/
*           /*                                                       */
*           /* INIT ECBS.                                            */
*           /*                                                       */
*           /*********************************************************/
*                                                               02S0310
*           APCTEULD=0;             /* CLEAR THE DISABLE UNLOAD SCAN
*                                      ECB.                          */
         ST    @08,APCTEULD(,APCTPTR)                           02S0310
*           APCTEIPL=0;             /* CLEAR THE DUMP/REIPL ECB.     */
         ST    @08,APCTEIPL(,APCTPTR)                           02S0311
*           APCTECPW=0;             /* CLEAR THE COMMAND PROCESSOR
*                                      ECB.                          */
         ST    @08,APCTECPW(,APCTPTR)                           02S0312
*           APCTAPST='0'B;          /* ALLOCATION DOESNT NEED TO POST
*                                      US YET.                       */
         L     @08,APCTAPST-1(,APCTPTR)                         02S0313
@GL00001 LR    @15,@08                                          02S0313
         N     @15,@CF04021                                     02S0313
         CS    @08,@15,APCTAPST-1(APCTPTR)                      02S0313
         BNE   @GL00001                                         02S0313
*           APCTAPIO=0;             /* CLEAR THE APIO PTR. SAME 02S0314
*                                      THEORY AS WITH APUBS.         */
         SLR   @08,@08                                          02S0314
         ST    @08,APCTAPIO(,APCTPTR)                           02S0314
*           APCTIOAP=0;             /* ZERO THE APIO BACK PTR.       */
         ST    @08,APCTIOAP(,APCTPTR)                           02S0315
*           RFY                                                 02S0316
*             R1P RSTD;             /* PARM REG FOR IGTUJA.          */
*           R1P=2;                  /* FUNCTION CODE FOR SET PURBE
*                                      RTN.                          */
         LA    R1P,2                                            02S0317
*           CALL APCVZUJA;          /* GO SET FUNCTION CODE.         */
         L     @15,APCTZUJA(,APCTPTR)                           02S0318
         BALR  @14,@15                                          02S0318
*           RFY                                                 02S0319
*             R1P UNRSTD;           /* RELEASE THE REG.              */
*           DO;                     /* CREATE THE APVU.              */
*             RFY                                               03S0321
*               ASVT BASED(CVTASVT);/* BASE FOR THE ASVT.            */
*             RFY                                               03S0322
*              (R0P,                                            03S0322
*               R3P) RSTD;          /* REGS FOR THE GETMAIN.         */
*             R3P=APVUHDLN+4*ASVTMAXU;/* SIZE OF THE APVU.           */
         L     @08,CVTPTR                                       03S0323
         L     @01,CVTASVT(,@08)                                03S0323
         L     R3P,ASVTMAXU(,@01)                               03S0323
         SLA   R3P,2                                            03S0323
         LA    R3P,3040(,R3P)                                   03S0323
*             R0P=(APVUSP*16777216)×R3P;/* LENGTH AND SUBPOOL.       */
         LR    R0P,R3P                                          03S0324
         O     R0P,@CF04011                                     03S0324
*             RFY                                               03S0325
*               R1P RSTD;           /* ADDRESS COMES BACK HERE.      */
*             R3P=R0P;              /* SAVE THE STUFF.               */
         LR    R3P,R0P                                          03S0326
*             GEN SETS(R0P,R1P)(GETMAIN R,LV=(0));/* GET THE STORAGE.*/
         GETMAIN R,LV=(0)
*             APCTAPVU=R1P;         /* SET PTR TO THE APVU           */
         ST    R1P,APCTAPVU(,APCTPTR)                           03S0328
*             APVUSPLN=R3P;         /* SUBPOOL AND LENGTH.           */
         L     @08,APCTAPVU(,APCTPTR)                           03S0329
         ST    R3P,APVUSPLN(,@08)                               03S0329
*             APVUID=APVUIDC;       /* IN CORE IDENTIFIER.           */
         MVC   APVUID(4,@08),@CC03793                           03S0330
*             APVUANUM=ASVTMAXU;    /* NUMBER OF ADDRESS SPACES.     */
         L     @15,CVTPTR                                       03S0331
         L     @15,CVTASVT(,@15)                                03S0331
         L     @15,ASVTMAXU(,@15)                               03S0331
         STH   @15,APVUANUM(,@08)                               03S0331
*             RFY                                               03S0332
*              (R2P,                                            03S0332
*               R14P,                                           03S0332
*               R15P) RSTD;         /* NEEDED FOR THE CLEARING OF
*                                      APVU.                         */
*             R2P=ADDR(APVUAUSE);   /* START OF CLEARED AREA.        */
         LA    R2P,APVUAUSE(,@08)                               03S0333
*             R15P=0;               /* WILL BE CLEARED BY PAD CHAR.  */
         SLR   R15P,R15P                                        03S0334
*             R3P=APVULEN-APVUHDLN; /* LENGTH TO BE CLEARED.         */
         L     R3P,APVULEN-1(,@08)                              03S0335
         LA    R3P,0(,R3P)                                      03S0335
         SL    R3P,@CF03787                                     03S0335
*             GEN(MVCL R2P,R14P);   /* CLEAR THE ASID PART.          */
         MVCL R2P,R14P
*             RFY                                               03S0337
*               ASCB BASED(APCTASCB);/* BASE ADDRESS FOR THE ASCB.   */
*           END;                    /* APVU IS BUILT.                */
*         END;                                                  02S0339
*       APVUMON(APCTASID)='1'B;     /* INDICATE THE MONITORS ADDR
*                                      SPACE                         */
@RC00257 LH    @04,APCTASID(,APCTPTR)                           02S0340
         SLA   @04,2                                            02S0340
         ST    @04,@TF00001                                     02S0340
         L     @04,APCTAPVU(,APCTPTR)                           02S0340
         AL    @04,@TF00001                                     02S0340
