TDID     TITLE 'IGTDID - PERFORM IPL DIALOGUE WITH 3838.               *
                        '
IGTDID   CSECT ,                                                01S0001
@MAINENT DS    0H                                               01S0001
         USING *,@15                                            01S0001
         B     @PROLOG                                          01S0001
         DC    AL1(16)                                          01S0001
         DC    C'IGTDID    77.092'                              01S0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                  01S0001
         BALR  @07,0                                            01S0001
@PSTART  DS    0H                                               01S0001
         USING @PSTART,@07                                      01S0001
         L     @00,@SIZDATD                                     01S0001
         IGTYGSV  R,LV=(0)
         LR    @06,@01                                          01S0001
         USING @DATD,@06                                        01S0001
         ST    @13,@SA00001+4                                   01S0001
         LM    @00,@01,20(@13)                                  01S0001
         ST    @06,8(,@13)                                      01S0001
         LR    @13,@06                                          01S0001
*   IOBSTART=ADDR(CCWS);            /* ADDRESS OF CCW BUILD AREA.    */
         L     @11,DCBPTR(,APUBPTR)                             01S0139
         L     @11,IOBSTPTR+16(,@11)                            01S0139
         LA    @04,CCWS                                         01S0139
         ST    @04,IOBSTART(,@11)                               01S0139
*   CCWPTR=ADDR(CCWS);              /* ADDRESS OF FIRST CCW.         */
         LR    CCWPTR,@04                                       01S0140
*   RC=0;                           /* INIT RETURN CODE.             */
         SLR   RC,RC                                            01S0141
*   CCW=RDCCW;                      /* MOVE IN SKELETON READ CCW.    */
         MVC   CCW(8,CCWPTR),RDCCW                              01S0142
*   CCWADDR=ADDR(STRGREC);          /* INSERT ADDRESS OF READIN AREA.*/
         LA    @04,STRGREC                                      01S0143
         STCM  @04,7,CCWADDR(CCWPTR)                            01S0143
*   CCWPTR=CCWPTR+8;                /* POINTER TO NEXT CCW SLOT.     */
         AL    CCWPTR,@CF00042                                  01S0144
*   CCW=NOPCCW;                     /* ADD NOP CCW TO END OF CHAIN.  */
         MVC   CCW(8,CCWPTR),NOPCCW                             01S0145
*   BSSIZE=64*1024;                 /* _________TEMPORARY------------
*                                      ---                           */
         MVC   BSSIZE(4),@CF01873                               01S0146
*   RSRVSIZE=4*1024;                                            01S0147
         MVC   RSRVSIZE(4),@CF01874                             01S0147
*   STATUS='0123FECD'X;                                         01S0148
         MVC   STATUS(4),@CB01759                               01S0148
*   RESPECIFY                                                   01S0149
*     R2P RESTRICTED;                                           01S0149
*   R2P=ADDR(IOBSTDRD);             /* POINTER TO IOB.               */
         LR    R2P,@11                                          01S0150
*   IOBFLAG1=IOBFLAG1&'3D'X;        /* TURN OFF CD AND CC AND   01S0151
*                                      RELATED.                      */
         NI    IOBFLAG1(@11),X'3D'                              01S0151
*                                                               02S0152
*   /*****************************************************************/
*   /*                                                               */
*   /* EXECUTE THE CHANNEL PROGRAM.                                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S0152
*   IOBCMDCH='1'B;                  /* INDICATE COMMAND CHAINING.    */
*   IOBUNREL='1'B;                  /* INDICATE UNRELATED REQUEST.   */
         OI    IOBCMDCH(@11),B'01000010'                        02S0153
*   RESPECIFY                                                   02S0154
*     R2P RESTRICTED;                                           02S0154
*   R2P=IOBSTPTR;                   /* ADDRESS OF THE IOB.           */
         LR    R2P,@11                                          02S0155
*   RFY                                                         02S0156
*     R3P RSTD;                     /* WILL CONTAIN THE ECB PTR.     */
*   R3P=IOBECBPT;                   /* POINTER TO THE ECB.           */
         L     R3P,IOBECBPT(,@11)                               02S0157
*   ECBCC='00'X;                    /* RESET COMPLETION CODE.        */
         L     @11,ECBPTR(,@11)                                 02S0158
         MVI   ECBCC(@11),X'00'                                 02S0158
*   GEN REFS(R2P,R3P) SETS(ECBCC);                              02S0159
*                                 /* ISSUE THE EXCP.                 */
         EXCP  (R2P)               DO THE I/O.
         WAIT 1,ECB=(R3P)          WAIT FOR I/O TO COMPLETE.
*   RFY                                                         02S0160
*     R3P UNRSTD;                   /* RELEASE THE REG.              */
*   IF ECBCC^='7F'X THEN            /* IF NOT GOOD                   */
         L     @11,DCBPTR(,APUBPTR)                             02S0161
         L     @11,IOBSTPTR+16(,@11)                            02S0161
         L     @11,ECBPTR(,@11)                                 02S0161
         CLI   ECBCC(@11),X'7F'                                 02S0161
         BE    @RF00161                                         02S0161
*     RC=12;                        /* SET ERROR CODE.               */
         LA    RC,12                                            02S0162
*   IF RC=0 THEN                    /* IF NO ERRORS DOING READ,      */
@RF00161 LTR   RC,RC                                            01S0163
         BNZ   @RF00163                                         01S0163
*                                                               02S0164
*     /***************************************************************/
*     /*                                                             */
*     /* THIS SECTION WILL LOAD THE AE MICRO CODE.                   */
*     /*                                                             */
*     /***************************************************************/
*                                                               02S0164
*     DO;                           /* LOAD THE AE MICROCODE.        */
*       SINAME='IAE     ';          /* BUILD AE LOAD MODULE NAME.    */
         MVC   SINAME(8),@CC01766                               02S0165
*       SINAME(4:5)=UCBNAME(1:2);   /* UNIT.                         */
         L     @11,UCBPTR(,APUBPTR)                             02S0166
         MVC   SINAME+3(2),UCBNAME(@11)                         02S0166
*       SINAME(6:7)=APUBIPL;        /* LEVEL (ID FIELD.)             */
         MVC   SINAME+5(2),APUBIPL(APUBPTR)                     02S0167
*       R2P=APCTDCLD;               /* LOAD3838.                     */
         L     R2P,APCTDCLD(,APCTPTR)                           02S0168
*       GEN FLOWS(NOAEMOD)          /* LOAD THE MODULE.              */
*           SETS(PARMPTR,MODLN,R1P) REFS(R2P,R0P);              02S0169
     LOAD  EPLOC=SINAME,ERRET=NOAEMOD,DCB=(R2P)
     ST    R0P,PARMPTR            ADDR OF MODULE
     SLL   R1P,3                  LENGTH IN DOUBLE WORDS.
     ST    R1P,MODLN
*       CCWPTR=ADDR(CCWS);          /* PREPARE TO BUILD WRITE/NDP
*                                      CCWS.                         */
         LA    CCWPTR,CCWS                                      02S0170
*       CCW=WRTPARM;                /* WRITE SKELETON.               */
         MVC   CCW(8,CCWPTR),WRTPARM                            02S0171
*       CCWADDR=PARMPTR;            /* ADDRESS OF AE CODE.           */
         MVC   CCWADDR(3,CCWPTR),PARMPTR+1                      02S0172
*       CCWCNT=MODLN;               /* LENGTH OF AE CODE.            */
         L     @11,MODLN                                        02S0173
         STH   @11,CCWCNT(,CCWPTR)                              02S0173
*       CCWPTR=CCWPTR+8;            /* BUMP TO NEW SLOT.             */
         AL    CCWPTR,@CF00042                                  02S0174
*       CCW=NOPCCW;                 /* PUT IN NOP.                   */
         MVC   CCW(8,CCWPTR),NOPCCW                             02S0175
*                                                               03S0176
*       /*************************************************************/
*       /*                                                           */
*       /* EXECUTE THE CHANNEL PROGRAM.                              */
*       /*                                                           */
*       /*************************************************************/
*                                                               03S0176
*       IOBCMDCH='1'B;              /* INDICATE COMMAND CHAINING.    */
*       IOBUNREL='1'B;              /* INDICATE UNRELATED REQUEST.   */
         L     @11,DCBPTR(,APUBPTR)                             03S0177
         L     @11,IOBSTPTR+16(,@11)                            03S0177
         OI    IOBCMDCH(@11),B'01000010'                        03S0177
*       RESPECIFY                                               03S0178
*         R2P RESTRICTED;                                       03S0178
*       R2P=IOBSTPTR;               /* ADDRESS OF THE IOB.           */
         LR    R2P,@11                                          03S0179
*       RFY                                                     03S0180
*         R3P RSTD;                 /* WILL CONTAIN THE ECB PTR.     */
*       R3P=IOBECBPT;               /* POINTER TO THE ECB.           */
         L     R3P,IOBECBPT(,@11)                               03S0181
*       ECBCC='00'X;                /* RESET COMPLETION CODE.        */
         L     @11,ECBPTR(,@11)                                 03S0182
         MVI   ECBCC(@11),X'00'                                 03S0182
*       GEN REFS(R2P,R3P) SETS(ECBCC);                          03S0183
*                                 /* ISSUE THE EXCP.                 */
         EXCP  (R2P)               DO THE I/O.
         WAIT 1,ECB=(R3P)          WAIT FOR I/O TO COMPLETE.
*       RFY                                                     03S0184
*         R3P UNRSTD;               /* RELEASE THE REG.              */
*       IF ECBCC^='7F'X THEN        /* IF NOT GOOD                   */
         L     @11,DCBPTR(,APUBPTR)                             03S0185
         L     @11,IOBSTPTR+16(,@11)                            03S0185
         L     @11,ECBPTR(,@11)                                 03S0185
         CLI   ECBCC(@11),X'7F'                                 03S0185
         BE    @RF00185                                         03S0185
*         RC=12;                    /* SET ERROR CODE.               */
         LA    RC,12                                            03S0186
*       GEN REFS(SINAME)            /* DELETE THE LOAD MODULE.       */
*           (DELETE EPLOC=SINAME);                              02S0187
@RF00185 DS    0H                                               02S0187
         DELETE EPLOC=SINAME
*       GOTO ENDAELD;                                           02S0188
         B     ENDAELD                                          02S0188
*NOAEMOD:                                                       02S0189
*       ;                           /* SET ERROR CODE.               */
NOAEMOD  DS    0H                                               02S0190
*       RC=32;                                                  02S0190
         LA    RC,32                                            02S0190
*ENDAELD:                                                       02S0191
*       ;                                                       02S0191
ENDAELD  DS    0H                                               02S0192
*     END;                                                      02S0192
*   IF RC=0 THEN                    /* IF STILL NO ERRORS,           */
@RF00163 LTR   RC,RC                                            01S0193
         BNZ   @RF00193                                         01S0193
*     DO;                           /* WRITE THE 3838 SYSTEM    02S0194
*                                      INITIALIZATION PARAMETER LIST.*/
*       CCWPTR=ADDR(CCWS);          /* ADDR OF CCW BUILD AREA.       */
         LA    CCWPTR,CCWS                                      02S0195
*       CCW=CONTROL;                /* MOVE IN CONTROL CCW SKELETON. */
         MVC   CCW(8,CCWPTR),CONTROL                            02S0196
*                                                               03S0197
*       /*************************************************************/
*       /*                                                           */
*       /* THIS CODE LOADS THE 3838 SYSTEM INITIALIZATION PARAMETER  */
*       /* LIST AND COMPLETES THE APUBS.                             */
*       /*                                                           */
*       /*************************************************************/
*                                                               03S0197
*       DO;                                                     03S0197
*         SINAME(1:4)='IGT3';       /* BUILD INIT MODULE NAME.       */
         MVC   SINAME(4),@CC01773                               03S0198
*         SINAME(5:6)=UCBNAME(1:2);                             03S0199
         L     @11,UCBPTR(,APUBPTR)                             03S0199
         MVC   SINAME+4(2),UCBNAME(@11)                         03S0199
*         SINAME(7:8)=APUBINIT;                                 03S0200
         MVC   SINAME+6(2),APUBINIT(APUBPTR)                    03S0200
*         R2P=APCTDCLD;                                         03S0201
         L     R2P,APCTDCLD(,APCTPTR)                           03S0201
*         GEN FLOWS(NOSIMOD) SETS(PARMPTR,MODLN)/* LOAD THE MODULE.  */
*             REFS(R2P,R0P);                                    03S0202
   LOAD  EPLOC=SINAME,ERRET=NOSIMOD,DCB=(R2P)
   ST    R0P,PARMPTR
   SLL   R1P,3                       COMVERT FROM DBL WDS TO BYTES.
   ST    R1P,MODLN                   SAVE LENGTH OF THE MODULE.
*                                                               04S0203
*         /***********************************************************/
*         /*                                                         */
*         /* OBTAIN AND VALIDATE REGION DATA.                        */
*         /*                                                         */
*         /***********************************************************/
*                                                               04S0203
*         DO;                       /* SET APUB REGION SIZES.        */
*           REGION=''X;             /* INIT REGION COUNTER.          */
         SLR   @11,@11                                          05S0204
         STH   @11,REGION                                       05S0204
*           TOTALSZ=REG1SIZE+REG2SIZE/* COMPUTE SIZE REQUIRED BY     */
*               +REG3SIZE+REG4SIZE  /* PARTITION 1-7.                */
*               +REG5SIZE+REG6SIZE+REG7SIZE+RSRVSIZE;/* AND RESERVED
*                                      BULK STORAGE.                 */
         L     @11,PARMPTR                                      05S0205
         L     TOTALSZ,REG1SIZE(,@11)                           05S0205
         AL    TOTALSZ,REG2SIZE(,@11)                           05S0205
         AL    TOTALSZ,REG3SIZE(,@11)                           05S0205
         AL    TOTALSZ,REG4SIZE(,@11)                           05S0205
         AL    TOTALSZ,REG5SIZE(,@11)                           05S0205
         AL    TOTALSZ,REG6SIZE(,@11)                           05S0205
         AL    TOTALSZ,REG7SIZE(,@11)                           05S0205
         AL    TOTALSZ,RSRVSIZE                                 05S0205
*           REGNO='7';              /* INIT REGION NUMBER.           */
         MVI   REGNO,C'7'                                       05S0206
*           IF TOTALSZ^=BSSIZE      /* IF NOT EQUAL, MUST ADJUST.    */
*             THEN                                              05S0207
         L     @11,BSSIZE                                       05S0207
         CR    TOTALSZ,@11                                      05S0207
         BE    @RF00207                                         05S0207
*             IF TOTALSZ>BSSIZE THEN/* IF TOO MUCH IN REGIONS        */
         CR    TOTALSZ,@11                                      05S0208
         BNH   @RF00208                                         05S0208
*               DO I=7 TO 1 BY-1    /* REDUCE THEM UNTIL FIT.        */
*                     WHILE TOTALSZ^=BSSIZE;                    05S0209
         LA    I,7                                              05S0209
@DL00209 L     @11,BSSIZE                                       05S0209
         CR    TOTALSZ,@11                                      05S0209
         BE    @DC00209                                         05S0209
*                 IF REGSIZE(I)<=   /* IF THIS REGION CAN'T DO IT ALL*/
*                     (TOTALSZ-BSSIZE) THEN                     05S0210
         L     @09,PARMPTR                                      05S0210
         LR    @15,I                                            05S0210
         SLA   @15,2                                            05S0210
         LA    @01,0(@15,@09)                                   05S0210
         AL    @01,@CF01878                                     05S0210
         L     @14,REGSIZE(,@01)                                05S0210
         LCR   @11,@11                                          05S0210
         ALR   @11,TOTALSZ                                      05S0210
         CR    @14,@11                                          05S0210
         BH    @RF00210                                         05S0210
*                   IF REGSIZE(I)^=0 THEN/* THEN, UNLESS IT'S NULL,  */
         SLR   @11,@11                                          05S0211
         CR    @14,@11                                          05S0211
         BE    @RF00211                                         05S0211
*                     DO;                                       05S0212
*                       TOTALSZ=TOTALSZ-REGSIZE(I);/* REDUCE OVERRUN */
         SLR   TOTALSZ,@14                                      05S0213
*                       REGSIZE(I)=0;/* BY THE AMOUNT POSSIBLE.      */
         AL    @09,@CF01878                                     05S0214
         ST    @11,REGSIZE(@15,@09)                             05S0214
*                     END;                                      05S0215
*                   ELSE                                        05S0216
*                     ;                                         05S0216
@RF00211 DS    0H                                               05S0217
*                 ELSE                                          05S0217
*                   DO;             /* THIS REGION CAN SWALLOW ALL
*                                      THE                           */
         B     @RC00210                                         05S0217
@RF00210 DS    0H                                               05S0218
*                     REGSIZE(I)=REGSIZE(I)-/* OVERRUN.              */
*                         (TOTALSZ-BSSIZE);                     05S0218
         L     @11,PARMPTR                                      05S0218
         LR    @09,I                                            05S0218
         SLA   @09,2                                            05S0218
         L     @15,BSSIZE                                       05S0218
         LR    @14,TOTALSZ                                      05S0218
         SLR   @14,@15                                          05S0218
         LCR   @14,@14                                          05S0218
         LA    @01,0(@09,@11)                                   05S0218
         AL    @01,@CF01878                                     05S0218
         AL    @14,REGSIZE(,@01)                                05S0218
         AL    @11,@CF01878                                     05S0218
         ST    @14,REGSIZE(@09,@11)                             05S0218
*                     TOTALSZ=BSSIZE;                           05S0219
         LR    TOTALSZ,@15                                      05S0219
*                   END;                                        05S0220
*                                                               06S0221
*                 /***************************************************/
*                 /*                                                 */
*                 /* GET REGION SIZE IN ECBDIC.                      */
*                 /*                                                 */
*                 /***************************************************/
*                                                               06S0221
*                 RESPECIFY                                     06S0221
*                   R2P RESTRICTED;                             06S0221
@RC00210 DS    0H                                               06S0222
*                 R2P=REGSIZE(I)/256;/* GET REG SIZE IN K BYTES      */
         LR    @11,I                                            06S0222
         SLA   @11,2                                            06S0222
         L     @09,PARMPTR                                      06S0222
         AL    @09,@CF01878                                     06S0222
         L     @00,REGSIZE(@11,@09)                             06S0222
         SRDA  @00,32                                           06S0222
         D     @00,@CF01778                                     06S0222
         LR    R2P,@01                                          06S0222
*                 CVD(R2P,PACKREG);                             06S0223
         CVD   R2P,@TS00001                                     06S0223
         MVC   PACKREG(3),@TS00001+5                            06S0223
*                 UNPK(EBCDIC,PACKREG);                         06S0224
         UNPK  EBCDIC(5),PACKREG(3)                             06S0224
*                 EBCDIC(5)=EBCDIC(5)×'F0'X;                    06S0225
*                                                               06S0225
         OI    EBCDIC+4,X'F0'                                   06S0225
*                 /***************************************************/
*                 /*                                                 */
*                 /* WRITE REGION SIZE MESSAGE.                      */
*                 /*                                                 */
*                 /***************************************************/
*                                                               06S0226
*                 CALL APCVZDWT(IGT250I,14,APCTSSID,4,20,UCBNAME,3,29,
*                     REGNO,1,38,EBCDIC,5);                     06S0226
         MVC   @AL00001(52),@AL00226                            06S0226
         LA    @11,APCTSSID(,APCTPTR)                           06S0226
         ST    @11,@AL00001+8                                   06S0226
         L     @11,UCBPTR(,APUBPTR)                             06S0226
         LA    @11,UCBNAME(,@11)                                06S0226
         ST    @11,@AL00001+20                                  06S0226
         LA    @11,REGNO                                        06S0226
         ST    @11,@AL00001+32                                  06S0226
         LA    @11,EBCDIC                                       06S0226
         ST    @11,@AL00001+44                                  06S0226
         L     @15,APCTZDWT(,APCTPTR)                           06S0226
         LA    @01,@AL00001                                     06S0226
         BALR  @14,@15                                          06S0226
*                 RESPECIFY                                     06S0227
*                   R2P UNRESTRICTED;                           06S0227
*                 REGION=REGION-1;  /* NEXT REGION.                  */
         LH    @11,REGION                                       05S0228
         BCTR  @11,0                                            05S0228
         STH   @11,REGION                                       05S0228
*               END;                                            05S0229
         BCTR  I,0                                              05S0229
         LTR   I,I                                              05S0229
         BP    @DL00209                                         05S0229
@DC00209 DS    0H                                               05S0230
*             ELSE                                              05S0230
*               DO;                 /* TOTAL WAS NOT ENOUGH, SO      */
         B     @RC00208                                         05S0230
@RF00208 DS    0H                                               05S0231
*                 DO I=7 TO 2 BY-1  /* INCREASE FIRST NON-ZERO       */
*                       WHILE REGSIZE(I)=0;/* PARTITION SIZE.        */
         LA    I,7                                              05S0231
@DL00231 LR    @11,I                                            05S0231
         SLA   @11,2                                            05S0231
         L     @09,PARMPTR                                      05S0231
         AL    @09,@CF01878                                     05S0231
         L     @11,REGSIZE(@11,@09)                             05S0231
         LTR   @11,@11                                          05S0231
         BNZ   @DC00231                                         05S0231
*                   REGION=REGION-1;/* NEXT REGION.                  */
         LH    @11,REGION                                       05S0232
         BCTR  @11,0                                            05S0232
         STH   @11,REGION                                       05S0232
*                 END;              /* IF THE WHILE CONDITION IS
*                                      TRUE, I WILL BE 2 OR GREATER.
*                                      IF THE LOOP IS TERMINATED BY
*                                      COUNT, I WILL BE 1 WHEN WE
*                                      DROP OUT.                     */
         BCTR  I,0                                              05S0233
         C     I,@CF00116                                       05S0233
         BNL   @DL00231                                         05S0233
@DC00231 DS    0H                                               05S0234
*                 REGSIZE(I)=REGSIZE(I)/* INCREASE BY ENOUGH TO 05S0234
*                                      ACCOUNT                       */
*                     +(BSSIZE-TOTALSZ);/* FOR ALL OF BULK STORAGE.  */
         L     @11,PARMPTR                                      05S0234
         LR    @09,I                                            05S0234
         SLA   @09,2                                            05S0234
         L     @02,BSSIZE                                       05S0234
         SLR   @02,TOTALSZ                                      05S0234
         LA    @01,0(@09,@11)                                   05S0234
         AL    @01,@CF01878                                     05S0234
         AL    @02,REGSIZE(,@01)                                05S0234
         AL    @11,@CF01878                                     05S0234
         ST    @02,REGSIZE(@09,@11)                             05S0234
*                                                               06S0235
*                 /***************************************************/
*                 /*                                                 */
*                 /* GET REGION SIZE IN ECBDIC.                      */
*                 /*                                                 */
*                 /***************************************************/
*                                                               06S0235
*                 RESPECIFY                                     06S0235
*                   R2P RESTRICTED;                             06S0235
*                 R2P=REGSIZE(I)/256;/* GET REG SIZE IN K BYTES      */
         ST    @02,@TF00001                                     06S0236
         L     @00,@TF00001                                     06S0236
         SRDA  @00,32                                           06S0236
         D     @00,@CF01778                                     06S0236
         LR    R2P,@01                                          06S0236
*                 CVD(R2P,PACKREG);                             06S0237
         CVD   R2P,@TS00001                                     06S0237
         MVC   PACKREG(3),@TS00001+5                            06S0237
*                 UNPK(EBCDIC,PACKREG);                         06S0238
         UNPK  EBCDIC(5),PACKREG(3)                             06S0238
*                 EBCDIC(5)=EBCDIC(5)×'F0'X;                    06S0239
*                                                               06S0239
         OI    EBCDIC+4,X'F0'                                   06S0239
*                 /***************************************************/
*                 /*                                                 */
*                 /* WRITE REGION SIZE MESSAGE.                      */
*                 /*                                                 */
*                 /***************************************************/
*                                                               06S0240
*                 CALL APCVZDWT(IGT250I,14,APCTSSID,4,20,UCBNAME,3,29,
*                     REGNO,1,38,EBCDIC,5);                     06S0240
         MVC   @AL00001(52),@AL00240                            06S0240
         LA    @11,APCTSSID(,APCTPTR)                           06S0240
         ST    @11,@AL00001+8                                   06S0240
         L     @11,UCBPTR(,APUBPTR)                             06S0240
         LA    @11,UCBNAME(,@11)                                06S0240
         ST    @11,@AL00001+20                                  06S0240
         LA    @11,REGNO                                        06S0240
         ST    @11,@AL00001+32                                  06S0240
         LA    @11,EBCDIC                                       06S0240
         ST    @11,@AL00001+44                                  06S0240
         L     @15,APCTZDWT(,APCTPTR)                           06S0240
         LA    @01,@AL00001                                     06S0240
         BALR  @14,@15                                          06S0240
*                 RESPECIFY                                     06S0241
*                   R2P UNRESTRICTED;                           06S0241
*               END;                                            05S0242
*         END;                                                  05S0243
@RC00208 DS    0H                                               05S0243
@RF00207 DS    0H                                               05S0244
