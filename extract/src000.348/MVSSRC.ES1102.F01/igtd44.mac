TD44     TITLE 'IGTD44 - DISABLE COMMAND PROCESSOR.                    *
                        '
IGTD44   CSECT ,                                                01S0001
@MAINENT DS    0H                                               01S0001
         USING *,@15                                            01S0001
         B     @PROLOG                                          01S0001
         DC    AL1(16)                                          01S0001
         DC    C'IGTD44    77.092'                              01S0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                  01S0001
         BALR  @07,0                                            01S0001
@PSTART  DS    0H                                               01S0001
         USING @PSTART,@07                                      01S0001
         L     @00,@SIZDATD                                     01S0001
         IGTYGSV  R,LV=(0)
         LR    @06,@01                                          01S0001
         USING @DATD,@06                                        01S0001
         ST    @13,@SA00001+4                                   01S0001
         LM    @00,@01,20(@13)                                  01S0001
         ST    @06,8(,@13)                                      01S0001
         LR    @13,@06                                          01S0001
*   CALL APCVZDEQ;                  /* OBTAIN CONTROL OF APUBS.      */
         L     @15,APCTZDEQ(,APCTPTR)                           01S0102
         BALR  @14,@15                                          01S0102
*   RESPECIFY                                                   01S0103
*     APCQ BASED(APCQPTR);                                      01S0103
*   RESPECIFY                                                   01S0104
*     APCQPTR RESTRICTED;                                       01S0104
*                                                               01S0104
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE ARGUMENTS FOR IGTDAP.                              */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0105
*   PARMLEN=APCQPLEN;               /* LENGTH OF UNITS FIELD.        */
         LH    @11,APCQPLEN(,APCQPTR)                           01S0105
         STH   @11,PARMLEN                                      01S0105
*   PARMADDR=ADDR(APCQCMND)         /* START OF UNITS FIELD.         */
*       +APCQPSTR-1;                                            01S0106
         LA    @08,APCQCMND(,APCQPTR)                           01S0106
         LH    @15,APCQPSTR(,APCQPTR)                           01S0106
         ALR   @08,@15                                          01S0106
         BCTR  @08,0                                            01S0106
         ST    @08,PARMADDR                                     01S0106
*   WORKAREA=''X;                   /* CLEAR WORKAREA.               */
         XC    WORKAREA(8),WORKAREA                             01S0107
*   IF APCQPLEN>=3 THEN             /* IF FIRST UNIT EXISTS, PUT IT
*                                      IN                            */
         C     @11,@CF00045                                     01S0108
         BL    @RF00108                                         01S0108
*     WORKAREA(2:4)=                /* WORKAREA. IF PARM IS EITHER   */
*         APCQCMND(APCQPSTR:        /* SCREWED UP OR ABSENT, IGTDAP  */
*         APCQPSTR+2);              /* WILL CATCH IT.                */
         ALR   @15,APCQPTR                                      01S0109
         MVC   WORKAREA+1(3),APCQCMND-1(@15)                    01S0109
*   DO UNTIL RC=4                   /* RC = 4 WHEN LAST ONE IS DONE. */
*         ×RC=8;                    /* RC = 8 WHEN ERROR             */
@RF00108 DS    0H                                               01S0110
@DL00110 DS    0H                                               01S0111
*     CALL APCVZDAP(PARMADDR,PARMLEN,WORKAREA);/* GET APUBPTR.       */
         LA    @11,PARMADDR                                     01S0111
         ST    @11,@AL00001                                     01S0111
         LA    @11,PARMLEN                                      01S0111
         ST    @11,@AL00001+4                                   01S0111
         LA    @11,WORKAREA                                     01S0111
         ST    @11,@AL00001+8                                   01S0111
         L     @15,APCTZDAP(,APCTPTR)                           01S0111
         LA    @01,@AL00001                                     01S0111
         BALR  @14,@15                                          01S0111
*     RC=R15P;                      /* SAVE RETURN CODE.             */
         LR    RC,R15P                                          01S0112
*     IF RC=8 THEN                  /* SEE IF ERROR.                 */
         LA    @11,8                                            01S0113
         CR    RC,@11                                           01S0113
         BNE   @RF00113                                         01S0113
*       DO;                                                     02S0114
*         IF RC=8 THEN              /* IF UNIT NOT FOUND             */
         CR    RC,@11                                           02S0115
         BNE   @RF00115                                         02S0115
*           UNIT=WORKAREA(2:4);     /* USE SEARCH ARGUMENT           */
         MVC   UNIT(3),WORKAREA+1                               02S0116
*         ELSE                      /* OTHERWISE                     */
*           UNIT=UCBNAME;           /* MUST USE UCBNAME SINCE SEARCH
*                                      ARGUMENT ALTERED BY IGTDAP.   */
         B     @RC00115                                         02S0117
@RF00115 L     @11,UCBPTR(,APUBPTR)                             02S0117
         MVC   UNIT(3),UCBNAME(@11)                             02S0117
*         CALL APCVZDWT(IGT235I,    /* WRITE INVALID DEVICE MESSAGE. */
*             14,APCTSSID,4,20,UNIT,3);                         02S0118
@RC00115 MVC   @AL00001(28),@AL00118                            02S0118
         LA    @11,APCTSSID(,APCTPTR)                           02S0118
         ST    @11,@AL00001+8                                   02S0118
         LA    @11,UNIT                                         02S0118
         ST    @11,@AL00001+20                                  02S0118
         L     @15,APCTZDWT(,APCTPTR)                           02S0118
         LA    @01,@AL00001                                     02S0118
         BALR  @14,@15                                          02S0118
*       END;                                                    02S0119
*     ELSE                          /* OTHERWISE,                    */
*       DO;                                                     02S0120
*                                                               02S0120
         B     @RC00113                                         02S0120
@RF00113 DS    0H                                               02S0121
*         /***********************************************************/
*         /*                                                         */
*         /* APUBPTR INITIALIZED BY IGTDAP.                          */
*         /*                                                         */
*         /***********************************************************/
*                                                               02S0121
*         IF APUBSBCH='1'B THEN     /* TEST FOR CONTROL SUBCHANNEL.  */
         TM    APUBSBCH(APUBPTR),B'10000000'                    02S0121
         BNO   @RF00121                                         02S0121
*           DO;                                                 02S0122
*             DO;                                               03S0123
*               IF RC=8 THEN        /* IF UNIT NOT FOUND             */
         C     RC,@CF00042                                      03S0124
         BNE   @RF00124                                         03S0124
*                 UNIT=WORKAREA(2:4);/* USE SEARCH ARGUMENT          */
         MVC   UNIT(3),WORKAREA+1                               03S0125
*               ELSE                /* OTHERWISE                     */
*                 UNIT=UCBNAME;     /* MUST USE UCBNAME SINCE SEARCH
*                                      ARGUMENT ALTERED BY IGTDAP.   */
         B     @RC00124                                         03S0126
@RF00124 L     @11,UCBPTR(,APUBPTR)                             03S0126
         MVC   UNIT(3),UCBNAME(@11)                             03S0126
*               CALL APCVZDWT(IGT235I,/* WRITE INVALID DEVICE   03S0127
*                                      MESSAGE.                      */
*                   14,APCTSSID,4,20,UNIT,3);                   03S0127
@RC00124 MVC   @AL00001(28),@AL00127                            03S0127
         LA    @11,APCTSSID(,APCTPTR)                           03S0127
         ST    @11,@AL00001+8                                   03S0127
         LA    @11,UNIT                                         03S0127
         ST    @11,@AL00001+20                                  03S0127
         L     @15,APCTZDWT(,APCTPTR)                           03S0127
         LA    @01,@AL00001                                     03S0127
         BALR  @14,@15                                          03S0127
*             END;                                              03S0128
*           END;                                                02S0129
*         ELSE                                                  02S0130
*           IF APUBIDAB='1'B THEN   /* SEE IF ITS ALLREADY DISABLED. */
         B     @RC00121                                         02S0130
@RF00121 TM    APUBIDAB(APUBPTR),B'01000000'                    02S0130
         BNO   @RF00130                                         02S0130
*             DO;                   /* IF SO, JUST PUT OUT THE  02S0131
*                                      MESSAGE                       */
*               CALL APCVZDWT(IGT233I,/* TELL OPERATOR THAT ITS 02S0132
*                                      DISABLED                      */
*                   14,APCTSSID,4,  /* SUBSYS ID.                    */
*                   20,UCBNAME,3);  /* UNIT NAME.                    */
         MVC   @AL00001(28),@AL00132                            02S0132
         LA    @11,APCTSSID(,APCTPTR)                           02S0132
         ST    @11,@AL00001+8                                   02S0132
         L     @11,UCBPTR(,APUBPTR)                             02S0132
         LA    @11,UCBNAME(,@11)                                02S0132
         ST    @11,@AL00001+20                                  02S0132
         L     @15,APCTZDWT(,APCTPTR)                           02S0132
         LA    @01,@AL00001                                     02S0132
         BALR  @14,@15                                          02S0132
*             END;                  /* END OF MESSAGE WRITE.         */
*           ELSE                    /* OTHER WISE TRY TO DISABLE IT. */
*             DO;                                               02S0134
         B     @RC00130                                         02S0134
@RF00130 DS    0H                                               02S0135
*               APUBDSBL='1'B;      /* MARK THE PORT AS DISABLED.    */
         L     @11,APUBDSBL-1(,APUBPTR)                         02S0135
@GL00001 LR    @08,@11                                          02S0135
         O     @08,@CF01213                                     02S0135
         CS    @11,@08,APUBDSBL-1(APUBPTR)                      02S0135
         BNE   @GL00001                                         02S0135
*               IF APUBOWN='1'B THEN/* IF THE PORT IS EXCLUSIVELY
*                                      OWNED                         */
         TM    APUBOWN(APUBPTR),B'01000000'                     02S0136
         BNO   @RF00136                                         02S0136
*                 DO;               /* IT CANNOT BE DISABLED NOW.    */
*                   APUBDSBP='1'B;  /* SO MARK IT DISABLED PENDING,  */
         L     @11,APUBDSBP-1(,APUBPTR)                         02S0138
@GL00002 LR    @08,@11                                          02S0138
         O     @08,@CF01216                                     02S0138
         CS    @11,@08,APUBDSBP-1(APUBPTR)                      02S0138
         BNE   @GL00002                                         02S0138
*                   APCTAPST='1'B;  /* AND GET UNALLOC TO POST US.   */
         L     @11,APCTAPST-1(,APCTPTR)                         02S0139
@GL00003 LR    @08,@11                                          02S0139
         O     @08,@CF01213                                     02S0139
         CS    @11,@08,APCTAPST-1(APCTPTR)                      02S0139
         BNE   @GL00003                                         02S0139
*                 END;              /* END OF EXCLUSIVE PORT    02S0140
*                                      PROCESSING.                   */
*               ELSE                                            02S0141
*                 DO;                                           02S0141
         B     @RC00136                                         02S0141
@RF00136 DS    0H                                               02S0142
*                   CALL APCVZDDA;  /* SEE IF THE PORT CAN BE   02S0142
*                                      DISABLED.                     */
         L     @15,APCTZDDA(,APCTPTR)                           02S0142
         BALR  @14,@15                                          02S0142
*                   IF R15P=0 THEN  /* ZERO MEANS THAT DISABLING IS  */
         LTR   R15P,R15P                                        02S0143
         BNZ   @RF00143                                         02S0143
*                     DO;           /* DELAYED UNTIL SOME USERS 02S0144
*                                      DEPART.                       */
*                       APUBDSBP='1'B;/* INDICATE DISABLE PENDING.   */
         L     @11,APUBDSBP-1(,APUBPTR)                         02S0145
@GL00004 LR    @08,@11                                          02S0145
         O     @08,@CF01216                                     02S0145
         CS    @11,@08,APUBDSBP-1(APUBPTR)                      02S0145
         BNE   @GL00004                                         02S0145
*                       APCTAPST='1'B;/* GET UNALLOC TO POST US.     */
         L     @11,APCTAPST-1(,APCTPTR)                         02S0146
@GL00005 LR    @08,@11                                          02S0146
         O     @08,@CF01213                                     02S0146
         CS    @11,@08,APCTAPST-1(APCTPTR)                      02S0146
         BNE   @GL00005                                         02S0146
*                     END;                                      02S0147
*                 END;                                          02S0148
@RF00143 DS    0H                                               02S0149
*               IF APUBDSBP='1'B THEN/* IF DISABLE IS PENDING, THE   */
@RC00136 TM    APUBDSBP(APUBPTR),B'00001000'                    02S0149
         BNO   @RF00149                                         02S0149
*                 CALL APCVZDWT(IGT234I,/* OPERATOR MUST BE TOLD.    */
*                     14,APCTSSID,4,20,UCBNAME,3);              02S0150
         MVC   @AL00001(28),@AL00150                            02S0150
         LA    @11,APCTSSID(,APCTPTR)                           02S0150
         ST    @11,@AL00001+8                                   02S0150
         L     @11,UCBPTR(,APUBPTR)                             02S0150
         LA    @11,UCBNAME(,@11)                                02S0150
         ST    @11,@AL00001+20                                  02S0150
         L     @15,APCTZDWT(,APCTPTR)                           02S0150
         LA    @01,@AL00001                                     02S0150
         BALR  @14,@15                                          02S0150
*             END;                                              02S0151
@RF00149 DS    0H                                               02S0152
*       END;                                                    02S0152
@RC00130 DS    0H                                               02S0152
@RC00121 DS    0H                                               02S0153
*   END;                                                        01S0153
@RC00113 DS    0H                                               01S0153
@DE00110 C     RC,@CF00052                                      01S0153
         BE    @DC00110                                         01S0153
         C     RC,@CF00042                                      01S0153
         BNE   @DL00110                                         01S0153
@DC00110 DS    0H                                               01S0154
*   CALL APCVZDDQ;                  /* DEQUEUE FROM APUBS.           */
         L     @15,APCTZDDQ(,APCTPTR)                           01S0154
         BALR  @14,@15                                          01S0154
*   RETURN;                         /* UP,UP AND AWAY. IGTXEPLG      */
@EL00001 L     @13,4(,@13)                                      01S0155
@EF00001 L     @00,@SIZDATD                                     01S0155
         LR    @01,@06                                          01S0155
         IGTYFSV  R,LV=(0),A=(1)
@ER00001 LM    @14,@12,12(@13)                                  01S0155
         BR    @14                                              01S0155
*                                                               01S0156
