TDIP     TITLE 'IGTDIP - COMMON IPL PROCESSING.                        *
                        '
IGTDIP   CSECT ,                                                01S0001
@MAINENT DS    0H                                               01S0001
         USING *,@15                                            01S0001
         B     @PROLOG                                          01S0001
         DC    AL1(16)                                          01S0001
         DC    C'IGTDIP    77.231'                              01S0001
         DROP  @15
@PROLOG  STM   @14,@08,12(@13)                                  01S0001
         STM   @10,@12,60(@13)                                  01S0001
         BALR  @07,0                                            01S0001
@PSTART  DS    0H                                               01S0001
         USING @PSTART,@07                                      01S0001
         L     @00,@SIZDATD                                     01S0001
         IGTYGSV  R,LV=(0)
         LR    @06,@01                                          01S0001
         USING @DATD,@06                                        01S0001
         ST    @13,@SA00001+4                                   01S0001
         LM    @00,@01,20(@13)                                  01S0001
         ST    @06,8(,@13)                                      01S0001
         LR    @13,@06                                          01S0001
*   LOADNAME(1:3)='IPL';            /* BUILD IPL LOAD MODULE NAME.   */
         MVC   LOADNAME(3),@CC01135                             01S0111
*   LOADNAME(4:5)=UCBNAME(1:2);     /* CHANNEL AND SUBCHANNEL.       */
         L     @11,UCBPTR(,APUBPTR)                             01S0112
         MVC   LOADNAME+3(2),UCBNAME(@11)                       01S0112
*   LOADNAME(6:7)=APUBIPL;          /* LOAD MODULE ID.               */
         MVC   LOADNAME+5(2),APUBIPL(APUBPTR)                   01S0113
*   LOADNAME(8)=' ';                /* BLANK LAST CHAR.              */
         MVI   LOADNAME+7,C' '                                  01S0114
*   IF APCQIINF=APCQRIPL THEN       /* IF THIS IS A REIPL COMMAND,   */
         CLC   APCQIINF(4,APCQPTR),@CF00224                     01S0115
         BNE   @RF00115                                         01S0115
*     DO;                           /* THEN AN INDICATOR MUST BE SET
*                                      ON TO SIGNIFY THAT BULK STORE
*                                      IS BAD.                       */
*       SAVEAPUB=APUBPTR;           /* SAVE THE ADDRESS OF THE  01S0117
*                                      MASTER.                       */
         LR    SAVEAPUB,APUBPTR                                 01S0117
*       DO UNTIL APUBPTR=0;         /* LOOP THRU THE APUBS ON 3838.  */
@DL00118 DS    0H                                               01S0119
*         APUBIRIP='1'B;            /* INDICATE THAT REIPL TAKES
*                                      PLACE.                        */
         L     @11,APUBIRIP-3(,APUBPTR)                         01S0119
@GL00001 LR    @08,@11                                          01S0119
         O     @08,@CF01250                                     01S0119
         CS    @11,@08,APUBIRIP-3(APUBPTR)                      01S0119
         BNE   @GL00001                                         01S0119
*         APUBPTR=APUBAPUB;         /* GO TO NEXT APUB.              */
         L     APUBPTR,APUBAPUB(,APUBPTR)                       01S0120
*       END;                        /* END OF LOOP.                  */
@DE00118 LTR   APUBPTR,APUBPTR                                  01S0121
         BNZ   @DL00118                                         01S0121
*       APUBPTR=SAVEAPUB;           /* RESTORE THE APUB PTR.         */
         LR    APUBPTR,SAVEAPUB                                 01S0122
*     END;                          /* END OF REIPL PROCESSING.      */
*   CALL APCVZDIO(LOADNAME);        /* PERFORM IPL I/O.              */
@RF00115 LA    @11,LOADNAME                                     01S0124
         ST    @11,@AL00001                                     01S0124
         MVI   @AL00001,X'80'                                   01S0124
         L     @15,APCTZDIO(,APCTPTR)                           01S0124
         LA    @01,@AL00001                                     01S0124
         BALR  @14,@15                                          01S0124
*   RESPECIFY                                                   01S0125
*     R15P RESTRICTED;              /* SAVE RETURN CODE.             */
*   IF R15P^=0 THEN                 /* PROCESS IPL ERRORS.           */
         LTR   R15P,R15P                                        01S0126
         BZ    @RF00126                                         01S0126
*     CALL ERRPROC;                 /* INVOKE THE IPL ERROR     02S0127
*                                      PROCEDURE.                    */
         BAL   @14,ERRPROC                                      02S0127
*   ELSE                                                        01S0128
*     DO;                                                       02S0128
         B     @RC00126                                         02S0128
@RF00126 DS    0H                                               02S0129
*       CALL APCVZDID;              /* COMPLETE THE IPL PROCEDURE.   */
         L     @15,APCTZDID(,APCTPTR)                           02S0129
         BALR  @14,@15                                          02S0129
*       IF R15P=0 THEN              /* SEE IF EVERYTHING WENT OK.    */
         LTR   R15P,R15P                                        02S0130
         BNZ   @RF00130                                         02S0130
*         DO;                       /* IF SO UPDATE THE APUBS.       */
*           SAVEAPUB=APUBPTR;       /* SAVE THE ORIGINAL APUB   02S0132
*                                      ADDRESS.                      */
         LR    SAVEAPUB,APUBPTR                                 02S0132
*           DO UNTIL APUBPTR=0;                                 02S0133
@DL00133 DS    0H                                               02S0134
*             IF UCBQISCE='0'B THEN /* PRESERVE SETTING OF QUIESCE   */
         L     @14,UCBPTR(,APUBPTR)                             02S0134
         TM    UCBQISCE(@14),B'00000001'                        02S0134
         BNZ   @RF00134                                         02S0134
*               APUBQISC='0'B;      /* BIT TO AID IN DEBUGGING.      */
         L     @14,APUBQISC(,APUBPTR)                           02S0135
@GL00002 LR    @11,@14                                          02S0135
         N     @11,@CF01253                                     02S0135
         CS    @14,@11,APUBQISC(APUBPTR)                        02S0135
         BNE   @GL00002                                         02S0135
*             ELSE                  /* APUBQISC SETTING WILL REFLECT */
*               DO;                 /* QUIESCE WAS ON SO RESET IT.   */
         B     @RC00134                                         02S0136
@RF00134 DS    0H                                               02S0137
*                 APUBQISC='1'B;    /* THE UCBQISCE BIT SETTING.     */
         L     @14,APUBQISC(,APUBPTR)                           02S0137
@GL00003 LR    @11,@14                                          02S0137
         O     @11,@CF01256                                     02S0137
         CS    @14,@11,APUBQISC(APUBPTR)                        02S0137
         BNE   @GL00003                                         02S0137
*                 RFY                                           02S0138
*                   R1P RSTD;       /* RESET THE QUIESCE BIT IN CASE
*                                      THAT WAS THE PROBLEM.         */
*                 R1P=1;            /* FUNCTION CODE = RESET QUIESCE.*/
         LA    R1P,1                                            02S0139
*                 CALL APCVZUJA;    /* RESET THE QUIESCE BIT.        */
         L     @15,APCTZUJA(,APCTPTR)                           02S0140
         BALR  @14,@15                                          02S0140
*                 RFY                                           02S0141
*                   R1P UNRSTD;     /* QUIESCE NOW OFF,BUT APUBQISC
*                                      WILL TELL US IT WAS ON.       */
*               END;                /* END OF RESET THE QUIESCE BIT. */
*             APUBRIPL='0'B;        /* RESET THE ALLOC. RE-IPL FLAG. */
@RC00134 L     @14,APUBRIPL(,APUBPTR)                           02S0143
@GL00004 LR    @11,@14                                          02S0143
         N     @11,@CF01259                                     02S0143
         CS    @14,@11,APUBRIPL(APUBPTR)                        02S0143
         BNE   @GL00004                                         02S0143
*             APUBBERR='0'B;        /* RESET ALLOCATION ERROR FLAG.  */
         L     @14,APUBBERR-1(,APUBPTR)                         02S0144
@GL00005 LR    @11,@14                                          02S0144
         N     @11,@CF01262                                     02S0144
         CS    @14,@11,APUBBERR-1(APUBPTR)                      02S0144
         BNE   @GL00005                                         02S0144
*             IF APUBDSBL='0'B THEN /* SEE IF OPERATOR REQUESTED
*                                                            @ZA25362
*                                      DISABLE PENDING.      @ZA25362*/
         TM    APUBDSBL(APUBPTR),B'01000000'                    02S0145
         BNZ   @RF00145                                         02S0145
*               APUBDSBP='0'B;      /* IF NOT RESET DISABLE PENDING.
*                                                            @ZA25362*/
         L     @14,APUBDSBP-1(,APUBPTR)                         02S0146
@GL00006 LR    @11,@14                                          02S0146
         N     @11,@CF01265                                     02S0146
         CS    @14,@11,APUBDSBP-1(APUBPTR)                      02S0146
         BNE   @GL00006                                         02S0146
*             APUBIERR='0'B;        /* RESET I/O ERROR FLAG.         */
@RF00145 DS    0H                                               02S0148
*             APUBIIPL='0'B;        /* RESET THE I/O RE-IPL FLAG.    */
         L     @14,APUBIIPL-1(,APUBPTR)                         02S0148
@GL00007 LR    @11,@14                                          02S0148
         N     @11,@CF01268                                     02S0148
         CS    @14,@11,APUBIIPL-1(APUBPTR)                      02S0148
         BNE   @GL00007                                         02S0148
*             APUBERRS=''X;         /* ZERO ERROR COUNTERS.          */
         XC    APUBERRS(22,APUBPTR),APUBERRS(APUBPTR)           02S0149
*             APUBERRT=0;           /* ZERO TRANSMISSION COUNTER.    */
         SLR   @14,@14                                          02S0150
         ST    @14,APUBERRT(,APUBPTR)                           02S0150
*             IF APCQIINF=APCQIPL THEN/* IS THIS IS AN IPL?          */
         CLC   APCQIINF(4,APCQPTR),@CF00208                     02S0151
         BNE   @RF00151                                         02S0151
*               DO;                 /* IF YES, ------                */
*                 APUBUNLD='0'B;    /* IT IS NOT UNLOADED,           */
*                 APUBUNLP='0'B;    /* OR UNLOAD PENDING,            */
*                 APUBDSBP='0'B;    /* OR DISABLE PENDING.           */
         L     @14,APUBDSBP-1(,APUBPTR)                         02S0155
@GL00008 LR    @11,@14                                          02S0155
         N     @11,@CF01271                                     02S0155
         CS    @14,@11,APUBDSBP-1(APUBPTR)                      02S0155
         BNE   @GL00008                                         02S0155
*                 APUBIDWN='0'B;    /* RESET UNLOADED FOR I/O FLAG.  */
         L     @14,APUBIDWN(,APUBPTR)                           02S0156
@GL00009 LR    @11,@14                                          02S0156
         N     @11,@CF01259                                     02S0156
         CS    @14,@11,APUBIDWN(APUBPTR)                        02S0156
         BNE   @GL00009                                         02S0156
*                 APUBIEXC='0'B;    /* IT IS NOT EXCLUSIVE,          */
         L     @14,APUBIEXC-2(,APUBPTR)                         02S0157
@GL00010 LR    @11,@14                                          02S0157
         N     @11,@CF01274                                     02S0157
         CS    @14,@11,APUBIEXC-2(APUBPTR)                      02S0157
         BNE   @GL00010                                         02S0157
*                 APUBIOCT=0;       /* NUMBER OF ACTIVE I/OS.        */
         SLR   @14,@14                                          02S0158
         ST    @14,APUBIOCT(,APUBPTR)                           02S0158
*                 IF APUBSBCH='0'B& /* IF NOT A CONTROL SUBCHANNEL,  */
*                     APUBZRGN='0'B THEN/* AND IT HAS A REGION,      */
         TM    APUBSBCH(APUBPTR),B'10000000'                    02S0159
         BNZ   @RF00159                                         02S0159
         TM    APUBZRGN(APUBPTR),B'10000000'                    02S0159
         BNZ   @RF00159                                         02S0159
*                   DO;                                         02S0160
*                     APUBDSBL='0'B;/* IT IS NO LONGER DISABLED,     */
         L     @14,APUBDSBL-1(,APUBPTR)                         02S0161
@GL00012 LR    @11,@14                                          02S0161
         N     @11,@CF01277                                     02S0161
         CS    @14,@11,APUBDSBL-1(APUBPTR)                      02S0161
         BNE   @GL00012                                         02S0161
*                     APUBIDAB='0'B;/* RESET DISABLED FOR I/O FLAG.  */
         L     @14,APUBIDAB(,APUBPTR)                           02S0162
@GL00013 LR    @11,@14                                          02S0162
         N     @11,@CF01280                                     02S0162
         CS    @14,@11,APUBIDAB(APUBPTR)                        02S0162
         BNE   @GL00013                                         02S0162
*                     APUBISHR='1'B;/* BUT IT IS ELIGABLE FOR SHARED.*/
         L     @14,APUBISHR-3(,APUBPTR)                         02S0163
@GL00014 LR    @11,@14                                          02S0163
         O     @11,@CF01283                                     02S0163
         CS    @14,@11,APUBISHR-3(APUBPTR)                      02S0163
         BNE   @GL00014                                         02S0163
*                     CALL APCVZUHA(APUB);/* ADD IT TO THE SHARED
*                                      QUEUE.                        */
         ST    APUBPTR,@AL00001                                 02S0164
         L     @15,APCTZUHA(,APCTPTR)                           02S0164
         LA    @01,@AL00001                                     02S0164
         BALR  @14,@15                                          02S0164
*                   END;                                        02S0165
*               END;                                            02S0166
@RF00159 DS    0H                                               02S0167
*             APUBPTR=APUBAPUB;     /* BUMP TO NEXT APUB ON THE 02S0167
*                                      CHAIN.                        */
@RF00151 L     APUBPTR,APUBAPUB(,APUBPTR)                       02S0167
*           END;                                                02S0168
@DE00133 LTR   APUBPTR,APUBPTR                                  02S0168
         BNZ   @DL00133                                         02S0168
*           APUBPTR=SAVEAPUB;                                   02S0169
         LR    APUBPTR,SAVEAPUB                                 02S0169
*           DO;                     /* IGTYPOST ECB(APCTEULD)        */
*             RFY                                               02S0171
*              (GPR00P,                                         02S0171
*               GPR01P,                                         02S0171
*               GPR14F,                                         02S0171
*               GPR15F) RSTD;       /* REGISTERS USED                */
*             GPR15F=APCTEULD;      /* CURRENT ECB CONTENTS          */
         L     GPR15F,APCTEULD(,APCTPTR)                        02S0172
*LTR0009X:                                                      02S0173
*             IF GPR15F<0 THEN      /* IF ECB ALREADY                */
LTR0009X LTR   GPR15F,GPR15F                                    02S0173
         BNM   @RF00173                                         02S0173
*               DO;                 /* WAITED UPON, MUST ISSUE POST  */
*                 GPR01P=ADDR(APCTEULD);/* ADDRESS OF ECB TO POST    */
         LA    GPR01P,APCTEULD(,APCTPTR)                        02S0175
*                 GPR00P=0;         /* POST CODE                     */
         SLR   GPR00P,GPR00P                                    02S0176
*                 SVC(2);           /* ISSUE POST                    */
         SVC   2                                                02S0177
*               END;                /* ECB POSTED                    */
*             ELSE                  /* ECB NOT YET POSTED            */
*               DO;                 /* SO TRY 'COMPARE-AND-SWAP' POST*/
         B     @RC00173                                         02S0179
@RF00173 DS    0H                                               02S0180
*                 GPR14F='40000000'X;/* POST CODE                    */
         L     GPR14F,@CF01151                                  02S0180
*                 CS(GPR15F,GPR14F,APCTEULD);/* POST THE ECB         */
         CS    GPR15F,@14,APCTEULD(APCTPTR)                     02S0181
*                 BC(7,LTR0009X);   /* RETRY IF 'CS' FAILED          */
         BC    7,LTR0009X                                       02S0182
*               END;                /* ECB POSTED VIA 'CS'           */
*             RFY                                               02S0184
*              (GPR00P,                                         02S0184
*               GPR01P,                                         02S0184
*               GPR14F,                                         02S0184
*               GPR15F) UNRSTD;     /* DONE WITH REGS                */
@RC00173 DS    0H                                               02S0185
*           END;                    /* POST LOGIC COMPLETE GENERATE A
*                                      DISABLE/UNLOAD SCAN.          */
*           APCTACTR=APCTACTR+1;    /* UPDATE THE ALLOCATION COUNTER.*/
         LA    @14,1                                            02S0186
         AL    @14,APCTACTR(,APCTPTR)                           02S0186
         ST    @14,APCTACTR(,APCTPTR)                           02S0186
*           DO;                     /* IGTYPOST                 02S0187
*                                      ECB(APCTEALP)ASCBLOC(APCTAALP)*/
*             RFY                                               02S0188
*              (GPR00P,                                         02S0188
*               GPR01P,                                         02S0188
*               GPR14F,                                         02S0188
*               GPR15F) RSTD;       /* REGISTERS USED                */
*             GPR15F=APCTEALP;      /* CURRENT ECB CONTENTS          */
         L     GPR15F,APCTEALP(,APCTPTR)                        02S0189
*LTR0011X:                                                      02S0190
*             IF GPR15F<0 THEN      /* IF ECB ALREADY                */
LTR0011X LTR   GPR15F,GPR15F                                    02S0190
         BNM   @RF00190                                         02S0190
*               DO;                 /* WAITED UPON, MUST ISSUE POST  */
*                 GPR01P=ADDR(APCTEALP);/* ADDRESS OF ECB TO POST    */
         LA    GPR01P,APCTEALP(,APCTPTR)                        02S0192
*                 GPR00P=APCTAALP;  /* ADDR OF ASCB FOR MEMORY TO BE
*                                      POSTED                        */
         L     GPR00P,APCTAALP(,APCTPTR)                        02S0193
*                 CALL APCVZUBB;    /* ISSUE CROSS-MEMORY POST       */
         L     @15,APCTZUBB(,APCTPTR)                           02S0194
         BALR  @14,@15                                          02S0194
*               END;                /* ECB POSTED                    */
*             ELSE                  /* ECB NOT YET POSTED            */
*               DO;                 /* SO TRY 'COMPARE-AND-SWAP' POST*/
         B     @RC00190                                         02S0196
@RF00190 DS    0H                                               02S0197
*                 GPR14F='40000000'X;/* POST CODE                    */
         L     GPR14F,@CF01151                                  02S0197
*                 CS(GPR15F,GPR14F,APCTEALP);/* POST THE ECB         */
         CS    GPR15F,@14,APCTEALP(APCTPTR)                     02S0198
*                 BC(7,LTR0011X);   /* RETRY IF 'CS' FAILED          */
         BC    7,LTR0011X                                       02S0199
*               END;                /* ECB POSTED VIA 'CS'           */
*             RFY                                               02S0201
*              (GPR00P,                                         02S0201
*               GPR01P,                                         02S0201
*               GPR14F,                                         02S0201
*               GPR15F) UNRSTD;     /* DONE WITH REGS                */
@RC00190 DS    0H                                               02S0202
*           END;                    /* POST LOGIC COMPLETE TELL 02S0202
*                                      ALLOCATION                    */
*           IF APCQIINF=APCQRIPL THEN/* DETERMINE IPL TYPE FOR MSG.  */
         CLC   APCQIINF(4,APCQPTR),@CF00224                     02S0203
         BNE   @RF00203                                         02S0203
*             TYPE='RE-';                                       02S0204
         MVC   TYPE(3),@CC01156                                 02S0204
*           ELSE                                                02S0205
*             TYPE='';                                          02S0205
         B     @RC00203                                         02S0205
@RF00203 MVI   TYPE,C' '                                        02S0205
         MVC   TYPE+1(2),TYPE                                   02S0205
*           CALL APCVZDWT(IGT222I,  /* OUTPUT SUCCESS MESSAGE.       */
*               14,APCTSSID,4,20,UCBNAME,3,24,TYPE,3);          02S0206
@RC00203 MVC   @AL00001(40),@AL00206                            02S0206
         LA    @14,APCTSSID(,APCTPTR)                           02S0206
         ST    @14,@AL00001+8                                   02S0206
         L     @14,UCBPTR(,APUBPTR)                             02S0206
         LA    @14,UCBNAME(,@14)                                02S0206
         ST    @14,@AL00001+20                                  02S0206
         LA    @14,TYPE                                         02S0206
         ST    @14,@AL00001+32                                  02S0206
         L     @15,APCTZDWT(,APCTPTR)                           02S0206
         LA    @01,@AL00001                                     02S0206
         BALR  @14,@15                                          02S0206
*         END;                                                  02S0207
*       ELSE                                                    02S0208
*         CALL ERRPROC;             /* INVOKE THE IPL ERROR     03S0208
*                                      PROCEDURE.                    */
         B     @RC00130                                         03S0208
@RF00130 BAL   @14,ERRPROC                                      03S0208
*     END;                                                      02S0209
*   RETURN;                                                     01S0210
@EL00001 L     @13,4(,@13)                                      01S0210
@EF00001 L     @00,@SIZDATD                                     01S0210
         LR    @01,@06                                          01S0210
         IGTYFSV  R,LV=(0),A=(1)
@ER00001 LM    @14,@08,12(@13)                                  01S0210
         LM    @10,@12,60(@13)                                  01S0210
         BR    @14                                              01S0210
*ERRPROC:                                                       02S0211
*   PROC;                           /* PROCESS IPL ERRORS.           */
ERRPROC  STM   @14,@12,@SA00002                                 02S0211
*   DO;                                                         02S0212
*     CVD(R15P,DIGITS);             /* CONVERT RETURN CODE TO   02S0213
*                                      DECIMAL.                      */
         CVD   R15P,@TS00001                                    02S0213
         MVC   DIGITS(2),@TS00001+6                             02S0213
*     UNPK(CODE,DIGITS);            /* UNPACK ANSWER.                */
         UNPK  CODE(2),DIGITS(2)                                02S0214
*     CODE(2)=CODE(2)×'F0'X;        /* MAKE IT AN UNSIGNED NUMBER.   */
*                                                               02S0215
         OI    CODE+1,X'F0'                                     02S0215
*     /***************************************************************/
*     /*                                                             */
*     /* ADDRESS OF THE APCT.                                        */
*     /*                                                             */
*     /***************************************************************/
*                                                               02S0216
*     IF APCQIINF=APCQRIPL THEN     /* IF THIS IS A RE-IPL.          */
         CLC   APCQIINF(4,APCQPTR),@CF00224                     02S0216
         BNE   @RF00216                                         02S0216
*       TYPE='RE-';                 /* SAY SO.                       */
         MVC   TYPE(3),@CC01156                                 02S0217
*     ELSE                                                      02S0218
*       TYPE='';                    /* OTHERWISE, BLANK IT OUT.      */
         B     @RC00216                                         02S0218
@RF00216 MVI   TYPE,C' '                                        02S0218
         MVC   TYPE+1(2),TYPE                                   02S0218
*     CALL APCVZDWT(IGT240I,14,APCTSSID,4,20,TYPE,3,30,UCBNAME,3,44,
*         CODE,2);                                              02S0219
@RC00216 MVC   @AL00001(52),@AL00219                            02S0219
         LA    @14,APCTSSID(,APCTPTR)                           02S0219
         ST    @14,@AL00001+8                                   02S0219
         LA    @14,TYPE                                         02S0219
         ST    @14,@AL00001+20                                  02S0219
         L     @14,UCBPTR(,APUBPTR)                             02S0219
         LA    @14,UCBNAME(,@14)                                02S0219
         ST    @14,@AL00001+32                                  02S0219
         LA    @14,CODE                                         02S0219
         ST    @14,@AL00001+44                                  02S0219
         L     @15,APCTZDWT(,APCTPTR)                           02S0219
         LA    @01,@AL00001                                     02S0219
         BALR  @14,@15                                          02S0219
*     CALL APCVZDIO('WAITXXXX');    /* RESET THE 3838 TO UNINIT 02S0220
*                                      STATE.                        */
         L     @15,APCTZDIO(,APCTPTR)                           02S0220
         LA    @01,@AL00220                                     02S0220
         BALR  @14,@15                                          02S0220
*     IF APCQIINF=APCQRIPL THEN     /* IF THIS IS A RE-IPL, THE 3838 */
         CLC   APCQIINF(4,APCQPTR),@CF00224                     02S0221
         BNE   @RF00221                                         02S0221
*       DO;                         /* MUST BE UNLOADED.             */
*         SAVEAPCQ=APCQPTR;         /* SAVE ORIGINAL APCQ            */
         LR    SAVEAPCQ,APCQPTR                                 02S0223
*         CALL APCVZDX1;            /* GET A NEW APCQ.               */
         L     @15,APCTZDX1(,APCTPTR)                           02S0224
         BALR  @14,@15                                          02S0224
*         APCQIINF=APCQUNLD;        /* SET UNLOAD COMMAND.           */
         MVC   APCQIINF(4,APCQPTR),@CF00210                     02S0225
*         APCQTXLN=3;               /* LENGTH OF UNIT NAME.          */
         LA    @14,3                                            02S0226
         STH   @14,APCQTXLN(,APCQPTR)                           02S0226
