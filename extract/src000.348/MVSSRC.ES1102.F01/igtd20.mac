*/********************************************************************/
*/*                                                                  */
*/*      THIS ROUTINE GENERATES A FORCE DUMP COMMAND.                */
*/*                                                                  */
*/********************************************************************/
*                                                               01S0277
*GENRIPL:                                                       01S0277
*   PROC;                                                       01S0277
GENRIPL  STM   @14,@12,@SA00006                                 01S0277
*   RIPLECB(2)='0'B;                /* RESET REIPL ECB.              */
         NI    RIPLECB(APCTPTR),B'10111111'                     01S0278
*   RESPECIFY                                                   01S0279
*     APUB BASED(APUBPTR);                                      01S0279
*   RESPECIFY                                                   01S0280
*     APUBPTR RESTRICTED;                                       01S0280
*   APUBPTR=APCTAPUB;               /* FIRST APUB.                   */
         L     APUBPTR,APCTAPUB(,APCTPTR)                       01S0281
*   DO WHILE APUBPTR^=0;            /* SCAN APUBS FOR DUMP REQUESTS. */
         B     @DE00282                                         01S0282
@DL00282 DS    0H                                               01S0283
*     IF APUBFDMP='1'B              /* IF EITHER TYPE OF DUMP.       */
*         ×APUBPDMP='1'B THEN       /* GENERATE A DUMP APCQ.         */
         TM    APUBFDMP(APUBPTR),B'01100000'                    01S0283
         BZ    @RF00283                                         01S0283
*       DO;                                                     01S0284
*         CALL APCVZDX1;            /* GET AN APCQ.                  */
         L     @15,APCTZDX1(,APCTPTR)                           01S0285
         BALR  @14,@15                                          01S0285
*         RESPECIFY                                             01S0286
*           APCQPTR RESTRICTED;                                 01S0286
*         RESPECIFY                                             01S0287
*           APCQ BASED(APCQPTR);                                01S0287
*         APCQCMND(1:3)=UCBNAME;    /* IDENTIFY UNIT.                */
         L     @11,UCBPTR(,APUBPTR)                             01S0288
         MVC   APCQCMND(3,APCQPTR),UCBNAME(@11)                 01S0288
*         IF APUBFDMP='1'B THEN     /* PUT IN APPROPRIATE COMMAND
*                                      CODE.                         */
         TM    APUBFDMP(APUBPTR),B'01000000'                    01S0289
         BNO   @RF00289                                         01S0289
*           DO;                                                 01S0290
*             APCQCMND(3)=APCQCMND(3)/* ENSURE THAT THE DUMP IS FOR  */
*                 &'F0'X;           /* * SUBCHANNEL 0.               */
         NI    APCQCMND+2(APCQPTR),X'F0'                        01S0291
*             APCQIINF=APCQFDMP;                                01S0292
         MVC   APCQIINF(4,APCQPTR),@CF00387                     01S0292
*           END;                                                01S0293
*         ELSE                                                  01S0294
*           APCQIINF=APCQUDMP;                                  01S0294
         B     @RC00289                                         01S0294
@RF00289 MVC   APCQIINF(4,APCQPTR),@CF00510                     01S0294
*         APUBPDMP='0'B;            /* RESET PORT DUMP REQUEST.      */
@RC00289 DS    0H                                               01S0296
*         APUBFDMP='0'B;            /* RESET FORCE DUMP REQUEST.     */
         L     @11,APUBFDMP(,APUBPTR)                           01S0296
@GL00006 LR    @08,@11                                          01S0296
         N     @08,@CF02246                                     01S0296
         CS    @11,@08,APUBFDMP(APUBPTR)                        01S0296
         BNE   @GL00006                                         01S0296
*         APCQTXLN=3;                                           01S0297
         LA    @11,3                                            01S0297
         STH   @11,APCQTXLN(,APCQPTR)                           01S0297
*         APCQPSTR=1;                                           01S0298
         MVC   APCQPSTR(2,APCQPTR),@CH00055                     01S0298
*         APCQPLEN=3;                                           01S0299
         STH   @11,APCQPLEN(,APCQPTR)                           01S0299
*         APCQINT='1'B;             /* INDICATE INTERNAL FORMAT.     */
         OI    APCQINT(APCQPTR),B'10000000'                     01S0300
*         CALL APCVZDX2;            /* QUEUE THE APCQ.               */
         L     @15,APCTZDX2(,APCTPTR)                           01S0301
         BALR  @14,@15                                          01S0301
*         RESPECIFY                                             01S0302
*           APCQ;                                               01S0302
*         RESPECIFY                                             01S0303
*           APCQPTR UNRESTRICTED;                               01S0303
*       END;                                                    01S0304
*     IF APUBAPUB=0 THEN            /* IF END OF USER PORTS          */
@RF00283 L     @11,APUBAPUB(,APUBPTR)                           01S0305
         LTR   @11,@11                                          01S0305
         BNZ   @RF00305                                         01S0305
*       APUBPTR=                    /* GO TO NEXT MASTER.            */
*           APUBMSTR->APUBMSTR;                                 01S0306
         L     @11,APUBMSTR(,APUBPTR)                           01S0306
         L     @11,APUBMSTR(,@11)                               01S0306
         LR    APUBPTR,@11                                      01S0306
*     ELSE                                                      01S0307
*       APUBPTR=APUBAPUB;           /* OTHERWISE GO TO NEXT USER
*                                      PORT.                         */
         B     @RC00305                                         01S0307
@RF00305 L     APUBPTR,APUBAPUB(,APUBPTR)                       01S0307
*   END;                                                        01S0308
@RC00305 DS    0H                                               01S0308
@DE00282 LTR   APUBPTR,APUBPTR                                  01S0308
         BNZ   @DL00282                                         01S0308
*   RETURN;                                                     01S0309
@EL00006 DS    0H                                               01S0309
@EF00006 DS    0H                                               01S0309
@ER00006 LM    @14,@12,@SA00006                                 01S0309
         BR    @14                                              01S0309
