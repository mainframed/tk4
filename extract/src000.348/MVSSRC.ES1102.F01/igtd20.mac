TD20     TITLE 'IGTD20 - COMMAND INTERFACE ROUTINE.                    *
                        '
IGTD20   CSECT ,                                                01S0001
@MAINENT DS    0H                                               01S0001
         USING *,@15                                            01S0001
         B     @PROLOG                                          01S0001
         DC    AL1(16)                                          01S0001
         DC    C'IGTD20    77.231'                              01S0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                  01S0001
         BALR  @07,0                                            01S0001
@PSTART  DS    0H                                               01S0001
         USING @PSTART,@07                                      01S0001
         L     @00,@SIZDATD                                     01S0001
         IGTYGSV  R,LV=(0)
         LR    @06,@01                                          01S0001
         USING @DATD,@06                                        01S0001
         ST    @13,@SA00001+4                                   01S0001
         LM    @00,@01,20(@13)                                  01S0001
         ST    @06,8(,@13)                                      01S0001
         LR    @13,@06                                          01S0001
         EJECT
*   DO;                             /* ESTABLISH THE ERROR RECOVERY  */
*     RFY                                                       01S0134
*      (R2P,                                                    01S0134
*       R15P) RSTD;                 /* NED THESE REGS.               */
*     D20APCT=APCTPTR;              /* POINTER TO THE APCT.          */
         ST    APCTPTR,D20APCT                                  01S0135
*     D20CODE=20;                   /* IDENTIFY THE ROUTINE.         */
         MVC   D20CODE(2),@CH00390                              01S0136
*     D20FLAGS=''X;                 /* CLEAR THE FLAGS.              */
         XC    D20FLAGS(2),D20FLAGS                             01S0137
*     R2P=APCTZD4E;                 /* ADDRESS OF IGTD4E.            */
         L     R2P,APCTZD4E(,APCTPTR)                           01S0138
*     GEN REFS(DSTAE,SSTAE,R2P);                                01S0139
*                                 /* ISSUE THE STAE OR ESTAE.        */
     MVC   DSTAE,SSTAE               MOVE IN THE SKELETON
     IGTYSTAE (R2P),MF=(E,DSTAE),PARAM=D20STAE ISSUE THE STAE.
*     RFY                                                       01S0140
*      (R2P,                                                    01S0140
*       R15P) UNRSTD;               /* RELEASE THE REGS.             */
*   END;                            /* END OF STAE PROCESSING.       */
*   IF R15P=0 THEN                  /* TEST FOR SUCCESSFUL      01S0142
*                                      STAE/ESTAE.                   */
         LTR   R15P,R15P                                        01S0142
         BNZ   @RF00142                                         01S0142
*     DO;                                                       01S0143
*                                                               01S0143
*       /*************************************************************/
*       /*                                                           */
*       /* ATTACH THE COMMAND PROCESSING ROUTINE.                    */
*       /*                                                           */
*       /*************************************************************/
*                                                               01S0144
*       IGTD40P(1)=APCTPTR;         /* ADDRESS OF THE APCT.          */
         ST    APCTPTR,IGTD40P                                  01S0144
*       IGTD40P(2)=ADDR(APCTZD40);  /* ADDRESS OF IGTD40.            */
         LA    @15,APCTZD40(,APCTPTR)                           01S0145
         ST    @15,IGTD40P+4                                    01S0145
*       RESPECIFY                                               01S0146
*        (R1P,                                                  01S0146
*         R2P,                                                  01S0146
*         R3P) RESTRICTED;                                      01S0146
*       R1P=ADDR(IGTD40P);          /* POINT R1 AT THE PARM LIST.    */
         LA    R1P,IGTD40P                                      01S0147
*       R3P=ADDR(APCTECPT);         /* ADDR OF IGTD40 TERMINATION
*                                      ECB.                          */
         LA    R3P,APCTECPT(,APCTPTR)                           01S0148
*       R2P=CVTLINK;                /* ADDR OF LINK LIB DCB.         */
         L     @15,CVTPTR                                       01S0149
         L     R2P,CVTLINK(,@15)                                01S0149
*       GEN( XC MFLATCH(MFLAL),MFLATCH  CLEAR THE PARM LIST. )  01S0150
*       REFS(MFLATCH);                                          01S0150
          XC MFLATCH(MFLAL),MFLATCH  CLEAR THE PARM LIST.
*       GEN REFS(R1P,R2P,R3P) SETS(MFLATCH)/* ATTACH IGTUEA - IGTD40.*/
*           ( ATTACH  ECB=(R3P),DCB=(R2P),EP=IGTUEA,SF=(E,MFLATCH) );
          ATTACH  ECB=(R3P),DCB=(R2P),EP=IGTUEA,SF=(E,MFLATCH)
*       D40TCB=R1P;                 /* SAVE TCB ADDRESS.             */
         ST    R1P,D40TCB                                       01S0152
         EJECT
*                                                               01S0153
*       /*************************************************************/
*       /*                                                           */
*       /* FIND THE OPERATOR INPUT AREA.                             */
*       /*                                                           */
*       /*************************************************************/
*                                                               01S0153
*       MFLEXT(1:12)=''X;           /* ZERO THE PARM LIST.           */
         XC    MFLEXT(12),MFLEXT                                01S0153
*       GEN SETS(COMMPTR,MFLEXT)    /* LOCATE THE COMMUNICATION AREA.*/
*           ( EXTRACT COMMPTR,'S',FIELDS=(COMM),MF=(E,MFLEXT) );
          EXTRACT COMMPTR,'S',FIELDS=(COMM),MF=(E,MFLEXT)
*       IF COMMCIB^=0 THEN          /* IS A CIB ALREADY ON THE QUEUE?*/
         L     @15,COMMPTR                                      01S0155
         L     @00,COMMCIB(,@15)                                01S0155
         LTR   @00,@00                                          01S0155
         BZ    @RF00155                                         01S0155
*         CALL GETCIB;              /* GO GET A CIB.                 */
         BAL   @14,GETCIB                                       01S0156
*       RESPECIFY                                               01S0157
*         R2P RESTRICTED;                                       01S0157
@RF00155 DS    0H                                               01S0158
*       R2P=ADDR(COMMCIB);                                      01S0158
         L     R2P,COMMPTR                                      01S0158
         LA    R2P,COMMCIB(,R2P)                                01S0158
*       GEN REFS(R2P)               /* ALLOW MODIFY CIBS TO BE  01S0159
*                                      CHAINED.                      */
*           ( QEDIT ORIGIN=(R2P),CIBCTR=10  );                  01S0159
          QEDIT ORIGIN=(R2P),CIBCTR=10
*       RESPECIFY                                               01S0160
*         R2P UNRESTRICTED;                                     01S0160
         EJECT
*                                                               01S0161
*       /*************************************************************/
*       /*                                                           */
*       /* BUILD THE ECB LIST.                                       */
*       /*                                                           */
*       /*************************************************************/
*                                                               01S0161
*       APCTECBL=ADDR(ECBLIST);     /* SET POINTER TO ECB LIST.      */
         LA    @02,ECBLIST                                      01S0161
         ST    @02,APCTECBL(,APCTPTR)                           01S0161
*       ECBLIST(1)=ADDR(COMMECB);   /* POINTER TO COMMUNICATION ECB. */
         L     @02,COMMPTR                                      01S0162
         L     @02,COMMECBP(,@02)                               01S0162
         ST    @02,ECBLIST                                      01S0162
*       ECBLIST(2)=ADDR(APCTEALC);  /* ALLOCATE ENQ COMPLETES ECB.   */
         LA    @02,APCTEALC(,APCTPTR)                           01S0163
         ST    @02,ECBLIST+4                                    01S0163
*       ECBLIST(3)=ADDR(APCTECPT);  /* IGTD40 TERMINATION ECB.       */
         LA    @02,APCTECPT(,APCTPTR)                           01S0164
         ST    @02,ECBLIST+8                                    01S0164
*       ECBLIST(4)=ADDR(APCTEULD);  /* DISABLE/UNLOAD SCAN ECB.      */
         LA    @02,APCTEULD(,APCTPTR)                           01S0165
         ST    @02,ECBLIST+12                                   01S0165
*       ECBLIST(5)=ADDR(APCTEIPL)   /* RE-IPL OR FORCE DUMP REQ'ED
*                                      ECB.                          */
*           ×'80000000'X;           /* SET VLIST INDICATOR.          */
         LA    @02,APCTEIPL(,APCTPTR)                           01S0166
         O     @02,@CF02102                                     01S0166
         ST    @02,ECBLIST+16                                   01S0166
*       RESPECIFY                                               01S0167
*        (R1P,                                                  01S0167
*         R2P,                                                  01S0167
*         R3P) UNRESTRICTED;                                    01S0167
*       DO UNTIL TERMECB(2)='1'B;   /* WHEN IGTD40 TERMINATES, QUIT. */
@DL00168 DS    0H                                               01S0169
*         DO;                       /* IGTYWAIT                 01S0169
*                                      (1)ECBLIST(ECBLIST)LONG(YES)  */
*           RFY                                                 01S0170
*            (GPR00F,                                           01S0170
*             GPR01P) RSTD;         /* REGISTERS USED                */
*           GPR01P=-ADDR(ECBLIST);  /* POINT TO ECB LIST             */
         LA    GPR01P,ECBLIST                                   01S0171
         LCR   GPR01P,GPR01P                                    01S0171
*           GPR00F=1×'80000000'X;   /* WAIT COUNT                    */
         L     GPR00F,@CF02227                                  01S0172
*           SVC(1);                 /* WAIT FOR ECB POSTING          */
         SVC   1                                                01S0173
*           RFY                                                 01S0174
*            (GPR00F,                                           01S0174
*             GPR01P) UNRSTD;       /* FINISHED WITH REGISTERS       */
*         END;                      /* ECB WAIT COMPLETE WAIT FOR ?  */
*         IF SMECB(2)='1'B THEN     /* STOP/MODIFY ECB?              */
         L     @01,COMMPTR                                      01S0176
         L     @01,COMMECBP(,@01)                               01S0176
         TM    SMECB(@01),B'01000000'                           01S0176
         BNO   @RF00176                                         01S0176
*           CALL GETCIB;            /* GO GET THE CIB AND MAKE AN
*                                      APCQ.                         */
         BAL   @14,GETCIB                                       01S0177
*         IF ALLCECB(2)='1'B THEN   /* ALLOCATE ENQ COMPLETE?        */
@RF00176 TM    ALLCECB(APCTPTR),B'01000000'                     01S0178
         BNO   @RF00178                                         01S0178
*           CALL GENHALT;           /* GO GENERATE A HALT COMMAND.   */
         BAL   @14,GENHALT                                      01S0179
*         IF SCANECB(2)='1'B THEN   /* DISABLE/UNLOAD SCAN ECB?      */
@RF00178 TM    SCANECB(APCTPTR),B'01000000'                     01S0180
         BNO   @RF00180                                         01S0180
*           CALL GENSCAN;           /* GO GENERATE A SCAN COMMAND.   */
         BAL   @14,GENSCAN                                      01S0181
*         IF RIPLECB(2)='1'B THEN   /* RE-IPL REQUIRED ECB?          */
@RF00180 TM    RIPLECB(APCTPTR),B'01000000'                     01S0182
         BNO   @RF00182                                         01S0182
*           CALL GENRIPL;           /* GO GENERATE A RE-IPL COMMAND. */
         BAL   @14,GENRIPL                                      01S0183
*       END;                                                    01S0184
@RF00182 DS    0H                                               01S0184
@DE00168 TM    TERMECB(APCTPTR),B'01000000'                     01S0184
         BNO   @DL00168                                         01S0184
         EJECT
*       GEN REFS(D40TCB)                                        01S0185
*           ( DETACH D40TCB           ELIMINATE IGTD40 TCB. );  01S0185
          DETACH D40TCB           ELIMINATE IGTD40 TCB.
*       APCTACT='0'B;               /* RESET MONITOR ACTIVE     01S0186
*                                      INDICATOR.                    */
         L     @15,APCTACT(,APCTPTR)                            01S0186
@GL00001 LR    @14,@15                                          01S0186
         N     @14,@CF02230                                     01S0186
         CS    @15,@14,APCTACT(APCTPTR)                         01S0186
         BNE   @GL00001                                         01S0186
*       MFENQ(1:4)=STATDEQ;         /* MOVE IN ENQ FLAGS.            */
         MVC   MFENQ(4),STATDEQ                                 01S0187
*       GEN REFS(APCTENQM,ALLOCATE) /* DEQUEUE FROM THE ALLOCATE     */
*           SETS(MFENQ)             /* RESOURCE SO ALLOCATION WILL
*                                      KNOW THAT THE MONITOR IS NOT
*                                      ACTIVE.                       */
*           ;                                                   01S0188
       DEQ   (APCTENQM-APCT(,APCTPTR),ALLOCATE,8,SYSTEM), DEQ FROM     X
               RET=HAVE,    PREVENT ABEND ON EXCEPTIONAL CASES @ZA25359*
               MF=(E,MFENQ)       /* THE RESOURCE.                   */
*       APCTTERM='0'B;              /* RESET THE TERMINATION    01S0189
*                                      INDICATOR.                    */
         L     @15,APCTTERM(,APCTPTR)                           01S0189
@GL00002 LR    @14,@15                                          01S0189
         N     @14,@CF02233                                     01S0189
         CS    @15,@14,APCTTERM(APCTPTR)                        01S0189
         BNE   @GL00002                                         01S0189
*       GEN(IGTYSTAE 0);            /* CANCEL THE ESTAE.             */
         IGTYSTAE 0
*     END;                                                      01S0191
*   RETURN CODE(D40CODE);           /* RETURN TO CALLER BUT LET HIM
*                                      KNOW HOW IGTD40 TERMINATED.   */
@RF00142 L     @11,D40CODE-1(,APCTPTR)                          01S0192
         LA    @11,0(,@11)                                      01S0192
         L     @13,4(,@13)                                      01S0192
         L     @00,@SIZDATD                                     01S0192
         LR    @01,@06                                          01S0192
         IGTYFSV  R,LV=(0),A=(1)
         LR    @15,@11                                          01S0192
         L     @14,12(,@13)                                     01S0192
         LM    @00,@12,20(@13)                                  01S0192
         BR    @14                                              01S0192
         EJECT
*                                                               01S0193
*/********************************************************************/
*/*                                                                  */
*/*      THIS ROUTINE GENERATES A HALT COMMAND                       */
*/*                                                                  */
*/********************************************************************/
*                                                               01S0193
*GENHALT:                                                       01S0193
*   PROC;                                                       01S0193
GENHALT  STM   @14,@12,@SA00002                                 01S0193
*   ALLCECB(2)='0'B;                /* RESET ALLOCATE ENQ ECB.       */
         NI    ALLCECB(APCTPTR),B'10111111'                     01S0194
*   CALL CLEANUP;                   /* INSURE 3838 UNLOAD    @ZA25359
*                                      WORKS                 @ZA25359*/
         BAL   @14,CLEANUP                                      01S0195
*   CALL APCVZDX1;                  /* GET AN APCQ.                  */
         L     @15,APCTZDX1(,APCTPTR)                           01S0196
         BALR  @14,@15                                          01S0196
*   RESPECIFY                                                   01S0197
*     APCQPTR RESTRICTED;                                       01S0197
*   RESPECIFY                                                   01S0198
*     APCQ BASED(APCQPTR);                                      01S0198
*   APCQIINF=APCQHALT;              /* INDICATE A HALT COMMAND.      */
         MVC   APCQIINF(4,APCQPTR),@CF00045                     01S0199
*   APCQINT='1'B;                   /* INDICATE INTERNALLY GENERATED */
         OI    APCQINT(APCQPTR),B'10000000'                     01S0200
*   CALL APCVZDX2;                  /* QUEUE THE APCQ.               */
         L     @15,APCTZDX2(,APCTPTR)                           01S0201
         BALR  @14,@15                                          01S0201
*   RESPECIFY                                                   01S0202
*     APCQPTR UNRESTRICTED;                                     01S0202
*   RESPECIFY                                                   01S0203
*     APCQ;                                                     01S0203
*   RETURN;                                                     01S0204
@EL00002 DS    0H                                               01S0204
@EF00002 DS    0H                                               01S0204
@ER00002 LM    @14,@12,@SA00002                                 01S0204
         BR    @14                                              01S0204
*   END;                                                        01S0205
         EJECT
*CLEANUP:                                                       01S0206
*   PROC;                           /*                       @ZA25359*/
*                                                               01S0206
CLEANUP  STM   @14,@12,@SA00003                                 01S0206
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS ROUTINE INSURES THAT NO USERS APPEAR TO BE       @ZA25359*/
*   /* ALLOCATED 3838 RESOURCES AND THEREBY PREVENT          @ZA25359*/
*   /* UNLOADING OF THE 3838.                                @ZA25359*/
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0207
*   CALL APCVZDEQ;                  /* SERIALIZE ACCESS TO   @ZA25359
*                                      APCT,APUBS, ETC.      @ZA25359*/
         L     @15,APCTZDEQ(,APCTPTR)                           01S0207
         BALR  @14,@15                                          01S0207
*                                                               01S0208
*/* SET APUBOWN TO INDICATE NO APUB EXCLUSIVELY ALLOCATED.   @ZA25359*/
*                                                               01S0208
*   MSTRAPUB=APCTAPUB;              /* MSTRAPUB POINTS TO FIRST MASTR
*                                                            @ZA25359*/
         L     MSTRAPUB,APCTAPUB(,APCTPTR)                      01S0208
*   RFY                                                         01S0209
*     APUBPTR RSTD;                 /* NEED REGISTER         @ZA25359*/
*   RFY                                                         01S0210
*     APUB BASED(APUBPTR);          /* PROVIDE BASING EXPRESSION
*                                                            @ZA25359*/
*   DO WHILE MSTRAPUB^=0;           /* DO FOR EACH MASTER APUB  01S0211
*                                                            @ZA25359*/
         B     @DE00211                                         01S0211
@DL00211 DS    0H                                               01S0212
*     APUBPTR=MSTRAPUB->APUBAPUB;   /* FIRST NON-CONTROL PORT APUB
*                                                            @ZA25359*/
         L     APUBPTR,APUBAPUB(,MSTRAPUB)                      01S0212
*     DO UNTIL APUBPTR=0;           /* DO ONCE FOR EACH NON-CNTL PORT
*                                                            @ZA25359*/
@DL00213 DS    0H                                               01S0214
*       APUBOWN=SHR;                /* INDICATE PORT NOT EXC ALLOC8D
*                                                            @ZA25359*/
         L     @11,APUBOWN(,APUBPTR)                            01S0214
@GL00003 LR    @03,@11                                          01S0214
         N     @03,@CF02238                                     01S0214
         CS    @11,@03,APUBOWN(APUBPTR)                         01S0214
         BNE   @GL00003                                         01S0214
*       APUBPTR=APUBAPUB;           /* ON TO NEXT APUB OR DONE W/3838
*                                                            @ZA25359*/
         L     APUBPTR,APUBAPUB(,APUBPTR)                       01S0215
*     END;                          /* DO UNTIL APUBPTR = 0  @ZA25359*/
@DE00213 LTR   APUBPTR,APUBPTR                                  01S0216
         BNZ   @DL00213                                         01S0216
*     MSTRAPUB=MSTRAPUB->APUBMSTR;  /* POINT TO NEXT MASTER OR DONE
*                                                            @ZA25359*/
         L     MSTRAPUB,APUBMSTR(,MSTRAPUB)                     01S0217
*   END;                            /* DO WHILE MSTRAPUB ^= 0@ZA25359*/
@DE00211 LTR   MSTRAPUB,MSTRAPUB                                01S0218
         BNZ   @DL00211                                         01S0218
*   RFY                                                         01S0219
*     APUBPTR UNRSTD;               /* NO LONGER NEED APUBPTR@ZA25359*/
*   RFY                                                         01S0220
*     APUB;                         /* REVERT                @ZA25359*/
*   APCTAPDS='';                    /* NO USERS OF VPSS RESOURCES
*                                                            @ZA25359*/
         SLR   @15,@15                                          01S0221
         ST    @15,APCTAPDS(,APCTPTR)                           01S0221
*   APCTDSAP='';                    /* DITTO                 @ZA25359*/
         ST    @15,APCTDSAP(,APCTPTR)                           01S0222
*   CALL APCVZDDQ;                  /* END SERIALIZATION ON APCT, ETC
*                                                            @ZA25359*/
         L     @15,APCTZDDQ(,APCTPTR)                           01S0223
         BALR  @14,@15                                          01S0223
*   END CLEANUP;                    /* END OF INTERNAL SUBROUTINE
*                                                            @ZA25359*/
@EL00003 DS    0H                                               01S0224
@EF00003 DS    0H                                               01S0224
@ER00003 LM    @14,@12,@SA00003                                 01S0224
         BR    @14                                              01S0224
         EJECT
*                                                               01S0225
*/********************************************************************
*                                                                    */
*/*                                                                  */
*/*       THIS ROUTINE GENERATES A DISABLE/UNLOAD SCAN COMMAND.      */
*/*                                                                  */
*/********************************************************************
*                                                                    */
*                                                               01S0225
*GENSCAN:                                                       01S0225
*   PROC;                                                       01S0225
GENSCAN  STM   @14,@12,@SA00004                                 01S0225
*   SCANECB(2)='0'B;                /* RESET THE ECB.                */
         NI    SCANECB(APCTPTR),B'10111111'                     01S0226
*   CALL APCVZDX1;                  /* GET AN APCQ.                  */
         L     @15,APCTZDX1(,APCTPTR)                           01S0227
         BALR  @14,@15                                          01S0227
*   RESPECIFY                                                   01S0228
*     APCQPTR RESTRICTED;                                       01S0228
*   RESPECIFY                                                   01S0229
*     APCQ BASED(APCQPTR);                                      01S0229
*   APCQIINF=APCQDULS;              /* INDICATE DISABLE/UNLOAD SCAN. */
         MVC   APCQIINF(4,APCQPTR),@CF00503                     01S0230
*   APCQINT='1'B;                   /* INDICATE INTERNAL COMMAND.    */
         OI    APCQINT(APCQPTR),B'10000000'                     01S0231
*   CALL APCVZDX2;                  /* QUEUE THE APCQ.               */
         L     @15,APCTZDX2(,APCTPTR)                           01S0232
         BALR  @14,@15                                          01S0232
*   RESPECIFY                                                   01S0233
*     APCQPTR UNRESTRICTED;                                     01S0233
*   RESPECIFY                                                   01S0234
*     APCQ;                                                     01S0234
*   RETURN;                                                     01S0235
@EL00004 DS    0H                                               01S0235
@EF00004 DS    0H                                               01S0235
@ER00004 LM    @14,@12,@SA00004                                 01S0235
         BR    @14                                              01S0235
*   END;                                                        01S0236
         EJECT
*                                                               01S0237
*/********************************************************************/
*/*                                                                  */
*/*    THIS ROUTINE PROCESSES COMMUNICATION INTERFACE BLOCKS THAT    */
*/*  HAVE BEEN QUEUED FOR IT.  THERE ARE THREE TYPES OF CIBS:        */
*/*       1. START COMMAND, WHICH IS DISCARDED.                      */
*/*       2. MODIFY COMMANDS, WHICH ARE CONVERTED INTO APCQS FOR THE */
*/*          COMMAND PROCESSOR TASK.                                 */
*/*       3. STOP COMMANDS, WHICH WILL CAUSE THE MONITOR TO TERMINATE*/
*/*          WHEN ALL USERS HAVE TERMINATED.                         */
*/*                                                                  */
*/********************************************************************/
*                                                               01S0237
*GETCIB:                                                        01S0237
*   PROC;                                                       01S0237
GETCIB   STM   @14,@12,@SA00005                                 01S0237
*   DCL                                                         01S0238
*     CIBPTR PTR(31) DEF(COMMCIB);  /* PTR TO THE CIB.               */
*/*   * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *  */
*/*                                                                  */
*/*      COMMAND INPUT BUFFER MAPPING MACRO                          */
*/*                                                                  */
*/*  OS/VS2 RELEASE 2, OS/VS1 RELEASE 3, 09/21/72, LEVEL=1           */
*/*                                                                  */
*/*  METHOD OF ACCESS                                                */
*/*      BAL  - A DSECT CARD SHOULD PRECEDE MACRO CALL.  USING ON    */
*/*             CIBNEXT GIVES ADDRESSABILITY FOR ALL SYMBOLS.        */
*/*      PL/S - DCL CIBPTR PTR                                       */
*/*   * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *  */
*                                                               01S0239
*   DECLARE                                                     01S0239
*     1 CIBNAME BASED(CIBPTR) BDY(WORD),                        01S0239
*      2 CIBNEXT PTR(31),           /* ADDRESS OF NEXT CIB IN QUEUE
*                                      (ZERO FOR LAST)               */
*      2 CIBVERB CHAR(1),           /* COMMAND VERB CODE             */
*      2 CIBLEN PTR(8),             /* LENGTH IN DOUBLEWORDS OF CIB
*                                      INCLUDING CIBDATA             */
*      2 * CHAR(4) BDY(WORD,3),     /* RESERVED FOR CSCB        01S0239
*                                      COMPATIBILITY                 */
*      2 CIBASID FIXED(15),         /* ADDRESS SPACE ID (OS/VS2)
*                                      MDC001                        */
*       3 CIBTJID CHAR(2),          /* TSO TERMINAL JOB IDENTIFIER
*                                      (OS/VS1) MDC001               */
*      2 CIBCONID CHAR(1),          /* IDENTIFIER OF CONSOLE ISSUING
*                                      COMMAND                       */
*      2 * CHAR(1),                 /* RESERVED                      */
*      2 CIBDATLN FIXED(15),        /* LENGTH IN BYTES OF DATA IN
*                                      CIBDATA                       */
*      2 CIBDATA CHAR(8) BDY(WORD); /* DATA FROM COMMAND OPERAND     */
*/*      (LENGTH OF CIBDATA IS A MULTIPLE OF EIGHT BYTES             */
*/*      DEPENDING ON THE VALUE CONTAINED IN CIBLEN)                 */
*/*            START -  FOURTH POSITIONAL PARAMETER (PARMVALUE)      */
*/*            MODIFY - RESIDUAL OPERAND IMAGE FOLLOWING COMMA       */
*/*                     TERMINATING FIRST POSITIONAL PARAMETER       */
*/*            STOP -   NONE (CIB GENERATED ONLY TO GIVE CONSOLE ID) */
*/*   * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *  */
*                                                               01S0240
*   DO UNTIL CIBPTR=0;              /* PROCESS ALL CIBS ON THE QUEUE.*/
@DL00240 DS    0H                                               01S0241
*     IF CIBVERB='40'X THEN         /* IS THIS A STOP COMMAND?       */
         L     @01,COMMPTR                                      01S0241
         L     @01,CIBPTR(,@01)                                 01S0241
         CLI   CIBVERB(@01),X'40'                               01S0241
         BNE   @RF00241                                         01S0241
*       DO;                         /* PROCESS STOP COMMAND.         */
*         CALL APCVZDWT(IGT255I,14,APCTSSID,4);/* ACKNOWLEDGE STOP.  */
         MVC   @AL00001(16),@AL00243                            01S0243
         LA    @15,APCTSSID(,APCTPTR)                           01S0243
         ST    @15,@AL00001+8                                   01S0243
         L     @15,APCTZDWT(,APCTPTR)                           01S0243
         LA    @01,@AL00001                                     01S0243
         BALR  @14,@15                                          01S0243
*         RESPECIFY                                             01S0244
*           R2P RESTRICTED;                                     01S0244
*         R2P=ADDR(COMMCIB);                                    01S0245
         L     R2P,COMMPTR                                      01S0245
         LA    R2P,COMMCIB(,R2P)                                01S0245
*         GEN REFS(R2P)             /* ALLOW MODIFY CIBS TO BE  01S0246
*                                      CHAINED.                      */
*             ( QEDIT ORIGIN=(R2P),CIBCTR=10  );                01S0246
          QEDIT ORIGIN=(R2P),CIBCTR=10
*         RESPECIFY                                             01S0247
*           R2P UNRESTRICTED;                                   01S0247
*         IF APCTTERM='0'B THEN     /* SEE IF WE ARE ALREADY    01S0248
*                                      TERMINATING, IF SO WE SHOULD
*                                      BYPASS THE ENQ.               */
         TM    APCTTERM(APCTPTR),B'00001000'                    01S0248
         BNZ   @RF00248                                         01S0248
*           DO;                     /* DO ONLY ONCE.                 */
*             APCTTERM='1'B;        /* INDICATE TERMINATION     01S0250
*                                      NECESSARY.                    */
         L     @15,APCTTERM(,APCTPTR)                           01S0250
@GL00004 LR    @14,@15                                          01S0250
         O     @14,@CF02241                                     01S0250
         CS    @15,@14,APCTTERM(APCTPTR)                        01S0250
         BNE   @GL00004                                         01S0250
*             MFENQ(1:4)=STATENQ;   /* MOVE IN ENQ FLAGS.            */
         MVC   MFENQ(4),STATENQ                                 01S0251
*             GEN SETS(MFENQ,APCTEALC)/* GET THE ALLOCATE RESOURCE SO*/
*                 REFS(APCTENQM,ALLOCATE);                      01S0252
*                                 /* ALLOCATION WILL STOP ALLOCATING.*/
     ENQ   (APCTENQM-APCT(,APCTPTR),ALLOCATE,E,8,SYSTEM), /*ENQ ON   */X
               ECB=APCTEALC-APCT(,APCTPTR),MF=(E,MFENQ) /*  ALLOCATE */
*             IF R15P=0×            /* IF GOT IT RIGHT AWAY, OR      */
*                 ENQRC='08'X THEN  /* ALREADY HAD IT, MUST DUMMY
*                                      POST                          */
         LTR   R15P,R15P                                        01S0253
         BZ    @RT00253                                         01S0253
         CLI   ENQRC(R15P),8                                    01S0253
         BNE   @RF00253                                         01S0253
@RT00253 DS    0H                                               01S0254
*               ALLCECB(1:8)='01000000'B;/* FAKE POST.               */
         MVI   ALLCECB(APCTPTR),B'01000000'                     01S0254
*           END;                    /* END OF ONE TIME PROCESSING.   */
*       END;                                                    01S0256
*     ELSE                                                      01S0257
*       IF CIBVERB='44'X THEN       /* IS THIS A MODIFY COMMAND?     */
         B     @RC00241                                         01S0257
@RF00241 L     @01,COMMPTR                                      01S0257
         L     @01,CIBPTR(,@01)                                 01S0257
         CLI   CIBVERB(@01),X'44'                               01S0257
         BNE   @RF00257                                         01S0257
*         DO;                                                   01S0258
*           CALL APCVZDX1;          /* GET AN APCQ.                  */
         L     @15,APCTZDX1(,APCTPTR)                           01S0259
         BALR  @14,@15                                          01S0259
*           RESPECIFY                                           01S0260
*             APCQPTR RESTRICTED;                               01S0260
*           RESPECIFY                                           01S0261
*             APCQ BASED(APCQPTR);                              01S0261
*           APCQCMND(1:CIBDATLN)=CIBDATA(1:CIBDATLN);/* MOVE IN 01S0262
*                                      COMMAND.                      */
         L     @08,COMMPTR                                      01S0262
         L     @08,CIBPTR(,@08)                                 01S0262
         LH    @15,CIBDATLN(,@08)                               01S0262
         LR    @14,@15                                          01S0262
         BCTR  @14,0                                            01S0262
         EX    @14,@SM02244                                     01S0262
*           APCQTXLN=CIBDATLN;      /* LENGTH OF COMMAND.            */
         STH   @15,APCQTXLN(,APCQPTR)                           01S0263
*           APCQSCID=CIBCONID;      /* INSERT SOURCE CONSOLE ID.     */
         IC    @15,CIBCONID(,@08)                               01S0264
         STC   @15,APCQSCID(,APCQPTR)                           01S0264
*           CALL APCVZDX2;          /* QUEUE THE APCQ.               */
         L     @15,APCTZDX2(,APCTPTR)                           01S0265
         BALR  @14,@15                                          01S0265
*           RESPECIFY                                           01S0266
*             APCQPTR UNRESTRICTED;                             01S0266
*           RESPECIFY                                           01S0267
*             APCQ;                                             01S0267
*         END;                                                  01S0268
*     RESPECIFY                                                 01S0269
*      (R3P,                                                    01S0269
*       R2P) RESTRICTED;                                        01S0269
@RF00257 DS    0H                                               01S0269
@RC00241 DS    0H                                               01S0270
*     R3P=CIBPTR;                   /* POINTER TO THE CIB.           */
         L     @15,COMMPTR                                      01S0270
         L     R3P,CIBPTR(,@15)                                 01S0270
*     R2P=ADDR(CIBPTR);             /* POINTER TO QUEUE HEAD.        */
         LA    R2P,CIBPTR(,@15)                                 01S0271
*     GEN REFS(R2P,R3P)             /* FREE THE CIB                  */
*         ( QEDIT ORIGIN=(R2P),BLOCK=(R3P)   );                 01S0272
          QEDIT ORIGIN=(R2P),BLOCK=(R3P)
*     RESPECIFY                                                 01S0273
*      (R2P,                                                    01S0273
*       R3P) UNRESTRICTED;                                      01S0273
*   END;                                                        01S0274
@DE00240 L     @01,COMMPTR                                      01S0274
         L     @15,CIBPTR(,@01)                                 01S0274
         LTR   @15,@15                                          01S0274
         BNZ   @DL00240                                         01S0274
*   RETURN;                                                     01S0275
@EL00005 DS    0H                                               01S0275
@EF00005 DS    0H                                               01S0275
@ER00005 LM    @14,@12,@SA00005                                 01S0275
         BR    @14                                              01S0275
*   END;                                                        01S0276
         EJECT
