***********************************************************************
* FREE LOCAL LOCK OBTAINED BY SVC SECOND LEVEL INTERRUPT HANDLER      *
***********************************************************************
         SETLOCK RELEASE,TYPE=LOCAL,RELATED=('SVC DUMP')       @YM00084
         LR    R11,R0          SAVE BASE OVER GETMAIN
         GETMAIN R,LV=@ENDDATD-@DATD,SP=253
         LR    AUTOPTR,PARMPTR       SAVE ADDR OF AUTOMATIC STORAGE
         USING @DATD,AUTOPTR      ESTABLISH BASE FOR AUTOMATIC STORAGE
         XC    0(@ENDDATD-@DATD,AUTOPTR),0(AUTOPTR)   0 STORAGE
*   RFY                                                            0213
*    (R5) RSTD;                                                    0213
*   RBSECPTR=R5;                    /* SAVE SNAPS RB ADDRESS @G33SPHW*/
         ST    R5,RBSECPTR                                         0214
*   RFY                                                            0215
*    (R5) UNRSTD;                                                  0215
*   RFY                                                            0216
*     R0 UNRSTD;                                                   0216
         EJECT
*                                                                  0217
*   /*****************************************************************/
*   /*                                                               */
*   /* OBTAIN STORAGE FROM SP253 FOR THE FOLLOWING : SNAP WORK AREA, */
*   /* PLUS 72 BYTES FOR REGISTER SAVE AREA FOR PRINT ROUTINE        */
*   /* (IEAVAD11)                                                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0217
*   RFY                                                            0217
*     R0 RSTD;                                                     0217
*   R0=LENGTH(COREAREA);            /* AMT OF CORE TO OBTAIN @Z40MPXG*/
         LA    R0,1140                                             0218
*   PARMPTR=ADDR(GMPARM2);          /* POINTER TO PARMLIST FOR     0219
*                                      GETMAIN                       */
         L     @10,RBSECPTR                                        0219
         LA    PARMPTR,GMPARM2(,@10)                               0219
*   RFY                                                            0220
*     R5 RSTD;                                                     0220
*   R5=RBSECPTR;                                                   0221
         LR    R5,@10                                              0221
*   GENERATE SETS(GMREPLY2) REFS(R5);                              0222
         GETMAIN EC,LV=(0),SP=253,A=GMREPLY2(R5),MF=(E,(1))
*   RFY                                                            0223
*     R5 UNRSTD;                                                   0223
*   IF RETCODE=0 THEN                                              0224
         LTR   RETCODE,RETCODE                                     0224
         BNZ   @RF00224                                            0224
*     DO;                                                          0225
*       SAVERET=0;                  /* INDICATE SUCCESSFUL GETMAIN   */
         SLR   SAVERET,SAVERET                                     0226
*       ABDARPTR=GMREPLY2;          /* SET BASE REGISTER FOR SNAPS 0227
*                                      WORK AREA                     */
         L     @10,RBSECPTR                                        0227
         L     ABDARPTR,GMREPLY2(,@10)                             0227
*       RFY                                                        0228
*        (COREAREA) BASED(ABDARPTR);/* SET BASE FOR GETM AREA      0228
*                                                            @Z40MPXG*/
*                                                                  0228
*       /*************************************************************/
*       /*                                                           */
*       /* ZERO CORE OBTAINED FOR SNAP'S WORK AREA, ESTAE PARAMETER  */
*       /* LIST, SAVE AREA FOR PRINT ROUTINE, QMNGRIO WORKAREA,      */
*       /*                                                   @Z40MPXG*/
*       /* BUFFER, AND PARAMETER LIST.                       @Z40MPXG*/
*       /*                                                           */
*       /*************************************************************/
*                                                                  0229
*       COREAREA(1:256)=''B;        /* CLEAR GOTTEN AREA     @Z40MPXG*/
         XC    COREAREA(256,ABDARPTR),COREAREA(ABDARPTR)           0229
*       COREAREA(257:512)=''B;      /*                       @Z40MPXG*/
         XC    COREAREA+256(256,ABDARPTR),COREAREA+256(ABDARPTR)   0230
*       COREAREA(513:768)=''B;      /*                       @Z40MPXG*/
         XC    COREAREA+512(256,ABDARPTR),COREAREA+512(ABDARPTR)   0231
*       COREAREA(769:1024)=''B;     /*                       @G33SPHW*/
         XC    COREAREA+768(256,ABDARPTR),COREAREA+768(ABDARPTR)   0232
*       COREAREA(1025:LENGTH(COREAREA))=''B;/*               @G33SPHW*/
*                                                                  0233
         XC    COREAREA+1024(116,ABDARPTR),COREAREA+1024(ABDARPTR) 0233
*       /*************************************************************/
*       /*                                                           */
*       /* INITIALIZE THE QMNGRIO PARAMETER LIST             @Z40MPXG*/
*       /*                                                           */
*       /*************************************************************/
*                                                                  0234
*       QMIOPPTR=ADDR(QMIPARM);     /* ADDR OF PARMLIST      @Z40MPXG*/
         LA    @05,QMIPARM(,ABDARPTR)                              0234
         ST    @05,QMIOPPTR                                        0234
*       QMIRECAD=ADDR(JFCBAREA);    /* PTR TO BUFFER FOR JFCB      0235
*                                                            @Z40MPXG*/
         LA    @10,JFCBAREA(,ABDARPTR)                             0235
         ST    @10,QMIRECAD(,@05)                                  0235
*       QMIWRKAD=ADDR(QMIAREA);     /* PTR TO WORKAREA       @Z40MPXG*/
         LA    @10,QMIAREA(,ABDARPTR)                              0236
         ST    @10,QMIWRKAD(,@05)                                  0236
*       QMIRECL=LENGTH(JFCBAREA);   /* LENGTH OF BUFFER      @Z40MPXG*/
         MVC   QMIRECL(2,@05),@CH04238                             0237
*       QMITCBAD=TCBPTR;            /* CURRENT TCB ADDRESS   @Z40MPXG*/
         ST    TCBPTR,QMITCBAD(,@05)                               0238
*       QMIOREAD=ON;                /* TURN ON READ ONLY BIT @Z40MPXG*/
*                                                                  0239
         OI    QMIOREAD(@05),B'10000000'                           0239
*       /*************************************************************/
*       /*                                                           */
*       /* INITIALIZE THE ABDAREA                            @Z40MPXG*/
*       /*                                                           */
*       /*************************************************************/
*                                                                  0240
*       ABDPARA=SNAPPTR;            /* SAVE ADDRESS OF SNAP PARMLIST */
         ST    SNAPPTR,ABDPARA(,ABDARPTR)                          0240
*       ABDCTCB=TCBPTR;             /* SAVE ADDRESS OF CURRENT TCB   */
         ST    TCBPTR,ABDCTCB(,ABDARPTR)                           0241
*       ABDCRB=RBSECPTR;            /* SAVE ADDRESS OF SNAP'S SVRB   */
         L     @10,RBSECPTR                                        0242
         ST    @10,ABDCRB(,ABDARPTR)                               0242
*       ABDSVCB=R11;                /* SAVE BASE OF SVC 51           */
         ST    R11,ABDSVCB(,ABDARPTR)                              0243
*       ABDRTRN=R3;                 /* SAVE ADDRESS OF SVC 51        */
         ST    R3,ABDRTRN(,ABDARPTR)                               0244
*       SAVREG=ADDR(SAVAREA);       /* GET ADDRESS OF SAVE AREA    0245
*                                                            @Z40MPXG*/
         LA    SAVREG,SAVAREA(,ABDARPTR)                           0245
*       ABDSAVE1=ADDR(SAVAREA1);    /* GET ADDRESS OF AD11 SAVEAREA
*                                      USED TO SAVE REGISTERS PRIOR
*                                      TO SYNCH TO WRITE     @ZA16374*/
         LA    @10,SAVAREA1(,ABDARPTR)                             0246
         ST    @10,ABDSAVE1(,ABDARPTR)                             0246
*       EMODNAME='IEAVAD01';        /* PUT MODNAME IN ESATE PARMLIST */
         MVC   EMODNAME(8,ABDARPTR),@CC04285                       0247
*       ESBSREG=BASE;               /* SAVE BASE REG                 */
         ST    BASE,ESBSREG(,ABDARPTR)                             0248
*       ESBSREG2=BASE2;             /* SAVE SECOND BASE REG  @G33SPHW*/
         ST    BASE2,ESBSREG2(,ABDARPTR)                           0249
*       ESAUTOP=AUTOPTR;            /* SAVE ADDR OF COMPILER STORAGE */
         ST    AUTOPTR,ESAUTOP(,ABDARPTR)                          0250
*       EABDAREA=ABDARPTR;          /* SAVE ADDR OF SNAP WORK AREA   */
         ST    ABDARPTR,EABDAREA(,ABDARPTR)                        0251
*       RESPECIFY                                                  0252
*        (R3,                                                      0252
*         R9) UNRESTRICTED;                                        0252
*       R11=ADDR(ESPARM);           /* ADDR ESTAE PARM LIST          */
         LA    R11,ESPARM(,ABDARPTR)                               0253
*       MFLESTAE=ESTAELST;          /* MOVE MF-L FORM TO GM STORAGE. */
         MVC   MFLESTAE(16),ESTAELST                               0254
*       PARMPTR=ADDR(MFLESTAE);     /* ADDR OF LIST FOR MF-E FORM. 0255
*                                                            @YM03533*/
         LA    PARMPTR,MFLESTAE                                    0255
*       GENERATE;                                                  0256
         ESTAE SNPRCVR,PARAM=(11),MF=(E,(1))
*     END;                                                         0257
*   ELSE                                                           0258
*     DO;                                                          0258
         B     @RC00224                                            0258
@RF00224 DS    0H                                                  0259
*       GEN(FREEMAIN R,LV=@ENDDATD-@DATD,SP=253,A=(AUTOPTR));      0259
         FREEMAIN R,LV=@ENDDATD-@DATD,SP=253,A=(AUTOPTR)
*       SAVERET=8;                  /* INDICATE GETMAIN UNSUCCESSFUL */
         LA    SAVERET,8                                           0260
*       R8=R11;                     /* RESTORE SVC 51 BASE REG       */
         LR    R8,R11                                              0261
*     END;                                                         0262
*   RFY                                                            0263
*    (R0,                                                          0263
*     R11) UNRSTD;                                                 0263
@RC00224 DS    0H                                                  0264
*   IF SAVERET=0                    /* GETMAIN SUCCESSFUL            */
*     THEN                          /* YES                           */
         LTR   SAVERET,SAVERET                                     0264
         BNZ   @RF00264                                            0264
*     DO;                                                          0265
*       TCBFX='1'B;                 /* ASYNCHRONOUS EXITS ARE NOT  0266
*                                      PERMITTED DURING LIFE OF DUMP */
         OI    TCBFX(TCBPTR),B'00000001'                           0266
         EJECT
*                                                                  0267
*       /*************************************************************/
*       /*                                                           */
*       /* DETERMINE CALLER OF SNAP. IF ABDUMP IS THE CALLER, MODESET*/
*       /* IS NOT ISSUED BEFORE MOVING THE SNAP PARAMETER LIST INTO  */
*       /* SNAP'S WORK AREA. OTHERWISE, MODESET IS ISSUED TO INSURE  */
*       /* THAT THE PARAMETER LIST IS ADDRESSABLE BY THE REQUESTOR OF*/
*       /* THE DUMP. AFTER THE PARAMETER LIST IS MOVED, RETURN TO    */
*       /* SUPERVISOR KEY                                            */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0267
*       IF SNPVS2='1'B              /* VS2-1 OR VS2-2 SNAP PARM    0267
*                                      LIST..                        */
*         THEN                      /* --LIST TYPE IS VS2-2.         */
         TM    SNPVS2(SNAPPTR),B'01000000'                         0267
         BNO   @RF00267                                            0267
*         DO;                                                      0268
*           IF SNPABEND='1'B        /* VS2-2 ABEND OR USER CALLING...*/
*             THEN                                                 0269
         TM    SNPABEND(SNAPPTR),B'00010000'                       0269
         BNO   @RF00269                                            0269
*             APFABEND='1'B;        /* --USER.                       */
         OI    APFABEND(ABDARPTR),B'00010000'                      0270
*           ELSE                                                   0271
*             ;                     /* --ABEND(IE, ABDUMP).          */
@RF00269 DS    0H                                                  0272
*         END;                                                     0272
*       ELSE                        /* --LIST TYPE IS VS2-1.         */
*         DO;                                                      0273
         B     @RC00267                                            0273
@RF00267 DS    0H                                                  0274
*           IF SPABEND='1'B         /* VS2-1 (ABEND OR) USER       0274
*                                      CALLING..                     */
*             THEN                                                 0274
         TM    SPABEND(SNAPPTR),B'10000000'                        0274
         BNO   @RF00274                                            0274
*             APFABEND='1'B;        /* --USER.                       */
         OI    APFABEND(ABDARPTR),B'00010000'                      0275
*           ELSE                                                   0276
*             ;                     /* --ABEND(IE, ABDUMP).          */
@RF00274 DS    0H                                                  0277
*         END;                                                     0277
*                                                                  0277
*       /*************************************************************/
*       /*                                                           */
*       /* COPY AND TRANSFORM LIST TO ABDAREA.                       */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0278
*       IF APFABEND='1'B            /* SHOULD MODESET BE ISSUED...   */
*         THEN                      /* -YES, USER IS CALLING.        */
@RC00267 TM    APFABEND(ABDARPTR),B'00010000'                      0278
         BNO   @RF00278                                            0278
*         DO;                                                      0279
*           RFY                                                    0280
*            (R3,                                                  0280
*             R10) RSTD;                                           0280
*           RFY                                                    0281
*             ABDAREA BASED(R3);    /*                       @YM06187*/
*           R3=ABDARPTR;            /* R2 DESTROYED BY MODESET       */
         LR    R3,ABDARPTR                                         0282
*           DO;                     /* MODESET SAVEKEY(ABDKEY)     0283
*                                      EXTKEY(RBT234) WORKREG(R10)   */
*             RESPECIFY                                            0284
*              (GPR02F) RESTRICTED;                                0284
*             R10=GPR02F;           /* SAVE REGISTER 2               */
         LR    R10,GPR02F                                          0285
*             IPK;                  /* SAVE CURRENT PSW KEY          */
         IPK                                                       0286
*             ABDKEY=GPR02F;                                       0287
         STC   GPR02F,ABDKEY(,R3)                                  0287
*             GPR02F=R10;           /* RESTORE REGISTER 2            */
         LR    GPR02F,R10                                          0288
*             R10=TCBRBP;           /* RB PTR FROM TCB               */
         L     R10,TCBRBP(,TCBPTR)                                 0289
*             R10=R10->RBLINK;      /* NEXT RB ADDRESS               */
         L     R10,RBLINK(,R10)                                    0290
*             R10=R10->RBOPSW(2);   /* PSW KEY FROM RB OLD PSW       */
         SLR   @11,@11                                             0291
         IC    @11,RBOPSW+1(,R10)                                  0291
         LR    R10,@11                                             0291
*             SPKA(R10);            /* SET PSW KEY                   */
         SPKA  0(R10)                                              0292
*             RESPECIFY                                            0293
*              (GPR02F) UNRESTRICTED;                              0293
*           END;                    /* MODESET SAVEKEY(ABDKEY)     0294
*                                      EXTKEY(RBT234) WORKREG(R10) GO
*                                      TO USER KEY                   */
*           R9=AUTOPTR;             /* SAVE PTR TO AUTOMATIC STORAGE */
         LR    R9,AUTOPTR                                          0295
*           IF SNPVS2=ON THEN       /* IS PARMLIST VS2-2 TYPE        */
         TM    SNPVS2(SNAPPTR),B'01000000'                         0296
         BNO   @RF00296                                            0296
*             DO;                   /* YES                           */
*               GEN(LM 11,15,0(SNAPPTR));/* CHECK FOR FETCH        0298
*                                      PROTECTION                    */
         LM 11,15,0(SNAPPTR)
*               GEN(STM 11,15,0(SNAPPTR));/* CHECK FOR STORE       0299
*                                      PROTECTION                    */
         STM 11,15,0(SNAPPTR)
*             END;                                                 0300
*           ELSE                                                   0301
*             DO;                   /* VS2-1 TYPE PARMLIST           */
         B     @RC00296                                            0301
@RF00296 DS    0H                                                  0302
*               GEN(LM 11,14,0(SNAPPTR));/* CHECK FOR FETCH        0302
*                                      PROTECTION                    */
         LM 11,14,0(SNAPPTR)
*               GEN(STM 11,14,0(SNAPPTR));/* CHECK FOR STORE       0303
*                                      PROTECTION                    */
         STM 11,14,0(SNAPPTR)
*             END;                                                 0304
*           DO;                     /* MODESET KEYADDR(ABDKEY)     0305
*                                      WORKREG(R10)                  */
@RC00296 DS    0H                                                  0306
*             R10=ABDKEY;           /* GET KEY FORM SAVE LOCATION    */
         SLR   R10,R10                                             0306
         IC    R10,ABDKEY(,R3)                                     0306
*             SPKA(R10);            /* SET PSW KEY                   */
         SPKA  0(R10)                                              0307
*           END;                    /* MODESET KEYADDR(ABDKEY)     0308
*                                      WORKREG(R10) RETURN TO SNAP 0308
*                                      KEY                           */
*           ABDARPTR=R3;            /* RESTORE WORKAREA PTR          */
         LR    ABDARPTR,R3                                         0309
*           RFY                                                    0310
*             ABDAREA BASED(ABDARPTR);/*                     @YM06187*/
*           AUTOPTR=R9;             /* RESTORE AUTOMATIC STORAGE PTR */
         LR    AUTOPTR,R9                                          0311
*           SAVERET=0;              /* REINIT RETURN CODE REG TO 0   */
         SLR   SAVERET,SAVERET                                     0312
         EJECT
*                                                                  0313
*           /*********************************************************/
*           /*                                                       */
*           /* BEFORE MOVING THE SNAP PARAMETER LIST INTO SNAP'S     */
*           /* WORKARE, TYPE OF PARAMETER LIST (OS/VS2 REL.1 OR      */
*           /* OS/VS2 REL.2) MUST BE DETERMINED.                     */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0313
*           IF SNPVS2=ON            /* IS PARMLIST VS2-2 TYPE        */
*             THEN                  /* YES                           */
         TM    SNPVS2(SNAPPTR),B'01000000'                         0313
         BNO   @RF00313                                            0313
*             DO;                                                  0314
*               IF SNPVS2EN=ON THEN /* ENHANCED SNAP PARMLIST      0315
*                                                            @G33SPHW*/
         TM    SNPVS2EN(SNAPPTR),B'00100000'                       0315
         BNO   @RF00315                                            0315
*                 DO;               /* YES...                @G33SPHW*/
*                   ABDPARMS=SNPPARMS;/* MOVE OVER ENTIRE PARMLIST 0317
*                                                            @G33SPHW*/
         MVC   ABDPARMS(24,ABDARPTR),SNPPARMS(SNAPPTR)             0317
*                 END;              /*                       @G33SPHW*/
*               ELSE                /* NOT ENHANCED SNAP     @G33SPHW*/
*                 DO;               /*                       @G33SPHW*/
         B     @RC00315                                            0319
@RF00315 DS    0H                                                  0320
*                   ABDPARMS(1:20)=SNPPARMS(1:20);/* MOVE OVER ONLY
*                                      OLD PARMLIST          @G33SPHW*/
         MVC   ABDPARMS(20,ABDARPTR),SNPPARMS(SNAPPTR)             0320
*                 END;                                             0321
*             END;                                                 0322
*           ELSE                                                   0323
*             DO;                   /* MOVE REL.1 PARMLIST TO SNAP 0323
*                                      WORKAREA                      */
         B     @RC00313                                            0323
@RF00313 DS    0H                                                  0324
*               ABDPID=SPIDENT;     /* INIT ID FIELD                 */
         MVC   ABDPID(1,ABDARPTR),SPIDENT(SNAPPTR)                 0324
*               IF SPABEND='1'B THEN/* CALLER (SNAP OR ABEND)        */
         TM    SPABEND(SNAPPTR),B'10000000'                        0325
         BNO   @RF00325                                            0325
*                 APFABEND='1'B;                                   0326
         OI    APFABEND(ABDARPTR),B'00010000'                      0326
*               IF SPID='1'B THEN   /* ID PROVIDED                   */
@RF00325 TM    SPID(SNAPPTR),B'00000010'                           0327
         BNO   @RF00327                                            0327
*                 APFID='1'B;                                      0328
         OI    APFID(ABDARPTR),B'00001000'                         0328
*               IF SPTCB='1'B THEN  /* TCB PROVIDED                  */
@RF00327 TM    SPTCB(SNAPPTR),B'01000000'                          0329
         BNO   @RF00329                                            0329
*                 APFTCB='1'B;                                     0330
         OI    APFTCB(ABDARPTR),B'00000100'                        0330
*               IF SPSNAP='1'B THEN /* SNAP STORAGE LIST PROVIDED    */
@RF00329 TM    SPSNAP(SNAPPTR),B'00000100'                         0331
         BNO   @RF00331                                            0331
*                 APFSNAPP='1'B;                                   0332
         OI    APFSNAPP(ABDARPTR),B'00000010'                      0332
*               IF SPNUC='1'B THEN  /* NUCLEUS REQESTED              */
@RF00331 TM    SPNUC(SNAPPTR),B'00001000'                          0333
         BNO   @RF00333                                            0333
*                 APFNUC='1'B;                                     0334
         OI    APFNUC(ABDARPTR),B'10000000'                        0334
*               IF SPTRACE='1'B THEN/* TRACE REQUESTED               */
@RF00333 TM    SPTRACE(SNAPPTR),B'00010000'                        0335
         BNO   @RF00335                                            0335
*                 APFTRACE='1'B;                                   0336
         OI    APFTRACE(ABDARPTR),B'00001000'                      0336
*               IF SPSUPDA='1'B THEN/* CONTROL BLOCKS REQUESTED      */
@RF00335 TM    SPSUPDA(SNAPPTR),B'00100000'                        0337
         BNO   @RF00337                                            0337
*                 APFSUPDA='1'B;                                   0338
         OI    APFSUPDA(ABDARPTR),B'00000100'                      0338
*               IF SPQCB='1'B THEN  /* QCBS AND QELS REQESTED        */
@RF00337 TM    SPQCB(SNAPPTR),B'00000001'                          0339
         BNO   @RF00339                                            0339
*                 APFQCB='1'B;                                     0340
         OI    APFQCB(ABDARPTR),B'00000010'                        0340
*               APFPDATA=SPFLAG2;   /* PDATA OPTIONS                 */
@RF00339 MVC   APFPDATA(1,ABDARPTR),SPFLAG2(SNAPPTR)               0341
*               ABDPDCBP=SPDCBP;    /* DCB ADDR                      */
         L     @11,SPDCBP(,SNAPPTR)                                0342
         ST    @11,ABDPDCBP(,ABDARPTR)                             0342
*               ABDPTCBP=SPTCBP;    /* TCB ADDR                      */
         L     @11,SPTCBP(,SNAPPTR)                                0343
         ST    @11,ABDPTCBP(,ABDARPTR)                             0343
*               ABDSNAPP=SPSNAPP;   /* STORAGE LIST ADDR             */
         L     @11,SPSNAPP(,SNAPPTR)                               0344
         ST    @11,ABDSNAPP(,ABDARPTR)                             0344
*             END;                                                 0345
*         END;                                                     0346
*       ELSE                        /* ABEND CALLING, MODESET      0347
*                                      UNNEEDED                      */
*         ABDPARMS=SNPPARMS;        /* COPY LIST TO WORKAREA.        */
         B     @RC00278                                            0347
@RF00278 MVC   ABDPARMS(24,ABDARPTR),SNPPARMS(SNAPPTR)             0347
*       RFY                                                        0348
*        (R3,                                                      0348
*         R10) UNRSTD;                                             0348
@RC00278 DS    0H                                                  0349
         EJECT
*       IF APFABEND=ON THEN         /* WAS ABEND THE CALLER...       */
         TM    APFABEND(ABDARPTR),B'00010000'                      0349
         BNO   @RF00349                                            0349
*         DO;                                                      0350
*                                                                  0350
*           /*********************************************************/
*           /*                                                       */
*           /* ENQ ON DUMP DATA SET WITH MAJOR NAME OF SYSIEA01 AND  */
*           /* MINOR NAME OF DCB ADDRESS SUPPLIED BY CALLER          */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0351
*           ENQLIST(1:20)=''B;      /* ZERO ENQ PARAMETER LIST       */
         L     @05,RBSECPTR                                        0351
         XC    ENQLIST(20,@05),ENQLIST(@05)                        0351
*           LASTIND='FF'X;          /* SET LAST ENTRY INDICATOR      */
         MVI   LASTIND(@05),X'FF'                                  0352
*           SIZEMIN='04'X;          /* SET MINOR NAME LENGTH TO 4    */
         MVI   SIZEMIN(@05),X'04'                                  0353
