         DC    C'IEAVAD01  78.026'                                 0001
         DROP  @15
@PROLOG  BALR  @06,0                                               0001
@PSTART  LA    @07,4095(,@06)                                      0001
         USING @PSTART,@06                                         0001
         USING @PSTART+4095,@07                                    0001
RBPRFXLN EQU 64
RBPRFLNA EQU 32
PRBLEN   EQU 136
SIRBLEN  EQU 200
TIRBLEN  EQU 136
IRBLEN   EQU 128
SVRBLEN  EQU 224
*   R0=R9;                          /* SAVE SVC 51'S BASE REG        */
         LR    R0,R9                                               0209
*   SNAPPTR=PARMPTR;                /* SAVE ADDR OF SNAP PARAMETER 0210
