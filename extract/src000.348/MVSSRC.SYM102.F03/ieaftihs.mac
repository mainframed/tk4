ACCESRTN ST    @14,@SA00009                                        0512
*/********************************************************************/
*/* FUNCTION-                                                      0513
*       ROUTINE WHICH CALLS THE ACCESS SERVICE ROUTINE AND MOVES THE
*     VALUE RETURNED IN R0 INTO DATAPTR.                           0513
*                                                                  0513
*                                                                  0513
*   INPUT -                                                        0513
*         VADR(REGISTER 0) CONTAINS THE VIRTUAL ADDRESS OF THE FIELD
*         TO BE ACCESSED.                                          0513
*                                                                  0513
*    CALLS-                                                        0513
*         ACCESS SERVICE ROUTINE PASSED IN PARMLIST                0513
*                                                                  0513
*    CALLED BY -                                                   0513
*         EEDBITS, IEAFTEED, IEAFTFRR, IEAFTRTC, IEAFTRT2,         0513
*         IEAFTSCB, RT1BITS, AND RT2BITS                           0513
*                                                                  0513
*    RETURNS TO -                                                  0513
*         CALLER WITH THE RETURN CODE SET BY THE ACCESS            0513
*         SERVICE ROUTINE.                                         0513
*                                                                  0513
*   THIS PROCEDURE HAS BEEN INCLUDED FROM SYSLIB(IHAFTCDE) ------  0513
*                                                                    */
*/********************************************************************/
*                                                                  0513
*   IF ADPLSNPR=OFF THEN            /* SNAP WAS INVOKER              */
         TM    ADPLSNPR(ABDPLPTR),B'10000000'                      0513
         BNZ   @RF00513                                            0513
*     DO;                                                          0514
*       RTYADR=ADDR(RETRY);         /* RETRY ADDR FOR ESTAE IN THE 0515
*                                      CASE OF EXPECTED ERRORS       */
         LA    @14,RETRY                                           0515
         ST    @14,RTYADR(,ESTAPTR)                                0515
*       DATAPTR=VADR;               /* USED TO BASE THE STORAGE ON   */
         LR    DATAPTR,VADR                                        0516
*       RCREG=ZERO;                 /* SUCCESS UNLESS AN ABEND OCCURS
*                                      DURING THE MOVE INTO A BUFFER */
         SLR   RCREG,RCREG                                         0517
*       ERREXP=ON;                  /* AN ERROR IS EXPECTED          */
         OI    ERREXP(ESTAPTR),B'10000000'                         0518
*       VERIFY=DATADWRD;            /* MOVE A DOUBLE WORD INTO     0519
*                                      INTERNAL BUFFER               */
*                                                                  0519
         MVC   VERIFY(8),DATADWRD(DATAPTR)                         0519
*       /*************************************************************/
*       /*                                                           */
*       /* THIS MOVE WILL CAUSE AN ABEND IF THE DUMP DOES NOT CONTAIN*/
*       /* THE AREA REFERENCED OR IF THE AREA IS PROTECTED. IF AN    */
*       /* ABEND OCCURS, THE ESTAE WILL GET CONTROL AND RETRY AT THE */
*       /* POINT WHICH SETS A BAD RETURN CODE AND RETURNS TO THE     */
*       /* CALLER. THIS THEN SIMULATES THE PRINT DUMP ACCESS ROUTINE.*/
*       /* IF THE AREA ASKED FOR IS IN THE DUMP, A DOUBLE WORD IS    */
*       /* MOVED INTO AN INTERNAL BUFFER TO PREVENT AN UNEXPECTED    */
*       /* ERROR WHEN REFERENCING THE AREA AGAIN                     */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0520
*       ERREXP=OFF;                 /* AN ERROR IS NO LONGER EXPECTED*/
         NI    ERREXP(ESTAPTR),B'01111111'                         0520
*       DATAPTR=ADDR(VERIFY);       /* REBASE DATA OFF INTERNAL    0521
*                                      BUFFER                        */
         LA    DATAPTR,VERIFY                                      0521
*     END;                          /* END SNAP WAS THE INVOKER      */
*   ELSE                            /* CALLER HAS OWN ACCESS ROUTINE
*                                      LIKE PRINT DUMP               */
*     DO;                                                          0523
         B     @RC00513                                            0523
@RF00513 DS    0H                                                  0524
*       CALL ACCESS;                /* ACCESS SERVICE ROUTINE PASSED
*                                      BY CALLER                     */
         L     @15,ADPLMEMA(,ABDPLPTR)                             0524
         BALR  @14,@15                                             0524
*       IF RCREG=ZERO THEN                                         0525
         LTR   RCREG,RCREG                                         0525
         BNZ   @RF00525                                            0525
*         DATAPTR=VADR;             /* SAVE PTR RETURNED BY ACCESS 0526
*                                      SERVICE ROUTINE               */
         LR    DATAPTR,VADR                                        0526
*       ELSE                        /* BAD RETURN CODE               */
*RETRY:                             /* RETRY ADDR FOR THE ESTAE      */
*         RCREG=4;                  /* SET A BAD RETURN CODE         */
         B     @RC00525                                            0527
@RF00525 DS    0H                                                  0527
RETRY    LA    RCREG,4                                             0527
*     END;                          /* END CALLER WAS NOT SNAP       */
@RC00525 DS    0H                                                  0529
*   RCCODE=RCREG;                   /* SAVE RETURN CODE FROM ACCESS
*                                      SERVICE RTN                   */
@RC00513 ST    RCREG,RCCODE                                        0529
*   RETURN CODE(RCCODE);                                           0530
         L     @04,RCCODE                                          0530
         LR    @15,@04                                             0530
@EL00009 DS    0H                                                  0530
@EF00009 DS    0H                                                  0530
@ER00009 L     @14,@SA00009                                        0530
         BR    @14                                                 0530
*   END ACCESRTN;                                                  0531
