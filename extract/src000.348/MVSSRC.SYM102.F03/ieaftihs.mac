ANALYZE  ST    @14,@SA00008                                        0501
*/****************************************************************** */
*/* FUNCTION                                                       0502
*     ROUTINE TO ANALYZE THE BITS IN THE BYTE PASSED AND PRINT OUT 0502
*     THE MESSAGE THAT CORRESPONDS TO THE BITS THAT ARE ON. IF A BLANK
*     LINE IS PASSED IN THE MESSAGE ARRAY (INDICATING A RESERVED BIT)
*     NO OUTPUT WILL CORRESPOND TO THAT BIT.                       0502
*                                                                  0502
*   INPUT-                                                         0502
*     AN ARRAY (SUMMARY) POINTED TO BY THE PARMLIST FIELD          0502
*     ADPLCOM2 WHICH CONTAINS THE ADDRESS OF THE BYTE TO BE        0502
*     ANALYZED, THE ADDRESS OF THE ARRAY OF CORRESPONDING          0502
*     MESSAGES, THE COLUMN NUMBER THE MESSAGE IS TO BEGIN IN,      0502
*     AND THE LENGTH OF THE MESSAGE ARRAY PASSED.                  0502
*                                                                  0502
*   NOTE- COLNUM + MSGLNGTH MUST BE LESS THAN OR EQUAL TO 120 BYTES
*         WHICH IS THE OUTPUT BUFFER LENGTH.                       0502
*                                                                  0502
*                                                                  0502
*   CALLS-                                                         0502
*       CALLS PRINT                                                0502
*                                                                  0502
*   CALLED BY -                                                    0502
*       BYTEMODE,EEDBITS,IEAFTESA,IEAFTSCB,RT2BITS                 0502
*                                                                  0502
*   RETURNS CONTROL TO -                                           0502
*       CALLER WITHOUT SETTING A RETURN CODE                       0502
*                                                                  0502
*   THIS PROCEDURE HAS BEEN INCLUDED FROM SYSLIB(IHAFTCDE)-------  0502
*                                                                    */
*/****************************************************************** */
*                                                                  0502
*   DO INDEX=1 TO BYTELNTH;         /* LOOP TO FIND WHICH BITS ARE ON*/
         LA    INDEX,1                                             0502
@DL00502 DS    0H                                                  0503
*     LNGTHEQU=((INDEX-1)*(MSGLNGTH)+1);/* OFFSET INTO MSGARY OF   0503
*                                      CORRESPONDING MESSAGE         */
         L     @14,MSGLNGTH                                        0503
         LR    @03,INDEX                                           0503
         BCTR  @03,0                                               0503
         MR    @02,@14                                             0503
         AH    @03,@CH00041                                        0503
         LR    LNGTHEQU,@03                                        0503
*     IF(BITSTR&TESTBITS(INDEX))^=ZEROBYTE&/* FOUND A BIT ON         */
*         MSGARY(LNGTHEQU:MSGLNGTH+LNGTHEQU-1)^=BLANKS(1:MSGLNGTH) THEN
         L     @09,BYTEADDR                                        0504
         SLR   @04,@04                                             0504
         IC    @04,BITSTR(,@09)                                    0504
         SLR   @09,@09                                             0504
         IC    @09,TESTBITS-1(INDEX)                               0504
         NR    @04,@09                                             0504
         SLR   @09,@09                                             0504
         IC    @09,ZEROBYTE                                        0504
         CR    @04,@09                                             0504
         BE    @RF00504                                            0504
         L     @09,MSGADDR                                         0504
         LR    @04,@14                                             0504
         BCTR  @04,0                                               0504
         LR    @03,@09                                             0504
         ALR   @03,LNGTHEQU                                        0504
         BCTR  @03,0                                               0504
         EX    @04,@SC02114                                        0504
         BE    @RF00504                                            0504
*       DO;                         /* CORRESPONDING MESSAGE WAS NOT
*                                      BLANK(RESERVED)               */
*         BUFFER(COLNUM:(MSGLNGTH+COLNUM-1))=MSGARY(LNGTHEQU:LNGTHEQU+
*             MSGLNGTH-1);          /* PLACE MSG IN BUFFER           */
         L     @04,COLNUM                                          0506
         L     @03,ADPLBUF(,ABDPLPTR)                              0506
         ALR   @03,@04                                             0506
         BCTR  @03,0                                               0506
         ALR   @14,@04                                             0506
         SLR   @14,@04                                             0506
         BCTR  @14,0                                               0506
         ALR   @09,LNGTHEQU                                        0506
         BCTR  @09,0                                               0506
         EX    @14,@SM02116                                        0506
*         CALL PRINT;               /* PRINT IT                      */
         L     @15,ADPLPRNT(,ABDPLPTR)                             0507
         BALR  @14,@15                                             0507
*       END;                        /* END FOUND THE MESSAGE         */
*     ELSE                                                         0509
*       ;                           /* EITHER BIT NOT ON OR RESERVED
*                                      BIT-NO MESSAGE                */
@RF00504 DS    0H                                                  0510
*   END;                            /* END SEARCH LOOP               */
         AH    INDEX,@CH00041                                      0510
         CH    INDEX,@CH00086                                      0510
         BNH   @DL00502                                            0510
*   END ANALYZE;                                                   0511
@EL00008 DS    0H                                                  0511
@EF00008 DS    0H                                                  0511
@ER00008 L     @14,@SA00008                                        0511
         BR    @14                                                 0511
