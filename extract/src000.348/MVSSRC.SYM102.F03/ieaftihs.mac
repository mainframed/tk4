FMTRTN   ST    @14,@SA00007                                        0480
*/****************************************************************** */
*/* FUNCTION                                                       0481
*       ROUTINE TO SET UP CALLING OF THE FORMAT SERVICE ROUTINE PASSED
*     IN THE PARMLIST AND CHECKS RETURN CODE.  IF A RETURN CODE IS BAD
*     IT TAKES THE MESSAGE WHOSE ADDRESS IS IN ADPLCOM1. AND PUTS IT
*     IN THE BUFFER AND PRINTS IT. IT SETS THE ERRSW TO INDICATE BAD
*     RETURN CODE AND RETURNS. IF THE RETURN CODE WAS GOOD, IT PRINTS
*     THE FORMATTED LINE.                                          0481
*                                                                  0481
*   CALLS-                                                         0481
*       CALLS FORMAT AND PRINT                                     0481
*                                                                  0481
*   INPUT -                                                        0481
*     LINEPTR(REGISTER 0) HAS THE ADDRESS OF THE FORMAT PATTERN TO BE
*    PASSED TO THE FORMAT SERVICE ROUTINE. ADPLFMT2 CONTAINS THE CURREN
*    ADDRESS OF THE CONTROL BLOCK TO BE FORMATTED AND ,IN MOST CASES,
*    ADPLFMT1 CONTAINS THE ADDRESS OF THE FIELD 'CONVERTD' WHICH IS
*    THE EBCDIC REPRESENTATION OF THE OFFSET TO BE PLACED ALONG SIDE
*    THE OUTPUT LINE.                                              0481
*                                                                  0481
*   CALLED BY -                                                    0481
*     IEAFTEED,IEAFTFRR,IEAFTIHS,IEAFTRTC,IEAFTRT2,IEAFTSCB,       0481
*     IEAFTSDW, RT2SUM,AND SPECIAL                                 0481
*                                                                  0481
*   RETURNS CONTROL TO -                                           0481
*     CALLER WITH RETURN CODE RECIEVED FROM FORMAT                 0481
*     SERVICE ROUTINE                                              0481
*                                                                  0481
*   THIS PROCEDURE HAS BEEN INCLUDED FROM SYSLIB(IHAFTCDE) ------  0481
*                                                                    */
*/****************************************************************** */
*                                                                  0481
*   IF ADPLSNPR=OFF THEN            /* IF SNAP IS THE CALLER, AN   0481
*                                      ACCESS ERROR CAN BE EXPECTED  */
         TM    ADPLSNPR(ABDPLPTR),B'10000000'                      0481
         BNZ   @RF00481                                            0481
*     DO;                                                          0482
*       RTYADR=ADDR(FMTRTY);        /* WHERE TO RETRY                */
         LA    @14,FMTRTY                                          0483
         ST    @14,RTYADR(,ESTAPTR)                                0483
*       ERREXP=ON;                  /* INDICATE AN ERROR IS EXPECTED */
         OI    ERREXP(ESTAPTR),B'10000000'                         0484
*     END;                          /* END SNAP IS THE CALLER        */
*   ELSE                                                           0486
*     ;                             /* SNAP WAS NOT INVOKER          */
@RF00481 DS    0H                                                  0487
*   CALL FORMAT;                    /* FORMAT ROUTINE PASSED BY    0487
*                                      CALLER                        */
         L     @15,ADPLFRMT(,ABDPLPTR)                             0487
         BALR  @14,@15                                             0487
*   ERREXP=OFF;                     /* ERROR IS NO LONGER EXPECTED   */
         NI    ERREXP(ESTAPTR),B'01111111'                         0488
*   IF RCREG^=ZERO THEN                                            0489
         LTR   RCREG,RCREG                                         0489
         BZ    @RF00489                                            0489
*     DO;                           /* FORMAT ROUTINE ERROR          */
*FMTRTY:                            /* RETRY POINT FROM ERROR        */
*       ERRSW=ON;                   /* SET ERROR INDICATOR           */
FMTRTY   OI    ERRSW,B'01000000'                                   0491
*       CALL PRINT;                 /* SPACE A LINE                  */
         L     @15,ADPLPRNT(,ABDPLPTR)                             0492
         BALR  @14,@15                                             0492
*       BUFFER=MSG;                 /* ERROR MSG PASSED BY CALLER  0493
*                                      WHOSE ADDR IS PLACED IN     0493
*                                      ADPLCOM1                      */
         L     @14,ADPLBUF(,ABDPLPTR)                              0493
         L     @04,ADPLCOM1(,ABDPLPTR)                             0493
         MVC   BUFFER(120,@14),MSG(@04)                            0493
*       RCREG=4;                    /* INDICATE BAD RETURN CODE FROM
*                                      FORMATTER                     */
         LA    RCREG,4                                             0494
*     END;                          /* END FORMAT ROUTINE ERROR      */
*   ELSE                                                           0496
*     ;                             /* FORMATTED LINE IS IN BUFFER   */
@RF00489 DS    0H                                                  0497
*   RCCODE=RCREG;                   /* SAVE RETURN CODE FROM FORMAT
*                                      SERVICE ROUTINE               */
         ST    RCREG,RCCODE                                        0497
*   CALL PRINT;                     /* PRINT THE ERROR MSG OR      0498
*                                      FORMATTED LINE                */
         L     @15,ADPLPRNT(,ABDPLPTR)                             0498
         BALR  @14,@15                                             0498
*   RETURN CODE(RCCODE);                                           0499
         L     @15,RCCODE                                          0499
@EL00007 DS    0H                                                  0499
@EF00007 DS    0H                                                  0499
@ER00007 L     @14,@SA00007                                        0499
         BR    @14                                                 0499
*   END FMTRTN;                                                    0500
*ANALYZE:                                                          0501
