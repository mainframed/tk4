*       CONVRT=SAVEPTR-ADPLFMT2;    /* NEGATIVE OFFSET VALUE         */
         SLR   @14,@04                                             0458
         ST    @14,CONVRT                                          0458
*     END;                          /* END NEGATIVE OFFSET           */
*   ELSE                            /* POSITIVE OR ZERO OFFSET       */
*     DO;                                                          0460
         B     @RC00455                                            0460
@RF00455 DS    0H                                                  0461
*       NEGBIT=OFF;                 /* INDICATE POS VALUE            */
