         L     @04,ADPLFMT2(,ABDPLPTR)                             0455
         CR    @14,@04                                             0455
         BNH   @RF00455                                            0455
*     DO;                                                          0456
*       NEGBIT=ON;                  /* INDICATE NEGATIVE OFFSET      */
