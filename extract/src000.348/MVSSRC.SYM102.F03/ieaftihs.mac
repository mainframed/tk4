*   GARBAGE='04'X;                  /* GARBAGE BYTE SO OFFSET WILL 0466
*                                      UNPACK OK                     */
*                                                                  0466
         MVI   GARBAGE,X'04'                                       0466
*   /*****************************************************************/
*   /*                                                               */
*   /* FOR AN ODD NUMBER OF DIGITS THE OUTPUT SHOULD LOOK LIKE +C OR */
*   /* +13C. IF THE VALUE WAS UNPACKED DIRECTLY INTO DIGITS THE      */
*   /* OUTPUT WOULD HAVE UNWANTED LEADING ZEROS SUCH AS +0C. TO      */
*   /* ELIMINATE THIS(ONLY FOR AN ODD NUMBER OF DIGITS) UNPACK ODD   */
*   /* NUMBER OF DIGITS INTO CONVERTD WHERE THE LEADING ZERO WILL BE */
*   /* OVERWRITTEN BY THE '+' OR '-' SIGN                            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0467
*   IF CONVRT<TWODGT THEN           /* ONLY 1 DIGIT TO OFFSET        */
         CH    @14,@CH00718                                        0467
         BNL   @RF00467                                            0467
*     UNPK(CONVERTD(1:3),UNPACK(4:5));/* UNPACK ONLY 1 DIGIT AND   0468
*                                      DUMMY                         */
         UNPK  CONVERTD(3),UNPACK+3(2)                             0468
*   ELSE                                                           0469
*     IF CONVRT<THREDGT THEN        /* 2 DIGITS TO OFFSET            */
         B     @RC00467                                            0469
@RF00467 L     @14,CONVRT                                          0469
         CH    @14,@CH00707                                        0469
         BNL   @RF00469                                            0469
*       UNPK(DIGIT(1:3),UNPACK(4:5));/* UNPACK LAST 2 DIGITS & DUMMY */
         UNPK  DIGIT(3),UNPACK+3(2)                                0470
*     ELSE                                                         0471
*       IF CONVRT<FOURDGT THEN      /* 3 DIGITS TO OFFSET            */
         B     @RC00469                                            0471
@RF00469 L     @14,CONVRT                                          0471
         CH    @14,@CH02080                                        0471
         BNL   @RF00471                                            0471
*         UNPK(CONVERTD(1:5),UNPACK(3:5));/* UNPACK LAST 3 DIGITS &
*                                      DUMMY                         */
         UNPK  CONVERTD(5),UNPACK+2(3)                             0472
*       ELSE                        /* 4 DIGITS (MAX)                */
*         UNPK(DIGIT(1:5),UNPACK(3:5));/* LAST 4 DIGITS & DUMMY BYTE */
         B     @RC00471                                            0473
@RF00471 UNPK  DIGIT(5),UNPACK+2(3)                                0473
*   TR(DIGIT,TRANS);                /* MAKE PRINTABLE                */
@RC00471 DS    0H                                                  0474
@RC00469 DS    0H                                                  0474
@RC00467 TR    DIGIT(5),TRANS                                      0474
*   IF NEGBIT=ON THEN               /* WAS A NEGATIVE OFFSET         */
         TM    NEGBIT,B'10000000'                                  0475
         BNO   @RF00475                                            0475
*     PLUS=NEGCHAR;                 /* MINUS SIGN                    */
         MVI   PLUS,C'-'                                           0476
*   ELSE                            /* POSITIVE OFFSET               */
*     PLUS=PLUSCHAR;                /* MOVE IN PLUS SIGN             */
         B     @RC00475                                            0477
@RF00475 MVI   PLUS,C'+'                                           0477
*   ADPLFMT1=ADDR(CONVERTD);        /* SET UP FOR FORMAT             */
@RC00475 LA    @14,CONVERTD                                        0478
         ST    @14,ADPLFMT1(,ABDPLPTR)                             0478
*   END CONVERT;                                                   0479
@EL00006 DS    0H                                                  0479
@EF00006 DS    0H                                                  0479
@ER00006 L     @14,12(,@13)                                        0479
         BR    @14                                                 0479
*FMTRTN:                                                           0480
