         TITLE 'IEAFTIHS-IHSA-FRRS CONTROL BLOCK FORMAT MODULE         *
                        '
IEAFTIHS CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL2(@EP00001-@MAINENT)
         DC    AL1(16)                                             0001
         DC    C'IEAFTIHS  78.033'                                 0001
IEAFTFRR DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL2(@EP00203-IEAFTFRR)
         ENTRY IEAFTFRR
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @11,0                                               0001
@PSTART  LA    @12,4095(,@11)                                      0001
         USING @PSTART,@11                                         0001
         USING @PSTART+4095,@12                                    0001
         L     @00,@SIZDATD                                        0001
         GETMAIN  R,LV=(0)
         LR    @10,@01                                             0001
         USING @DATD,@10                                           0001
         ST    @13,@SA00001+4                                      0001
         LM    @15,@01,16(@13)                                     0001
         ST    @10,8(,@13)                                         0001
         LR    @13,@10                                             0001
         AH    @15,4(,@15)                                         0001
         BR    @15                                                 0001
@EP00001 DS    0H                                                  0002
*                                                                  0120
*/********************************************************************/
*/* FUNCTION -                                                     0120
*      ROUTINE WHICH FORMATS THE INTERRUPT HANDLER SAVE AREA.      0120
*      IT SETS UP THE NEEDED POINTERS TO BE PASSED TO THE FORMAT   0120
*      ROUTINE SENT IN THE PARAMETER LIST.  FOR ANY ERROR ACCESSING
*      A NEEDED FIELD IN THE IHSA OR RT1W OR FOR NON-ZERO RETURN CODE
*      FROM THE FORMATTING SERVICE, THE FOLLOWING MESSAGE WILL BE  0120
*      PRINTED:                                                    0120
*               *** FORMATTING SERVICE ROUTINE ERROR DURING        0120
*               IHSA FORMATTING(POSSIBLE CAUSE:  DATA NOT IN       0120
*               DUMP) ***                                          0120
*      AT THIS POINT PROCESSING WOULD TERMINATE. IF THE IHSA IS    0120
*      FORMATTED WITHOUT ERROR, A BIT SUMMARY OF THE RT1W IS PRINTED
*      USING THE ANALYZE INTERNAL PROCEDURE.                       0120
*                                                                  0120
*   INPUT -                                                        0120
*      REGISTER 1 POINTS TO THE PARMLIST WHICH CONTAINS THE        0120
*      ADDRESS OF THE CONTROL BLOCK TO BE FORMATTED IN ADPLFMT2.   0120
*      ADPLFMT1, ALSO DEFINED AS START, CONTAINS THE OFFSET TO BE  0120
*      PLACED ALONG THE FIRST FIELD IN THE CONTROL BLOCK AND ADPLCOM1
*      CONTAINS THE ADDRESS OF THE ESTAE PARAMETER LIST.           0120
*                                                                  0120
*   CALLS -                                                        0120
*      CALLS CONVERT, FMTRTN, PRINT, AND RT1RTN                    0120
*                                                                  0120
*   CALLED BY -                                                    0120
*       IEAVTFMT TO FORMAT IHSA                                    0120
*                                                                  0120
*   RETURNS TO -                                                   0120
*       IEAVTFMT WITH RETURN CODE SET BY THE ACCESS OR FORMAT SERVICE
*       ROUTINES OR BY RT1RTN.                                       */
*/********************************************************************/
*                                                                  0120
*   ESTAPTR=ADPLCOM1;               /* PTR TO ESTAE PARM LIST        */
         L     ESTAPTR,ADPLCOM1(,ABDPLPTR)                         0120
*   STM(VADR,RCREG,LCALREGS);       /* SAVE REGS FOR RETRY           */
         STM   VADR,RCREG,LCALREGS(ESTAPTR)                        0121
*   ADPLCOM1=ADDR(IHSAFMT);         /* FORMAT ROUTINE ERROR MSG      */
         LA    @14,IHSAFMT                                         0122
         ST    @14,ADPLCOM1(,ABDPLPTR)                             0122
*   IHSAPTR=ADPLFMT2;               /* PTR TO IHSA                   */
         L     IHSAPTR,ADPLFMT2(,ABDPLPTR)                         0123
*   BITS=BITS&&BITS;                /* ZERO ALL INTERNAL BITS        */
*                                                                  0124
         XC    BITS(1),BITS                                        0124
*   /*****************************************************************/
*   /*                                                               */
*   /* IN RELEASE 2 THE POINTER TO THE SUBITS WAS NOT DEFINED.       */
*   /* THEREFORE BEFORE ACCESSING THE SUBIT POINTER, CHECK FOR       */
*   /* RELEASE 2                                                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0125
*   VADR=ADDR(CVTNUMB);             /* RELEASE NUMBER                */
         L     @14,CVTPTR(,ABDPLPTR)                               0125
         AL    @14,@CF02109                                        0125
         LR    VADR,@14                                            0125
*   CALL ACCESRTN;                  /* ACCESS IT                     */
         BAL   @14,ACCESRTN                                        0126
*   IF RCREG^=ZERO THEN             /* ACCESS ERROR                  */
         LTR   RCREG,RCREG                                         0127
         BZ    @RF00127                                            0127
*     BUFFER(1:LENGTH(IHSAFMT))=IHSAFMT;/* ERROR MESSAGE             */
         L     @14,ADPLBUF(,ABDPLPTR)                              0128
         MVC   BUFFER(120,@14),IHSAFMT                             0128
*   ELSE                            /* FOUND RELEASE NUMBER          */
*     DO;                                                          0129
         B     @RC00127                                            0129
@RF00127 DS    0H                                                  0130
*       IF DATAHALF^=REL2 THEN      /* NOT A RELEASE 2 MVS SYSTEM    */
         CLC   DATAHALF(2,DATAPTR),REL2                            0130
         BE    @RF00130                                            0130
*         DO;                                                      0131
*                                                                  0131
*           /*********************************************************/
*           /*                                                       */
*           /* FIND WHICH SUS ARE ON SYSTEM                          */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0132
*           VADR=ADDR(CVTIHASU);    /* SUBIT PTR                     */
         L     @14,CVTPTR(,ABDPLPTR)                               0132
         LA    VADR,CVTIHASU(,@14)                                 0132
*           CALL ACCESRTN;          /* ACCESS IT                     */
         BAL   @14,ACCESRTN                                        0133
*           IF RCREG^=ZERO THEN     /* ACCESS ERROR                  */
         LTR   RCREG,RCREG                                         0134
         BZ    @RF00134                                            0134
*             DO;                                                  0135
*               BUFFER(1:LENGTH(IHSAFMT))=IHSAFMT;                 0136
         L     @14,ADPLBUF(,ABDPLPTR)                              0136
         MVC   BUFFER(120,@14),IHSAFMT                             0136
*               ERRSW=ON;           /* INDICATE ACCESS ERROR         */
         OI    ERRSW,B'01000000'                                   0137
*             END;                  /* END ACCESS ERROR              */
*           ELSE                    /* FOUND SUBIT PTR               */
*             DO;                                                  0139
         B     @RC00134                                            0139
@RF00134 DS    0H                                                  0140
*               IF DATAWORD^=ZERO THEN/* THIS IS AN SU SYSTEM        */
         L     @14,DATAWORD(,DATAPTR)                              0140
         LTR   @14,@14                                             0140
         BZ    @RF00140                                            0140
*                 DO;                                              0141
*                   VADR=DATAWORD;  /* ADDRESS OF SUBIT STRING       */
         LR    VADR,@14                                            0142
*                   CALL ACCESRTN;  /* ACCESS IT                     */
         BAL   @14,ACCESRTN                                        0143
*                   IF RCREG^=ZERO THEN/* ACCESS ERROR               */
         LTR   RCREG,RCREG                                         0144
         BZ    @RF00144                                            0144
*                     DO;                                          0145
*                       BUFFER(1:LENGTH(IHSAFMT))=IHSAFMT;         0146
         L     @14,ADPLBUF(,ABDPLPTR)                              0146
         MVC   BUFFER(120,@14),IHSAFMT                             0146
*                       ERRSW=ON;   /* INDICATE ERROR HAS OCCURED    */
         OI    ERRSW,B'01000000'                                   0147
*                     END;          /* END ACCESS ERROR              */
*                   ELSE            /* FOUND SUBITS                  */
*                     DO;                                          0149
         B     @RC00144                                            0149
@RF00144 DS    0H                                                  0150
*                       IF SU50BIT=ON THEN                         0150
         TM    SU50BIT(DATAPTR),B'00100000'                        0150
         BNO   @RF00150                                            0150
*                         SU50SAVE=ON;/* INDICATE SU50 ON SYSTEM     */
         OI    SU50SAVE,B'10000000'                                0151
*                       ELSE                                       0152
*                         ;         /* SU50 NOT ON SYSTEM            */
@RF00150 DS    0H                                                  0153
*                     END;          /* END FOUND SUBITS              */
*                 END;              /* END THIS IS AN SU SYSTEM      */
*               ELSE                                               0155
*                 ;                 /* PRE SU SYSTEM -NO BIT NEEDED  */
@RF00140 DS    0H                                                  0156
*             END;                  /* END COULD ACCESS SUBIT POINTER*/
*         END;                      /* END NOT A RELEASE 2 MVS SYSTEM*/
*       ELSE                                                       0158
*         ;                         /* IS A RELEASE 2 MVS SYSTEM, NO
*                                      SU                            */
@RF00130 DS    0H                                                  0159
*       IF ERRSW=OFF THEN           /* FOUND SYSTEM OK               */
@RC00130 TM    ERRSW,B'01000000'                                   0159
         BNZ   @RF00159                                            0159
*         DO;                                                      0160
*                                                                  0160
*           /*********************************************************/
*           /*                                                       */
*           /* PUT OUT IHSA HEADER                                   */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0161
*           SAVEPTR=ADPLFMT2-START; /* VALUE FOR OFFSET GENERATION   */
         L     @14,ADPLFMT2(,ABDPLPTR)                             0161
         SL    @14,START(,ABDPLPTR)                                0161
         ST    @14,SAVEPTR                                         0161
*           BUFFER(1:LENGTH(IHSAHDR))=IHSAHDR;/* TITLE LINE          */
         L     @14,ADPLBUF(,ABDPLPTR)                              0162
         MVC   BUFFER(54,@14),IHSAHDR                              0162
*           PACKAREA=IHSAPTR;       /* PLACE ADDR IN CHAR AREA       */
         ST    IHSAPTR,PACKAREA                                    0163
*           GARBAGE='04'X;          /* GARBAGE BYTE FOR UNPACK       */
         MVI   GARBAGE,X'04'                                       0164
*           UNPK(ADDRESS,UNPACK);   /* PLACE INTO BUFFER             */
         UNPK  ADDRESS(9,@14),UNPACK(5)                            0165
*           TR(ADDRESS,TRANS);      /* TRANSLATE TO PRINTABLE HEX    */
         TR    ADDRESS(9,@14),TRANS                                0166
*           CALL PRINT;             /* PRINT TITLE LINE              */
         L     @15,ADPLPRNT(,ABDPLPTR)                             0167
         BALR  @14,@15                                             0167
*           BUFFER(1:LENGTH(IHSAUS))=IHSAUS;/* UNDERSCORE            */
         L     @14,ADPLBUF(,ABDPLPTR)                              0168
         MVC   BUFFER(28,@14),IHSAUS                               0168
*           CALL PRINT;             /* PRINT IT                      */
         L     @15,ADPLPRNT(,ABDPLPTR)                             0169
         BALR  @14,@15                                             0169
*           CALL PRINT;             /* BLANK                         */
         L     @15,ADPLPRNT(,ABDPLPTR)                             0170
         BALR  @14,@15                                             0170
*           CALL PRINT;             /* BLANK                         */
*                                                                  0171
         L     @15,ADPLPRNT(,ABDPLPTR)                             0171
         BALR  @14,@15                                             0171
*           /*********************************************************/
*           /*                                                       */
*           /* SECTION UNIQUE TO IHSA                                */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0172
*           DO ARRAYCNT=1 TO DIM(IHSANAME) WHILE(ERRSW=OFF);/* FORMAT
*                                      IHSA UP TO FRRS               */
         LA    ARRAYCNT,1                                          0172
@DL00172 TM    ERRSW,B'01000000'                                   0172
         BNZ   @DC00172                                            0172
*             LINEPTR=IHSANAME(ARRAYCNT);/* FORMAT PATTERN           */
         LR    @14,ARRAYCNT                                        0173
         SLA   @14,2                                               0173
         L     LINEPTR,IHSANAME-4(@14)                             0173
*             CALL CONVERT;         /* OFFSET GENERATOR              */
         BAL   @14,CONVERT                                         0174
*             CALL FMTRTN;          /* FORMAT                        */
         BAL   @14,FMTRTN                                          0175
*           END;                    /* END FORMAT LOOP               */
         AH    ARRAYCNT,@CH00041                                   0176
         CH    ARRAYCNT,@CH00080                                   0176
         BNH   @DL00172                                            0176
@DC00172 DS    0H                                                  0177
*           IF ERRSW=OFF THEN       /* NO ERRORS SO FAR              */
         TM    ERRSW,B'01000000'                                   0177
         BNZ   @RF00177                                            0177
*             DO;                                                  0178
*               CALL PRINT;         /* BLANK                         */
*                                                                  0179
         L     @15,ADPLPRNT(,ABDPLPTR)                             0179
         BALR  @14,@15                                             0179
*               /*****************************************************/
*               /*                                                   */
*               /* IHSA SECTION WHICH CHANGES WITH SU50              */
*               /*                                                   */
*               /*****************************************************/
*                                                                  0180
*               BUFFER(31:(LENGTH(IHSAFMSG)+30))=IHSAFMSG;/* FRRS  0180
*                                      STACK HDR                     */
         L     @14,ADPLBUF(,ABDPLPTR)                              0180
         MVC   BUFFER+30(9,@14),IHSAFMSG                           0180
*               CALL PRINT;         /* PRINT IT                      */
         L     @15,ADPLPRNT(,ABDPLPTR)                             0181
         BALR  @14,@15                                             0181
*               BUFFER(31:(LENGTH(IHSAFRUS)+30))=IHSAFRUS;/*       0182
*                                      UNDERSCORE                    */
         L     @14,ADPLBUF(,ABDPLPTR)                              0182
         MVC   BUFFER+30(9,@14),IHSAFRUS                           0182
*               CALL PRINT;         /* PRINT IT                      */
         L     @15,ADPLPRNT(,ABDPLPTR)                             0183
         BALR  @14,@15                                             0183
*               CALL PRINT;         /* BLANK                         */
*                                                                  0184
         L     @15,ADPLPRNT(,ABDPLPTR)                             0184
         BALR  @14,@15                                             0184
*               /*****************************************************/
*               /*                                                   */
*               /* THIS LINE OF THE FRRS IS FORMATTED HERE BECAUSE   */
*               /* SU50 CHANGED THE CONTENTS OF THIS AREA            */
*               /*                                                   */
*               /*****************************************************/
*                                                                  0185
*               FRRSPTR=ADPLFMT2;   /* SET PTR FOR FIGURING NUM    0185
*                                      ENTRIES                       */
         MVC   FRRSPTR(4),ADPLFMT2(ABDPLPTR)                       0185
*               ARRAYCNT=1;                                        0186
         LA    ARRAYCNT,1                                          0186
*               IF SU50SAVE=ON THEN /* SU50 ON SYSTEM                */
         TM    SU50SAVE,B'10000000'                                0187
         BNO   @RF00187                                            0187
*                 LINEPTR=IHSASU50(ARRAYCNT);/* SU50 VERSION         */
         LR    @14,ARRAYCNT                                        0188
         SLA   @14,2                                               0188
         L     LINEPTR,IHSASU50-4(@14)                             0188
*               ELSE                /* NON SU50 VERSION              */
*                 LINEPTR=FRRNAMES(ARRAYCNT);/* COMMON FRRS LINE     */
         B     @RC00187                                            0189
@RF00187 LR    @14,ARRAYCNT                                        0189
         SLA   @14,2                                               0189
         L     LINEPTR,FRRNAMES-4(@14)                             0189
*               CALL CONVERT;       /* OFFSET                        */
@RC00187 BAL   @14,CONVERT                                         0190
*               CALL FMTRTN;        /* FORMAT                        */
         BAL   @14,FMTRTN                                          0191
*               IF ERRSW=OFF THEN                                  0192
         TM    ERRSW,B'01000000'                                   0192
         BNZ   @RF00192                                            0192
*                 CALL RT1RTN;      /* IF STILL NO ERRORS FINISH   0193
*                                      PROCESSING                    */
         BAL   @14,RT1RTN                                          0193
*               ELSE                                               0194
*                 ;                 /* FRRS ERROR                    */
@RF00192 DS    0H                                                  0195
*             END;                  /* END NO IHSA ERROR             */
*           ELSE                                                   0196
*             ;                     /* IHSA ERROR                    */
@RF00177 DS    0H                                                  0197
*         END;                      /* END FOUND SYSTEM              */
*       ELSE                                                       0198
*         ;                         /* ERROR FINDING SYSTEM          */
@RF00159 DS    0H                                                  0199
*     END;                          /* END COULD ACCESS REL NUMBER   */
*   CALL PRINT;                     /* ERROR MSG OR BLANK            */
@RC00127 L     @15,ADPLPRNT(,ABDPLPTR)                             0200
         BALR  @14,@15                                             0200
*   LCALREGS=RTYREGS;               /* RESTORE RETRY REGS OF CALLER  */
         MVC   LCALREGS(64,ESTAPTR),RTYREGS(ESTAPTR)               0201
*   RETURN CODE(RCCODE);            /* DO NOT ENTER FRR CODE         */
         L     @09,RCCODE                                          0202
         L     @13,4(,@13)                                         0202
         L     @00,@SIZDATD                                        0202
         LR    @01,@10                                             0202
         FREEMAIN R,LV=(0),A=(1)
         LR    @15,@09                                             0202
         L     @14,12(,@13)                                        0202
         LM    @00,@12,20(@13)                                     0202
         BR    @14                                                 0202
*IEAFTFRR:                                                         0203
*   ENTRY;                                                         0203
*                                                                  0203
@EP00203 DS    0H                                                  0204
*/********************************************************************/
*/* FUNCTION-                                                      0204
*       ROUTINE WHICH FORMATS THE FRR STACK TITLE AND LINES WHICH ARE
*     DIFFERENT THAN THOSE IN THE INTERRUPT HANDLER SAVE AREA.     0204
*     IT SETS UP THE NEEDED POINTERS TO BE PASSED TO THE FORMAT ROUTINE
*     SENT IN THE PARAMETER LIST.  FOR ANY ERROR ACCESSING A NEEDED
*     FIELD IN THE FRRS OR FOR NON-ZERO RETURN CODE FROM THE       0204
*     FORMATTING SERVICE, THE FOLLOWING MESSAGES WILL BE PRINTED:  0204
*                       *** FORMATTING SERVICE ROUTINE ERROR DURING FRR
*                           FORMATTING (POSSIBLE CAUSE: DATA NOT IN
*                           DUMP ) ***.                            0204
*     AT THIS POINT, FOR EITHER REASON, THE FRR PROCESSING WOULD   0204
*     TERMINATE. IF THE FRR LINES ARE FORMATTED WITHOUT AN ACCESS  0204
*     ERROR, THE ROUTINE TO FORMAT THE FRRS-IHSA COMMON AREAS IS   0204
*     CALLED.                                                      0204
*                                                                  0204
*   INPUT-                                                         0204
*       REGISTER 1 POINTS TO THE PARMLIST WHICH CONTAINS THE ADDRESS
*     OF THE CONTROL BLOCK TO BE FORMATTED IN ADPLFMT2. ADPLFMT1 ALSO
*     DEFINED AS START, CONTAINS THE OFFSET TO BE PLACED ALONG SIDE
*     THE FIRST FIELD IN THE CONTROL BLOCK AND ADPLCOM1 CONTAINS THE
*     ADDRESS OF THE ESTAE PARAMETER LIST.                         0204
*                                                                  0204
*   CALLS-                                                         0204
*       CALLS ACCESRTN,CONVERT,FMTRTN,RT1RTN,AND PRINT             0204
*                                                                  0204
*   CALLED BY -                                                    0204
*       IEAVTFMT TO FORMAT FRRS AND RT1W.                          0204
*                                                                  0204
*   RETURNS TO -                                                   0204
*       CALLER WITH A RETURN CODE SET BY THE ACCESS OR FORMAT      0204
*       SERVICE ROUTINES OR BY RT1RTN.                               */
*/********************************************************************/
*                                                                  0204
*   ESTAPTR=ADPLCOM1;               /* PTR TO ESTAE PARM LIST        */
         L     ESTAPTR,ADPLCOM1(,ABDPLPTR)                         0204
*   STM(VADR,RCREG,LCALREGS);       /* SAVE REGS FOR RETRY           */
         STM   VADR,RCREG,LCALREGS(ESTAPTR)                        0205
*   ADPLCOM1=ADDR(FRRSFMT);         /* FORMAT ROUTINE ERROR MESSAGE  */
         LA    @14,FRRSFMT                                         0206
         ST    @14,ADPLCOM1(,ABDPLPTR)                             0206
*   FRRSPTR=ADPLFMT2;               /* PTR TO FRRS CONTROL BLOCK     */
         L     @14,ADPLFMT2(,ABDPLPTR)                             0207
         ST    @14,FRRSPTR                                         0207
*   BITS=BITS&&BITS;                /* ZERO THE INTERNAL BITS        */
         XC    BITS(1),BITS                                        0208
*   SAVEPTR=ADPLFMT2-START;         /* VALUE FOR DYNAMIC OFFSET    0209
*                                      GENERATOR                     */
         LR    @08,@14                                             0209
         SL    @08,START(,ABDPLPTR)                                0209
         ST    @08,SAVEPTR                                         0209
*   BUFFER(1:LENGTH(FRRHDR))=FRRHDR;/* TITLE LINE                    */
         L     @08,ADPLBUF(,ABDPLPTR)                              0210
         MVC   BUFFER(54,@08),FRRHDR                               0210
*   PACKAREA=FRRSPTR;               /* PLACE ADDR INTO CHAR AREA     */
         ST    @14,PACKAREA                                        0211
*   GARBAGE='04'X;                  /* GARBAGE BYTE FOR THE UNPACK   */
         MVI   GARBAGE,X'04'                                       0212
*   UNPK(ADDRESS,UNPACK);           /* PLACE ADDRESS INTO BUFFER LINE*/
         UNPK  ADDRESS(9,@08),UNPACK(5)                            0213
*   TR(ADDRESS,TRANS);              /* TRANSLATE TO PRINTABLE HEX    */
         TR    ADDRESS(9,@08),TRANS                                0214
*   CALL PRINT;                     /* PRINT TITLE                   */
         L     @15,ADPLPRNT(,ABDPLPTR)                             0215
         BALR  @14,@15                                             0215
*   BUFFER(1:LENGTH(FRRSUS))=FRRSUS;/* UNDERSCORE                    */
         L     @14,ADPLBUF(,ABDPLPTR)                              0216
         MVC   BUFFER(18,@14),FRRSUS                               0216
*   CALL PRINT;                     /* PRINT IT                      */
         L     @15,ADPLPRNT(,ABDPLPTR)                             0217
         BALR  @14,@15                                             0217
*   CALL PRINT;                     /* BLANK                         */
*                                                                  0218
         L     @15,ADPLPRNT(,ABDPLPTR)                             0218
         BALR  @14,@15                                             0218
*   /*****************************************************************/
*   /*                                                               */
*   /* SECTION OF THE FRR STACK PRECEDING THE FRR ENTRIES            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0219
*   ARRAYCNT=1;                     /* FIRST FORMAT PATTERN          */
         LA    ARRAYCNT,1                                          0219
*   LINEPTR=FRRNAMES(ARRAYCNT);     /* ADDRESS OF FIRST FORMAT     0220
*                                      PATTERN                       */
         LR    @14,ARRAYCNT                                        0220
         SLA   @14,2                                               0220
         L     LINEPTR,FRRNAMES-4(@14)                             0220
*   CALL CONVERT;                   /* GENERATE OFFSET               */
         BAL   @14,CONVERT                                         0221
*   CALL FMTRTN;                    /* FORMAT                        */
         BAL   @14,FMTRTN                                          0222
*   IF ERRSW=OFF THEN               /* CONTINUE ONLY IF FORMATTED OK */
         TM    ERRSW,B'01000000'                                   0223
         BNZ   @RF00223                                            0223
*     CALL RT1RTN;                  /* ROUTINE TO FORMAT FRR IHSA  0224
*                                      COMMON AREAS                  */
         BAL   @14,RT1RTN                                          0224
*   ELSE                                                           0225
*     ;                             /* DID NOT FORMAT OK             */
@RF00223 DS    0H                                                  0226
*   LCALREGS=RTYREGS;               /* RESTORE RETRY REGS OF CALLER  */
         MVC   LCALREGS(64,ESTAPTR),RTYREGS(ESTAPTR)               0226
*   RETURN CODE(RCCODE);            /* END ENTRY POINT IEAFTFRR      */
         L     @09,RCCODE                                          0227
         L     @13,4(,@13)                                         0227
         L     @00,@SIZDATD                                        0227
         LR    @01,@10                                             0227
         FREEMAIN R,LV=(0),A=(1)
         LR    @15,@09                                             0227
         L     @14,12(,@13)                                        0227
         LM    @00,@12,20(@13)                                     0227
         BR    @14                                                 0227
*RT1RTN:                                                           0228
*   PROC OPTIONS(SAVE(14));                                        0228
*                                                                  0228
@EL00001 L     @13,4(,@13)                                         0228
@EF00001 L     @00,@SIZDATD                                        0228
         LR    @01,@10                                             0228
         FREEMAIN R,LV=(0),A=(1)
@ER00001 LM    @14,@12,12(@13)                                     0228
         BR    @14                                                 0228
RT1RTN   ST    @14,@SA00002                                        0228
*/********************************************************************/
*/* FUNCTION-                                                      0229
*       ROUTINE WHICH FORMATS THE FRR STACK FROM THE RT1W ON.      0229
*     THIS SECTION OF THE CONTROL BLOCK IS COMMON BETWEEN THE IHSA 0229
*     AND FRR STACK.                                               0229
*     IT SETS UP THE NEEDED POINTERS TO BE PASSED TO THE FORMAT ROUTINE
*     SENT IN THE PARAMETER LIST.  FOR ANY ERROR ACCESSING A NEEDED
*     FIELD IN THE RT1W OR FRRS OR FOR NON-ZERO RETURN CODE FROM THE
*     FORMATTING SERVICE, THE FOLLOWING MESSAGES WILL BE PRINTED:  0229
*                       *** FORMATTING SERVICE ROUTINE ERROR DURING FRR
*                           FORMATTING (POSSIBLE CAUSE: DATA NOT IN
*                           DUMP ) ***.                            0229
*     IF THE NEEDED FIELDS FOR DETERMINING THE NUMBER OF ENTRIES COULD
*     BE ACCESSED BUT THE NUMBER OF ENTRIES IS NOT BETWEEN 0 AND 16-THE
*     VALUES MUST BE IN ERROR AND THE FOLLOWING MESSAGE WILL BE    0229
*     PRINTED:                                                     0229
*                       *** ERROR IN THE FIELDS USED TO DETERMINE  0229
*                           THE NUMBER OF FRR ENTRIES ***          0229
*     AT THIS POINT, FOR EITHER REASON, THE FRR PROCESSING WOULD   0229
*     TERMINATE. IF THE FRR STACK IS FORMATTED WITHOUT AN ACCESS   0229
*     ERROR, A BIT SUMMARY OF THE RT1W IS PRINTED USING THE ANALYZE
*     PROC.                                                        0229
*                                                                  0229
*                                                                  0229
*   CALLS-                                                         0229
*       CALLS ACCESRTN,CONVERT,FMTRTN,RT1BITS,AND PRINT            0229
*                                                                  0229
*   CALLED BY -                                                    0229
*             IEAFTFRR AND IEAFTIHS TO FORMAT THE IHSA FRRS COMMON 0229
*             AREAS.                                               0229
*                                                                  0229
*   RETURNS TO -                                                   0229
*              CALLER WITH A RETURN CODE SET BY THE ACCESS OR FORMAT
*             SERVICE ROUTINES.                                      */
*/********************************************************************/
*                                                                  0229
*   ERRSW=OFF;                                                     0229
         NI    ERRSW,B'10111111'                                   0229
*   CALL PRINT;                     /* BLANK LINE                    */
         L     @15,ADPLPRNT(,ABDPLPTR)                             0230
         BALR  @14,@15                                             0230
*   RT1PTR=ADPLFMT2;                /* RT1W ADDRESS                  */
         MVC   RT1PTR(4),ADPLFMT2(ABDPLPTR)                        0231
*                                                                  0232
*   /*****************************************************************/
*   /*                                                               */
*   /* FIGURE OUT THE NUMBER OF POSSIBLE ENTRIES IN THE FRR STACK.   */
*   /* FOR RELEASE 2 OF MVS THE NUMBER OF ENTRIES WAS 8. FOR RELEASE */
*   /* 3 THIS NUMBER CHANGED TO 16. THE NUMBER CAN BE DETERMINED BY  */
*   /* TAKING THE LAST POSSIBLE ENTRY ADDRESS AND SUBTRACTING FROM IT*/
*   /* THE FIRST POSSIBLE ADDRESS AND THEN DIVIDING BY THE LENGTH OF */
*   /* OF THE ENTRY.                                                 */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0232
*   IF SU50SAVE=ON THEN             /* SET NUMBER OF FRR ENTRIES -16 */
         TM    SU50SAVE,B'10000000'                                0232
         BNO   @RF00232                                            0232
*     ENTRYMAX=SU50MAX;                                            0233
         LA    ENTRYMAX,16                                         0233
*   ELSE                            /* DYNAMICALLY FIGURE NUM ENTRIES*/
*     DO;                                                          0234
         B     @RC00232                                            0234
@RF00232 DS    0H                                                  0235
*       VADR=ADDR(FRRSLAST);        /* LAST POSSIBLE ENTRY ADDRESS   */
         L     @14,FRRSPTR                                         0235
         LA    VADR,FRRSLAST(,@14)                                 0235
*       CALL ACCESRTN;              /* ACCESS IT                     */
         BAL   @14,ACCESRTN                                        0236
*       IF RCREG^=ZERO THEN         /* ERROR ACCESSING THE FRR SATCK */
         LTR   RCREG,RCREG                                         0237
         BZ    @RF00237                                            0237
*         DO;                                                      0238
*           BUFFER(1:LENGTH(FRRSFMT))=FRRSFMT;/* ERROR MESSAGE       */
         L     @14,ADPLBUF(,ABDPLPTR)                              0239
         MVC   BUFFER(120,@14),FRRSFMT                             0239
*           CALL PRINT;             /* PRINT IT                      */
         L     @15,ADPLPRNT(,ABDPLPTR)                             0240
         BALR  @14,@15                                             0240
*           ERRSW=ON;               /* ERROR INDICATOR               */
         OI    ERRSW,B'01000000'                                   0241
*         END;                      /* END ERROR ACCESSING           */
*       ELSE                        /* FOUND LAST ENTRY ADDRESS      */
*         DO;                                                      0243
         B     @RC00237                                            0243
@RF00237 DS    0H                                                  0244
*           ENTRYMAX=DATAWORD;      /* SAVE LAST ADDRESS             */
         L     ENTRYMAX,DATAWORD(,DATAPTR)                         0244
*           VADR=ADDR(FRRSEMP);     /* FIRST POSSIBLE ADDRESS        */
         L     VADR,FRRSPTR                                        0245
*           CALL ACCESRTN;          /* ACCESS IT                     */
         BAL   @14,ACCESRTN                                        0246
*           IF RCREG^=ZERO THEN     /* ERROR ACCESSING FIRST POSSIBLE
*                                      ADR                           */
         LTR   RCREG,RCREG                                         0247
         BZ    @RF00247                                            0247
*             DO;                                                  0248
*               BUFFER(1:LENGTH(FRRSFMT))=FRRSFMT;/* ERROR MSG       */
         L     @14,ADPLBUF(,ABDPLPTR)                              0249
         MVC   BUFFER(120,@14),FRRSFMT                             0249
*               CALL PRINT;         /* PRINT IT                      */
         L     @15,ADPLPRNT(,ABDPLPTR)                             0250
         BALR  @14,@15                                             0250
*               ERRSW=ON;           /* ERROR INDICATOR               */
         OI    ERRSW,B'01000000'                                   0251
*             END;                  /* ERROR ACCESSING               */
*           ELSE                    /* FOUND FIRST POSSIBLE ADDRESS  */
*             DO;                                                  0253
         B     @RC00247                                            0253
@RF00247 DS    0H                                                  0254
*               ENTRYMAX=ENTRYMAX-DATAWORD;/* LENGTH OF SPACE FOR  0254
*                                      ENTRIES                       */
         SL    ENTRYMAX,DATAWORD(,DATAPTR)                         0254
*               VADR=ADDR(FRRSELEN);/* LENGTH OF EACH ENTRY          */
         L     @14,FRRSPTR                                         0255
         LA    VADR,FRRSELEN(,@14)                                 0255
*               CALL ACCESRTN;      /* ACCESS IT                     */
         BAL   @14,ACCESRTN                                        0256
*               IF RCREG^=ZERO THEN /* ERROR ACCESSING LENGTH        */
         LTR   RCREG,RCREG                                         0257
         BZ    @RF00257                                            0257
*                 DO;                                              0258
*                   BUFFER(1:LENGTH(FRRSFMT))=FRRSFMT;/* ERROR MSG   */
         L     @14,ADPLBUF(,ABDPLPTR)                              0259
         MVC   BUFFER(120,@14),FRRSFMT                             0259
*                   CALL PRINT;     /* PRINT IT                      */
         L     @15,ADPLPRNT(,ABDPLPTR)                             0260
         BALR  @14,@15                                             0260
*                   ERRSW=ON;       /* ERROR INDICATOR               */
         OI    ERRSW,B'01000000'                                   0261
*                 END;              /* END ACCESS ERROR              */
*               ELSE                /* FOUND ENTRY LENGTH            */
*                 IF DATAWORD=ZERO THEN/* TEST FOR ZERO STACK      0263
*                                                            @ZA27290*/
         B     @RC00257                                            0263
@RF00257 ICM   @14,15,DATAWORD(DATAPTR)                            0263
         BNZ   @RF00263                                            0263
*                   DO;                                            0264
*                     BUFFER(1:LENGTH(FRRLZERO))=FRRLZERO;/* ERROR 0265
*                                      MSG                   @ZA27290*/
         L     @14,ADPLBUF(,ABDPLPTR)                              0265
         MVC   BUFFER(64,@14),FRRLZERO                             0265
*                     CALL PRINT;   /* PRINT IT              @ZA27290*/
         L     @15,ADPLPRNT(,ABDPLPTR)                             0266
         BALR  @14,@15                                             0266
*                     ERRSW=ON;     /* SET UP FOR ERROR EXIT @ZA27290*/
         OI    ERRSW,B'01000000'                                   0267
*                   END;            /* END.... ERROR RTN     @ZA27290*/
*                 ELSE              /* STACK NOT ZERO        @ZA27290*/
*                   DO;             /*                       @ZA27290*/
         B     @RC00263                                            0269
@RF00263 DS    0H                                                  0270
*                     ENTRYMAX=ENTRYMAX/DATAWORD;/* NUMBER OF ENTRIES*/
         LR    @02,ENTRYMAX                                        0270
         SRDA  @02,32                                              0270
         D     @02,DATAWORD(,DATAPTR)                              0270
         LR    ENTRYMAX,@03                                        0270
*                     IF(ENTRYMAX>16)×(ENTRYMAX<1) THEN/* SOMETHING
*                                      WRONG WITH VALUES FOUND       */
         CH    ENTRYMAX,@CH00718                                   0271
         BH    @RT00271                                            0271
         LTR   ENTRYMAX,ENTRYMAX                                   0271
         BP    @RF00271                                            0271
@RT00271 DS    0H                                                  0272
*                       DO;                                        0272
*                         BUFFER(1:LENGTH(BADMAX))=BADMAX;/* ERROR 0273
*                                      MSG                           */
         L     @14,ADPLBUF(,ABDPLPTR)                              0273
         MVC   BUFFER(72,@14),BADMAX                               0273
*                         CALL PRINT;/* PRINT IT                     */
         L     @15,ADPLPRNT(,ABDPLPTR)                             0274
         BALR  @14,@15                                             0274
*                         ERRSW=ON; /* ERROR INDICATOR               */
         OI    ERRSW,B'01000000'                                   0275
*                         RCCODE=4; /* INHIBIT EED SEARCHING IN MAIN
*                                      RTN                           */
         MVC   RCCODE(4),@CF00080                                  0276
*                       END;        /* END WRONG VALUES              */
*                   END;            /* FOUND ENTRY LENGTH            */
@RF00271 DS    0H                                                  0279
*             END;                  /* END FOUND FIRST ADR           */
@RC00263 DS    0H                                                  0279
@RC00257 DS    0H                                                  0280
*         END;                      /* END FOUND LAST ADR            */
@RC00247 DS    0H                                                  0281
*     END;                          /* END DYNAMICALLY FIGURE ENTRIES*/
@RC00237 DS    0H                                                  0282
*   IF ERRSW=OFF THEN               /* FIGURED NUM ENTRIES OK        */
@RC00232 TM    ERRSW,B'01000000'                                   0282
         BNZ   @RF00282                                            0282
*     DO;                                                          0283
*                                                                  0284
*       /*************************************************************/
*       /*                                                           */
*       /* CONTINUE PROCESSING AT RT1W                               */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0284
*       BUFFER(16:(LENGTH(RT1MSG)+15))=RT1MSG;/* RT1W TO FOLLOW    0284
*                                      MESSAGE                       */
         L     @14,ADPLBUF(,ABDPLPTR)                              0284
         MVC   BUFFER+15(38,@14),RT1MSG                            0284
*       CALL CONVERT;               /* GENERATE OFFSET               */
         BAL   @14,CONVERT                                         0285
*       OFFSET=CONVERTD;            /* FILL IN OFFSET                */
         L     @14,ADPLBUF(,ABDPLPTR)                              0286
         MVC   OFFSET(6,@14),CONVERTD                              0286
*       CALL PRINT;                 /* PRINT TITLE                   */
         L     @15,ADPLPRNT(,ABDPLPTR)                             0287
         BALR  @14,@15                                             0287
*       CALL PRINT;                 /* BLANK                         */
         L     @15,ADPLPRNT(,ABDPLPTR)                             0288
         BALR  @14,@15                                             0288
*       BUFFER(31:(LENGTH(FRREHDR)+30))=FRREHDR;/* FRR ENTRY TITLE   */
         L     @14,ADPLBUF(,ABDPLPTR)                              0289
         MVC   BUFFER+30(11,@14),FRREHDR                           0289
*       CALL PRINT;                 /* PRINT IT                      */
         L     @15,ADPLPRNT(,ABDPLPTR)                             0290
         BALR  @14,@15                                             0290
*       BUFFER(31:(LENGTH(FRREUS)+30))=FRREUS;/* UNDERSCORE          */
         L     @14,ADPLBUF(,ABDPLPTR)                              0291
         MVC   BUFFER+30(11,@14),FRREUS                            0291
*       CALL PRINT;                 /* PRINT IT                      */
         L     @15,ADPLPRNT(,ABDPLPTR)                             0292
         BALR  @14,@15                                             0292
*       CALL PRINT;                 /* BLANK                         */
*                                                                  0293
         L     @15,ADPLPRNT(,ABDPLPTR)                             0293
         BALR  @14,@15                                             0293
*       /*************************************************************/
*       /*                                                           */
*       /* SINCE THE RT1W IS TO BE FORMATTED AFTER THE FRR ENTRIES,  */
*       /* INCREMENT THE CONTROL BLOCK ADDRESS TO FRR ENTRIES.       */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0294
*       ADPLFMT2=ADPLFMT2+LENGTH(RT1W);/* INCREMENT AROUND RT1W      */
         LA    @14,68                                              0294
         AL    @14,ADPLFMT2(,ABDPLPTR)                             0294
         ST    @14,ADPLFMT2(,ABDPLPTR)                             0294
*       ARRAYCNT=ARRAYCNT+1;        /* NEXT FORMAT PATTERN           */
         LA    @14,1                                               0295
         ALR   ARRAYCNT,@14                                        0295
*       DO LOOPCNTR=1 TO ENTRYMAX WHILE(ERRSW=OFF);/* FORMAT LOOP    */
         B     @DE00296                                            0296
@DL00296 TM    ERRSW,B'01000000'                                   0296
         BNZ   @DC00296                                            0296
*         LINEPTR=FRRNAMES(ARRAYCNT);/* FORMAT PATTERN               */
         LR    @14,ARRAYCNT                                        0297
         SLA   @14,2                                               0297
         L     LINEPTR,FRRNAMES-4(@14)                             0297
*         CALL CONVERT;             /* OFFSET GENERATOR              */
         BAL   @14,CONVERT                                         0298
*         CALL FMTRTN;              /* FORMAT                        */
         BAL   @14,FMTRTN                                          0299
*       END;                        /* END LOOP                      */
         LA    @14,1                                               0300
         AL    @14,LOOPCNTR                                        0300
@DE00296 ST    @14,LOOPCNTR                                        0300
         CR    @14,ENTRYMAX                                        0300
         BNH   @DL00296                                            0300
@DC00296 DS    0H                                                  0301
*       CALL PRINT;                 /* 2 BLANKS                      */
         L     @15,ADPLPRNT(,ABDPLPTR)                             0301
         BALR  @14,@15                                             0301
*       CALL PRINT;                 /* BLANK                         */
*                                                                  0302
         L     @15,ADPLPRNT(,ABDPLPTR)                             0302
         BALR  @14,@15                                             0302
*       /*************************************************************/
*       /*                                                           */
*       /* FRR ENTRIES FORMATTED OK NOW FORMAT THE RT1 WORK AREA     */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0303
*       IF ERRSW=OFF THEN           /* OK SO FAR                     */
         TM    ERRSW,B'01000000'                                   0303
         BNZ   @RF00303                                            0303
*         DO;                                                      0304
*           BUFFER(31:(LENGTH(RT1HDR)+30))=RT1HDR;/* RT1W TITLE      */
         L     @14,ADPLBUF(,ABDPLPTR)                              0305
         MVC   BUFFER+30(14,@14),RT1HDR                            0305
*           CALL PRINT;             /* PRINT IT                      */
         L     @15,ADPLPRNT(,ABDPLPTR)                             0306
         BALR  @14,@15                                             0306
*           BUFFER(31:(LENGTH(RT1US)+30))=RT1US;/* UNDERSCORE        */
         L     @14,ADPLBUF(,ABDPLPTR)                              0307
         MVC   BUFFER+30(14,@14),RT1US                             0307
*           CALL PRINT;             /* PRINT IT                      */
         L     @15,ADPLPRNT(,ABDPLPTR)                             0308
         BALR  @14,@15                                             0308
*           CALL PRINT;             /* BLANK                         */
         L     @15,ADPLPRNT(,ABDPLPTR)                             0309
         BALR  @14,@15                                             0309
*           VADR=ADDR(RT1TLPN);     /* LOGICAL PHASE #               */
         L     VADR,RT1PTR                                         0310
*           CALL ACCESRTN;          /* ACCESS                        */
         BAL   @14,ACCESRTN                                        0311
*           IF RCREG=ZERO THEN      /* COULD ACCESS LPN              */
         LTR   RCREG,RCREG                                         0312
         BNZ   @RF00312                                            0312
*             DO;                                                  0313
*               SAVEPTR=RT1PTR;     /* START OFFSETS AT 0            */
         L     @14,RT1PTR                                          0314
         ST    @14,SAVEPTR                                         0314
*               ADPLFMT2=RT1PTR;    /* BEGINNING OF RT1W             */
         ST    @14,ADPLFMT2(,ABDPLPTR)                             0315
*               IF DATABYTE=ZEROBYTE THEN/* UNPREDICTABLE RT1W       */
         CLC   DATABYTE(1,DATAPTR),ZEROBYTE                        0316
         BNE   @RF00316                                            0316
*                 DO;                                              0317
*                   BUFFER(8:(LENGTH(RT1WARN)+7))=RT1WARN;/* WARNING
*                                      MSG                           */
         L     @14,ADPLBUF(,ABDPLPTR)                              0318
         MVC   BUFFER+7(49,@14),RT1WARN                            0318
*                   CALL PRINT;     /* PRINT IT                      */
*                                                                  0319
         L     @15,ADPLPRNT(,ABDPLPTR)                             0319
         BALR  @14,@15                                             0319
*                   /*************************************************/
*                   /*                                               */
*                   /* FORMAT RT1W WITH ONLY THE OFFSETS             */
*                   /*                                               */
*                   /*************************************************/
*                                                                  0320
*                   DO ARRAYCNT=1 TO DIM(RT1ENAME) WHILE(ERRSW=OFF);
         LA    ARRAYCNT,1                                          0320
@DL00320 TM    ERRSW,B'01000000'                                   0320
         BNZ   @DC00320                                            0320
*                                   /* FORMAT LOOP                   */
*                     LINEPTR=RT1ENAME(ARRAYCNT);/* FORMAT PATTERN   */
         LR    @14,ARRAYCNT                                        0321
         SLA   @14,2                                               0321
         L     LINEPTR,RT1ENAME-4(@14)                             0321
*                     CALL CONVERT; /* OFFSET GENERATOR              */
         BAL   @14,CONVERT                                         0322
*                     CALL FMTRTN;  /* FORMAT                        */
         BAL   @14,FMTRTN                                          0323
*                   END;            /* END FORMAT LOOP               */
         AH    ARRAYCNT,@CH00041                                   0324
         CH    ARRAYCNT,@CH00080                                   0324
         BNH   @DL00320                                            0324
@DC00320 DS    0H                                                  0325
*                   RCCODE=4;       /* INHIBIT EED SEARCH IN MAIN RTN*/
         MVC   RCCODE(4),@CF00080                                  0325
*                 END;              /* END UNPREDICTABLE RT1W        */
*               ELSE                /* NORMAL RT1W                   */
*                 DO;                                              0327
         B     @RC00316                                            0327
@RF00316 DS    0H                                                  0328
*                   LPN=DATABYTE;   /* SAVE LOGICAL PHASE NUM        */
*                                                                  0328
         SLR   LPN,LPN                                             0328
         IC    LPN,DATABYTE(,DATAPTR)                              0328
*                   /*************************************************/
*                   /*                                               */
*                   /* FORMAT THE RT1W WITH ALL THE HEADINGS         */
*                   /*                                               */
*                   /*************************************************/
*                                                                  0329
*                   DO ARRAYCNT=1 TO DIM(RT1WNAME) WHILE(ERRSW=OFF);
         LA    ARRAYCNT,1                                          0329
@DL00329 TM    ERRSW,B'01000000'                                   0329
         BNZ   @DC00329                                            0329
*                                   /* FORMAT LOOP                   */
*                     LINEPTR=RT1WNAME(ARRAYCNT);                  0330
         LR    @14,ARRAYCNT                                        0330
         SLA   @14,2                                               0330
         L     LINEPTR,RT1WNAME-4(@14)                             0330
*                     CALL CONVERT;                                0331
         BAL   @14,CONVERT                                         0331
*                     CALL FMTRTN;                                 0332
         BAL   @14,FMTRTN                                          0332
*                   END;            /* END FORMAT LOOP               */
         AH    ARRAYCNT,@CH00041                                   0333
         CH    ARRAYCNT,@CH00080                                   0333
         BNH   @DL00329                                            0333
@DC00329 DS    0H                                                  0334
*                   IF ERRSW=OFF THEN/* RT1W FORMATTED FINE          */
         TM    ERRSW,B'01000000'                                   0334
         BNZ   @RF00334                                            0334
*                     DO;                                          0335
*                       CALL RT1BITS;/* CALL BIT SUMMARY             */
*                                                                  0336
         BAL   @14,RT1BITS                                         0336
*                       /*********************************************/
*                       /*                                           */
*                       /* IF ALL PARTS FORMATTED WITHOUT PROBLEM,   */
*                       /* SEND THE ADDRESS OF THE EED CHAIN HEADER  */
*                       /* TO THE MAINLINE CODE                      */
*                       /*                                           */
*                       /*********************************************/
*                                                                  0337
*                       VADR=ADDR(RT1WEED);/* GET ADDR OF EED QUEUE  */
         L     @14,RT1PTR                                          0337
         LA    VADR,RT1WEED(,@14)                                  0337
*                       CALL ACCESRTN;/* ACCESS IT                   */
         BAL   @14,ACCESRTN                                        0338
*                       IF RCREG=ZERO THEN/* FOUND EED QUEUE OK      */
         SLR   @14,@14                                             0339
         CR    RCREG,@14                                           0339
         BNE   @RF00339                                            0339
*                         DO;                                      0340
*                           ADPLFMT2=DATAWORD;/* FILL IN PARMLIST    */
         MVC   ADPLFMT2(4,ABDPLPTR),DATAWORD(DATAPTR)              0341
*                           START=ZERO;                            0342
         ST    @14,START(,ABDPLPTR)                                0342
*                         END;      /* END FOUND EED QUEUE OK        */
*                       ELSE                                       0344
*                         ;         /* DID NOT FIND EED QUEUE        */
@RF00339 DS    0H                                                  0345
*                     END;          /* END RT1W FORMATTED OK         */
*                   ELSE                                           0346
*                     ;             /* ERROR ABOVE IN RT1W           */
@RF00334 DS    0H                                                  0347
*                 END;              /* END NORMAL RT1W               */
*             END;                  /* END COULD ACCESS LPN          */
*           ELSE                                                   0349
*             ;                     /* COULD NOT ACCESS LPN          */
@RF00312 DS    0H                                                  0350
*         END;                      /* END FORMATTED FRR ENTRIES OK  */
*       ELSE                                                       0351
*         ;                         /* FRR ENTRIES NOT FORMATTED OK  */
@RF00303 DS    0H                                                  0352
*     END;                          /* END FIGURED ENTRIES OK        */
*   ELSE                                                           0353
*     ;                             /* ERROR FIGURING FRR ENTRIES    */
@RF00282 DS    0H                                                  0354
*   RETURN CODE(RCCODE);                                           0354
@RC00282 L     @15,RCCODE                                          0354
@EL00002 DS    0H                                                  0354
@EF00002 DS    0H                                                  0354
@ER00002 L     @14,@SA00002                                        0354
         BR    @14                                                 0354
*   END RT1RTN;                                                    0355
*RT1BITS:                                                          0356
*   PROC OPTIONS(SAVE(14));                                        0356
*                                                                  0356
RT1BITS  ST    @14,@SA00003                                        0356
*/****************************************************************** */
*/* FUNCTION -                                                     0357
*      ROUTINE TO SUMMARIZE THE BITS FOR THE RT1W WORK AREA.  IT   0357
*      LOOKS FOR ANY OF THE BIT FLAGS BEING ON AND OUTPUTS THE     0357
*      SUMMARY HEADER AND THE BIT SUMMARY.  IF NO FLAGS ARE ON,    0357
*      NO TITLE NOR ANY MESSAGES WILL BE PRINTED.                  0357
*                                                                  0357
*   INPUT -                                                        0357
*      THE RT1W POINTER MUST HAVE THE ADDRESS OF THE RT1W CONTROL  0357
*      BLOCK IN IT AND REGISTER 1 MUST POINT TO THE PARAMETER LIST 0357
*      PASSED BY THE CALLER.                                       0357
*                                                                  0357
*   CALLS -                                                        0357
*      ACCESRTN, BYTEMODE, CONVERT, ENTRYRT1, AND PRINT            0357
*                                                                  0357
*   CALLED BY -                                                    0357
*      IEAFTFRR TO PUT OUT THE BIT FLAG SUMMARY FOR RT1W           0357
*                                                                  0357
*    RETURNS TO -                                                  0357
*      IEAFTFRR WITH RETURN CODE SET BY ACCESS SERVICE             0357
*      ROUTINES.                                                     */
*/********************************************************************/
*                                                                  0357
*   CALL PRINT;                     /* PRINT BLANK LINE              */
         L     @15,ADPLPRNT(,ABDPLPTR)                             0357
         BALR  @14,@15                                             0357
*   CALL PRINT;                     /* PRINT BLANK LINE              */
         L     @15,ADPLPRNT(,ABDPLPTR)                             0358
         BALR  @14,@15                                             0358
*   BUFFER(31:(LENGTH(RT1SUM)+30))=RT1SUM;/* SUMMARY TITLE           */
         L     @14,ADPLBUF(,ABDPLPTR)                              0359
         MVC   BUFFER+30(21,@14),RT1SUM                            0359
*   CALL PRINT;                     /* PRINT IT                      */
         L     @15,ADPLPRNT(,ABDPLPTR)                             0360
         BALR  @14,@15                                             0360
*   BUFFER(31:(LENGTH(RT1SUS)+30))=RT1SUS;/* UNDERSCORE              */
         L     @14,ADPLBUF(,ABDPLPTR)                              0361
         MVC   BUFFER+30(21,@14),RT1SUS                            0361
*   CALL PRINT;                     /* PRINT IT                      */
         L     @15,ADPLPRNT(,ABDPLPTR)                             0362
         BALR  @14,@15                                             0362
*   CALL PRINT;                     /* BLANK                         */
         L     @15,ADPLPRNT(,ABDPLPTR)                             0363
         BALR  @14,@15                                             0363
*   CALL PRINT;                     /* BLANK                         */
*                                                                  0364
         L     @15,ADPLPRNT(,ABDPLPTR)                             0364
         BALR  @14,@15                                             0364
*   /*****************************************************************/
*   /*                                                               */
*   /* ANALYZE LOGICAL PHASE NUMBER                                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0365
*   ADPLFMT2=ADDR(RT1TLPN);                                        0365
         MVC   ADPLFMT2(4,ABDPLPTR),RT1PTR                         0365
*   CALL CONVERT;                   /* GENERATE OFFSET               */
         BAL   @14,CONVERT                                         0366
*   OFFSET=CONVERTD;                /* FILL IN GENERATED OFFSET      */
         L     @14,ADPLBUF(,ABDPLPTR)                              0367
         MVC   OFFSET(6,@14),CONVERTD                              0367
*   BUFFER(8:(LENGTH(LPNSTART)+7))=LPNSTART;/* BEGINNING OF MSG      */
         MVC   BUFFER+7(24,@14),LPNSTART                           0368
*   PACKAREA(4:4)=LPN;              /* PLACE LOGICAL PHASE NUM IN  0369
*                                      CHAR AREA                     */
         STC   LPN,PACKAREA+3                                      0369
*   GARBAGE='04'X;                  /* GARBAGE BYTE FOR THE UNPK     */
         MVI   GARBAGE,X'04'                                       0370
*   UNPK(BUFFER(LENGTH(LPNSTART)+8:LENGTH(LPNSTART)+10),UNPACK(4:5));
