         TITLE 'IEAFTRT2-RTM2WA-ESA-SDWA FORMAT MODULE                 *
                        '
IEAFTRT2 CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL2(@EP00001-@MAINENT)
         DC    AL1(16)                                             0001
         DC    C'IEAFTRT2  77.091'                                 0001
IEAFTESA DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL2(@EP00536-IEAFTESA)
         ENTRY IEAFTESA
IEAFTSDW DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL2(@EP00636-IEAFTSDW)
         ENTRY IEAFTSDW
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @10,0                                               0001
@PSTART  LA    @11,4095(,@10)                                      0001
         LA    @12,4095(,@11)                                      0001
         USING @PSTART,@10                                         0001
         USING @PSTART+4095,@11                                    0001
         USING @PSTART+8190,@12                                    0001
         L     @00,@SIZDATD                                        0001
         GETMAIN  R,LV=(0)
         LR    @09,@01                                             0001
         USING @DATD,@09                                           0001
         ST    @13,@SA00001+4                                      0001
         LM    @15,@01,16(@13)                                     0001
         ST    @09,8(,@13)                                         0001
         LR    @13,@09                                             0001
         AH    @15,4(,@15)                                         0001
         BR    @15                                                 0001
@EP00001 DS    0H                                                  0002
RBPRFXLN EQU 64
RBPRFLNA EQU 32
PRBLEN   EQU 136
SIRBLEN  EQU 200
TIRBLEN  EQU 136
IRBLEN   EQU 128
SVRBLEN  EQU 224
*                                                                  0189
*/********************************************************************/
*/* FUNCTION -                                                     0189
*      ROUTINE WHICH FORMATS THE RTM2 WORK AREA.                   0189
*      IT SETS UP THE NEEDED POINTERS TO BE PASSED TO THE FORMAT   0189
*      ROUTINE SENT IN THE PARAMETER LIST.  FOR ANY ERROR ACCESSING
*      A NEEDED FIELD IN THE RTM2WA OR FOR NON-ZERO RETURN CODE    0189
*      FROM THE FORMATTING SERVICE, THE FOLLOWING MESSAGE WILL BE  0189
*      PRINTED:                                                    0189
*               *** FORMATTING SERVICE ROUTINE ERROR DURING        0189
*               RTM2WA FORMATTING(POSSIBLE CAUSE:  DATA NOT IN     0189
*               DUMP) ***                                          0189
*      AT THIS POINT PROCESSING WOULD TERMINATE. IF THE RTM2WA IS  0189
*      FORMATTED WITHOUT ERROR, A SUMMARY OF THE RTM2WA IMPORTANT  0189
*      AREA IS PRINTED. IF THIS COMPLETES SUCCESSFULLY, A BIT SUMMARY
*      IS PRINTED USING THE ANALYZE PROC.                          0189
*                                                                  0189
*   INPUT -                                                        0189
*      REGISTER 1 POINTS TO THE PARMLIST WHICH CONTAINS THE        0189
*      ADDRESS OF THE CONTROL BLOCK TO BE FORMATTED IN ADPLFMT2.   0189
*      ADPLFMT1 ALSO DEFINES AS START, CONTAINS THE OFFSET TO BE   0189
*      PLACED ALONG SIDE THE FIRST FIELD IN THE CONTROL BLOCK, AND 0189
*      ADPLCOM1 CONTAINS THE ADDRESS OF THE ESTAE PARAMETER LIST.  0189
*                                                                  0189
*   CALLS -                                                        0189
*      CALLS ACCESRTN, CONVERT, FMTRTN, PRINT, RT2SUM, AND SPECIAL 0189
*                                                                  0189
*   CALLED BY -                                                    0189
*      IEAVTFMT TO FORMAT RTM2WA                                   0189
*                                                                  0189
*   RETURNS TO -                                                   0189
*      IEAVTFMT WITH RETURN CODE SET BY ACCESS OR FORMAT           0189
*      SERVICE ROUTINES                                              */
*/********************************************************************/
*                                                                  0189
*   ESTAPTR=ADPLCOM1;               /* SAVE PTR TO ESTAE PARM LIST   */
         L     @14,ADPLCOM1(,ABDPLPTR)                             0189
         ST    @14,ESTAPTR                                         0189
*   STM(VADR,RCREG,LCALREGS);       /* SAVE REGS FOR RETRY           */
         STM   VADR,RCREG,LCALREGS(@14)                            0190
*   ADPLCOM1=ADDR(RT2FMT);          /* FORMAT ERROR MSG              */
         LA    @14,RT2FMT                                          0191
         ST    @14,ADPLCOM1(,ABDPLPTR)                             0191
*   RTM2PTR=ADPLFMT2;               /* PTR TO RTM2 WORK AREA         */
         L     @14,ADPLFMT2(,ABDPLPTR)                             0192
         LR    RTM2PTR,@14                                         0192
*   SAVEPTR=ADPLFMT2-START;         /* VALUE FOR OFFSET              */
         SL    @14,START(,ABDPLPTR)                                0193
         ST    @14,SAVEPTR                                         0193
*   BITS=BITS&&BITS;                /* TURN OFF THE SUBIT FLAGS AND
*                                      ERROR                   SWITCH*/
*                                                                  0194
         XC    BITS(1),BITS                                        0194
*   /*****************************************************************/
*   /*                                                               */
*   /* IN RELEASE 2, THE POINTER TO THE SUBITS WAS NOT DEFINED.      */
*   /* THEREFORE, BEFORE ACCESSING THE SUBIT POINTER, CHECK FOR      */
*   /* RELEASE 2                                                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0195
*   VADR=ADDR(CVTNUMB);             /* RELEASE NUMBER                */
         L     @14,CVTPTR(,ABDPLPTR)                               0195
         AL    @14,@CF03887                                        0195
         LR    VADR,@14                                            0195
*   CALL ACCESRTN;                  /* ACCESS IT                     */
         BAL   @14,ACCESRTN                                        0196
*   IF RCREG^=ZERO THEN             /* ACCESS ERROR                  */
         LTR   RCREG,RCREG                                         0197
         BZ    @RF00197                                            0197
*     BUFFER(1:LENGTH(RT2FMT))=RT2FMT;                             0198
         L     @14,ADPLBUF(,ABDPLPTR)                              0198
         MVC   BUFFER(120,@14),RT2FMT                              0198
*   ELSE                            /* FOUND RELEASE NUMBER          */
*     DO;                                                          0199
         B     @RC00197                                            0199
@RF00197 DS    0H                                                  0200
*       IF DATAHALF^=REL2 THEN      /* NOT A RELEASE 2 SYSTEM        */
         CLC   DATAHALF(2,DATAPTR),REL2                            0200
         BE    @RF00200                                            0200
*         DO;                                                      0201
*                                                                  0201
*           /*********************************************************/
*           /*                                                       */
*           /* FIND WHICH SUS ARE ON THE SYSTEM                      */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0202
*           VADR=ADDR(CVTIHASU);    /* SU BIT PTR                    */
         L     @14,CVTPTR(,ABDPLPTR)                               0202
         LA    VADR,CVTIHASU(,@14)                                 0202
*           CALL ACCESRTN;          /* ACCESS IT                     */
         BAL   @14,ACCESRTN                                        0203
*           IF RCREG^=ZERO THEN     /* ACCESS ERROR                  */
         LTR   RCREG,RCREG                                         0204
         BZ    @RF00204                                            0204
*             DO;                                                  0205
*               BUFFER(1:LENGTH(RT2FMT))=RT2FMT;/* ERROR MSG         */
         L     @14,ADPLBUF(,ABDPLPTR)                              0206
         MVC   BUFFER(120,@14),RT2FMT                              0206
*               ERRSW=ON;           /* INDICATE ACCESS ERROR         */
         OI    ERRSW,B'00100000'                                   0207
*             END;                  /* END ACCESS ERROR              */
*           ELSE                                                   0209
*             DO;                   /* FOUND SU BIT PTR              */
         B     @RC00204                                            0209
@RF00204 DS    0H                                                  0210
*               IF DATAWORD^=ZERO THEN/* IF PTR TO SUBITS NOT ZERO-SU
*                                                              SYSTEM*/
         L     @14,DATAWORD(,DATAPTR)                              0210
         LTR   @14,@14                                             0210
         BZ    @RF00210                                            0210
*                 DO;                                              0211
*                   VADR=DATAWORD;  /* SU BITS                       */
         LR    VADR,@14                                            0212
*                   CALL ACCESRTN;  /* ACCESS BITS-THEY ARE ON A   0213
*                                      DWORD BDY SO 2 WORDS CAN BE 0213
*                                      GUARENTEED                    */
         BAL   @14,ACCESRTN                                        0213
*                   IF RCREG^=ZERO THEN/* ACCESS ERROR               */
         LTR   RCREG,RCREG                                         0214
         BZ    @RF00214                                            0214
*                     DO;                                          0215
*                       BUFFER(1:LENGTH(RT2FMT))=RT2FMT;           0216
         L     @14,ADPLBUF(,ABDPLPTR)                              0216
         MVC   BUFFER(120,@14),RT2FMT                              0216
*                       ERRSW=ON;   /* ERROR INDICATOR TO STOP     0217
*                                      PROCESSING                    */
         OI    ERRSW,B'00100000'                                   0217
*                     END;          /* END ACCESS ERROR              */
*                   ELSE                                           0219
*                     DO;           /* FOUND SU BIT STRING           */
         B     @RC00214                                            0219
@RF00214 DS    0H                                                  0220
*                       IF SU17BIT=ON THEN                         0220
         TM    SU17BIT(DATAPTR),B'01000000'                        0220
         BNO   @RF00220                                            0220
*                         SU17SAVE=ON;/* SU 17 ON SYSTEM             */
         OI    SU17SAVE,B'10000000'                                0221
*                       ELSE                                       0222
*                         ;         /* SU17 NOT ON SYSTEM            */
@RF00220 DS    0H                                                  0223
*                       IF SU33BIT=ON THEN                         0223
         TM    SU33BIT(DATAPTR),B'01000000'                        0223
         BNO   @RF00223                                            0223
*                         SU33SAVE=ON;/* SU 33 ON SYSTEM             */
         OI    SU33SAVE,B'01000000'                                0224
*                       ELSE                                       0225
*                         ;         /* SU33 NOT ON SYSTEM            */
@RF00223 DS    0H                                                  0226
*                     END;          /* END SU BIT STRING FOUND       */
*                 END;              /* END SU SYSTEM                 */
*               ELSE                                               0228
*                 ;                 /* PRE SU SYSTEM-NO SU BITS TO 0228
*                                      SET                           */
@RF00210 DS    0H                                                  0229
*             END;                  /* END FOUND SUBIT POINTER       */
*         END;                      /* END NOT A RELEASE 2 SYSTEM    */
*       ELSE                                                       0231
*         ;                         /* IS A RELEASE 2 SYSTEM, NO SUS */
@RF00200 DS    0H                                                  0232
*       IF ERRSW=OFF THEN           /* CONTINUE IF FOUND SYSTEM OK   */
@RC00200 TM    ERRSW,B'00100000'                                   0232
         BNZ   @RF00232                                            0232
*         DO;                                                      0233
*           BUFFER(1:LENGTH(RT2HDR))=RT2HDR;/* TITLE                 */
         L     @14,ADPLBUF(,ABDPLPTR)                              0234
         MVC   BUFFER(54,@14),RT2HDR                               0234
*           PACKAREA=RTM2PTR;       /* PLACE PTR INTO CHAR AREA      */
         ST    RTM2PTR,PACKAREA                                    0235
*           GARBAGE='04'X;          /* GARBAGE BYTE FOR THE UNPK     */
         MVI   GARBAGE,X'04'                                       0236
*           UNPK(ADDRESS,UNPACK);   /* PLACE IN BUFFER               */
         UNPK  ADDRESS(9,@14),UNPACK(5)                            0237
*           TR(ADDRESS,TRANS);      /* TRANSLATE TO PRINTABLE HEX    */
         TR    ADDRESS(9,@14),TRANS                                0238
*           CALL PRINT;             /* PRINT IT                      */
         L     @15,ADPLPRNT(,ABDPLPTR)                             0239
         BALR  @14,@15                                             0239
*           BUFFER(1:LENGTH(RT2US))=RT2US;/* UNDERSCORE              */
         L     @14,ADPLBUF(,ABDPLPTR)                              0240
         MVC   BUFFER(7,@14),RT2US                                 0240
*           CALL PRINT;             /* PRINT IT                      */
         L     @15,ADPLPRNT(,ABDPLPTR)                             0241
         BALR  @14,@15                                             0241
*           CALL PRINT;             /* BLANK                         */
*                                                                  0242
         L     @15,ADPLPRNT(,ABDPLPTR)                             0242
         BALR  @14,@15                                             0242
*           /*********************************************************/
*           /*                                                       */
*           /* FORMAT RTM2WA                                         */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0243
*           DO ARRAYCNT=1 TO ARRAYMAX WHILE(ERRSW=OFF);/* FORMAT LOOP*/
         LA    ARRAYCNT,1                                          0243
@DL00243 TM    ERRSW,B'00100000'                                   0243
         BNZ   @DC00243                                            0243
*             IF RT2ARRAY(ARRAYCNT)>=SPEC THEN                     0244
         LR    @14,ARRAYCNT                                        0244
         SLA   @14,2                                               0244
         L     @14,RT2ARRAY-4(@14)                                 0244
         CH    @14,@CH01675                                        0244
         BL    @RF00244                                            0244
*               CALL SPECIAL;       /* ROUTINE TO DIFFERENCIATE SU'S */
         BAL   @14,SPECIAL                                         0245
*             ELSE                                                 0246
*               IF RT2ARRAY(ARRAYCNT)=BLANK THEN                   0246
         B     @RC00244                                            0246
@RF00244 LR    @14,ARRAYCNT                                        0246
         SLA   @14,2                                               0246
         L     @14,RT2ARRAY-4(@14)                                 0246
         CH    @14,@CH02753                                        0246
         BNE   @RF00246                                            0246
*                 CALL PRINT;       /* PRINT A BLANK LINE            */
         L     @15,ADPLPRNT(,ABDPLPTR)                             0247
         BALR  @14,@15                                             0247
*               ELSE                                               0248
*                 DO;               /* LINE TO BE OUTPUT             */
         B     @RC00246                                            0248
@RF00246 DS    0H                                                  0249
*                   LINEPTR=RTM2NAME(RT2ARRAY(ARRAYCNT));/* FORMAT 0249
*                                      PATTERN PTR                   */
         LR    @14,ARRAYCNT                                        0249
         SLA   @14,2                                               0249
         L     @14,RT2ARRAY-4(@14)                                 0249
         LR    @07,@14                                             0249
         SLA   @07,2                                               0249
         L     LINEPTR,RTM2NAME-4(@07)                             0249
*                   IF RT2ARRAY(ARRAYCNT)>=9 THEN/* TITLE LINE OR  0250
*                                      HEADER TEXT                   */
         CH    @14,@CH01564                                        0250
         BL    @RF00250                                            0250
*                     DO;                                          0251
*                       BUFFER=TITLE;/* PUT IN BUFFER                */
         L     @14,ADPLBUF(,ABDPLPTR)                              0252
         MVI   BUFFER+31(@14),C' '                                 0252
         MVC   BUFFER+32(88,@14),BUFFER+31(@14)                    0252
         LR    @07,LINEPTR                                         0252
         MVC   BUFFER(31,@14),TITLE(@07)                           0252
*                       CALL PRINT; /* PRINT IT                      */
         L     @15,ADPLPRNT(,ABDPLPTR)                             0253
         BALR  @14,@15                                             0253
*                     END;          /* END A TITLE LINE              */
*                   ELSE                                           0255
*                     DO;           /* FORMATTED LINE                */
         B     @RC00250                                            0255
@RF00250 DS    0H                                                  0256
*                       CALL CONVERT;/* GEN OFFSET                   */
         BAL   @14,CONVERT                                         0256
*                       CALL FMTRTN;/* FORMAT                        */
         BAL   @14,FMTRTN                                          0257
*                     END;          /* END FORMAT LINE               */
*                 END;              /* END LINE TO BE OUTPUT         */
@RC00250 DS    0H                                                  0260
*           END;                    /* END FORMAT LOOP               */
@RC00246 DS    0H                                                  0260
@RC00244 AH    ARRAYCNT,@CH00042                                   0260
         CH    ARRAYCNT,@CH03443                                   0260
         BNH   @DL00243                                            0260
@DC00243 DS    0H                                                  0261
*           IF ERRSW=OFF THEN       /* FORMATTED WITHOUT PROBLEM     */
         TM    ERRSW,B'00100000'                                   0261
         BNZ   @RF00261                                            0261
*             DO;                                                  0262
*               CALL RT2SUM;        /* FORMAT RTM2WA SUMMARY         */
*                                                                  0263
         BAL   @14,RT2SUM                                          0263
*               /*****************************************************/
*               /*                                                   */
*               /* IF RTM2WA FORMATTED ALL PARTS WITHOUT ANY         */
*               /* PROBLEMS, SEND THE SVRB ADDRESS TO THE MAIN       */
*               /* ROUTINE FOR THE ESA BIT SUMMARY                   */
*               /*                                                   */
*               /*****************************************************/
*                                                                  0264
*               IF RCREG=ZERO THEN  /* SUMMARIES OK                  */
         LTR   RCREG,RCREG                                         0264
         BNZ   @RF00264                                            0264
*                 DO;                                              0265
*                   VADR=ADDR(RTM2VRBC);/* GET ADDR OF RELATED SVRB  */
         LA    VADR,RTM2VRBC(,RTM2PTR)                             0266
*                   CALL ACCESRTN;  /* ACCESS IT                     */
         BAL   @14,ACCESRTN                                        0267
*                   IF RCREG=ZERO THEN/* PASS BACK SVRB ADR TO MAIN
*                                      PROCEDURE                     */
         SLR   @14,@14                                             0268
         CR    RCREG,@14                                           0268
         BNE   @RF00268                                            0268
*                     DO;                                          0269
*                       ADPLFMT2=DATAWORD;/* PUT SVRB ADDR IN      0270
*                                      PARMLIST                      */
         MVC   ADPLFMT2(4,ABDPLPTR),DATAWORD(DATAPTR)              0270
*                       START=ZERO; /* OFFSET FOR CONTROL BLOCK      */
         ST    @14,START(,ABDPLPTR)                                0271
*                     END;          /* FOUND SVRB                    */
*                   ELSE                                           0273
*                     ;             /* ACCESS ERROR -BAD RC WILL BE
*                                      PASSED TO MAIN PROC INHIBITING
*                                      ESA PROCESSING                */
@RF00268 DS    0H                                                  0274
*                 END;              /* END SUMMARIES OK              */
*               ELSE                                               0275
*                 ;                 /* SUMMARIES HAD ERROR           */
@RF00264 DS    0H                                                  0276
*             END;                  /* END FORMATTED WITHOUT PROBLEM */
*           ELSE                                                   0277
*             ;                     /* ERROR FORMATTING              */
@RF00261 DS    0H                                                  0278
*         END;                      /* END FOUND SYSTEM              */
*       ELSE                                                       0279
*         ;                         /* COULD NOT FIND SYSTEM         */
@RF00232 DS    0H                                                  0280
*     END;                          /* END FOUND RELEASE NUMBER      */
*   CALL PRINT;                     /* ERROR MSG OR BLANK            */
@RC00197 L     @15,ADPLPRNT(,ABDPLPTR)                             0281
         BALR  @14,@15                                             0281
*   LCALREGS=RTYREGS;               /* RESTORE CALLERS EXPECTED RETRY
*                                      REGS                          */
         L     @14,ESTAPTR                                         0282
         MVC   LCALREGS(64,@14),RTYREGS(@14)                       0282
*   RETURN CODE(RCCODE);            /* TO PREVENT ENTERING ESA CODE  */
         L     @08,RCCODE                                          0283
         L     @13,4(,@13)                                         0283
         L     @00,@SIZDATD                                        0283
         LR    @01,@09                                             0283
         FREEMAIN R,LV=(0),A=(1)
         LR    @15,@08                                             0283
         L     @14,12(,@13)                                        0283
         LM    @00,@12,20(@13)                                     0283
         BR    @14                                                 0283
*RT2SUM:                                                           0284
*   PROC OPTIONS(SAVE(14));                                        0284
*                                                                  0284
RT2SUM   ST    @14,@SA00002                                        0284
*/********************************************************************/
*/* FUNCTION -                                                     0285
*      ROUTINE WHICH FORMATS THE RTM2 WORK AREA SUMMARY.           0285
*      IT SETS UP THE NEEDED POINTERS TO BE PASSED TO THE FORMAT   0285
*      ROUTINE SENT IN THE PARAMETER LIST.  FOR ANY ERROR ACCESSING
*      A NEEDED FIELD IN THE RTM2WA OR FOR NON-ZERO RETURN CODE    0285
*      FROM THE FORMATTING SERVICE, THE FOLLOWING MESSAGE WILL BE  0285
*      PRINTED:                                                    0285
*               *** FORMATTING SERVICE ROUTINE ERROR DURING        0285
*               RTM2WA FORMATTING(POSSIBLE CAUSE:  DATA NOT IN     0285
*               DUMP) ***                                          0285
*      AT THIS POINT PROCESSING WOULD TERMINATE. IF THE RTM2WA IS  0285
*      FORMATTED WITHOUT ERROR, A BIT SUMMARY OF THE RTM2WA IMPORTANT
*      AREA IS PRINTED.                                            0285
*                                                                  0285
*   INPUT -                                                        0285
*      THIS ROUTINE EXPECTS THE PARAMETER LIST ADDRESS IN REGISTER 1,
*      THE ADDRESS OF THE RTM2WA IN RTM2PTR, AND THE SUSAVE BITS FILLED
*      IN.                                                         0285
*                                                                  0285
*   CALLS -                                                        0285
*      CALLS CONVERT, FMTRTN, PRINT,AND RT2BITS                    0285
*                                                                  0285
*   CALLED BY -                                                    0285
*      IEAFTRT2 TO FORMAT THE RTM2WA SUMMARY                       0285
*                                                                  0285
*   RETURNS TO -                                                   0285
*      IEAFTRT2 WITH RETURN CODE SET BY FORMAT                     0285
*      SERVICE ROUTINES                                              */
*/********************************************************************/
*                                                                  0285
*   CALL PRINT;                     /* BLANK                         */
         L     @15,ADPLPRNT(,ABDPLPTR)                             0285
         BALR  @14,@15                                             0285
*   BUFFER(31:(LENGTH(RT2SMHDR)+30))=RT2SMHDR;/* SUMMARY HEADER      */
         L     @14,ADPLBUF(,ABDPLPTR)                              0286
         MVC   BUFFER+30(14,@14),RT2SMHDR                          0286
*   CALL PRINT;                     /* PRINT HDR                     */
         L     @15,ADPLPRNT(,ABDPLPTR)                             0287
         BALR  @14,@15                                             0287
*   BUFFER(31:(LENGTH(RT2SMUS)+30))=RT2SMUS;/* UNDERSCORE            */
         L     @14,ADPLBUF(,ABDPLPTR)                              0288
         MVC   BUFFER+30(14,@14),RT2SMUS                           0288
*   CALL PRINT;                     /* PRINT IT                      */
         L     @15,ADPLPRNT(,ABDPLPTR)                             0289
         BALR  @14,@15                                             0289
*   CALL PRINT;                     /* BLANK                         */
*                                                                  0290
         L     @15,ADPLPRNT(,ABDPLPTR)                             0290
         BALR  @14,@15                                             0290
*   /*****************************************************************/
*   /*                                                               */
*   /* FIRST GROUP OF SUMMARY OUTPUT - ABEND INFORMATION             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0291
*   DO ARRAYCNT=1 TO GROUP1 WHILE(ERRSW=OFF);/* FORMAT GROUP 1       */
         LA    ARRAYCNT,1                                          0291
@DL00291 TM    ERRSW,B'00100000'                                   0291
         BNZ   @DC00291                                            0291
*     FIELDADR=FIELDARY(ARRAYCNT);  /* ADDRESS OF FIELD IS INAN ARRAY*/
         LR    @14,ARRAYCNT                                        0292
         SLA   @14,2                                               0292
         L     FIELDADR,FIELDARY-4(@14)                            0292
*     ADPLFMT2=FIELDADR+RTM2PTR;    /* ADR OF FIELD TO BE FORMATTED  */
         LR    @04,FIELDADR                                        0293
         ALR   @04,RTM2PTR                                         0293
         ST    @04,ADPLFMT2(,ABDPLPTR)                             0293
*     LINEPTR=SUMNAMES(ARRAYCNT);   /* FORMAT PATTERN WHICH MATCHES
*                                      FIELD                         */
         L     LINEPTR,SUMNAMES-4(@14)                             0294
*     CALL CONVERT;                 /* GENERATE OFFSET               */
         BAL   @14,CONVERT                                         0295
*     CALL FMTRTN;                  /* FORMAT THE LINE MACRO TO SET
*                                      UP FOR SUMMARY                */
         BAL   @14,FMTRTN                                          0296
*   END;                                                           0297
*                                                                  0297
         AH    ARRAYCNT,@CH00042                                   0297
         CH    ARRAYCNT,@CH00312                                   0297
         BNH   @DL00291                                            0297
@DC00291 DS    0H                                                  0298
*   /*****************************************************************/
*   /*                                                               */
*   /* SECOND GROUP OF OUTPUT - REGS, PSW, AND SDWA COMPLETION CODE  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0298
*   IF ERRSW=OFF THEN               /* FORMATTED FIRST SECTION OK    */
         TM    ERRSW,B'00100000'                                   0298
         BNZ   @RF00298                                            0298
*     DO;                                                          0299
*       CALL PRINT;                 /* BLANK TO SEPARATE SECTIONS    */
         L     @15,ADPLPRNT(,ABDPLPTR)                             0300
         BALR  @14,@15                                             0300
*       DO ARRAYCNT=ARRAYCNT TO GROUP2 WHILE(ERRSW=OFF);/* FORMAT  0301
*                                      NEXT SECTION(2)               */
         B     @DE00301                                            0301
@DL00301 TM    ERRSW,B'00100000'                                   0301
         BNZ   @DC00301                                            0301
*         FIELDADR=FIELDARY(ARRAYCNT);/* ADDRESS OF FIELD IS INAN  0302
*                                      ARRAY                         */
         LR    @14,ARRAYCNT                                        0302
         SLA   @14,2                                               0302
         L     FIELDADR,FIELDARY-4(@14)                            0302
*         ADPLFMT2=FIELDADR+RTM2PTR;/* ADR OF FIELD TO BE FORMATTED  */
         LR    @04,FIELDADR                                        0303
         ALR   @04,RTM2PTR                                         0303
         ST    @04,ADPLFMT2(,ABDPLPTR)                             0303
*         LINEPTR=SUMNAMES(ARRAYCNT);/* FORMAT PATTERN WHICH MATCHES
*                                      FIELD                         */
         L     LINEPTR,SUMNAMES-4(@14)                             0304
*         CALL CONVERT;             /* GENERATE OFFSET               */
         BAL   @14,CONVERT                                         0305
*         CALL FMTRTN;              /* FORMAT THE LINE MACRO TO    0306
*                                      FORMAT                        */
         BAL   @14,FMTRTN                                          0306
*       END;                        /* END FORMAT SECTION 2          */
*                                                                  0307
         AH    ARRAYCNT,@CH00042                                   0307
@DE00301 CH    ARRAYCNT,@CH00778                                   0307
