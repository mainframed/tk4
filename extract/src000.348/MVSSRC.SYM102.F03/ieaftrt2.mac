         TM    ERRSW,B'00100000'                                   0513
         BNZ   @RF00513                                            0513
*                     DO;           /*                       @ZM45190*/
*                       LINEPTR=RTM2NAME(1);/* ONE WORD OF DATA    0515
*                                                            @ZM45190*/
         L     LINEPTR,RTM2NAME                                    0515
*                       CALL CONVERT;/* OFFSET               @ZM95190*/
         BAL   @14,CONVERT                                         0516
*                       CALL FMTRTN;/* FORMAT                @ZM45190*/
         BAL   @14,FMTRTN                                          0517
*                     END;          /* END NO PROBLEM WITH LINE 3  0518
