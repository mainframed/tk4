         SL    @08,START(,ABDPLPTR)                                0640
         ST    @08,SAVEPTR                                         0640
*   SDWAPTR=ADPLFMT2;               /* PTR TO THE SDWA               */
         LR    SDWAPTR,@14                                         0641
