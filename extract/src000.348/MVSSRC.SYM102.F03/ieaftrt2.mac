*   CALL PRINT;                     /* PRINT IT                      */
         L     @15,ADPLPRNT(,ABDPLPTR)                             0651
         BALR  @14,@15                                             0651
*   CALL PRINT;                     /* BLANK                         */
         L     @15,ADPLPRNT(,ABDPLPTR)                             0652
         BALR  @14,@15                                             0652
*   CALL PRINT;                     /* BLANK                         */
         L     @15,ADPLPRNT(,ABDPLPTR)                             0653
         BALR  @14,@15                                             0653
*   ADPLFMT2=ADDR(SDWAGRSV);        /* REGISTERS AT ERROR            */
         LA    @14,SDWAGRSV(,SDWAPTR)                              0654
         ST    @14,ADPLFMT2(,ABDPLPTR)                             0654
*   LINEPTR=SUMNAMES(4);            /* FORMAT PATTERN FOR REGS PART 1*/
         L     LINEPTR,SUMNAMES+12                                 0655
*   CALL CONVERT;                   /* GENERATE OFFSET               */
         BAL   @14,CONVERT                                         0656
*   CALL FMTRTN;                    /* FORMAT                        */
         BAL   @14,FMTRTN                                          0657
*   IF ERRSW=OFF THEN               /* FORMATTED PART1 OK            */
