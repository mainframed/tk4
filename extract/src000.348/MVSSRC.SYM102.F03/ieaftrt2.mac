*     BLANKS CHAR(120) INIT(' ');   /* USED TO BLANK OUT DIGIT FIELD
*                                      OR TO COMPARE WITH THE MESSAGE
*                                      ARRAYS FOR BLANK LINES        */
*   DCL                                                            0702
*     BYTELNTH FIXED(31) CONSTANT(8);/* NUM BITS IN A BYTE           */
*   DCL                                                            0703
*     CONVRT FIXED(31);             /* VALUE OF OFFSET               */
*   DCL                                                            0704
*     INDEX FIXED(31);              /* LOOP COUNTER FOR ANALYZE    0704
*                                      ROUTINE                       */
*   DCL                                                            0705
*     LNGTHEQU FIXED(31);           /* CALCULATE SUBSTRING ADR FOR 0705
*                                      PROPER MSG                    */
*   DCL                                                            0706
*     MSG CHAR(120) BASED(ADPLCOM1);/* ERROR MSG TO BE PUT OUT BY  0706
*                                      FORMAT ROUTINE ERROR          */
*   DCL                                                            0707
*     MSGARY CHAR(*) BASED(MSGADDR);/* MESSAGES PASSED TO BE OUTPUT  */
*   DCL                                                            0708
*     NEGBIT BIT(1);                /* FLAG TO INDICATE NEG OFFSET   */
*   DCL                                                            0709
*     NEGCHAR CHAR(1) CONSTANT('-');/* CONSTANT '-' TO BE PUT IN   0709
*                                      OFFSET                        */
*   DCL                                                            0710
*     PLUSCHAR CHAR(1) CONSTANT('+');/* CONSTANT '+' TO BE PUT IN  0710
*                                      OFFSET                        */
*   DCL                                                            0711
*     TESTBITS(8) BIT(8) INIT('80'X,'40'X,'20'X,'10'X,'08'X,'04'X,'02'X
*         ,'01'X);                  /* TO TELL WHICH BIT IS ON       */
*   DCL                                                            0712
*     TWODGT FIXED(31) CONSTANT(16);/* FIRST OF 2 HEX DIGITS         */
*   DCL                                                            0713
*     THREDGT FIXED(31) CONSTANT(256);/* FIRST OF 3 HEX DIGITS       */
*   DCL                                                            0714
*     FOURDGT FIXED(31) CONSTANT(4096);/* FIRST OF 4 HEX DIGITS      */
*CONVERT:                                                          0715
*   PROC OPTIONS(SAVE(14));                                        0715
*                                                                  0715
CONVERT  ST    @14,12(,@13)                                        0715
*/********************************************************************/
*/* FUNCTION -                                                     0716
*      ROUTINE TO DYNAMICALLY GENERATE THE OFFSETS TO BE PUT ALONGSIDE
*      THE CONTROL BLOCK LINE.  IT FIGURES THE OFFSET FROM THE     0716
*      BEGINNING OF THE CONTROL BLOCK, CONVERTS IT TO PRINTABLE HEX,
*      PLACES IT INTO A BUFFER AREA AND PUTS THE ADDRESS OF THIS   0716
*      BUFFER AREA INTO THE PARAMETER LIST TO BE PICKED UP IN THE  0716
*      FORMAT PATTERNS BY THE FORMAT SERVICE ROUTINE.              0716
*                                                                  0716
*   INPUT -                                                        0716
*      SAVEPTR MUST HAVE THE ADDRESS OF THE BEGINNING OF THE CONTROL
*      BLOCK, REGISTER 1 POINTS TO THE PARMLIST WHICH CONTAINS THE 0716
*      CURRENT CONTROL BLOCK ADDRESS IN ADPLFMT2.                  0716
*                                                                  0716
*   CALLS -                                                        0716
*      MAKES NO OUTSIDE CALLS                                      0716
*                                                                  0716
*   CALLED BY -                                                    0716
*      BYTEMODE, EEDBITS, ENTRYRT1, IEAFTEED, IEAFTESA, IEAFTFRR,  0716
*      IEAFTIHS, IEAFTRTC, IEAFTRT2, IEAFTSCB, IEAFTSDW, RT1BITS,  0716
*      RT2BITS, RT2SUM, AND SPECIAL                                0716
*                                                                  0716
*   RETURNS TO -                                                   0716
*      CALLER.                                                     0716
*                                                                  0716
*   THIS PROCEDURE HAS BEEN INCLUDED FROM SYSLIB(IHAFTCDE) ------  0716
*                                                                    */
*/********************************************************************/
*                                                                  0716
*   IF SAVEPTR>ADPLFMT2 THEN        /* THIS WILL OCCUR FOR A PREFIX
*                                      WITH A NEGATIVE OFFSET        */
         L     @14,SAVEPTR                                         0716
         L     @07,ADPLFMT2(,ABDPLPTR)                             0716
         CR    @14,@07                                             0716
         BNH   @RF00716                                            0716
*     DO;                                                          0717
*       NEGBIT=ON;                  /* INDICATE NEGATIVE OFFSET      */
         OI    NEGBIT,B'10000000'                                  0718
*       CONVRT=SAVEPTR-ADPLFMT2;    /* NEGATIVE OFFSET VALUE         */
         L     @04,ADPLFMT2(,ABDPLPTR)                             0719
         SLR   @14,@04                                             0719
         LR    CONVRT,@14                                          0719
*     END;                          /* END NEGATIVE OFFSET           */
*   ELSE                            /* POSITIVE OR ZERO OFFSET       */
*     DO;                                                          0721
         B     @RC00716                                            0721
@RF00716 DS    0H                                                  0722
*       NEGBIT=OFF;                 /* INDICATE POS VALUE            */
         NI    NEGBIT,B'01111111'                                  0722
*       CONVRT=ADPLFMT2-SAVEPTR;    /* RELATIVE OFFSET IN CONTROL  0723
*                                      BLOCK                         */
         L     CONVRT,ADPLFMT2(,ABDPLPTR)                          0723
         SL    CONVRT,SAVEPTR                                      0723
*     END;                          /* END POS VALUE                 */
*   DIGIT=BLANKS(1:5);              /* BLANK OUT DIGITS              */
@RC00716 MVC   DIGIT(5),BLANKS                                     0725
*   PACKAREA=CONVRT;                /* PLACE OFFSET INTO CHAR AREA 0726
*                                      FOR THE UNPACK                */
         ST    CONVRT,PACKAREA                                     0726
*   GARBAGE='04'X;                  /* GARBAGE BYTE SO OFFSET WILL 0727
*                                      UNPACK OK                     */
*                                                                  0727
         MVI   GARBAGE,X'04'                                       0727
*   /*****************************************************************/
*   /*                                                               */
*   /* FOR AN ODD NUMBER OF DIGITS THE OUTPUT SHOULD LOOK LIKE +C OR */
*   /* +13C. IF THE VALUE WAS UNPACKED DIRECTLY INTO DIGITS THE      */
*   /* OUTPUT WOULD HAVE UNWANTED LEADING ZEROS SUCH AS +0C. TO      */
*   /* ELIMINATE THIS(ONLY FOR AN ODD NUMBER OF DIGITS) UNPACK ODD   */
*   /* NUMBER OF DIGITS INTO CONVERTD WHERE THE LEADING ZERO WILL BE */
*   /* OVERWRITTEN BY THE '+' OR '-' SIGN                            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0728
*   IF CONVRT<TWODGT THEN           /* ONLY 1 DIGIT TO OFFSET        */
         CH    CONVRT,@CH00781                                     0728
         BNL   @RF00728                                            0728
*     UNPK(CONVERTD(1:3),UNPACK(4:5));/* UNPACK ONLY 1 DIGIT AND   0729
*                                      DUMMY                         */
         UNPK  CONVERTD(3),UNPACK+3(2)                             0729
*   ELSE                                                           0730
*     IF CONVRT<THREDGT THEN        /* 2 DIGITS TO OFFSET            */
         B     @RC00728                                            0730
@RF00728 CH    CONVRT,@CH00704                                     0730
         BNL   @RF00730                                            0730
*       UNPK(DIGIT(1:3),UNPACK(4:5));/* UNPACK LAST 2 DIGITS & DUMMY */
         UNPK  DIGIT(3),UNPACK+3(2)                                0731
*     ELSE                                                         0732
*       IF CONVRT<FOURDGT THEN      /* 3 DIGITS TO OFFSET            */
         B     @RC00730                                            0732
@RF00730 CH    CONVRT,@CH03858                                     0732
         BNL   @RF00732                                            0732
*         UNPK(CONVERTD(1:5),UNPACK(3:5));/* UNPACK LAST 3 DIGITS &
*                                      DUMMY                         */
         UNPK  CONVERTD(5),UNPACK+2(3)                             0733
*       ELSE                        /* 4 DIGITS (MAX)                */
*         UNPK(DIGIT(1:5),UNPACK(3:5));/* LAST 4 DIGITS & DUMMY BYTE */
         B     @RC00732                                            0734
@RF00732 UNPK  DIGIT(5),UNPACK+2(3)                                0734
*   TR(DIGIT,TRANS);                /* MAKE PRINTABLE                */
@RC00732 DS    0H                                                  0735
@RC00730 DS    0H                                                  0735
@RC00728 TR    DIGIT(5),TRANS                                      0735
*   IF NEGBIT=ON THEN               /* WAS A NEGATIVE OFFSET         */
         TM    NEGBIT,B'10000000'                                  0736
         BNO   @RF00736                                            0736
*     PLUS=NEGCHAR;                 /* MINUS SIGN                    */
         MVI   PLUS,C'-'                                           0737
*   ELSE                            /* POSITIVE OFFSET               */
*     PLUS=PLUSCHAR;                /* MOVE IN PLUS SIGN             */
         B     @RC00736                                            0738
@RF00736 MVI   PLUS,C'+'                                           0738
*   ADPLFMT1=ADDR(CONVERTD);        /* SET UP FOR FORMAT             */
@RC00736 LA    @14,CONVERTD                                        0739
         ST    @14,ADPLFMT1(,ABDPLPTR)                             0739
*   END CONVERT;                                                   0740
@EL00008 DS    0H                                                  0740
@EF00008 DS    0H                                                  0740
@ER00008 L     @14,12(,@13)                                        0740
         BR    @14                                                 0740
*FMTRTN:                                                           0741
*   PROC OPTIONS(SAVE(14));                                        0741
*                                                                  0741
FMTRTN   ST    @14,@SA00009                                        0741
*/****************************************************************** */
*/* FUNCTION                                                       0742
*       ROUTINE TO SET UP CALLING OF THE FORMAT SERVICE ROUTINE PASSED
*     IN THE PARMLIST AND CHECKS RETURN CODE.  IF A RETURN CODE IS BAD
*     IT TAKES THE MESSAGE WHOSE ADDRESS IS IN ADPLCOM1. AND PUTS IT
*     IN THE BUFFER AND PRINTS IT. IT SETS THE ERRSW TO INDICATE BAD
*     RETURN CODE AND RETURNS. IF THE RETURN CODE WAS GOOD, IT PRINTS
*     THE FORMATTED LINE.                                          0742
*                                                                  0742
*   CALLS-                                                         0742
*       CALLS FORMAT AND PRINT                                     0742
*                                                                  0742
*   INPUT -                                                        0742
*     LINEPTR(REGISTER 0) HAS THE ADDRESS OF THE FORMAT PATTERN TO BE
*    PASSED TO THE FORMAT SERVICE ROUTINE. ADPLFMT2 CONTAINS THE CURREN
*    ADDRESS OF THE CONTROL BLOCK TO BE FORMATTED AND ,IN MOST CASES,
*    ADPLFMT1 CONTAINS THE ADDRESS OF THE FIELD 'CONVERTD' WHICH IS
*    THE EBCDIC REPRESENTATION OF THE OFFSET TO BE PLACED ALONG SIDE
*    THE OUTPUT LINE.                                              0742
*                                                                  0742
*   CALLED BY -                                                    0742
*     IEAFTEED,IEAFTFRR,IEAFTIHS,IEAFTRTC,IEAFTRT2,IEAFTSCB,       0742
*     IEAFTSDW, RT2SUM,AND SPECIAL                                 0742
*                                                                  0742
*   RETURNS CONTROL TO -                                           0742
*     CALLER WITH RETURN CODE RECIEVED FROM FORMAT                 0742
*     SERVICE ROUTINE                                              0742
*                                                                  0742
*   THIS PROCEDURE HAS BEEN INCLUDED FROM SYSLIB(IHAFTCDE) ------  0742
*                                                                    */
*/****************************************************************** */
*                                                                  0742
*   IF ADPLSNPR=OFF THEN            /* IF SNAP IS THE CALLER, AN   0742
*                                      ACCESS ERROR CAN BE EXPECTED  */
         TM    ADPLSNPR(ABDPLPTR),B'10000000'                      0742
         BNZ   @RF00742                                            0742
*     DO;                                                          0743
*       RTYADR=ADDR(FMTRTY);        /* WHERE TO RETRY                */
         L     @14,ESTAPTR                                         0744
         LA    @07,FMTRTY                                          0744
         ST    @07,RTYADR(,@14)                                    0744
*       ERREXP=ON;                  /* INDICATE AN ERROR IS EXPECTED */
         OI    ERREXP(@14),B'10000000'                             0745
*     END;                          /* END SNAP IS THE CALLER        */
*   ELSE                                                           0747
*     ;                             /* SNAP WAS NOT INVOKER          */
@RF00742 DS    0H                                                  0748
*   CALL FORMAT;                    /* FORMAT ROUTINE PASSED BY    0748
*                                      CALLER                        */
         L     @15,ADPLFRMT(,ABDPLPTR)                             0748
         BALR  @14,@15                                             0748
*   ERREXP=OFF;                     /* ERROR IS NO LONGER EXPECTED   */
         L     @14,ESTAPTR                                         0749
         NI    ERREXP(@14),B'01111111'                             0749
*   IF RCREG^=ZERO THEN                                            0750
         LTR   RCREG,RCREG                                         0750
         BZ    @RF00750                                            0750
*     DO;                           /* FORMAT ROUTINE ERROR          */
*FMTRTY:                            /* RETRY POINT FROM ERROR        */
*       ERRSW=ON;                   /* SET ERROR INDICATOR           */
FMTRTY   OI    ERRSW,B'00100000'                                   0752
*       CALL PRINT;                 /* SPACE A LINE                  */
         L     @15,ADPLPRNT(,ABDPLPTR)                             0753
         BALR  @14,@15                                             0753
*       BUFFER=MSG;                 /* ERROR MSG PASSED BY CALLER  0754
*                                      WHOSE ADDR IS PLACED IN     0754
*                                      ADPLCOM1                      */
         L     @14,ADPLBUF(,ABDPLPTR)                              0754
         L     @07,ADPLCOM1(,ABDPLPTR)                             0754
         MVC   BUFFER(120,@14),MSG(@07)                            0754
*       RCREG=4;                    /* INDICATE BAD RETURN CODE FROM
*                                      FORMATTER                     */
         LA    RCREG,4                                             0755
*     END;                          /* END FORMAT ROUTINE ERROR      */
*   ELSE                                                           0757
*     ;                             /* FORMATTED LINE IS IN BUFFER   */
@RF00750 DS    0H                                                  0758
*   RCCODE=RCREG;                   /* SAVE RETURN CODE FROM FORMAT
*                                      SERVICE ROUTINE               */
         ST    RCREG,RCCODE                                        0758
*   CALL PRINT;                     /* PRINT THE ERROR MSG OR      0759
*                                      FORMATTED LINE                */
         L     @15,ADPLPRNT(,ABDPLPTR)                             0759
         BALR  @14,@15                                             0759
*   RETURN CODE(RCCODE);                                           0760
         L     @15,RCCODE                                          0760
@EL00009 DS    0H                                                  0760
@EF00009 DS    0H                                                  0760
@ER00009 L     @14,@SA00009                                        0760
         BR    @14                                                 0760
*   END FMTRTN;                                                    0761
*ANALYZE:                                                          0762
*   PROC OPTIONS(SAVE(14));                                        0762
*                                                                  0762
ANALYZE  ST    @14,@SA00010                                        0762
*/****************************************************************** */
*/* FUNCTION                                                       0763
*     ROUTINE TO ANALYZE THE BITS IN THE BYTE PASSED AND PRINT OUT 0763
*     THE MESSAGE THAT CORRESPONDS TO THE BITS THAT ARE ON. IF A BLANK
*     LINE IS PASSED IN THE MESSAGE ARRAY (INDICATING A RESERVED BIT)
*     NO OUTPUT WILL CORRESPOND TO THAT BIT.                       0763
*                                                                  0763
*   INPUT-                                                         0763
*     AN ARRAY (SUMMARY) POINTED TO BY THE PARMLIST FIELD          0763
*     ADPLCOM2 WHICH CONTAINS THE ADDRESS OF THE BYTE TO BE        0763
*     ANALYZED, THE ADDRESS OF THE ARRAY OF CORRESPONDING          0763
*     MESSAGES, THE COLUMN NUMBER THE MESSAGE IS TO BEGIN IN,      0763
*     AND THE LENGTH OF THE MESSAGE ARRAY PASSED.                  0763
*                                                                  0763
*   NOTE- COLNUM + MSGLNGTH MUST BE LESS THAN OR EQUAL TO 120 BYTES
*         WHICH IS THE OUTPUT BUFFER LENGTH.                       0763
*                                                                  0763
*                                                                  0763
*   CALLS-                                                         0763
*       CALLS PRINT                                                0763
*                                                                  0763
*   CALLED BY -                                                    0763
*       BYTEMODE,EEDBITS,IEAFTESA,IEAFTSCB,RT2BITS                 0763
*                                                                  0763
*   RETURNS CONTROL TO -                                           0763
*       CALLER WITHOUT SETTING A RETURN CODE                       0763
*                                                                  0763
*   THIS PROCEDURE HAS BEEN INCLUDED FROM SYSLIB(IHAFTCDE)-------  0763
*                                                                    */
*/****************************************************************** */
*                                                                  0763
*   DO INDEX=1 TO BYTELNTH;         /* LOOP TO FIND WHICH BITS ARE ON*/
         LA    INDEX,1                                             0763
@DL00763 DS    0H                                                  0764
*     LNGTHEQU=((INDEX-1)*(MSGLNGTH)+1);/* OFFSET INTO MSGARY OF   0764
*                                      CORRESPONDING MESSAGE         */
         L     @14,MSGLNGTH                                        0764
         LR    @03,INDEX                                           0764
         BCTR  @03,0                                               0764
         MR    @02,@14                                             0764
         AH    @03,@CH00042                                        0764
         LR    LNGTHEQU,@03                                        0764
*     IF(BITSTR&TESTBITS(INDEX))^=ZEROBYTE&/* FOUND A BIT ON         */
*         MSGARY(LNGTHEQU:MSGLNGTH+LNGTHEQU-1)^=BLANKS(1:MSGLNGTH) THEN
         L     @04,BYTEADDR                                        0765
         SLR   @03,@03                                             0765
         IC    @03,BITSTR(,@04)                                    0765
         SLR   @04,@04                                             0765
         IC    @04,TESTBITS-1(INDEX)                               0765
         NR    @03,@04                                             0765
         SLR   @04,@04                                             0765
         IC    @04,ZEROBYTE                                        0765
         CR    @03,@04                                             0765
         BE    @RF00765                                            0765
         L     @04,MSGADDR                                         0765
         LR    @03,@14                                             0765
         BCTR  @03,0                                               0765
         LR    @02,@04                                             0765
         ALR   @02,LNGTHEQU                                        0765
         BCTR  @02,0                                               0765
         EX    @03,@SC03895                                        0765
         BE    @RF00765                                            0765
*       DO;                         /* CORRESPONDING MESSAGE WAS NOT
*                                      BLANK(RESERVED)               */
*         BUFFER(COLNUM:(MSGLNGTH+COLNUM-1))=MSGARY(LNGTHEQU:LNGTHEQU+
*             MSGLNGTH-1);          /* PLACE MSG IN BUFFER           */
         L     @03,COLNUM                                          0767
         L     @02,ADPLBUF(,ABDPLPTR)                              0767
         ALR   @02,@03                                             0767
         BCTR  @02,0                                               0767
         ALR   @14,@03                                             0767
         SLR   @14,@03                                             0767
         BCTR  @14,0                                               0767
         ALR   @04,LNGTHEQU                                        0767
         BCTR  @04,0                                               0767
         EX    @14,@SM03897                                        0767
*         CALL PRINT;               /* PRINT IT                      */
         L     @15,ADPLPRNT(,ABDPLPTR)                             0768
         BALR  @14,@15                                             0768
*       END;                        /* END FOUND THE MESSAGE         */
*     ELSE                                                         0770
*       ;                           /* EITHER BIT NOT ON OR RESERVED
*                                      BIT-NO MESSAGE                */
@RF00765 DS    0H                                                  0771
*   END;                            /* END SEARCH LOOP               */
         AH    INDEX,@CH00042                                      0771
         CH    INDEX,@CH00087                                      0771
         BNH   @DL00763                                            0771
*   END ANALYZE;                                                   0772
@EL00010 DS    0H                                                  0772
@EF00010 DS    0H                                                  0772
@ER00010 L     @14,@SA00010                                        0772
         BR    @14                                                 0772
*ACCESRTN:                                                         0773
*   PROC OPTIONS(SAVE(14));                                        0773
*                                                                  0773
ACCESRTN ST    @14,@SA00011                                        0773
*/********************************************************************/
*/* FUNCTION-                                                      0774
*       ROUTINE WHICH CALLS THE ACCESS SERVICE ROUTINE AND MOVES THE
*     VALUE RETURNED IN R0 INTO DATAPTR.                           0774
*                                                                  0774
*                                                                  0774
*   INPUT -                                                        0774
*         VADR(REGISTER 0) CONTAINS THE VIRTUAL ADDRESS OF THE FIELD
*         TO BE ACCESSED.                                          0774
*                                                                  0774
*    CALLS-                                                        0774
*         ACCESS SERVICE ROUTINE PASSED IN PARMLIST                0774
*                                                                  0774
*    CALLED BY -                                                   0774
*         EEDBITS, IEAFTEED, IEAFTFRR, IEAFTRTC, IEAFTRT2,         0774
*         IEAFTSCB, RT1BITS, AND RT2BITS                           0774
*                                                                  0774
*    RETURNS TO -                                                  0774
*         CALLER WITH THE RETURN CODE SET BY THE ACCESS            0774
*         SERVICE ROUTINE.                                         0774
*                                                                  0774
*   THIS PROCEDURE HAS BEEN INCLUDED FROM SYSLIB(IHAFTCDE) ------  0774
*                                                                    */
*/********************************************************************/
*                                                                  0774
*   IF ADPLSNPR=OFF THEN            /* SNAP WAS INVOKER              */
         TM    ADPLSNPR(ABDPLPTR),B'10000000'                      0774
         BNZ   @RF00774                                            0774
*     DO;                                                          0775
*       RTYADR=ADDR(RETRY);         /* RETRY ADDR FOR ESTAE IN THE 0776
*                                      CASE OF EXPECTED ERRORS       */
         L     @14,ESTAPTR                                         0776
         LA    @07,RETRY                                           0776
         ST    @07,RTYADR(,@14)                                    0776
*       DATAPTR=VADR;               /* USED TO BASE THE STORAGE ON   */
         LR    DATAPTR,VADR                                        0777
*       RCREG=ZERO;                 /* SUCCESS UNLESS AN ABEND OCCURS
*                                      DURING THE MOVE INTO A BUFFER */
         SLR   RCREG,RCREG                                         0778
*       ERREXP=ON;                  /* AN ERROR IS EXPECTED          */
         OI    ERREXP(@14),B'10000000'                             0779
*       VERIFY=DATADWRD;            /* MOVE A DOUBLE WORD INTO     0780
*                                      INTERNAL BUFFER               */
*                                                                  0780
         MVC   VERIFY(8),DATADWRD(DATAPTR)                         0780
*       /*************************************************************/
*       /*                                                           */
*       /* THIS MOVE WILL CAUSE AN ABEND IF THE DUMP DOES NOT CONTAIN*/
*       /* THE AREA REFERENCED OR IF THE AREA IS PROTECTED. IF AN    */
*       /* ABEND OCCURS, THE ESTAE WILL GET CONTROL AND RETRY AT THE */
*       /* POINT WHICH SETS A BAD RETURN CODE AND RETURNS TO THE     */
*       /* CALLER. THIS THEN SIMULATES THE PRINT DUMP ACCESS ROUTINE.*/
*       /* IF THE AREA ASKED FOR IS IN THE DUMP, A DOUBLE WORD IS    */
*       /* MOVED INTO AN INTERNAL BUFFER TO PREVENT AN UNEXPECTED    */
*       /* ERROR WHEN REFERENCING THE AREA AGAIN                     */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0781
*       ERREXP=OFF;                 /* AN ERROR IS NO LONGER EXPECTED*/
         NI    ERREXP(@14),B'01111111'                             0781
*       DATAPTR=ADDR(VERIFY);       /* REBASE DATA OFF INTERNAL    0782
*                                      BUFFER                        */
         LA    DATAPTR,VERIFY                                      0782
*     END;                          /* END SNAP WAS THE INVOKER      */
*   ELSE                            /* CALLER HAS OWN ACCESS ROUTINE
*                                      LIKE PRINT DUMP               */
*     DO;                                                          0784
         B     @RC00774                                            0784
@RF00774 DS    0H                                                  0785
*       CALL ACCESS;                /* ACCESS SERVICE ROUTINE PASSED
*                                      BY CALLER                     */
         L     @15,ADPLMEMA(,ABDPLPTR)                             0785
         BALR  @14,@15                                             0785
*       IF RCREG=ZERO THEN                                         0786
         LTR   RCREG,RCREG                                         0786
         BNZ   @RF00786                                            0786
*         DATAPTR=VADR;             /* SAVE PTR RETURNED BY ACCESS 0787
*                                      SERVICE ROUTINE               */
         LR    DATAPTR,VADR                                        0787
*       ELSE                        /* BAD RETURN CODE               */
*RETRY:                             /* RETRY ADDR FOR THE ESTAE      */
*         RCREG=4;                  /* SET A BAD RETURN CODE         */
         B     @RC00786                                            0788
@RF00786 DS    0H                                                  0788
RETRY    LA    RCREG,4                                             0788
*     END;                          /* END CALLER WAS NOT SNAP       */
@RC00786 DS    0H                                                  0790
*   RCCODE=RCREG;                   /* SAVE RETURN CODE FROM ACCESS
*                                      SERVICE RTN                   */
@RC00774 ST    RCREG,RCCODE                                        0790
*   RETURN CODE(RCCODE);                                           0791
         L     @07,RCCODE                                          0791
         LR    @15,@07                                             0791
@EL00011 DS    0H                                                  0791
@EF00011 DS    0H                                                  0791
@ER00011 L     @14,@SA00011                                        0791
         BR    @14                                                 0791
*   END ACCESRTN;                                                  0792
*   DCL                                                            0793
*     RT2PATCH CHAR(200) INIT(                                     0793
*         'PATCH AREA FOR IEAFTRT2 - IT IS 200 BYTES LONG');       0793
*   END IEAFTRT2                                                   0794
*                                                                  0794
*/* THE FOLLOWING INCLUDE STATEMENTS WERE FOUND IN THIS PROGRAM.     */
*/*%INCLUDE SYSLIB  (IHAABDPL)                                       */
*/*%INCLUDE SYSLIB  (CVT     )                                       */
