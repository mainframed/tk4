*   TR(SVRBADR,TRANS);              /* TRANSLATE TO PRINTABLE HEX    */
         TR    SVRBADR(9),TRANS                                    0646
*   HDR(1:LENGTH(SDWAHDR))=SDWAHDR; /* HEADER LINE                   */
         MVC   HDR(42),SDWAHDR                                     0647
*   BUFFER(31:(LENGTH(DUMMYHDR)+30))=DUMMYHDR;/* FILL IN HDR         */
         L     @14,ADPLBUF(,ABDPLPTR)                              0648
