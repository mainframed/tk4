         L     @14,ESTAPTR                                         0666
         MVC   LCALREGS(64,@14),RTYREGS(@14)                       0666
*   RETURN CODE(RCCODE);            /* END IEAFTSDW                  */
         L     @08,RCCODE                                          0667
         L     @13,4(,@13)                                         0667
         L     @00,@SIZDATD                                        0667
         LR    @01,@09                                             0667
         FREEMAIN R,LV=(0),A=(1)
         LR    @15,@08                                             0667
         L     @14,12(,@13)                                        0667
