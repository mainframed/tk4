*   STM(VADR,RCREG,LCALREGS);       /* SAVE REGS FOR RETRY           */
         STM   VADR,RCREG,LCALREGS(@14)                            0638
*   ADPLCOM1=ADDR(SDWAFMT);         /* ERROR MESSAGE                 */
         LA    @14,SDWAFMT                                         0639
         ST    @14,ADPLCOM1(,ABDPLPTR)                             0639
*   SAVEPTR=ADPLFMT2-START;         /* BEGINNING OF SDWA             */
