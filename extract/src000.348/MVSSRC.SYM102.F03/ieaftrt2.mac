@RF00658 DS    0H                                                  0665
*   DUMMYHDR=BLANKS(1:LENGTH(DUMMYHDR));/* CLEAR DUMMY HDR FOR NEXT
*                                      USER                          */
         MVC   DUMMYHDR(70),BLANKS                                 0665
*   LCALREGS=RTYREGS;               /* RESTORE CALLERS EXPECTED RETRY
