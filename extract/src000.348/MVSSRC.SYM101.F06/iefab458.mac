         TITLE 'IEFAB458---DCB RESOLUTION                              *
                        '
IEFAB458 CSECT ,                                                01S0001
@MAINENT DS    0H                                               01S0001
         USING *,@15                                            01S0001
         B     @PROLOG                                          01S0001
         DC    AL1(16)                                          01S0001
         DC    C'IEFAB458  74.081'                              01S0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                  01S0001
         BALR  @12,0                                            01S0001
@PSTART  DS    0H                                               01S0001
         USING @PSTART,@12                                      01S0001
         L     @00,@SIZDATD                                     01S0001
         MODESET EXTKEY=SCHED,SAVEKEY=(2)
         GSPACE  R,LV=(0)
         LR    @11,@01                                          01S0001
         USING @DATD,@11                                        01S0001
         ST    @13,@SA00001+4                                   01S0001
         STC   @02,@OLDKEY                                      01S0001
         MODESET KEYADDR=(2)
         LM    @00,@02,20(@13)                                  01S0001
         ST    @11,8(,@13)                                      01S0001
         LR    @13,@11                                          01S0001
         MODESET EXTKEY=ZERO
         MVC   @PC00001(28),0(@01)                              01S0001
         MODESET EXTKEY=SCHED
*                                                                    */
*   IF SIODDSNL=ZERO THEN                                       01S0063
         L     @10,@PC00001+12                                  01S0063
         L     @10,SIOTPTR(,@10)                                01S0063
         CLI   SIODDSNL(@10),0                                  01S0063
         BNE   @RF00063                                         01S0063
*     IF SCTSGDGS=ON&SCTSNEW=ON THEN/* NEW GDG. THE BASE DSN @YM07486*/
         TM    SCTSGDGS(@10),B'10000000'                        01S0064
         BNO   @RF00064                                         01S0064
         TM    SCTSNEW(@10),B'00000100'                         01S0064
         BNO   @RF00064                                         01S0064
*       DO;                         /* WILL BE DETERMINED AND        */
*         DO SCAN=MAXDSNLN TO 1 BY-1 WHILE(JFCBDSNM(SCAN)=BLANK);
         LA    SCAN,44                                          01S0066
@DL00066 L     @10,@PC00001+16                                  01S0066
         L     @10,JFCBPTR(,@10)                                01S0066
         ALR   @10,SCAN                                         01S0066
         AL    @10,@CF00893                                     01S0066
         CLI   JFCBDSNM(@10),C' '                               01S0066
         BNE   @DC00066                                         01S0066
*         END;                      /* COPIED INTO THE 'OBTAIN'      */
         BCTR  SCAN,0                                           01S0067
         LTR   SCAN,SCAN                                        01S0067
         BP    @DL00066                                         01S0067
@DC00066 DS    0H                                               01S0068
*         OBDSNAME=JFCBDSNM(1:SCAN-9);/* PARAMETER LIST.     @YM04955*/
         MVI   OBDSNAME+1,C' '                                  01S0068
         MVC   OBDSNAME+2(42),OBDSNAME+1                        01S0068
         LR    @10,SCAN                                         01S0068
         SH    @10,@CH00888                                     01S0068
         L     @03,@PC00001+16                                  01S0068
         L     @03,JFCBPTR(,@03)                                01S0068
         EX    @10,@SM00896                                     01S0068
*         OBVOLSER=HWCATSER;        /* HSKWA CONTAINS THE    @YM04955
*                                      VOLUME SERIAL         @YM04955*/
         L     @10,@PC00001+4                                   01S0069
         L     @10,HWDATAP(,@10)                                01S0069
         MVC   OBVOLSER(6),HWCATSER(@10)                        01S0069
*       END;                                                    01S0070
*     ELSE                                                      01S0071
*       ;                           /* NULL FOR GDG TEST     @YM07486*/
@RF00064 DS    0H                                               01S0072
*   ELSE                            /* DCB = DSN WAS SPECIFIED       */
*     DO;                           /* CALL SWA INTERFACE TO 'READ   */
         B     @RC00063                                         01S0072
@RF00063 DS    0H                                               01S0073
*       SWACTLS=ZERO;               /* LOCATE' THE DSN TABLE TO      */
         SLR   @10,@10                                          01S0073
         STH   @10,SWACTLS                                      01S0073
*       SWREAD=ON;                  /* RETRIEVE THE DSNAME           */
         OI    SWREAD,B'00100000'                               01S0074
*       IF HWLEPARP^=ZERO THEN      /* USE THE HSKP 'READ' EPA       */
         L     @15,@PC00001+4                                   01S0075
         L     @01,HWDATAP(,@15)                                01S0075
         C     @10,HWLEPARP(,@01)                               01S0075
         BE    @RF00075                                         01S0075
*         SWMOD=ON;                 /* IF IT EXISTS, OR HAVE SWA     */
         OI    SWMOD,B'00000010'                                01S0076
*       ELSE                        /* INTERFACE CREATE ONE.         */
*         SWNEW=ON;                                             01S0077
         B     @RC00075                                         01S0077
@RF00075 OI    SWNEW,B'00001000'                                01S0077
*       DSNTSVAP=SCTADSTB;          /* SVA OF FIRST DSNT             */
@RC00075 L     @10,@PC00001+20                                  01S0078
         L     @10,SCTPTR(,@10)                                 01S0078
         MVC   DSNTSVAP(3),SCTADSTB(@10)                        01S0078
*       DO BLKSCTR=1 TO SIODSNTE/DSNTBLN+1;/* NUMBER TO BLOCKS TO
*                                      READ                          */
         LA    BLKSCTR,1                                        01S0079
         B     @DE00079                                         01S0079
@DL00079 DS    0H                                               01S0080
*         CALL IEFAB4F7(SWACTLS,HWLEPARP,0,ADDR(DSNTSVAP),/* READ
*                                      DSNT                          */
*             ADDR(DSNTPTR),0,1);                               01S0080
         LA    @10,SWACTLS                                      01S0080
         ST    @10,@AL00001                                     01S0080
         L     @10,@PC00001+4                                   01S0080
         L     @10,HWDATAP(,@10)                                01S0080
         LA    @10,HWLEPARP(,@10)                               01S0080
         ST    @10,@AL00001+4                                   01S0080
         LA    @10,@CF00062                                     01S0080
         ST    @10,@AL00001+8                                   01S0080
         LA    @10,DSNTSVAP                                     01S0080
         ST    @10,@TF00001                                     01S0080
         LA    @10,@TF00001                                     01S0080
         ST    @10,@AL00001+12                                  01S0080
         LA    @10,DSNTPTR                                      01S0080
         ST    @10,@TF00002                                     01S0080
         LA    @10,@TF00002                                     01S0080
         ST    @10,@AL00001+16                                  01S0080
         LA    @10,@CF00062                                     01S0080
         ST    @10,@AL00001+20                                  01S0080
         LA    @10,@CF00064                                     01S0080
         ST    @10,@AL00001+24                                  01S0080
         L     @15,@CV00049                                     01S0080
         LA    @01,@AL00001                                     01S0080
         BALR  @14,@15                                          01S0080
*         DSNTSVAP=DSNTNSVA;        /* AND GET SVA OF NEXT DSNT.     */
         L     @10,DSNTPTR                                      01S0081
         MVC   DSNTSVAP(3),DSNTNSVA(@10)                        01S0081
*         SWMOD=ON;                 /* USE THE SAME EPA              */
*         SWNEW=OFF;                                            01S0083
         OI    SWMOD,B'00000010'                                01S0083
         NI    SWNEW,B'11110111'                                01S0083
*       END;                                                    01S0084
         AH    BLKSCTR,@CH00064                                 01S0084
@DE00079 L     @10,@PC00001+12                                  01S0084
         L     @10,SIOTPTR(,@10)                                01S0084
         LH    @00,SIODSNTE(,@10)                               01S0084
         SRDA  @00,32                                           01S0084
         D     @00,@CF00129                                     01S0084
         AH    @01,@CH00064                                     01S0084
         CR    BLKSCTR,@01                                      01S0084
         BNH   @DL00079                                         01S0084
*       OFFSET=SIODSNTE//DSNTBLN+1; /* LOCATION OF DSN IN DSNT       */
         L     @10,@PC00001+12                                  01S0085
         L     @15,SIOTPTR(,@10)                                01S0085
         LH    @00,SIODSNTE(,@15)                               01S0085
         SRDA  @00,32                                           01S0085
         D     @00,@CF00129                                     01S0085
         AH    @00,@CH00064                                     01S0085
         LR    OFFSET,@00                                       01S0085
*       OBDSNAME=DSNENTRY(OFFSET:OFFSET+SIODDSNL-1);/* COPY THE DSN  */
         MVI   OBDSNAME+1,C' '                                  01S0086
         MVC   OBDSNAME+2(42),OBDSNAME+1                        01S0086
         SLR   @03,@03                                          01S0086
         IC    @03,SIODDSNL(,@15)                               01S0086
         BCTR  @03,0                                            01S0086
         L     @01,DSNTPTR                                      01S0086
         ALR   @01,OFFSET                                       01S0086
         EX    @03,@SM00901                                     01S0086
*       HWJLCTLS=ZERO;              /* TO 'OBTAIN' PARAMETER LIST    */
         L     @15,@PC00001                                     01S0087
         L     @14,HWCNTRLP(,@15)                               01S0087
         SLR   @09,@09                                          01S0087
         STH   @09,HWJLCTLS(,@14)                               01S0087
*       HWSYSLOC=ON;                /* HAVE JLOCATE ISSUE A LOCATE   */
         OI    HWSYSLOC(@14),B'00100000'                        01S0088
*       CALL IEFAB469(HWCNTRLP,HWDATAP,HWRTNP,SIOTPTR,JFCBPTR,ADDR(
*           OBDSNAME));             /* TO OBTAIN THE VOL SER         */
         ST    @15,@AL00001                                     01S0089
         L     @15,@PC00001+4                                   01S0089
         ST    @15,@AL00001+4                                   01S0089
         L     @15,@PC00001+8                                   01S0089
         ST    @15,@AL00001+8                                   01S0089
         ST    @10,@AL00001+12                                  01S0089
         L     @10,@PC00001+16                                  01S0089
         ST    @10,@AL00001+16                                  01S0089
         LA    @10,OBDSNAME                                     01S0089
         ST    @10,@TF00001                                     01S0089
         LA    @10,@TF00001                                     01S0089
         ST    @10,@AL00001+20                                  01S0089
         L     @15,@CV00048                                     01S0089
         LA    @01,@AL00001                                     01S0089
         BALR  @14,@15                                          01S0089
*       IF HWRTCD=SUCCESS THEN      /* JLOCATE SUCCESSFUL            */
         L     @10,@PC00001+8                                   01S0090
         L     @10,HWRTNP(,@10)                                 01S0090
         ICM   @15,3,HWRTCD(@10)                                01S0090
         BNZ   @RF00090                                         01S0090
*         IF HWGDGALL=GDGALL THEN   /* REF DATA SET A GDG ALL,       */
         L     @15,@PC00001                                     01S0091
         L     @01,HWCNTRLP(,@15)                               01S0091
         TM    HWGDGALL(@01),B'00010000'                        01S0091
         BNO   @RF00091                                         01S0091
*           DO;                     /* ERROR CODES INTO HSKPWA       */
*             HWRTCD=DDERROR;       /* RETURN CODE - 4               */
         MVC   HWRTCD(2,@10),@CH00074                           01S0093
*             HWRSNCD=GDGALLRF;     /* REASON CODE - 5               */
         MVC   HWRSNCD(2,@10),@CH00076                          01S0094
*           END;                                                01S0095
*         ELSE                      /* IF NOT GDG ALL THEN           */
*           OBVOLSER=CTGVLVOL(1);   /* COPY THE VOL SER TO THE DSNT  */
         B     @RC00091                                         01S0096
@RF00091 L     @10,@PC00001+24                                  01S0096
         L     @10,CTGPLPTR(,@10)                               01S0096
         L     @10,CTGWKA(,@10)                                 01S0096
         L     @10,CTGWAVL(,@10)                                01S0096
         MVC   OBVOLSER(6),CTGVLVOL(@10)                        01S0096
*     END;                                                      01S0097
@RC00091 DS    0H                                               01S0097
@RF00090 DS    0H                                               01S0098
*   IF HWRTCD=SUCCESS THEN          /* NO ERRORS.                    */
@RC00063 L     @10,@PC00001+8                                   01S0098
         L     @10,HWRTNP(,@10)                                 01S0098
         ICM   @15,3,HWRTCD(@10)                                01S0098
         BNZ   @RF00098                                         01S0098
*     IF SCTSGDGS=OFF×(SCTSNEW=ON×SIODDSNL^=0) THEN/* NON-GDG OR NEW
*                                      GDG OR OLD GDG WITH DCB=DSN
*                                                            @YM07486*/
         L     @10,@PC00001+12                                  01S0099
         L     @10,SIOTPTR(,@10)                                01S0099
         TM    SCTSGDGS(@10),B'10000000'                        01S0099
         BZ    @RT00099                                         01S0099
         TM    SCTSNEW(@10),B'00000100'                         01S0099
         BO    @RT00099                                         01S0099
         CLI   SIODDSNL(@10),0                                  01S0099
         BE    @RF00099                                         01S0099
@RT00099 DS    0H                                               01S0100
*       DO;                         /* THEN PATTERN.            01S0100
*                                                                    */
*                                                               02S0101
*/********************************************************************
*                                                                    */
*/*                                                                  */
*/* SEGMENT NAME - OBTNDSCB                                          */
*/*                                                                  */
*/* DESCRIPTIVE NAME - OBTAIN DSCB ROUTINE                           */
*/*                                                                  */
*/* FUNCTION - TO OBTAIN THE FMT1 DSCB FOR THE SPECIFIED DSN         */
*/*                                                                  */
*/*   OPERATION - UPON ENTRY THE OBTAIN PARAMETER LIST IS INITIALIZED*/
*/*               AN OBTAIN IS ISSUED TO RETRIEVE THE DSCB. IF OBTAIN*/
*/*               IS UNSUCCESSFUL APPROPRIATE RETURN AND REASON      */
*/*               CODES ARE SET.                                     */
*/*                                                                  */
*/* ENTRY - NONE                                                     */
*/*   PURPOSE - SEE FUNCTION                                         */
*/*                                                                  */
*/*   INPUT DATA - N/A                                               */
*/*                                                                  */
*/* EXIT - NONE                                                      */
*/*   CONDITION - THE DCSB HAS BEEN RETRIEVED OR OBTAIN COMPLETED    */
*/*               UNSUCCESSFULLY.                                    */
*/*                                                                  */
*/*   OUTPUT DATA - THE DSCB HAS BEEN READ INTO THE WORKAREA. IF AN  */
*/*                 ERROR HAS OCCURRED THE RETURN INFORMATION AREA   */
*/*                 CONTAINS THE APPROPRIATE RETURN AND REASON CODES.*/
*/*                                                                  */
*/*   RETURN CODES - 0 - SUCCESS                                     */
*/*                  4 - ERROR                                       */
*/*                                                                  */
*/********************************************************************/
*                                                               02S0101
*         OBOPCODE=DSNOBTN;         /* SET OBTAIN OP CODE            */
         MVC   OBOPCODE(4),DSNOBTN                              02S0101
*         OBDSNPTR=ADDR(OBDSNAME);  /* GET ADDR OF SPECIFIED DSN     */
         LA    @10,OBDSNAME                                     02S0102
         ST    @10,OBDSNPTR                                     02S0102
*         OBVLSPTR=ADDR(OBVOLSER);  /* GET ADDR OF REQ VOL SER       */
         LA    @10,OBVOLSER                                     02S0103
         ST    @10,OBVLSPTR                                     02S0103
*         OBWRKPTR=ADDR(OBWORKAR);  /* GET ADDR OF OBTAIN WORKAREA   */
         LA    @10,OBWORKAR                                     02S0104
         ST    @10,OBWRKPTR                                     02S0104
*         RESPECIFY                                             02S0105
*          (R1,                                                 02S0105
*           R15) RSTD;              /* RESTRICT PARAMETER REGS       */
*         R1=ADDR(OBPLIST);         /* INIT THE PTR TO PARM LIST     */
         LA    R1,OBPLIST                                       02S0106
*         GEN( OBTAIN (R1) ) ;      /* 'OBTAIN' THE FORMAT 1 DSCB    */
          OBTAIN (R1)
*         IF R15^=ZERO THEN         /* OBTAIN UNSUCCESSFUL           */
         LTR   R15,R15                                          02S0108
         BZ    @RF00108                                         02S0108
*           IF R15=4 THEN           /* VOLUME NOT MOUNTED            */
         LA    @14,4                                            02S0109
         CR    R15,@14                                          02S0109
         BNE   @RF00109                                         02S0109
*             DO;                                               02S0110
*               HWRTCD=DDERROR;     /* RETURN CODE - 4               */
         L     @10,@PC00001+8                                   02S0111
         L     @10,HWRTNP(,@10)                                 02S0111
         STH   @14,HWRTCD(,@10)                                 02S0111
*               HWRSNCD=VOLNTMTD;   /* REASON CODE - 67              */
         MVC   HWRSNCD(2,@10),@CH00096                          02S0112
*             END;                                              02S0113
*           ELSE                    /* VOLUME WAS MOUNTED            */
*             IF R15=8 THEN         /* DSCB NOT FOUND                */
         B     @RC00109                                         02S0114
@RF00109 CH    R15,@CH00094                                     02S0114
         BNE   @RF00114                                         02S0114
*               DO;                                             02S0115
*                 HWRTCD=DDERROR;   /* RETURN CODE - 4               */
         L     @14,@PC00001+8                                   02S0116
         L     @14,HWRTNP(,@14)                                 02S0116
         MVC   HWRTCD(2,@14),@CH00074                           02S0116
*                 HWRSNCD=RECNTFND; /* REASON CODE - 68              */
         MVC   HWRSNCD(2,@14),@CH00098                          02S0117
*               END;                                            02S0118
*             ELSE                  /* I/O ERROR                     */
*               DO;                                             02S0119
         B     @RC00114                                         02S0119
@RF00114 DS    0H                                               02S0120
*                 HWRTCD=STPERROR;  /* RETURN CODE - 8               */
         L     @14,@PC00001+8                                   02S0120
         L     @14,HWRTNP(,@14)                                 02S0120
         MVC   HWRTCD(2,@14),@CH00094                           02S0120
*                 HWRSNCD=IOERROR;  /* REASON CODE - 14              */
         MVC   HWRSNCD(2,@14),@CH00100                          02S0121
*               END;                                            02S0122
*         RESPECIFY                                             02S0123
*          (R1,                                                 02S0123
*           R15) UNRSTD;            /* RELEASE THE PARM REGS    02S0123
*                                                                    */
@RC00114 DS    0H                                               02S0123
@RC00109 DS    0H                                               02S0123
@RF00108 DS    0H                                               02S0124
*         IF HWRTCD=SUCCESS THEN    /* WAS OBTAIN SUCCESSFUL ??      */
         SLR   @10,@10                                          01S0124
         L     @15,@PC00001+8                                   01S0124
         L     @01,HWRTNP(,@15)                                 01S0124
         CH    @10,HWRTCD(,@01)                                 01S0124
         BNE   @RF00124                                         01S0124
*           DO;                     /* UPDATE THE JFCB          01S0125
*                                                                    */
*                                                               02S0126
*/********************************************************************/
*/*                                                                  */
*/* SEGMENT NAME - UPDTJFCB                                          */
*/*                                                                  */
*/* DESCRIPTIVE NAME - JFCB UPDATE ROUTINE                           */
*/*                                                                  */
*/* FUNCTION - TO UPDATE REQUIRED FIELDS IN THE JFCB WITH APPROPRIATE*/
*/*            FIELDS FROM THE DSCB.                                 */
*/*                                                                  */
*/*   OPERATION - A 'ZERO MERGE' IS PERFORMED TO UPDATE REQUIRED,    */
*/*               ZERO FIELDS IN THE CURRENT JFCB WITH NECESSARY     */
*/*               LIKE FIELDS IN THE FORMAT 1 DSCB.                  */
*/*               IF THE EXPIRATION DATE IS ALL HEX 'FF'S, (SET BY   */
*/*               INTERPRETER TO INDICATE A ZERO FIELD), IT WILL BE  */
*/*               RESET TO A PROPER VALUE OF ZERO.                   */
*/*                                                                  */
*/* ENTRY NONE                                                       */
*/*   PURPOSE - SINGLE ENTRY POINT                                   */
*/*                                                                  */
*/*   INPUT DATA - FORMAT 1 DSCB AND CURRENT JFCB                    */
*/*                                                                  */
*/* EXIT - INLINE                                                    */
*/*   CONDITION - ALL                                                */
*/*                                                                  */
*/*   OUTPUT DATA - AN UPDATED JFCB                                  */
*/*                                                                  */
*/*   RETURN CODE - NONE                                             */
*/*                                                                  */
*/********************************************************************/
*                                                               02S0126
*             F1PTR=ADDR(OBWORKAR)-44;/* MAKE THE DSCB ADDRESSABLE   */
         LA    F1PTR,OBWORKAR                                   02S0126
         SH    F1PTR,@CH00078                                   02S0126
*             IF JFCBVLSQ=ZERO THEN /* IF VOL SEQ ZEROED             */
         L     @03,@PC00001+16                                  02S0127
         L     @03,JFCBPTR(,@03)                                02S0127
         CH    @10,JFCBVLSQ(,@03)                               02S0127
         BNE   @RF00127                                         02S0127
*               JFCBVLSQ=DS1VOLSQ;  /* MOVE VOL SEQ FROM DSCB        */
         MVC   JFCBVLSQ(2,@03),DS1VOLSQ(F1PTR)                  02S0128
*             IF JFCBXPDT=ZEROS THEN/* IF EXPIRATION DATE ZEROED     */
@RF00127 L     @10,@PC00001+16                                  02S0129
         L     @10,JFCBPTR(,@10)                                02S0129
         CLC   JFCBXPDT(3,@10),ZEROS                            02S0129
         BNE   @RF00129                                         02S0129
*               JFCBXPDT=DS1EXPDT;  /* MOVE DATE FROM DSCB           */
         MVC   JFCBXPDT(3,@10),DS1EXPDT(F1PTR)                  02S0130
*             IF JFCBXPDT=EXPTFFFF THEN/* EXPIRATION DATE WAS ZERO,  */
@RF00129 L     @10,@PC00001+16                                  02S0131
         L     @10,JFCBPTR(,@10)                                02S0131
         CLC   JFCBXPDT(3,@10),EXPTFFFF                         02S0131
         BNE   @RF00131                                         02S0131
*               JFCBXPDT=ZEROS;     /* RESET TO ZERO.                */
         MVC   JFCBXPDT(3,@10),ZEROS                            02S0132
*             IF JFCDSORG=ZEROS THEN/* IF DSORG NOT SPECIFIED        */
@RF00131 L     @10,@PC00001+16                                  02S0133
         L     @10,JFCBPTR(,@10)                                02S0133
         CLC   JFCDSORG(2,@10),ZEROS                            02S0133
         BNE   @RF00133                                         02S0133
*               JFCDSORG=DS1DSORG;  /* MOVE DSORG FROM DSCB          */
         MVC   JFCDSORG(2,@10),DS1DSORG(F1PTR)                  02S0134
*             IF JFCRECFM=ZEROS THEN/* IF RECORD FMT NOT SPECIFIED   */
@RF00133 L     @10,@PC00001+16                                  02S0135
         L     @10,JFCBPTR(,@10)                                02S0135
         CLC   JFCRECFM(1,@10),ZEROS                            02S0135
         BNE   @RF00135                                         02S0135
*               JFCRECFM=DS1RECFM;  /* MOVE REC FMT FROM DSCB        */
         MVC   JFCRECFM(1,@10),DS1RECFM(F1PTR)                  02S0136
*             IF JFCOPTCD=ZEROS THEN/* IF OPTIONS CODES NOT SPECIFIED*/
@RF00135 L     @10,@PC00001+16                                  02S0137
         L     @10,JFCBPTR(,@10)                                02S0137
         CLC   JFCOPTCD(1,@10),ZEROS                            02S0137
         BNE   @RF00137                                         02S0137
*               JFCOPTCD=DS1OPTCD;  /* USE OPTIONS CODES FROM DSCB   */
         MVC   JFCOPTCD(1,@10),DS1OPTCD(F1PTR)                  02S0138
*             IF JFCBLKSI=ZERO THEN /* IF BLOCKSIZE NOT SPECIFIED    */
@RF00137 L     @10,@PC00001+16                                  02S0139
         L     @10,JFCBPTR(,@10)                                02S0139
         ICM   @03,3,JFCBLKSI(@10)                              02S0139
         BNZ   @RF00139                                         02S0139
*               JFCBLKSI=DS1BLKL;   /* USE BLOCKSIZE FROM DSCB       */
         MVC   JFCBLKSI(2,@10),DS1BLKL(F1PTR)                   02S0140
*             IF JFCLRECL=ZERO THEN /* IF LOGICAL RECORD LENGTH ZERO */
@RF00139 L     @10,@PC00001+16                                  02S0141
         L     @10,JFCBPTR(,@10)                                02S0141
         ICM   @03,3,JFCLRECL(@10)                              02S0141
         BNZ   @RF00141                                         02S0141
*               JFCLRECL=DS1LRECL;  /* USE RECORD LENGTH FROM DSCB   */
         MVC   JFCLRECL(2,@10),DS1LRECL(F1PTR)                  02S0142
*             IF JFCKEYLE=ZEROS THEN/* IF KEY LENGTH NOT SPECIFIED   */
@RF00141 L     @10,@PC00001+16                                  02S0143
         L     @10,JFCBPTR(,@10)                                02S0143
         CLC   JFCKEYLE(1,@10),ZEROS                            02S0143
         BNE   @RF00143                                         02S0143
*               JFCKEYLE=DS1KEYL;   /* USE KEY LENGTH FROM DSCB      */
         MVC   JFCKEYLE(1,@10),DS1KEYL(F1PTR)                   02S0144
