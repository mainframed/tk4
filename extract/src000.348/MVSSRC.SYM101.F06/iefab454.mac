         TITLE 'IEFAB454 - DD FUNCTION CONTROL                         *
                        '
IEFAB454 CSECT ,                                                01S0001
@MAINENT DS    0H                                               01S0001
         USING *,@15                                            01S0001
         B     @PROLOG                                          01S0001
         DC    AL1(16)                                          01S0001
         DC    C'IEFAB454  74.122'                              01S0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                  01S0001
         BALR  @12,0                                            01S0001
@PSTART  DS    0H                                               01S0001
         USING @PSTART,@12                                      01S0001
         L     @00,@SIZDATD                                     01S0001
         MODESET EXTKEY=SCHED,SAVEKEY=(2)
         GSPACE  R,LV=(0)
         LR    @11,@01                                          01S0001
         USING @DATD,@11                                        01S0001
         ST    @13,@SA00001+4                                   01S0001
         STC   @02,@OLDKEY                                      01S0001
         MODESET KEYADDR=(2)
         LM    @00,@02,20(@13)                                  01S0001
         ST    @11,8(,@13)                                      01S0001
         LR    @13,@11                                          01S0001
         MODESET EXTKEY=ZERO
         MVC   @PC00001(20),0(@01)                              01S0001
         MODESET EXTKEY=SCHED
*   HWRTCD=0;                       /* SET NORMAL RETURN CODE        */
         L     @10,@PC00001+8                                   01S0026
         L     @10,HWRTNP(,@10)                                 01S0026
         SLR   @15,@15                                          01S0026
         STH   @15,HWRTCD(,@10)                                 01S0026
*   IF HWEDT=YES THEN               /* IF THE UNIT PARAMETER WAS     */
         L     @10,@PC00001                                     01S0027
         L     @10,HWCNTRLP(,@10)                               01S0027
         TM    HWEDT(@10),B'10000000'                           01S0027
         BNO   @RF00027                                         01S0027
*     DEVPTR=ADDR(HWEDTLUV);        /* CONVERTED VIA EDT THEN USE LUV*/
         L     @10,@PC00001+4                                   01S0028
         L     @10,HWDATAP(,@10)                                01S0028
         MVC   DEVPTR(4),HWUCLPTR(@10)                          01S0028
*   ELSE                                                        01S0029
*     IF HWUCB=YES THEN             /* CONVERTED VIA UCB THEN USE UCB*/
         B     @RC00027                                         01S0029
@RF00027 L     @10,@PC00001                                     01S0029
         L     @10,HWCNTRLP(,@10)                               01S0029
         TM    HWUCB(@10),B'00000001'                           01S0029
         BNO   @RF00029                                         01S0029
*       DEVPTR=ADDR(HWDEVTYP);      /* DEVICE TYPE FOR CLASS CHECK   */
         L     @10,@PC00001+4                                   01S0030
         L     @10,HWDATAP(,@10)                                01S0030
         L     @10,HWUCLPTR(,@10)                               01S0030
         LA    @10,HWDEVTYP(,@10)                               01S0030
         ST    @10,DEVPTR                                       01S0030
*   RFY                                                         01S0031
*     HWDEV BASED(DEVPTR);                                      01S0031
@RF00029 DS    0H                                               01S0031
@RC00027 DS    0H                                               01S0032
*   IF(HWUCB=NO&HWEDT=NO)           /* IF UNIT WAS CODED THEN        */
*       ×HWTAPE=YES×HWDA=YES THEN   /* IT MUST BE TAPE OR DA         */
         L     @10,@PC00001                                     01S0032
         L     @10,HWCNTRLP(,@10)                               01S0032
         TM    HWUCB(@10),B'00000001'                           01S0032
         BNZ   @GL00001                                         01S0032
         TM    HWEDT(@10),B'10000000'                           01S0032
         BZ    @RT00032                                         01S0032
@GL00001 L     @10,DEVPTR                                       01S0032
         TM    HWTAPE(@10),B'10100000'                          01S0032
         BZ    @RF00032                                         01S0032
@RT00032 DS    0H                                               01S0033
*     DO;                           /* TO BE IN PDI OR CATALOG.      */
*       IF(SCTSGDGS=NO              /* IF DD IS NOT GDG SINGLE  01S0034
*                                                            @YM06106*/
*           ×(SCTRECVD=YES×JFCCAT=YES))/* OR A RECEIVED GDG, @YM06106*/
*           &SCTSNEW=NO             /* NOT DISP=NEW,                 */
*           &SCTVOLAF=NO            /* NO VOL=REF, NO VOLSERS OR     */
*           &(SCTVOLCT=0            /* VOLSERS FOR RECEIVED OR  01S0034
*                                      CATALOGED DATA SET    @YM06106*/
*           ×(SCTRECVD=YES×JFCCAT=YES))/* DURING CHKPT RESTART, 01S0034
*                                                            @YM06106*/
*           &HWDSNREC=NO&HWVUREC=NO /* NOT DSN OR VOL/UNIT RECURSION,*/
*           &HWDSNRQD=YES THEN      /* AND DSN REQUIRED THEN GET GDG */
         L     @10,@PC00001+12                                  01S0034
         L     @10,CSIOTP(,@10)                                 01S0034
         TM    SCTSGDGS(@10),B'10000000'                        01S0034
         BZ    @GL00006                                         01S0034
         TM    SCTRECVD(@10),B'00000001'                        01S0034
         BO    @GL00006                                         01S0034
         L     @10,@PC00001+16                                  01S0034
         L     @10,CJFCBP(,@10)                                 01S0034
         TM    JFCCAT(@10),B'10000000'                          01S0034
         BNO   @RF00034                                         01S0034
@GL00006 L     @10,@PC00001+12                                  01S0034
         L     @10,CSIOTP(,@10)                                 01S0034
         TM    SCTSNEW(@10),B'00000100'                         01S0034
         BNZ   @RF00034                                         01S0034
         TM    SCTVOLAF(@10),B'00100000'                        01S0034
         BNZ   @RF00034                                         01S0034
         CLI   SCTVOLCT(@10),0                                  01S0034
         BE    @GL00003                                         01S0034
         TM    SCTRECVD(@10),B'00000001'                        01S0034
         BO    @GL00003                                         01S0034
         L     @10,@PC00001+16                                  01S0034
         L     @10,CJFCBP(,@10)                                 01S0034
         TM    JFCCAT(@10),B'10000000'                          01S0034
         BNO   @RF00034                                         01S0034
@GL00003 L     @10,@PC00001                                     01S0034
         L     @10,HWCNTRLP(,@10)                               01S0034
         TM    HWDSNRQD(@10),B'00100000'                        01S0034
         BNO   @RF00034                                         01S0034
         TM    HWDSNREC(@10),B'11000000'                        01S0034
         BNZ   @RF00034                                         01S0034
*         DO;                       /* ALL INDICATOR OR CRI/PDI INFO.
*                                                                    */
*                                                               02S0036
*/********************************************************************/
*/*                                                                  */
*/* SEGMENT NAME - ESTABDSN                                          */
*/*                                                                  */
*/* DESCRIPTIVE NAME -DDFC - ESTABLISH DATA SET LOCATION             */
*/*                                                                  */
*/* FUNCTION -  TO SET UP THE APPROPRIATE PARAMETERS AND ISSUE       */
*/*             JLOCATE TO FIND THE DATA SET LOCATION(PDI OR CATALOG)*/
*/*             AND CHARACTERISTICS(GDG ALL OR NOT) AND MAP THE      */
*/*             RETURN INFO INTO HSKPWA.                             */
*/*             MODIFY THE PDI FOR CHECKPOINT RESTART.       @YM08117*/
*/*             OBTAIN AND INITIALIZE CATALOG CONTROL        @YM06106*/
*/*             BLOCKS FOR CHECKPOINT RESTART.               @YM06106*/
*/* ENTRY -                                                          */
*/*   PURPOSE - TO SET UP JLOCATE CONTROLS AS A FUNCTION OF HSKPWA   */
*/*             CONTROLS THEN ISSUE JLOCATE.                         */
*/*                                                                  */
*/*             UPDATE THE PDI BY RECEIVING, BUT NOT COPYING @YM08117*/
*/*             VOLUME/UNIT INFORMATION, DATA SETS THAT WERE @YM08117*/
*/*             RECEIVED WHEN A RESTARTING STEP WAS          @YM08117*/
*/*             INITIALLY PROCESSED.                         @YM08117*/
*/*             GET A PCCB FOR DATA SETS REQUIRING           @YM06106*/
*/*             IMPLIED (NON JOB/STEPCATS) PRIVATE CATALOGS. @YM06106*/
*/*                                                                  */
*/*   INPUT DATA - POINTERS TO:HSKP CONTROLS,HSKP DATA AREA, HSKP    */
*/*                CURRENT SIOT                                      */
*/* EXITS -                                                          */
*/*   CONDITION - JLOCATE COMPLETE                                   */
*/*                                                                  */
*/*   OUTPUT DATA-HSKPWA CONTROLS UPDATED WITH                       */
*/*                  1. REFERENCED SIOT PRESENT(IE. DSN FOUND IN PDI)*/
*/*                  2. DATA SET FOUND IN CATALOG                    */
*/*                  3. GDG ALL                                      */
*/*                AND HSKPWA DATA AREA UPDATED WITH                 */
*/*                   1.REFERENCED SIOT POINTER FROM PDI             */
*/*                   2.INITIALIZED CRI(CATALOG RETURN INFORMATION). */
*/*                THIS OUTPUT IS A DIRECT RESULT OF JLOCATE RETURN  */
*/*                INFORMATION.                                      */
*/*                OR                                        @YM08117*/
*/*                PDI UPDATED FOR RESTARTING STEPS.         @YM08117*/
*/*                OR                                        @YM06106*/
*/*                PCCB FOR RESTARTING STEPS.                @YM06106*/
*/*                                                                  */
*/*   RETURN CODE- AS SET BY JLOCATE                                 */
*/*                                                                  */
*/*                                                                  */
*/********************************************************************/
*                                                               02S0036
*           HWJLCTLS=0;             /* ZERO JLOCATE CONTROLS         */
         SLR   @15,@15                                          02S0036
         STH   @15,HWJLCTLS(,@10)                               02S0036
*/*   INDICATE SCAN PDI FOR DSN, DELETE ENTRY IF FOUND, IF DATA SET  */
*/*   IS NON TEMPORARY ISSUE SYSTEM LOCATE FOR DSN NOT IN PDI AND    */
*/*   CREATE A DD FOR ANY CATALOGS REQUIRED                          */
*                                                               02S0037
*           IF SIOTJSCT=NO THEN     /* IF THIS ISN'T A JOBCAT        */
         L     @15,@PC00001+12                                  02S0037
         L     @01,CSIOTP(,@15)                                 02S0037
         TM    SIOTJSCT(@01),B'00000001'                        02S0037
         BNZ   @RF00037                                         02S0037
*             DO;                   /* OR STEPCAT THEN               */
*               HWPDISCN=YES;       /* SCAN PDI FOR DSN AND          */
*               HWPDIEDL=YES;       /* DELETE PDIE IF FOUND.         */
         OI    HWPDISCN(@10),B'11000000'                        02S0040
*             END;                  /* END NON-PVT. CAT ONLY.        */
*           IF SIOTGDSN=NO&SCTRECVD=NO THEN/* NON-TEMP DATA SET BUT
*                                      NOT A RESTART         @YM08117*/
@RF00037 L     @10,@PC00001+12                                  02S0042
         L     @10,CSIOTP(,@10)                                 02S0042
         TM    SIOTGDSN(@10),B'00010000'                        02S0042
         BNZ   @RF00042                                         02S0042
         TM    SCTRECVD(@10),B'00000001'                        02S0042
         BNZ   @RF00042                                         02S0042
*             HWSYSLOC=YES;         /* THEN ISSUE SYSTEM LOCATE      */
         L     @10,@PC00001                                     02S0043
         L     @10,HWCNTRLP(,@10)                               02S0043
         OI    HWSYSLOC(@10),B'00100000'                        02S0043
*           HWVSAME=YES;            /* RETURN DD REQUIREMENTS AS
*                                      DETERMINED BY CATALOG    02S0044
*                                      MANAGEMENT (IE. DSORG,   02S0044
*                                      DISPOSITION AND PARALLEL MOUNT*/
@RF00042 L     @10,@PC00001                                     02S0044
         L     @10,HWCNTRLP(,@10)                               02S0044
         OI    HWVSAME(@10),B'00000001'                         02S0044
*           HWDOMOD=YES;            /* PROCESS MOD DISPOSITION -
*                                      CHANGE TO NEW IF NECESSARY    */
         OI    HWDOMOD(@10),B'10000000'                         02S0045
*           IF SCTRECVD=YES×JFCCAT=YES THEN/* RESTART WITH      02S0046
*                                      RRECEIVED/CATALOGED DATA SET.
*                                                            @YM06106*/
         L     @10,@PC00001+12                                  02S0046
         L     @10,CSIOTP(,@10)                                 02S0046
         TM    SCTRECVD(@10),B'00000001'                        02S0046
         BO    @RT00046                                         02S0046
         L     @10,@PC00001+16                                  02S0046
         L     @10,CJFCBP(,@10)                                 02S0046
         TM    JFCCAT(@10),B'10000000'                          02S0046
         BNO   @RF00046                                         02S0046
@RT00046 DS    0H                                               02S0047
*             HWNOVOLU=YES;         /* DELETE PDI ENTRY OR SETUP
*                                      PRIVATE CATALOG, BUT DO NOT
*                                      RETURN INDICATORS.    @YM06106*/
         L     @10,@PC00001                                     02S0047
         L     @10,HWCNTRLP(,@10)                               02S0047
         OI    HWNOVOLU(@10),B'01000000'                        02S0047
*           CALL IEFAB469(HWCNTRLP,HWDATAP,HWRTNP,CSIOTP,CJFCBP,ADDR(
*               JFCBDSNM));         /* ISSUE JLOCATE            02S0048
*                                                                    */
@RF00046 L     @10,@PC00001                                     02S0048
         ST    @10,@AL00001                                     02S0048
         L     @10,@PC00001+4                                   02S0048
         ST    @10,@AL00001+4                                   02S0048
         L     @10,@PC00001+8                                   02S0048
         ST    @10,@AL00001+8                                   02S0048
         L     @10,@PC00001+12                                  02S0048
         ST    @10,@AL00001+12                                  02S0048
         L     @10,@PC00001+16                                  02S0048
         ST    @10,@AL00001+16                                  02S0048
         ST    @10,@AL00001+20                                  02S0048
         L     @15,@CV00048                                     02S0048
         LA    @01,@AL00001                                     02S0048
         BALR  @14,@15                                          02S0048
*         END;                                                  01S0049
*     END;                                                      01S0050
@RF00034 DS    0H                                               01S0051
*   IF HWRTCD=0 THEN                /* SHOULD PROCESSING CONTINUE    */
@RF00032 L     @10,@PC00001+8                                   01S0051
         L     @10,HWRTNP(,@10)                                 01S0051
         ICM   @15,3,HWRTCD(@10)                                01S0051
         BNZ   @RF00051                                         01S0051
*     DO;                           /* YES MAKE DSN RESOLUTION CHECK */
*       IF SCTSGDGS=YES×HWGDGALL=YES/* DSN RESOLUTION NEEDED FOR     */
*           ×HWDSNREC=YES THEN      /* GDG SINGLE,GDG ALL,DSN   01S0053
*                                      RECURSION                     */
         L     @10,@PC00001+12                                  01S0053
         L     @10,CSIOTP(,@10)                                 01S0053
         TM    SCTSGDGS(@10),B'10000000'                        01S0053
         BO    @RT00053                                         01S0053
         L     @10,@PC00001                                     01S0053
         L     @10,HWCNTRLP(,@10)                               01S0053
         TM    HWGDGALL(@10),B'10010000'                        01S0053
         BZ    @RF00053                                         01S0053
@RT00053 DS    0H                                               01S0054
*         CALL IEFAB456(HWCNTRLP,HWDATAP,HWRTNP,CSIOTP,CJFCBP,HWCRIP,
*             HWGDGNTP,HWJCTP);     /* RESOLVE DSN INFORMATION       */
         L     @10,@PC00001                                     01S0054
         ST    @10,@AL00001                                     01S0054
         L     @10,@PC00001+4                                   01S0054
         ST    @10,@AL00001+4                                   01S0054
         L     @15,@PC00001+8                                   01S0054
         ST    @15,@AL00001+8                                   01S0054
         L     @15,@PC00001+12                                  01S0054
         ST    @15,@AL00001+12                                  01S0054
         L     @15,@PC00001+16                                  01S0054
         ST    @15,@AL00001+16                                  01S0054
         L     @10,HWDATAP(,@10)                                01S0054
         LA    @15,HWCRIP(,@10)                                 01S0054
         ST    @15,@AL00001+20                                  01S0054
         LA    @15,HWGDGNTP(,@10)                               01S0054
         ST    @15,@AL00001+24                                  01S0054
         ST    @10,@AL00001+28                                  01S0054
         L     @15,@CV00044                                     01S0054
         LA    @01,@AL00001                                     01S0054
         BALR  @14,@15                                          01S0054
*     END;                                                      01S0055
@RF00053 DS    0H                                               01S0056
*   IF HWRTCD=0 THEN                /* BYPASS IF BAD RETURN CODE     */
@RF00051 L     @10,@PC00001+8                                   01S0056
         L     @01,HWRTNP(,@10)                                 01S0056
         ICM   @00,3,HWRTCD(@01)                                01S0056
         BNZ   @RF00056                                         01S0056
*     CALL IEFAB457(HWCNTRLP,HWDATAP,HWRTNP,CSIOTP,CJFCBP,HWCRIP,
*         HWRSIOTP);                /* RESOLVE VOL/UNIT INFORMATION  */
         L     @15,@PC00001                                     01S0057
         ST    @15,@AL00001                                     01S0057
         L     @15,@PC00001+4                                   01S0057
         ST    @15,@AL00001+4                                   01S0057
         ST    @10,@AL00001+8                                   01S0057
         L     @10,@PC00001+12                                  01S0057
         ST    @10,@AL00001+12                                  01S0057
         L     @10,@PC00001+16                                  01S0057
         ST    @10,@AL00001+16                                  01S0057
         L     @10,HWDATAP(,@15)                                01S0057
         LA    @15,HWCRIP(,@10)                                 01S0057
         ST    @15,@AL00001+20                                  01S0057
         LA    @10,HWRSIOTP(,@10)                               01S0057
         ST    @10,@AL00001+24                                  01S0057
         L     @15,@CV00045                                     01S0057
         LA    @01,@AL00001                                     01S0057
         BALR  @14,@15                                          01S0057
*   IF HWRTCD=0 THEN                /* IF RETURN IS OK AND ITS A GDG */
@RF00056 L     @10,@PC00001+8                                   01S0058
         L     @10,HWRTNP(,@10)                                 01S0058
         ICM   @15,3,HWRTCD(@10)                                01S0058
         BNZ   @RF00058                                         01S0058
*     DO;                           /* SINGLE OR DCB=DSNAME THEN GET
*                                      DCB                           */
*       IF JFCPAT=NO THEN           /* INFO. IF THIS ISN'T A RESTART */
         L     @10,@PC00001+16                                  01S0060
         L     @10,CJFCBP(,@10)                                 01S0060
         TM    JFCPAT(@10),B'00000001'                          01S0060
         BNZ   @RF00060                                         01S0060
*         IF SCTSGDGS=YES×SIODDSNL^=0 THEN/* WITH PATTERNING COMPLETE*/
         L     @10,@PC00001+12                                  01S0061
         L     @10,CSIOTP(,@10)                                 01S0061
         TM    SCTSGDGS(@10),B'10000000'                        01S0061
         BO    @RT00061                                         01S0061
         CLI   SIODDSNL(@10),0                                  01S0061
         BE    @RF00061                                         01S0061
@RT00061 DS    0H                                               01S0062
*           CALL IEFAB458(HWCNTRLP,HWDATAP,HWRTNP,CSIOTP,CJFCBP,HWSCTP,
*               HWCRIP);            /* DO DCB PATTERNING.            */
         L     @10,@PC00001                                     01S0062
         ST    @10,@AL00001                                     01S0062
         L     @10,@PC00001+4                                   01S0062
         ST    @10,@AL00001+4                                   01S0062
         L     @15,@PC00001+8                                   01S0062
         ST    @15,@AL00001+8                                   01S0062
         L     @15,@PC00001+12                                  01S0062
         ST    @15,@AL00001+12                                  01S0062
         L     @15,@PC00001+16                                  01S0062
         ST    @15,@AL00001+16                                  01S0062
         L     @10,HWDATAP(,@10)                                01S0062
         LA    @15,HWSCTP(,@10)                                 01S0062
         ST    @15,@AL00001+20                                  01S0062
         LA    @10,HWCRIP(,@10)                                 01S0062
         ST    @10,@AL00001+24                                  01S0062
         L     @15,@CV00046                                     01S0062
         LA    @01,@AL00001                                     01S0062
         BALR  @14,@15                                          01S0062
*     END;                                                      01S0063
@RF00061 DS    0H                                               01S0063
@RF00060 DS    0H                                               01S0064
*   IF HWRTCD=0 THEN                /* DO DISP. PROCESSING FOR ALL
*                                      DDS                           */
@RF00058 L     @10,@PC00001+8                                   01S0064
         L     @01,HWRTNP(,@10)                                 01S0064
         ICM   @00,3,HWRTCD(@01)                                01S0064
         BNZ   @RF00064                                         01S0064
*     CALL IEFAB459(HWCNTRLP,HWDATAP,HWRTNP,CSIOTP,CJFCBP);     01S0065
         L     @15,@PC00001                                     01S0065
         ST    @15,@AL00001                                     01S0065
         L     @15,@PC00001+4                                   01S0065
         ST    @15,@AL00001+4                                   01S0065
         ST    @10,@AL00001+8                                   01S0065
         L     @10,@PC00001+12                                  01S0065
         ST    @10,@AL00001+12                                  01S0065
         L     @10,@PC00001+16                                  01S0065
         ST    @10,@AL00001+16                                  01S0065
         L     @15,@CV00047                                     01S0065
         LA    @01,@AL00001                                     01S0065
         BALR  @14,@15                                          01S0065
*   END IEFAB454                                                01S0066
