*    MSGIDX = MSGIDX + ONE;            /*INCREMENT MESSAGE AREA 02S0264
*                                        INDEX               @G29AN2F*/
         AL    @14,@CF00040                                     02S0264
         ST    @14,MSGIDX                                       02S0264
*  END;                                /*                    @G29AN2F*/
*SARBSPTR = SIOTPTR;                   /*SAVE PTR TO SIOT FOR   02S0266
