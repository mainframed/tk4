         L     @15,LCLSASTP                                     01S0271
         LR    @14,I                                            01S0271
         MH    @14,@CH00959                                     01S0271
         LA    @01,0(@14,@15)                                   01S0271
         AL    @01,@CF01582                                     01S0271
         LH    @07,SASTNORQ-8(,@01)                             01S0271
         BCTR  @07,0                                            01S0271
         AL    @15,@CF01582                                     01S0271
         STH   @07,SASTNORQ-8(@14,@15)                          01S0271
*                      END;            /*                    @G29AN2F*/
*                    ELSE              /*NOT GROUP REQUEST   @G29AN2F*/
*                      DO;             /*DO SYSIN/SYSOUT     @G29AN2F*/
         B     @RC00236                                         01S0273
@RF00236 DS    0H                                               01S0274
*                        CALL ALOCNGRP;/*ALLOCATE NON-GROUP REQUESTS
