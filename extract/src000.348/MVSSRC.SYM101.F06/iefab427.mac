         TITLE 'IEFAB427 -ALLOCATE SUBSYSTEM DATASETS                  *
                        '
IEFAB427 CSECT ,                                                01S0001
@MAINENT DS    0H                                               01S0001
         USING *,@15                                            01S0001
         B     @PROLOG                                          01S0001
         DC    AL1(24)                                          01S0001
         DC    C'IEFAB427 78108  UZ18216 '                      01S0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                  01S0001
         BALR  @12,0                                            01S0001
@PSTART  DS    0H                                               01S0001
         USING @PSTART,@12                                      01S0001
         L     @00,@SIZDATD                                     01S0001
         MODESET EXTKEY=SCHED,SAVEKEY=(2)
         GSPACE   R,LV=(0)
         LR    @11,@01                                          01S0001
         USING @DATD,@11                                        01S0001
         ST    @13,@SA00001+4                                   01S0001
         STC   @02,@OLDKEY                                      01S0001
         MODESET KEYADDR=(2)
         LM    @00,@02,20(@13)                                  01S0001
         ST    @11,8(,@13)                                      01S0001
         LR    @13,@11                                          01S0001
         MODESET EXTKEY=ZERO
         MVC   @PC00001(4),0(@01)                               01S0001
         MODESET EXTKEY=SCHED
*RETCODE = ZERO;                       /*CLEAR RETURN CODE   @G29AN2F*/
         SLR   RETCODE,RETCODE                                  01S0082
*/********************************************************************/
*/*                                                                  */
*/* SEGMENT NAME - BILDSSOB                                          */
*/*                                                                  */
*/* DESCRIPTIVE NAME - BUILD SUBSYSTEM INTERFACE COMMON BLOCK        */
*/*                                                                  */
*/* FUNCTION -                                                       */
*/*   THE PURPOSE OF THIS ROUTINE IS TO BUILD THE SSOB BLOCK,        */
*/*   WHICH IS PART OF THS SUBSYSTEM INTERFACE, (ALLOCATE SUBSYSTEM  */
*/*   REQUESTS). IT ALSO FILLS IN CANCEL AND WAIT INDICATOR FIELDS   */
*/*   IN THE SSAL BLOCK.                                             */
*/*                                                                  */
*/* ENTRY - INLINE SUBROUTINE                                        */
*/*                                                                  */
*/*   PURPOSE - SEE FUNCTION                                         */
*/*                                                                  */
*/* IN LINE SUBROUTINES -                                            */
*/*    BILDSSOB  INITIALIZE SUBSYSTEM INTERFACE COMMON  BLOCK        */
*/*    BILDSSAL  BUILD SSAL BLOCK                                    */
*/*                                                                  */
*/*   INPUT -                                                        */
*/*     FMAPPTR  - POINTER TO FUNCTION MAP                           */
*/*     ALCWAPTR - POINTER TO ALLOCATION WORK AREA                   */
*/*     SIOTPTR  - POINTER TO SIOT                                   */
*/*                                                                  */
*/*                                                                  */
*/* EXIT - INLINE                                                    */
*/*                                                                  */
*/*   CONDITON - SSOB BLOCK CONSTRUCTED                              */
*/*              SSAL PARTIALLY CONSTRUCTED                          */
*/*                                                                  */
*/*     OUTPUT -                                                     */
*/*       SSOB BLOCK                                                 */
*/*       SSALCNCL,SSALWAIT,SSALDYNM SET                             */
*/*                                                                  */
*/*     RETURN CODE - (NONE)                                         */
*/*                                                                  */
*/*                                                                  */
*/********************************************************************/
*SSOB = SSOB && SSOB;                  /*CLEAR SSOB                  */
         XC    SSOB(20),SSOB                                    02S0083
*SSAL = SSAL && SSAL;                  /*CLEAR SSAL                  */
         XC    SSAL(64),SSAL                                    02S0084
*SSOBID   = CNSTSSOB;                  /*CNTRL BLOCK IDENTIFIER      */
         MVC   SSOBID(4),CNSTSSOB                               02S0085
*SSOBLEN  = LENGTH(SSOB);              /*LENGTH OF SSOB HEADER       */
         MVC   SSOBLEN(2),@CH01157                              02S0086
*SSOBFUNC = SSOBALOC;                  /*FUNCTION IS ALLOCATION      */
         MVC   SSOBFUNC(2),@CH01038                             02S0087
*SSOBINDV = ADDR(SSAL);                /*POINT TO SSAL BLOCK         */
         LA    @08,SSAL                                         02S0088
         ST    @08,SSOBINDV                                     02S0088
*SSALLEN = LENGTH(SSAL);               /*BUILD SSAL                  */
         MVC   SSALLEN(2),@CH01575                              02S0089
*IF WAITOKSW = ON THEN                 /*CHECK FUNCTION MAP          */
         L     @08,@PC00001                                     02S0090
         L     @08,ALCWAPTR(,@08)                               02S0090
         L     @01,FMAPPTR(,@08)                                02S0090
         TM    WAITOKSW(@01),B'00000100'                        02S0090
         BNO   @RF00090                                         02S0090
*  DO;                                 /*OK TO WAIT FOR SS DATA SET  */
*    SSALCNCL = CANCELP;               /*PTR TO ADDRESS OF CANCEL ECB*/
         MVC   SSALCNCL(4),CANCELP(@08)                         02S0092
*    SSALWAIT = ON;                    /*OK TO WAIT FOR DEVICES      */
         OI    SSALWAIT,B'00010000'                             02S0093
*  END;                                                         02S0094
*ELSE;                                                          02S0095
@RF00090 DS    0H                                               02S0096
*SSIBPTR = ADDR(LSSIB);                /*SET SSIB BASE PTR   @G29AN2F*/
         LA    @08,LSSIB                                        01S0096
         ST    @08,SSIBPTR                                      01S0096
*IF SU29BIT ^= ON THEN                 /*IF NEW FUNCTION SU  @G29AN2F*/
         L     @08,CVTPTR                                       01S0097
         L     @08,CVTIHASU(,@08)                               01S0097
         TM    SU29BIT(@08),B'00000100'                         01S0097
         BO    @RF00097                                         01S0097
*  DO;                                 /*NOT INSTALLED DO AS    01S0098
*                                        BEFORE              @G29AN2F*/
*    SIOTPTR = SIOT1P;                 /*POINT TO FIRST SIOT         */
         L     @15,@PC00001                                     01S0099
         L     @01,ALCWAPTR(,@15)                               01S0099
         L     SIOTPTR,SIOT1P(,@01)                             01S0099
*    DO WHILE SIOTPTR ^= ZERO          /*PROCESS ALL SIOTS           */
*           & SUBSREQS^= ZERO          /*PROCESS ALL SUBSYS REQUESTS */
*             & RETCODE= ZERO;         /*WHILE NO ERROR OCCURS       */
         B     @DE00100                                         01S0100
@DL00100 DS    0H                                               01S0101
*      IF SIOTSSDS = ON THEN           /*IS THIS A SUBSYSTEM REQUEST?*/
         TM    SIOTSSDS(SIOTPTR),B'10000000'                    01S0101
         BNO   @RF00101                                         01S0101
*        CALL ALOCNGRP;                /*ALLOCATE REQUESTS   @G29AN2F*/
         BAL   @14,ALOCNGRP                                     01S0102
*      ELSE;                                                    01S0103
@RF00101 DS    0H                                               01S0104
*      SIOTPTR = SIOTNPTR;             /*POINT TO NEXT SIOT          */
         L     SIOTPTR,SIOTNPTR(,SIOTPTR)                       01S0104
*    END;                                                       01S0105
@DE00100 SLR   @09,@09                                          01S0105
         CR    SIOTPTR,@09                                      01S0105
         BE    @DC00100                                         01S0105
         L     @15,@PC00001                                     01S0105
         L     @01,ALCWAPTR(,@15)                               01S0105
         C     @09,SUBSREQS(,@01)                               01S0105
         BE    @DC00100                                         01S0105
         CR    RETCODE,@09                                      01S0105
         BE    @DL00100                                         01S0105
@DC00100 DS    0H                                               01S0106
*  END;                                /*END NON-SU PROCESS  @G29AN2F*/
*ELSE                                  /*SU IS INSTALLED     @G29AN2F*/
*  DO;                                 /*DO SU PROCESSING    @G29AN2F*/
         B     @RC00097                                         01S0107
@RF00097 DS    0H                                               01S0108
*/********************************************************************/
*/*                                                          @G29AN2F*/
*/* SEGMENT NAME - BLDTBLS                                   @G29AN2F*/
*/*                                                          @G29AN2F*/
*/* DESCRIPTIVE NAME - BUILD SUBSYSTEM ALLOCATION SEQUENCE   @G29AN2F*/
*/*                    TABLE AND REQUEST BLOCK AREA          @G29AN2F*/
*/*                                                          @G29AN2F*/
*/* FUNCTION -                                               @G29AN2F*/
*/*   THE PURPOSE OF THIS SEGMENT IS TO: 1) BUILD AND        @G29AN2F*/
*/*   INITIALIZE A LOCAL COPY OF THE ALLOCATION SEQUENCE     @G29AN2F*/
*/*   TABLE AND 2) DYNAMICALLY ACQUIRE STORAGE NEEDED TO     @G29AN2F*/
*/*   PROCESS GROUP REQUESTS - AN AREA FOR GROUP REQUEST     @G29AN2F*/
*/*   BLOCKS AND IF ERROR MESSAGES ARE TO BE RETURNED BY     @G29AN2F*/
*/*   THE SUBSYSTEM, THE SUBSYSTEM MESSAGE BLOCK.            @G29AN2F*/
*/*                                                          @G29AN2F*/
*/*   ENOUGH STORAGE IS OBTAINED FOR THE GROUP REQUEST       @G29AN2F*/
*/*   BLOCKS TO ACCOMODATE THE SUBSYSTEM WITH THE HIGHEST    @G29AN2F*/
*/*   NUMBER OF REQUESTS. LIKEWISE, THE AMOUNT OF STORAGE    @G29AN2F*/
*/*   OBTAINED FOR THE SUBSYSTEM MESSAGE BLOCK IS ENOUGH     @G29AN2F*/
*/*   TO ACCOMODATE A MESSAGE FOR EACH REQUEST FOR THE SUB-  @G29AN2F*/
*/*   SYSTEM WITH THE LARGEST NUMBER OF REQUESTS. (THESE     @G29AN2F*/
*/*   AMOUNTS ARE SUFFICIENT BECAUSE THE STORAGE IS REUSED   @G29AN2F*/
*/*   BY EACH SUBSYSTEM IN TURN.)                            @G29AN2F*/
*/*                                                          @G29AN2F*/
*/* ENTRY - IN LINE SUBROUTINE                               @G29AN2F*/
*/*                                                          @G29AN2F*/
*/*   PURPOSE - SEE FUNCTION                                 @G29AN2F*/
*/*                                                          @G29AN2F*/
*/*   INPUT -                                                @G29AN2F*/
*/*                                                          @G29AN2F*/
*/*     SIOT1P    - PTR TO FIRST SIOT                        @G29AN2F*/
*/*     SUBSREQS  - COUNT OF TOTAL SUBSYSTEM REQUESTS        @G29AN2F*/
*/*     LOFJSSNM  - NAME OF THE DEFAULT SUBSYSTEM            @G29AN2F*/
*/*     JSAST     - SUBSYSTEM ALLOCATION SEQUENCE TABLE      @G29AN2F*/
*/*                 BUILT DURING SYSTEM INITIALIZATION       @G29AN2F*/
*/*                                                          @G29AN2F*/
*/* EXIT - IN LINE                                           @G29AN2F*/
*/*                                                          @G29AN2F*/
*/*   CONDITION - LOCAL COPY OF SAST COMPLETED               @G29AN2F*/
*/*               REQUEST BLOCK AREA OBTAINED IF GROUP       @G29AN2F*/
*/*                REQUESTS EXIST                            @G29AN2F*/
*/*               SUBSYSTEM MESSAGE BLOCK OBTAINED IF        @G29AN2F*/
*/*                GROUP REQUESTS EXIST AND THE FUNCTION     @G29AN2F*/
*/*                MAP INDICATES THAT ERROR MESSAGES ARE     @G29AN2F*/
*/*                TO BE GENERATED BY THE SUBSYSTEM          @G29AN2F*/
*/*     OUTPUT -                                             @G29AN2F*/
*/*                                                          @G29AN2F*/
*/*       LCLSASTP - PTR TO COMPLETED LOCAL COPY OF THE SAST @G29AN2F*/
*/*       NUMSUBS  - NUMBER OF SUBSYSTEMS IN SAST            @G29AN2F*/
*/*       MAXGPCT  - MAXIMUM NUMBER OF GROUP REQUEST FOR     @G29AN2F*/
*/*                   A SUBSYSTEM                            @G29AN2F*/
*/*       RBAREAP  - PTR TO REQUEST BLOCK AREA               @G29AN2F*/
*/*       SSMSGBKP - PTR TO SUBSYSTEM MESSAGE BLOCK AREA     @G29AN2F*/
*/*       MSGIDX   - MESSAGE INDEX SET TO FIRST MESSAGE      @G29AN2F*/
*/*                                                          @G29AN2F*/
*/*     RETURN CODE - NONE                                   @G29AN2F*/
*/*                                                          @G29AN2F*/
*/*   CONDITION - ERROR  SUBSYSTEM NAMED IN SIOT DOES NOT    @G29AN2F*/
*/*               EXIST                                      @G29AN2F*/
*/*                                                          @G29AN2F*/
*/*     OUTPUT -                                             @G29AN2F*/
*/*                                                          @G29AN2F*/
*/*       LCLSASTP - PTR TO PARTIALLY COMPLETED SAST         @G29AN2F*/
*/*       SIOTRSNC - SIOT REASON CODE OF REQUEST IN ERROR    @G29AN2F*/
*/*                  SET TO INDICATE THE ERROR               @G29AN2F*/
*/*                                                          @G29AN2F*/
*/*     RETURN CODE - RETCODE=ERROR                          @G29AN2F*/
*/*                                                          @G29AN2F*/
*/********************************************************************/
*MAXGPCT = ZERO;                       /*INIT GROUP COUNT    @G29AN2F*/
         SLR   @08,@08                                          02S0108
         ST    @08,MAXGPCT                                      02S0108
*IF SUBSREQS > ONE THEN                /*IS THERE MORE THAN ONE 02S0109
*                                        SUBSYSTEM REQUEST?  @G29AN2F*/
         L     @08,@PC00001                                     02S0109
         L     @08,ALCWAPTR(,@08)                               02S0109
         L     @08,SUBSREQS(,@08)                               02S0109
         CH    @08,@CH00040                                     02S0109
         BNH   @RF00109                                         02S0109
*  DO;                                 /*YES, SORT REQUESTS  @G29AN2F*/
*    RFY REG1 RSTD;                    /*RESTRICT REG 1      @G29AN2F*/
*    DO; /*GETMAIN (R) LV(JSASTTBS)SP(SPNUM)*/                  02S0112
*    RESPECIFY (GPR01F,GPR00F) RESTRICTED;                      02S0113
*    GPR00F = JSASTTBS; /* LENGTH REQUESTED*/                   02S0114
         L     @15,CVTPTR                                       02S0114
         L     @15,CVTJESCT(,@15)                               02S0114
         L     @15,JESSASTA(,@15)                               02S0114
         LH    GPR00F,JSASTTBS(,@15)                            02S0114
*    GPR01F = SPNUM; /* SP IN REG 1 */                          02S0115
         LA    GPR01F,230                                       02S0115
*    GEN   (SLL   GPR01F,24)                                    02S0116
*    NODEFS NOREFS NOSETS NOEXIT NOFLOWS SEQFLOW;               02S0116
         SLL   GPR01F,24
*    GPR00F = GPR00F × GPR01F; /* SP IN HIGH BYTE */            02S0117
         OR    GPR00F,GPR01F                                    02S0117
*    GPR01F = -1; /* INDICATE A GETMAIN REQUEST */              02S0118
         SLR   GPR01F,GPR01F                                    02S0118
         BCTR  GPR01F,0                                         02S0118
*    SVC(10); /* R-FORM OF GETMAIN*/                            02S0119
         SVC   10                                               02S0119
*    RESPECIFY (GPR01F,GPR00F) UNRESTRICTED;                    02S0120
*    END; /*GETMAIN (R) LV(JSASTTBS)SP(SPNUM)*//* GET SAST CORE     @G2
*AN2F*/                                                         02S0121
*    LCLSASTP = REG1;                  /*SET PTR TO AREA     @G29AN2F*/
         ST    REG1,LCLSASTP                                    02S0122
*    RFY REG1 UNRSTD;                  /*RELEASE REG 1       @G29AN2F*/
*    RFY (REG2,REG3,REG4,REG5) RSTD;   /*RESTRICT MVCL REGS  @G29AN2F*/
*    REG2 = LCLSASTP;                  /*PTR TO LCL SAST     @G29AN2F*/
         L     @15,LCLSASTP                                     02S0125
         LR    REG2,@15                                         02S0125
*    REG3 = JSASTTBS;                  /*LENGTH OF SAST      @G29AN2F*/
         L     @01,CVTPTR                                       02S0126
         L     @01,CVTJESCT(,@01)                               02S0126
         L     @15,JESSASTA(,@01)                               02S0126
         LH    REG3,JSASTTBS(,@15)                              02S0126
*    REG4 = ADDR(JSASTTBL);            /*PTR TO SYSTEM SAST  @G29AN2F*/
         LA    REG4,JSASTTBL(,@15)                              02S0127
*    REG5 = REG3;                      /*USE SAME LENGTH     @G29AN2F*/
         LR    REG5,REG3                                        02S0128
*    MVCL(REG2,REG4);                  /*MAKE COPY OF SAST   @G29AN2F*/
         MVCL  REG2,REG4                                        02S0129
*    RFY (REG2,REG3,REG4,REG5) UNRSTD; /*RELEASE MVCL REGS   @G29AN2F*/
*    RFY SASTTBLE BASED(LCLSASTP);     /*SET SAST MAP BASE   @G29AN2F*/
*    LCLSSCNT = SUBSREQS;              /*GET COUNT OF REQUESTS  02S0132
*                                        FOR SUBSYSTEMS      @G29AN2F*/
         L     @15,@PC00001                                     02S0132
         L     @15,ALCWAPTR(,@15)                               02S0132
         MVC   LCLSSCNT(4),SUBSREQS(@15)                        02S0132
*    SIOTPTR = SIOT1P;                 /*GET PTR TO SIOTS    @G29AN2F*/
         L     SIOTPTR,SIOT1P(,@15)                             02S0133
*    DO WHILE LCLSSCNT ^= ZERO         /*PROCESS ALL RQSTS   @G29AN2F*/
*           & SIOTPTR  ^= ZERO         /*AND ALL SIOTS       @G29AN2F*/
*           & RETCODE   = ZERO;        /*WHILE NO ERRORS     @G29AN2F*/
         B     @DE00134                                         02S0134
@DL00134 DS    0H                                               02S0135
*      IF SIOTSSDS = ON THEN           /*IS SIOT A SUBSYSTEM    02S0135
*                                        REQUEST?            @G29AN2F*/
         TM    SIOTSSDS(SIOTPTR),B'10000000'                    02S0135
         BNO   @RF00135                                         02S0135
*        DO;                           /*YES, PROCESS IT     @G29AN2F*/
*          IF SIOTSSNM = NOSSNM THEN   /*IS SUBSYSTEM NAME      02S0137
*                                        NOT SPECIFIED?      @G29AN2F*/
         CLC   SIOTSSNM(4,SIOTPTR),NOSSNM                       02S0137
         BNE   @RF00137                                         02S0137
*            SUBSYSNM = LOFJSSNM;      /*YES, USE DEFAULT    @G29AN2F*/
         L     @15,@PC00001                                     02S0138
         L     @01,ALCWAPTR(,@15)                               02S0138
         L     @01,LOJSSNMP(,@01)                               02S0138
         MVC   SUBSYSNM(4),LOFJSSNM(@01)                        02S0138
*          ELSE                        /*NAME IS SPECIFIED   @G29AN2F*/
*            SUBSYSNM = SIOTSSNM;      /*USE SPECIFIED NAME  @G29AN2F*/
         B     @RC00137                                         02S0139
@RF00137 MVC   SUBSYSNM(4),SIOTSSNM(SIOTPTR)                    02S0139
*          DO I = 1 TO JSASTTCT        /*SCAN TO LAST ENTRY  @G29AN2F*/
*             WHILE SASTSSNM(I)^=SUBSYSNM;/*UNTIL SUBSYSTEM NAME
*                                          IS FOUND          @G29AN2F*/
@RC00137 LA    I,1                                              02S0140
         B     @DE00140                                         02S0140
@DL00140 LR    @03,I                                            02S0140
         MH    @03,@CH00959                                     02S0140
         L     @01,LCLSASTP                                     02S0140
         ALR   @01,@03                                          02S0140
         AL    @01,@CF01581                                     02S0140
         CLC   SASTSSNM(4,@01),SUBSYSNM                         02S0140
         BE    @DC00140                                         02S0140
*          END;                        /*                    @G29AN2F*/
         AH    I,@CH00040                                       02S0141
@DE00140 L     @03,CVTPTR                                       02S0141
         L     @03,CVTJESCT(,@03)                               02S0141
         L     @03,JESSASTA(,@03)                               02S0141
         CH    I,JSASTTCT(,@03)                                 02S0141
         BNH   @DL00140                                         02S0141
@DC00140 DS    0H                                               02S0142
*          IF I > JSASTTCT THEN        /*WAS END OF TABLE REACHED
*                                        WITHOUT A MATCH?    @G29AN2F*/
         L     @03,CVTPTR                                       02S0142
         L     @03,CVTJESCT(,@03)                               02S0142
         L     @03,JESSASTA(,@03)                               02S0142
         CH    I,JSASTTCT(,@03)                                 02S0142
         BNH   @RF00142                                         02S0142
*            DO;                       /*YES, ERROR          @G29AN2F*/
*              SIOTRSNC = SUBSIFEC(SSRTNOSS/4);/*TREAT AS IF SSREQ
*                                        RETURN FOR NO NAME  @G29AN2F*/
         MVC   SIOTRSNC(2,SIOTPTR),SUBSIFEC+4                   02S0144
*              RETCODE = ERROR;        /*INDICATE ERROR      @G29AN2F*/
         LA    RETCODE,4                                        02S0145
*            END;                      /*                    @G29AN2F*/
*          ELSE                        /*MATCH WAS FOUND     @G29AN2F*/
*            DO;                       /*THEN PROCESS SIOT   @G29AN2F*/
         B     @RC00142                                         02S0147
@RF00142 DS    0H                                               02S0148
*              SASTNORQ(I) = SASTNORQ(I)+ONE;/*UP COUNT OF REQUESTS
*                                             FOR SUBSYSTEM  @G29AN2F*/
         L     @03,LCLSASTP                                     02S0148
         LR    @15,I                                            02S0148
         MH    @15,@CH00959                                     02S0148
         LA    @14,1                                            02S0148
         LA    @01,0(@15,@03)                                   02S0148
         AL    @01,@CF01582                                     02S0148
         LH    @07,SASTNORQ-8(,@01)                             02S0148
         ALR   @07,@14                                          02S0148
         LA    @06,0(@15,@03)                                   02S0148
         AL    @06,@CF01582                                     02S0148
         STH   @07,SASTNORQ-8(,@06)                             02S0148
*              IF SIOTSSGP = ON THEN   /*IS SIOT A GROUP        02S0149
*                                        REQUEST?            @G29AN2F*/
         TM    SIOTSSGP(SIOTPTR),B'00010000'                    02S0149
         BNO   @RF00149                                         02S0149
*                DO;                   /*YES                 @G29AN2F*/
*                  SASTNOGP(I)=SASTNOGP(I)+ONE;/*INCREMENT CNT  02S0151
*                                          OF GROUP REQUESTS @G29AN2F*/
         LA    @01,0(@15,@03)                                   02S0151
         BCTR  @01,0                                            02S0151
         BCTR  @01,0                                            02S0151
         AH    @14,SASTNOGP-10(,@01)                            02S0151
         BCTR  @03,0                                            02S0151
         BCTR  @03,0                                            02S0151
         STH   @14,SASTNOGP-10(@15,@03)                         02S0151
*                  MAXGPCT = MAX(SASTNOGP(I),MAXGPCT);/*SAVE LARGEST
*                                              NUMBER OF GROUP  02S0152
*                                              REQUESTS      @G29AN2F*/
         L     @03,MAXGPCT                                      02S0152
         CR    @14,@03                                          02S0152
         BNL   *+6
         LR    @14,@03                                          02S0152
         ST    @14,MAXGPCT                                      02S0152
*                END;                  /*                    @G29AN2F*/
*              IF SASTNORQ(I) =ONE THEN/*IS THIS FIRST REQUEST  02S0154
*                                        FOR THE SUBSYSTEM ? @G29AN2F*/
@RF00149 L     @03,LCLSASTP                                     02S0154
         LR    @15,I                                            02S0154
         MH    @15,@CH00959                                     02S0154
         LA    @01,0(@15,@03)                                   02S0154
         AL    @01,@CF01582                                     02S0154
         LH    @14,SASTNORQ-8(,@01)                             02S0154
         CH    @14,@CH00040                                     02S0154
         BNE   @RF00154                                         02S0154
*                SASTSPTR(I) = SIOTPTR;/*YES, SAVE SIOT PTR  @G29AN2F*/
         AL    @03,@CF01583                                     02S0155
         ST    SIOTPTR,SASTSPTR-4(@15,@03)                      02S0155
*              LCLSSCNT = LCLSSCNT-ONE;/*DECREMENT COUNT     @G29AN2F*/
@RF00154 L     @15,LCLSSCNT                                     02S0156
         BCTR  @15,0                                            02S0156
         ST    @15,LCLSSCNT                                     02S0156
*            END;                      /*                    @G29AN2F*/
*        END;                          /*END SUBSYSTEM SIOT  @G29AN2F*/
@RC00142 DS    0H                                               02S0159
*      SIOTPTR = SIOTNPTR;             /*POINT TO NEXT SIOT  @G29AN2F*/
@RF00135 L     SIOTPTR,SIOTNPTR(,SIOTPTR)                       02S0159
*    END;                              /*PROCESS NEXT SIOT   @G29AN2F*/
@DE00134 SLR   @15,@15                                          02S0160
         C     @15,LCLSSCNT                                     02S0160
         BE    @DC00134                                         02S0160
         CR    SIOTPTR,@15                                      02S0160
         BE    @DC00134                                         02S0160
         CR    RETCODE,@15                                      02S0160
         BE    @DL00134                                         02S0160
@DC00134 DS    0H                                               02S0161
*    NUMSUBS = JSASTTCT;               /*SET NUMBER OF SUBSYSTEMS
*                                        IN TABLE            @G29AN2F*/
         L     @08,CVTPTR                                       02S0161
         L     @08,CVTJESCT(,@08)                               02S0161
         L     @08,JESSASTA(,@08)                               02S0161
         LH    NUMSUBS,JSASTTCT(,@08)                           02S0161
*  END;                                /*                    @G29AN2F*/
*ELSE                                  /*ONE REQUEST - BUILD @G29AN2F*/
*  DO;                                 /*SAST OF ONE ENTRY   @G29AN2F*/
         B     @RC00109                                         02S0163
@RF00109 DS    0H                                               02S0164
*    LCLSASTP = ADDR(LCLSAST);         /*SET PTR TO SAST     @G29AN2F*/
         LA    @15,LCLSAST                                      02S0164
         ST    @15,LCLSASTP                                     02S0164
