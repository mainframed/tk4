         BAL   @14,ALOCNGRP                                     01S0274
*                        SASTNORQ(I) = SASTNORQ(I)-ONE;/*DECREMENT
*                                        SUBSYSTEM'S COUNT   @G29AN2F*/
         L     @15,LCLSASTP                                     01S0275
         LR    @14,I                                            01S0275
         MH    @14,@CH00959                                     01S0275
         LA    @01,0(@14,@15)                                   01S0275
         AL    @01,@CF01582                                     01S0275
         LH    @07,SASTNORQ-8(,@01)                             01S0275
         BCTR  @07,0                                            01S0275
         AL    @15,@CF01582                                     01S0275
         STH   @07,SASTNORQ-8(@14,@15)                          01S0275
*                      END;            /*                    @G29AN2F*/
*                  END;                /*END OF REQUEST      @G29AN2F*/
*                ELSE;                 /*NOT A SUBSYSTEM        01S0278
*                                        REQUEST             @G29AN2F*/
@RF00234 DS    0H                                               01S0279
*                SIOTPTR = SIOTNPTR;   /*GET NEXT SIOT       @G29AN2F*/
@RC00234 L     SIOTPTR,SIOTNPTR(,SIOTPTR)                       01S0279
*              END;                    /*END SIOT LOOP       @G29AN2F*/
@DE00233 SLR   @15,@15                                          01S0280
         CR    SIOTPTR,@15                                      01S0280
