         LA    @15,@TF00002                                     02S0255
         ST    @15,@AL00001+12                                  02S0255
         LA    @15,SSWAPTR                                      02S0255
         ST    @15,@TF00003                                     02S0255
         LA    @15,@TF00003                                     02S0255
         ST    @15,@AL00001+16                                  02S0255
         LA    @15,@CF00111                                     02S0255
         ST    @15,@AL00001+20                                  02S0255
         LA    @15,@CF00040                                     02S0255
         ST    @15,@AL00001+24                                  02S0255
         L     @15,@CV01428                                     02S0255
         LA    @01,@AL00001                                     02S0255
         BALR  @14,@15                                          02S0255
*  END;                                /*                    @G29AN2F*/
*ELSE                                  /*NO SSWA SVA         @G29AN2F*/
*  SSWAPTR = ZERO;                     /*INDICATE NO SSWA    @G29AN2F*/
         B     @RC00248                                         02S0257
@RF00248 SLR   @15,@15                                          02S0257
