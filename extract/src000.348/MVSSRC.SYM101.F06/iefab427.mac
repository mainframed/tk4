         LA    @14,LCLEPA                                       02S0253
         SLR   @07,@07                                          02S0253
         ST    @07,SWCHNPTR(,@14)                               02S0253
*    SMISVA = SIOTSSWA;                /*SVA OF SSWA TO READ @G29AN2F*/
         STCM  @15,7,SMISVA                                     02S0254
*    CALL IEFAB4F7(SWACTLS,ADDR(LCLEPA),0,                      02S0255
*                  ADDR(SMISVA),ADDR(SSWAPTR),                  02S0255
