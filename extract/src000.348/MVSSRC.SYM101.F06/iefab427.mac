*SSAGSSNM = ADDR(SIOTSSNM);            /*PTR TO SUBSYSTEM       02S0246
*                                        NAME                @G29AN2F*/
         LA    @15,SIOTSSNM(,SIOTPTR)                           02S0246
         ST    @15,SSAGSSNM(,SSARBPTR)                          02S0246
*SSAGJFCB = SJFCBPTR;                  /*PTR TO JFCB         @G29AN2F*/
         MVC   SSAGJFCB(4,SSARBPTR),SJFCBPTR(SIOTPTR)           02S0247
*IF SIOTSSWA ^= ZERO THEN              /*DOES AN SSWA EXIST? @G29AN2F*/
