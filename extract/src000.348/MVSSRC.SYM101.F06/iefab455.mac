         TITLE 'IEFAB455 - PDI SCAN                                    *
                        '
IEFAB455 CSECT ,                                                01S0001
@MAINENT DS    0H                                               01S0001
         USING *,@15                                            01S0001
         B     @PROLOG                                          01S0001
         DC    AL1(16)                                          01S0001
         DC    C'IEFAB455  73.325'                              01S0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                  01S0001
         BALR  @12,0                                            01S0001
@PSTART  DS    0H                                               01S0001
         USING @PSTART,@12                                      01S0001
         L     @00,@SIZDATD                                     01S0001
         MODESET EXTKEY=SCHED,SAVEKEY=(2)
         GSPACE  R,LV=(0)
         LR    @11,@01                                          01S0001
         USING @DATD,@11                                        01S0001
         ST    @13,@SA00001+4                                   01S0001
         STC   @02,@OLDKEY                                      01S0001
         MODESET KEYADDR=(2)
         LM    @00,@02,20(@13)                                  01S0001
         ST    @11,8(,@13)                                      01S0001
         LR    @13,@11                                          01S0001
         MODESET EXTKEY=ZERO
         MVC   @PC00001(24),0(@01)                              01S0001
         MODESET EXTKEY=SCHED
*                                                               01S0030
*   /*****************************************************************/
*   /*                                                               */
*   /* THE REST OF THE FIELDS HAVE THE FOLLOWING FORMAT FOR JOB      */
*   /* ACCOUNTING- 1 BYTE- LENGTH OF FIELD VARIABLE BYTES- CONTENTS  */
*   /* OF FIELD (LAST 2 REPEATED FOR N FIELDS ) STEP ACCOUNTING HAS  */
*   /* THE FOLLOWING FORMAT FOR EACH STEP- 3 BYTES- MAXIMUM STEP     */
*   /* RUNNING TIME 1 BYTE- NBR OF FIELDS IN STEP 1 BYTE- LENGTH OF  */
*   /* FIELD VARIABLE BYTES- CONTENTS OF FIELD (LAST 2 REPEATED N    */
*   /* TIMES )                                                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0031
*   IF HWPDIP=0 THEN                /* READ AND CHAIN THE PDI.       */
         L     @10,@PC00001+4                                   01S0031
         L     @10,HWDATAP(,@10)                                01S0031
         ICM   @15,15,HWPDIP(@10)                               01S0031
         BNZ   @RF00031                                         01S0031
*     CALL IEFAB4EB(JCTPDIP,HWPDIP,HWLPDIBP,HWLEPAP);           01S0032
         L     @15,@PC00001+20                                  01S0032
         L     @01,JCTPTR(,@15)                                 01S0032
         LA    @15,JCTPDIP(,@01)                                01S0032
         ST    @15,@AL00001                                     01S0032
         LA    @15,HWPDIP(,@10)                                 01S0032
         ST    @15,@AL00001+4                                   01S0032
         LA    @15,HWLPDIBP(,@10)                               01S0032
         ST    @15,@AL00001+8                                   01S0032
         LA    @10,HWLEPAP(,@10)                                01S0032
         ST    @10,@AL00001+12                                  01S0032
         L     @15,@CV00060                                     01S0032
         LA    @01,@AL00001                                     01S0032
         BALR  @14,@15                                          01S0032
*   PDIBLPTR=HWPDIP;                /* GET PTR TO FIRST PDIB.        */
@RF00031 L     @10,@PC00001+4                                   01S0033
         L     @10,HWDATAP(,@10)                                01S0033
         L     PDIBLPTR,HWPDIP(,@10)                            01S0033
*   DO WHILE(HWRESIOT=NO&PDIBLPTR^=0);/* SEARCH PDI FOR DATA SET.    */
         B     @DE00034                                         01S0034
@DL00034 DS    0H                                               01S0035
*     DO PDIENO=1 TO PDICTR         /* SEARCH THIS PDI BLOCK FOR     */
*           WHILE(HWRESIOT=NO);     /* THE DATA SET NAME.            */
         LA    PDIENO,1                                         01S0035
         B     @DE00035                                         01S0035
@DL00035 L     @10,@PC00001                                     01S0035
         L     @10,HWCNTRLP(,@10)                               01S0035
         TM    HWRESIOT(@10),B'00001000'                        01S0035
         BNZ   @DC00035                                         01S0035
*       IF DSN=PDIDSNAM(PDIENO) THEN/* DATA SET NAME MATCH-          */
         L     @10,@PC00001+16                                  01S0036
         L     @10,DSNPTR(,@10)                                 01S0036
         LR    @15,PDIENO                                       01S0036
         MH    @15,@CH00787                                     01S0036
         LA    @14,PDIENTRY(,PDIBLPTR)                          01S0036
         ST    @15,@TF00001                                     01S0036
         ALR   @15,@14                                          01S0036
         AL    @15,@CF00789                                     01S0036
         CLC   DSN(44,@10),PDIDSNAM(@15)                        01S0036
         BNE   @RF00036                                         01S0036
*         IF PDIJOBLB(PDIENO)=NO    /* ONLY A JOBLIB DD CAN OBTAIN   */
*             ×SCTJOBLB=YES THEN    /* A PREVIOUS JOBLIB'S INFO.     */
         AL    @14,@TF00001                                     01S0037
         AL    @14,@CF00790                                     01S0037
         TM    PDIJOBLB-44(@14),B'00000010'                     01S0037
         BZ    @RT00037                                         01S0037
         L     @10,@PC00001+12                                  01S0037
         L     @10,SIOTPTR(,@10)                                01S0037
         TM    SCTJOBLB(@10),B'00010000'                        01S0037
         BNO   @RF00037                                         01S0037
@RT00037 DS    0H                                               01S0038
*           IF PDIPASIN(PDIENO)=YES /* DATA SET MUST BE PASSED,BUT   */
*               &PDISTPNO(PDIENO)^=HWSTEPNO THEN/* NOT BY THIS STEP- */
         LR    @10,PDIENO                                       01S0038
         MH    @10,@CH00787                                     01S0038
         LA    @15,PDIENTRY(,PDIBLPTR)                          01S0038
         ST    @10,@TF00001                                     01S0038
         ALR   @10,@15                                          01S0038
         AL    @10,@CF00790                                     01S0038
         TM    PDIPASIN-44(@10),B'00100000'                     01S0038
         BNO   @RF00038                                         01S0038
         L     @10,@PC00001+4                                   01S0038
         L     @10,HWDATAP(,@10)                                01S0038
         L     @14,@TF00001                                     01S0038
         AL    @14,@CF00791                                     01S0038
         LH    @14,PDISTPNO-46(@14,@15)                         01S0038
         SLR   @09,@09                                          01S0038
         IC    @09,HWSTEPNO(,@10)                               01S0038
         CR    @14,@09                                          01S0038
         BE    @RF00038                                         01S0038
*             DO;                   /* REQUIREMENTS MET              */
*               HWRESIOT=YES;       /* DSN FOUND                     */
         L     @10,@PC00001                                     01S0040
         L     @10,HWCNTRLP(,@10)                               01S0040
         OI    HWRESIOT(@10),B'00001000'                        01S0040
*               PDIEPTR=ADDR(PDIE(PDIENO));/* PTR TO PDIE            */
         L     @10,@TF00001                                     01S0041
         AL    @15,@CF00789                                     01S0041
         LA    @10,PDIE(@10,@15)                                01S0041
         ST    @10,PDIEPTR                                      01S0041
*             END;                                              01S0042
*     END;                          /* END DSN SCAN OF THIS PDIB.    */
@RF00038 DS    0H                                               01S0043
@RF00037 DS    0H                                               01S0043
@RF00036 AH    PDIENO,@CH00048                                  01S0043
@DE00035 SLR   @10,@10                                          01S0043
         IC    @10,PDICTR(,PDIBLPTR)                            01S0043
         CR    PDIENO,@10                                       01S0043
         BNH   @DL00035                                         01S0043
@DC00035 DS    0H                                               01S0044
*     IF HWRESIOT=NO THEN           /* IF DATA SET WAS NOT IN        */
         L     @10,@PC00001                                     01S0044
         L     @10,HWCNTRLP(,@10)                               01S0044
         TM    HWRESIOT(@10),B'00001000'                        01S0044
         BNZ   @RF00044                                         01S0044
*       PDIBLPTR=PDIBNXTP;          /* THIS PDIB,GET NEXT PDIB.      */
         L     PDIBLPTR,PDIBNXTP(,PDIBLPTR)                     01S0045
*   END;                            /* END SEARCH OF PDI.            */
@RF00044 DS    0H                                               01S0046
@DE00034 L     @10,@PC00001                                     01S0046
         L     @10,HWCNTRLP(,@10)                               01S0046
         TM    HWRESIOT(@10),B'00001000'                        01S0046
         BNZ   @DC00034                                         01S0046
         LTR   PDIBLPTR,PDIBLPTR                                01S0046
         BNZ   @DL00034                                         01S0046
@DC00034 DS    0H                                               01S0047
*   RFY                                                         01S0047
*     PDIE BASED(PDIEPTR);                                      01S0047
*   IF HWRESIOT=YES THEN            /* DATA SET FOUND READ ITS DD.   */
         L     @10,@PC00001                                     01S0048
         L     @10,HWCNTRLP(,@10)                               01S0048
         TM    HWRESIOT(@10),B'00001000'                        01S0048
         BNO   @RF00048                                         01S0048
*     DO;                           /* AND POSSIBLY DELETE PDIE.
*                                                                    */
*                                                               02S0050
*/********************************************************************/
*/*                                                                  */
*/* SEGMENT NAME - PDIDDRD                                           */
*/*                                                                  */
*/* DESCRIPTIVE NAME - READ LOCATE AND CHAIN DD FOR PASSED DATA SET. */
*/*                                                                  */
*/* FUNCTION -                                                       */
*/*     READ-LOCATE THE DD THAT PASSED THE DATA SET JUST FOUND IN    */
*/*     PDI-SCAN. CHAIN VIA SVAB(SWA VIRTUAL ADDRESS OF BLOCK) THE   */
*/*     BLOCKS(SIOT, JFCB AND JFCBX) THAT REPRESENT THE PASSING DD.  */
*/*     RETURN THE SVAB OF THE REFERENCED SIOT.                      */
*/*                                                                  */
*/*   OPERATION - 1. OBTAIN SIOT SVA FROM PDIE(PASSED DATA SET       */
*/*                  INFORMATION ENTRY).                             */
*/*               2. READ-LOCATE THE SIOT.                           */
*/*               3. INDICATE REFERENCED SIOT PRESENT AND PLACE SIOT */
*/*                  SVAB IN HSKPWA.                                 */
*/*               4. READ-LOCATE JFCB AND CHAIN,VIA SVAB, TO         */
*/*                  REFERENCED SIOT.                                */
*/*               5. READ-LOCATE AND CHAIN ALL JFCBXS.               */
*/*                                                                  */
*/* ENTRY - N/A                                                      */
*/*                                                                  */
*/*   PURPOSE - SEE FUNCTION                                         */
*/*                                                                  */
*/*   INPUT DATA - PDIE THAT CONTAINS THE SVA OF PASSING DD'S SIOT.  */
*/*                                                                  */
*/* EXIT - N/A                                                       */
*/*                                                                  */
*/*   CONDITION - ALL                                                */
*/*                                                                  */
*/*   OUTPUT DATA - SVAB OF REFERENCED SIOT                          */
*/*                 JFCB AND JFCBX(S) CHAINED(VIA SVABS) TO SIOT.    */
*/*                                                                  */
*/********************************************************************/
*                                                               02S0050
*       SWACTLS=0;                  /* CLEAR SWA CONTROLS.           */
         SLR   @10,@10                                          02S0050
         STH   @10,SWACTLS                                      02S0050
*       SWREAD=YES;                 /* READ OPERATION.               */
         OI    SWREAD,B'00100000'                               02S0051
*       IF HWLEPARP=0 THEN          /* IF NO EPA EXISTS THEN         */
         L     @15,@PC00001+4                                   02S0052
         L     @01,HWDATAP(,@15)                                02S0052
         C     @10,HWLEPARP(,@01)                               02S0052
         BNE   @RF00052                                         02S0052
*         SWNEW=YES;                /* CREATE AN EPA.                */
         OI    SWNEW,B'00001000'                                02S0053
*       ELSE                        /* AN EPA EXISTS SO,             */
*         SWMOD=YES;                /* USE EXISTING EPA.             */
         B     @RC00052                                         02S0054
@RF00052 OI    SWMOD,B'00000010'                                02S0054
*       CALL IEFAB4F7(SWACTLS,HWLEPARP,0,/* READ THE SIOT FOR DD     */
*           ADDR(PDISIOTA),ADDR(HWRSIOTP),0,1);/* THAT PASSED.       */
@RC00052 LA    @10,SWACTLS                                      02S0055
         ST    @10,@AL00001                                     02S0055
         L     @10,@PC00001+4                                   02S0055
         L     @10,HWDATAP(,@10)                                02S0055
         LA    @15,HWLEPARP(,@10)                               02S0055
         ST    @15,@AL00001+4                                   02S0055
         LA    @15,@CF00621                                     02S0055
         ST    @15,@AL00001+8                                   02S0055
         L     @01,PDIEPTR                                      02S0055
         LA    @15,PDISIOTA(,@01)                               02S0055
         ST    @15,@TF00001                                     02S0055
         LA    @15,@TF00001                                     02S0055
         ST    @15,@AL00001+12                                  02S0055
         LA    @10,HWRSIOTP(,@10)                               02S0055
         ST    @10,@TF00002                                     02S0055
         LA    @10,@TF00002                                     02S0055
         ST    @10,@AL00001+16                                  02S0055
         LA    @10,@CF00621                                     02S0055
         ST    @10,@AL00001+20                                  02S0055
         LA    @10,@CF00048                                     02S0055
         ST    @10,@AL00001+24                                  02S0055
         L     @15,@CV00061                                     02S0055
         LA    @01,@AL00001                                     02S0055
         BALR  @14,@15                                          02S0055
*       RFY                                                     02S0056
*         INDMSIOT BASED(HWRSIOTP); /* BASE REFERENCED SIOT.         */
*       SWMOD=YES;                  /* INDICATE EPA REUSE.           */
*       SWNEW=NO;                   /* NO LONGER A NEW EPA NEEDED.   */
         OI    SWMOD,B'00000010'                                02S0058
         NI    SWNEW,B'11110111'                                02S0058
*       CALL IEFAB4F7(SWACTLS,HWLEPARP,0,/* READ REFERENCED          */
*           ADDR(SCTPJFCB),ADDR(SJFCBPTR),0,1);/* JFCB AND SVAB CHAIN
*                                      JFCB TO SIOT.                 */
         LA    @10,SWACTLS                                      02S0059
         ST    @10,@AL00001                                     02S0059
         L     @10,@PC00001+4                                   02S0059
         L     @10,HWDATAP(,@10)                                02S0059
         LA    @15,HWLEPARP(,@10)                               02S0059
         ST    @15,@AL00001+4                                   02S0059
         LA    @15,@CF00621                                     02S0059
         ST    @15,@AL00001+8                                   02S0059
         L     @10,HWRSIOTP(,@10)                               02S0059
         LA    @15,SCTPJFCB(,@10)                               02S0059
         ST    @15,@TF00001                                     02S0059
         LA    @15,@TF00001                                     02S0059
         ST    @15,@AL00001+12                                  02S0059
         LA    @10,SJFCBPTR(,@10)                               02S0059
         ST    @10,@TF00002                                     02S0059
         LA    @10,@TF00002                                     02S0059
         ST    @10,@AL00001+16                                  02S0059
         LA    @10,@CF00621                                     02S0059
         ST    @10,@AL00001+20                                  02S0059
         LA    @10,@CF00048                                     02S0059
         ST    @10,@AL00001+24                                  02S0059
         L     @15,@CV00061                                     02S0059
         LA    @01,@AL00001                                     02S0059
         BALR  @14,@15                                          02S0059
*       RFY                                                     02S0060
*         INFMJFCB BASED(SJFCBPTR); /* BASE REFERENCED JFCB.         */
*       IF JFCBEXAD^=0 THEN         /* READ JFCB EXTENSIONS IF       */
         L     @10,@PC00001+4                                   02S0061
         L     @10,HWDATAP(,@10)                                02S0061
         L     @01,HWRSIOTP(,@10)                               02S0061
         L     @15,SJFCBPTR(,@01)                               02S0061
         ICM   @14,7,JFCBEXAD(@15)                              02S0061
         BZ    @RF00061                                         02S0061
*         DO;                       /* THERE ARE ANY.                */
*           CALL IEFAB4F7(SWACTLS,HWLEPARP,0,/* READ FIRST           */
*               ADDR(JFCBEXAD),ADDR(JFCBXPTR),0,1);/* JFCBX          */
         LA    @14,SWACTLS                                      02S0063
         ST    @14,@AL00001                                     02S0063
         LA    @10,HWLEPARP(,@10)                               02S0063
         ST    @10,@AL00001+4                                   02S0063
         LA    @10,@CF00621                                     02S0063
         ST    @10,@AL00001+8                                   02S0063
         LA    @10,JFCBEXAD(,@15)                               02S0063
         ST    @10,@TF00001                                     02S0063
         LA    @10,@TF00001                                     02S0063
         ST    @10,@AL00001+12                                  02S0063
         LA    @10,JFCBXPTR                                     02S0063
         ST    @10,@TF00002                                     02S0063
         LA    @10,@TF00002                                     02S0063
         ST    @10,@AL00001+16                                  02S0063
         LA    @10,@CF00621                                     02S0063
         ST    @10,@AL00001+20                                  02S0063
         LA    @10,@CF00048                                     02S0063
         ST    @10,@AL00001+24                                  02S0063
         L     @15,@CV00061                                     02S0063
         LA    @01,@AL00001                                     02S0063
         BALR  @14,@15                                          02S0063
*           SIOTJFX=JFCBXPTR;       /* SVAB CHAIN JFCBX TO SIOT.     */
         L     @10,@PC00001+4                                   02S0064
         L     @10,HWDATAP(,@10)                                02S0064
         L     @10,HWRSIOTP(,@10)                               02S0064
         MVC   SIOTJFX(4,@10),JFCBXPTR                          02S0064
*           DO WHILE(JFCBXTTR^=0);  /* READ REMAINING JFCBX(S).      */
         B     @DE00065                                         02S0065
@DL00065 DS    0H                                               02S0066
*             CALL IEFAB4F7(SWACTLS,HWLEPARP,/* AND SVAB CHAIN THEM  */
*                 0,ADDR(JFCBXTTR),ADDR(JFCBXNXT),0,1);         02S0066
         LA    @10,SWACTLS                                      02S0066
         ST    @10,@AL00001                                     02S0066
         L     @10,@PC00001+4                                   02S0066
         L     @10,HWDATAP(,@10)                                02S0066
         LA    @10,HWLEPARP(,@10)                               02S0066
         ST    @10,@AL00001+4                                   02S0066
         LA    @10,@CF00621                                     02S0066
         ST    @10,@AL00001+8                                   02S0066
         LA    @10,JFCBXPTR                                     02S0066
         ST    @10,@AL00001+12                                  02S0066
         L     @10,JFCBXPTR                                     02S0066
         LA    @10,JFCBXNXT(,@10)                               02S0066
         ST    @10,@TF00001                                     02S0066
         LA    @10,@TF00001                                     02S0066
         ST    @10,@AL00001+16                                  02S0066
         LA    @10,@CF00621                                     02S0066
         ST    @10,@AL00001+20                                  02S0066
         LA    @10,@CF00048                                     02S0066
         ST    @10,@AL00001+24                                  02S0066
         L     @15,@CV00061                                     02S0066
         LA    @01,@AL00001                                     02S0066
         BALR  @14,@15                                          02S0066
*             JFCBXPTR=JFCBXNXT;    /* PTR TO JFCBX JUST READ.       */
         L     @10,JFCBXPTR                                     02S0067
         L     @10,JFCBXNXT(,@10)                               02S0067
         ST    @10,JFCBXPTR                                     02S0067
*           END;                    /* END JFCBX READ.               */
@DE00065 L     @10,JFCBXPTR                                     02S0068
         ICM   @15,7,JFCBXTTR(@10)                              02S0068
         BNZ   @DL00065                                         02S0068
*         END;                      /* END PDI DD READ.              */
*       RFY                                                     02S0070
*         INDMSIOT BASED(SIOTPTR);  /* RESET SIOT.                   */
@RF00061 DS    0H                                               02S0071
*       RFY                                                     02S0071
*         INFMJFCB BASED(JFCBPTR);  /* JFCB BASES               02S0071
*                                                                    */
*       IF HWPDIEDL=YES THEN        /* THE ENTRY IS TO BE DELETED-   */
         L     @10,@PC00001                                     01S0072
         L     @10,HWCNTRLP(,@10)                               01S0072
         TM    HWPDIEDL(@10),B'01000000'                        01S0072
         BNO   @RF00072                                         01S0072
*         DO;                       /* RECEIVED.POINTER TO DELETED   */
*           HWRPDIEP=PDIEPTR;       /* PDIE FOR PDI BUILD REUSE.     */
         L     @10,PDIEPTR                                      01S0074
         L     @15,@PC00001+4                                   01S0074
         L     @01,HWDATAP(,@15)                                01S0074
         ST    @10,HWRPDIEP(,@01)                               01S0074
*           PDIPASIN(1)=NO;         /* INDICATE PDIE DSN RECEIVED.   */
         NI    PDIPASIN(@10),B'11011111'                        01S0075
*           SCTRECVD=YES;           /* INDICATE DATA SET RECEIVED.   */
         L     @15,@PC00001+12                                  01S0076
         L     @15,SIOTPTR(,@15)                                01S0076
         OI    SCTRECVD(@15),B'00000001'                        01S0076
*           SIOTIPDI=YES;           /* DATASET-IN-PDI INDICATOR      */
         OI    SIOTIPDI(@15),B'00000010'                        01S0077
*           IF PDIISTAT(1)=ON THEN  /* IF INITIAL STATUS IS OLD      */
         TM    PDIISTAT(@10),B'10000000'                        01S0078
         BNO   @RF00078                                         01S0078
*             SIOTOMN=ON;           /* INDICATE SAME IN SIOT.        */
         OI    SIOTOMN(@15),B'00000001'                         01S0079
*           PDISIOTA(1)=SIOTDSKA;   /* RECEIVING DD FOR UNALLOCATION
*                                      DISPOSITION PROCESSING.       */
@RF00078 L     @10,@PC00001+12                                  01S0080
         L     @10,SIOTPTR(,@10)                                01S0080
         L     @01,PDIEPTR                                      01S0080
         MVC   PDISIOTA(3,@01),SIOTDSKA(@10)                    01S0080
*         END;                      /* END DATA SET RECEIVED.        */
*       DSN=HWRSIOTP->SJFCBPTR->JFCBDSNM;/* MAY BE AN ALIAS, RETURN
*                                      REAL DSNAME                   */
@RF00072 L     @10,@PC00001+16                                  01S0082
         L     @10,DSNPTR(,@10)                                 01S0082
         L     @15,@PC00001+4                                   01S0082
         L     @01,HWDATAP(,@15)                                01S0082
         L     @01,HWRSIOTP(,@01)                               01S0082
         L     @01,SJFCBPTR(,@01)                               01S0082
         MVC   DSN(44,@10),JFCBDSNM(@01)                        01S0082
*     END;                          /* END DATA SET FOUND.           */
*   END IEFAB455                                                01S0084
*                                                               01S0084
*/* THE FOLLOWING INCLUDE STATEMENTS WERE FOUND IN THIS PROGRAM.     */
*/*%INCLUDE SYSUT5  (SCANSPEC)                                       */
*/*%INCLUDE SYSUT5  (DCLS455 )                                       */
*/*%INCLUDE SYSLIB  (IEFZB437)                                       */
*/*%INCLUDE SYSLIB  (IEFASIOT)                                       */
*/*%INCLUDE SYSLIB  (IEFJFCBN)                                       */
*/*%INCLUDE SYSLIB  (IEFJFCBX)                                       */
*/*%INCLUDE SYSLIB  (IEFZB436)                                       */
*/*%INCLUDE SYSLIB  (IEFZB435)                                       */
*/*%INCLUDE SYSLIB  (IEFAJCTB)                                       */
*/*%INCLUDE SYSUT5  (DDRDSPEC)                                       */
*/*%INCLUDE SYSUT5  (PDIDDRD )                                       */
*                                                               01S0084
*       ;                                                       01S0084
@EL00001 IC    @02,@OLDKEY                                      01S0084
         L     @13,4(,@13)                                      01S0084
@EF00001 L     @00,@SIZDATD                                     01S0084
         LR    @01,@11                                          01S0084
         FSPACE   R,LV=(0),A=(1)
         MODESET KEYADDR=(2)
@ER00001 LM    @14,@12,12(@13)                                  01S0084
         BR    @14                                              01S0084
@DATA    DS    0H
@CH00787 DC    H'56'
@DATD    DSECT
         DS    0F
@SA00001 DS    18F
@PC00001 DS    6F
@AL00001 DS    7A
@TF00001 DS    F
@TF00002 DS    F
IEFAB455 CSECT
         DS    0F
@CF00621 DC    F'0'
@CF00048 DC    F'1'
@CH00048 EQU   @CF00048+2
@CF00789 DC    F'-56'
@CF00790 DC    F'-12'
@CF00791 DC    F'-10'
@DATD    DSECT
         DS    0D
JFCBXPTR DS    A
JFCBPTR  DS    A
PDIEPTR  DS    A
SWACTLS  DS    FL2
         ORG   SWACTLS
SWASN    DS    BL1
