@RF00205 DS    0H                                               01S0211
*/********************************************************************/
*/*                                                                  */
*/* SEGMENT NAME - BLDSPEDL                                          */
*/*                                                                  */
*/* DESCRIPTIVE NAME - BUILD EDL FOR SPECIFIC UNIT ADDRESS           */
*/*                                                                  */
*/* FUNCTION -                                                       */
*/*   THE PURPOSE OF THIS SEGMENT IS TO BUILD AN ELIGIBLE DEVICE     */
*/*   LIST(EDL) FOR A SPECIFIC UNIT REQUEST. AN EDL WITH ONE GENERIC */
*/*   ENTRY, ONE GROUP ENTRY AND ONE ENTRY IN THE UCB LIST IS BUILT. */
*/*   THE GROUP ID FOR A SPECIFIC UNIT ADDRESS IS DEFINED TO BE THE  */
*/*   GROUP ID OF THE GENERIC WHICH CONTAINS THIS DEVICE.            */
*/*                                                                  */
*/* OPERATION -                                                      */
*/*   IN GENERAL THE OPERATION IS DEFINED IN THE CODE, HOWEVER       */
*/*   THE FOLLOWING METHOD IS USED TO FIND THE GROUP ID:             */
*/*                                                                  */
*/*   1. SCTUTYPE(7:8) CONTAINS THE UCB ADDRESS FOR A SPECIFIC UNIT  */
*/*      REQUEST. PART OF THE UCBTYPE FIELD WILL BE USED AS A SEARCH */
*/*      ARGUMENT INTO THE EDT LUV SECTION. THE PART OF THE UCBTYP   */
*/*      FIELD THAT IS USED DEPENDS ON THE DEVICE CLASS(THIRD BYTE)  */
*/*      AS FOLLOWS:                                                 */
*/*        DIRECT ACCESS - BYTES 3 AND 4 PLUS THE VIRTUAL    @Y30LPPD*/
*/*          BIT IN BYTE 2 OF UCBTYPE ARE COMPARED AGAINST   @Y30LPPD*/
*/*          THE THIRD AND FOURTH BYTES AND THE VIRTUAL BIT  @Y30LPPD*/
*/*          IN BYTE 2 OF EDTLUV.                            @Y30LPPD*/
*/*                                                                  */
*/*        UNIT RECORD - SAME AS DIRECT ACCESS.                      */
*/*                                                                  */
*/*        MAGNETIC TAPE - BYTES 1,2 (LESS THE ALTERNATE     @YM05477*/
*/*          POWER USAGE BITS), 3 AND 4 OF UCBTYP ARE        @YM05477*/
*/*          COMPARED AGAINST BYTES 1,2,3 AND 4 OF EDTLUV.   @YM05477*/
*/*                                                                  */
*/*        GRAPHIC DEVICE CLASS - BYTES 1,3 AND 4 OF UCBTYP ARE      */
*/*          COMPARED AGAINST BYTES 1,3 AND 4 OF EDTLUV.             */
*/*                                                                  */
*/*        COMMUNICATIONS DEVICE CLASS - NO GROUP IS NECESSARY       */
*/*          FOR COMMUNICATIONS DEVICES. THE GROUP ID IS SET TO 0.   */
*/*                                                                  */
*/* ENTRY - IN-LINE SEGMENT                                          */
*/*   PURPOSE - SEE FUNCTION                                         */
*/*                                                                  */
*/*   INPUT -                                                        */
*/*     SIOT                                                         */
*/*     EDT                                                          */
*/*                                                                  */
*/* EXITS -                                                          */
*/* 1 CONDITION - EDL SUCCESSFULLY BUILT                             */
*/*                                                                  */
*/*   OUTPUT -                                                       */
*/*     SIOT -                                                       */
*/*       SIOTEDLP - POINTER TO EDL                                  */
*/*       SIOTEDLS - SIZE OF EDL = 48 BYTES                          */
*/*       S3400DSP - SET ON IF 3400 TAPE                     @YM07622*/
*/*                                                                  */
*/*     EDL -                                                        */
*/*       EDLLUV   - LUV FROM EDT                                    */
*/*       EDLGMTP  - PTR. TO GROUP MASK TABLE                        */
*/*       EDLFGENP - PTR. TO FIRST GENERIC ENTRY                     */
*/*       EDLGENNO - 1                                               */
*/*       EDLNXTNT - 0                                               */
*/*       EDLDEVT  - UCBTYPE FROM UCB                                */
*/*       EDLGRPNO - 1                                               */
*/*       EDLGRPTR - PTR. TO GROUP ENTRY                             */
*/*       EINDEVTP - 0                                               */
*/*       EDLNXGRP - 0                                               */
*/*       EDLUCBNO - 1                                               */
*/*       EDLLISP - PTR. TO UCB ADDRESS                              */
*/*       EDLGRIP - GROUP ID OF GENERIC                              */
*/*       EDLUCBAD - UCB ADDRESS FROM SIOT                           */
*/*                                                                  */
*/*   RETURN CODE - 0                                                */
*/*                                                                  */
*/* 2 CONDITION - EDL NOT BUILT                                      */
*/*                                                                  */
*/*   OUTPUT -                                                       */
*/*     STEPRSNC - STEP RELATED REASON CODE                  @G24LPSJ*/
*/*       2 = GETMAIN FAILURE                                @G24LPSJ*/
*/*                                                          @G24LPSJ*/
*/*     SIOTRSNC - DD RELATE REASON CODE                     @G24LPSJ*/
*/*       72 = MSS NOT UP FOR SINGLE GENERIC REQ             @G24LPSJ*/
*/*                                                                  */
*/*   RETURN CODE - 4                                                */
*/********************************************************************/
*SIOTEDLS = LENGTH(EDLHDR)+LENGTH(EDLENTRY)+LENGTH(EDLGNTRY)    02S0211
*                +LENGTH(EDLUCBL);     /* SIZE FOR SPEC. UNIT EDL    */
         LA    @10,68                                           02S0211
         ST    @10,SIOTEDLS(,SIOTPTR)                           02S0211
* /* ?GETSTORG (SPID,SIOTEDLS,SIOTEDLP);*/                      02S0212
*   RFY (GTSTR0,GTSTR1,GTSTR2) RSTD; /*SAVE RGS*/               02S0212
*   GTSTR0 = SIOTEDLS; /* SET AMOUNT*/                          02S0213
         LR    GTSTR0,@10                                       02S0213
*   GTSTR2 = SPID; /* SET SUBPOOL*/                             02S0214
         LA    GTSTR2,230                                       02S0214
*   GENERATE CODE;                                              02S0215
     GETMAIN RC,LV=(0),SP=(2)  DO GETMAIN
*   GTSTRTCD = GTSTR15; /* SAVE GETMAIN RET COD*/               02S0216
         ST    GTSTR15,GTSTRTCD                                 02S0216
*   SIOTEDLP = GTSTR1; /* ADDR OF GOTN STORG*/                  02S0217
         ST    GTSTR1,SIOTEDLP(,SIOTPTR)                        02S0217
*   RFY (GTSTR0,GTSTR1,GTSTR2) UNRSTD; /*RLSE */                02S0218
*   IF GTSTRTCD = 0 THEN /*GETMAIN SUCCESSFUL? */               02S0219
         L     @10,GTSTRTCD                                     02S0219
         LTR   @10,@10                                          02S0219
         BNZ   @RF00219                                         02S0219
*     DO;               /* YES, CLEAR STORAGE.*/                02S0220
*       RFY (GTSTR2,GTSTR4,GTSTR5,GTSTR6,                       02S0221
*                          GTSTR7,GTSTR8) RSTD;                 02S0221
*       GTSTR4 = SIOTEDLP; /*STORG PTR*/                        02S0222
         L     @10,SIOTEDLP(,SIOTPTR)                           02S0222
         LR    GTSTR4,@10                                       02S0222
*       GTSTR5 = SIOTEDLS; /*STORG AMNT*/                       02S0223
         L     GTSTR5,SIOTEDLS(,SIOTPTR)                        02S0223
*       GTSTR7 = '00000000'X; /*PAD CHAR & LNTH*/               02S0224
         SLR   GTSTR7,GTSTR7                                    02S0224
*       IF SPID = GTST253 THEN                                  02S0225
         B     @RF00225                                         02S0225
*      GEN                                                      02S0226
*   (MODESET KEYADDR=GTSTZERO,SAVEKEY=(2),WORKREG=8);           02S0226
         MODESET KEYADDR=GTSTZERO,SAVEKEY=(2),WORKREG=8
*       GEN REFS(GTSTR4,GTSTR6);                                02S0227
@RF00225 DS    0H                                               02S0227
          MVCL  GTSTR4,GTSTR6 CLEAR GOTTEN STORG
*       IF SPID = GTST253 THEN                                  02S0228
         B     @RF00228                                         02S0228
*         GEN (MODESET KEYADDR=(2));                            02S0229
         MODESET KEYADDR=(2)
*       RFY (GTSTR2,GTSTR4,GTSTR5,GTSTR6,                       02S0230
*                          GTSTR7,GTSTR8) UNRSTD;               02S0230
@RF00228 DS    0H                                               02S0231
*     END;       /* STORAGE CLEARING COMPLETED */               02S0231
*   ELSE                                                        02S0232
*   SIOTEDLP= 0;   /* CLEAR RETURN ADDR. */                     02S0232
         B     @RC00219                                         02S0232
@RF00219 SLR   @10,@10                                          02S0232
