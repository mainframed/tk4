         TITLE 'IEFAB424 - BUILD ELIGIBLE DEVICE LIST                  *
                        '
IEFAB424 CSECT ,                                                01S0001
@MAINENT DS    0H                                               01S0001
         USING *,@15                                            01S0001
         B     @PROLOG                                          01S0001
         DC    AL1(16)                                          01S0001
         DC    C'IEFAB424  76.334'                              01S0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                  01S0001
         BALR  @12,0                                            01S0001
@PSTART  DS    0H                                               01S0001
         USING @PSTART,@12                                      01S0001
         L     @00,@SIZDATD                                     01S0001
         GSPACE   R,LV=(0)
         LR    @11,@01                                          01S0001
         USING @DATD,@11                                        01S0001
         ST    @13,@SA00001+4                                   01S0001
         LM    @00,@01,20(@13)                                  01S0001
         ST    @11,8(,@13)                                      01S0001
         LR    @13,@11                                          01S0001
         MVC   @PC00001(4),0(@01)                               01S0001
*RETNCODE = ZERO;                      /* INITIALIZE ERROR WORD      */
         SLR   @10,@10                                          01S0064
         STH   @10,RETNCODE                                     01S0064
*ATCAPTR = CVTQMWR;                    /*PTR TO AREA WITH IEFAB438 AD*/
         L     @08,CVTPTR                                       01S0065
         MVC   ATCAPTR(4),CVTQMWR(@08)                          01S0065
*SIOTPTR = SIOT1P;                                              01S0066
         L     @15,@PC00001                                     01S0066
         L     @01,ALCWAPTR(,@15)                               01S0066
         L     SIOTPTR,SIOT1P(,@01)                             01S0066
*IF SU24BIT = ON THEN                  /* IF SU 24 INSTALLED @G24LPSJ*/
         L     @01,CVTIHASU(,@08)                               01S0067
         TM    SU24BIT(@01),B'10000000'                         01S0067
         BNO   @RF00067                                         01S0067
*  IF CVTICB ^= ZERO&                  /* THEN IF ICB EXISTS @G24LPSJ*/
*     ICBI = ON&                       /* AND MSS IS INIT.   @G24LPSJ*/
*     ICBB = OFF THEN                  /* AND MSS NOT BROKEN @G24LPSJ*/
         L     @08,CVTEXT2(,@08)                                01S0068
         L     @08,CVTICB(,@08)                                 01S0068
         CR    @08,@10                                          01S0068
         BE    @RF00068                                         01S0068
         TM    ICBI(@08),B'10000000'                            01S0068
         BNO   @RF00068                                         01S0068
         TM    ICBB(@08),B'00010000'                            01S0068
         BNZ   @RF00068                                         01S0068
*    MSSELIG = ON;                     /* SET MSS REQUESTS ELIGIBLE
*                                                            @G24LPSJ*/
         OI    MSSELIG,B'10000000'                              01S0069
*  ELSE                                /* ELSE               @G24LPSJ*/
*    MSSELIG = OFF;                    /* SET MSS REQUEST INELIGIBLE
*                                                            @G24LPSJ*/
         B     @RC00068                                         01S0070
@RF00068 NI    MSSELIG,B'01111111'                              01S0070
*ELSE                                  /* IF NO SU 24 INSTALLED 01S0071
*                                                            @G24LPSJ*/
*  MSSELIG = ON;                       /* SET MSS REQUESTS ELIGIBLE
*                                                            @G24LPSJ*/
         B     @RC00067                                         01S0071
@RF00067 OI    MSSELIG,B'10000000'                              01S0071
*DO SIOTCTR = ONE TO NUMREQS WHILE(RETNCODE=ZERO); /* PROC ALL SIOTS */
@RC00067 LA    @10,1                                            01S0072
         B     @DE00072                                         01S0072
@DL00072 ICM   @10,3,RETNCODE                                   01S0072
         BNZ   @DC00072                                         01S0072
*  RFY EDTHDR BASED(EDTABPTR);         /* SET BASE FOR EDT           */
*  NOBUMP=OFF;                         /* CONTINUE TO BUMP SIOTPTR   */
         NI    NOBUMP,B'01111111'                               01S0074
*  IF SIOTALCD = OFF  THEN             /* SIOT IS NOT ALLOC'D        */
         TM    SIOTALCD(SIOTPTR),B'00000010'                    01S0075
         BNZ   @RF00075                                         01S0075
*    DO;                               /* PROCESS THIS SIOT          */
*      CALL IEFAB438;                  /* GET DDR COUNT              */
         L     @10,ATCAPTR                                      01S0077
         L     @15,ATCA438P(,@10)                               01S0077
         BALR  @14,@15                                          01S0077
*      DDRCNT = REG15;                 /* SAVE DDR COUNT             */
         ST    REG15,DDRCNT                                     01S0078
*      IF SIOTDMND = OFF THEN          /* NOT SPECIFIC UNIT REQUEST  */
         TM    SIOTDMND(SIOTPTR),B'10000000'                    01S0079
         BNZ   @RF00079                                         01S0079
*        DO;                           /* BUILD NON-SPECIFIC EDL     */
*/********************************************************************/
*/*                                                                  */
*/* SEGMENT NAME - BLDNSEDL                                          */
*/*                                                                  */
*/* DESCRIPTIVE NAME - BUILD EDL FOR NON-SPECIFIC UNIT REQUEST       */
*/*                                                                  */
*/* FUNCTION -                                                       */
*/*   THE FUNCTION OF THIS SEGMENT IS TO BUILD THE ELIGIBLE DEVICE   */
*/*   LIST (EDL) FOR A NON-SPECIFIC UNIT REQUEST. THE EDL LISTS ALL  */
*/*   UNITS WHICH A GIVEN REQUEST IS ELIGIBLE TO BE ALLOCATED TO.    */
*/*   THE EDL IS CONSTRUCTED FROM THE ELIGIBLE DEVICE TABLE(EDT).    */
*/*   SEE IEFZB421(EDT MAPPING MACRO) AND IEFZB422(EDL MAPPING MACRO)*/
*/*   FOR AN EXPLANATION OF THE STRUCTURE OF THE EDT AND EDL.        */
*/*                                                                  */
*/* OPERATION -                                                      */
*/*   1. LOCATE THE EDT LOOKUP VALUE ENTRY FOR THIS REQUEST -        */
*/*      THE SIOT CONTAINS A LOOK-UP-VALUE WHICH IS USED AS A SEARCH */
*/*      ARGUMENT INTO THE EDT LOOK-UP-VALUE ENTRIES. THE LOOK-UP-   */
*/*      VALUE IN EACH EDT LOOK-UP-VALUE ENTRY IS COMPARED WITH THE  */
*/*      LOOK-UP-VALUE IN THE SIOT UNTIL A MATCH IS FOUND.           */
*/*                                                                  */
*/*   2. DETERMINE THE SIZE OF THE EDL -                             */
*/*      SIZE OF THE EDL = LENGTH(EDLHDR) +                          */
*/*                        LUVGENNO*LENGTH(EDLENTRY) +               */
*/*                        TOTGRP*LENGTH(EDLGNTRY) +                 */
*/*                        TOTNUM*LENGTH(EDLUCBL)                    */
*/*      WHERE :                                                     */
*/*        LUVGENNO = TOTAL NUMBER OF GENERICS FOR THIS LOOK-UP-VALUE*/
*/*        LENGTH(EDLHDR) = LENGTH OF THE EDL HEADER                 */
*/*        LENGTH(EDLENTRY) = LENGTH OF EACH GENERIC ENTRY IN THE EDL*/
*/*        TOTGRP = TOTAL NUMBER OF GROUPS FOR THIS LOOK-UP-VALUE    */
*/*        LENGTH(EDLGNTRY) = LENGTH OF EACH GROUP ENTRY IN THE EDL  */
*/*        TOTUN = TOTAL NUMBER OF UNITS FOR THIS LOOK-UP-VALUE      */
*/*        LENGTH(EDLUCBL) = LENGTH OF EACH ENTRY IN THE EDL UCB LIST*/
*/*                                                                  */
*/*   3. GET SPACE FOR THE EDL -                                     */
*/*      THE SIZE CALCULATED ABOVE IS USED TO GET SPACE FOR THE      */
*/*      EDL. THE ADDRESS OF THE GOTTEN SPACE IS PUT IN THE SIOT.    */
*/*      THE SIZE IS ALSO PUT IN THE SIOT. IF SPACE IS NOT AVAILABLE */
*/*      FOR THE EDL AN ERROR RETURN CODE IS SET AND A STEP RELATED  */
*/*      REASON CODE INDICATING GETMAIN ERROR IS SET.                */
*/*                                                                  */
*/*   4. FILL IN THE EDL -                                           */
*/*      INFORMATION OBTAINED FROM THE EDT IS USED TO FILL IN THE    */
*/*      EDL.                                                        */
*/*                                                                  */
*/* ENTRY - IN-LINE SEGMENT                                          */
*/*   PURPOSE - SEE FUNCTION                                         */
*/*                                                                  */
*/*   INPUT -                                                        */
*/*     SIOT -                                                       */
*/*       SCTUTYPE(1:4) - LOOK-UP-VALUE(CONVERTED FROM UNIT PARAMETER*/
*/*                                                                  */
*/*     EDT - ELIGIBLE DEVICE TABLE (MAPPED BY IEFZB421)             */
*/*                                                                  */
*/* EXITS -                                                          */
*/* 1 CONDITION - EDL SUCCESSFULLY CONSTRUCTED                       */
*/*                                                                  */
*/*   OUTPUT -                                                       */
*/*     SIOT -                                                       */
*/*       SIOTEDLP - POINTER TO THE EDL                              */
*/*       SIOTEDLS - SIZE OF THE EDL                                 */
*/*                                                                  */
*/*     EDL - ELIGIBLE DEVICE LIST (MAPPED BY IEFZB422)              */
*/*                                                                  */
*/*   RETURN CODE - 0                                                */
*/*                                                                  */
*/* 2 CONDITION - EDL NOT CONSTRUCTED                                */
*/*                                                                  */
*/*   OUTPUT -                                                       */
*/*     STEPRSNC - STEP RELATED REASON CODE                          */
*/*       2 = GETMAIN FAILURE                                        */
*/*       8 = INVALID UNIT TYPE                                      */
*/*                                                                  */
*/*     SIOTRSNC =- DD RELATED REASON CODE -                         */
*/*       10 = INSUFFICIENT UNITS TO SATISFY REQUEST.                */
*/*       72 = MSS NOT UP FOR SINGLE GENERIC REQ             @G24LPSJ*/
*/*                                                                  */
*/*   RETURN CODE - 4                                                */
*/*                                                                  */
*/********************************************************************/
*LUVPTR = EDTLUVSP + LENGTH(LUVHDR);   /* ADDR OF FIRST EDT LUVENTRY */
         L     @08,@PC00001                                     02S0081
         L     @08,ALCWAPTR(,@08)                               02S0081
         L     @08,EDTABPTR(,@08)                               02S0081
         LA    LUVPTR,8                                         02S0081
         AL    LUVPTR,EDTLUVSP(,@08)                            02S0081
*LUVFNDSW = OFF;                       /* LUV NOT YET FOUND IN EDT   */
         NI    LUVFNDSW,B'01111111'                             02S0082
*DO LUVCTR = ONE TO LUVENTNO           /* SEARCH ALL LUV ENTRIES     */
*    WHILE (LUVFNDSW = OFF);           /* UNTIL A MATCH IS FOUND     */
         LA    LUVCTR,1                                         02S0083
         B     @DE00083                                         02S0083
@DL00083 TM    LUVFNDSW,B'10000000'                             02S0083
         BNZ   @DC00083                                         02S0083
*  IF SCTUTYPE(1:4) = LUVALUE THEN     /* LUV FOUND                  */
         CLC   SCTUTYPE(4,SIOTPTR),LUVALUE(LUVPTR)              02S0084
         BNE   @RF00084                                         02S0084
*    LUVFNDSW = ON;                    /* TURN FOUND SW ON           */
         OI    LUVFNDSW,B'10000000'                             02S0085
*  ELSE                                /* NOT RIGHT LUV SECTION      */
*    LUVPTR = LUVPTR + LENGTH(LUVENTRY); /* NEXT LUV ENTRY           */
         B     @RC00084                                         02S0086
@RF00084 AH    LUVPTR,@CH00092                                  02S0086
*END;                                  /* END SEARCH ALL LUV ENTRIES */
@RC00084 AH    LUVCTR,@CH00051                                  02S0087
@DE00083 L     @08,@PC00001                                     02S0087
         L     @08,ALCWAPTR(,@08)                               02S0087
         L     @08,EDTABPTR(,@08)                               02S0087
         L     @08,EDTLUVSP(,@08)                               02S0087
         C     LUVCTR,LUVENTNO(,@08)                            02S0087
         BNH   @DL00083                                         02S0087
@DC00083 DS    0H                                               02S0088
*IF LUVFNDSW = ON THEN                 /* LUV WAS FOUND IN THE EDT   */
         TM    LUVFNDSW,B'10000000'                             02S0088
         BNO   @RF00088                                         02S0088
*  DO;                                 /* BUILD THE EDL FOR THIS SIOT*/
*    TOTGRP = ZERO;                    /* INIT TOTAL NUMBR OF GROUPS */
         SLR   @08,@08                                          02S0090
         ST    @08,TOTGRP                                       02S0090
*    TOTUN = ZERO;                     /* INIT TOTAL NUMBER OF UNITS */
         SLR   TOTUN,TOTUN                                      02S0091
*    GENPTR = LUVGENP;                 /* ADDR OF FIRST GENERIC ENTRY
*                                         FOR THIS LOOK-UP-VALUE     */
         L     GENPTR,LUVGENP(,LUVPTR)                          02S0092
*    DO GENCTR = ONE TO LUVGENNO;      /* COUNT ALL GROUP ENTRIES    */
         LA    @08,1                                            02S0093
         B     @DE00093                                         02S0093
@DL00093 DS    0H                                               02S0094
*      GRPTABP = GENGRPTR;             /* FIRST GROUP ENTRY FOR THIS
*                                         GENERIC                    */
         L     GRPTABP,GENGRPTR(,GENPTR)                        02S0094
*      DO GRPCTR = ONE TO GENGRPNO;    /* COUNT ALL GROUP ENTRIES    */
         LA    @08,1                                            02S0095
         B     @DE00095                                         02S0095
@DL00095 DS    0H                                               02S0096
*        TOTUN = TOTUN + GRPUCBNO;     /* ADD NUMBER OF UNITS FOR
*                                         THIS GROUP TO TOTAL UNITS  */
         L     @07,GRPTR(,GRPTABP)                              02S0096
         AL    TOTUN,GRPUCBNO(,@07)                             02S0096
*        GRPTABP = GRPTABP + LENGTH(GRPTRTAB); /* NEXT GROUP PTR TABLE
*                                         ENTRY                      */
         AH    GRPTABP,@CH00062                                 02S0097
*      END;                            /* END OF GROUP LOOP          */
         AH    @08,@CH00051                                     02S0098
@DE00095 ST    @08,GRPCTR                                       02S0098
         C     @08,GENGRPNO(,GENPTR)                            02S0098
         BNH   @DL00095                                         02S0098
*      TOTGRP = TOTGRP + GENGRPNO;     /* ADD NUMBER OF GROUPS FOR
*                                         THIS GENERIC TO TOTL GROUPS*/
         L     @08,TOTGRP                                       02S0099
         AL    @08,GENGRPNO(,GENPTR)                            02S0099
         ST    @08,TOTGRP                                       02S0099
*      GENPTR = GENPTR + LENGTH(GENENTRY); /* NEXT EDT GENERIC ENTRY */
         AH    GENPTR,@CH01488                                  02S0100
*    END;                              /* END OF GENERIC LOOP        */
         LA    @08,1                                            02S0101
         AL    @08,GENCTR                                       02S0101
@DE00093 ST    @08,GENCTR                                       02S0101
         C     @08,LUVGENNO(,LUVPTR)                            02S0101
         BNH   @DL00093                                         02S0101
*    SIOTEDLS = LUVGENNO*LENGTH(EDLENTRY) + TOTGRP*LENGTH(EDLGNTRY) +
*      TOTUN*LENGTH(EDLUCBL) + LENGTH(EDLHDR); /* SIZE OF EDL        */
         LA    @08,20                                           02S0102
         L     @07,LUVGENNO(,LUVPTR)                            02S0102
         MH    @07,@CH00269                                     02S0102
         L     @01,TOTGRP                                       02S0102
         MR    @00,@08                                          02S0102
         ALR   @07,@01                                          02S0102
         LR    @15,TOTUN                                        02S0102
         SLA   @15,2                                            02S0102
         ALR   @07,@15                                          02S0102
         ALR   @07,@08                                          02S0102
         ST    @07,SIOTEDLS(,SIOTPTR)                           02S0102
*     /* ?GETSTORG (SPID,SIOTEDLS,SIOTEDLP);*/                  02S0103
*       /* DEFINITIONS OF GETMAIN PARMS */                      02S0103
*       DCL                                                     02S0103
*         GTSTR0   FIXED(31) REG(0),/*GTMN PARM REG*/           02S0103
*         GTSTR1   FIXED(31) REG(1),/*GTMN PARM REG*/           02S0103
*         GTSTR2   PTR(31)   REG(2),/*GTMN PARM REG*/           02S0103
*         GTSTR15  FIXED(31) REG(15),/*GTMN RET COD*/           02S0103
*         GTSTRTCD FIXED(31); /* GETMAIN RTN CODE  */           02S0103
*       DCL                                                     02S0104
*         GTSTR3   FIXED(31) REG(3),/* CLEARING REG*/           02S0104
*         GTSTR4   FIXED(31) REG(4),/* CLEARING REG*/           02S0104
*         GTSTR5   FIXED(31) REG(5),/* CLEARING REG*/           02S0104
*         GTSTR6   FIXED(31) REG(6),/* CLEARING REG*/           02S0104
*         GTSTR7   FIXED(31) REG(7),/* CLEARING REG*/           02S0104
*         GTSTR8   FIXED(31) REG(8),/* CLEARING REG*/           02S0104
*         GTSTZERO FIXED(31) INIT(0),                           02S0104
*         GTST253  FIXED(31) CONSTANT(253);                     02S0104
*       RFY (GTSTR0,GTSTR1,GTSTR2) RSTD; /*SAVE RGS*/           02S0105
*       GTSTR0 = SIOTEDLS; /* SET AMOUNT*/                      02S0106
         LR    GTSTR0,@07                                       02S0106
*       GTSTR2 = SPID; /* SET SUBPOOL*/                         02S0107
         LA    GTSTR2,230                                       02S0107
*       GENERATE CODE;                                          02S0108
         GETMAIN RC,LV=(0),SP=(2)  DO GETMAIN
*       GTSTRTCD = GTSTR15; /* SAVE GETMAIN RET COD*/           02S0109
         ST    GTSTR15,GTSTRTCD                                 02S0109
*       SIOTEDLP = GTSTR1; /* ADDR OF GOTN STORG*/              02S0110
         ST    GTSTR1,SIOTEDLP(,SIOTPTR)                        02S0110
*       RFY (GTSTR0,GTSTR1,GTSTR2) UNRSTD; /*RLSE */            02S0111
*       IF GTSTRTCD = 0 THEN /*GETMAIN SUCCESSFUL? */           02S0112
         L     @08,GTSTRTCD                                     02S0112
         LTR   @08,@08                                          02S0112
         BNZ   @RF00112                                         02S0112
*         DO;               /* YES, CLEAR STORAGE.*/            02S0113
*           RFY (GTSTR2,GTSTR4,GTSTR5,GTSTR6,                   02S0114
*                              GTSTR7,GTSTR8) RSTD;             02S0114
*           GTSTR4 = SIOTEDLP; /*STORG PTR*/                    02S0115
         L     @03,SIOTEDLP(,SIOTPTR)                           02S0115
         LR    GTSTR4,@03                                       02S0115
*           GTSTR5 = SIOTEDLS; /*STORG AMNT*/                   02S0116
         L     GTSTR5,SIOTEDLS(,SIOTPTR)                        02S0116
*           GTSTR7 = '00000000'X; /*PAD CHAR & LNTH*/           02S0117
         SLR   GTSTR7,GTSTR7                                    02S0117
*           IF SPID = GTST253 THEN                              02S0118
         B     @RF00118                                         02S0118
*          GEN                                                  02S0119
*       (MODESET KEYADDR=GTSTZERO,SAVEKEY=(2),WORKREG=8);       02S0119
         MODESET KEYADDR=GTSTZERO,SAVEKEY=(2),WORKREG=8
*           GEN REFS(GTSTR4,GTSTR6);                            02S0120
@RF00118 DS    0H                                               02S0120
              MVCL  GTSTR4,GTSTR6 CLEAR GOTTEN STORG
*           IF SPID = GTST253 THEN                              02S0121
         B     @RF00121                                         02S0121
*             GEN (MODESET KEYADDR=(2));                        02S0122
         MODESET KEYADDR=(2)
*           RFY (GTSTR2,GTSTR4,GTSTR5,GTSTR6,                   02S0123
*                              GTSTR7,GTSTR8) UNRSTD;           02S0123
@RF00121 DS    0H                                               02S0124
*         END;       /* STORAGE CLEARING COMPLETED */           02S0124
*       ELSE                                                    02S0125
*       SIOTEDLP= 0;   /* CLEAR RETURN ADDR. */                 02S0125
         B     @RC00112                                         02S0125
@RF00112 SLR   @08,@08                                          02S0125
         ST    @08,SIOTEDLP(,SIOTPTR)                           02S0125
*       GTSTR15 = GTSTRTCD;    /* GTMN RETURN CODE */           02S0126
@RC00112 L     GTSTR15,GTSTRTCD                                 02S0126
*                                      /* RESTORE LIST PRINT STATUS  */
*/* GET CORE FOR EDL          */                                02S0127
*    IF REG15 = ZERO THEN              /* GETMAIN SUCCESSFUL         */
         LTR   REG15,REG15                                      02S0127
         BNZ   @RF00127                                         02S0127
*      DO;                             /* BUILD THE EDL              */
*        EDLLUV = LUVALUE;             /* LOOK-UP-VALUE FROM EDT     */
         L     @08,SIOTEDLP(,SIOTPTR)                           02S0129
         MVC   EDLLUV(4,@08),LUVALUE(LUVPTR)                    02S0129
*        EDLGENNO = LUVGENNO;          /* NO OF GENERICS FROM EDT    */
         MVC   EDLGENNO(4,@08),LUVGENNO(LUVPTR)                 02S0130
*        EDLFGENP = SIOTEDLP + LENGTH(EDLHDR); /* EDL PTR TO FIRST
*                                         GENERIC ENTRY              */
         LA    @02,20                                           02S0131
         ALR   @02,@08                                          02S0131
         ST    @02,EDLFGENP(,@08)                               02S0131
*        EDLGMTP = LUVGMTP;            /* EDL PTR TO GROUP MASK      */
         MVC   EDLGMTP(4,@08),LUVGMTP(LUVPTR)                   02S0132
*        ONEGENER = OFF;               /* TURN OFF SWITCH    @G24LPSJ*/
         NI    ONEGENER,B'01111111'                             02S0133
*/********************************************************************/
*/*                                                                  */
*/* SEGMENT NAME - FILLEDL                                           */
*/*                                                                  */
*/* DESCRIPTIVE NAME - FILL IN EDL FOR A NON-SPECIFIC UNIT REQUEST   */
*/*                                                                  */
*/* FUNCTION -                                                       */
*/*   THE FUNCTION OF THIS SEGMENT IS TO FILL IN THE EDL FIELDS IN   */
*/*   THE EDL FOR A NON-SPECIFIC UNIT REQUEST.  THIS SEGMENT ALSO    */
*/*   DETERMINES IF THERE ARE ENOUGH UNITS IN THE SYSTEM TO SATISFY  */
*/*   THE REQUEST.                                                   */
*/*                                                                  */
*/* OPERATION -                                                      */
*/*   1. FILL IN THE EDL GENERIC ENTRIES.                            */
*/*      THE NUMBER OF GENERIC ENTRIES IS OBTAINED FROM THE EDT      */
*/*      LOOK UP VALUE ENTRY.  THE FOLLOWING FIELDS ARE FILLED IN:   */
*/*        DEVICE TYPE - FROM DEVICE TYPE IN EDT GENERIC ENTRY       */
*/*        NUMBER OF GROUPS - FROM NUMBER OF GROUPS IN EDT GENERIC   */
*/*          ENTRY                                                   */
*/*        POINTER TO NEXT GENERIC ENTRY - POINTER TO NEXT GENERIC   */
*/*          OR ZERO FOR LAST ENTRY                                  */
*/*        POINTER TO FIRST GROUP ENTRY                              */
*/*      IF THE ALLOCATION ACROSS GENERICS BIT IS ON IN THE EDT IT   */
*/*      IS SET ON IN THE EDL.                                       */
*/*                                                                  */
*/*      IF A REQUEST IS FOR AN ESOTERIC WHICH CONTAINS              */
*/*      ONLY 3400 TAPE DEVICES, SET THE '3400 ONLY' BIT IN          */
*/*      THE SIOT.                                                   */
*/*                                                                  */
*/*   2. FILL IN THE EDL GROUP ENTRIES FOR EACH GENERIC.             */
*/*      FOR EACH GENERIC, IF IT IS MSS AND MSS IS           @G24LPSJ*/
*/*      INELIGIBLE, THEN MARK THE EDL GROUP ENTRY           @G24LPSJ*/
*/*      INELIGIBLE (EDLJINEL).                              @G24LPSJ*/
*/*      THE NUMBER OF GROUP ENTRIES IS OBTAINED FROM THE EDT GENERIC*/
*/*      ENTRY.  THE FOLLOWING FIELDS ARE FILLED IN:                 */
*/*        POINTER TO NEXT GROUP ENTRY - POINTER TO NEXT GROUP ENTRY */
*/*          OR ZERO FOR LAST ENTRY.                                 */
*/*        NUMBER OF UCBS FOR GROUP - FROM EDT GROUP ENTRY           */
*/*        POINTER TO UCB LIST                                       */
*/*        GROUP ID - FROM EDT GROUP ENTRY                           */
*/*                                                                  */
*/*   3. FILL IN THE EDL UCB LIST FOR EACH GROUP.                    */
*/*      THE NUMBER OF UCBS IN THE LIST IS OBTAINED FROM THE EDT     */
*/*      GROUP ENTRY. THE EDT UCB LIST ENTRY HAS AN INDEX INTO THE   */
*/*      IOS UCB LOOK UP TABLE. THIS INDEX IS USED TO LOCATE THE     */
*/*      UCB ADDRESS. THE INDEX IN THE EDT HAS A 0 ORIGIN.           */
*/*                                                                  */
*/*   4. DETERMINE IF THERE ARE ENOUGH UNITS TO SATISFY THE REQUEST. */
*/*      IF THERE ARE ENOUGH UNITS, VERIFY THAT THE GENERIC  @G24LPSJ*/
*/*      IS NOT MSS OR THAT MSS REQUESTS ARE ELIGIBLE.       @G24LPSJ*/
*/*      THERE ARE TWO CASES FOR THIS:                               */
*/*      A. NO ALLOCATION ACROSS GENERICS -                          */
*/*         IF THE SUM OF THE NUMBER OF ALL UCBS TAKEN OVER ALL      */
*/*         GROUPS FOR A GENERIC IS LESS THAN THE TOTAL UNITS        */
*/*         REQUIRED FROM THE SIOT, THE INELIGIBLE DEVICE TYPE BIT   */
*/*         IS SET IN THAT GENERIC ENTRY EDL. IF THERE ARE ENOUGH    */
*/*         UNITS AN INDICATOR IS SET MEANING THAT AT LEAST ONE      */
*/*         GENERIC HAD ENOUGH UNITS.                                */
*/*      B. ALLOCATION ACROSS GENERICS IS POSSIBLE -                 */
*/*         IF THE SUM OF ALL UCBS FOR ALL GENERICS IS GREATER THAN  */
*/*         THE TOTAL NUMBER OF UNITS REQUIRED FROM THE SIOT, AN     */
*/*         INDICATOR IS SET MEANING THAT THERE ARE ENOUGH  UNITS    */
*/*         FOR THIS REQUEST.                                        */
*/*                                                                  */
*/* NOTE: ALTHOUGH THE EDL ENTRIES ARE CHAINED THEY ARE BUILT        */
*/*       IN CONTIGUOUS CORE.                                        */
*/*                                                                  */
*/* ENTRY - IN-LINE SEGMENT                                          */
*/*   PURPOSE - SEE FUNCTION                                         */
*/*                                                                  */
*/*   INPUT -                                                        */
*/*     EDT                                                          */
*/*                                                                  */
*/*     SIOT -                                                       */
*/*       SIOTEDLP - PTR TO CORE FOR EDL                             */
*/*       SIOTOTUN - TOTAL NUMBER OF UNITS REQUIRED                  */
*/*                                                                  */
*/* EXIT -                                                           */
*/*   CONDITION - EDL BUILT                                          */
*/*                                                                  */
*/*   OUTPUT -                                                       */
*/*     EDL FILLED IN                                                */
*/*                                                                  */
*/*     ELIGSW - IF ON INDICATES THERE ARE ENOUGH UNITS TO SATISFY   */
*/*              THIS REQUEST.                                       */
*/*     S3400DSP - SET ON IF 3400 TAPE                               */
*/*                                                          @G24LPSJ*/
*/*     ONEGENER - SET ON IF ONE GENERIC                     @G24LPSJ*/
