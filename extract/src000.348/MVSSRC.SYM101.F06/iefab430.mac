         ST    @10,@AL00001+4                                   01S0166
         LA    @10,PSPRMPTR                                     01S0166
         ST    @10,@AL00001+8                                   01S0166
         L     @15,@CV00077                                     01S0166
         LA    @01,@AL00001                                     01S0166
         BALR  @14,@15                                          01S0166
*        ERRCODE2 = REG15;             /*SAVE RET CODE               */
         LR    ERRCODE2,REG15                                   01S0167
*        STORAGE = OFF;                /*SET STORAGE  OFF            */
         NI    STORAGE,B'10111111'                              01S0168
*      END;                            /*DONE                        */
*    ELSE;                             /*NO STORAGE REQUESTS         */
@RF00163 DS    0H                                               01S0171
*    IF PUBLREQS ^= ZERO & ERRCODE2 = ZERO THEN /*PROCESS PUBLICS    */
         SLR   @10,@10                                          01S0171
         L     @07,ALCWAPTR                                     01S0171
         C     @10,PUBLREQS(,@07)                               01S0171
         BE    @RF00171                                         01S0171
         CR    ERRCODE2,@10                                     01S0171
         BNE   @RF00171                                         01S0171
*      DO;                                                      01S0172
*        PUBLIC = ON;                  /*SET TO PROCESS PUBLICS      */
         OI    PUBLIC,B'10000000'                               01S0173
*        CALL IEFAB436(ALCWAPTR,PRLSTPTR,PSPRMPTR); /*ALLOC PUBLICS  */
         LA    @10,ALCWAPTR                                     01S0174
         ST    @10,@AL00001                                     01S0174
         LA    @10,PRLSTPTR                                     01S0174
         ST    @10,@AL00001+4                                   01S0174
         LA    @10,PSPRMPTR                                     01S0174
         ST    @10,@AL00001+8                                   01S0174
         L     @15,@CV00077                                     01S0174
         LA    @01,@AL00001                                     01S0174
         BALR  @14,@15                                          01S0174
*        ERRCODE2 = REG15;             /*SAVE RET CODE               */
         LR    ERRCODE2,REG15                                   01S0175
