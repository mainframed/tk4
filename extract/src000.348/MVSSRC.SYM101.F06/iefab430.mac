@RF00171 DS    0H                                               01S0179
*    IF PUBLREQS^=ZERO & ERRCODE2 =ZERO THEN /*ANY PUBLIC REQUESTS
*                                        NOT YET ALLOCATED           */
         SLR   @10,@10                                          01S0179
         L     @07,ALCWAPTR                                     01S0179
         C     @10,PUBLREQS(,@07)                               01S0179
         BE    @RF00179                                         01S0179
         CR    ERRCODE2,@10                                     01S0179
         BNE   @RF00179                                         01S0179
*      DO;                                                      01S0180
*        PUBSTOR = ON;                 /*SET TO PROCESS PUBLIC REQ'S
*                                        TO PUBLIC & STORAGE VOLUMES */
         OI    PUBSTOR,B'00100000'                              01S0181
*        CALL IEFAB436(ALCWAPTR,PRLSTPTR,PSPRMPTR); /*ALLOCATE PUBLC
*                                       REQUEST TO PUBLIC & STORAGE
*                                       VOLUMES                      */
         LA    @10,ALCWAPTR                                     01S0182
         ST    @10,@AL00001                                     01S0182
         LA    @10,PRLSTPTR                                     01S0182
         ST    @10,@AL00001+4                                   01S0182
         LA    @10,PSPRMPTR                                     01S0182
         ST    @10,@AL00001+8                                   01S0182
         L     @15,@CV00077                                     01S0182
         LA    @01,@AL00001                                     01S0182
         BALR  @14,@15                                          01S0182
*        ERRCODE2 = REG15;             /*SAVE RET  CODE              */
         LR    ERRCODE2,REG15                                   01S0183
*        PUBSTOR = OFF;                                         01S0184
         NI    PUBSTOR,B'11011111'                              01S0184
