@RF00179 DS    0H                                               01S0187
*    IF PRLSTPTR ^= ZERO THEN          /*ANY PRLST TO FREE ?         */
         L     @10,PRLSTPTR                                     01S0187
         LTR   @10,@10                                          01S0187
         BZ    @RF00187                                         01S0187
*      DO;                             /*YES                         */
*        RFY(REG0,REG1) RSTD;                                   01S0189
*        REG0 = PRLSTLEN;              /*LENGTH OF PERM/RES LIST     */
         L     REG0,PRLSTLEN                                    01S0190
*        REG0 = REG0 × SUBPL230;       /*FREE FROM SUBPOOL 230       */
         O     REG0,@CF00088                                    01S0191
