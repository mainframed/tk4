         LR    REG1,@10                                         01S0192
*        GEN (FREEMAIN R,LV=(0),A=(1)); /*FREE PERMRES RESVE LST CORE*/
         FREEMAIN R,LV=(0),A=(1)
*        RFY (REG0,REG1) UNRSTD;                                01S0194
*      END;                                                     01S0195
