*ELSE;                                 /*NO PUB OR STRG REQS         */
@RF00115 DS    0H                                               01S0198
*IF ERRCODE2 = ZERO THEN               /*ANY ERRORS ?                */
@RC00115 LTR   ERRCODE2,ERRCODE2                                01S0198
         BNZ   @RF00198                                         01S0198
*  DO;                                 /*NO,CHECK DEMAND REQS        */
*/********************************************************************/
*/*                                                                  */
*/* SEGMENT NAME - CHEKDMND                                          */
*/*                                                                  */
*/* DESCRIPTIVE NAME - ERROR CHECKER FOR DUPLICATE DEMAND REQUESTS   */
*/*                                                                  */
*/* FUNCTION -                                                       */
*/*     THIS ROUTINE DETERMINES IF THERE IS MORE THAN ONE DEMAND     */
*/*     REQUEST FOR THE SAME UNIT (E.G. TWO DD'S WITH UNIT=190).     */
*/*     IF THERE ARE TWO REQUESTS FOR THE SAME UNIT THE ALLOCATION   */
*/*     IS FAILED UNLESS:                                            */
*/*       (1) THE TWO REQUESTS ARE GOING TO USE THE SAME VOLUME.     */
*/*                                                                  */
*/*        OR                                                        */
*/*                                                                  */
*/*       (2) THE UNIT REQUESTED IS DIRECT ACCESS , AND              */
*/*           BOTH REQUESTS ARE NOT FOR SPECIFIC VOLUMES, AND        */
*/*           NEITHER REQUEST IS PRIVATE OR NONSHAREABLE.            */
*/*                                                          @YA00062*/
*/*       OR                                                 @YA00062*/
*/*                                                          @YA00062*/
*/*       (3) ONE OF THE TWO REQUESTS IS A DD DUMMY.         @YA00062*/
*/*                                                                  */
*/*   OPERATION - THE SIOT CHAIN IS SCANNED TO LOCATE EACH           */
*/*               NON-ALLOCATED DEMAND REQUEST. WHEN A DEMAND        */
*/*               REQ IS FOUND, THE SIOT CHAIN IS RESCANNED TO FIND  */
*/*               OTHER NON-ALLOCATED DEMAND REQS FOR                */
*/*               THE SAME UNIT -- THEN THE ABOVE TESTS ARE MADE.    */
*/* ENTRY - (INLINE SUBROUTINE)                                      */
*/*                                                                  */
*/*   PURPOSE - (SEE FUNCTION)                                       */
*/*                                                                  */
*/*   INPUT -                                                        */
*/*       ALCWAPTR - PTR TO THE ALLOCATION WORK AREA                 */
*/*         SIOT1P - PTR TO THE FIRST SIOT IN THE CHAIN              */
*/*                                                                  */
*/* EXIT - NORMAL - (INLINE SUBROUTINE)                              */
*/*                                                                  */
*/*   CONDITIONS - NO ERRORS DETECTED                                */
*/*                                                                  */
*/*   OUTPUT - (NONE)                                                */
*/*                                                                  */
*/*   RETURN CODES - (SAME AS ON ENTRY)                              */
*/*                                                                  */
*/* EXIT - ERROR - (INLINE SUBROUTINE)                               */
*/*                                                                  */
*/*   CONDITIONS - ERROR DUE TO 2 DEMAND REQS FOR THE SAME UNIT      */
*/*                                                                  */
*/*   OUTPUT - SIOT REASON CODE (SIOTRSNC) SET TO 65                 */
*/*                                                                  */
*/*   RETURN CODES - ERRCODE2 SET TO 4                               */
*/*                                                                  */
*/********************************************************************/
*SIOTPTR = SIOT1P;                     /*POINT TO THE FIRST SIOT     */
         L     @10,ALCWAPTR                                     02S0200
         L     SIOTPTR,SIOT1P(,@10)                             02S0200
*DO WHILE (SIOTPTR^=ZERO & ERRCODE2=ZERO); /*SCAN ALL SIOTS          */
         B     @DE00201                                         02S0201
@DL00201 DS    0H                                               02S0202
*  IF SIOTDMND = ON                    /*DEMAND REQUEST AND          */
*     & SCTDUMMY = OFF THEN            /*NOT DUMMY ?         @YA00062*/
         TM    SIOTDMND(SIOTPTR),B'10000000'                    02S0202
         BNO   @RF00202                                         02S0202
         TM    SCTDUMMY(SIOTPTR),B'10000000'                    02S0202
         BNZ   @RF00202                                         02S0202
*    DO;                               /*YES,LOOK FOR OTHERS         */
*      VUPTR = SVOLUNAD;               /*POINT TO SIOT'S FIRST VU    */
         L     VUPTR,SVOLUNAD(,SIOTPTR)                         02S0204
*      SPTR = SIOT1P;                  /*POINT TO THE FIRST SIOT     */
         L     @10,ALCWAPTR                                     02S0205
         L     SPTR,SIOT1P(,@10)                                02S0205
*      DO WHILE (SPTR^=ZERO & ERRCODE2=ZERO); /*RESCAN ALL SIOTS     */
         B     @DE00206                                         02S0206
@DL00206 DS    0H                                               02S0207
*        IF SPTR->SIOTDMND = ON        /*DEMAND REQUEST              */
*            & SPTR->SCTDUMMY = OFF    /*NOT DUMMY           @YA00062*/
*            & SPTR->SIOUCBAD = SIOUCBAD /*..FOR THE SAME UNIT WITH  */
*            & SPTR->SVOLUNAD->VOLUNTID^=VOLUNTID  /*..NO AFF        */
*            & SIOTALCD=OFF THEN       /*NOT ALLOCATED       @ZA00869*/
         TM    SIOTDMND(SPTR),B'10000000'                       02S0207
         BNO   @RF00207                                         02S0207
         TM    SCTDUMMY(SPTR),B'10000000'                       02S0207
         BNZ   @RF00207                                         02S0207
         CLC   SIOUCBAD(3,SPTR),SIOUCBAD(SIOTPTR)               02S0207
         BNE   @RF00207                                         02S0207
         L     @10,SVOLUNAD(,SPTR)                              02S0207
         CLC   VOLUNTID(2,@10),VOLUNTID(VUPTR)                  02S0207
         BE    @RF00207                                         02S0207
         TM    SIOTALCD(SIOTPTR),B'00000010'                    02S0207
         BNZ   @RF00207                                         02S0207
*          DO;                         /*YES,TEST IF ERROR           */
*            IF SIOUCBAD->UCB3DACC=ON THEN /*DA UNIT REQUESTED ?     */
         ICM   @07,7,SIOUCBAD(SIOTPTR)                          02S0209
         TM    UCB3DACC(@07),B'00100000'                        02S0209
         BNO   @RF00209                                         02S0209
*              DO;                     /*YES,ONLY ERROR IF BOTH SPEC
*                                        VOL REQS OR IF ONE REQ IS
*                                        PRIVATE OR NONSHAREABLE     */
*                IF (SPTR->SVOLUNAD->VOLSPEC=ON & VOLSPEC=ON)   02S0211
*                    × (SPTR->SVOLUNAD->VOLPRV=ON × VOLPRV=ON)  02S0211
*                    × (SPTR->SVOLUNAD->VOLNSHR=ON              02S0211
*                    × VOLNSHR=ON) THEN /*CHECK FOR ERROR COND       */
         TM    VOLSPEC(@10),B'00100000'                         02S0211
         BNO   @GL00020                                         02S0211
         TM    VOLSPEC(VUPTR),B'00100000'                       02S0211
         BO    @RT00211                                         02S0211
@GL00020 L     @10,SVOLUNAD(,SPTR)                              02S0211
         TM    VOLPRV(@10),B'01000000'                          02S0211
         BO    @RT00211                                         02S0211
         TM    VOLPRV(VUPTR),B'01000000'                        02S0211
         BO    @RT00211                                         02S0211
         TM    VOLNSHR(@10),B'00001000'                         02S0211
         BO    @RT00211                                         02S0211
         TM    VOLNSHR(VUPTR),B'00001000'                       02S0211
         BNO   @RF00211                                         02S0211
@RT00211 DS    0H                                               02S0212
*                  DO;                                          02S0212
*                    ERRCODE2 = FAILURE; /*SET ERROR RETURN CODE     */
         LA    ERRCODE2,4                                       02S0213
*                    SIOTRSNC=SAMUNITS;/*SET SIOT REASON CODE        */
         MVC   SIOTRSNC(2,SIOTPTR),@CH00073                     02S0214
*                  END;                                         02S0215
*                ELSE;                 /*NO ERROR                    */
@RF00211 DS    0H                                               02S0217
*              END;                                             02S0217
*            ELSE                      /*NOT A DIRECT ACCESS UNIT    */
*              DO;                                              02S0218
         B     @RC00209                                         02S0218
@RF00209 DS    0H                                               02S0219
*                ERRCODE2 = FAILURE;   /*SET ERROR RETURN CODE       */
         LA    ERRCODE2,4                                       02S0219
*                SIOTRSNC = SAMUNITS;  /*SET SIOT ERROR REASON CODE  */
         MVC   SIOTRSNC(2,SIOTPTR),@CH00073                     02S0220
*              END;                                             02S0221
*          END;                                                 02S0222
*        ELSE;                         /*SAME UNIT NOT DEMANDED      */
@RF00207 DS    0H                                               02S0224
*        SPTR = SPTR->SIOTNPTR;        /*POINT TO THE NEXT SIOT      */
@RC00207 L     SPTR,SIOTNPTR(,SPTR)                             02S0224
*      END;                                                     02S0225
@DE00206 SLR   @10,@10                                          02S0225
         CR    SPTR,@10                                         02S0225
         BE    @DC00206                                         02S0225
         CR    ERRCODE2,@10                                     02S0225
         BE    @DL00206                                         02S0225
@DC00206 DS    0H                                               02S0226
*    END;                                                       02S0226
*  ELSE;                               /*NOT A DEMAND REQUEST        */
@RF00202 DS    0H                                               02S0228
*  SIOTPTR = SIOTNPTR;                 /*POINT TO THE NEXT SIOT      */
         L     SIOTPTR,SIOTNPTR(,SIOTPTR)                       02S0228
*END;                                                           02S0229
@DE00201 SLR   @10,@10                                          02S0229
         CR    SIOTPTR,@10                                      02S0229
         BE    @DC00201                                         02S0229
         CR    ERRCODE2,@10                                     02S0229
         BE    @DL00201                                         02S0229
@DC00201 DS    0H                                               02S0230
*  END;                                                         01S0230
*ELSE;                                 /*ERROR                       */
@RF00198 DS    0H                                               01S0232
*  PERMRES = OFF;                      /*SIGNIFY END OF PERMRES/RESVD
*                                        PROCESSING                  */
         NI    PERMRES,B'11101111'                              01S0232
*RETURN CODE(ERRCODE2);                /*RETURN TO CALLER            */
         L     @13,4(,@13)                                      01S0233
         L     @00,@SIZDATD                                     01S0233
         LR    @01,@11                                          01S0233
         FSPACE   R,LV=(0),A=(1)
         LR    @15,@06                                          01S0233
         L     @14,12(,@13)                                     01S0233
         LM    @00,@12,20(@13)                                  01S0233
         BR    @14                                              01S0233
*END IEFAB430                          /*END OF FIXED DEVICE CONTROL */
