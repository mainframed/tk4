         TITLE 'IEFAB430 - FIXED DEVICE CONTROL                        *
                        '
IEFAB430 CSECT ,                                                01S0001
@MAINENT DS    0H                                               01S0001
         USING *,@15                                            01S0001
         B     @PROLOG                                          01S0001
         DC    AL1(24)                                          01S0001
         DC    C'IEFAB430 78089  UZ17788 '                      01S0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                  01S0001
         BALR  @12,0                                            01S0001
@PSTART  DS    0H                                               01S0001
         USING @PSTART,@12                                      01S0001
         L     @00,@SIZDATD                                     01S0001
         GSPACE   R,LV=(0)
         LR    @11,@01                                          01S0001
         USING @DATD,@11                                        01S0001
         ST    @13,@SA00001+4                                   01S0001
         LM    @00,@01,20(@13)                                  01S0001
         ST    @11,8(,@13)                                      01S0001
         LR    @13,@11                                          01S0001
         MVC   @PC00001(4),0(@01)                               01S0001
*ALCWAPTR = PARMPTR;                   /*SET POINTER TO ALCWA        */
         L     @10,@PC00001                                     01S0054
         L     @10,PARMPTR(,@10)                                01S0054
         ST    @10,ALCWAPTR                                     01S0054
*PSPRMPTR = ADDR(PSCPARM);             /*SET PTR TO CALLED RTN PARAM */
         LA    @07,PSCPARM                                      01S0055
         ST    @07,PSPRMPTR                                     01S0055
*PSSWTCHS = PSSWTCHS && PSSWTCHS;      /*SET SWITCHES OFF            */
         XC    PSSWTCHS(1),PSSWTCHS                             01S0056
*ERRCODE2 = ZERO;                      /*INIT RETURN CODE TO ZERO    */
         SLR   ERRCODE2,ERRCODE2                                01S0057
*PERMRES = ON;                         /*INDICATES PROCESSING IS TO
*                                        BE DONE FOR PERMRES OR RESVD
*                                        VOLUMES                     */
         OI    PERMRES,B'00010000'                              01S0058
*IF SPECREQS ^= ZERO & ERRCODE2 = ZERO THEN /* PROCESS SPECIFIC 01S0059
*                                        REQUESTS IF ANY             */
         SLR   @07,@07                                          01S0059
         C     @07,SPECREQS(,@10)                               01S0059
         BE    @RF00059                                         01S0059
         CR    ERRCODE2,@07                                     01S0059
         BNE   @RF00059                                         01S0059
*  DO;                                                          01S0060
*    CALL IEFAB433(ALCWAPTR,PSPRMPTR); /*CALL IEFAB433               */
         LA    @10,ALCWAPTR                                     01S0061
         ST    @10,@AL00001                                     01S0061
         LA    @10,PSPRMPTR                                     01S0061
         ST    @10,@AL00001+4                                   01S0061
         L     @15,@CV00076                                     01S0061
         LA    @01,@AL00001                                     01S0061
         BALR  @14,@15                                          01S0061
*    ERRCODE2 = REG15;                 /*SAVE RETURN CODE            */
         LR    ERRCODE2,REG15                                   01S0062
*  END;                                                         01S0063
*ELSE;                                 /*DONT PROCESS SPECIFICS      */
@RF00059 DS    0H                                               01S0065
*IF ERRCODE2 = ZERO THEN               /*ANY ERRORS?                 */
         LTR   ERRCODE2,ERRCODE2                                01S0065
         BNZ   @RF00065                                         01S0065
*  DO;                                 /*NO, UPDATE COUNT TABLE      */
*/********************************************************************/
*/*                                                                  */
*/* SEGMENT NAME - UPDTCNT                                           */
*/*                                                                  */
*/* DESCRIPTIVE NAME - UPDATE COUNT TABLE                            */
*/*                                                                  */
*/* FUNCTION -                                                       */
*/*   THIS ROUTINE DOES THE FOLLOWING FOR EACH UNALLOCATED VOLUNIT   */
*/*   ENTRY:                                                         */
*/*     1. UPDATES THE TOTAL COUNT FIELD(TOTVOLUN)                   */
*/*     2. DETERMINES THE TYPE OF THIS VOLUNIT ENTRY AND UPDATES A   */
*/*        COUNT FIELD AS FOLLOWS:                                   */
*/*          - TYPE -                     - FIELD UPDATED -          */
*/*          PRIVATE OR SPECIFIC     SPECREQS (COUNT OF NO. SPECIFIC)*/
*/*          PUBLIC                  PUBLREQS (COUNT OF NO. PUBLIC)  */
*/*          STORAGE                 STRGREQS(COUNT OF NO. STORAGE)  */
*/*          PRIVATE/NON-SPECIFIC    PVTNREQS(COUNT OF PRIV/NON-SPEC)*/
*/*                                                                  */
*/*   THIS ROUTINE ALSO DETERMINES THE NUMBER OF PUBLIC/STORAGE UNITS*/
*/*   REMAINING TO BE ALLOCATED PER REQUEST.                         */
*/*                                                                  */
*/* ENTRY -  INLINE SEGMENT OF IEFAB430                              */
*/*   PURPOSE - SEE FUNCTION.                                        */
*/*                                                                  */
*/*   INPUT  -                                                       */
*/*     SIOT1P - POINTER TO FIRST SIOT, IN IEFZB425                  */
*/*     CNTABLE - TABLE OF COUNTS BY REQUEST TYPE, IN IEFZB425       */
*/* EXITS -                                                          */
*/*   CONDITION - COUNT TABLE UPDATED                                */
*/*                                                                  */
*/*   OUTPUT  -                                                      */
*/*     CNTABLE - UPDATED TO REFLECT THE FACT THAT SPECIFIC REQUESTS */
*/*               HAVE BEEN ALLOCATED.                               */
*/*                                                                  */
*/*     SIOPSCNT - IN IEFASIOT, UPDATED COUNT OF THE NUMBER OF PUBLIC*/
*/*                AND STORAGE UNITS TO BE ALLOCATED.                */
*/*                                                                  */
*/*   RETURN CODE - NONE                                             */
*/*                                                                  */
*/********************************************************************/
*COUNTERS = COUNTERS && COUNTERS;      /*CLEAR COUNT ACCUMULATORS    */
         XC    COUNTERS(20),COUNTERS                            02S0067
*SIOTPTR = SIOT1P;                     /*GET POINTER TO FIRST SIOT   */
         L     @10,ALCWAPTR                                     02S0068
         L     SIOTPTR,SIOT1P(,@10)                             02S0068
*DO I = 1 TO NUMREQS;                  /*LOOP THRU ALL REQUESTS      */
         LA    I,1                                              02S0069
         B     @DE00069                                         02S0069
@DL00069 DS    0H                                               02S0070
*  VUPTR = SVOLUNAD;                   /*PT TO FIRST ENTRY OF REQ    */
         L     VUPTR,SVOLUNAD(,SIOTPTR)                         02S0070
*  IF SIOTALCD = OFF THEN              /*IF SIOT IS NOT ALLOC'D 02S0071
*                                        PROCESS ITS VOLUNIT ENTRYS  */
         TM    SIOTALCD(SIOTPTR),B'00000010'                    02S0071
         BNZ   @RF00071                                         02S0071
*    DO VUCTR = ONE TO SVOLUNNO;       /*LOOP THRU ALL VOLUNIT ENTRYS*/
         LA    VUCTR,1                                          02S0072
         B     @DE00072                                         02S0072
@DL00072 DS    0H                                               02S0073
*      IF VOLALOC = OFF & VDEVREQD = ON THEN/*IF ANY NOT ALLOC'D
*                                        AND A UNIT IS REQUIRED      */
         TM    VDEVREQD(VUPTR),B'00100000'                      02S0073
         BNO   @RF00073                                         02S0073
         TM    VOLALOC(VUPTR),B'10000000'                       02S0073
         BNZ   @RF00073                                         02S0073
*        DO;                                                    02S0074
*          TOTLCNT = TOTLCNT+ONE;      /*INCREMENT TOTAL COUNT OF
*                                        UNITS LEFT TO BE ALLOC'D FOR
*                                        THIS STEP                   */
         LA    @10,1                                            02S0075
         L     @07,TOTLCNT                                      02S0075
         ALR   @07,@10                                          02S0075
         ST    @07,TOTLCNT                                      02S0075
*          IF VOLSPEC = ON THEN        /*IS THIS A SPECIFIC REQ      */
         TM    VOLSPEC(VUPTR),B'00100000'                       02S0076
         BNO   @RF00076                                         02S0076
*            DO;                                                02S0077
*              SPECCNT  = SPECCNT+ONE; /*UPDATE NUMBER OF UNITS 02S0078
*                                          STILL TO BE ALLOCATED TO
*                                          SPECIFICS FOR THIS STEP   */
         AL    @10,SPECCNT                                      02S0078
         ST    @10,SPECCNT                                      02S0078
*            END;                                               02S0079
*          ELSE                        /*NOT SPECIFIC                */
*            DO;                                                02S0080
         B     @RC00076                                         02S0080
@RF00076 DS    0H                                               02S0081
*              IF VOLPUB = ON THEN     /*IS THIS A PUBLIC VOL REQEST */
         TM    VOLPUB(VUPTR),B'10000000'                        02S0081
         BNO   @RF00081                                         02S0081
*                DO;                                            02S0082
*                  PUBLCNT  = PUBLCNT+ONE; /*UPDATE NO. PUBLICS      */
         LA    @10,1                                            02S0083
         L     @07,PUBLCNT                                      02S0083
         ALR   @07,@10                                          02S0083
         ST    @07,PUBLCNT                                      02S0083
*                  IF VOLNSHR = OFF THEN /*IF VOLUME IS SHAREABLE
*                                        INCREMENT COUNT OF     02S0084
*                                        PUBLIC AND STORAGE     02S0084
*                                        DEVICES NEEDED              */
         TM    VOLNSHR(VUPTR),B'00001000'                       02S0084
         BNZ   @RF00084                                         02S0084
*                    SIOPSCNT = SIOPSCNT + ONE;                 02S0085
         SLR   @07,@07                                          02S0085
         IC    @07,SIOPSCNT(,SIOTPTR)                           02S0085
         ALR   @10,@07                                          02S0085
         STC   @10,SIOPSCNT(,SIOTPTR)                           02S0085
*                END;                                           02S0086
*              ELSE                    /*NOT PUBLIC                  */
*                DO;                                            02S0087
         B     @RC00081                                         02S0087
@RF00081 DS    0H                                               02S0088
*                  IF VOLSTG = ON THEN /*IS THIS STORAGE             */
         TM    VOLSTG(VUPTR),B'00010000'                        02S0088
         BNO   @RF00088                                         02S0088
*                    DO;                                        02S0089
*                      STRGCNT = STRGCNT+ONE; /*UPDATE NO. STORAGE
*                                        DEVICES REQUIRED            */
         LA    @10,1                                            02S0090
         L     @07,STRGCNT                                      02S0090
         ALR   @07,@10                                          02S0090
         ST    @07,STRGCNT                                      02S0090
*                      IF VOLNSHR = OFF THEN /*IF VOL SHAREABLE      */
         TM    VOLNSHR(VUPTR),B'00001000'                       02S0091
         BNZ   @RF00091                                         02S0091
*                        SIOPSCNT = SIOPSCNT + ONE; /*INCREMENT 02S0092
*                                        COUNT OF PUBLIC AND STORAGE
*                                        DEVICES NEEDED              */
         SLR   @07,@07                                          02S0092
         IC    @07,SIOPSCNT(,SIOTPTR)                           02S0092
         ALR   @10,@07                                          02S0092
         STC   @10,SIOPSCNT(,SIOTPTR)                           02S0092
*                    END;                                       02S0093
*                  ELSE                /*NOT STORAGE                 */
*                    DO;                                        02S0094
         B     @RC00088                                         02S0094
@RF00088 DS    0H                                               02S0095
*                      IF VOLPRV= ON & VOLSPEC = OFF THEN /*IF VOL
*                                        IS PRIVATE NON-SPECIFIC     */
         TM    VOLPRV(VUPTR),B'01000000'                        02S0095
         BNO   @RF00095                                         02S0095
         TM    VOLSPEC(VUPTR),B'00100000'                       02S0095
         BNZ   @RF00095                                         02S0095
*                        PNSPCNT = PNSPCNT +ONE; /*INCREMENT COUNT
*                                        OF UNIT STILL TO BE ALLOC-
*                                        ATED FOR THIS STEP          */
         LA    @10,1                                            02S0096
         AL    @10,PNSPCNT                                      02S0096
         ST    @10,PNSPCNT                                      02S0096
*                      ELSE;           /*NOT PRIVATE                 */
@RF00095 DS    0H                                               02S0098
*                    END;                                       02S0098
*                END;                                           02S0099
*            END;                                               02S0100
*        END;                                                   02S0101
*      ELSE;                           /*THIS ENTRY WAS ALLOC'D      */
@RF00073 DS    0H                                               02S0103
*      VUPTR = VUPTR + VULENGTH;       /*UPDATE TO NEXT ENTRY        */
@RC00073 AL    VUPTR,VULENGTH                                   02S0103
*    END;                                                       02S0104
         AH    VUCTR,@CH00052                                   02S0104
@DE00072 CH    VUCTR,SVOLUNNO(,SIOTPTR)                         02S0104
         BNH   @DL00072                                         02S0104
*  ELSE;                               /*REQ WAS COMPLETELY ALLOC'D  */
@RF00071 DS    0H                                               02S0106
*  SIOTPTR = SIOTNPTR;                 /*PT TO NEXT SIOT             */
         L     SIOTPTR,SIOTNPTR(,SIOTPTR)                       02S0106
*END;                                                           02S0107
         AH    I,@CH00052                                       02S0107
@DE00069 L     @10,ALCWAPTR                                     02S0107
         C     I,NUMREQS(,@10)                                  02S0107
         BNH   @DL00069                                         02S0107
*SPECREQS = SPECCNT;                   /*UPDATE COUNT TABLE OF  02S0108
*                                        DEVICES STILL NEEDED        */
         L     @10,ALCWAPTR                                     02S0108
         MVC   SPECREQS(4,@10),SPECCNT                          02S0108
*PUBLREQS = PUBLCNT;                   /* NO. OF PUBLIC              */
         MVC   PUBLREQS(4,@10),PUBLCNT                          02S0109
*STRGREQS = STRGCNT;                   /* NO. OF STORAGE             */
         MVC   STRGREQS(4,@10),STRGCNT                          02S0110
*PVTNREQS = PNSPCNT;                   /* NO. OF PRIVATE/NON-   02S0111
*                                         SPECIFIC                   */
         MVC   PVTNREQS(4,@10),PNSPCNT                          02S0111
*TOTVOLUN = TOTLCNT;                   /*TOTAL UNALLOCATED VU   02S0112
*                                         ENTRIES                    */
         MVC   TOTVOLUN(4,@10),TOTLCNT                          02S0112
*  END;                                                         01S0113
*ELSE;                                 /*NULL                        */
@RF00065 DS    0H                                               01S0115
*IF (PUBLREQS ^= ZERO × STRGREQS ^= ZERO) /*ANY PUBLIC OR STORAGE    */
*     & ERRCODE2 = ZERO THEN           /*REQUESTS AND NO ERRORS YET? */
         SLR   @10,@10                                          01S0115
         L     @07,ALCWAPTR                                     01S0115
         C     @10,PUBLREQS(,@07)                               01S0115
         BNE   @GL00002                                         01S0115
         C     @10,STRGREQS(,@07)                               01S0115
         BE    @RF00115                                         01S0115
@GL00002 LTR   ERRCODE2,ERRCODE2                                01S0115
         BNZ   @RF00115                                         01S0115
*  DO;                                                          01S0116
*/********************************************************************/
*/*                                                                  */
*/* SEGMENT NAME - BLDPRLST                                          */
*/*                                                                  */
*/* DESCRIPTIVE NAME - BUILD PERMRES/RESERVED LIST                   */
*/*                                                                  */
*/* FUNCTION - THE FUNCTION OF THIS ROUTINE IS TO BUILD A LIST OF    */
*/*   HALFWORD POINTERS TO UCBS. THESE UCBS WILL BE THOSE WHICH      */
*/*   REPRESENT DIRECT ACCESS DEVICES WHICH ARE ONLINE, READY, NOT   */
*/*   PENDING OFFLINE NOR PENDING UNLOAD,  NOT                       */
*/*   IN USE BY OLTEP, AND PERMANENTLY RESIDENT OR RESERVED          */
*/*   WITH A VOLUME SERIAL NUMBER IN THE UCB.                @ZA18146*/
*/*   THIS LIST IS THEN USED BY THE ALLOCATION ROUTINES FOR THEIR    */
*/*   DEVICE SELECTION. IF CORE IS NOT AVAILABLE FOR THE LIST A      */
*/*   GETMAIN ERROR INDICATOR WILL BE RETURNED IN REG 15.            */
*/*                                                                  */
*/* ENTRY -  INLINE SEGMENT OF IEFAB430                              */
*/*   PURPOSE - SEE FUNCTION                                         */
*/*                                                                  */
*/*   INPUT  -                                                       */
*/*     ALCWAPTR - POINTER TO ALLOCATION WORK AREA.                  */
*/*     EDTNUCBS - NUMBER OF ENTRIES IN IOS LOOK-UP TABLE.   @Z40FPPD*/
*/*                USED TO DETERMINE MAXIMUM SIZE OF LIST.   @Z40FPPD*/
*/*                                                                  */
*/* EXITS -                                                          */
*/*   CONDITION - PERMRES/RESERVED LIST BUILT                        */
*/*                                                                  */
*/*     OUTPUT-                                                      */
*/*        PRLST - A LIST OF HALFWORD POINTERS TO UCBS WHICH ARE     */
*/*              DIRECT ACCESS,ONLINE,READY,PERMANENTLY RESIDENT OR  */
*/*              RESERVED.                                           */
*/*                                                                  */
*/*        PRLSTLEN - A FULL WORD FIELD IN PSCPARM CONTAINING THE    */
*/*                   LENGTH OF THE PERMRES/RESERVED LIST.           */
*/*                                                                  */
*/*                                                                  */
*/*        PRLSTPTR - A FULL WORD POINTER TO THE PRLIST.             */
*/*                                                                  */
*/*     RETURN CODE -                                                */
*/*        ERRCODE2 - 0, INDICATING PRLIST WAS SUCCESSFULLY BUILT.   */
*/*                                                                  */
*/*                                                                  */
*/*   CONDITION - NO CORE AVAILABLE TO BUILD PRLIST.                 */
*/*                                                                  */
*/*     OUTPUT-                                                      */
*/*        FAILCODE - 2, INDICATING GETMAIN FAILURE                  */
*/*                                                                  */
*/*     RETURN CODE -                                                */
*/*        ERRCODE2 - 4,INDICATING GETMAIN FAILURE TRYING TO GET     */
*/*                     CORE TO BUILD PRLIST                         */
*/*                                                                  */
*/*                                                                  */
*/*                                                                  */
*/********************************************************************/
*RFY IOSLUT BASED(IOSLUTP);                                     02S0117
*SLOTNO = ONE;                         /*INIT PRLST SLOT POINTER     */
         LA    SLOTNO,1                                         02S0118
*PRLSTLEN =  (ALCNUCBS * TWO) + TWO;   /*CALC MAX LENGTH OF LIST PLUS
*                                        LIST END INDICATOR  @Z40FPPJ*/
         L     @08,ALCWAPTR                                     02S0119
         L     @08,ALCNUCBS(,@08)                               02S0119
         ALR   @08,@08                                          02S0119
         AH    @08,@CH00091                                     02S0119
         ST    @08,PRLSTLEN                                     02S0119
* /* ?GETSTORG (SPID,PRLSTLEN,PRLSTPTR);*/                      02S0120
*   /* DEFINITIONS OF GETMAIN PARMS */                          02S0120
*   DCL                                                         02S0120
*     GTSTR0   FIXED(31) REG(0),/*GTMN PARM REG*/               02S0120
*     GTSTR1   FIXED(31) REG(1),/*GTMN PARM REG*/               02S0120
*     GTSTR2   PTR(31)   REG(2),/*GTMN PARM REG*/               02S0120
*     GTSTR15  FIXED(31) REG(15),/*GTMN RET COD*/               02S0120
*     GTSTRTCD FIXED(31); /* GETMAIN RTN CODE  */               02S0120
*   DCL                                                         02S0121
*     GTSTR3   FIXED(31) REG(3),/* CLEARING REG*/               02S0121
*     GTSTR4   FIXED(31) REG(4),/* CLEARING REG*/               02S0121
*     GTSTR5   FIXED(31) REG(5),/* CLEARING REG*/               02S0121
*     GTSTR6   FIXED(31) REG(6),/* CLEARING REG*/               02S0121
*     GTSTR7   FIXED(31) REG(7),/* CLEARING REG*/               02S0121
*     GTSTR8   FIXED(31) REG(8),/* CLEARING REG*/               02S0121
*     GTSTZERO FIXED(31) INIT(0),                               02S0121
*     GTST253  FIXED(31) CONSTANT(253);                         02S0121
*   RFY (GTSTR0,GTSTR1,GTSTR2) RSTD; /*SAVE RGS*/               02S0122
*   GTSTR0 = PRLSTLEN; /* SET AMOUNT*/                          02S0123
         LR    GTSTR0,@08                                       02S0123
*   GTSTR2 = SPID; /* SET SUBPOOL*/                             02S0124
         LA    GTSTR2,230                                       02S0124
*   GENERATE CODE;                                              02S0125
     GETMAIN RC,LV=(0),SP=(2)  DO GETMAIN
*   GTSTRTCD = GTSTR15; /* SAVE GETMAIN RET COD*/               02S0126
         LR    GTSTRTCD,GTSTR15                                 02S0126
*   PRLSTPTR = GTSTR1; /* ADDR OF GOTN STORG*/                  02S0127
         ST    GTSTR1,PRLSTPTR                                  02S0127
*   RFY (GTSTR0,GTSTR1,GTSTR2) UNRSTD; /*RLSE */                02S0128
*   IF GTSTRTCD = 0 THEN /*GETMAIN SUCCESSFUL? */               02S0129
         LTR   GTSTRTCD,GTSTRTCD                                02S0129
         BNZ   @RF00129                                         02S0129
*     DO;               /* YES, CLEAR STORAGE.*/                02S0130
*       RFY (GTSTR2,GTSTR4,GTSTR5,GTSTR6,                       02S0131
*                          GTSTR7,GTSTR8) RSTD;                 02S0131
*       GTSTR4 = PRLSTPTR; /*STORG PTR*/                        02S0132
         L     @03,PRLSTPTR                                     02S0132
         LR    GTSTR4,@03                                       02S0132
*       GTSTR5 = PRLSTLEN; /*STORG AMNT*/                       02S0133
         L     GTSTR5,PRLSTLEN                                  02S0133
*       GTSTR7 = '00000000'X; /*PAD CHAR & LNTH*/               02S0134
         SLR   GTSTR7,GTSTR7                                    02S0134
*       IF SPID = GTST253 THEN                                  02S0135
         B     @RF00135                                         02S0135
*      GEN                                                      02S0136
*   (MODESET KEYADDR=GTSTZERO,SAVEKEY=(2),WORKREG=8);           02S0136
         MODESET KEYADDR=GTSTZERO,SAVEKEY=(2),WORKREG=8
*       GEN REFS(GTSTR4,GTSTR6);                                02S0137
@RF00135 DS    0H                                               02S0137
          MVCL  GTSTR4,GTSTR6 CLEAR GOTTEN STORG
*       IF SPID = GTST253 THEN                                  02S0138
         B     @RF00138                                         02S0138
*         GEN (MODESET KEYADDR=(2));                            02S0139
         MODESET KEYADDR=(2)
*       RFY (GTSTR2,GTSTR4,GTSTR5,GTSTR6,                       02S0140
*                          GTSTR7,GTSTR8) UNRSTD;               02S0140
@RF00138 DS    0H                                               02S0141
*     END;       /* STORAGE CLEARING COMPLETED */               02S0141
*   ELSE                                                        02S0142
*   PRLSTPTR= 0;   /* CLEAR RETURN ADDR. */                     02S0142
         B     @RC00129                                         02S0142
@RF00129 SLR   @08,@08                                          02S0142
         ST    @08,PRLSTPTR                                     02S0142
*   GTSTR15 = GTSTRTCD;    /* GTMN RETURN CODE */               02S0143
@RC00129 LR    GTSTR15,GTSTRTCD                                 02S0143
*                                      /* RESTORE LIST PRINT STATUS  */
*   /*GET CORE FOR PRLIST         */                            02S0144
*ERRCODE2 = REG15;                     /*SAVE RETURN CODE            */
         LR    ERRCODE2,REG15                                   02S0144
*IF ERRCODE2 = ZERO THEN               /*IF CORE GOTTEN CONTINUE     */
         LTR   ERRCODE2,ERRCODE2                                02S0145
         BNZ   @RF00145                                         02S0145
*  DO;                                                          02S0146
*    DO LUTENTNO = 1 BY 1 WHILE(IOSLUT(LUTENTNO)^=IOSLTEND);    02S0147
         LA    LUTENTNO,1                                       02S0147
         B     @DE00147                                         02S0147
@DL00147 DS    0H                                               02S0148
*      IF IOSLUT(LUTENTNO)^=NOUCBADR THEN /*NO MATCH IN LOOK UP TBL  */
         LR    @10,LUTENTNO                                     02S0148
         ALR   @10,@10                                          02S0148
         L     @08,ALCWAPTR                                     02S0148
         L     @08,IOSLUTP(,@08)                                02S0148
         ALR   @08,@10                                          02S0148
         BCTR  @08,0                                            02S0148
         BCTR  @08,0                                            02S0148
         SLR   @10,@10                                          02S0148
         ICM   @10,3,IOSLUT(@08)                                02S0148
         LTR   @10,@10                                          02S0148
         BZ    @RF00148                                         02S0148
*        DO;                                                    02S0149
*          UCBPTR = IOSLUT(LUTENTNO);                           02S0150
         LR    UCBPTR,@10                                       02S0150
*          IF UCB3DACC = ON            /*IS DEVICE DIRECT ACCESS     */
*            & UCBCHGS = OFF           /*NOT PENDING OFFLINE         */
*            & UCBUNLD = OFF           /*NOT PENDING UNLOAD          */
*            &UCBNOTRD = OFF           /*IS DEVICE READY             */
*            &UCBONLI  = ON            /*IS DEVICE ONLINE            */
*            &UCBNALOC=OFF             /*DEVICE IN USE BY OLTEP      */
*            &(UCBRESV=ON              /*RESERVED OR         @ZA18146*/
*            ×(UCBPRES = ON            /*PERMANENTLY RESIDENT@ZA18146*/
*            & UCBVOLI ^= NOVOLSER)) THEN /*WITH A VOLSER    @ZA18146*/
         TM    UCB3DACC(UCBPTR),B'00100000'                     02S0151
         BNO   @RF00151                                         02S0151
         TM    UCBCHGS(UCBPTR),B'01010000'                      02S0151
         BNZ   @RF00151                                         02S0151
         TM    UCBNOTRD(UCBPTR),B'01000000'                     02S0151
         BNZ   @RF00151                                         02S0151
         TM    UCBONLI(UCBPTR),B'10000000'                      02S0151
         BNO   @RF00151                                         02S0151
         TM    UCBNALOC(UCBPTR),B'00000100'                     02S0151
         BNZ   @RF00151                                         02S0151
         TM    UCBRESV(UCBPTR),B'00100000'                      02S0151
         BO    @RT00151                                         02S0151
         TM    UCBPRES(UCBPTR),B'00000100'                      02S0151
         BNO   @RF00151                                         02S0151
         CLC   UCBVOLI(6,UCBPTR),NOVOLSER                       02S0151
         BE    @RF00151                                         02S0151
@RT00151 DS    0H                                               02S0152
*            DO;                                                02S0152
*              PRLST(SLOTNO) = UCBPTR; /*IF SO MOVE ADDRESS TO PRLIST*/
         LR    @10,SLOTNO                                       02S0153
         ALR   @10,@10                                          02S0153
         L     @08,PRLSTPTR                                     02S0153
         BCTR  @08,0                                            02S0153
