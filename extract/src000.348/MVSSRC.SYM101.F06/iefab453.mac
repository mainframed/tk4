*                         HWEDT=ON; /* INDICATE FOUND IN EDT         */
         OI    HWEDT(CTRLPREG),B'10000000'                      02S0117
*                         HWEDTLUV=SIOTDEVT;/* COPY LOOK-UP-VALUE    */
         L     @10,HWUCLPTR(,DATAPREG)                          02S0118
         MVC   HWEDTLUV(4,@10),SIOTDEVT(SIOTPTR)                02S0118
*                       END;                                    02S0119
*                     ELSE          /* UCB DEVICE ENTRY              */
*                       DO;                                     02S0120
         B     @RC00115                                         02S0120
