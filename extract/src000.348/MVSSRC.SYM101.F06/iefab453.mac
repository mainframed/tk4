         TITLE ' IEFAB453 - DD PREPARATION                             *
                        '
IEFAB453 CSECT ,                                                01S0001
@MAINENT DS    0H                                               01S0001
         USING *,@15                                            01S0001
         B     @PROLOG                                          01S0001
         DC    AL1(16)                                          01S0001
         DC    C'IEFAB453  74.099'                              01S0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                  01S0001
         BALR  @12,0                                            01S0001
@PSTART  DS    0H                                               01S0001
         USING @PSTART,@12                                      01S0001
         L     @00,@SIZDATD                                     01S0001
         MODESET EXTKEY=SCHED,SAVEKEY=(2)
         GSPACE  R,LV=(0)
         LR    @11,@01                                          01S0001
         USING @DATD,@11                                        01S0001
         ST    @13,@SA00001+4                                   01S0001
         STC   @02,@OLDKEY                                      01S0001
         MODESET KEYADDR=(2)
         LM    @00,@02,20(@13)                                  01S0001
         ST    @11,8(,@13)                                      01S0001
         LR    @13,@11                                          01S0001
         MODESET EXTKEY=ZERO
         MVC   @PC00001(16),0(@01)                              01S0001
         MODESET EXTKEY=SCHED
*                                                                    */
*   CTRLPREG=HWCNTRLP;              /* REMAP HSKP CONTROL AND DATA   */
         L     @10,@PC00001                                     01S0025
         L     CTRLPREG,HWCNTRLP(,@10)                          01S0025
*   DATAPREG=HWDATAP;               /* PTRS FOR PROCESSING SPEED     */
         L     @10,@PC00001+4                                   01S0026
         L     DATAPREG,HWDATAP(,@10)                           01S0026
*   RFY                                                         01S0027
*     HWCNTRL BASED(CTRLPREG);                                  01S0027
*   RFY                                                         01S0028
*     HWDATA BASED(DATAPREG);                                   01S0028
*   SIOTPTR=PREPSIOT;               /* MAP THE CURRENT SIOT          */
         L     @10,@PC00001+12                                  01S0029
         L     SIOTPTR,PREPSIOT(,@10)                           01S0029
*   JFCBPTR=SJFCBPTR;               /* GET JFCB POINTER FROM SIOT    */
         L     JFCBPTR,SJFCBPTR(,SIOTPTR)                       01S0030
*   HWCJFCBP=JFCBPTR;               /* PLACE COPY IN WORKAREA        */
         ST    JFCBPTR,HWCJFCBP(,DATAPREG)                      01S0031
*   HWEDT=NOTCOMP;                  /* RESET FOUND-IN-EDT SWITCH     */
         NI    HWEDT(CTRLPREG),B'01111111'                      01S0032
*   HWUCB=NOTCOMP;                  /* RESET FOUND-IN-UCB SWITCH     */
*   HWRESIOT=NOTCOMP;               /* RESET FOUND-IN-PDI SCAN       */
*   HWDSNCAT=NOTCOMP;               /* RESET FOUND-IN-CATALOG   01S0035
*                                                                    */
         NI    HWUCB(CTRLPREG),B'11110010'                      01S0035
*                                                               02S0036
*/********************************************************************/
*/*                                                                  */
*/* SEGMENT NAME - FASTPATH                                          */
*/*                                                                  */
*/* DESCRIPTIVE NAME - JFCB HOUSEKEEPING FASTPATH DETERMINATION      */
*/*                                                                  */
*/* FUNCTION - DETERMINE IF TERM=TS OR QNAME=  WAS SPECIFIED ON THE  */
*/*            DD. IF YES, INDICATE NO MORE PROCESSING IS REQUIRED   */
*/*            FOR THE DD. OTHERWISE, DETERMINE IF THE SIOT IS FOR   */
*/*            A DD DUMMY, OR SUB-SYSTEM DATA SET, AND INDICATE THAT */
*/*            NO DSNAME RESOLUTION IS REQUIRED FOR THIS DD. A SUB-  */
*/*            SYSTEM DATA SET IS HASP... SYSIN,SYSOUT,ETC.  IF A    */
*/*            DD DUMMY HAS NO DATA SET NAME, MAKE IT 'NULLFILE'.    */
*/*                                                                  */
*/* ENTRY - N/A                                                      */
*/*   PURPOSE - TO DETERMINE IF ANY OR ALL PROCESSING MAY BE         */
*/*             ELIMINATED FOR THE SPECIFIED DD.                     */
*/*                                                                  */
*/*   INPUT DATA - THE CURRENT SIOT                                  */
*/*                THE HOUSEKEEPING WORK AREA CONTROLS               */
*/*                                                                  */
*/* EXITS -                                                          */
*/*   CONDITION - AFTER DETERMINING IF ANY OR ALL PROCESSING MAY BE  */
*/*               ELIMINATED FOR THE DD                              */
*/*                                                                  */
*/*   OUTPUT DATA - BITS ARE SET IN THE HOUSEKEEPING CONTROL AREA TO */
*/*                 INDICATE IF ANY(HWDSNRQD) OR ALL(HWDDONE)        */
*/*                 PROCESSING MAY BE ELIMINATED FOR THE SPECIFIED DD*/
*/*                                                                  */
*/*   RETURN CODE - ALWAYS ZERO                                      */
*/*                                                                  */
*/********************************************************************/
*                                                               02S0036
*   HWDDDONE=NOTCOMP;               /* RESET DD-DONE SWITCH          */
         NI    HWDDDONE(CTRLPREG),B'11101111'                   02S0036
*   HWDSNRQD=REQUIRED;              /* RESET DSN-MAY-BE-RQD SW.      */
         OI    HWDSNRQD(CTRLPREG),B'00100000'                   02S0037
*   IF SIOTTERM=ON×                 /* IF TSO TERMINAL(DD TERM=TS)   */
*       SIOTQNAM=ON THEN            /* OR QNAME= ON DD STATEMENT     */
         TM    SIOTTERM(SIOTPTR),B'01000001'                    02S0038
         BZ    @RF00038                                         02S0038
*     HWDDDONE=COMPLETE;            /* THEN THIS DD COMPLETE         */
         OI    HWDDDONE(CTRLPREG),B'00010000'                   02S0039
*/*  DETERMINE IF DSN RESOLUTION MAY BE ELIMINATED                   */
*                                                               02S0040
*   ELSE                                                        02S0040
*     IF SCTDUMMY=ON×               /* IF DD DUMMY OR SUB-SYSTEM     */
*         SIOTSSDS=ON THEN          /* DATA-SET,(HASP-SYSIN,ETC).
*                                                            @YM08189*/
         B     @RC00038                                         02S0040
@RF00038 TM    SCTDUMMY(SIOTPTR),B'10000000'                    02S0040
         BO    @RT00040                                         02S0040
         TM    SIOTSSDS(SIOTPTR),B'10000000'                    02S0040
         BNO   @RF00040                                         02S0040
@RT00040 DS    0H                                               02S0041
*       DO;                         /* THEN NO DSN RESOLUTION IS     */
*         HWDSNRQD=NODSNRQD;        /* REQUIRED                      */
         NI    HWDSNRQD(CTRLPREG),B'11011111'                   02S0042
*         IF SCTDUMMY=ON&           /* DD DUMMY                      */
*             JFCBDSNM=BLANK THEN   /* NO DATA SET NAME IN JFCB      */
         TM    SCTDUMMY(SIOTPTR),B'10000000'                    02S0043
         BNO   @RF00043                                         02S0043
         CLC   JFCBDSNM(8,JFCBPTR),@CC00052                     02S0043
         BNE   @RF00043                                         02S0043
*           JFCBDSNM=NULLFILE;      /* SUBALLOCATION DSNAME          */
         MVI   JFCBDSNM+8(JFCBPTR),C' '                         02S0044
         MVC   JFCBDSNM+9(35,JFCBPTR),JFCBDSNM+8(JFCBPTR)       02S0044
         MVC   JFCBDSNM(8,JFCBPTR),@CC00054                     02S0044
*       END;                                                    02S0045
@RF00043 DS    0H                                               02S0046
*   IF HWDDDONE^=COMPLETE THEN      /* DD PROCESSING NOT COMPLETE    */
@RF00040 DS    0H                                               01S0046
@RC00038 TM    HWDDDONE(CTRLPREG),B'00010000'                   01S0046
         BO    @RF00046                                         01S0046
*     IF SCTGOSTP=ON&SCTDDNAM=PGMFETCH THEN/* NEED PROGRAM FETCH
*                                                            @YM07284*/
         L     @10,HWSCTP(,DATAPREG)                            01S0047
         TM    SCTGOSTP(@10),B'10000000'                        01S0047
         BNO   @RF00047                                         01S0047
         CLC   SCTDDNAM(8,SIOTPTR),@CC00070                     01S0047
         BNE   @RF00047                                         01S0047
*       DO;                                                     01S0048
*                                                               02S0049
*/********************************************************************/
*/*                                                                  */
*/* SEGMENT NAME - FETCHLIB                                          */
*/*                                                                  */
*/* DESCRIPTIVE NAME - PROGRAM FETCH PROCESSING                      */
*/*                                                                  */
*/* FUNCTION - RETREIVE VOLUME AND UNIT INFORMATION FOR PROGRAM      */
*/*            FETCH DCB   -  DDNAME = 'PGM=*.DD'                    */
*/*                                                                  */
*/*   OPERATION- THE SIOT AND JFCB REPRESENTING THE PROGRAM ARE READ.*/
*/*            REFERENCED SIOT NOT ALLOCATED (STEP NOT RUN), OR      */
*/*            INVALID UNIT TYPE (NOT DIRECT ACCESS OR VIRTUAL       */
*/*            ACCESS), OR THE DATASET IS A SUBSYSTEM (SYSIN ETC.),  */
*/*            CAUSES TERMINATION WITH RETURN/REASON CODES IN THE    */
*/*            HSKP RETURN INFORMATION AREA.                         */
*/*            THE PROGRAM NAME IN THE SCT IS OBTAINED FROM THE      */
*/*            ELEMENT NUMBER/MEMBER NAME FIELD OF THE READ JFCB,    */
*/*            WHICH ALSO YIELDS THE VOLUME INFORMATION.             */
*/*            VARIOUS FIELDS IN THE CURRENT SIOT ARE RESET OR       */
*/*            PARTIALLY RESET FROM THEIR ORIGINAL VALUE AS          */
*/*            INITIALIZED BY THE INTERPRETER FROM THE OLD TABLES.   */
*/*            'HWDDDONE' THEN INDICATES  'DD PROCESSING COMPLETE'.  */
*/*                                                                  */
*/* ENTRY - DDNAME IS 'PGM=*.DD'                                     */
*/*                                                                  */
*/*   PURPOSE - PREPARE THE SIOT AND JFCB FOR DCB CREATION FOR       */
*/*             PROGRAM FETCH.                                       */
*/*                                                                  */
*/*   INPUT DATA - SIOT, SCT, JFCB                                   */
*/*                                                                  */
*/* EXIT - IN LINE                                                   */
*/*                                                                  */
*/*   CONDITION - ALL                                                */
*/*                                                                  */
*/*   OUTPUT DATA - COMPLETED SIOT, SCT AND JFCB IF SUCCESSFUL       */
*/*             -   REASON CODE 7 IF ERROR DETECTED:                 */
*/*                 REFERENCED STEP NOT RUN                          */
*/*                                                                  */
*/*   RETURN CODES - SUCCESS  0                                      */
*/*                - FAILURE  4                                      */
*/*                                                                  */
*/********************************************************************/
*                                                               02S0049
*         SWACTLS=0;                /* READ THE FETCHLIB SIOT FROM   */
         SLR   @10,@10                                          02S0049
         STH   @10,SWACTLS                                      02S0049
*         SWREAD=ON;                /* SWA TO CHECK UNIT TYPE        */
         OI    SWREAD,B'00100000'                               02S0050
*         IF HWLEPARP=0 THEN        /* USE OLD EPA, IF ONE EXISTS    */
         C     @10,HWLEPARP(,DATAPREG)                          02S0051
         BNE   @RF00051                                         02S0051
*           SWNEW=ON;               /* OR HAVE A NEW ONE CREATED     */
         OI    SWNEW,B'00001000'                                02S0052
*         ELSE                                                  02S0053
*           SWMOD=ON;                                           02S0053
         B     @RC00051                                         02S0053
@RF00051 OI    SWMOD,B'00000010'                                02S0053
*         CALL IEFAB4F7(SWACTLS,HWLEPARP,0,ADDR(SCTGOTTR),ADDR(PTABLP),
*             0,1);                 /* SWA INTERFACE                 */
@RC00051 LA    @10,SWACTLS                                      02S0054
         ST    @10,@AL00001                                     02S0054
         LA    @10,HWLEPARP(,DATAPREG)                          02S0054
         ST    @10,@AL00001+4                                   02S0054
         LA    @10,@CF00493                                     02S0054
         ST    @10,@AL00001+8                                   02S0054
         L     @10,HWSCTP(,DATAPREG)                            02S0054
         LA    @10,SCTGOTTR(,@10)                               02S0054
         ST    @10,@TF00001                                     02S0054
         LA    @10,@TF00001                                     02S0054
         ST    @10,@AL00001+12                                  02S0054
         LA    @10,PTABLP                                       02S0054
         ST    @10,@TF00002                                     02S0054
         LA    @10,@TF00002                                     02S0054
         ST    @10,@AL00001+16                                  02S0054
         LA    @10,@CF00493                                     02S0054
         ST    @10,@AL00001+20                                  02S0054
         LA    @10,@CF00043                                     02S0054
         ST    @10,@AL00001+24                                  02S0054
         L     @15,@CV00083                                     02S0054
         LA    @01,@AL00001                                     02S0054
         BALR  @14,@15                                          02S0054
*         IF PTABLP->SIOTALCD=ON&   /* REFERENCED DD MUST HAVE BEEN  */
*             (PTABLP->SIO3DACC=ON× /* ALLOCATED, ON DIRECT ACCESS   */
*             PTABLP->SIOVAMDS=ON)& /* OR VAM AND NOT A SUBSYSTEM    */
*             PTABLP->SIOTSSDS^=ON THEN/* DATASET (SYSIN ETC.)       */
         L     @10,PTABLP                                       02S0055
         TM    SIOTALCD(@10),B'00000010'                        02S0055
         BNO   @RF00055                                         02S0055
         TM    SIO3DACC(@10),B'00100000'                        02S0055
         BO    @GL00004                                         02S0055
         TM    SIOVAMDS(@10),B'00100000'                        02S0055
         BNO   @RF00055                                         02S0055
@GL00004 L     @10,PTABLP                                       02S0055
         TM    SIOTSSDS(@10),B'10000000'                        02S0055
         BO    @RF00055                                         02S0055
*           DO;                     /* TO BE A VALID PROGRAM SOURCE  */
*             IF PTABLP->SIOVAMDS=ON THEN/* IF THE REFERENCED DS     */
         TM    SIOVAMDS(@10),B'00100000'                        02S0057
         BNO   @RF00057                                         02S0057
*               DO;                 /* IS VIRTUAL ACCESS, COPY THE   */
*                 SCTUTYPE=PTABLP->SCTUTYPE;/* UNIT INFORMATION      */
         MVC   SCTUTYPE(8,SIOTPTR),SCTUTYPE(@10)                02S0059
*                 SIOVAMDS=ON;      /* AND INDICATE 'VAM' IN         */
         OI    SIOVAMDS(SIOTPTR),B'00100000'                    02S0060
*                 JFCVRDS=ON;       /* THE SIOT AND IN THE JFCB      */
         OI    JFCVRDS(JFCBPTR),B'00000100'                     02S0061
*               END;                                            02S0062
*             ELSE                  /* GET UNIT INFORMATION FOR      */
*               DO;                 /* NON-VAM DATA SET.             */
         B     @RC00057                                         02S0063
@RF00057 DS    0H                                               02S0064
*                 SCTUTYPE=SCTUTYPE&&SCTUTYPE;/* CLEAR UNIT FIELD    */
         XC    SCTUTYPE(8,SIOTPTR),SCTUTYPE(SIOTPTR)            02S0064
*                 SIOTDEVT=PTABLP->SIOTDEVT;/* DEVICE TYPE           */
         L     @10,PTABLP                                       02S0065
         MVC   SIOTDEVT(4,SIOTPTR),SIOTDEVT(@10)                02S0065
*               END;                                            02S0066
*             SWNEW=OFF;            /* READ PREVIOUS JFCB USING      */
@RC00057 DS    0H                                               02S0068
*             SWMOD=ON;             /* SAME EPA AS FOR SIOT          */
         OI    SWMOD,B'00000010'                                02S0068
         NI    SWNEW,B'11110111'                                02S0068
*             CALL IEFAB4F7(SWACTLS,HWLEPARP,0,ADDR(PTABLP->SCTPJFCB),
*                 ADDR(PTABLP),0,1);                            02S0069
         LA    @10,SWACTLS                                      02S0069
         ST    @10,@AL00001                                     02S0069
         LA    @10,HWLEPARP(,DATAPREG)                          02S0069
         ST    @10,@AL00001+4                                   02S0069
         LA    @10,@CF00493                                     02S0069
         ST    @10,@AL00001+8                                   02S0069
         L     @10,PTABLP                                       02S0069
         LA    @10,SCTPJFCB(,@10)                               02S0069
         ST    @10,@TF00001                                     02S0069
         LA    @10,@TF00001                                     02S0069
         ST    @10,@AL00001+12                                  02S0069
         LA    @10,PTABLP                                       02S0069
         ST    @10,@TF00002                                     02S0069
         LA    @10,@TF00002                                     02S0069
         ST    @10,@AL00001+16                                  02S0069
         LA    @10,@CF00493                                     02S0069
         ST    @10,@AL00001+20                                  02S0069
         LA    @10,@CF00043                                     02S0069
         ST    @10,@AL00001+24                                  02S0069
         L     @15,@CV00083                                     02S0069
         LA    @01,@AL00001                                     02S0069
         BALR  @14,@15                                          02S0069
*             SCTPGMNM=PTABLP->JFCBELNM;/* PGM NAME FROM PREVIOUS
*                                      JFCB                          */
         L     @10,HWSCTP(,DATAPREG)                            02S0070
         L     @07,PTABLP                                       02S0070
         MVC   SCTPGMNM(8,@10),JFCBELNM(@07)                    02S0070
*             JFCBVOLS(1:6)=PTABLP->JFCBVOLS;/* FIRST (ONLY) VOL
*                                                              SERIAL*/
         MVC   JFCBVOLS(6,JFCBPTR),JFCBVOLS(@07)                02S0071
*             SCTUNAFF=OFF;         /* RESET UNIT AFFINITY,IF ANY,   */
         NI    SCTUNAFF(SIOTPTR),B'11111101'                    02S0072
*             SIOTUNAF=0;                                       02S0073
         SLR   @10,@10                                          02S0073
         STH   @10,SIOTUNAF(,SIOTPTR)                           02S0073
*             SIOTVRSB=0;           /* VOLREF,                       */
         STCM  @10,7,SIOTVRSB(SIOTPTR)                          02S0074
*             SIOTREFN=0;           /* INTRA=STEP VOL=REF,           */
         STH   @10,SIOTREFN(,SIOTPTR)                           02S0075
*             SCTSPOOL=0;           /* AND THE POOL DD NUMBER.       */
         MVI   SCTSPOOL(SIOTPTR),X'00'                          02S0076
*             SCTVOLCT=1;           /* ONE VOLUME DATA SET           */
         MVI   SCTVOLCT(SIOTPTR),X'01'                          02S0077
*             SIOTALTD=0;           /* ALT DISP NOT FOR THIS STEP.   */
         MVI   SIOTALTD(SIOTPTR),X'00'                          02S0078
*             SCTSDISP=SCTSDISP&RPRVT;/* RETAIN,PRVT UNCHANGED.      */
         NI    SCTSDISP(SIOTPTR),X'60'                          02S0079
*             SIOTKEEP=ON;          /* FORCE DISP TO KEEP.           */
         OI    SIOTKEEP(SIOTPTR),B'00001000'                    02S0080
*             SCTSBYT1=SCTSBYT1&DUMY;/* DUMMY IS UNCHANGED           */
         NI    SCTSBYT1(SIOTPTR),X'80'                          02S0081
*             SCTSBYT2=SCTSBYT2&UNLBL;/* UNLABELED UNCHANGED         */
         NI    SCTSBYT2(SIOTPTR),X'08'                          02S0082
*             SCTSBYT3=0;           /* CLEAR STATUS FIELD AND        */
         MVI   SCTSBYT3(SIOTPTR),X'00'                          02S0083
*             SCTSOLD=ON;           /* MARK DATA SET 'OLD'           */
         OI    SCTSOLD(SIOTPTR),B'00000001'                     02S0084
*             SCTSBYT4=0;           /* 'BYTE 4' ZEROED.              */
         MVI   SCTSBYT4(SIOTPTR),X'00'                          02S0085
*             HWDDDONE=ON;          /* INDICATE PROCESSING COMPLETE  */
         OI    HWDDDONE(CTRLPREG),B'00010000'                   02S0086
*           END;                                                02S0087
*         ELSE                      /* SIOT WAS NOT ALLOCATED OR     */
*           DO;                     /* DATASET TYPE IS INVALID       */
         B     @RC00055                                         02S0088
@RF00055 DS    0H                                               02S0089
*             HWRTCD=DDERR;         /* RETURN CODE - 4               */
         L     @10,@PC00001+8                                   02S0089
         L     @10,HWRTNP(,@10)                                 02S0089
         MVC   HWRTCD(2,@10),@CH00068                           02S0089
*             HWRSNCD=STPNORUN;     /* REASON CODE - 7               */
         MVC   HWRSNCD(2,@10),@CH00066                          02S0090
*           END;                                                02S0091
*       END;                                                    01S0092
@RC00055 DS    0H                                               01S0093
*   IF HWDDDONE^=COMPLETE&          /* UNIT PROCESSING UNLESS DONE   */
*       HWRTCD=NOERROR&             /* AN ERROR HAS BEEN FOUND OR    */
*       SIOTSSDS^=ON THEN           /* A SUB-SYSTEM DATA SET         */
@RF00047 DS    0H                                               01S0093
@RF00046 TM    HWDDDONE(CTRLPREG),B'00010000'                   01S0093
         BO    @RF00093                                         01S0093
         L     @10,@PC00001+8                                   01S0093
         L     @07,HWRTNP(,@10)                                 01S0093
         ICM   @04,3,HWRTCD(@07)                                01S0093
         BNZ   @RF00093                                         01S0093
         TM    SIOTSSDS(SIOTPTR),B'10000000'                    01S0093
         BO    @RF00093                                         01S0093
*     IF SCTUTYPE^=BLANK&           /* UNIT NAME SPECIFIED, AND      */
*         SIOUCNVT^=0&              /* NOT DYNAMICALLY ALLOCATED     */
*         SCTUNAFF^=ON THEN         /* NOT A UNIT AFFINITY REQUEST   */
         CLC   SCTUTYPE(8,SIOTPTR),@CC00052                     01S0094
         BE    @RF00094                                         01S0094
         CLI   SIOUCNVT(SIOTPTR),0                              01S0094
         BE    @RF00094                                         01S0094
         TM    SCTUNAFF(SIOTPTR),B'00000010'                    01S0094
         BO    @RF00094                                         01S0094
*       CALL IEFAB470(HWCNTRLP,HWDATAP,HWRTNP,HWIOSLTP,SCTUTYPE);/*
*                                      UNIT NAME CONVERSION          */
         L     @07,@PC00001                                     01S0095
         ST    @07,@AL00001                                     01S0095
         L     @07,@PC00001+4                                   01S0095
         ST    @07,@AL00001+4                                   01S0095
         ST    @10,@AL00001+8                                   01S0095
         LA    @10,HWIOSLTP(,DATAPREG)                          01S0095
         ST    @10,@AL00001+12                                  01S0095
         LA    @10,SCTUTYPE(,SIOTPTR)                           01S0095
         ST    @10,@AL00001+16                                  01S0095
         L     @15,@CV00080                                     01S0095
         LA    @01,@AL00001                                     01S0095
         BALR  @14,@15                                          01S0095
*     ELSE                                                      01S0096
*                                                               02S0096
*/********************************************************************/
*/*                                                                  */
*/* SEGMENT NAME - SRCHSIOT                                          */
*/*                                                                  */
*/* DESCRIPTIVE NAME - SEARCH SIOTS FOR UNIT TYPE INFORMATION        */
*/*                                                                  */
*/* FUNCTION - ATTEMPTS TO OBTAIN UNIT INFORMATION FROM THE CURRENT  */
*/*            SIOT, IF THE UNIT PARAMETER WAS PREVIOUSLY            */
*/*            CONVERTED FOR NON VIO DATA SETS (VIO DATA SETS        */
*/*            REQUIRE UCB PTR REMAIN INTACT), OR SEARCHES   @YM0XXXX*/
*/*            THE SIOT CHAIN FOR A UNIT AFFINITY REQUEST            */
*/*            BY COMPARING THE 'UNIT AFFINITY TO DD' NUMBER TO THE  */
*/*            DD NUMBER (SCTDDINO) OF THE SIOTS IN THE SIOT CHAIN.  */
*/*            UPON LOCATING THE PROPER SIOT, IF THE UNIT TYPE FIELD */
*/*            WAS PREVIOUSLY CONVERTED (SCTUTYPE BYTE 4 = 0),       */
*/*            THIS FIELD IS COPIED INTO THE UNIT CONVERTION LIST OF */
*/*            THE HSKP WORKAREA.  IF A VAM DATA SET WAS INDICATED,  */
*/*            THAT WILL BE NOTED ALSO IN THE WORKAREA. UNIT NAME    */
*/*            CONVERSION WILL BE CALLED FOR SUBSYSTEM DATA SETS TO  */
*/*            CONVERT THE SYSTEM NAME 'SYSALLDA'.                   */
*/*                                                                  */
*/* ENTRY - INLINE SEGMENT                                           */
*/*                                                                  */
*/* EXITS - COMPLETION OF SIOT SEARCH AND UNIT TYPE VALIDITY CHECK   */
*/*                                                                  */
*/* OUTPUT DATA - UNIT CONVERTION LIST COMPLETED AND WHETHER THE     */
*/*               UNIT TYPE WAS ORIGINALLY CONVERTED FROM THE EDT    */
*/*               OR THE UCB LIST AND VAM ELIGIBILITY.               */
*/*                                                                  */
*/* RETURN CODE - ALWAYS ZERO                                        */
*/* * * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * */
*                                                               02S0096
*       IF SCTUNAFF=ON×             /* UNIT AFFINITY REQUEST OR      */
*           (SIOUCNVT=0&SIOVAMDS=OFF) THEN/* NON-VIO CONVERTED  02S0096
*                                                            @YM07413*/
         B     @RC00094                                         02S0096
@RF00094 TM    SCTUNAFF(SIOTPTR),B'00000010'                    02S0096
         BO    @RT00096                                         02S0096
         CLI   SIOUCNVT(SIOTPTR),0                              02S0096
         BNE   @RF00096                                         02S0096
         TM    SIOVAMDS(SIOTPTR),B'00100000'                    02S0096
         BNZ   @RF00096                                         02S0096
@RT00096 DS    0H                                               02S0097
*         DO;                                                   02S0097
*           IF SIOUCNVT=0 THEN      /* PREVIOUSLY CONVERTED          */
         CLI   SIOUCNVT(SIOTPTR),0                              02S0098
         BNE   @RF00098                                         02S0098
*             PRIDDNUM=SCTDDINO;    /* THIS IS CORRECT SIOT          */
         LH    PRIDDNUM,SCTDDINO(,SIOTPTR)                      02S0099
*           ELSE                                                02S0100
*             DO;                                               02S0100
         B     @RC00098                                         02S0100
@RF00098 DS    0H                                               02S0101
*               PRIDDNUM=SIOTUNAF;  /* SAVE PRIMARY DD NUMBER        */
