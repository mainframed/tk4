*/********************************************************************/
*/*      TRACE ROUTINE                                               */
*/********************************************************************/
*                                                                  0077
*C12C0050:                                                         0077
*   ;                                                              0077
C12C0050 DS    0H                                                  0078
*   IF $MACID='47'&                 /* CHK FOR ROUTINE ZTR2473       */
*       TRBKSV(5)=4                 /* CALL WHEN NOT ZTR2473         */
*     THEN                          /* RUN IF FOUND ZTR2473          */
         CLC   $MACID(2,WAPTR),@CC00533                            0078
         BNE   @RF00078                                            0078
         L     @01,$R13PTR(,WAPTR)                                 0078
         CLC   TRBKSV+16(4,@01),@CF00032                           0078
         BE    @RT00078                                            0078
*     GOTO C12C0070;                /* DO NOT TRACE ZTR2473          */
*   $PRTONLY='1'B;                  /* SET PRINTER ONLY              */
@RF00078 OI    $PRTONLY(WAPTR),B'00001000'                         0080
*   CVD($TRTN#,TRWK);               /* CONVERT ACTIVE RTN TO DEC.    */
         SLR   @14,@14                                             0081
         IC    @14,$TRTN#(,WAPTR)                                  0081
         CVD   @14,@TS00001                                        0081
         MVC   TRWK(4),@TS00001+4                                  0081
*   UNPK($INBUFR(2:4),TRWK(3:4));   /* UNPACK IT FOR PRINTING        */
         UNPK  $INBUFR+1(3,WAPTR),TRWK+2(2)                        0082
*   $INBUFR(4:4)=$INBUFR(4:4)×'0';  /* GET PRINTABLE CHAR            */
         OI    $INBUFR+3(WAPTR),C'0'                               0083
*   $INBUFR(5:6)=$MACID;            /* MOVE IN MACRO CALL ID         */
         MVC   $INBUFR+4(2,WAPTR),$MACID(WAPTR)                    0084
*   TRWK=TRBKSV(4);                 /* GET RETURN ADDRESS            */
*                                                                  0085
         L     @14,$R13PTR(,WAPTR)                                 0085
         MVC   TRWK(4),TRBKSV+12(@14)                              0085
*   /*****************************************************************/
*   /*                                                               */
*   /* ITACONV FROM(TRWK)TO($INBUFR(7),8) INVOKED                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0086
*   DCL                                                            0086
*    (ITATCF,                                                      0086
*     ITATCT) CHAR(*) BASED;        /* GENERATE WORK VARABLES        */
*   DCL                                                            0087
*     ITATCTS FIXED;                /* TEMP SPOT FOR ALTERED BYTE    */
*   RFY                                                            0088
*     ITATCF BASED(ADDR(TRWK));     /* MAP VARIABLE OVER FROM        */
*   RFY                                                            0089
*     ITATCT BASED(ADDR($INBUFR(7)));/* MAP VARIBABLE OVER TO        */
*   ITATCTS=ITATCT(8+1:8+1);        /* SAVE BYTE PAST TO FIELD       */
         LA    @12,$INBUFR+6(,WAPTR)                               0090
         SLR   ITATCTS,ITATCTS                                     0090
         IC    ITATCTS,ITATCT+8(,@12)                              0090
*   UNPK(ITATCT(1:8+1),ITATCF(1:LENGTH(TRWK)+1));/* EXPAND HEX TO  0091
*                                      EBCDIC                        */
         LA    @07,TRWK                                            0091
         UNPK  ITATCT(9,@12),ITATCF(5,@07)                         0091
*   TR(ITATCT(1:8),$TRTBL);         /* CONVERT FA-FF TO C1-C6        */
         L     @01,RCAPTR(,WAPTR)                                  0092
         L     @01,$TRTBLA(,@01)                                   0092
         TR    ITATCT(8,@12),$TRTBL(@01)                           0092
*   ITATCT(8+1:8+1)=ITATCTS;        /* RESTORE BYTE PAST TO FIELD    */
         STC   ITATCTS,ITATCT+8(,@12)                              0093
*/* CONVERT TO EBCDIC                                                */
*                                                                  0094
*   TRWK=TRBKSV(5);                 /* GET RETURN CODE               */
         MVC   TRWK(4),TRBKSV+16(@14)                              0094
*   IF $MACID='47'                  /* WAS ROUTINE CALLED?           */
*     THEN                                                         0095
         CLC   $MACID(2,WAPTR),@CC00533                            0095
         BNE   @RF00095                                            0095
