*       $INBUFR(15:20)=' ';         /* CLEAR PART OF MSG             */
         MVI   $INBUFR+15(WAPTR),C' '                              0097
         MVC   $INBUFR+16(4,WAPTR),$INBUFR+15(WAPTR)               0097
         MVI   $INBUFR+14(WAPTR),C' '                              0097
*       GO TO C12C0060;             /* GO OUTPUT TRACE MSG           */
         B     C12C0060                                            0098
*     END;                                                         0099
*   $INBUFR(15:18)=',RC ';          /* MOVE IN RETURN CODE           */
*                                                                  0100
@RF00095 MVC   $INBUFR+14(4,WAPTR),@CC00550                        0100
