         LA    @12,RETCD                                           0104
         UNPK  ITATCT(3,@14),ITATCF(2,@12)                         0104
*   TR(ITATCT(1:2),$TRTBL);         /* CONVERT FA-FF TO C1-C6        */
         L     @01,RCAPTR(,WAPTR)                                  0105
         L     @01,$TRTBLA(,@01)                                   0105
         TR    ITATCT(2,@14),$TRTBL(@01)                           0105
*   ITATCT(2+1:2+1)=ITATCTS;        /* RESTORE BYTE PAST TO FIELD    */
         STC   ITATCTS,ITATCT+2(,@14)                              0106
