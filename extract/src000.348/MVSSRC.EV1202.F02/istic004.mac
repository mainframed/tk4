*   /*****************************************************************/
*                                                                  0158
*   R6=ADDR(ALPHABET);              /* IS THE CHAR. IN INBUFR+2      */
CECTST17 LA    R6,ALPHABET                                         0158
*   DO I=26 TO 1 BY-1;              /* ALPHABETIC. NO, SET ERROR     */
         LA    I,26                                                0159
@DL00159 DS    0H                                                  0160
*     IF INPUTBUF(3)=LETTER THEN    /* CODE 07 AND PRINT ERROR       */
         CLC   INPUTBUF+2(1,R5),LETTER(R6)                         0160
         BE    @RT00160                                            0160
*       GO TO CECTST19;             /* MESSAGE IFD112I. YES,         */
*     R6=R6+1;                      /* CONTINUE TO DECODE            */
         AL    R6,@CF00072                                         0162
*   END;                                                           0163
         BCTR  I,0                                                 0163
         LTR   I,I                                                 0163
         BP    @DL00159                                            0163
*   R5=R5+2;                        /* PT TO ERROR                   */
         AL    R5,@CF00139                                         0164
*   GO TO CECTST03;                                                0165
         B     CECTST03                                            0165
*CECTST19:                          /* IS SECTION NAME OF INCLUSIVE
