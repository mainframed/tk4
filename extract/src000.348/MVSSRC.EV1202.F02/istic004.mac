         TITLE 'ISTIC004-TEST DECODE'
ISTIC004 CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'ISTIC004  78.336'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @12,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@12                                         0001
*   R5=R13;                         /* CALLERS SAVE PTR              */
         LR    R5,R13                                              0055
*   R13=ADDR(SVEIC003);             /* PTR TO MY SAVEAREA            */
         LA    R13,SVEIC003(,WAPTR)                                0056
*   BACKSAVE(3)=R13;                /* INTO CALLERS SAVE             */
         ST    R13,BACKSAVE+8(,R5)                                 0057
*   SVEIC003(2)=R5;                 /* INTO MY SAVE                  */
         ST    R5,SVEIC003+4(,WAPTR)                               0058
*   RCAPTR=$RCA;                    /* GET POINTER TO RCA            */
         L     RCAPTR,$RCA(,WAPTR)                                 0059
*   $CUTESTB='0'B;                  /* TURN OFF CUTEST ACTIVE FLAG   */
         NI    $CUTESTB(RCAPTR),B'11101111'                        0060
*   $CUCNT=0;                       /* SET CUTEST COUNT TO ZERO      */
         SLR   @15,@15                                             0061
         STH   @15,$CUCNT(,WAPTR)                                  0061
*   R5=$SLASHAD(1)+1;               /* GET INBUFR POINTER            */
         LA    R5,1                                                0062
         AL    R5,$SLASHAD(,WAPTR)                                 0062
*   GO TO CECTST04;                 /* NO, CONTINUE TO DECODE        */
         B     CECTST04                                            0063
*CECTST03:                                                         0064
*                                                                  0064
*   /*****************************************************************/
*   /*                                                               */
*   /* PWTO MESSAGES IFD112I AND IFD155I                             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0064
*   R9=$SLASHAD(1)+1;               /* PT TO FIRST CHAR OF INPUT     */
CECTST03 LA    R9,1                                                0064
         AL    R9,$SLASHAD(,WAPTR)                                 0064
*   $INBUFR(1:8)=' ';               /* BLK OUT SUB FIELD             */
         MVI   $INBUFR+1(WAPTR),C' '                               0065
         MVC   $INBUFR+2(6,WAPTR),$INBUFR+1(WAPTR)                 0065
         MVI   $INBUFR(WAPTR),C' '                                 0065
*   DO I=8 TO 1 BY-1;               /* SCAN INPUT                    */
         LA    I,8                                                 0066
@DL00066 DS    0H                                                  0067
*     $INBUFR(I)=INPUTBUF(1);       /* MOVE ERROR IN MSG             */
         LR    @15,WAPTR                                           0067
         ALR   @15,I                                               0067
         MVC   $INBUFR-1(1,@15),INPUTBUF(R5)                       0067
*     IF R9=R5 THEN                 /* END OF FIELD ?                */
         CR    R9,R5                                               0068
         BE    @RT00068                                            0068
*       GO TO CECTST05;             /* GO WRITE MSG                  */
*     R5=R5-1;                      /* DECR INPUT PTR                */
         BCTR  R5,0                                                0070
*   END;                                                           0071
         BCTR  I,0                                                 0071
         LTR   I,I                                                 0071
         BP    @DL00066                                            0071
*CECTST05:                                                         0072
*                                                                  0072
*   /*****************************************************************/
*   /*                                                               */
*   /* ISSUE MESSAGE 112 TCEC MSGN(112)SUB(YES) INVOKED              */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0072
*   $TCECM='1'B;                    /* SET TCEC SW IN RCA            */
CECTST05 OI    $TCECM(WAPTR),B'00000001'                           0072
*   R1=ADDR($TCA09);                /* SET UP PARM LIST $TR          */
         LA    R1,$TCA09                                           0073
*   CALL #EXIT;                     /* CALL CECOM                    */
         L     @15,#EXITA(,WAPTR)                                  0074
         BALR  @14,@15                                             0074
*   DCL                                                            0075
*     1 $TCA09,                     /* TCEC PARM LIST                */
*      2 $TCB09 FIXED(32) INIT(112),/* MSG NUMBER                    */
*      2 $TCC09 FIXED(32) INIT(0),  /* INCT LEN                      */
*      2 $TCD09 BIT(8) INIT('10000000'B),/* FLAG                     */
*      2 $TCE09 CHAR(4) INIT('FFFFFFFF'X);/* MSSAGE MASK             */
*   GO TO CECTST07;                 /* SET TST ERROR FLAG ON         */
         B     CECTST07                                            0076
*CECTST06:                                                         0077
*                                                                  0077
*   /*****************************************************************/
*   /*                                                               */
*   /* ISSUE MESSAGE 155 TCEC MSGN(155)SUB(NO) INVOKED               */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0077
*   $TCECM='1'B;                    /* SET TCEC SW IN RCA            */
CECTST06 OI    $TCECM(WAPTR),B'00000001'                           0077
*   R1=ADDR($TCA10);                /* SET UP PARM LIST $TR          */
         LA    R1,$TCA10                                           0078
*   CALL #EXIT;                     /* CALL CECOM                    */
         L     @15,#EXITA(,WAPTR)                                  0079
         BALR  @14,@15                                             0079
