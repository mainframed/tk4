*   R6=ADDR(ALPHABET);              /* IS THE CHAR. IN INBUFR+2      */
CECTST21 LA    R6,ALPHABET                                         0181
*   DO I=26 TO 1 BY-1;              /* ALPHABETIC. YES, SET          */
         LA    I,26                                                0182
@DL00182 DS    0H                                                  0183
*     IF INPUTBUF(3)=LETTER THEN    /* POINTER TO IT AND             */
         CLC   INPUTBUF+2(1,R5),LETTER(R6)                         0183
         BNE   @RF00183                                            0183
*       DO;                         /* CONTINUE TO DECODE            */
*         LET3PTR=LET2PTR;                                         0185
         L     @15,LET2PTR                                         0185
         ST    @15,LET3PTR                                         0185
*         LET2PTR=LET1PTR;                                         0186
         L     @15,LET1PTR                                         0186
         ST    @15,LET2PTR                                         0186
*         LET1PTR=R6;                                              0187
         ST    R6,LET1PTR                                          0187
*         GO TO CECTST25;                                          0188
         B     CECTST25                                            0188
*       END;                                                       0189
*     R6=R6+1;                                                     0190
@RF00183 AL    R6,@CF00072                                         0190
*   END;                                                           0191
         BCTR  I,0                                                 0191
         LTR   I,I                                                 0191
         BP    @DL00182                                            0191
*   SECTIONS='010'B;                /* NO, SET BIT INDICATING        */
*                                                                  0192
         NI    SECTIONS,B'01011111'                                0192
         OI    SECTIONS,B'01000000'                                0192
*   /*****************************************************************/
