CECTST19 AL    R5,@CF00139                                         0166
*   R6=ADDR(ALPHABET);              /* IS THE CHAR. IN INBUFR+1      */
         LA    R6,ALPHABET                                         0167
*   DO I=26 TO 1 BY-1;              /* ALPHABETIC. NO, GO TO         */
         LA    I,26                                                0168
@DL00168 DS    0H                                                  0169
*     IF INPUTBUF(2)=LETTER THEN    /* ROUTINE FOR INCLUSIVE         */
         CLC   INPUTBUF+1(1,R5),LETTER(R6)                         0169
