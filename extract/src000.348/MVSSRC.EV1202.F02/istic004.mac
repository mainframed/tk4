         MVC   FSTSEC2(1),INPUTBUF(R5)                             0149
*   IF INPUTBUF(2)='-' THEN         /* IS THE CHAR. IN INBUFR+1      */
         CLI   INPUTBUF+1(R5),C'-'                                 0150
         BNE   @RF00150                                            0150
*     DO;                                                          0151
*       DASH='1'B;                                                 0152
         OI    DASH,B'00000001'                                    0152
*       GO TO CECTST17;             /* A HYPHEN. YES,CONTINUE        */
         B     CECTST17                                            0153
*     END;                                                         0154
*                                                                  0154
*   /*****************************************************************/
*   /*                                                               */
