*      2 $TCB10 FIXED(32) INIT(155),/* MSG NUMBER                    */
*      2 $TCC10 FIXED(32) INIT(0),  /* INCT LEN                      */
*      2 $TCD10 BIT(8) INIT('00000000'B),/* FLAG                     */
*      2 $TCE10 CHAR(4) INIT('FFFFFFFF'X);/* MSSAGE MASK             */
*   $TSTERR='1'B;                   /* SET ERROR FLAG ON             */
         OI    $TSTERR(WAPTR),B'01000000'                          0081
*   GO TO CECTST99;                 /* RETURN TO COMM INT            */
         B     CECTST99                                            0082
*CECTST07:                          /* SET ERROR FLAG ON             */
*   $TSTERR='1'B;                                                  0083
CECTST07 OI    $TSTERR(WAPTR),B'01000000'                          0083
*   GO TO CECTST77;                 /* RETURN TO COMM INT            */
         B     CECTST77                                            0084
*CECTST04:                                                         0085
*                                                                  0085
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE TEST DECODE                                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0085
*   $TSTCNT=$TSTCNT&&$TSTCNT;       /* INIT SECTION COUNT            */
CECTST04 LH    @15,$TSTCNT(,WAPTR)                                 0085
         LR    @00,@15                                             0085
         XR    @00,@15                                             0085
         STH   @00,$TSTCNT(,WAPTR)                                 0085
*   $RTNSLCT='0'B;                  /* INIT RTN SELECT BIT           */
         NI    $RTNSLCT(WAPTR),B'11011111'                         0086
*   $FAMILY(1)=' ';                 /* INIT FAMILY OF TESTS          */
         MVI   $FAMILY(WAPTR),C' '                                 0087
*   $FAMILY(2:5)=$FAMILY(1:4);      /* BE RUN                        */
         MVC   @TS00001(4),$FAMILY(WAPTR)                          0088
         MVC   $FAMILY+1(4,WAPTR),@TS00001                         0088
*   $VALDTST='0'B;                  /* INIT VALID TEST BIT           */
         NI    $VALDTST(WAPTR),B'11111011'                         0089
*   SWITCHES=SWITCHES&&SWITCHES;    /* INIT INTERNAL SWCHS           */
         XC    SWITCHES(1),SWITCHES                                0090
*   $RT0108=$RT0108&&$RT0108;       /* INITIALIZE                    */
         XC    $RT0108(1,WAPTR),$RT0108(WAPTR)                     0091
*   $RT0916=$RT0916&&$RT0916;       /* ROUTINE                       */
         XC    $RT0916(1,WAPTR),$RT0916(WAPTR)                     0092
*   $ROUTMSK=$ROUTMSK&&$ROUTMSK;    /* MASKS                         */
         XC    $ROUTMSK(30,WAPTR),$ROUTMSK(WAPTR)                  0093
*   DO I=1 TO 26 BY 1;              /* CLEAR OUT SECTION LIST        */
         LA    I,1                                                 0094
@DL00094 DS    0H                                                  0095
*     $SECLST(I)=SECINIT;                                          0095
         LR    @15,I                                               0095
         SLA   @15,2                                               0095
         ALR   @15,WAPTR                                           0095
         MVC   $SECLST-4(4,@15),SECINIT                            0095
*   END;                                                           0096
         AL    I,@CF00072                                          0096
         C     I,@CF00493                                          0096
         BNH   @DL00094                                            0096
*   SECCNTR=0;                                                     0097
         SLR   @15,@15                                             0097
         ST    @15,SECCNTR                                         0097
*   R8=ADDR($SECLST);               /* GET ADDR OF SECT LIST         */
         LA    R8,$SECLST(,WAPTR)                                  0098
*   $FAMILY(1)='T';                 /* ENTER T AS DEFAULT            */
         MVI   $FAMILY(WAPTR),C'T'                                 0099
*   IF INPUTBUF(1)='T'×             /* IF                            */
*       INPUTBUF(1)='P'×            /* TEST PREFIX                   */
*       INPUTBUF(1)='N'×            /* IS VALID                      */
*       INPUTBUF(1)='R' THEN        /* MOVE IT                       */
         CLI   INPUTBUF(R5),C'T'                                   0100
         BE    @RT00100                                            0100
         CLI   INPUTBUF(R5),C'P'                                   0100
         BE    @RT00100                                            0100
         CLI   INPUTBUF(R5),C'N'                                   0100
         BE    @RT00100                                            0100
         CLI   INPUTBUF(R5),C'R'                                   0100
         BNE   @RF00100                                            0100
@RT00100 DS    0H                                                  0101
*     DO;                           /* TO THE                        */
*       $FAMILY(1)=INPUTBUF(1);     /* SECTION LIST                  */
         MVC   $FAMILY(1,WAPTR),INPUTBUF(R5)                       0102
*       R5=R5+1;                    /* POINT TO NUMERICS             */
         AL    R5,@CF00072                                         0103
*     END;                                                         0104
*CECTST11:                                                         0105
*                                                                  0105
*   /*****************************************************************/
*   /*                                                               */
*   /* IS TEST ID NUMERIC                                            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0105
*   DO J=1 TO 4;                    /* ARE ALL CHARACTERS IN         */
@RF00100 DS    0H                                                  0105
CECTST11 LA    @15,1                                               0105
         STH   @15,J                                               0105
@DL00105 DS    0H                                                  0106
*     R6=ADDR(NUMBERS);             /* ID NUMERIC. NO, SET           */
         LA    R6,NUMBERS                                          0106
*     DO I=10 TO 1 BY-1;            /* ERROR CODE TO -05-            */
         LA    I,10                                                0107
@DL00107 DS    0H                                                  0108
*       IF INPUTBUF(J)=NUMBER THEN  /* AND PRINT ERROR               */
         LH    @15,J                                               0108
         ALR   @15,R5                                              0108
         BCTR  @15,0                                               0108
         CLC   INPUTBUF(1,@15),NUMBER(R6)                          0108
         BE    @RT00108                                            0108
*         GO TO CECTST12;           /* MESSAGE IFD112I               */
*       R6=R6+1;                    /* YES, CONTINUE TO              */
         AL    R6,@CF00072                                         0110
*     END;                          /* DECODE                        */
         BCTR  I,0                                                 0111
         LTR   I,I                                                 0111
         BP    @DL00107                                            0111
*     R5=R5+J-1;                    /* PT TO ERROR                   */
         LR    @15,R5                                              0112
         AH    @15,J                                               0112
         BCTR  @15,0                                               0112
         LR    R5,@15                                              0112
*     GO TO CECTST03;                                              0113
         B     CECTST03                                            0113
*CECTST12:                                                         0114
*   END;                                                           0114
CECTST12 LA    @15,1                                               0114
         AH    @15,J                                               0114
         STH   @15,J                                               0114
         C     @15,@CF00068                                        0114
         BNH   @DL00105                                            0114
*   $FAMILY(2:5)=INPUTBUF(1:4);     /* MOVE NUMERIC ENTRY            */
         MVC   $FAMILY+1(4,WAPTR),INPUTBUF(R5)                     0115
*CECTST13:                                                         0116
*                                                                  0116
*   /*****************************************************************/
*   /*                                                               */
*   /* DECODE SECTIONS ENTERED, IF THERE ARE ANY. IF NONE ARE        */
*   /* ENTERED, USE DEFAULT RANGE A-Z.                               */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0116
*   R5=R5+4;                        /* INCREMENT INBUFR POINTER BY 4 */
CECTST13 AL    R5,@CF00068                                         0116
*   IF INPUTBUF(1)='/' THEN         /* IS CHAR IN INBUFR+0 A SLASH.  */
         CLI   INPUTBUF(R5),C'/'                                   0117
         BNE   @RF00117                                            0117
*     DO;                           /* YES, SET BIT INDICATING       */
*       SECTIONS='100'B;            /* TEST SECTION HAS A LENGTH     */
*                                                                  0119
         NI    SECTIONS,B'10011111'                                0119
         OI    SECTIONS,B'10000000'                                0119
*       /*************************************************************/
*       /*                                                           */
*       /* OF ONE & SET DEFAULT RANGE                                */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0120
*       FSTSEC='  A';               /* OF SECTIONS A-Z & SET         */
         MVC   FSTSEC(3),@CC00764                                  0120
*       LSTSEC='  Z';               /* POINTER 1 TO ADDRESS OF 1ST   */
         MVC   LSTSEC(3),@CC00765                                  0121
*       LET1PTR=ADDR(ALPHABET);     /* LETTER IN ALPHABET & GO TO    */
         LA    @15,ALPHABET                                        0122
         ST    @15,LET1PTR                                         0122
*       GO TO CECTST29;             /* ROUTINE THAT BUILDS SECLST    */
         B     CECTST29                                            0123
*     END;                                                         0124
*CECTST14:                                                         0125
*                                                                  0125
*   /*****************************************************************/
*   /*                                                               */
*   /* DECODE THE ENTERED SECTIONS                                   */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0125
*   FSTSEC='   ';                   /* INITIALIZE CONSTANTS          */
@RF00117 DS    0H                                                  0125
CECTST14 MVC   FSTSEC(3),@CC00768                                  0125
*   LSTSEC=FSTSEC;                  /* TO DECODE SECTIONS            */
         MVC   LSTSEC(3),FSTSEC                                    0126
*   R6=ADDR(ALPHABET);              /* IS THE CHAR. IN INBUFR        */
         LA    R6,ALPHABET                                         0127
*   DO I=26 TO 1 BY-1;              /* ALPHABETIC. YES, SET          */
         LA    I,26                                                0128
@DL00128 DS    0H                                                  0129
*     IF INPUTBUF(1)=LETTER THEN    /* POINTER TO IT AND             */
         CLC   INPUTBUF(1,R5),LETTER(R6)                           0129
         BNE   @RF00129                                            0129
*       DO;                         /* CONTINUE DECODING. NO         */
*         LET1PTR=R6;               /* SET ERROR CODE -06- AND       */
         ST    R6,LET1PTR                                          0131
*         GO TO CECTST15;           /* PRINT ERROR MESSAGE           */
         B     CECTST15                                            0132
*       END;                        /* IFD112I                       */
*     R6=R6+1;                                                     0134
@RF00129 AL    R6,@CF00072                                         0134
*   END;                                                           0135
         BCTR  I,0                                                 0135
         LTR   I,I                                                 0135
         BP    @DL00128                                            0135
*   GOTO CECTST03;                                                 0136
         B     CECTST03                                            0136
*CECTST15:                          /* DOES SECTION NAME HAVE LENGTH
*                                      TWO                           */
*   SECTIONS='000'B;                /* CLEAR BITS THAT INDICATE THE  */
*                                                                  0137
CECTST15 NI    SECTIONS,B'00011111'                                0137
*   /*****************************************************************/
*   /*                                                               */
*   /* LENGTH OF THE TEST SECTION                                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0138
*   R6=ADDR(ALPHABET);              /* IS THE CHAR IN INBUFR+1       */
         LA    R6,ALPHABET                                         0138
*   DO I=26 TO 1 BY-1;              /* ALPHABETIC. YES, SET A        */
         LA    I,26                                                0139
@DL00139 DS    0H                                                  0140
*     IF INPUTBUF(2)=LETTER THEN    /* POINTER TO IT AND CONTINUE    */
         CLC   INPUTBUF+1(1,R5),LETTER(R6)                         0140
         BNE   @RF00140                                            0140
*       DO;                         /* TO DECODE.                    */
*         LET2PTR=LET1PTR;                                         0142
         L     @15,LET1PTR                                         0142
         ST    @15,LET2PTR                                         0142
*         LET1PTR=R6;                                              0143
         ST    R6,LET1PTR                                          0143
*         GO TO CECTST21;                                          0144
         B     CECTST21                                            0144
*       END;                                                       0145
*     R6=R6+1;                                                     0146
@RF00140 AL    R6,@CF00072                                         0146
*   END;                                                           0147
         BCTR  I,0                                                 0147
         LTR   I,I                                                 0147
         BP    @DL00139                                            0147
*   SECT1='1'B;                     /* NO, SET BIT INDICATING TEST   */
*                                                                  0148
         OI    SECT1,B'10000000'                                   0148
*   /*****************************************************************/
*   /*                                                               */
*   /* SECTION OF LENGTH ONE                                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0149
