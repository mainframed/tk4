*     R6=R6+1;                      /* YES, CONTINUE TO DECODE       */
         AL    R6,@CF00072                                         0171
*   END;                                                           0172
         BCTR  I,0                                                 0172
         LTR   I,I                                                 0172
         BP    @DL00168                                            0172
*   GO TO CECTST16;                 /* CONTINUE DECODING.            */
         B     CECTST16                                            0173
*CECTST20:                          /* IS SECTION NAME OF INCLUSIVE
*                                      RANGE LENGTH THREE            */
*   R6=ADDR(ALPHABET);              /* IS THE CHAR. IN INBUFR+2      */
CECTST20 LA    R6,ALPHABET                                         0174
*   DO I=26 TO 1 BY-1;              /* ALPHABETIC. NO,GO TO          */
         LA    I,26                                                0175
@DL00175 DS    0H                                                  0176
*     IF INPUTBUF(3)=LETTER THEN    /* ROUTINE FOR INCLUSIVE         */
         CLC   INPUTBUF+2(1,R5),LETTER(R6)                         0176
