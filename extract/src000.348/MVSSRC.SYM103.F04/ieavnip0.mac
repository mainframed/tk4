*/*                                                                  */
*/*                   GETMAIN PATH                                   */
*/*  REGISTER ONE CONTAINS ZERO ON INPUT FOR A GETMAIN REQUEST.      */
*/*  IF R1 IS ZERO, THEN SPACE IS OBTAINED FOR A SA/WA AND VECTOR    */
*/*  TABLE.  AFTER A SUCCESSFUL RETURN FROM GETMAIN, THE TABLE       */
*/*  IS INITIALIZED.                                                 */
*/*                                                                  */
*/********************************************************************/
*                                                                  2519
*GMAIN:                                                            2519
*         DO;                       /* GETMAIN PATH                  */
GMAIN    DS    0H                                                  2520
*           FLAGS=CDGM;             /* CONDITIONAL GETMAIN           */
         MVI   FLAGS,X'00'                                         2520
*           RFY                                                    2521
*            (GMREG,                                               2521
*             TBLPTR) RSTD;                                        2521
*           R3SAVAR=GMREG;          /* SAVE CONTENTS OF REG. 3       */
         LR    R3SAVAR,GMREG                                       2522
*           GMREG=GMTBL;            /* PARMS IN REG. 3               */
         L     GMREG,GMTBL                                         2523
*/* GETMAIN VECTOR TABLE AND SAVEAREAS FROM SUBPOOL 245              */
*                                                                  2524
*SGTWSA:                                                           2524
*           GEN REFS(CVTSPSA,CVTGLMN,CVTPTR,CVTMAP,WSAGGMFM,WSAG); 2524
SGTWSA   DS    0H                                                  2524
         GETMAIN RC,LV=(0),BRANCH=(YES,GLOBAL),KEY=(3),SP=(3),        XX
               RELATED=('SUPERVISOR ROUTINE WORK/SAVE AREAS FOR A CPU.XX
               THIS GETMAIN IS ISSUED IN THIS MACRO WHICH IS INCLUDED XX
               IN VARIOUS MODULES.  THE CORRESPONDING FREEMAIN IS AT  XX
               LABEL: SFRWSA, IN THIS MACRO')
*           GMREG=R3SAVAR;          /* RESTORE REG. 3, BRANCH ENTRY
*                                      TO GETMAIN DESTROYS REG. 3    */
         LR    GMREG,R3SAVAR                                       2525
*           IF RTCODE^=ZZERO        /* ERROR IN GETMAIN              */
*             THEN                  /* YES                           */
         LTR   RTCODE,RTCODE                                       2526
         BZ    @RF02526                                            2526
*             HOLDRC=FFOUR;         /* RETAIN RETURN CODE            */
         LA    HOLDRC,4                                            2527
*           ELSE                                                   2528
*WSTFORMT:                                                         2528
*             DO;                   /* INITIALIZE VECTOR TABLE     2528
*                                                            @ZM30488*/
         B     @RC02526                                            2528
@RF02526 DS    0H                                                  2528
WSTFORMT DS    0H                                                  2529
*               HOLDRC=ZZERO;       /* RETAIN RETURN CODE            */
         SLR   HOLDRC,HOLDRC                                       2529
*               P=TBLPTR;           /* POINT TO START OF TABLE       */
         LR    P,TBLPTR                                            2530
*               SAPTR=TBLPTR+(NUM*FFOUR);/* GET PTR TO SAVEAREAS     */
         L     @14,CVTWSAC(,RCVT)                                  2531
         LH    SAPTR,NUM(,@14)                                     2531
         SLA   SAPTR,2                                             2531
         ALR   SAPTR,TBLPTR                                        2531
*/* ROUND ADDR. OF FIRST WORK/SAVE AREA UP TO AN 8-BYTE MULT.  ALL   */
*/* WORK/SAVE AREAS MUST BEGIN ON DOUBLE WORD BOUNDARIES.  ALL WORK/ */
*/* SAVE AREAS ARE MULTIPLES OF 8 BYTES IN SIZE.  THUS IF THE FIRST  */
*/* WORK/SAVE AREA IS ON A DOUBLE WORD BOUNDARY, THEY ALL WILL BE.   */
*                                                                  2532
*               SAPTR=((SAPTR+7)×7)&&7;                            2532
         LA    @07,7                                               2532
         LR    @14,SAPTR                                           2532
         ALR   @14,@07                                             2532
         OR    @14,@07                                             2532
         XR    @14,@07                                             2532
         LR    SAPTR,@14                                           2532
*               RFY                                                2533
*                (TBLPTR) UNRSTD;                                  2533
*               NSIZEPTR=ADDR(FSIZE);/* PICK UP ADDR. OF FIELD     2534
*                                      CONTAINING FIRST WORK/SAVE  2534
*                                      AREA SIZE                     */
         L     @14,CVTWSAC(,RCVT)                                  2534
         LA    NSIZEPTR,FSIZE(,@14)                                2534
*INTZE:                                                            2535
*               DO LOOP=LUPONE TO NUM;/* DO FOR EACH ENTRY IN VECTOR
*                                      TABLE                         */
INTZE    LA    LOOP,1                                              2535
         B     @DE02535                                            2535
@DL02535 DS    0H                                                  2536
*                 SVPTR=SAPTR;      /* SET PTR TO SAVEAREA           */
         ST    SAPTR,SVPTR(,P)                                     2536
*                 SAPTR=SAPTR+NWSASIZE;/* INCREMENT PTR BY SIZE OF 2537
*                                      SAVEAREA                      */
         AH    SAPTR,NWSASIZE(,NSIZEPTR)                           2537
*                 P=P+LENGTH(SVPTR);/* POINT TO NEXT VECTOR TABLE  2538
*                                      ENTRY                         */
         AH    P,@CH00062                                          2538
*                 NSIZEPTR=NSIZEPTR /* POINT TO NEXT SIZE            */
*                     +LENGTH(NWSASIZE);                           2539
         AL    NSIZEPTR,@CF00082                                   2539
*               END INTZE;                                         2540
         AH    LOOP,@CH00070                                       2540
@DE02535 L     @14,CVTWSAC(,RCVT)                                  2540
         CH    LOOP,NUM(,@14)                                      2540
         BNH   @DL02535                                            2540
