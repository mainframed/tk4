PRBLEN   EQU 136
SIRBLEN  EQU 200
TIRBLEN  EQU 136
IRBLEN   EQU 128
SVRBLEN  EQU 224
*                                                                  0559
*/*  SAVE INPUT REG'S IN SAVE AREA                                   */
*                                                                  0560
*   NP0IPLD=R3;                     /* R3 CONTAINS ADDR OF IPLDATA   */
         LR    NP0IPLD,R3                                          0560
*   NP0ISTSZ=R6;                    /* R6 CONTAINS STOR SIZE AS    0561
*                                      DETERMINED BY IPL             */
         ST    R6,NP0ISTSZ                                         0561
*   NP0NCEND=R7;                    /* R7 CONTAINS ADDR OF END (TOP)
*                                      OF NUCLEUS, EXCLUDING NIP0  0562
*                                      CSECT                         */
         LR    NP0NCEND,R7                                         0562
*   NP0IPLU=R10;                    /* R10 CONTAINS IPL DEVICE ADDR
*                                      IN CUA FORM.                  */
         STH   R10,NP0IPLU                                         0563
*   RCVT=FLCCVT;                    /* PUT ADDR OF CVT IN R3, THIS IS
*                                      A NIP STANDARD                */
*                                                                  0564
         L     @15,NPSAPTR                                         0564
         L     RCVT,FLCCVT(,@15)                                   0564
*   /*****************************************************************/
*   /*                                                               */
*   /* TRAP ANY REQUESTS FOR 'SVC 60' THAT SVC HAS NOT BEEN LOADED   */
*   /* YET AND MIGHT BE REQUESTED BEFORE IT IS LOADED (STAE)         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0565
*   CVTSV60=ADDR(NSVC60TP);         /* SET PTR. TO SVC 60 IN CVT TO
*                                      ADDR. OF TRAP RTN. IT WILL  0565
*                                      CLEAR REG 15 AND RETURN TO  0565
*                                      CALLER                        */
         LA    @14,NSVC60TP                                        0565
         ST    @14,CVTSV60(,RCVT)                                  0565
*   RFY                                                            0566
*    (SVCENTRY) BASED(NP0SVCBA);                                   0566
*   NP0SVCBA=NVTSVCTB               /* GET ADDR. OF SVCTABLE ENTRY   */
*       +(60                        /* FOR SVC 60 IN BASE FOR THE    */
*       *LENGTH(SVCENTRY));         /* SVCTABLE ENTRY MAPPING MACRO  */
         LA    NP0SVCBA,480                                        0567
         AL    NP0SVCBA,NVTSVCTB                                   0567
*   NVTSVC60=SVCENTRY;              /* MOVE SVC 60 ENTRY TO SAVE AREA*/
         MVC   NVTSVC60(8),SVCENTRY(NP0SVCBA)                      0568
*   SVCEP=ADDR(NSVC60TP);           /* MOVE ADDR OF NIP0 TRAP INTO 0569
*                                      SVC 60 ENTRY IN SVC TABLE     */
         LA    @14,NSVC60TP                                        0569
         ST    @14,SVCEP(,NP0SVCBA)                                0569
*   SVCATTR1=SVCATTR1&&             /* CLEAR ATTRIBUTE 1 FIELD TO 0  */
*       SVCATTR1;                                                  0570
         XC    SVCATTR1(2,NP0SVCBA),SVCATTR1(NP0SVCBA)             0570
*   SVCATTR2=SVCATTR2&&             /* CLEAR ATTRIBUTE 2 FIELD TO 0  */
*       SVCATTR2;                                                  0571
         XC    SVCATTR2(2,NP0SVCBA),SVCATTR2(NP0SVCBA)             0571
*   SVCLL='1'B;                     /* TURN ON FLAG TO INDICATE LOCAL
*                                      LOCK REQUIRED. THE SVC 60   0572
*                                      ENTRY NOW DESCRIBES A TYPE 1
*                                      SVC, UN-AUTHORIZED, AND     0572
*                                      REQUIRING THE LOCAL LOCK THE
*                                      ROUTINE POINTED TO IS THE NIP0
*                                      SVC 60 TRAP WHICH JUST CLEARS
*                                      REG. 15 TO ZERO AND RETURNS TO
*                                      THE CALLER.                   */
         OI    SVCLL(NP0SVCBA),B'10000000'                         0572
*   RFY                                                            0573
*    (R3,                                                          0573
*     R6,                                                          0573
*     R7,                                                          0573
*     R10) UNRSTD;                                                 0573
*                                                                  0573
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK IF THIS IS AN OS/VS2 SYSTEM ---- IF SO, NIP0 WILL HAVE  */
*   /* BEEN LOADED JUST ABOVE THE NUCLEUS IF NOT, NIP0 WILL HAVE BEEN*/
*   /* LOADED ELSEWHERE                                              */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0574
*   IF NP0ENTRY^=NP0NCEND THEN                                     0574
         C     NP0NCEND,NP0ENTRY                                   0574
         BNE   @RT00574                                            0574
*     GOTO NP0NNIPM;                /* LOAD WAIT STATE PSW TO      0575
*                                      INDICATE NIPM NOT FOUND.      */
*/* TRAP PROGRAM CHECKS BY REPLACING SYSGEN'D P/C NEW PSW            */
*/* PROG. CHECKS ARE TRAPPED SINCE THE PROG. CHECK FLIH WOULD TRY    */
*/* TO INVOKE SVC 13 (ABEND) TO ABEND THE OFFENDING TASK.  DURING    */
*/* NIP THAT TASK WOULD BE THE MASTER SCHEDULER.  ALSO, SVC 13 IS    */
*/* NOT INITIALIZED YET AND IS THEREFORE UNUSABLE                    */
*/*                                                                  */
*/* NOTE: THE SYS. GEN'D. PROGRAM CHECK NEW PSW HAS THE WAIT BIT ON  */
*/*        SO THAT IF A PROGRAM CHECK OCCURRS BEFORE NIP0 SETS UP    */
*/*        THIS TRAP, THE SYSTEM WILL BE PUT INTO A WAIT STATE.      */
*/*                                                                  */
*                                                                  0576
*   NP0SPCSA=FLCPNPSW;              /* SAVE SYSGEN'D P/C NEW PSW     */
         MVC   NP0SPCSA(8),FLCPNPSW(@15)                           0576
*   PCBR10=ADDR(NP0UXPC);           /* GET ADDR OF NIP0 ROUTINE TO 0577
*                                      HANDLE UNEXPECTED PROG.CHECKS */
         LA    PCBR10,NP0UXPC                                      0577
*   FLCPNPSW=NP0PCPSW;              /* PUT REPLACEMENT P/C NEW PSW IN
*                                      PLACE, IT POINTS TO A 'BR 10'
*                                      INSTRUCTION                   */
         MVC   FLCPNPSW(8,@15),NP0PCPSW                            0578
*   NP0SRSSA=FLCRNPSW;              /* SAVE SYSGEN'D RESTART PSW, THE
*                                      SYSGEN'D PSW WILL BE MODIFIED
*                                      IN THE SIGP RTN,THIS SAVED  0579
*                                      COPY WILL BE USED TO RECREATE
*                                      THE ORIGINAL                  */
*                                                                  0579
         MVC   NP0SRSSA(8),FLCRNPSW(@15)                           0579
*   /*****************************************************************/
*   /*                                                               */
*   /* THE FOLLOWING CODE WILL DETERMINE IF THE TOD CLOCK IS IN THE  */
*   /* ERROR STATE, AND IF SO WILL PROVIDE LOGIC TO CLEAR THE ERROR  */
*   /* STATE VIA SET CLOCK INSTRUCTION. IF THE CLOCK REMAINED IN THE */
*   /* ERROR STATE A SET CPU TIMER INSTRUCTION WOULD CAUSE A MACHINE */
*   /* CHECK TO BECOME PENDING. WHEN THIS MODULE WOULD THEN REACH THE*/
*   /* POINT IN PROCESSING TO ENABLE FOR MACHINE CHECKS, IT WOULD    */
*   /* TERMINATE IN A WAIT STATE BECAUSE 0F THE PENDING MACHINE      */
*   /* CHECK.                                                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0580
*/*                                                          @YM03537*/
*                                                                  0580
*   RFY                                                            0580
*     WORKREG6 RSTD;                /* RESTRICT R6           @YM03537*/
*   WORKREG6=ADDR(CLOKWORK);        /* R6 = ADDR OF DWORD TO STORE 0581
*                                      TOD CLOCK VALUE.      @YM03537*/
         LA    WORKREG6,CLOKWORK                                   0581
*   GEN(STCK 0(WORKREG6));          /* STORE TOD CLOCK TO DETERMINE
*                                      IF IT IS IN THE ERR STATE.  0582
*                                                            @YM03537*/
         STCK 0(WORKREG6)
*   BC(13,ROUNDNUC);                /* BRANCH AROUND SET CLOCK LOGIC
*                                      IF TOD CLK IS NOT IN ERR.   0583
*                                                            @YM03537*/
         BC    13,ROUNDNUC                                         0583
*   CLOKWORK=''B;                   /* ZERO OUT CLOK WORK AREA FOR 0584
*                                      SET CLOCK INSTRUCTION.      0584
*                                                            @YM03537*/
         XC    CLOKWORK(8),CLOKWORK                                0584
*   FLCRNPSW=SETCKPSW;              /* SET RESTART NEW PSW POINTING
*                                      TO SET CLOCK INSTRUCTION.   0585
*                                                            @YM03537*/
         L     @07,NPSAPTR                                         0585
         MVC   FLCRNPSW(8,@07),SETCKPSW                            0585
*   NVTFLWSC=NVTWCN05;              /* PUT WAIT STATE CODE OF 4A INTO
*                                      WAIT PSW.             @YM03537*/
         MVI   NVTFLWSC,X'4A'                                      0586
*   LPSW(NVTWTPSW);                 /* LOAD WAIT STATE AND CAUSE THE
*                                      OPERATOR TO DEPRESS THE     0587
*                                      RESTART BUTTON AND TO OPERATE
*                                      THE CLOCK SECURITY SWITCH.  0587
*                                                            @YM03537*/
*                                                                  0587
         LPSW  NVTWTPSW                                            0587
*   /*****************************************************************/
*   /*                                                               */
*   /* WE WILL RESUME PROCESSING HERE WHEN THE OPERATOR HITS RESTART.*/
*   /* THIS SET CLOCK INSTRUCTION WILL CLEAR THE ERROR STATE. CLOCK  */
*   /* IS SET TO ZERO.                                               */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0588
*/*                                                          @YM03537*/
*                                                                  0588
*SETTODCK:                                                         0588
*   GEN(SCK 0(WORKREG6));           /* SET CLOCK TO ZERO     @YM03537*/
SETTODCK SCK 0(WORKREG6)
*   BC(7,SETTODCK);                 /* LOOP BACK TO SET CLOCK      0589
*                                      INSTRUCT IF OPERATOR HAS NOT
*                                      OPERATED THE CLOCK SECURITY 0589
*                                      SWITCH.               @YM03537*/
         BC    7,SETTODCK                                          0589
*   NVTCLKER='1'B;                  /* FORCE PROMPTING AT TOD CLOCK
*                                      INITIALIZATION.       @ZA10111*/
         OI    NVTCLKER,B'00000001'                                0590
*   FLCRNPSW=NP0SRSSA;              /* RESTORE RESTART NEW TO SYSGEND
*                                      VALUE, IN THE EVENT RESTART IS
*                                      DEPRESSED AGAIN.      @YM03537*/
         L     @07,NPSAPTR                                         0591
         MVC   FLCRNPSW(8,@07),NP0SRSSA                            0591
*   RFY                                                            0592
*     WORKREG6 UNRSTD;              /* FREE R6 FOR USE       @YM03537*/
*                                                                  0592
*   /*****************************************************************/
*   /*                                                               */
*   /* ROUND ADDR OF TOP OF NUCLEUS UP TO AN 8-BYTE MULTIPLE AND     */
*   /* STORE IN INITIAL NVT                                          */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0593
*ROUNDNUC:                          /* BRANCH HERE IF SET CLOCK LOGIC
*                                      IS BYPASSED.          @YM03537*/
*   NVTNUCND=((NP0NCEND+7)×7)&&7;   /* ROUND TO 8 BYTE BOUNDARY      */
ROUNDNUC LA    @02,7                                               0593
         LR    @15,NP0NCEND                                        0593
         ALR   @15,@02                                             0593
         OR    @15,@02                                             0593
         XR    @15,@02                                             0593
         ST    @15,NVTNUCND                                        0593
*/* PUT ADDR. OF NEXT AVAIL. BYTE IN NUC BUFF IN NIP0 POINTER        */
*                                                                  0594
*   NP0NBAVL=NVTNUCND;                                             0594
*                                                                  0594
         ST    @15,NP0NBAVL                                        0594
*/* ROUND REAL STORAGE SIZE (PASSED FROM IPL) DOWN TO PAGE BOUND     */
*/* SAVE ROUNDED-DOWN VALUE IN SAVEAREA                              */
*                                                                  0595
*   NP0STSZ=(NP0ISTSZ/4096)*4096;                                  0595
         L     @02,NP0ISTSZ                                        0595
         SRL   @02,12                                              0595
         SLA   @02,12                                              0595
         ST    @02,NP0STSZ                                         0595
*   CVTNUCLS=IPLDNUCM;              /* PUT ID OF NUCLEUS MEMBER IN 0596
*                                      CVT                           */
*                                                                  0596
         L     @02,CVTEXT2(,RCVT)                                  0596
         MVC   CVTNUCLS(1,@02),IPLDNUCM(NP0IPLD)                   0596
*   /*****************************************************************/
*   /*                                                               */
*   /* SET UP PARAMETERS AND USE IOSGEN MACRO TO INVOKE UCB LOOK-UP  */
*   /* ALGORITHM TO GET ADDR. OF UCB FOR IPL DEVICE                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0597
*   RFY                                                            0597
*    (RENTRY,                                                      0597
*     R6,                                                          0597
*     UCBADR) RSTD;                                                0597
*   R6=NP0IPLU;                     /* PUT IPL DEVICE ADDR. IN PARAM
*                                      REG FOR IOSGEN                */
         SLR   R6,R6                                               0598
         ICM   R6,3,NP0IPLU                                        0598
*   GENERATE REFS(CVTILK1,CVTILK2,CVTIXAVL,CVTPTR,CVTMAP,CVTILCH,  0599
*       IOCOMEX,UCBOB,UCBCHAN,UCBFL5,UCBLCI,LCH,LCHTCH,IOCOM,IOX,  0599
*       IOCUCBBM,IOXHICH);                                         0599
*                                                       /*@YM06032*/
        CNOP  0,4
        IOSGEN UCBLOOK,UCB=(7),REG=(6)
*/* ON RETURN; REG 15 CONTAINS A RETURN CODE                       */*
*/*   IF REG 15 = 0  UCB FOUND, UCB ADDR IN REG 7.                 */*
*/*   IF REG 15 ^= 0  UCB NOT FOUND                                */*
        LTR   15,15             */*CHECK RETURN CODE               */*
        BNZ   NP0NIPLU          */*IF NON-ZERO, BRANCH TO A RTN TO */*
*/*                                LOAD WAIT STATE PSW             */*
*   RFY                                                            0600
*    (R1,                                                          0600
*     RENTRY,                                                      0600
*     R6) UNRSTD;                                                  0600
*   NPBNUCBA=UCBADR;                /* PUT UCB ADDR IN DEB FOR     0601
*                                      SYS1.NUCLEUS DATA SET         */
         STCM  UCBADR,7,NPBNUCBA                                   0601
*   CVTSYSAD=UCBADR;                /* PUT UCB ADDR IN CVT           */
         ST    UCBADR,CVTSYSAD(,RCVT)                              0602
*   RFY                                                            0603
*    (UCBOB) BASED(NPBNUCBA);                                      0603
*   RFY                                                            0604
*    (UCBADR) UNRSTD;                                              0604
*   UCBPRES='1'B;                   /* TURN ON PERM. RESID. UCB FLAG */
*   UCBSYSR='1'B;                   /* TURN ON SYS.RESID UCB FLAG    */
         SLR   @02,@02                                             0606
         ICM   @02,7,NPBNUCBA                                      0606
         OI    UCBPRES(@02),B'00000110'                            0606
*   UCBVOLI=IPLDVSER;               /* PUT VOL. SER. OF IPL VOLUME IN
*                                      UCB                           */
         MVC   UCBVOLI(6,@02),IPLDVSER(NP0IPLD)                    0607
*/********************************************************************/
*/*                                                                  */
*/* SET UP TO CONVERT MBBCCHHR MACHINE ADDRESSES TO TTR ADDRESSES.   */
*/* CONVERT RTN (IECPRLTV) EXPECTS AS INPUT:                         */
*/*                  R1 - ADDR OF THE DEB FOR THE DATA SET           */
*/*                  R2 - ADDR OF 8 BYTE AREA CONTAINING             */
*/*                       'MBBCCHHR' MACHINE ADDRESS                 */
*/*                  R14 - RETURN ADDR                               */
*/*                  R15 - ADDR OF 'IECPRLTV' RTN.                   */
*/* CONVERT RTN OUTPUT:                                              */
*/*                  R0 - TTRN, REL. ADDR, N IS CATENATION NUMBER    */
*/*                                                                  */
*/* NOTE: THE 'IECPRLTV' ROUTINE USES REGISTERS 9 - 13 BUT DOES NOT  */
*/* RESTORE THEM, THUS THE CALLER SHOULD PRESERVE THE REGISTERS IF   */
*/* THEY CONTAIN VALUABLE INFORMATION.                               */
*/*                                                                  */
*/* THE CONVERT ROUTINE DOES NOT USE REGISTERS 3 - 8                 */
*/****************************************************************** */
*                                                                  0608
*   NP0ACTA1=IPLDVTOC;              /* PUT MACH.ADDR.OF IPL VOL VTOC
*                                      IN WORKAREA                   */
         MVC   NP0ACTA1(5),IPLDVTOC(NP0IPLD)                       0608
*   RFY                                                            0609
*    (WORKPTR,                                                     0609
*     DEBPNTR,                                                     0609
*     RENTRY,                                                      0609
*     REXIT,                                                       0609
*     RTTRN,                                                       0609
*     R6) RSTD;                                                    0609
*   WORKPTR=ADDR(NP0ACTAD);         /* PUT ADDR 8-BYTE WORK AREA IN
*                                      PARAM. REG                    */
         LA    WORKPTR,NP0ACTAD                                    0610
*   DEBPNTR=ADDR(NIPDEBSN);         /* PUT DEB ADDR IN PARAM REG     */
         LA    DEBPNTR,NIPDEBSN                                    0611
*   R6=ADDR(NREGSAV);               /* PICK UP ADDR OF REG SAVE AREA */
         LA    R6,NREGSAV                                          0612
*   GEN(STM     9,13,0(R6));        /* SAVE REG'S 9 - 13             */
         STM     9,13,0(R6)
*   RENTRY=CVTPRLTV;                /* PICK UP ADDR OF IECPRLTV RTN  */
         L     RENTRY,CVTPRLTV(,RCVT)                              0614
*   GEN(BALR   REXIT,RENTRY);       /* BRANCH TO CONVERT RTN         */
         BALR   REXIT,RENTRY
*   GEN(LM     9,13,0(R6));         /* RESTORE REG'S 9 - 13          */
         LM     9,13,0(R6)
*   UCBVTOC=RTTRN;                  /* PUT TTR ADDR IN IPL UCB       */
         SLR   @07,@07                                             0617
         ICM   @07,7,NPBNUCBA                                      0617
         ST    RTTRN,UCBVTOC(,@07)                                 0617
*   UCBCHAN=NP0IPLU;                /* OVERLAY CHAN ADDR. IN IPL UCB
*                                      IN CASE ALT. CHAN. PATH ADDR.
*                                      IS NOT SET TO CHAN USED IN IPL*/
*                                                                  0618
         MVC   UCBCHAN(2,@07),NP0IPLU                              0618
*   /*****************************************************************/
*   /*                                                               */
*   /* DEB INITIALIZATION FOR SYS1.NUCLEUS                           */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0619
*   NP0NSTCH=NIPLNUCS;              /* PUT CCHH START ADDR. OF SYS1.
*                                      NUCLEUS DATA SET IN DEB       */
         MVC   NP0NSTCH(4),NIPLNUCS(NP0IPLD)                       0619
*   NP0NNDCH=NIPLNUCE;              /* PUT CCHH END ADDR.OF SYS1.  0620
*                                      NUCLEUS DATA SET IN DEB       */
         MVC   NP0NNDCH(4),NIPLNUCE(NP0IPLD)                       0620
*   NP0ACTA3=0;                     /* ZERO OUT 'R' BYTE IN 8-BYTE 0621
*                                      WORK AREA                     */
         MVI   NP0ACTA3,X'00'                                      0621
*   NP0ACTA2=NIPLNUCS;              /* PUT SYS1.NUCLEUS START ADDR IN
*                                      WORKAREA                      */
         MVC   NP0ACTA2(4),NIPLNUCS(NP0IPLD)                       0622
*   GEN(STM     9,13,0(R6));        /* SAVE REG'S 9 - 13             */
         STM     9,13,0(R6)
*   RENTRY=CVTPRLTV;                /* LOAD ADDR OF IECPRLTV RTN     */
         L     RENTRY,CVTPRLTV(,RCVT)                              0624
*   GEN(BALR    REXIT,RENTRY);      /* BRANCH TO CONVERT RTN         */
         BALR    REXIT,RENTRY
*   GEN(LM     9,13,0(R6));         /* RESTORE REG'S 9 - 13          */
         LM     9,13,0(R6)
*   NP0RELAD=RTTRN;                 /* SAVE TTR ADDR.                */
         LR    NP0RELAD,RTTRN                                      0627
*   NP0ACTA2=NIPLNUCE;              /* PUT SYS1.NUCLEUS END ADDR. IN
*                                      WORKAREA                      */
         MVC   NP0ACTA2(4),NIPLNUCE(NP0IPLD)                       0628
*   GEN(STM     9,13,0(R6));        /* SAVE REG'S 9 - 13             */
         STM     9,13,0(R6)
*   RENTRY=CVTPRLTV;                /* LOAD ADDR OF IECPRLTV RTN     */
         L     RENTRY,CVTPRLTV(,RCVT)                              0630
*   GEN(BALR     REXIT,RENTRY);     /* BRANCH TO CONVERT RTN         */
         BALR     REXIT,RENTRY
*   GEN(LM     9,13,0(R6));         /* RESTORE REG'S 9 - 13          */
         LM     9,13,0(R6)
*   RFY                                                            0633
*    (R6) UNRSTD;                                                  0633
