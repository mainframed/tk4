*                                      TABLE ENTRY SUBTRACT LENGTH 2197
*                                      FROM CURR. REAL ADDR AND PUT
*                                      RESULTING ADDR. IN CORRES   2197
*                                      TABLE ENTRY AND PRESERVE      */
         LR    TABPTR,NP0SATBL                                     2197
*NSPAL01:                                                          2198
*   NALLOCLN=((NP0LNGTH             /* ROUND LENGTH FROM TABLE ENTRY */
*       +7)                         /* UP TO AN 8-BYTE MULTIPLE      */
*       ×7)&&7;                                                    2198
NSPAL01  LA    @06,7                                               2198
         L     NALLOCLN,NP0LNGTH(,TABPTR)                          2198
         ALR   NALLOCLN,@06                                        2198
         OR    NALLOCLN,@06                                        2198
         XR    NALLOCLN,@06                                        2198
*   NP0RADDR=NP0SAHIR-NALLOCLN;                                    2199
         LR    @06,NP0SAHIR                                        2199
