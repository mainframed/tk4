*     GOTO NSPAL01;                 /* (ALL ZEROS), IF NOT-LOOP      */
*   END NSPALLOC;                   /* RETURN TO CALLER              */
@EL00003 DS    0H                                                  2206
@EF00003 DS    0H                                                  2206
@ER00003 LM    @14,@01,12(@13)                                     2206
