         TITLE 'IEDQRS - SNA RESPONSE GENERATION PROGRAM               *
                        '
IEDQRS   CSECT ,                                                   0001
@PROLOG  DS    0H                                                  0002
         USING *,RBASE
IEDQRS   IEDHJN AFTERHJN,HJN
         LA    RLCB,0(RLCB)
*                                                                  0448
*/********************************************************************/
*/*                                                                  */
*/*      M A I N   C O D E   S E G M E N T                           */
*/*                                                                  */
*/********************************************************************/
*                                                                  0448
*   STM(R0,R15,SAVEREGS);           /* SAVE REGS                     */
         STM   R0,R15,SAVEREGS                                     0448
*   IF R0^=0 THEN                   /* IEDQUI (IEDRESP ISSUED IN   0449
*                                      INHDR OR INBUF) OR IEDQA4   0449
*                                      ENTRY                         */
         SLR   @10,@10                                             0449
         CR    R0,@10                                              0449
         BE    @RF00449                                            0449
*     DO;                           /* YES                           */
*       IF RPARM=0 THEN             /* ENTRY FROM IEDQA4             */
         CR    RPARM,@10                                           0451
         BNE   @RF00451                                            0451
*         DO;                       /* YES                           */
*           CALL QA4ENTRY;          /* IEDQA4 ENTRY PROCESSING       */
         BAL   @14,QA4ENTRY                                        0453
*           LM(R0,R15,SAVEREGS);    /* RESTORE REGS                  */
         LM    R0,R15,SAVEREGS                                     0454
*           RETURN;                 /* RETURN TO QA4                 */
@EL00001 DS    0H                                                  0455
@EF00001 DS    0H                                                  0455
@ER00001 BR    @14                                                 0455
*         END;                                                     0456
*       ELSE                        /* IEDQUI ENTRY                  */
*         DO;                                                      0457
@RF00451 DS    0H                                                  0458
*           PARM=RPARM;             /* SET UP IEDRESP MACRO PARAMTER
*                                      LIST POINTER                  */
         ST    RPARM,PARM                                          0458
*           SAVE13=R13;             /* SAVE R13                      */
         ST    R13,SAVE13                                          0459
*           R13=R9+LENGTH(SAVEREGS);/* ADDR OF AVTSAVE2              */
         LA    R13,72                                              0460
         ALR   R13,R9                                              0460
*           CALL QUIENTRY;          /* IEDQUI ENTRY PROCESSING       */
         BAL   @14,QUIENTRY                                        0461
*           R15=RC;                 /* SET UP RETURN CODE            */
         L     R15,RC                                              0462
*           R14=AVTUI+4;            /* RETURN TO 2ND ENTRY IN IEDQUI */
         LA    R14,4                                               0463
         AL    R14,AVTUI(,IEDQAPTR)                                0463
*           R13=SAVE13;             /* RESTORE R13                   */
         L     R13,SAVE13                                          0464
*           RETURN;                 /* RETURN TO IEDQUI              */
         B     @EL00001                                            0465
*         END;                                                     0466
*     END;                                                         0467
*   ELSE                            /* IEDQBD ENTRY                  */
*     DO;                                                          0468
@RF00449 DS    0H                                                  0469
*       LCBERBLK=RPARM;             /* SET UP POST LIST              */
         STCM  RPARM,7,LCBERBLK(IEDQLPTR)                          0469
*       IF R9=0 THEN                /* 'DEFAULT' ENTRY (FROM IEDQBD
*                                      AFTER ALL INMSG MACROS HAVE 0470
*                                      BEEN EXECUTED)                */
         LTR   R9,R9                                               0470
         BNZ   @RF00470                                            0470
*         CALL DEFAULT;             /* 'DEFAULT' ENTRY PROCESSING    */
         BAL   @14,DEFAULT                                         0471
*       ELSE                        /* ENTRY AS A RESULT OF IEDRESP
*                                      MACRO ISSUED IN INMSG SUBGROUP*/
*         DO;                                                      0472
         B     @RC00470                                            0472
@RF00470 DS    0H                                                  0473
*           PARM=R8;                /* ADDR OF IEDRESP MACRO PARAMTER
*                                      LIST                          */
         ST    R8,PARM                                             0473
*           CALL INMSG;             /* DO INMSG IEDRESP PROCESSING   */
         BAL   @14,INMSG                                           0474
*         END;                                                     0475
*                                                                  0475
*/*                                                                  */
*                                                                  0476
*       LCBERBQB=IEDQBD;            /* SET ERB UP IEDQBD TPOST     0476
*                                      ADDRESS                       */
@RC00470 L     @10,AVTMSGS-1(,IEDQAPTR)                            0476
         MVC   LCBERBQB(3,IEDQLPTR),IEDQBD+1(@10)                  0476
*       LCBERBPY=PRIEDISP;          /* SET TPOST PRIORITY            */
         MVI   LCBERBPY(IEDQLPTR),X'E0'                            0477
*       RPARM=ADDR(LCBERB);         /* ELEMENT ADDRESS = ERB         */
         LA    RPARM,LCBERB(,IEDQLPTR)                             0478
*       RDISP=AVTEA;                /* DISPATCHER BASE               */
         L     RDISP,AVTEA(,IEDQAPTR)                              0479
*       CALL DSPCHAIN;              /* TPOST ERB BACK TO IEDQBD      */
         LA    @15,DSPCHAIN(,RDISP)                                0480
         BALR  @14,@15                                             0480
*     END;                                                         0481
*                                                                  0481
*/********************************************************************/
*/*  END OF MAIN ROUTINE                                             */
*/********************************************************************/
*                                                                  0482
*                                                                  0482
*/********************************************************************/
*/*                                                                  */
*/* THIS ROUTINE GETS A BUFFER UNIT BY EITHER STEALING IT OR IF THAT */
*/* FAILS  POSTING AN ERB.                                           */
*/*                                                                  */
*/********************************************************************/
*                                                                  0482
*ALLOCBUF:                                                         0482
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0482
         B     @EL00001                                            0482
ALLOCBUF DS    0H                                                  0483
*   DCL                                                            0483
*     BFRQCB LABEL GEND;            /* IEDQRS'S INTERNAL RETURN QCB  */
*   LCBLINK=R14;                    /* SAVE RETURN REG               */
         STCM  R14,7,LCBLINK(IEDQLPTR)                             0484
*   R0=1;                           /* ONE UNIT PER BUFFER           */
         LA    @10,1                                               0485
         LR    R0,@10                                              0485
*   RPARM=1;                        /* ONE BUFFER                    */
         LR    RPARM,@10                                           0486
*   CALL IEDQGA02;                  /* TRY TO STEAL A BUFFER         */
         L     @15,AVTSTEAL(,IEDQAPTR)                             0487
         BALR  @14,@15                                             0487
*   IF R15^=0 THEN                  /* GOT ONE                       */
         LTR   R15,R15                                             0488
         BZ    @RF00488                                            0488
*     RPRF=R15;                     /* SET BUFFER ADDRESSABILITY     */
         LR    RPRF,R15                                            0489
*   ELSE                            /* DID NOT GET ONE               */
*     DO;                                                          0490
         B     @RC00488                                            0490
@RF00488 DS    0H                                                  0491
*       LCBDLNKN=OFF;               /* SHOW ERB IN USE               */
         NI    LCBDLNKN(IEDQLPTR),B'11111110'                      0491
*       LCBRCQCB=ADDR(BFRQCB);      /* RETURN QCB ADDRESS            */
         LA    @10,BFRQCB                                          0492
         ST    @10,LCBRCQCB(,IEDQLPTR)                             0492
*       LCBERBQB=ADDR(AVTBFREB);    /* ADDRESS OF BUFFER REQUEST QCB */
         LA    @10,AVTBFREB(,IEDQAPTR)                             0493
         STCM  @10,7,LCBERBQB(IEDQLPTR)                            0493
*       LCBERBPY=PRIINTRQ;          /* TPOST PRIORITY                */
         MVI   LCBERBPY(IEDQLPTR),X'E4'                            0494
*       LCBRBCT1=1;                 /* # BUFFERS                     */
         MVI   LCBRBCT1(IEDQLPTR),X'01'                            0495
*       LCBRBCT2=1;                 /* # UNITS PER BUFFER            */
         MVI   LCBRBCT2(IEDQLPTR),X'01'                            0496
*       LCBPRCPG=ON;                /* INDICATE APPLICATION TYPE   0497
*                                      BUFFER REQUEST                */
         OI    LCBPRCPG(IEDQLPTR),B'00000010'                      0497
*       RPARM=ADDR(LCBERB);         /* ELEMENT ADDRESS FOR POST      */
         LA    RPARM,LCBERB(,IEDQLPTR)                             0498
*       RDISP=AVTEA;                /* SET DISPATCHER BASE           */
         L     RDISP,AVTEA(,IEDQAPTR)                              0499
*       CALL DSPCHAIN;              /* TPOST ERB                     */
         LA    @15,DSPCHAIN(,RDISP)                                0500
         BALR  @14,@15                                             0500
*                                                                  0501
*       /*************************************************************/
*       /*                                                           */
*       /* THE FOLLOWING GEN BUILDS THE QCB TO WHICH THE ERB IS      */
*       /* TPOSTED AFTER THE BUFFER IS ALLOCATED. THE STCB LINK      */
*       /* POINTER IS THE LAST 3 BYTES OF THE 4 BYTE STCB. THE CODE  */
*       /* IMMEDIATELY FOLLOWING THE STCB GETS CONTROL WHEN THE ERB  */
*       /* IS RETURNED. ENTRY CONDITIONS ARE THEN RESET TO LOOK AS IF*/
*       /* IEDQBD ENTRY AND NON-USER REQUEST. NOTE THAT ALL LOCAL    */
*       /* STORAGE AS WELL AS ALL REGISTERS MUST BE RESTORED IF      */
*       /* NEEDED. THIS IS BECAUSE WHILE THIS INMSG ENTRY IS OFF     */
*       /* GETTING BUFFERS ANOTHER INMSG IN ANOTHER MH COULD BE      */
*       /* CAUSING IEDQRS TO GET CONTROL WHICH WILL RESULT IN LOCAL  */
*       /* STORAGE GETTING CHANGED.                                  */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0501
*       GEN CODE DEFS(BFRQCB) REFS(RBASE);                         0501
ADDRQRS  DC    A(IEDQRS)                IEDQRS BASE ADDRESS
         ORG *-8
         USING *,7
BFRQCB   EQU   *                        BUFFER RETURN QCB ADDRESS
         ORG
         DC    AL1(06)                  STCB VTO
         DC    AL3(*-1)                 STCB POINTER
REENTRY  L     RBASE,ADDRQRS            RESET PROGRAM BASE
         DROP  7
         USING IEDQRS,RBASE
*       RLCB=RPARM-(ADDR(LCBERB)-ADDR(IEDQLCB));/* PLCB ADDRESS      */
         LR    @10,RPARM                                           0502
         SL    @10,@CF02040                                        0502
         LR    RLCB,@10                                            0502
*       LCBERBLK=0;                 /* ZERO POSTLIST POINTER, WE'VE
*                                      ALREADY SENT IT       @YM09027*/
         SLR   @10,@10                                             0503
         STCM  @10,7,LCBERBLK(IEDQLPTR)                            0503
*       GEN(LA    RLCB,0(RLCB));    /* CLEAR HIGH ORDER BYTE         */
         LA    RLCB,0(RLCB)
*       RPRF=LCBERBCH;              /* SA ADDRESS OF ALLOCATED UNIT  */
         L     RPRF,LCBERBCH-1(,IEDQLPTR)                          0505
         LA    RPRF,0(,RPRF)                                       0505
*       LCBERBCH=0;                 /* RESET ERB BUFFER ADDRESS      */
         SLR   @10,@10                                             0506
         STCM  @10,7,LCBERBCH(IEDQLPTR)                            0506
*       LCBDLNKN=ON;                /* MARK ERB AVAILABLE            */
*       LCBPRCPG=OFF;               /* TURN OFF APPLICATION REQUEST
*                                      BIT                           */
         OI    LCBDLNKN(IEDQLPTR),B'00000001'                      0508
         NI    LCBPRCPG(IEDQLPTR),B'11111101'                      0508
*       LCBRCQCB=0;                 /* RESET RETURN QCB ADDRESS      */
         ST    @10,LCBRCQCB(,IEDQLPTR)                             0509
*     END;                                                         0510
*                                                                  0510
*/*                                                                  */
*                                                                  0511
*   R14=LCBLINK;                    /* RESTORE RETURN REG            */
@RC00488 L     R14,LCBLINK-1(,IEDQLPTR)                            0511
         LA    R14,0(,R14)                                         0511
*   RETURN;                                                        0512
@EL00002 DS    0H                                                  0512
@EF00002 DS    0H                                                  0512
@ER00002 BR    @14                                                 0512
*   END ALLOCBUF;                                                  0513
*                                                                  0514
*/********************************************************************/
*/*                                                                  */
*/*  THIS ROUTINE LOCATES THE SENSE BYTES AND VALIDATES THEM.        */
*/*     INPUT:                                                       */
*/*          CMDBYTE - CONTAINS THE DATA FLOW CONTROL COMMAND OR 0   */
*/*                                                                  */
*/********************************************************************/
*                                                                  0514
*CKSNS:                                                            0514
*   PROC OPTIONS(SAVE(R14));                                       0514
CKSNS    ST    @14,@SA00003                                        0514
*   IF RLCB^=SAVTSLCB THEN          /* SSCP FLOW                     */
         L     @10,AVTSAVTP(,IEDQAPTR)                             0515
         C     RLCB,SAVTSLCB(,@10)                                 0515
         BE    @RF00515                                            0515
*     DO;                           /* NO- DO VALIDITY CHECKING      */
*       RPARM=PARM;                 /* ADDR OF PARM LIST             */
         L     RPARM,PARM                                          0517
*       IF PLMASKI=ON THEN          /* ON-NO MASK PRESENT            */
         TM    PLMASKI(RPARM),B'00000001'                          0518
         BNO   @RF00518                                            0518
*         RPARM=RPARM-4;            /* ADJUST PARM LIST PTR          */
         SL    RPARM,@CF00255                                      0519
*       IF PLQAENDX=0 THEN          /* SENSE IN OPTION FIELD         */
@RF00518 CLI   PLQAENDX(RPARM),0                                   0520
         BNE   @RF00520                                            0520
*         DO;                       /* NO                            */
*           IF PLCOUNT=OFF THEN     /* 'COUNT=' SPECIFIED            */
         TM    PLCOUNT,B'10000000'                                 0522
         BNZ   @RF00522                                            0522
*             RPARM=RPARM-8;        /* NO, ADJUST PARM LIST PTR      */
         SL    RPARM,@CF00044                                      0523
*           LCBLUSNS=PLSNS;         /* MOVE SENSE TO LCB             */
@RF00522 MVC   LCBLUSNS(4,IEDQLPTR),PLSNS(RPARM)                   0524
*           CALL SNSER;             /* VALIDATE SENSE                */
         BAL   @14,SNSER                                           0525
*         END;                                                     0526
*       ELSE                        /* SENSE BYTES ARE IN OPTION   0527
*                                      FIELD                         */
*         DO;                                                      0527
         B     @RC00520                                            0527
@RF00520 DS    0H                                                  0528
*           QAEOPNO=PLOPNO;         /* SET OPTION NUMBER IN QAE PARM
*                                      LIST                          */
         IC    @10,PLOPNO(,RPARM)                                  0528
         STC   @10,QAEOPNO                                         0528
*           R15=(AVTMSGS+PLQAENDX)->PTR31;/* IEDQAE(OPTION LOOK UP)
*                                      ADDRESS                       */
         L     @10,AVTMSGS-1(,IEDQAPTR)                            0529
         LA    @10,0(,@10)                                         0529
         SLR   @07,@07                                             0529
         IC    @07,PLQAENDX(,RPARM)                                0529
         ALR   @10,@07                                             0529
         L     R15,PTR31(,@10)                                     0529
*           R2=R13;                 /* SAVE AVT ADDRESS      @YM09013*/
         LR    R2,R13                                              0530
*           R13=ADDR(AVTSAVE4);     /* SAVE AREA FOR IEDQAE  @YM09013*/
         LA    @10,AVTSAVE4(,IEDQAPTR)                             0531
         LR    R13,@10                                             0531
*           RPARM=ADDR(QAEPL);      /* PARM LIST ADDR IN REG1        */
         LA    RPARM,QAEPL                                         0532
*           GEN(BAL  R14,4(R15));   /* GET OPTION FIELD CONTAINING 0533
*                                      SENSE                         */
         BAL  R14,4(R15)
*           R13=R2;                 /* RESTORE AVT ADDRESS   @YM09013*/
         LR    R13,R2                                              0534
*           IF R15=0 THEN           /* GOT OPTION FIELD O.K.         */
         LTR   R15,R15                                             0535
         BNZ   @RF00535                                            0535
*             DO;                   /* YES                           */
*               LCBLUSNS=RPARM->CHAR4;/* MOVE SENSE TO LCB           */
         MVC   LCBLUSNS(4,IEDQLPTR),CHAR4(RPARM)                   0537
*               CALL SNSER;         /* VALIDATE SENSE                */
         BAL   @14,SNSER                                           0538
*             END;                                                 0539
*           ELSE                    /* OPTION NOT FOUND              */
*             RC=INVLOPTN;          /* SET INVALID OPTION RETURN CODE*/
         B     @RC00535                                            0540
@RF00535 MVC   RC(4),@CF00044                                      0540
*         END;                                                     0541
*     END;                                                         0542
*   RETURN;                                                        0543
@EL00003 DS    0H                                                  0543
@EF00003 DS    0H                                                  0543
@ER00003 L     @14,@SA00003                                        0543
         BR    @14                                                 0543
*   END CKSNS;                                                     0544
         B     @EL00003                                            0544
*                                                                  0545
*/********************************************************************/
*/*                                                                  */
*/* THIS ROUTINE HANDLES DEFAULT RESPONSES.                          */
*/*                                                                  */
*/********************************************************************/
*                                                                  0545
*DEFAULT:                                                          0545
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0545
DEFAULT  DS    0H                                                  0546
*   LCBQCBA=R14;                    /* SAVE RETURN ADDRESS           */
         STCM  R14,7,LCBQCBA(IEDQLPTR)                             0546
*   IF LCBRESP=ON THEN              /* RSP STILL NEEDED FOR THIS   0547
*                                      CHAIN                         */
         TM    LCBRESP(IEDQLPTR),B'00000001'                       0547
         BNO   @RF00547                                            0547
*     DO;                           /* YES                           */
*       LCBRESP=OFF;                /* INDICATE RSP NOT NEEDED     0549
*                                      ANYMORE                       */
         NI    LCBRESP(IEDQLPTR),B'11111110'                       0549
*       RHLCB=ADDR(LCBRHSV);        /* ADDR OF RH OF REQUEST WE ARE
*                                      RESPONDING TO                 */
         LA    RHLCB,LCBRHSV(,IEDQLPTR)                            0550
*       IF RHLCB->TRHDR1=ON×        /* A REQUEST ASKING FOR...       */
*           RHLCB->TRHDR2=ON THEN   /* SOME KIND OF RESPONSE         */
         TM    TRHDR1(RHLCB),B'10100000'                           0551
         BZ    @RF00551                                            0551
*         DO;                       /* YES                           */
*           IF RHLCB->TRHRUCAT=TRHDFC THEN/* DATA FLOW CONTROL     0553
*                                      COMMAND                       */
         TM    TRHRUCAT(RHLCB),B'01000000'                         0553
         BNO   @RF00553                                            0553
         TM    TRHRUCAT(RHLCB),B'00100000'                         0553
         BNZ   @RF00553                                            0553
*             CMDBYTE=LCBDFCMD;     /* SET UP COMMAND BYTE           */
         MVC   CMDBYTE(1),LCBDFCMD(IEDQLPTR)                       0554
*           ELSE                    /* NOT DFC                       */
*             CMDBYTE=0;            /* SET COMMAND BYTE TO ZERO      */
         B     @RC00553                                            0555
@RF00553 MVI   CMDBYTE,X'00'                                       0555
*/*                                                                  */
*                                                                  0556
*           IF LCBRSRH=ON THEN      /* HAS A RESPONSE BEEN PREVIOUSLY
*                                      GENERATED BUT NOT SENT        */
@RC00553 TM    LCBRSRH(IEDQLPTR),B'01000000'                       0556
         BNO   @RF00556                                            0556
*             DO;                   /* YES                           */
*               CALL ALLOCBUF;      /* ALLOCATE A BUFFER UNIT        */
         BAL   @14,ALLOCBUF                                        0558
*               EXCPRSP=ON;         /* INDICATE AN EXCEPTION RESPONSE
*                                      IS TO BE SENT                 */
         OI    EXCPRSP,B'10000000'                                 0559
*               CALL SENDRESP;      /* SEND THE GENERATED RESPONSE   */
         BAL   @14,SENDRESP                                        0560
*             END;                                                 0561
*           ELSE                    /* RSP NOT PREVIOUSLY GENERATED  */
*             DO;                                                  0562
         B     @RC00556                                            0562
@RF00556 DS    0H                                                  0563
*               IF CMDBYTE^=CD1CANCL&/* NOT CANCEL COMMAND AND..     */
*                   CMDBYTE^=CD1RTR&/* NOT RTR COMMAND AND...        */
*                   RHLCB->TRHER=OFF THEN/* REQUEST IS ASKING FOR  0563
*                                      DEFINITE RESPONSE             */
         CLI   CMDBYTE,X'83'                                       0563
         BE    @RF00563                                            0563
         CLI   CMDBYTE,X'05'                                       0563
         BE    @RF00563                                            0563
         TM    TRHER(RHLCB),B'00010000'                            0563
         BNZ   @RF00563                                            0563
*                 DO;               /* YES                           */
*                   CALL ALLOCBUF;  /* ALLOCATE A BUFFER UNIT        */
         BAL   @14,ALLOCBUF                                        0565
*                   EXCPRSP=OFF;    /* INDICATE A POSITIVE RESPONSE
*                                      IS TO BE SENT                 */
         NI    EXCPRSP,B'01111111'                                 0566
*                   CALL SENDRESP;  /* SEND A POSITIVE RESPONSE      */
         BAL   @14,SENDRESP                                        0567
*                 END;                                             0568
*             END;                                                 0569
@RF00563 DS    0H                                                  0570
*         END;                                                     0570
@RC00556 DS    0H                                                  0571
*     END;                                                         0571
*                                                                  0571
@RF00551 DS    0H                                                  0572
*/*                                                                  */
*                                                                  0572
*   IF LCBCANXT=ON THEN             /* RESPONSE OWED TO CANCEL       */
@RF00547 TM    LCBCANXT(IEDQLPTR),B'10000000'                      0572
         BNO   @RF00572                                            0572
*     DO;                           /* YES                           */
*       LCBCANXT=OFF;               /* INDICATE RSP SENT TO CANCEL   */
         NI    LCBCANXT(IEDQLPTR),B'01111111'                      0574
*       LCBRSRH=OFF;                /* INDICATE +RSP (FOR ERRORMSG)
*                                                            @YM07705*/
         NI    LCBRSRH(IEDQLPTR),B'10111111'                       0575
*       LCBTHSQ=LCBCANTH;           /* CANCEL SEQUENCE NUMBER        */
         MVC   LCBTHSQ(2,IEDQLPTR),LCBCANTH(IEDQLPTR)              0576
*       LCBRHSV=CANCELRH;           /* CANCEL RH                     */
         MVC   LCBRHSV(3,IEDQLPTR),@CB01934                        0577
*       LCBDFCMD=CD1CANCL;          /* CANCEL COMMAND BYTE           */
         MVI   LCBDFCMD(IEDQLPTR),X'83'                            0578
*       LCBEXPI=OFF;                /* NORMAL FLOW                   */
         NI    LCBEXPI(IEDQLPTR),B'01111111'                       0579
*/*                                                                  */
*                                                                  0580
*       CALL ALLOCBUF;              /* GET BUF UNIT FOR RESPONSE     */
         BAL   @14,ALLOCBUF                                        0580
*       EXCPRSP=OFF;                /* INDICATE POSITIVE RESPONSE IS
*                                      TO BE SENT                    */
         NI    EXCPRSP,B'01111111'                                 0581
*       CALL SENDRESP;              /* SEND A POS. RESPONSE TO CANCEL*/
         BAL   @14,SENDRESP                                        0582
*     END;                                                         0583
*                                                                  0583
*/*                                                                  */
*                                                                  0584
*   R14=LCBQCBA;                    /* RESTORE RETURN ADDRESS        */
@RF00572 L     R14,LCBQCBA-1(,IEDQLPTR)                            0584
         LA    R14,0(,R14)                                         0584
*   RETURN;                                                        0585
@EL00004 DS    0H                                                  0585
@EF00004 DS    0H                                                  0585
@ER00004 BR    @14                                                 0585
*   END DEFAULT;                                                   0586
*                                                                  0587
*/********************************************************************/
*/*                                                                  */
*/* THIS ROUTINE HANDLES PROCESSING FOR IEDRESP MACRO ISSUED IN      */
*/* INMSG SUBGROUP.                                                  */
*/*                                                                  */
*/********************************************************************/
*                                                                  0587
*INMSG:                                                            0587
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0587
INMSG    DS    0H                                                  0588
*   LCBQCBA=R14;                    /* SAVE RETURN ADDRESS           */
         STCM  R14,7,LCBQCBA(IEDQLPTR)                             0588
*/*                                                                  */
*                                                                  0589
*   IF LCBFLAG1=0&                  /* PLCB AND...                   */
*       LCBLUNIT=ON THEN            /* SNA LOGICAL UNIT      @YM08571*/
         CLI   LCBFLAG1(IEDQLPTR),0                                0589
         BNE   @RF00589                                            0589
         TM    LCBLUNIT(IEDQLPTR),B'01000000'                      0589
         BNO   @RF00589                                            0589
*     DO;                           /*                       @YM08571*/
*       IF LCBRESP=ON THEN          /* RESPONSE STILL NEEDED @YM08571*/
         TM    LCBRESP(IEDQLPTR),B'00000001'                       0591
         BNO   @RF00591                                            0591
*         DO;                       /* YES                           */
*           IF LCBRSRH=OFF THEN     /* RSP NOT PREVIOUSLY GENERATED  */
         TM    LCBRSRH(IEDQLPTR),B'01000000'                       0593
         BNZ   @RF00593                                            0593
*             DO;                   /* YES                           */
*               RC=0;               /* INIT RETURN CODE TO ZERO      */
         SLR   @10,@10                                             0595
         ST    @10,RC                                              0595
*               CALL TESTCOND;      /* TEST TO SEE IF IEDRESP SHOULD
*                                      BE EXECUTED                   */
         BAL   @14,TESTCOND                                        0596
*               IF RC=0 THEN        /* IEDRESP SHOULD BE EXECUTED    */
         L     @10,RC                                              0597
         LTR   @10,@10                                             0597
         BNZ   @RF00597                                            0597
*                 DO;               /* YES                           */
*                   RHLCB=ADDR(LCBRHSV);/* ADDR OF RH OF REQUEST WE
*                                      ARE RESPONDING TO             */
         LA    RHLCB,LCBRHSV(,IEDQLPTR)                            0599
*/*                                                                  */
*                                                                  0600
*                   IF RHLCB->TRHDR1=ON×RHLCB->TRHDR2=ON THEN/*    0600
*                                      REQUEST ASKING FOR SOME     0600
*                                      RESPONSE                      */
         TM    TRHDR1(RHLCB),B'10100000'                           0600
         BZ    @RF00600                                            0600
*                     DO;           /* YES                           */
*                       IF RHLCB->TRHRUCAT=TRHDFC THEN/* DATA FLOW 0602
*                                      CTRL.                         */
         TM    TRHRUCAT(RHLCB),B'01000000'                         0602
         BNO   @RF00602                                            0602
         TM    TRHRUCAT(RHLCB),B'00100000'                         0602
         BNZ   @RF00602                                            0602
*                         CMDBYTE=LCBDFCMD;/* YES, SET DFC COMMAND   */
         MVC   CMDBYTE(1),LCBDFCMD(IEDQLPTR)                       0603
*                       ELSE        /* NOT DFC                       */
*                         CMDBYTE=0;/* SET COMMAND TO 0              */
         B     @RC00602                                            0604
@RF00602 MVI   CMDBYTE,X'00'                                       0604
*/*                                                                  */
*                                                                  0605
*                       IF PLSNSI=OFF THEN/* POSITIVE RESPONSE       */
@RC00602 TM    PLSNSI,B'00100000'                                  0605
         BNZ   @RF00605                                            0605
*                         DO;       /* YES                           */
*                           IF CMDBYTE^=CD1RTR&/* NOT RTR (WHEN QRS
*                                      IS ENTERED BEFORE THE 1ST   0607
*                                      MSGGEN MACRO IN INMSG IT IS 0607
*                                      NOT KNOWN WHETHER THE MSGGEN
*                                      WILL BE SENT. THEREFORE A   0607
*                                      POSITIVE RESPONSE TO RTR MUST
*                                      NOT BE SENT)                  */
*                               RHLCB->TRHER=OFF THEN/* REQUEST IS 0607
*                                      ASKING FOR POSITIVE RESPONSE  */
         CLI   CMDBYTE,X'05'                                       0607
         BE    @RF00607                                            0607
         TM    TRHER(RHLCB),B'00010000'                            0607
         BNZ   @RF00607                                            0607
*                             DO;   /* YES                           */
*                               IF CMDBYTE=CD1CANCL&/* CANCEL AND... */
*                                   LCBCANXT=ON THEN/* RESPONSE    0609
*                                      INFORMATION SET UP FOR CANCEL */
         CLI   CMDBYTE,X'83'                                       0609
         BNE   @RF00609                                            0609
         TM    LCBCANXT(IEDQLPTR),B'10000000'                      0609
         BNO   @RF00609                                            0609
*                                 DO;/* YES                          */
*                                   LCBCANXT=OFF;/* RESET RSP NEEDED */
         NI    LCBCANXT(IEDQLPTR),B'01111111'                      0611
*                                   LCBTHSQ=LCBCANTH;/* CANCEL SEQ # */
         MVC   LCBTHSQ(2,IEDQLPTR),LCBCANTH(IEDQLPTR)              0612
*                                   LCBRHSV=CANCELRH;/* CANCEL RH    */
         MVC   LCBRHSV(3,IEDQLPTR),@CB01934                        0613
*                                   LCBDFCMD=CD1CANCL;/* SET DFC   0614
*                                      COMMAND                       */
         MVI   LCBDFCMD(IEDQLPTR),X'83'                            0614
*                                   LCBEXPI=OFF;/* NORMAL FLOW       */
         NI    LCBEXPI(IEDQLPTR),B'01111111'                       0615
*                                 END;                             0616
*                                                                  0616
*/*                                                                  */
*                                                                  0617
*                               CALL ALLOCBUF;/* GET BUFFER UNIT FOR
*                                      RSP                           */
@RF00609 BAL   @14,ALLOCBUF                                        0617
*                               EXCPRSP=OFF;/* INDICATE POSITIVE RSP
*                                      IS TO BE SENT                 */
         NI    EXCPRSP,B'01111111'                                 0618
*                               CALL SENDRESP;/* SEND POSITIVE     0619
*                                      RESPONSE                      */
         BAL   @14,SENDRESP                                        0619
*                             END;                                 0620
*                         END;                                     0621
*                       ELSE        /* EXCEPTION RESPONSE            */
*                         DO;                                      0622
         B     @RC00605                                            0622
@RF00605 DS    0H                                                  0623
*                           CALL CKSNS;/* CHECK FOR VALID SENSE      */
         BAL   @14,CKSNS                                           0623
*                           IF RC=0 THEN/* SENSE VALID               */
         L     @10,RC                                              0624
         LTR   @10,@10                                             0624
         BNZ   @RF00624                                            0624
*                             DO;   /* YES                           */
*                               LCBRSRH=ON;/* INDICATE EXCEPTION   0626
*                                      RESPONSE (FOR ERRORMSG)     0626
*                                                            @YM07705*/
         OI    LCBRSRH(IEDQLPTR),B'01000000'                       0626
*                               CALL ALLOCBUF;/* GET A BUFFER UNIT   */
         BAL   @14,ALLOCBUF                                        0627
*                               EXCPRSP=ON;/* INDICATE AN EXCEPTION
*                                      RESPONSE IS TO BE SENT        */
         OI    EXCPRSP,B'10000000'                                 0628
*                               CALL SENDRESP;/* SEND A -RSP         */
         BAL   @14,SENDRESP                                        0629
*                             END;                                 0630
*                         END;                                     0631
*                     END;                                         0632
*                 END;                                             0633
*             END;                                                 0634
*           ELSE                    /* RESPONSE PREVIOUSLY GENERATED */
*             DO;                                                  0635
         B     @RC00593                                            0635
@RF00593 DS    0H                                                  0636
*               LCBRESP=OFF;        /* INDICATE RESPONSE SENT        */
         NI    LCBRESP(IEDQLPTR),B'11111110'                       0636
*               RHLCB=ADDR(LCBRHSV);/* ADDR OF RH OF REQUEST WE ARE
*                                      RESPONDING TO                 */
         LA    RHLCB,LCBRHSV(,IEDQLPTR)                            0637
*               IF RHLCB->TRHDR1=ON×RHLCB->TRHDR2=ON THEN/* REQUEST
*                                      ASKING FOR SOME RESPONSE    0638
*                                                            @OY20636*/
         TM    TRHDR1(RHLCB),B'10100000'                           0638
         BZ    @RF00638                                            0638
*                 DO;               /* YES                           */
*                   CALL ALLOCBUF;  /* GET A BUFFER UNIT             */
         BAL   @14,ALLOCBUF                                        0640
*                   EXCPRSP=ON;     /* INDICATE AN EXCEPTION RESPONSE
*                                      IS TO BE BUILT                */
         OI    EXCPRSP,B'10000000'                                 0641
*                   CALL SENDRESP;  /* SEND EXCEPTION RESPONSE       */
         BAL   @14,SENDRESP                                        0642
*                 END;                                             0643
*                                                                  0643
*/*                                                                  */
*                                                                  0644
*               IF LCBCANXT=ON THEN /* IS RESPONSE OWED TO CANCEL    */
@RF00638 TM    LCBCANXT(IEDQLPTR),B'10000000'                      0644
         BNO   @RF00644                                            0644
*                 DO;               /* YES                           */
*                   LCBCANXT=OFF;   /* RESET RESPONSE OWED TO CANCEL */
         NI    LCBCANXT(IEDQLPTR),B'01111111'                      0646
*                   LCBRSRH=OFF;    /* INDICATE +RSP(FOR ERRORMSG)   */
         NI    LCBRSRH(IEDQLPTR),B'10111111'                       0647
*                   LCBTHSQ=LCBCANTH;/* CANCEL SEQUENCE NUMBER       */
         MVC   LCBTHSQ(2,IEDQLPTR),LCBCANTH(IEDQLPTR)              0648
*                   LCBRHSV=CANCELRH;/* CANCEL REPSONSE RH           */
         MVC   LCBRHSV(3,IEDQLPTR),@CB01934                        0649
*                   LCBDFCMD=CD1CANCL;/* CANCEL COMMAND CODE         */
         MVI   LCBDFCMD(IEDQLPTR),X'83'                            0650
*                   LCBEXPI=OFF;    /* 'NORMAL' FLOW                 */
         NI    LCBEXPI(IEDQLPTR),B'01111111'                       0651
*                   CALL ALLOCBUF;  /* GET A BUFFER UNIT             */
         BAL   @14,ALLOCBUF                                        0652
*                   EXCPRSP=OFF;    /* INDICATE POSITIVE RESPONSE IS
*                                      TO BE BUILT                   */
         NI    EXCPRSP,B'01111111'                                 0653
*                   CALL SENDRESP;  /* SEND +RSP TO CANCEL           */
         BAL   @14,SENDRESP                                        0654
*                 END;                                             0655
*             END;                                                 0656
*         END;                                                     0657
*       ELSE                        /* RESPONSE TO FM DATA ALREADY 0658
*                                      SENT                  @YM08571*/
*         DO;                       /*                       @YM08571*/
         B     @RC00591                                            0658
@RF00591 DS    0H                                                  0659
*           IF LCBCANXT=ON THEN     /* IS RSP OWED TO CANCEL @YM08571*/
         TM    LCBCANXT(IEDQLPTR),B'10000000'                      0659
         BNO   @RF00659                                            0659
*             DO;                   /* YES                   @YM08571*/
*               LCBCANXT=OFF;       /* RESET RSP OWED TO CANCEL    0661
*                                                            @YM08571*/
         NI    LCBCANXT(IEDQLPTR),B'01111111'                      0661
*               LCBRSRH=OFF;        /* INDICATE +RSP(FOR ERRORMSG) 0662
*                                                            @YM08571*/
         NI    LCBRSRH(IEDQLPTR),B'10111111'                       0662
*               LCBTHSQ=LCBCANTH;   /* CANCEL SEQUENCE NUMBER      0663
*                                                            @YM08571*/
         MVC   LCBTHSQ(2,IEDQLPTR),LCBCANTH(IEDQLPTR)              0663
*               LCBRHSV=CANCELRH;   /* CANCEL REPSONSE RH    @YM08571*/
         MVC   LCBRHSV(3,IEDQLPTR),@CB01934                        0664
*               LCBDFCMD=CD1CANCL;  /* CANCEL COMMAND CODE   @YM08571*/
         MVI   LCBDFCMD(IEDQLPTR),X'83'                            0665
*               LCBEXPI=OFF;        /* 'NORMAL' FLOW         @YM08571*/
         NI    LCBEXPI(IEDQLPTR),B'01111111'                       0666
*               CALL ALLOCBUF;      /* GET A BUFFER UNIT     @YM08571*/
         BAL   @14,ALLOCBUF                                        0667
*               EXCPRSP=OFF;        /* INDICATE POSITIVE RESPONSE IS
*                                      TO BE BUILT           @YM08571*/
         NI    EXCPRSP,B'01111111'                                 0668
*               CALL SENDRESP;      /* SEND +RSP TO CANCEL   @YM08571*/
         BAL   @14,SENDRESP                                        0669
*             END;                  /*                       @YM08571*/
*         END;                      /*                       @YM08571*/
@RF00659 DS    0H                                                  0672
*     END;                          /*                       @YM08571*/
@RC00591 DS    0H                                                  0673
*/*                                                                  */
*                                                                  0673
*   R14=LCBQCBA;                    /* RESTORE RETURN ADDRESS        */
@RF00589 L     R14,LCBQCBA-1(,IEDQLPTR)                            0673
         LA    R14,0(,R14)                                         0673
*   RETURN;                                                        0674
@EL00005 DS    0H                                                  0674
@EF00005 DS    0H                                                  0674
@ER00005 BR    @14                                                 0674
*   END INMSG;                                                     0675
*                                                                  0676
*/********************************************************************/
*/*                                                                  */
*/* THIS ROUTINE GETS CONTROL ON ENTRY FOR IEDQA4 TO SEND PREVIOUSLY */
*/* GENERATED RESPONSES.                                             */
*/*                                                                  */
*/********************************************************************/
*                                                                  0676
*QA4ENTRY:                                                         0676
*   PROC OPTIONS(SAVE(R14));                                       0676
QA4ENTRY ST    @14,@SA00006                                        0676
*   RHLCB=ADDR(LCBRHSV);            /* ADDR OF RH OF REQUEST WE ARE
*                                      RESPONDING TO                 */
         LA    RHLCB,LCBRHSV(,IEDQLPTR)                            0677
*   IF RHLCB->TRHDR1=OFF&RHLCB->TRHDR2=OFF THEN/* REQUEST ASKING FOR
*                                      NO RESPONSE                   */
         TM    TRHDR1(RHLCB),B'10100000'                           0678
         BNZ   @RF00678                                            0678
*     LCBRESP=OFF;                  /* YES, INDICATE RSP NO LONGER 0679
*                                      NEEDED FOR THIS CHAIN         */
         NI    LCBRESP(IEDQLPTR),B'11111110'                       0679
*   ELSE                            /* RESPONSE IS REQUESTED         */
*     DO;                                                          0680
         B     @RC00678                                            0680
@RF00678 DS    0H                                                  0681
*       R0=1;                       /* ONE UNIT PER BUFFER           */
         LA    @10,1                                               0681
         LR    R0,@10                                              0681
*       RPARM=1;                    /* ONE BUFFER                    */
         LR    RPARM,@10                                           0682
*       CALL IEDQGA02;              /* TRY TO STEAL A BUFFER         */
         L     @15,AVTSTEAL(,IEDQAPTR)                             0683
         BALR  @14,@15                                             0683
*       IF R15^=0 THEN              /* SUCCESSFUL                    */
         LTR   R15,R15                                             0684
         BZ    @RF00684                                            0684
*         DO;                       /* YES                           */
*           RPRF=R15;               /* SET BUFFER ADDRESS            */
         LR    RPRF,R15                                            0686
*           EXCPRSP=ON;             /* INDICATE AN EXCEPTION RESPONSE
*                                      IS TO BE SENT                 */
         OI    EXCPRSP,B'10000000'                                 0687
*           CALL SENDRESP;          /* SEND A -RSP                   */
         BAL   @14,SENDRESP                                        0688
*         END;                                                     0689
*     END;                                                         0690
*   RETURN;                                                        0691
@EL00006 DS    0H                                                  0691
@EF00006 DS    0H                                                  0691
@ER00006 L     @14,@SA00006                                        0691
         BR    @14                                                 0691
*   END QA4ENTRY;                                                  0692
         B     @EL00006                                            0692
*                                                                  0693
*/********************************************************************/
*/*                                                                  */
*/* THIS ROUTINE DOES PROCESSING FOR IEDRESP MACRO ISSUED IN INHDR OR*/
*/* IN BUF SUBGOUPS.                                                 */
*/*                                                                  */
*/********************************************************************/
*                                                                  0693
*QUIENTRY:                                                         0693
*   PROC OPTIONS(SAVE(R14));                                       0693
QUIENTRY ST    @14,@SA00007                                        0693
*   IF LCBFLAG1=0&                  /* PLCB AND...                   */
*       LCBLUNIT=ON THEN            /* SNA LOGOCAL UNIT              */
         CLI   LCBFLAG1(IEDQLPTR),0                                0694
         BNE   @RF00694                                            0694
         TM    LCBLUNIT(IEDQLPTR),B'01000000'                      0694
         BNO   @RF00694                                            0694
*     DO;                           /* YES                           */
*       IF LCBRESP=ON&              /* RSP STILL NEEDED AND ...      */
*           LCBRSRH=OFF THEN        /* RSP NOT PREVIOUSLY GENERATED  */
         TM    LCBRESP(IEDQLPTR),B'00000001'                       0696
         BNO   @RF00696                                            0696
         TM    LCBRSRH(IEDQLPTR),B'01000000'                       0696
         BNZ   @RF00696                                            0696
*         DO;                       /* YES                           */
*           RC=0;                   /* INIT RETURN CODE TO ZERO      */
         SLR   @10,@10                                             0698
         ST    @10,RC                                              0698
*           CALL TESTCOND;          /* TEST CONDITIONAL EXECUTION    */
         BAL   @14,TESTCOND                                        0699
*           IF RC=0 THEN            /* SHOULD WE EXECUTE             */
         L     @10,RC                                              0700
         LTR   @10,@10                                             0700
         BNZ   @RF00700                                            0700
*             DO;                   /* YES                           */
*               RPRF=AVTADBUF;      /* BUFFER ADDRESS                */
         L     RPRF,AVTADBUF(,IEDQAPTR)                            0702
*               RHPRF1=ADDR(PRF1RH);/* RH ADDRESS                    */
         LR    RHPRF1,IEDQPPTR                                     0703
         SL    RHPRF1,@CF00044                                     0703
*               IF RHPRF1->TRHDR1=OFF&RHPRF1->TRHDR2=OFF THEN/*    0704
*                                      REQUEST ASKING FOR NO RESPONSE*/
         TM    TRHDR1(RHPRF1),B'10100000'                          0704
         BNZ   @RF00704                                            0704
*                 RC=CANTRESP;      /* YES, SET CAN'T RESPOND RET  0705
*                                      CODE                          */
         MVC   RC(4),@CF01939                                      0705
*               ELSE                /* REQUEST ASKING FOR A RESPONSE */
*                 DO;                                              0706
         B     @RC00704                                            0706
@RF00704 DS    0H                                                  0707
*                   IF RHPRF1->TRHRUCAT=TRHDFC THEN/* DATA FLOW    0707
*                                      CONTROL                       */
         TM    TRHRUCAT(RHPRF1),B'01000000'                        0707
         BNO   @RF00707                                            0707
         TM    TRHRUCAT(RHPRF1),B'00100000'                        0707
         BNZ   @RF00707                                            0707
*                     CMDBYTE=LCBDFCMD;/* YES, SET UP DFC COMMAND  0708
*                                      BYTE                          */
         MVC   CMDBYTE(1),LCBDFCMD(IEDQLPTR)                       0708
*                   ELSE            /* NOT DFC                       */
*                     CMDBYTE=0;    /* SET COMMAND BYTE TO ZERO      */
         B     @RC00707                                            0709
@RF00707 MVI   CMDBYTE,X'00'                                       0709
*/*                                                                  */
*                                                                  0710
*                   CALL CKSNS;     /* VALIDATE SENSE                */
@RC00707 BAL   @14,CKSNS                                           0710
*                   IF RC=0 THEN    /* SENSE O.K.                    */
         L     @10,RC                                              0711
         LTR   @10,@10                                             0711
         BNZ   @RF00711                                            0711
*                     CALL SETRINFO;/* GENERATE RESPONSE INFORMATION */
         BAL   @14,SETRINFO                                        0712
*                 END;                                             0713
*             END;                                                 0714
*         END;                                                     0715
*       ELSE                        /* RESPONSE ALREADY SENT OR    0716
*                                      GENERATED                     */
*         RC=RESPSENT;              /* SET RETURN CODE FOR RESPONSE
*                                      ALREADY SENT                  */
         B     @RC00696                                            0716
@RF00696 MVC   RC(4),@CF00174                                      0716
*     END;                                                         0717
*   ELSE                            /* NOT PLCB OR NOT SNA LU        */
*     RC=NOTSNALU;                  /* SET RETURN CODE               */
         B     @RC00694                                            0718
@RF00694 MVC   RC(4),@CF00075                                      0718
*/*                                                                  */
*                                                                  0719
*   RETURN;                                                        0719
@EL00007 DS    0H                                                  0719
@EF00007 DS    0H                                                  0719
@ER00007 L     @14,@SA00007                                        0719
         BR    @14                                                 0719
*   END QUIENTRY;                                                  0720
         B     @EL00007                                            0720
*                                                                  0721
*/********************************************************************/
*/*                                                                  */
*/* THIS ROUTINE BUILDS A RSP PIU AND POSTS IT TO IEDDFOUT(DATA FLOW */
*/* CONTROL OUTBOUND).                                               */
*/*     INPUT-                                                       */
*/*          EXCPRSP -  ON MEANS AN EXCEPTION RESPONSE IS TO BE BUILT*/
*/*                     OFF MEANS A POSITIVE RESPONSE IS TO BE BUILT */
*/*          LCB     -  ALL NECESSARY RESPONSE INFORMATION MUST BE   */
*/*                     SET UP IN LCB                                */
*/*          RPRF    -  MUST CONTAIN ADDRESS OF BUFFER UNIT INTO     */
*/*                     WHICH RESPONSE WILL BE BUILT                 */
*/*                                                                  */
*/********************************************************************/
*                                                                  0721
*SENDRESP:                                                         0721
*   PROC OPTIONS(SAVE(R14));                                       0721
SENDRESP ST    @14,@SA00008                                        0721
*   IEDPF2=''B;                     /* INIT PREFIX TO ZERO           */
         LR    @10,IEDQPPTR                                        0722
         SL    @10,@CF00044                                        0722
         XC    IEDPF2(8,@10),IEDPF2(@10)                           0722
*   PRF2TNTI=ON;                    /* INDICATE OAF AND DAF ARE TNT
*                                      OFFSETS                       */
         OI    PRF2TNTI(@10),B'10000000'                           0723
*   PRFPIUO=1;                      /* SET PIU OFFSET VALUE TO 1     */
         LA    @10,1                                               0724
         STC   @10,PRFPIUO(,IEDQPPTR)                              0724
*   PIUPTR=ADDR(PRFPIUO)+PRFPIUO;   /* PIU ADDRESSABILITY            */
         LA    PIUPTR,PRFPIUO(,IEDQPPTR)                           0725
         ALR   PIUPTR,@10                                          0725
*   IEDPIU=''B;                     /* INIT PIU TO ZERO              */
         XC    IEDPIU(13,PIUPTR),IEDPIU(PIUPTR)                    0726
*   TTHFTYPE=TTHFID1;               /* FID1 TH TYPE                  */
         NI    TTHFTYPE(PIUPTR),B'00011111'                        0727
         OI    TTHFTYPE(PIUPTR),B'00010000'                        0727
*   TTHBBIU=ON;                     /* BEGINNING OF BIU AND ...      */
*   TTHEBIU=ON;                     /* END OF BIU                    */
         OI    TTHBBIU(PIUPTR),B'00001100'                         0729
*   IF LCBEXPI=ON THEN              /* EXPIDITED FLOW                */
         TM    LCBEXPI(IEDQLPTR),B'10000000'                       0730
         BNO   @RF00730                                            0730
*     TTHEFI=ON;                    /* YES, MARK TH EXPIDITED        */
         OI    TTHEFI(PIUPTR),B'00000001'                          0731
*   TTHDAF=LCBTTCIN;                /* SET DESTINATION TNT OFFSET    */
@RF00730 LH    @10,LCBTTCIN(,IEDQLPTR)                             0732
         N     @10,@CF02057                                        0732
         STCM  @10,3,TTHDAF(PIUPTR)                                0732
*/*                                                                  */
*                                                                  0733
*   RPARM=LCBTTCIN;                 /* TNT OFFSET                    */
         LR    RPARM,@10                                           0733
*   CALL IEDQTNT;                   /* GET DESTINATION'S TERMINAL  0734
*                                      ENTRY                         */
         L     @15,AVTRNMPT(,IEDQAPTR)                             0734
         BALR  @14,@15                                             0734
*   RTRM=RPARM;                     /* TRM ADDRESSABILITY            */
         LR    RTRM,RPARM                                          0735
*   IF RLCB=SAVTSLCB THEN           /* SSCP FLOW             @YM08151*/
         L     @10,AVTSAVTP(,IEDQAPTR)                             0736
         C     RLCB,SAVTSLCB(,@10)                                 0736
         BNE   @RF00736                                            0736
*     TTHOAF=SAVTSCPT;              /* YES - SET OAF=SSCP    @YM08151*/
         MVC   TTHOAF(2,PIUPTR),SAVTSCPT(@10)                      0737
*   ELSE                            /* NOT SSCP FLOW         @YM08151*/
*     TTHOAF=SIBINDEX;              /* SET OAF TNT OFFSET FROM LU-LU
*                                      SIB                   @YM08151*/
         B     @RC00736                                            0738
@RF00736 LR    @10,IEDQTPTR                                        0738
         AL    @10,@CF02058                                        0738
         SLR   @07,@07                                             0738
         ICM   @07,7,TRMSIBPT-7(@10)                               0738
         MVC   TTHOAF(2,PIUPTR),SIBINDEX(@07)                      0738
*/*                                                                  */
*                                                                  0739
*   TTHSNF=LCBTHSQ;                 /* SET SEQUENCE NUMBER(OR EXP ID)*/
@RC00736 MVC   TTHSNF(2,PIUPTR),LCBTHSQ(IEDQLPTR)                  0739
*   IEDRH=LCBRHSV;                  /* THE REQUEST RH WHICH WE ARE 0740
*                                      RESPONDING TO                 */
         LA    @10,10                                              0740
         ALR   @10,PIUPTR                                          0740
         MVC   IEDRH(3,@10),LCBRHSV(IEDQLPTR)                      0740
*   TRHTYPE=ON;                     /* SET RESPONSE INDICATOR        */
*   TRHBC=ON;                       /* BEGIN CHAIN AND ...           */
*   TRHEC=ON;                       /* END CHAIN                     */
         OI    TRHTYPE(@10),B'10000011'                            0743
*/*                                                                  */
*                                                                  0744
*   CALL SETRUDCF;                  /* SET UP THE RU AND DATA COUNT
*                                      FIELD FOR BOTH POSITIVE AND 0744
*                                      EXCEPTION RESPONSES           */
         BAL   @14,SETRUDCF                                        0744
*   IF TRHRUCAT=TRHDFC&             /* DATA FLOW CONTROL AND...      */
*       LCBDFCMD=CD1RTR THEN        /* RTR COMMAND                   */
         TM    TRHRUCAT+10(PIUPTR),B'01000000'                     0745
         BNO   @RF00745                                            0745
         TM    TRHRUCAT+10(PIUPTR),B'00100000'                     0745
         BNZ   @RF00745                                            0745
         CLI   LCBDFCMD(IEDQLPTR),X'05'                            0745
         BNE   @RF00745                                            0745
*     LCBRTRRO=OFF;                 /* RESET RESPONSE OWED TO RTR SO
*                                      SCHEDULERS WON'T TRY TO SEND
*                                      ONE                           */
         NI    LCBRTRRO(IEDQLPTR),B'11111011'                      0746
*/*                                                                  */
*                                                                  0747
*   PRFPRI=PRIOUTBD;                /* POST PRIORITY                 */
@RF00745 MVI   PRFPRI(IEDQPPTR),X'E4'                              0747
*   PRFQCBA=SAVTDFCO;               /* IEDDFOUT'S QCB                */
         L     @10,AVTSAVTP(,IEDQAPTR)                             0748
         MVC   PRFQCBA(3,IEDQPPTR),SAVTDFCO+1(@10)                 0748
*   RDISP=AVTEA;                    /* DISPATCHER ADDRESS            */
         L     RDISP,AVTEA(,IEDQAPTR)                              0749
*   RPARM=RPRF;                     /* ADDR OF RESPONSE PIU BUFFER   */
         LR    RPARM,RPRF                                          0750
*   CALL DSPPOSTR;                  /* POST RSP TO IEDDFOUT QCB      */
         LA    @15,DSPPOSTR(,RDISP)                                0751
         BALR  @14,@15                                             0751
*   LCBRESP=OFF;                    /* INDICATE RESPONSE IS NO LONGER
*                                      NEEDED FOR THIS CHAIN         */
         NI    LCBRESP(IEDQLPTR),B'11111110'                       0752
*   RETURN;                                                        0753
