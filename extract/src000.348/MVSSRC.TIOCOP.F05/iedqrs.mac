*/********************************************************************/
*/*                                                                  */
*/* THIS ROUTINE VALIDATES THE SENSE BYTES(IN LCBLUSNS).             */
*/*    INPUT:                                                        */
*/*         CMDBYTE - CONTAINS THE DATA FLOW CONTROL COMMAND OR 0(FOR*/
*/*                   FM DATA)                                       */
*/*         LCBLUSNS- CONTAINS THE 4 BYTES OF SNA SENSE              */
*/*                                                                  */
*/********************************************************************/
*                                                                  0824
*SNSER:                                                            0824
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0824
SNSER    DS    0H                                                  0825
*   IF CMDBYTE=CD1CANCL THEN        /* CANCEL COMMAND                */
         CLI   CMDBYTE,X'83'                                       0825
         BNE   @RF00825                                            0825
*     RC=CANTRESP;                  /* SET 'NOT ALLOWED TO RESPOND'
*                                      CODE                          */
         MVC   RC(4),@CF01939                                      0826
*   ELSE                            /* NOT CANCEL                    */
*     DO;                                                          0827
         B     @RC00825                                            0827
@RF00825 DS    0H                                                  0828
*       SNSPTR=ADDR(LCBLUSNS);      /* ADDRESS OF SENSE              */
         LA    SNSPTR,LCBLUSNS(,IEDQLPTR)                          0828
*       R2=ADDR(SNSCODES);          /* ADDR OF ALLOWABLE MAJOR SENSE
*                                      CODES                         */
         LA    R2,SNSCODES                                         0829
*       FOUNDSW=OFF;                /* INIT FOUND SWITCH TO OFF      */
         NI    FOUNDSW,B'10111111'                                 0830
*       DO WHILE(R2->BIT8^='FF'X&   /* NOT END OF TABLE AND...       */
*             FOUNDSW=OFF);         /* MAJOR SENSE CODE NOT FOUND YET*/
         B     @DE00831                                            0831
@DL00831 DS    0H                                                  0832
*         IF R2->BIT8=SNSMAJ THEN   /* SENSE CODE FOUND IN TABLE     */
         CLC   BIT8(1,R2),SNSMAJ(SNSPTR)                           0832
         BNE   @RF00832                                            0832
*           FOUNDSW=ON;             /* YES, SET FOUND SWITCH         */
         OI    FOUNDSW,B'01000000'                                 0833
*         ELSE                      /* NOT FOUND                     */
*           R2=R2+1;                /* INCREMENT TO NEXT TABLE ENTRY */
         B     @RC00832                                            0834
@RF00832 AL    R2,@CF00060                                         0834
*       END;                                                       0835
*                                                                  0835
@RC00832 DS    0H                                                  0835
@DE00831 CLI   BIT8(R2),X'FF'                                      0835
         BE    @DC00831                                            0835
         TM    FOUNDSW,B'01000000'                                 0835
         BZ    @DL00831                                            0835
@DC00831 DS    0H                                                  0836
*/*                                                                  */
*                                                                  0836
*       IF FOUNDSW=OFF THEN         /* MAJOR SENSE CODE FOUND IN   0836
*                                      TABLE                         */
         TM    FOUNDSW,B'01000000'                                 0836
         BNZ   @RF00836                                            0836
*         RC=INVLRESP;              /* NO, SET 'INVALID SENSE' CODE  */
         MVC   RC(4),@CF00244                                      0837
*     END;                                                         0838
*   RETURN;                                                        0839
@EL00011 DS    0H                                                  0839
@EF00011 DS    0H                                                  0839
@ER00011 BR    @14                                                 0839
*   END SNSER;                                                     0840
         B     @EL00011                                            0840
