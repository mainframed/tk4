*/*                                                                  */
*/* THIS ROUTINE TEST FOR CONDITIONAL EXECUTION.  THE 'COUNT='       */
*/* OPERAND ON THE IEDRESP MACRO IS USED TO CONTROL EXECUTION.       */
*/*                                                                  */
*/********************************************************************/
*                                                                  0841
*TESTCOND:                                                         0841
*   PROC OPTIONS(SAVE(R14));                                       0841
TESTCOND ST    @14,@SA00012                                        0841
*   RPARM=PARM;                     /* ADDR OF PARM LIST             */
         L     RPARM,PARM                                          0842
*   IF PLMASKI=ON THEN              /* MASK PRESENT                  */
         TM    PLMASKI(RPARM),B'00000001'                          0843
         BNO   @RF00843                                            0843
*     RPARM=RPARM-4;                /* NO, ADJUST PARM LIST PTR      */
         SL    RPARM,@CF00255                                      0844
*   PARMFLAG=PLFLAGS;               /* GET FLAG BYTE                 */
@RF00843 MVC   PARMFLAG(1),PLFLAGS(RPARM)                          0845
*   IF PLCOUNT=ON THEN              /* 'COUNT=' SPECIFIED            */
         TM    PLCOUNT,B'10000000'                                 0846
         BNO   @RF00846                                            0846
*     DO;                           /* YES                           */
*       R2=RPARM;                   /* SAVE PARMLIST POINTER         */
         LR    R2,RPARM                                            0848
*       RPARM=LCBTTCIN;             /* TNT OFFSET                    */
         LH    RPARM,LCBTTCIN(,IEDQLPTR)                           0849
         N     RPARM,@CF02057                                      0849
*       CALL IEDQTNT;               /* GET TERMINAL ENTRY            */
         L     @15,AVTRNMPT(,IEDQAPTR)                             0850
         BALR  @14,@15                                             0850
*       RTRM=RPARM;                 /* SET TRM ADDRESSABILITY        */
         LR    RTRM,RPARM                                          0851
*       RPARM=R2;                   /* RESTORE PARMLIST POINTER      */
         LR    RPARM,R2                                            0852
*/*                                                                  */
*                                                                  0853
*       STM(R0,RBASE,SAVEREGS);     /* SAVE REGS                     */
         STM   R0,RBASE,SAVEREGS                                   0853
*       R2=ADDR(PLQBJPL);           /* IEDQBJ'S PARM LIST ADDR       */
         LA    R2,PLQBJPL(,RPARM)                                  0854
*       R15=(AVTMSGS+PLQBJNDX)->PTR31;/* ADDRESS OF IEDQBJ           */
         L     @10,AVTMSGS-1(,IEDQAPTR)                            0855
         LA    @10,0(,@10)                                         0855
         SLR   @07,@07                                             0855
         IC    @07,PLQBJNDX(,RPARM)                                0855
         ALR   @10,@07                                             0855
         L     R15,PTR31(,@10)                                     0855
*       RPARM=RTRM;                 /* TERMINAL ENTRY                */
         LR    RPARM,RTRM                                          0856
*       GEN(BALR  R14,R15);         /* BALR TO IEDQBJ TO CHECK FOR 0857
*                                      CONDITIONAL EXECUTION         */
         BALR  R14,R15
*       RC=R15;                     /* SET RETURN CODE... POSSIBLE 0858
*                                      RETURN CODES ARE: 0-MEANS   0858
*                                      EXECUTE, 4-MEANS DO NOT     0858
*                                      EXECUTE, 8-MEANS OPTION FIELD
*                                      NOT FOUND                     */
         ST    R15,RC                                              0858
*       LM(R0,RBASE,SAVEREGS);      /* RESTORE REGS                  */
         LM    R0,RBASE,SAVEREGS                                   0859
*     END;                                                         0860
*   RETURN;                                                        0861
@EL00012 DS    0H                                                  0861
@EF00012 DS    0H                                                  0861
@ER00012 L     @14,@SA00012                                        0861
         BR    @14                                                 0861
*   END TESTCOND;                                                  0862
         B     @EL00012                                            0862
