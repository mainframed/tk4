         TITLE 'IGF2503D, DDR - SWAP COMMAND PROCESSOR                 *
                       '
IGF2503D CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'IGF2503D  73.318'                                 0001
         DROP  @15
@PROLOG  BALR  @08,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@08                                         0001
         L     @00,@SIZDATD                                        0001
         GETMAIN R,LV=(0)
         LR    @09,@01                                             0001
         USING @DATD,@09                                           0001
         ST    @13,@SA00001+4                                      0001
         LR    @13,@09                                             0001
*                                      INTERFACE TO IGFDV1           */
*   SRTNADDR=RTNADDR;               /* SAVE RETURN ADDRESS - THIS IS
*                                      NECESSARY SINCE REGISTERS WERE
*                                      NOT AUTOMATICALLY SAVED BY PLS
*                                      BECAUSE NO SAVE AREA POINTER
*                                      IS PASSED TO THIS MODULE      */
         LR    SRTNADDR,RTNADDR                                    0135
*   DDRPTR=ADDR(LDDRCOM);           /* POINT TO LOCAL DDRCOM         */
         LA    DDRPTR,LDDRCOM                                      0136
*   LDDRCOM=LDDRCOM&&LDDRCOM;       /* CLEAR LOCAL DDRCOM            */
         XC    LDDRCOM(100),LDDRCOM                                0137
*   SAVEXSA=XSAPTR;                 /* SAVE XSA ADDRESS              */
         ST    XSAPTR,SAVEXSA                                      0138
*   DO;                             /* ?IGFDERR INIT                 */
*                                                                  0139
*     /***************************************************************/
*     /*                                                             */
*     /* IGFDERR - INITIALIZE RECOVERY ENVIRONMENT                   */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0140
*     DERPLIST=DERPLIST&&DERPLIST;  /* CLEAR PARAMETER LIST          */
         XC    DERPLIST(48),DERPLIST                               0140
*     DERFUNK=DERINIT;              /* INDICATE INIT FUNCTION        */
         MVI   DERFUNK,X'01'                                       0141
*     DERREC=ADDR(DERRECPM);        /* POINT TO REC PARMS            */
         LA    @12,DERRECPM                                        0142
         ST    @12,DERREC                                          0142
*     DERRETRY=ADDR(IGF2503R);      /* POINT TO RETRY ROUTINE        */
         LA    @12,IGF2503R                                        0143
         ST    @12,DERRETRY                                        0143
*     GEN SETS(DERDATD)(MVC DERDATD,@SIZDATD);/* SET UP SPN, SIZE    */
         MVC DERDATD,@SIZDATD
*     DERPFX=DERRMASK;              /* SET UP RUB MASK               */
         MVC   DERPFX(2),@CB00750                                  0145
*     DERRCODE=CODEBASE;            /* SAVE CODEREG                  */
         ST    CODEBASE,DERRCODE                                   0146
*     DERRDATA=DATABASE;            /* SAVE DATAREG                  */
         ST    DATABASE,DERRDATA                                   0147
*     DERRDDR=DDRPTR;               /* SAVE DDRCOM POINTER           */
         ST    DDRPTR,DERRDDR                                      0148
*     DERRSAVE=SAVEPTR;             /* SAVE REGISTER 13              */
         ST    SAVEPTR,DERRSAVE                                    0149
*     CALL IGFDE0(DERPLIST);        /* ISSUE ESTAE                   */
         LA    @12,DERPLIST                                        0150
         ST    @12,@AL00001                                        0150
         L     @15,@CV00080                                        0150
         LA    @01,@AL00001                                        0150
         BALR  @14,@15                                             0150
*   END;                            /* ?IGFDERR INIT                 */
*   DERLRC=INVALID;                 /* INDICATE PARAMETER ERROR      */
*                                                                  0152
         MVI   DERLRC,X'04'                                        0152
*   /*****************************************************************/
*   /*                                                               */
*   /* THERE MUST BE AN OPERAND ON THE SWAP COMMAND - IF SO, PROCESS */
*   /* IT - IF NOT, THE SWAP COMMAND IS INVALID                      */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0153
*   IF XAL=0 THEN                   /* IS THERE AN OPERAND           */
         ICM   @12,7,XAL(XSAPTR)                                   0153
         BNZ   @RF00153                                            0153
*     XAE=PARMMSNG;                 /* NO, INDICATE PARAMETER MISSING*/
         MVI   XAE(XSAPTR),X'0B'                                   0154
*   ELSE                            /* YES, PROCESS COMMAND          */
*     IF CTEXT(1:2)='ON' THEN       /* IS IT 'SWAP ON'               */
         B     @RC00153                                            0155
@RF00153 SR    @12,@12                                             0155
         ICM   @12,7,XAL(XSAPTR)                                   0155
         CLC   CTEXT(2,@12),@CC01615                               0155
         BNE   @RF00155                                            0155
*       CALL IGFON;                 /* YES, ACTIVATE DDR             */
         BAL   @14,IGFON                                           0156
*     ELSE                          /* NO, TRY FOR OTHER OPERANDS    */
*       IF CTEXT(1:3)='OFF' THEN    /* IS IT 'SWAP OFF'              */
         B     @RC00155                                            0157
@RF00155 SR    @12,@12                                             0157
         ICM   @12,7,XAL(XSAPTR)                                   0157
         CLC   CTEXT(3,@12),@CC01617                               0157
         BNE   @RF00157                                            0157
*         CALL IGFOFF;              /* YES, DEACTIVATE DDR           */
         BAL   @14,IGFOFF                                          0158
*       ELSE                        /* NO, MUST BE 'SWAP XXX,YYY'    */
*         CALL IGFSWAP;             /* VALIDATE SWAP REQUEST         */
         B     @RC00157                                            0159
@RF00157 BAL   @14,IGFSWAP                                         0159
*IGF2503R:                          /* ESTAE RETRY ROUTINE ENTRY PT  */
*   DO;                             /* ?IGFDERR CANCEL               */
*                                                                  0160
@RC00157 DS    0H                                                  0160
@RC00155 DS    0H                                                  0160
@RC00153 DS    0H                                                  0160
IGF2503R DS    0H                                                  0161
*     /***************************************************************/
*     /*                                                             */
*     /* IGFDERR - CANCEL RECOVERY ENVIRONMENT                       */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0161
*     DERFUNK=DERCANC;              /* INDICATE CANCEL               */
         MVI   DERFUNK,X'02'                                       0161
*     CALL IGFDE0(DERPLIST);        /* CANCEL ESTAE                  */
         LA    @12,DERPLIST                                        0162
         ST    @12,@AL00001                                        0162
         L     @15,@CV00080                                        0162
         LA    @01,@AL00001                                        0162
         BALR  @14,@15                                             0162
*   END;                            /* ?IGFDERR CANCEL               */
*   XSAPTR=SAVEXSA;                 /* REFRESH XSA ADDRESS           */
         L     XSAPTR,SAVEXSA                                      0164
*   IF DERLRC=ERROR THEN            /* IF THE ESTAE ROUTINE WAS    0165
*                                      ENTERED, THE SWAP COMMAND MUST
*                                      BE TERMINATED                 */
         CLI   DERLRC,12                                           0165
         BNE   @RF00165                                            0165
*     DO;                           /* ESTAE WAS ENTERED             */
*       XAE=COMABORT;               /* INDICATE COMMAND ABORTED      */
         MVI   XAE(XSAPTR),X'3A'                                   0167
*       XAV='SWAP';                 /* INDICATE SWAP COMMAND         */
         MVI   XAV+4(XSAPTR),C' '                                  0168
         MVC   XAV+5(3,XSAPTR),XAV+4(XSAPTR)                       0168
         MVC   XAV(4,XSAPTR),@CC01621                              0168
*     END;                                                         0169
*                                                                  0169
*   /*****************************************************************/
*   /*                                                               */
*   /* SET UP FOR EXIT - THE ADDRESS OF THE ROUTINE TO BE ENTERED    */
*   /* NEXT IS PUT INTO ENTRYADR (REGISTER 15), THE NORMAL RETURN    */
*   /* ADDRESS IS PUT INTO EXITADDR (REGISTER 10), AND A PL/S RETURN */
*   /* IS EXECUTED TO GENERATE THE PROPER EPILOG CODE - HOWEVER,     */
*   /* CONTROL IS PASSED TO IGF2503E INSTEAD OF THE CALLING ROUTINE  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0170
*   IF DERLRC=0 THEN                /* WAS AN ERROR ENCOUNTERED      */
@RF00165 CLI   DERLRC,0                                            0170
         BNE   @RF00170                                            0170
*     ENTRYADR=SRTNADDR;            /* NO, SET UP FOR NORMAL RETURN  */
         LR    ENTRYADR,SRTNADDR                                   0171
*   ELSE                            /* YES, ERROR DETECTED           */
*     ENTRYADR=ADDR(IEE0503D);      /* SET UP FOR ERROR EXIT         */
         B     @RC00170                                            0172
@RF00170 L     ENTRYADR,@CV00078                                   0172
*   EXITADDR=SRTNADDR;              /* PUT RETURN ADDRESS INTO A   0173
*                                      REGISTER                      */
@RC00170 LR    EXITADDR,SRTNADDR                                   0173
*   RETURN TO IGF2503E CODE(ENTRYADR);/* GENERATE EPILOG CODE        */
*                                                                  0174
         LR    @12,@15                                             0174
         L     @13,4(,@13)                                         0174
         L     @00,@SIZDATD                                        0174
         LR    @01,@09                                             0174
         FREEMAIN R,LV=(0),A=(1)
         LR    @15,@12                                             0174
         LA    @14,IGF2503E                                        0174
         BR    @14                                                 0174
*   /*****************************************************************/
*   /*                                                               */
*   /* AT THIS POINT, REGISTER 10 (EXITADDR) CONTAINS THE NORMAL     */
*   /* RETURN ADDRESS AND REGISTER 15 (ENTRYADR) CONTAINS THE ACTUAL */
*   /* EXIT ADDRESS - THE FOLLOWING CODE DOES NOT HAVE ADDRESSABILITY*/
*   /* AND MAY ONLY MANIPULATE REGISTERS - IT COPIES THE RETURN      */
*   /* ADDRESS INTO REGISTER 14 (RTNADDR) AND BRANCHES TO THE ADDRESS*/
*   /* SPECIFIED IN REGISTER 15 - THIS RESULTS IN AN EXIT TO THE     */
*   /* ORIGINAL CALLER (IEE0403D) OR TO THE SVC 34 MESSAGE WRITER    */
*   /* (IEE0503D) (WITH REGISTER 14 STILL POINTING BACK TO IEE0403D, */
*   /* AS REQUIRED)                                                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0175
*IGF2503E:                                                         0175
*   RTNADDR=EXITADDR;               /* SET UP RETURN ADDRESS         */
IGF2503E LR    RTNADDR,EXITADDR                                    0175
*   GOTO NEXTRTN;                   /* EXIT TO IEE0403D OR IEE0503D  */
         BR    ENTRYADR                                            0176
*   GENERATE NOREFS NOSETS(EJECT);                                 0177
*                                                                  0177
         EJECT
*   /*****************************************************************/
*   /*                                                               */
*   /* SUBROUTINE IGFON - PROCESSES THE 'SWAP ON' COMMAND            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0178
*IGFON:                             /* SUBROUTINE ENTRY POINT        */
*   PROCEDURE;                                                     0178
         B     @PB00002                                            0178
IGFON    STM   @14,@12,12(@13)                                     0178
*   IF CTEXT(3:3)=' ' THEN          /* IS 'ON' FOLLOWED BY A BLANK   */
         SR    @12,@12                                             0179
         ICM   @12,7,XAL(XSAPTR)                                   0179
         CLI   CTEXT+2(@12),C' '                                   0179
         BNE   @RF00179                                            0179
*     DO;                           /* YES, THE COMMAND IS VALID     */
*       DERLRC=VALID;               /* INDICATE VALID SWAP COMMAND   */
         MVI   DERLRC,X'00'                                        0181
*       CVTDDR='1'B;                /* INDICATE SYSTEM SWAPS ACTIVE  */
         L     @12,CVTPTR                                          0182
         OI    CVTDDR(@12),B'00100000'                             0182
*     END;                                                         0183
*   ELSE                            /* NO, INVALID COMMAND           */
*     XAE=DELERROR;                 /* INDICATE DELIMITER ERROR      */
         B     @RC00179                                            0184
@RF00179 MVI   XAE(XSAPTR),X'07'                                   0184
*   RETURN;                         /* RETURN TO CALLER              */
@EL00002 DS    0H                                                  0185
@EF00002 DS    0H                                                  0185
@ER00002 LM    @14,@12,12(@13)                                     0185
         BR    @14                                                 0185
*   END IGFON;                      /* END OF SUBROUTINE             */
         B     @EL00002                                            0186
@PB00002 DS    0H                                                  0187
*   GENERATE NOREFS NOSETS(EJECT);                                 0187
*                                                                  0187
         EJECT
*   /*****************************************************************/
*   /*                                                               */
*   /* SUBROUTINE IGFOFF - PROCESSES THE 'SWAP OFF' COMMAND          */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0188
*IGFOFF:                            /* SUBROUTINE ENTRY POINT        */
*   PROCEDURE;                                                     0188
         B     @PB00003                                            0188
IGFOFF   STM   @14,@12,12(@13)                                     0188
*   IF CTEXT(4:4)=' ' THEN          /* IS 'OFF' FOLLOWED BY A BLANK  */
         SR    @12,@12                                             0189
         ICM   @12,7,XAL(XSAPTR)                                   0189
         CLI   CTEXT+3(@12),C' '                                   0189
         BNE   @RF00189                                            0189
*     DO;                           /* YES, THE COMMAND IS VALID     */
*       DERLRC=VALID;               /* INDICATE VALID SWAP COMMAND   */
         MVI   DERLRC,X'00'                                        0191
*       CVTDDR='0'B;                /* INDICATE SYSTEM SWAPS INACTIVE*/
         L     @12,CVTPTR                                          0192
         NI    CVTDDR(@12),B'11011111'                             0192
*     END;                                                         0193
*   ELSE                            /* NO, INVALID COMMAND           */
*     XAE=DELERROR;                 /* INDICATE DELIMITER ERROR      */
         B     @RC00189                                            0194
@RF00189 MVI   XAE(XSAPTR),X'07'                                   0194
*   RETURN;                         /* RETURN TO CALLER              */
@EL00003 DS    0H                                                  0195
@EF00003 DS    0H                                                  0195
@ER00003 LM    @14,@12,12(@13)                                     0195
         BR    @14                                                 0195
*   END IGFOFF;                     /* END OF SUBROUTINE             */
         B     @EL00003                                            0196
@PB00003 DS    0H                                                  0197
*   GENERATE NOREFS NOSETS(EJECT);                                 0197
*                                                                  0197
         EJECT
*   /*****************************************************************/
*   /*                                                               */
*   /* SUBROUTINE IGFSWAP - PROCESS THE 'SWAP XXX,YYY' COMMAND       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0198
*IGFSWAP:                           /* SUBROUTINE ENTRY POINT        */
*   PROCEDURE;                                                     0198
         B     @PB00004                                            0198
IGFSWAP  STM   @14,@12,@SA00004                                    0198
*   IF CTEXT(4:4)^=','×CTEXT(8:8)^=' ' THEN/* ARE THE DEVICE       0199
*                                      ADDRESSES SEPARATED BY A COMMA
*                                      AND FOLLOWED BY A BLANK       */
         SR    @12,@12                                             0199
         ICM   @12,7,XAL(XSAPTR)                                   0199
         CLI   CTEXT+3(@12),C','                                   0199
         BNE   @RT00199                                            0199
         CLI   CTEXT+7(@12),C' '                                   0199
         BE    @RF00199                                            0199
@RT00199 DS    0H                                                  0200
*     XAE=DELERROR;                 /* NO, INDICATE DELIMITER ERROR  */
         MVI   XAE(XSAPTR),X'07'                                   0200
*   ELSE                            /* YES, SYNTAX IS OK (SO FAR)    */
*     DO;                                                          0201
*                                                                  0201
         B     @RC00199                                            0201
@RF00199 DS    0H                                                  0202
*       /*************************************************************/
*       /*                                                           */
*       /* INITIALIZE WORKAREA DDRCOM FOR VALIDATION REQUEST         */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0202
*       DDRID='DDR ';               /* PUT IN ID FIELD               */
         MVC   DDRID(4,DDRPTR),@CC01637                            0202
*       DDROPER='1'B;               /* INDICATE OPERATOR SWAP        */
         OI    DDROPER(DDRPTR),B'10000000'                         0203
*       DDRFMCUA=CTEXT(1:3);        /* PUT IN FROM CUA               */
         SR    @12,@12                                             0204
         ICM   @12,7,XAL(XSAPTR)                                   0204
         MVC   DDRFMCUA(3,DDRPTR),CTEXT(@12)                       0204
*       DDRTOCUA=CTEXT(5:7);        /* PUT IN TO CUA                 */
         MVC   DDRTOCUA(3,DDRPTR),CTEXT+4(@12)                     0205
*       DDRINV2=DDRVFM;             /* INDICATE 'FROM' VALIDATION    */
*                                                                  0206
         MVI   DDRINV2(DDRPTR),X'01'                               0206
*       /*************************************************************/
*       /*                                                           */
*       /* VALIDATE THE FROM DEVICE                                  */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0207
*       CALL IGFDV1;                /* PERFORM VALIDATION            */
         L     @15,@CV00081                                        0207
         BALR  @14,@15                                             0207
*       DERLRC=RTNCODE;             /* COPY RETURN CODE              */
         STC   RTNCODE,DERLRC                                      0208
*       IF DERLRC^=0                /* DID VALIDATION FAIL           */
*         THEN                      /* YES, DETERMINE CAUSE          */
         CLI   DERLRC,0                                            0209
         BE    @RF00209                                            0209
*         IF DERLRC^=ERROR THEN     /* DID A CATASTROPHIC ERROR OCCUR*/
         CLI   DERLRC,12                                           0210
         BE    @RF00210                                            0210
*           DO;                     /* NO, DEVICE IS JUST INVALID    */
*             XAE=ERRCODE(DDRINV1); /* CONVERT INVALID DEVICE CODE TO
*                                      ERROR CODE                    */
         SR    @12,@12                                             0212
         IC    @12,DDRINV1(,DDRPTR)                                0212
         LA    @11,ERRCODE-1(@12)                                  0212
         MVC   XAE(1,XSAPTR),0(@11)                                0212
*             XAV=DDRFMCUA;         /* PUT DEVICE ADDRESS INTO XSA   */
         MVI   XAV+3(XSAPTR),C' '                                  0213
         MVC   XAV+4(4,XSAPTR),XAV+3(XSAPTR)                       0213
         MVC   XAV(3,XSAPTR),DDRFMCUA(DDRPTR)                      0213
*           END;                                                   0214
*         ELSE                                                     0215
*           ;                       /* YES, CANNOT PROCESS ANY     0215
*                                      FURTHER AT THIS TIME          */
@RF00210 DS    0H                                                  0216
*       ELSE                        /* VALIDATION WAS SUCESSFUL      */
*         DO;                       /* SET UP TO SCHEDULE DDR        */
*                                                                  0216
         B     @RC00209                                            0216
@RF00209 DS    0H                                                  0217
*           /*********************************************************/
*           /*                                                       */
*           /* COMPLETE THE FIELDS WHICH WILL BE COPIED TO THE SHORT */
*           /* DDRCOM WHEN DDR IS SCHEDULED                          */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0217
*           UCBPTR=DDRFMUCB;        /* POINT TO UCB                  */
         L     UCBPTR,DDRFMUCB(,DDRPTR)                            0217
*           DDRASID=MASTERID;       /* DDR MUST RUN IN THE MASTER  0218
*                                      SCHEDULERS ADDRESS SPACE      */
         MVC   DDRASID(2,DDRPTR),@CH00052                          0218
*           IF DDRDA='0'B THEN      /* IS THIS A DISK SWAP           */
         TM    DDRDA(DDRPTR),B'01000000'                           0219
         BNZ   @RF00219                                            0219
*             DDRUASID=UCBASID;     /* NO, COPY THE ADDRESS SPACE OF
*                                      THE USER                      */
         L     @12,UCBEXTPT(,UCBPTR)                               0220
         MVC   DDRUASID(2,DDRPTR),UCBASID(@12)                     0220
*           IF DDRUASID^=0 THEN     /* IS THERE A LOCATABLE USER OF
*                                      THE DEVICE BEING SWAPPED      */
@RF00219 LH    @12,DDRUASID(,DDRPTR)                               0221
         LTR   @12,@12                                             0221
         BZ    @RF00221                                            0221
*             DDRUASCB=ASVTENTY(DDRUASID);/* YES, COPY THE ADDRESS OF
*                                      THE USERS ASCB                */
         SLA   @12,2                                               0222
         L     @11,CVTPTR                                          0222
         L     @11,ASVTPTR(,@11)                                   0222
         L     @12,ASVTENTY-4(@12,@11)                             0222
         ST    @12,DDRUASCB(,DDRPTR)                               0222
*           IF DDRMT='1'B&DDRASID=0 THEN/* IS THIS A TAPE SWAP FOR A
*                                      DEVICE WHICH IS NOT IN USE    */
@RF00221 TM    DDRMT(DDRPTR),B'00100000'                           0223
         BNO   @RF00223                                            0223
         ICM   @12,3,DDRASID(DDRPTR)                               0223
         BNZ   @RF00223                                            0223
*             DO;                   /* YES, INDICATE ERROR           */
*               DERLRC=INVALID;     /* INDICATE INVALID SWAP         */
         MVI   DERLRC,X'04'                                        0225
*               XAV=DDRFMCUA;       /* COPY DEVICE ADDRESS TO XSA    */
         MVI   XAV+3(XSAPTR),C' '                                  0226
         MVC   XAV+4(4,XSAPTR),XAV+3(XSAPTR)                       0226
         MVC   XAV(3,XSAPTR),DDRFMCUA(DDRPTR)                      0226
*               XAE=NOTALOC;        /* INDICATE DEVICE NOT ALLOCATED */
         MVI   XAE(XSAPTR),X'33'                                   0227
*             END;                                                 0228
*           ELSE                    /* NO, SWAP CAN CONTINUE         */
*             DO;                                                  0229
         B     @RC00223                                            0229
@RF00223 DS    0H                                                  0230
*               DERLRC=VALID;       /* INDICATE VALID SWAP COMMAND   */
*                                                                  0230
         MVI   DERLRC,X'00'                                        0230
*               /*****************************************************/
*               /*                                                   */
*               /* GETMAIN SPACE FOR A SHORT DDRCOM AND AN SRB -     */
*               /* THESE BLOCKS ARE NEEDED TO SCHEDULE DDR           */
*               /*                                                   */
*               /*****************************************************/
*                                                                  0231
*               DO;                 /* GETMAIN (EU) LV(LENGTH(DDRSHOR
*                                      T)+LENGTH(SRBSECT))         0231
*                                      A(DERGMADR) SP(SPN)           */
*                 RESPECIFY                                        0232
*                  (GPR01P)RESTRICTED;                             0232
*                 I004=I004&&I004;  /* INIT LIST                     */
         XC    I004(10),I004                                       0233
*                 I00404='00'X;     /* TYPE                          */
         MVI   I00404,X'00'                                        0234
*                 I00401=LENGTH(DDRSHORT)+LENGTH(SRBSECT);/* LENGTH  */
         MVC   I00401(4),@CF01661                                  0235
*                 I00403=ADDR(DERGMADR);/* ADDR OF ADDR LIST         */
         LA    @12,DERGMADR                                        0236
         STCM  @12,7,I00403                                        0236
*                 I00405=SPN;       /* SUBPOOL VALUE                 */
         MVI   I00405,X'F5'                                        0237
*                 GPR01P=ADDR(I004);/* REG1 POINTS TO LIST           */
         LA    GPR01P,I004                                         0238
*                 SVC(4);           /* STANDARD-FORM OF GETMAIN      */
         SVC   4                                                   0239
*                 RESPECIFY                                        0240
*                  (GPR01P)UNRESTRICTED;                           0240
*               END;                /* GETMAIN (EU) LV(LENGTH(DDRSHOR
*                                      T)+LENGTH(SRBSECT))         0241
*                                      A(DERGMADR) SP(SPN)           */
*               DERGMLNG=LENGTH(DDRSHORT)+LENGTH(SRBSECT);/* INDICATE
*                                      LENGTH OF GETMAINED AREA      */
         MVC   DERGMLNG(3),@CF01661+1                              0242
*               DERGMSPN=SPN;       /* INDICATE SUBPOOL NUMBER       */
         MVI   DERGMSPN,X'F5'                                      0243
*               SDDRPTR=DERGMADR;   /* PICK UP ADDR OF SHORT DDRCOM  */
*                                                                  0244
         L     SDDRPTR,DERGMADR                                    0244
*               /*****************************************************/
*               /*                                                   */
*               /* COPY SHORT PART OF WORKAREA DDRCOM TO SHORT DDRCOM*/
*               /*                                                   */
*               /*****************************************************/
*                                                                  0245
*               SDDRPTR->DDRSHORT=DDRSHORT;/* COPY SHORT PART TO NEW
*                                      DDRCOM                        */
         MVC   DDRSHORT(24,SDDRPTR),DDRSHORT(DDRPTR)               0245
*               SDDRPTR->DDRID='DDRS';/* INDICATE SHORT DDRCOM       */
*                                                                  0246
         MVC   DDRID(4,SDDRPTR),@CC01646                           0246
*               /*****************************************************/
*               /*                                                   */
*               /* FILL IN THE SRB                                   */
*               /*                                                   */
*               /*****************************************************/
*                                                                  0247
*               SRBPTR=SDDRPTR+LENGTH(DDRSHORT);/* POINT TO SRB - IT
*                                      IS IMMEDIATELY AFTER THE SHORT
*                                      DDRCOM                        */
         LA    SRBPTR,24                                           0247
         AR    SRBPTR,SDDRPTR                                      0247
*               SRBSECT=SRBSECT&&SRBSECT;/* CLEAR THE SRB            */
         XC    SRBSECT(44,SRBPTR),SRBSECT(SRBPTR)                  0248
*               SRBID='SRB ';       /* FILL IN SRB ID FIELD          */
         MVC   SRBID(4,SRBPTR),@CC01647                            0249
*               SRBASCB=ASVTENTY(DDRASID);/* FILL IN ADDRESS OF THE
*                                      ASCB UNDER WHICH DDR IS TO RUN*/
