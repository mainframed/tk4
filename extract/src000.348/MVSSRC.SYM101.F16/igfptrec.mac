         TITLE 'IGFPTREC - MCH EMERGENCY RECORDER                      *
                        '
IGFPTREC CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'IGFPTREC  75.301'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @12,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@12                                         0001
         ST    @13,@SA00001+4                                      0001
         LA    @14,@SA00001                                        0001
         ST    @14,8(,@13)                                         0001
         LR    @13,@14                                             0001
*                                                                  0106
*   /*****************************************************************/
*   /*                                                               */
*   /* READ LOGREC HEADER AND PREPARE TO WRITE TERMINATION RECORD    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0106
*   CALL READHDR;                   /* ATTEMPT TO READ HEADER        */
         BAL   @14,READHDR                                         0106
*   IF RETCODE^=IOGOOD THEN         /* Q. HEADER SUCCESSFULLY READ   */
         LTR   RETCODE,RETCODE                                     0107
         BNZ   @RT00107                                            0107
*     RETURN;                       /* NO, RETURN TO CALLER          */
*   UPDATSW='0'B;                   /* INITIALIZE 'HEADER UPDATED' 0109
*                                      SWITCH                        */
         NI    UPDATSW,B'01111111'                                 0109
*   REMBYTES=HDRREMBT;              /* GET NUMBER OF BYTES REMAINING
*                                      ON CURRENT TRACK              */
         LH    REMBYTES,HDRREMBT                                   0110
         N     REMBYTES,@CF02443                                   0110
*   WRITESK=HDRLAST;                /* GET BBCCHHR OF LAST RECORD  0111
*                                      WRITTEN                       */
         MVC   WRITESK(7),HDRLAST                                  0111
*   RECADDR=ADDR(LRB);              /* PUT ADDR OF TERMINATION RECORD
*                                      IN CCW                        */
         L     @07,CVTPTR                                          0112
         L     @07,RVTPTR(,@07)                                    0112
         L     @07,RWAPTR(,@07)                                    0112
         L     @07,LRBPTR(,@07)                                    0112
         STCM  @07,7,RECADDR                                       0112
*   RECLNH=LRBTLNH;                 /* PUT LENGTH OF TERMINATION   0113
*                                      RECORD IN CCW                 */
*                                                                  0113
         L     @07,LRBTLNH(,@07)                                   0113
         STH   @07,RECLNH                                          0113
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK LOGREC DATA SET TO DETERMINE IF AND WHERE RECORD SHOULD */
*   /* BE WRITTEN                                                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0114
*CHECKLOG:                                                         0114
*   CALL SEEIFFIT;                  /* SEE IF RECORD WILL FIT ON   0114
*                                      CURRENT TRACK                 */
CHECKLOG BAL   @14,SEEIFFIT                                        0114
*   IF RETCODE=BADEVTYP THEN        /* Q. VALID DEVICE TYPE FOR    0115
*                                      LOGREC                        */
         C     RETCODE,@CF00032                                    0115
         BE    @RT00115                                            0115
*     RETURN;                       /* NO, RETURN TO CALLER          */
*   IF RETCODE^=TOOBIG THEN         /* Q. RECORD TOO BIG EVEN FOR  0117
*                                      EMPTY TRACK                   */
         C     RETCODE,@CF00042                                    0117
         BE    @RF00117                                            0117
*     DO;                           /* NO, CONTINUE TESTS - OTHERWISE
*                                      FORGET THIS RECORD            */
*       IF RETCODE=ENDTRK THEN      /* Q. RECORD TOO BIG FOR       0119
*                                      REMAINDER OF CURRENT TRACK    */
         C     RETCODE,@CF00040                                    0119
         BNE   @RF00119                                            0119
*         DO;                                                      0120
*           CALL CCHHUPDT;          /* YES, UPDATE TO NEXT TRACK IF
*                                      ANY                           */
         BAL   @14,CCHHUPDT                                        0121
*           IF RETCODE=DSFULL THEN  /* Q. MORE TRACKS ON DATA SET    */
         C     RETCODE,@CF00040                                    0122
         BE    @RT00122                                            0122
*             GOTO DONEREC;         /* NO, HAVE TO QUIT HERE         */
*         END;                                                     0124
*                                                                  0124
*       /*************************************************************/
*       /*                                                           */
*       /* WRITE A RECORD ON LOGREC                                  */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0125
*       CALL WRITEREC;              /* ATTEMPT TO WRITE RECORD       */
@RF00119 BAL   @14,WRITEREC                                        0125
*       IF RETCODE^=IOGOOD THEN     /* Q. RECORD WRITTEN SUCCESSFULLY*/
         LTR   RETCODE,RETCODE                                     0126
         BNZ   @RT00126                                            0126
*         GOTO DONEREC;             /* NO, FORGET CURRENT AND ANY  0127
*                                      REMAINING RECORDS             */
*     END;                                                         0128
*                                                                  0128
*   /*****************************************************************/
*   /*                                                               */
*   /* GET NEXT SYSTEM RECORD TO BE RECORDED                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0129
*   GEN(RECORD TYPE=TERM)                                          0129
*   REFS(FLCCVT,PSA,CVTRECRQ,CVTMAP);                              0129
@RF00117 DS    0H                                                  0129
         RECORD TYPE=TERM
*   RFY                                                            0130
*     R15 RSTD;                     /* RESTRICT R15                  */
*   IF R15=RECFOUND THEN            /* Q. RECORD FOUND               */
         LTR   R15,R15                                             0131
         BNZ   @RF00131                                            0131
*     DO;                                                          0132
*       RECADDR=R1;                 /* YES, PUT RECORD ADDR IN CCW   */
         STCM  R1,7,RECADDR                                        0133
*       RECLNH=R0;                  /* PUT RECORD LENGTH IN CCW      */
         STH   R0,RECLNH                                           0134
*       GOTO CHECKLOG;              /* ATTEMPT TO RECORD IT          */
         B     CHECKLOG                                            0135
*     END;                                                         0136
*   RFY                                                            0137
*     R15 UNRSTD;                   /* UNRESTRICT R15                */
*                                                                  0137
@RF00131 DS    0H                                                  0138
*   /*****************************************************************/
*   /*                                                               */
*   /* WRITE THE UPDATED LOGREC HEADER AND AN EOF ON SYS1.LOGREC     */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0138
*DONEREC:                                                          0138
*   IF UPDATSW='1'B THEN            /* Q. ANY RECORDS WRITTEN TO   0138
*                                      LOGREC                        */
DONEREC  TM    UPDATSW,B'10000000'                                 0138
         BNO   @RF00138                                            0138
*     DO;                                                          0139
*       CALL WRITEHDR;              /* YES, WRITE THE UPDATED HEADER */
         BAL   @14,WRITEHDR                                        0140
*       CALL WRITEOF;               /* WRITE AN EOF                  */
         BAL   @14,WRITEOF                                         0141
*     END;                                                         0142
*   RETURN;                         /* RETURN TO CALLER              */
@EL00001 L     @13,4(,@13)                                         0143
@EF00001 DS    0H                                                  0143
@ER00001 LM    @14,@12,12(@13)                                     0143
         BR    @14                                                 0143
*READHDR:                                                          0144
*                                                                  0144
*   /*****************************************************************/
*   /*                                                               */
*   /* SUBROUTINE TO READ SYS1.LOGREC HEADER RECORD                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0144
*   PROC OPTIONS(SAVE,NOSAVEAREA);                                 0144
         B     @EL00001                                            0144
READHDR  STM   @14,@01,12(@13)                                     0144
         STM   @03,@12,32(@13)                                     0144
*   IF DEBPTR^=0 THEN               /* Q. LOGREC DEB INITIALIZED     */
         L     @15,CVTPTR                                          0145
         L     @15,DCBPTR-1(,@15)                                  0145
         L     @15,DEBPTR+40(,@15)                                 0145
         LTR   @15,@15                                             0145
         BZ    @RF00145                                            0145
*     DO;                           /* YES, PREPARE TO READ HEADER   */
*       LOGSKBB=DEBBINUM;           /* GET BB OF LOGREC HEADER RECORD*/
         AL    @15,@CF00845                                        0147
         MVC   LOGSKBB(2),DEBBINUM(@15)                            0147
*       LOGSKCC=DEBSTRCC;           /* GET CC                        */
         MVC   LOGSKCC(2),DEBSTRCC(@15)                            0148
*       LOGSKHH=DEBSTRHH;           /* GET HH                        */
         MVC   LOGSKHH(2),DEBSTRHH(@15)                            0149
*       HREADCMD=READDATA;          /* INSURE CORRECT COMMAND CODE IN
*                                      'READ DATA' CCW               */
         MVI   HREADCMD,X'06'                                      0150
*       R1=ADDR(HCCWS);             /* POINT TO CCW'S TO READ HEADER */
         LA    R1,HCCWS                                            0151
*       BACKPTR=R13;                /* CHAIN SAVE AREA TO CALLER'S   */
         ST    R13,BACKPTR                                         0152
*       R13=ADDR(REGSAVE);          /* ESTABLISH NEW SAVE AREA PTR   */
         LA    R13,REGSAVE                                         0153
*       CALL IGFPTRIO;              /* ATTEMPT I/O TO READ HEADER    */
         BAL   @14,IGFPTRIO                                        0154
*       R13=BACKPTR;                /* RESTORE CALLER'S SAVE AREA PTR*/
         L     R13,BACKPTR                                         0155
*       IF RETCODE=IOGOOD           /* IF I/O WAS SUCCESSFUL         */
*           &HDRCHKBT='FF'X THEN    /* AND 'CHECK BYTE' IN HEADER IS
*                                      CORRECT                       */
         LTR   RETCODE,RETCODE                                     0156
         BNZ   @RF00156                                            0156
         CLI   HDRCHKBT,X'FF'                                      0156
         BE    @RT00156                                            0156
*         RETURN;                   /* THEN RETURN TO CALLER WITH  0157
*                                      'IOGOOD' CODE                 */
*     END;                                                         0158
@RF00156 DS    0H                                                  0159
*   RETCODE=DSNOTRDY;               /* INDICATE LOGREC CAN'T BE USED */
@RF00145 LA    RETCODE,4                                           0159
*   END READHDR;                    /* RETURN TO CALLER              */
@EL00002 DS    0H                                                  0160
@EF00002 DS    0H                                                  0160
@ER00002 LM    @14,@01,12(@13)                                     0160
         LM    @03,@12,32(@13)                                     0160
         BR    @14                                                 0160
*SEEIFFIT:                                                         0161
*                                                                  0161
*   /*****************************************************************/
*   /*                                                               */
*   /* SUBROUTINE TO DETERMINE IF RECORD WILL FIT ON CURRENT TRACK   */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0161
*   PROC OPTIONS(SAVE,NOSAVEAREA);                                 0161
*                                                                  0161
SEEIFFIT STM   @14,@01,12(@13)                                     0161
         STM   @04,@05,36(@13)                                     0161
         STM   @07,@12,48(@13)                                     0161
*   /*****************************************************************/
*   /*                                                               */
*   /* DETERMINE THE NUMBER OF BYTES NEEDED ON THE LOGREC DEVICE TO  */
*   /* CONTAIN THE CURRENT RECORD - THIS AMOUNT IS DEVICE DEPENDENT  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0162
*   IF HDRDEVCD=T2314 THEN          /* CHECK FOR 2314                */
         CLI   HDRDEVCD,X'08'                                      0162
         BNE   @RF00162                                            0162
*     SPACEREQ=101+2137*RECLNH/2048;                               0163
         LH    SPACEREQ,RECLNH                                     0163
         N     SPACEREQ,@CF02443                                   0163
         M     @02,@CF02364                                        0163
         D     @02,@CF02365                                        0163
         AL    SPACEREQ,@CF02363                                   0163
*   ELSE                                                           0164
*     IF HDRDEVCD=T3330×HDRDEVCD=T3330C THEN/* CHECK FOR 3330 OR   0164
*                                      3330C                         */
         B     @RC00162                                            0164
@RF00162 CLI   HDRDEVCD,X'09'                                      0164
         BE    @RT00164                                            0164
         CLI   HDRDEVCD,X'0D'                                      0164
         BNE   @RF00164                                            0164
@RT00164 DS    0H                                                  0165
*       SPACEREQ=135+RECLNH;                                       0165
         LA    SPACEREQ,135                                        0165
         LH    @07,RECLNH                                          0165
         N     @07,@CF02443                                        0165
         ALR   SPACEREQ,@07                                        0165
*     ELSE                                                         0166
*       IF HDRDEVCD=T3340 THEN      /* CHECK FOR 3340                */
         B     @RC00164                                            0166
@RF00164 CLI   HDRDEVCD,X'0A'                                      0166
         BNE   @RF00166                                            0166
*         SPACEREQ=167+RECLNH;                                     0167
         LA    SPACEREQ,167                                        0167
         LH    @07,RECLNH                                          0167
         N     @07,@CF02443                                        0167
         ALR   SPACEREQ,@07                                        0167
*       ELSE                                                       0168
*         IF HDRDEVCD=T2305I THEN   /* CHECK FOR 2305(I)             */
         B     @RC00166                                            0168
@RF00166 CLI   HDRDEVCD,X'06'                                      0168
         BNE   @RF00168                                            0168
*           SPACEREQ=434+RECLNH;                                   0169
         LA    SPACEREQ,434                                        0169
         LH    @07,RECLNH                                          0169
         N     @07,@CF02443                                        0169
         ALR   SPACEREQ,@07                                        0169
*         ELSE                                                     0170
*           IF HDRDEVCD=T2305II THEN/* CHECK FOR 2305(II)            */
         B     @RC00168                                            0170
@RF00168 CLI   HDRDEVCD,X'07'                                      0170
         BNE   @RF00170                                            0170
*             SPACEREQ=198+RECLNH;                                 0171
         LA    SPACEREQ,198                                        0171
         LH    @07,RECLNH                                          0171
         N     @07,@CF02443                                        0171
         ALR   SPACEREQ,@07                                        0171
*           ELSE                                                   0172
*             IF HDRDEVCD=T3350 THEN/* CHECK FOR 3350        @Z30RSMT*/
         B     @RC00170                                            0172
@RF00170 CLI   HDRDEVCD,X'0B'                                      0172
         BNE   @RF00172                                            0172
*               SPACEREQ=185+RECLNH;/* ZM40049                       */
         LA    SPACEREQ,185                                        0173
         LH    @07,RECLNH                                          0173
         N     @07,@CF02443                                        0173
         ALR   SPACEREQ,@07                                        0173
*             ELSE                  /*                       @Z30RSMT*/
*               DO;                 /* NONE OF THE ABOVE             */
         B     @RC00172                                            0174
@RF00172 DS    0H                                                  0175
*                 SPACEREQ=0;       /* GIVE 'SPACEREQ' A VALUE SO  0175
*                                      PL/S DOESN'T GET CONFUSED     */
         SLR   SPACEREQ,SPACEREQ                                   0175
*                 RETCODE=BADEVTYP; /* INDICATE BAD DEVICE TYPE      */
         LA    RETCODE,12                                          0176
*                 RETURN;           /* RETURN TO CALLER              */
@EL00003 DS    0H                                                  0177
@EF00003 DS    0H                                                  0177
@ER00003 LM    @14,@01,12(@13)                                     0177
         LM    @04,@05,36(@13)                                     0177
         LM    @07,@12,48(@13)                                     0177
         BR    @14                                                 0177
*               END;                                               0178
*                                                                  0178
*   /*****************************************************************/
*   /*                                                               */
*   /* SEE IF REQUIRED SPACE IS AVAILABLE                            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0179
*   IF SPACEREQ>HDRMAXBT THEN       /* Q. DOES SPACE REQUIRED EXCEED
*                                      TRACK CAPACITY                */
@RC00172 DS    0H                                                  0179
@RC00170 DS    0H                                                  0179
@RC00168 DS    0H                                                  0179
@RC00166 DS    0H                                                  0179
@RC00164 DS    0H                                                  0179
@RC00162 LH    @07,HDRMAXBT                                        0179
         N     @07,@CF02443                                        0179
         CR    SPACEREQ,@07                                        0179
         BNH   @RF00179                                            0179
*     DO;                                                          0180
*       RETCODE=TOOBIG;             /* YES, INDICATE RECORD TOO BIG  */
         LA    RETCODE,8                                           0181
*       RETURN;                     /* RETURN TO CALLER              */
         B     @EL00003                                            0182
*     END;                                                         0183
*   IF SPACEREQ>HDRREMBT THEN       /* Q. ENOUGH SPACE ON CURRENT  0184
*                                      TRACK                         */
@RF00179 LH    @07,HDRREMBT                                        0184
         N     @07,@CF02443                                        0184
         CR    SPACEREQ,@07                                        0184
         BNH   @RF00184                                            0184
*     DO;                                                          0185
*       RETCODE=ENDTRK;             /* NO, INDICATE END OF TRACK     */
         LA    RETCODE,4                                           0186
*       RETURN;                     /* RETURN TO CALLER              */
         B     @EL00003                                            0187
*     END;                                                         0188
*   REMBYTES=HDRREMBT-SPACEREQ;     /* COMPUTE NUMBER OF BYTES THAT
*                                      WILL REMAIN ON TRACK IF RECORD
*                                      IS WRITTEN                    */
@RF00184 LH    REMBYTES,HDRREMBT                                   0189
         N     REMBYTES,@CF02443                                   0189
         SLR   REMBYTES,SPACEREQ                                   0189
*   RETCODE=GOODFIT;                /* INDICATE RECORD WILL FIT ON 0190
*                                      CURRENT TRACK                 */
         SLR   RETCODE,RETCODE                                     0190
*   END SEEIFFIT;                   /* RETURN TO CALLER              */
         B     @EL00003                                            0191
*CCHHUPDT:                                                         0192
*                                                                  0192
*   /*****************************************************************/
*   /*                                                               */
*   /* SUBROUTINE TO UPDATE TARGET CCHH ON SYS1.LOGREC AFTER TRACK   */
*   /* FULL CONDITION                                                */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0192
*   PROC OPTIONS(SAVE,NOSAVEAREA);                                 0192
*                                                                  0192
CCHHUPDT STM   @14,@01,12(@13)                                     0192
         STM   @03,@05,32(@13)                                     0192
         STM   @07,@12,48(@13)                                     0192
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK FOR DATA SET FULL                                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0193
*   IF WRITCCHH^<HDRHILIM THEN      /* Q. LAST RECORD WRITTEN ON LAST
*                                      TRACK OF DATA SET             */
         CLC   WRITCCHH(4),HDRHILIM                                0193
         BL    @RF00193                                            0193
*     DO;                                                          0194
*       RETCODE=DSFULL;             /* YES, INDICATE DATA SET FULL   */
         LA    RETCODE,4                                           0195
*       RETURN;                     /* RETURN TO CALLER              */
@EL00004 DS    0H                                                  0196
@EF00004 DS    0H                                                  0196
@ER00004 LM    @14,@01,12(@13)                                     0196
         LM    @03,@05,32(@13)                                     0196
         LM    @07,@12,48(@13)                                     0196
         BR    @14                                                 0196
*     END;                                                         0197
*                                                                  0197
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK FOR END OF CYLINDER                                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0198
*   IF WRITEHH^<HDRHITRK THEN       /* Q. LAST RECORD WRITTEN ON LAST
*                                      TRACK OF CYLINDER             */
@RF00193 CLC   WRITEHH(2),HDRHITRK                                 0198
         BL    @RF00198                                            0198
*     DO;                                                          0199
*       WRITECC=WRITECC+1;          /* YES, BUMP ADDR TO NEXT      0200
*                                      CYLINDER                      */
         LH    @02,WRITECC                                         0200
         LA    @02,1(,@02)                                         0200
         STH   @02,WRITECC                                         0200
*       WRITEHH=0;                  /* SET HEAD ADDR TO ZERO         */
         SLR   @02,@02                                             0201
         STH   @02,WRITEHH                                         0201
*     END;                                                         0202
*   ELSE                                                           0203
*     WRITEHH=WRITEHH+1;            /* BUMP HEAD ADDR BY ONE SINCE 0203
*                                      NOT AT END OF CYLINDER        */
         B     @RC00198                                            0203
@RF00198 LH    @02,WRITEHH                                         0203
         LA    @02,1(,@02)                                         0203
         STH   @02,WRITEHH                                         0203
*   REMBYTES=HDRMAXBT-SPACEREQ;     /* BYTES REMAINING ON TRACK AFTER
*                                      CURRENT RECORD IS WRITTEN = 0204
*                                      MAX NUMBER OF BYTES TRACK CAN
*                                      HOLD MINUS NUMBER OF BYTES  0204
*                                      NEEDED TO WRITE CURRENT RECORD*/
@RC00198 LH    REMBYTES,HDRMAXBT                                   0204
         N     REMBYTES,@CF02443                                   0204
         SLR   REMBYTES,SPACEREQ                                   0204
*   WRITERM1=0;                     /* SET RECORD NUMBER FOR SEARCH
*                                      TO ZERO                       */
         MVI   WRITERM1,X'00'                                      0205
*   RETCODE=NOTFULL;                /* INDICATE DATA SET NOT FULL    */
         SLR   RETCODE,RETCODE                                     0206
*   END CCHHUPDT;                   /* RETURN TO CALLER              */
         B     @EL00004                                            0207
*WRITEREC:                                                         0208
*                                                                  0208
*   /*****************************************************************/
*   /*                                                               */
*   /* SUBROUTINE TO WRITE A RECORD TO SYS1.LOGREC                   */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0208
*   PROC OPTIONS(SAVE,NOSAVEAREA);                                 0208
*                                                                  0208
WRITEREC STM   @14,@01,12(@13)                                     0208
         STM   @03,@12,32(@13)                                     0208
*   /*****************************************************************/
*   /*                                                               */
*   /* SET UP COUNT FIELD OF RECORD                                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0209
*   CNTCCHH=WRITCCHH;               /* SET CCHH OF RECORD TO BE    0209
*                                      WRITTEN                       */
         MVC   CNTCCHH(4),WRITCCHH                                 0209
*   CNTR=WRITERM1+1;                /* SET RECORD NUMBER OF RECORD TO
*                                      BE WRITTEN                    */
         IC    @07,WRITERM1                                        0210
         LA    @07,1(,@07)                                         0210
         STC   @07,CNTR                                            0210
*   CNTDL=RECLNH;                   /* SET LENGTH OF RECORD TO BE  0211
*                                      WRITTEN                       */
*                                                                  0211
         LH    @07,RECLNH                                          0211
         N     @07,@CF02443                                        0211
         STH   @07,CNTDL                                           0211
*   /*****************************************************************/
*   /*                                                               */
*   /* ATTEMPT I/O AND CHECK RESULTS                                 */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0212
*   RWCKDCHN='1'B;                  /* INSURE DATA CHAINING ON IN  0212
*                                      WCKD CCW                      */
         OI    RWCKDCHN,B'10000000'                                0212
*   R1=ADDR(RCCWS);                 /* POINT TO CCW'S TO WRITE RECORD*/
         LA    R1,RCCWS                                            0213
*   BACKPTR=R13;                    /* CHAIN SAVE AREA TO CALLER'S   */
         ST    R13,BACKPTR                                         0214
*   R13=ADDR(REGSAVE);              /* ESTABLISH NEW SAVE AREA PTR   */
         LA    R13,REGSAVE                                         0215
*   CALL IGFPTRIO;                  /* ATTEMPT I/O TO WRITE RECORD   */
         BAL   @14,IGFPTRIO                                        0216
*   R13=BACKPTR;                    /* RESTORE CALLER'S SAVE AREA PTR*/
         L     R13,BACKPTR                                         0217
*   IF RETCODE^=IOGOOD THEN         /* Q. I/O SUCCESSFUL             */
         LTR   RETCODE,RETCODE                                     0218
         BNZ   @RT00218                                            0218
*     RETURN;                       /* NO, RETURN WITH CODE PASSED BY
*                                      IGFPTRIO                      */
*   WRITERM1=WRITERM1+1;            /* UPDATE SEEK ADDR TO RECORD  0220
*                                      JUST WRITTEN                  */
         IC    @07,WRITERM1                                        0220
         LA    @07,1(,@07)                                         0220
         STC   @07,WRITERM1                                        0220
*   HDRLAST=WRITESK;                /* UPDATE LOGREC HEADER WITH ADDR
*                                      OF RECORD JUST WRITTEN        */
         MVC   HDRLAST(7),WRITESK                                  0221
*   HDRREMBT=REMBYTES;              /* UPDATE HEADER WITH BYTES    0222
*                                      REMAINING ON CURRENT TRACK    */
         STH   REMBYTES,HDRREMBT                                   0222
*   UPDATSW='1'B;                   /* INDICATE HEADER HAS BEEN    0223
*                                      UPDATED                       */
         OI    UPDATSW,B'10000000'                                 0223
*   END WRITEREC;                   /* RETURN TO CALLER WITH 'IOGOOD'
*                                      RETURN CODE                   */
@EL00005 DS    0H                                                  0224
@EF00005 DS    0H                                                  0224
@ER00005 LM    @14,@01,12(@13)                                     0224
         LM    @03,@12,32(@13)                                     0224
         BR    @14                                                 0224
*WRITEHDR:                                                         0225
*                                                                  0225
*   /*****************************************************************/
*   /*                                                               */
*   /* SUBROUTINE TO WRITE SYS1.LOGREC HEADER RECORD                 */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0225
*   PROC OPTIONS(SAVE,NOSAVEAREA);                                 0225
WRITEHDR STM   @14,@12,12(@13)                                     0225
*   HREADCMD=WRITDATA;              /* CHANGE COMMAND CODE IN 'READ
*                                      DATA' CCW TO 'WRITE DATA'     */
         MVI   HREADCMD,X'05'                                      0226
*   R1=ADDR(HCCWS);                 /* POINT TO CCWS TO WRITE LOGREC
*                                      HEADER                        */
         LA    R1,HCCWS                                            0227
*   BACKPTR=R13;                    /* CHAIN SAVE AREA TO CALLER'S   */
         ST    R13,BACKPTR                                         0228
*   R13=ADDR(REGSAVE);              /* ESTABLISH NEW SAVE AREA PTR   */
         LA    R13,REGSAVE                                         0229
*   CALL IGFPTRIO;                  /* ATTEMPT I/O TO WRITE HEADER   */
         BAL   @14,IGFPTRIO                                        0230
*   R13=BACKPTR;                    /* RESTORE CALLER'S SAVE AREA PTR*/
         L     R13,BACKPTR                                         0231
*   END WRITEHDR;                   /* RETURN TO CALLER              */
@EL00006 DS    0H                                                  0232
@EF00006 DS    0H                                                  0232
@ER00006 LM    @14,@12,12(@13)                                     0232
         BR    @14                                                 0232
*WRITEOF:                                                          0233
*                                                                  0233
*   /*****************************************************************/
*   /*                                                               */
*   /* SUBROUTINE TO WRITE EOF RECORD ON SYS1.LOGREC                 */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0233
*   PROC OPTIONS(SAVE,NOSAVEAREA);                                 0233
WRITEOF  STM   @14,@12,12(@13)                                     0233
*   BACKPTR=R13;                    /* CHAIN SAVE AREA TO CALLER'S   */
         ST    R13,BACKPTR                                         0234
*   R13=ADDR(REGSAVE);              /* ESTABLISH NEW SAVE AREA PTR   */
         LA    R13,REGSAVE                                         0235
*   CNTDL=0;                        /* ZERO DATA LENGTH PORTION OF 0236
*                                      COUNT FIELD TO SIGNIFY EOF  0236
*                                      RECORD                        */
         SLR   @07,@07                                             0236
         STH   @07,CNTDL                                           0236
*   IF HDRLSTCH=HDRHILIM THEN       /* Q. LAST RECORD PUT ON LAST  0237
*                                      TRACK                         */
         CLC   HDRLSTCH(4),HDRHILIM                                0237
         BNE   @RF00237                                            0237
*     DO;                           /* YES, EOF WILL BE WRITTEN AS 0238
*                                      NEXT RECORD ON LAST TRACK     */
*       WRITERM1=HDRLSTR;           /* SET SEARCH ID TO RECORD NUMBER
*                                      OF LAST RECORD WRITTEN        */
         IC    @15,HDRLSTR                                         0239
         STC   @15,WRITERM1                                        0239
*       CNTR='FF'X;                 /* SET RECORD NUMBER IN COUNT  0240
*                                      FIELD TO MAXIMUM VALUE        */
         MVI   CNTR,X'FF'                                          0240
*       RECLNH=0;                   /* SET UP PARM FOR SEEIFFIT    0241
*                                      (LENGTH OF DATA PORTION OF  0241
*                                      RECORD TO BE WRITTEN)         */
         STH   @07,RECLNH                                          0241
*       CALL SEEIFFIT;              /* SEE IF EOF RECORD WILL FIT    */
         BAL   @14,SEEIFFIT                                        0242
*       IF RETCODE^=GOODFIT THEN    /* Q. ENOUGH SPACE FOR EOF RECORD*/
         LTR   RETCODE,RETCODE                                     0243
         BZ    @RF00243                                            0243
*         DO;                       /* NO, FORGET ABOUT IT           */
*           R13=BACKPTR;            /* RESTORE CALLER'S SAVE AREA PTR*/
         L     R13,BACKPTR                                         0245
*           RETURN;                 /* RETURN TO CALLER              */
@EL00007 DS    0H                                                  0246
@EF00007 DS    0H                                                  0246
@ER00007 LM    @14,@12,12(@13)                                     0246
         BR    @14                                                 0246
*         END;                                                     0247
*     END;                                                         0248
*   ELSE                            /* EOF WILL BE WRITTEN AS FIRST
*                                      RECORD OF LAST TRACK          */
*     DO;                                                          0249
         B     @RC00237                                            0249
@RF00237 DS    0H                                                  0250
*       CNTCCHH=HDRHILIM;           /* SET CCHH IN COUNT FIELD TO  0250
*                                      CCHH OF LAST TRACK            */
         MVC   CNTCCHH(4),HDRHILIM                                 0250
*       CNTR=1;                     /* SET RECORD NUMBER IN COUNT  0251
*                                      FIELD TO 1                    */
         MVI   CNTR,X'01'                                          0251
*       WRITCCHH=HDRHILIM;          /* SET CCHH IN SEEK FIELD TO CCHH
*                                      OF LAST TRACK                 */
         MVC   WRITCCHH(4),HDRHILIM                                0252
*       WRITERM1=0;                 /* SET RECORD NUMBER FOR SEARCH
*                                      TO 0                          */
         MVI   WRITERM1,X'00'                                      0253
*     END;                                                         0254
*   RWCKDCHN='0'B;                  /* REMOVE DATA CHAINING FROM WCKD
*                                      CCW SINCE NO DATA IS TO BE  0255
*                                      WRITTEN                       */
@RC00237 NI    RWCKDCHN,B'01111111'                                0255
*   R1=ADDR(RCCWS);                 /* POINT TO CCW'S TO WRITE EOF   */
         LA    R1,RCCWS                                            0256
*   CALL IGFPTRIO;                  /* ATTEMPT I/O TO WRITE EOF      */
         BAL   @14,IGFPTRIO                                        0257
*   R13=BACKPTR;                    /* RESTORE CALLER'S SAVE AREA PTR*/
         L     R13,BACKPTR                                         0258
*   END WRITEOF;                    /* RETURN TO CALLER              */
         B     @EL00007                                            0259
*IGFPTRIO:                                                         0260
*                                                                  0260
*   /*****************************************************************/
*   /*                                                               */
*   /* SUBROUTINE TO DO I/O TO SYS1.LOGREC                           */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0260
*   PROC OPTIONS(SAVE,NOSAVEAREA);                                 0260
*                                                                  0260
IGFPTRIO STM   @14,@01,12(@13)                                     0260
         STM   @03,@12,32(@13)                                     0260
*   /*****************************************************************/
*   /*                                                               */
*   /* SAVE CURRENT I/O STATUS AND PERFORM INITIAL HOUSEKEEPING      */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0261
*   IONEWSAV=FLCINPSW;              /* SAVE I/O NEW PSW              */
         MVC   IONEWSAV(8),FLCINPSW                                0261
*   IOOLDSAV=FLCIOPSW;              /* SAVE I/O OLD PSW              */
         MVC   IOOLDSAV(8),FLCIOPSW                                0262
*   IOADSAVE=FLCIOA;                /* SAVE I/O ADDRESS              */
         MVC   IOADSAVE(4),FLCIOA                                  0263
*   CAWSAVE=FLCCAW;                 /* SAVE CAW                      */
         MVC   CAWSAVE(4),FLCCAW                                   0264
*   CSWSAVE=FLCCSW;                 /* SAVE CSW                      */
         MVC   CSWSAVE(8),FLCCSW                                   0265
*   GEN REFS(CHMSKSAV,CHMSKNEW) SETS(CHMSKSAV);                    0266
         STCTL 2,2,CHMSKSAV           SAVE CURRENT CHANNEL MASKS
         LCTL  2,2,CHMSKNEW           INSURE ALL CHANNELS MASKED ON
*   IOSWTCHS=0;                     /* INITIALIZE SWITCHES USED BY 0267
*                                      THIS SUBROUTINE               */
         MVI   IOSWTCHS,X'00'                                      0267
*   FLCINPSW=MYIONPSW;              /* CHANGE I/O NEW SO THAT THIS 0268
*                                      MODULE WILL HANDLE ALL I/O  0268
*                                      INTERRUPTS                    */
         MVC   FLCINPSW(8),MYIONPSW                                0268
*   LOGCUA=UCBCHAN;                 /* GET CHANNEL AND UNIT ADDR OF
*                                      LOGREC DEVICE                 */
         L     @15,CVTPTR                                          0269
         L     @15,DCBPTR-1(,@15)                                  0269
         L     @15,DEBPTR+40(,@15)                                 0269
         L     @15,UCBPTR+32(,@15)                                 0269
         LH    LOGCUA,UCBCHAN(,@15)                                0269
         N     LOGCUA,@CF02443                                     0269
*CAWSETUP:                                                         0270
*   FLCCAW=R1;                      /* PUT INPUT CCW ADDR IN CAW     */
*                                                                  0270
CAWSETUP ST    R1,FLCCAW                                           0270
*   /*****************************************************************/
*   /*                                                               */
*   /* INSURE THAT THE PATH TO THE LOGREC DEVICE IS CLEAR AND ISSUE  */
*   /* SIO                                                           */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0271
*CLRPATH:                                                          0271
*   TCH(LOGCUA);                    /* TEST AVAILABILITY OF CHANNEL  */
CLRPATH  TCH   0(LOGCUA)                                           0271
*   BC(1,NOGOOD);                   /* CC=3, CHANNEL NOT OPERATIONAL
*                                      - QUIT                        */
         BC    1,NOGOOD                                            0272
*   BC(6,INTPEND);                  /* CC=1 OR 2, INTERRUPT PENDING -
*                                      WAIT FOR IT                   */
         BC    6,INTPEND                                           0273
*/*                                    CC=0, CHANNEL AVAILABLE       */
*                                                                  0274
*   HIO(LOGCUA);                    /* SEE IF DEVICE CAN BE ACCESSED */
         HIO   0(LOGCUA)                                           0274
*   BC(1,NOGOOD);                   /* CC=3, HARDWARE NOT OPERATIONAL
*                                      - QUIT                        */
         BC    1,NOGOOD                                            0275
*   DO SPINCTR=SPINVAL BY-1 TO 1;   /* HIO MAY CAUSE BUSY CONDITION
*                                      IN CONTROL UNIT - GIVE IT TIME
*                                      TO CLEAR              @YM08206*/
         L     SPINCTR,@CF02302                                    0276
@DL00276 DS    0H                                                  0277
*   END;                            /*                       @YM08206*/
         BCTR  SPINCTR,0                                           0277
         LTR   SPINCTR,SPINCTR                                     0277
         BP    @DL00276                                            0277
*   CLRIO(LOGCUA);                  /* CLEAR CURRENT OPERATION ON  0278
*                                      DEVICE                        */
         CLRIO 0(LOGCUA)                                           0278
*   BC(11,STARTIO);                 /* CC^=1, SIO CAN BE ATTEMPTED   */
         BC    11,STARTIO                                          0279
*/*                                    CC=1,CSW STORED               */
*                                                                  0280
*   IF CSWICC='1'B×CSWCCC='1'B THEN /* Q. INTERFACE OR CHANNEL     0280
*                                      CONTROL CHECK                 */
         TM    CSWICC,B'00000110'                                  0280
         BZ    @RF00280                                            0280
*     DO;                           /* YES, FORGET SIO FOR NOW AND 0281
*                                      SEE IF ANOTHER ATTEMPT SHOULD
*                                      BE MADE                       */
*CHKRETRY:                                                         0282
*       IF RETRYSW='0'B THEN        /* Q. RETRY ALREADY ATTEMPTED  0282
*                                      ONCE                          */
CHKRETRY TM    RETRYSW,B'00001000'                                 0282
         BNZ   @RF00282                                            0282
*         DO;                                                      0283
*           RETRYSW='1'B;           /* NO, SET RETRY ONCE SWITCH     */
         OI    RETRYSW,B'00001000'                                 0284
*           GOTO CLRPATH;           /* ATTEMPT I/O RETRY             */
         B     CLRPATH                                             0285
*         END;                                                     0286
*       ELSE                                                       0287
*         GOTO NOGOOD;              /* OTHERWISE, QUIT               */
*     END;                                                         0288
*STARTIO:                                                          0289
*   SIO(LOGCUA);                    /* ATTEMPT START I/O             */
@RF00280 DS    0H                                                  0289
STARTIO  SIO   0(LOGCUA)                                           0289
*   BC(1,NOGOOD);                   /* CC=3, HARDWARE NOT OPERATIONAL
*                                      - QUIT                        */
         BC    1,NOGOOD                                            0290
*   BC(4,CHKCSW);                   /* CC=1, CSW STORED - CHECK IT 0291
*                                      OUT                           */
         BC    4,CHKCSW                                            0291
*   BC(2,SIOBUSY);                  /* CC=2, CHANNEL BUSY - WAIT FOR
*                                      IT                            */
         BC    2,SIOBUSY                                           0292
*/*                                    CC=0, I/O INITIATED           */
*                                                                  0293
*   DEVENDSW='1'B;                  /* INDICATE DEVICE END EXPECTED  */
         OI    DEVENDSW,B'00100000'                                0293
*   LPSW(WAITPSW);                  /* WAIT FOR DEVICE END INTERRUPT */
         LPSW  WAITPSW                                             0294
*SIOBUSY:                                                          0295
*   BUSYSW='1'B;                    /* INDICATE CHANNEL BUSY ON SIO  */
SIOBUSY  OI    BUSYSW,B'01000000'                                  0295
*   LPSW(WAITPSW);                  /* WAIT FOR CHANNEL TO COME FREE */
         LPSW  WAITPSW                                             0296
*INTPEND:                                                          0297
*   IPENDSW='1'B;                   /* INDICATE INTERRUPT PENDING    */
INTPEND  OI    IPENDSW,B'10000000'                                 0297
*   LPSW(WAITPSW);                  /* WAIT FOR INTERRUPT            */
*                                                                  0298
         LPSW  WAITPSW                                             0298
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK STATUS BITS IN CSW                                      */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0299
*CHKCSW:                                                           0299
*   IF CSWLOGP='1'B                 /* CHECK FOR CHANNEL LOGOUT    0299
*                                      PENDING                       */
*       ×CSWICC='1'B                /* CHECK FOR INTERFACE CONTROL 0299
*                                      CHECK                         */
*       ×CSWCDC='1'B                /* CHECK FOR CHANNEL DATA CHECK  */
*       ×CSWCCC='1'B THEN           /* CHECK FOR CHANNEL CONTROL   0299
*                                      CHECK                         */
CHKCSW   TM    CSWLOGP,B'00000100'                                 0299
         BO    @RT00299                                            0299
         TM    CSWICC,B'00001110'                                  0299
         BNZ   @RT00299                                            0299
*     GOTO CHKRETRY;                /* IF ANY OF THE ABOVE THEN SEE
*                                      IF RETRY SHOULD BE ATTEMPTED  */
*   IF CSWBUSY='1'B THEN            /* MAKE FOLLOWING TESTS IF 'BUSY'
*                                      INDICATED                     */
         TM    CSWBUSY,B'00010000'                                 0301
         BNO   @RF00301                                            0301
*     IF CSWCUE='1'B                /* CHECK FOR CONTROL UNIT END    */
*         ×CSWCE='1'B               /* CHECK FOR CHANNEL END         */
*         ×CSWDE='1'B               /* CHECK FOR DEVICE END          */
*         ×CSWATTN='1'B THEN        /* CHECK FOR ATTENTION           */
         TM    CSWCUE,B'10101100'                                  0302
         BNZ   @RT00302                                            0302
*       GOTO CLRPATH;               /* IF ANY OF THE ABOVE THEN    0303
*                                      REPEAT SEQUENCE TO CLEAR THE
*                                      PATH TO THE LOGREC DEVICE     */
*     ELSE                                                         0304
*       GOTO INTPEND;               /* OTHERWISE, WAIT FOR PENDING 0304
*                                      INTERRUPT                     */
         B     INTPEND                                             0304
*   IF CSWUE='1'B                   /* CHECK FOR UNIT EXCEPTION      */
*       ×CSWPCI='1'B                /* CHECK FOR PROGRAM CONTROLLED
*                                      INTERRUPTION                  */
*       ×CSWIL='1'B                 /* CHECK FOR INCORRECT LENGTH    */
*       ×CSWPTNCK='1'B              /* CHECK FOR PROTECTION CHECK    */
*       ×CSWPGMCK='1'B              /* CHECK FOR PROGRAM CHECK       */
*       ×CSWCHNCK='1'B THEN         /* CHECK FOR CHAINING CHECK      */
@RF00301 TM    CSWUE,B'00000001'                                   0305
         BO    @RT00305                                            0305
         TM    CSWPCI,B'11110001'                                  0305
         BNZ   @RT00305                                            0305
*     GOTO NOGOOD;                  /* IF ANY OF THE ABOVE THEN QUIT */
*   IF CSWUC='1'B THEN              /* Q. UNIT CHECK                 */
         TM    CSWUC,B'00000010'                                   0307
         BNO   @RF00307                                            0307
*     DO;                           /* YES, ISSUE SENSE TO CLEAR IT  */
*       FLCCAW=ADDR(SENSECCW);      /* SET UP CAW FOR SENSE          */
         LA    @03,SENSECCW                                        0309
         ST    @03,FLCCAW                                          0309
*       SIO(LOGCUA);                /* ATTEMPT START I/O FOR SENSE   */
         SIO   0(LOGCUA)                                           0310
*       BC(7,NOGOOD);               /* CC^=0, QUIT                   */
         BC    7,NOGOOD                                            0311
*       SENSESW='1'B;               /* INDICATE WAITING FOR SENSE    */
         OI    SENSESW,B'00010000'                                 0312
*       LPSW(WAITPSW);              /* WAIT FOR SENSE TO COMPLETE    */
         LPSW  WAITPSW                                             0313
*     END;                                                         0314
*   ELSE                                                           0315
*     GOTO NOGOOD;                  /* IF CSW STORED AND NONE OF   0315
*                                      ABOVE TESTS ARE SATISFIED THEN
*                                      QUIT                          */
*                                                                  0315
*   /*****************************************************************/
*   /*                                                               */
*   /* INTERNAL I/O INTERRUPT HANDLER POINTED TO BY I/O NEW PSW      */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0316
*IGFPTRE2:                                                         0316
*   IF IPENDSW='1'B×BUSYSW='1'B THEN/* Q. WAS WAIT ENTERED FOR     0316
*                                      PENDING INTERRUPT OR SIO BUSY
*                                      CONDITION                     */
IGFPTRE2 TM    IPENDSW,B'11000000'                                 0316
         BZ    @RF00316                                            0316
*     DO;                                                          0317
*       IPENDSW='0'B;               /* YES, RESET INTERRUPT PENDING
*                                      SWITCH                        */
*       BUSYSW='0'B;                /* RESET SIO BUSY SWITCH         */
         NI    IPENDSW,B'00111111'                                 0319
*       GOTO CLRPATH;               /* GO THROUGH CLEAR PATH SEQUENCE
*                                      AGAIN                         */
         B     CLRPATH                                             0320
*     END;                                                         0321
*   IF INTADDR^=LOGCUA THEN         /* Q. INTERRUPT FROM LOGREC    0322
*                                      DEVICE                        */
@RF00316 LH    @03,INTADDR                                         0322
         N     @03,@CF02443                                        0322
         CR    LOGCUA,@03                                          0322
         BE    @RF00322                                            0322
*     LPSW(WAITPSW);                /* NO, WAIT FOR IT               */
*                                                                  0323
         LPSW  WAITPSW                                             0323
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK IF INTERRUPT CAME IN WHILE WAITING FOR DEVICE END       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0324
*   IF DEVENDSW='1'B THEN           /* Q. WAITING FOR DEVICE END     */
@RF00322 TM    DEVENDSW,B'00100000'                                0324
         BNO   @RF00324                                            0324
*     IF(CSWCE='1'B×CSWDE='1'B)     /* YES,                          */
*         &CSWLOGP='0'B             /* INSURE                        */
*         &CSWATTN='0'B             /* THAT                          */
*         &CSWSM='0'B               /* CHANNEL END                   */
*         &CSWCUE='0'B              /* OR                            */
*         &CSWBUSY='0'B             /* DEVICE END                    */
*         &CSWUC='0'B               /* STATUS                        */
*         &CSWUE='0'B               /* WAS                           */
*         &CSWPCI='0'B              /* PRESENTED                     */
*         &CSWIL='0'B               /* IN                            */
*         &CSWPGMCK='0'B            /* THE                           */
*         &CSWPTNCK='0'B            /* CSW                           */
*         &CSWCDC='0'B              /* WITHOUT                       */
*         &CSWCCC='0'B              /* ANY OTHER                     */
*         &CSWICC='0'B              /* STATUS                        */
*         &CSWCHNCK='0'B THEN                                      0325
         TM    CSWCE,B'00001100'                                   0325
         BZ    @RF00325                                            0325
         TM    CSWLOGP,B'00000100'                                 0325
         BNZ   @RF00325                                            0325
         TM    CSWATTN,B'11110011'                                 0325
         BNZ   @RF00325                                            0325
         TM    CSWPCI,B'11111111'                                  0325
         BNZ   @RF00325                                            0325
*       IF CSWDE='1'B THEN          /* AT THIS POINT IT IS KNOWN THAT
*                                      THE ONLY STATUS PRESENTED WAS
*                                      CHANNEL END OR DEVICE END - 0326
*                                      NOW SEE IF DEVICE END WAS   0326
*                                      PRESENTED                     */
         TM    CSWDE,B'00000100'                                   0326
         BNO   @RF00326                                            0326
*         DO;                       /* DEVICE END HAS COME IN        */
*           RETCODE=IOGOOD;         /* INDICATE I/O SUCCESSFULLY   0328
*                                      COMPLETED                     */
         SLR   RETCODE,RETCODE                                     0328
*           GOTO RESTORE;           /* RESTORE SAVED I/O STATUS AND
*                                      RETURN TO CALLER              */
         B     RESTORE                                             0329
*         END;                                                     0330
*       ELSE                        /* CHANNEL END PRESENTED WITHOUT
*                                      DEVICE END                    */
*         LPSW(WAITPSW);            /* WAIT FOR DEVICE END INTERRUPT */
@RF00326 LPSW  WAITPSW                                             0331
*     ELSE                          /* AT THIS POINT IT IS KNOWN THAT
*                                      THE STATUS PRESENTED WAS    0332
*                                      SOMETHING OTHER THAN ONLY   0332
*                                      CHANNEL END OR DEVICE END     */
*       DO;                                                        0332
         B     @RC00325                                            0332
@RF00325 DS    0H                                                  0333
*         DEVENDSW='0'B;            /* CLEAR DEVICE END EXPECTED   0333
*                                      SWITCH                        */
         NI    DEVENDSW,B'11011111'                                0333
*         GOTO CHKCSW;              /* EXAMINE CSW STATUS MORE     0334
*                                      CLOSELY IN ORDER TO DETERMINE
*                                      WHAT TO DO                    */
         B     CHKCSW                                              0334
*       END;                                                       0335
*   ELSE                                                           0336
*                                                                  0336
*     /***************************************************************/
*     /*                                                             */
*     /* CHECK IF INTERRUPT CAME IN WHILE WAITING FOR SENSE TO       */
*     /* COMPLETE                                                    */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0336
*     IF SENSESW='1'B THEN          /* Q. WAITING FOR SENSE DATA     */
         B     @RC00324                                            0336
@RF00324 TM    SENSESW,B'00010000'                                 0336
         BNO   @RF00336                                            0336
*       IF(CSWCE='1'B×CSWDE='1'B)   /* YES,                          */
*           &CSWLOGP='0'B           /* INSURE THAT                   */
*           &CSWATTN='0'B           /* CHANNEL END OR DEVICE END     */
*           &CSWSM='0'B             /* WAS PRESENTED                 */
*           &CSWCUE='0'B            /* IN THE CSW                    */
*           &CSWBUSY='0'B           /* WITHOUT ANY ERROR STATUS      */
*           &CSWUC='0'B             /* OTHER THAN                    */
*           &CSWUE='0'B             /* CHANNEL DATA CHECK -          */
*           &CSWPCI='0'B            /* A CHANNEL DATA CHECK CONDITION*/
*           &CSWIL='0'B             /* INDICATES THAT                */
*           &CSWPGMCK='0'B          /* THE SENSE DATA TRANSFERRED    */
*           &CSWPTNCK='0'B          /* IS UNRELIABLE                 */
*           &CSWCCC='0'B            /* BUT THE OPERATION             */
*           &CSWICC='0'B            /* OTHERWISE PROCEEDED NORMALLY  */
*           &CSWCHNCK='0'B THEN                                    0337
         TM    CSWCE,B'00001100'                                   0337
         BZ    @RF00337                                            0337
         TM    CSWLOGP,B'00000100'                                 0337
         BNZ   @RF00337                                            0337
         TM    CSWATTN,B'11110011'                                 0337
         BNZ   @RF00337                                            0337
         TM    CSWPCI,B'11110111'                                  0337
         BNZ   @RF00337                                            0337
*         IF CSWDE='1'B THEN        /* AT THIS POINT IT IS KNOWN THAT
*                                      CHANNEL END OR DEVICE END WAS
*                                      PRESENTED WITHOUT ANY ERROR 0338
*                                      STATUS OTHER THAN CHANNEL DATA
*                                      CHECK - NOW SEE IF DEVICE END
*                                      IS INDICATED                  */
         TM    CSWDE,B'00000100'                                   0338
         BNO   @RF00338                                            0338
*           DO;                     /* DEVICE END HAS COME IN        */
*             SENSESW='0'B;         /* RESET SWITCH - SENSE IS     0340
*                                      COMPLETED                     */
         NI    SENSESW,B'11101111'                                 0340
*             IF RETRYSW='0'B       /* CHECK IF RETRY ONCE NOT YET 0341
*                                      ATTEMPTED                     */
*                 &(CSWCDC='1'B     /* CHECK FOR CHANNEL DATA CHECK  */
*                 ×BUSOUT='1'B      /* CHECK FOR BUS OUT CHECK       */
*                 ×DATACHK='1'B     /* CHECK FOR DATA CHECK CONDITION*/
*                 ×OVERRUN='1'B     /* CHECK FOR OVERRUN CONDITION   */
*                 ×(SEEKCHK='1'B&CMDREJ='0'B)) THEN/* CHECK FOR SEEK
*                                      CHECK WITHOUT COMMAND REJECT  */
         TM    RETRYSW,B'00001000'                                 0341
         BNZ   @RF00341                                            0341
         TM    CSWCDC,B'00001000'                                  0341
         BO    @RT00341                                            0341
         TM    BUSOUT,B'00101100'                                  0341
         BNZ   @RT00341                                            0341
         TM    SEEKCHK,B'00000001'                                 0341
         BNO   @RF00341                                            0341
         TM    CMDREJ,B'10000000'                                  0341
         BNZ   @RF00341                                            0341
@RT00341 DS    0H                                                  0342
*               DO;                 /* ATTEMPT RETRY IF ABOVE TESTS
*                                      SATISFIED                     */
*                 RETRYSW='1'B;     /* SET RETRY ONCE SWITCH         */
         OI    RETRYSW,B'00001000'                                 0343
*                 GOTO CAWSETUP;    /* GO BACK TO ATTEMPT RETRY      */
         B     CAWSETUP                                            0344
*               END;                                               0345
*             ELSE                                                 0346
*               ;                   /* OTHERWISE, QUIT               */
