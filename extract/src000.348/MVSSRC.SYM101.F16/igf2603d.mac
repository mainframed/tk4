         TITLE 'IGF2603D - MODE COMMAND PROCESSOR                      *
                        '
IGF2603D CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'IGF2603D  78.053'                                 0001
         DROP  @15
@PROLOG  BALR  @11,0                                               0001
@PSTART  LA    @12,4095(,@11)                                      0001
         USING @PSTART,@11                                         0001
         USING @PSTART+4095,@12                                    0001
         L     @00,@SIZDATD                                        0001
         GETMAIN  R,LV=(0)
         LR    @09,@01                                             0001
         USING @DATD,@09                                           0001
         ST    @13,@SA00001+4                                      0001
         LR    @13,@09                                             0001
         EJECT
*                                                                  0113
*   /*****************************************************************/
*   /*                                                               */
*   /* SAVE REGISTER 14 FOR RETURN AS NO SAVE WAS PROVIDED BY CALLER */
*   /* AND INITIALIZE THE TEXT POINTER                               */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0113
*   SAVERTN=RETRNREG;                                              0113
         LR    SAVERTN,RETRNREG                                    0113
*   TEXTPTR=XAL;                                                   0114
*                                                                  0114
         MVC   TEXTPTR(3),XAL(XSAPTR)                              0114
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE VARIABLES USED BY IGF2603D                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0115
*   RETCODE=0;                      /* RETURN CODE                   */
         SLR   @10,@10                                             0115
         STH   @10,RETCODE                                         0115
*   CPUADDR=0;                      /* CPU ADDRESS SET BY VALCPU   0116
*                                      SUBROUTINE                    */
*                                                                  0116
         STH   @10,CPUADDR                                         0116
*   /*****************************************************************/
*   /*                                                               */
*   /* MOVE THE IGF955I,IGF956I MESSAGE SKELETONS            @G55CP2P*/
*   /* INTO THE 955I,956I MESSAGE BUFFERS,                   @G55CP2P*/
*   /* CLEAR VARIABLE MESSAGE BUFFER USED TO BUILD           @G55CP2P*/
*   /* THE VARIABLE SECTIONS OF IGF955I AND IGF953I          @G55CP2P*/
*   /* AND INVOKE THE SYNTAX SUBROUTINE TO SYNTAX CHECK THE INPUT    */
*   /* SUPPLIED BY THE OPERATOR IN THE MODE COMMAND                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0117
*   BUF955=MSG955;                                                 0117
         MVC   BUF955(78),MSG955                                   0117
*   VBUFMSG=' ';                    /*                       @G55CP2P*/
         MVI   VBUFMSG+1,C' '                                      0118
         MVC   VBUFMSG+2(19),VBUFMSG+1                             0118
         MVI   VBUFMSG,C' '                                        0118
*   BUF956=MSG956;                  /*                       @G556P2P*/
         MVC   BUF956(58),MSG956                                   0119
*   CALL SYNTAX;                                                   0120
*                                                                  0120
         BAL   @14,SYNTAX                                          0120
*   /*****************************************************************/
*   /*                                                               */
*   /* IF THE RETURN FROM SYNTAX WAS NORMAL CONSTRUCT A DUMMY        */
*   /* THRESHOLD BLOCK (THB) USING THE BUILDTHB SUBROUTINE           */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0121
*   IF RETCODE=0 THEN                                              0121
         LH    @10,RETCODE                                         0121
         LTR   @10,@10                                             0121
         BNZ   @RF00121                                            0121
*     DO;                                                          0122
*       CALL BUILDTHB;                                             0123
*                                                                  0123
         BAL   @14,BUILDTHB                                        0123
*       /*************************************************************/
*       /*                                                           */
*       /* IF THE THB WAS CONSTRUCTED WITHOUT ERROR THEN THE THB OF  */
*       /* ALL ONLINE CPU'S THAT THE CHANGE WAS REQUESTED FOR WILL   */
*       /* HAVE THEIR THB REPLACED BY THE DUMMY THB VIA THE          */
*       /* SUBROUTINE STATCHAG                                       */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0124
*       IF RETCODE=0 THEN                                          0124
         LH    @10,RETCODE                                         0124
         LTR   @10,@10                                             0124
         BNZ   @RF00124                                            0124
*         CALL STATCHAG;                                           0125
         BAL   @14,STATCHAG                                        0125
*     END;                                                         0126
*                                                                  0126
@RF00124 DS    0H                                                  0127
*   /*****************************************************************/
*   /*                                                               */
*   /* AT THIS POINT PREPERATIONS WILL BE MADE TO RETURN TO THE      */
*   /* CALLER. IF THE XAE CONTAINS A NON-ZERO VALUE THEN IEE0503D    */
*   /* (SVC 34 MESSAGE WRITTER) WILL BE INVOKED.                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0127
*   RESPECIFY                                                      0127
*    (EXITREG,                                                     0127
*     ENTRYSAV) RSTD;                                              0127
@RF00121 DS    0H                                                  0128
*   IF XAE=0 THEN                                                  0128
         CLI   XAE(XSAPTR),0                                       0128
         BNE   @RF00128                                            0128
*     ENTRYSAV=SAVERTN;             /* NORMAL EXIT                   */
         LR    ENTRYSAV,SAVERTN                                    0129
*   ELSE                                                           0130
*     ENTRYSAV=ADDR(IEE0503D);      /* ERROR EXIT MESSAGE WRITTER  0130
*                                      REQUIRED                      */
*                                                                  0130
         B     @RC00128                                            0130
@RF00128 L     ENTRYSAV,@CV01921                                   0130
*   /*****************************************************************/
*   /*                                                               */
*   /* THE RETURN ADDRESS WILL NOW ALSO BE PLACED INTO REGISTER 10 AS*/
*   /* THE EXIT CODE WHICH FOLLOWS DOES NOT HAVE ADDRESSABILITY AND  */
*   /* CAN ONLY MANIPULATE REGISTERS                                 */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0131
*   EXITREG=SAVERTN;                /* RETURN ADDRESS IN REG 10      */
@RC00128 LR    EXITREG,SAVERTN                                     0131
*   RETURN TO EXITCODE;             /* GENERATE EPILOG CODE AND    0132
*                                      BRANCH TO EXIT ROUTINE        */
         L     @13,4(,@13)                                         0132
         L     @00,@SIZDATD                                        0132
         LR    @01,@09                                             0132
         FREEMAIN R,LV=(0),A=(1)
         LA    @14,EXITCODE                                        0132
         BR    @14                                                 0132
*EXITCODE:                                                         0133
*   RETRNREG=EXITREG;               /* REG 14 IS NOW SET TO RETURN TO
*                                      ORIGINAL CALLER OF IGF2603D   */
EXITCODE LR    RETRNREG,EXITREG                                    0133
*   ENTRYREG=ENTRYSAV;              /* LOAD REG 15 WITH ENTRY POINT
*                                      ADDRESS                       */
         LR    ENTRYREG,ENTRYSAV                                   0134
*   GOTO NEXTRTN;                   /* BRANCH TO ADDRESS SET UP IN 0135
*                                      REGISTER 15 (IEE0403D OR    0135
*                                      IEE0503D)                     */
         BR    ENTRYREG                                            0135
*   RESPECIFY                                                      0136
*    (EXITREG,                                                     0136
*     ENTRYSAV) UNRSTD;                                            0136
*   GEN(EJECT);                                                    0137
         EJECT
*                                                                  0138
*/*     STATCHAG - SUBROUTINE OF MODE COMMAND PROCESSOR (IGF2603D) 0138
*                                                                  0138
*       THIS SUBROUTINE IS INVOKED BY MAINLINE OF THE MODE COMMAND 0138
*       PROCESSOR. IT HAS TWO FUNCTIONS.                           0138
*                                                                  0138
*       ONE FUNCTION IS TO REPLACE THE THRESHOLD BLOCKS OF         0138
*       THE CPU'S SPECIFIED IN THE MODE COMMAND WITH THE DUMMY THRESHOL
*       BLOCK CONSTRUCTED BY THE BUILDTHB SUBROUTINE.  IF THE CPU=ALL
*       WAS SPECIFIED OR DEFAULTED THEN THE THRESHOLD BLOCK IN THE RWA
*       WILL ALSO BE REPLACED. IF THE THRESHOLD BLOCK TO BE REPLACED
*       IS A SR OR DG THRESHOLD BLOCK, THEN EACH SPECIFIED PROCESSOR
*       IS SIGNALED TO UPDATE CONTROL REGISTERS.                   0138
*                                                            @G556P2P
*       THE OTHER FUNCTION IS TO SET A BIT (PWAENABL) REQUESTING   0138
*       IGFPEXIT TO ENABLE THE HIGH SPEED BUFFER AND TLB  (MODELS  0138
*       168 AND 165-II ONLY) IN RESPONSE TO THE ENABLE KEYWORD ON  0138
*       THE MODE COMMAND. EACH SPECIFIED PROCESSOR IS SIGNALED TO  0138
*       PERFORM THE DESIRED FUNCTION.                              0138
*                                                                  0138
*       IN BOTH CASES A WTO IS ISSUED TO INFORM THE OPERATOR OF THE
*       ACTION TAKEN BY THE MODE COMMAND.                          0138
*                                                                  0138
*                                                                  0138
*       THE EXTERNAL PROCEDURE CALLING THIS SUBROUTINE MUST:       0138
*          1. HAVE SET THE VARIABLES 'EVENTID' AND 'CPUADDR'.      0138
*          2. HAVE PROVIDED AN 18 WORD REGISTER SAVE AREA WHOSE ADDRESS
*             IS IN REGISTER 13 AT ENTRY TO THIS SUBROUTINE.       0138
*          3. HAVE DECLARED THE STRUCTURES 'CPUSW', 'EVENTSW'      0138
*             AND THE VARIABLE 'RETCODE'.                          0138
*                                                                  0138
*                                                                  0138
*       AT EXIT ALL REGISTERS ARE RETURNED TO THE CALLER UNCHANGED.
*                                                                  0138
*                                                                    */
*                                                                  0138
*STATCHAG:                                                         0138
*   PROC;                                                          0138
*                                                                  0138
         B     @PB00002                                            0138
STATCHAG STM   @14,@07,@SA00002                                    0138
         STM   @09,@12,@SA00002+40                                 0138
*   /*****************************************************************/
*   /*                                                               */
*   /* INTERNAL DECLARES                                             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0139
*   DCL                                                            0139
*     RTNCDREG FIXED(31) REG(15);   /* RPSGNL RETURN CODE REG        */
*   DCL                                                            0140
*     REGONE FIXED(31) REG(1) RSTD; /* REGISTER 1 DECLARE            */
*   DCL                                                            0141
*     1 EXITSAVE BDY(DWORD),        /* SAVE AREA FOR PEXIT           */
*      2 REGSAVE CHAR(64),          /* REGISTER SAVE AREA            */
*      2 RTNPSW CHAR(8) BDY(DWORD), /* RETURN PSW                    */
*       3 SYSMASK CHAR(1),          /* SYSTEM MASK                   */
*       3 KEYMASK CHAR(1),          /* PROTECTION KEY AND MASK       */
*       3 * CHAR(2),                                               0141
*       3 RTNADDR PTR(31);          /* RETURN ADDRESS FOR PEXIT      */
*   RESPECIFY                                                      0142
*    (I,                                                           0142
*     CPUALREG) RSTD;                                              0142
*                                                                  0142
*   /*****************************************************************/
*   /*                                                               */
*   /* DETERMINE IF CPU=ALL WAS SPECIFIED OR DEFAULTED (CPUID = 1)   */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0143
*   IF CPUID=ALLCPUS                                               0143
*                                                                  0143
*   /*****************************************************************/
*   /*                                                               */
*   /* UPDATE THE REQUIRED RWA THRESHOLD BLOCK                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0143
*     THEN                                                         0143
         CLI   CPUID,1                                             0143
         BNE   @RF00143                                            0143
*     DO;                           /* CPU=ALL                       */
*       IF EVENTID=RECVRY THEN                                     0145
         CLI   EVENTID,2                                           0145
         BNE   @RF00145                                            0145
*         RWASRDF=DUMYTHB;          /* UPDATE SR THRESHOLD BLOCK     */
         L     @07,CVTPTR                                          0146
         L     @07,RVTPTR(,@07)                                    0146
         L     @07,RWAPTR(,@07)                                    0146
         MVC   RWASRDF(8,@07),DUMYTHB                              0146
*       ELSE                                                       0147
*         IF EVENTID=DG             /*                       @G556P2P*/
*           THEN                                                   0147
         B     @RC00145                                            0147
@RF00145 CLI   EVENTID,3                                           0147
         BNE   @RF00147                                            0147
*           RWADGDF=DUMYTHB;        /* UPDATE DG THRESHOLD BLOCK   0148
*                                                            @G556P2P*/
         L     @07,CVTPTR                                          0148
         L     @07,RVTPTR(,@07)                                    0148
         L     @07,RWAPTR(,@07)                                    0148
         MVC   RWADGDF(8,@07),DUMYTHB                              0148
*         ELSE                                                     0149
*           IF EVENTID=PD           /*                       @G55CP2P*/
*             THEN                                                 0149
         B     @RC00147                                            0149
@RF00147 CLI   EVENTID,4                                           0149
         BNE   @RF00149                                            0149
*             RWAPDDF=DUMYTHBL;     /* UPDATE PD THRESHOLD BLOCK   0150
*                                                            @G55CP2P*/
         L     @07,CVTPTR                                          0150
         L     @07,RVTPTR(,@07)                                    0150
         L     @07,RWAPTR(,@07)                                    0150
         MVC   RWAPDDF(16,@07),DUMYTHBL                            0150
*           ELSE                                                   0151
*             IF EVENTID=SD         /*                       @G55CP2P*/
*               THEN                                               0151
         B     @RC00149                                            0151
@RF00149 CLI   EVENTID,5                                           0151
         BNE   @RF00151                                            0151
*               RWASDDF=DUMYTHBL;   /* UPDATE SD THRESHOLD BLOCK   0152
*                                                            @G55CP2P*/
         L     @07,CVTPTR                                          0152
         L     @07,RVTPTR(,@07)                                    0152
         L     @07,RWAPTR(,@07)                                    0152
         MVC   RWASDDF(16,@07),DUMYTHBL                            0152
*             ELSE                                                 0153
*               IF EVENTID=IV       /*                       @G55CP2P*/
*                 THEN                                             0153
         B     @RC00151                                            0153
@RF00151 CLI   EVENTID,6                                           0153
         BNE   @RF00153                                            0153
*                 RWAIVDF=DUMYTHBL; /* UPDATE IV THRESHOLD BLOCK   0154
*                                                            @G55CP2P*/
         L     @07,CVTPTR                                          0154
         L     @07,RVTPTR(,@07)                                    0154
         L     @07,RWAPTR(,@07)                                    0154
         MVC   RWAIVDF(16,@07),DUMYTHBL                            0154
*               ELSE                                               0155
*                 IF EVENTID=TC     /*                       @G55CP2P*/
*                   THEN                                           0155
         B     @RC00153                                            0155
@RF00153 CLI   EVENTID,7                                           0155
         BNE   @RF00155                                            0155
*                   RWATCDF=DUMYTHBL;/* UPDATE TC THRESHOLD BLOCK  0156
*                                                            @G55CP2P*/
         L     @07,CVTPTR                                          0156
         L     @07,RVTPTR(,@07)                                    0156
         L     @07,RWAPTR(,@07)                                    0156
         MVC   RWATCDF(16,@07),DUMYTHBL                            0156
*                 ELSE                                             0157
*                   IF EVENTID=PT   /*                       @G55CP2P*/
*                     THEN                                         0157
         B     @RC00155                                            0157
@RF00155 CLI   EVENTID,8                                           0157
         BNE   @RF00157                                            0157
*                     RWAPTDF=DUMYTHBL;/* UPDATE PT THRESHOLD BLOCK
*                                                            @G55CP2P*/
         L     @07,CVTPTR                                          0158
         L     @07,RVTPTR(,@07)                                    0158
         L     @07,RWAPTR(,@07)                                    0158
         MVC   RWAPTDF(16,@07),DUMYTHBL                            0158
*                   ELSE                                           0159
*                     IF EVENTID=CC /*                       @G55CP2P*/
*                       THEN                                       0159
         B     @RC00157                                            0159
@RF00157 CLI   EVENTID,9                                           0159
         BNE   @RF00159                                            0159
*                       RWACCDF=DUMYTHBL;/* UPDATE CC THRESHOLD BLOCK
*                                                            @G55CP2P*/
*                                                                  0160
         L     @07,CVTPTR                                          0160
         L     @07,RVTPTR(,@07)                                    0160
         L     @07,RWAPTR(,@07)                                    0160
         MVC   RWACCDF(16,@07),DUMYTHBL                            0160
*       /*************************************************************/
*       /*                                                           */
*       /* INITIALIZE LOOP INDEX AND LOOP CONTROL                    */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0161
*       I=0;                                                       0161
@RF00159 DS    0H                                                  0161
@RC00157 DS    0H                                                  0161
@RC00155 DS    0H                                                  0161
@RC00153 DS    0H                                                  0161
@RC00151 DS    0H                                                  0161
@RC00149 DS    0H                                                  0161
@RC00147 DS    0H                                                  0161
@RC00145 SLR   I,I                                                 0161
*       SINGLCPU=BITOFF;                                           0162
         NI    SINGLCPU,B'10111111'                                0162
*     END;                                                         0163
*                                                                  0163
*   /*****************************************************************/
*   /*                                                               */
*   /* IF A SINGLE CPU WAS SPECIFIED SET THE LOOP INDEX TO THE CPU   */
*   /* VALUE AND SET THE SINGLE CPU FLAG                             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0164
*   ELSE                                                           0164
*     DO;                                                          0164
         B     @RC00143                                            0164
@RF00143 DS    0H                                                  0165
*       I=CPUADDR;                                                 0165
         LH    I,CPUADDR                                           0165
*       SINGLCPU=BITON;                                            0166
         OI    SINGLCPU,B'01000000'                                0166
*     END;                                                         0167
*                                                                  0167
*   /*****************************************************************/
*   /*                                                               */
*   /* A TEST WILL NOW BE MADE TO DETERMINE IF THE CPU(S) SPECIFIED  */
*   /* ARE ON LINE. IF ON LINE THE VARIABLE 'PCCAPTR' WILL BE SET TO */
*   /* POINT TO THE PCCA OF THE REQUESTED CPU. THE THB IS LOCATED AND*/
*   /* UPDATED. IF BUFFER/TLB UPDATING IS REQUESTED, A BIT IN THE PWA*/
*   /* OF THE REQUESTED CPU IS SET. IF THE THRESHOLD BLOCK IS A SR OR*/
*   /* DG THRESHOLD BLOCK, A SIGP WILL BE ISSUED TO CAUSE THE CPU'S  */
*   /* CONTROL REGISTERS TO BE UPDATED AND/OR TO CAUSE ENABLEMENT OF */
*   /* THAT CPU'S BUFFER/TLB                                 @G556P2P*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0168
*   DO WHILE I^>CVTMAXMP;                                          0168
@RC00143 B     @DE00168                                            0168
@DL00168 DS    0H                                                  0169
*     CPUALREG=CSDCPUAL;            /* OBTAIN BIT MASK OF ONLINE   0169
*                                      CPU'S                         */
         L     @07,CVTPTR                                          0169
         L     @07,CVTCSD(,@07)                                    0169
         LH    CPUALREG,CSDCPUAL(,@07)                             0169
         N     CPUALREG,@CF02044                                   0169
*     GEN REFS(I) SETS(CPUALREG)(SLL CPUALREG,16(I));/* SHIFT LEFT BY
*                                      I                             */
*                                                                  0170
         SLL CPUALREG,16(I)
*     /***************************************************************/
*     /*                                                             */
*     /* IF THE REGISTER CPUALREG CONTAINES A NEGATIVE VALUE THEN THE*/
*     /* CPU WAS ON LINE AND PROCESSING CONTINUES. IF NOT NEGATIVE   */
*     /* THEN THE NEXT PROCESSOR WILL BE CHECKED UNTIL ALL CPU'S     */
*     /* SPECIFIED HAVE BEEN TESTED                                  */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0171
*     IF CPUALREG<0 THEN                                           0171
         LTR   CPUALREG,CPUALREG                                   0171
         BNM   @RF00171                                            0171
*       DO;                                                        0172
*         PCCAPTR=PCCAT00P(I+1);                                   0173
*                                                                  0173
         LR    @07,I                                               0173
         SLA   @07,2                                               0173
         L     @06,CVTPTR                                          0173
         L     @06,CVTPCCAT(,@06)                                  0173
         L     PCCAPTR,PCCAT00P(@07,@06)                           0173
*         /***********************************************************/
*         /*                                                         */
*         /* DETERMINE IF BUFFER/TLB IS TO BE ENABLED- IF SO, SET BIT*/
*         /* TO REQUEST ENABLE                               @G556P2P*/
*         /*                                                         */
*         /***********************************************************/
*                                                                  0174
*         IF DUMYENAB^=0            /* IS ENABLE IN ORDER    @G556P2P*/
*           THEN                                                   0174
         CLI   DUMYENAB,0                                          0174
         BE    @RF00174                                            0174
*           DO;                     /* YES - SET PWA TO      @G556P2P*/
*             PWAENABL='1'B;        /* REQUEST BUFF/TLB      @G556P2P*/
         L     @07,PWAPTR(,PCCAPTR)                                0176
         OI    PWAENABL(@07),B'00010000'                           0176
*           END;                    /* ENABLE                @G556P2P*/
*                                                                  0177
*         /***********************************************************/
*         /*                                                         */
*         /* DETERMINE THRESHOLD BLOCK TO BE UPDATED, SET THE        */
*         /* VARIABLE 'THBPTR' TO THE ADDRESS OF THE ONE TO BE       */
*         /* UPDATED AND MOVE THB TYPE INTO THE IGF955I MESSAGE      */
*         /* BUFFER                                          @G55CP2P*/
*         /*                                                         */
*         /***********************************************************/
*                                                                  0178
*         IF EVENTID=RECVRY         /* TEST FOR SR                   */
*           THEN                                                   0178
@RF00174 CLI   EVENTID,2                                           0178
         BNE   @RF00178                                            0178
*           DO;                                                    0179
*             THBPTR=ADDR(PWASRTHB);                               0180
         L     @07,PWAPTR(,PCCAPTR)                                0180
         LA    @07,PWASRTHB(,@07)                                  0180
         ST    @07,THBPTR                                          0180
*             EVENT955='SR';                                       0181
         MVC   EVENT955(2),@CC01659                                0181
*           END;                                                   0182
*         ELSE                                                     0183
*           IF EVENTID=DG           /* TEST FOR DG           @G55CP2P*/
*             THEN                                                 0183
         B     @RC00178                                            0183
@RF00178 CLI   EVENTID,3                                           0183
         BNE   @RF00183                                            0183
*             DO;                   /*                       @G55CP2P*/
*               THBPTR=ADDR(PWADGTHB);                             0185
         L     @07,PWAPTR(,PCCAPTR)                                0185
         LA    @07,PWADGTHB(,@07)                                  0185
         ST    @07,THBPTR                                          0185
*               EVENT955='DG';                                     0186
         MVC   EVENT955(2),@CC01663                                0186
*             END;                                                 0187
*           ELSE                                                   0188
*             IF EVENTID=PD         /* TEST FOR PD           @G55CP2P*/
*               THEN                                               0188
         B     @RC00183                                            0188
@RF00183 CLI   EVENTID,4                                           0188
         BNE   @RF00188                                            0188
*               DO;                 /*                       @G55CP2P*/
*                 THBPTR=ADDR(PWAPDTHB);/*                   @G55CP2P*/
         L     @07,PWAPTR(,PCCAPTR)                                0190
         LA    @07,PWAPDTHB(,@07)                                  0190
         ST    @07,THBPTR                                          0190
*                 EVENT955='PD';    /*                       @G55CP2P*/
         MVC   EVENT955(2),@CC01667                                0191
*               END;                /*                       @G55CP2P*/
*             ELSE                                                 0193
*               IF EVENTID=SD       /* TEST FOR SD           @G55CP2P*/
*                 THEN                                             0193
         B     @RC00188                                            0193
@RF00188 CLI   EVENTID,5                                           0193
         BNE   @RF00193                                            0193
*                 DO;               /*                       @G55CP2P*/
*                   THBPTR=ADDR(PWASDTHB);/*                 @G55CP2P*/
         L     @07,PWAPTR(,PCCAPTR)                                0195
         LA    @07,PWASDTHB(,@07)                                  0195
         ST    @07,THBPTR                                          0195
*                   EVENT955='SD';  /*                       @G55CP2P*/
         MVC   EVENT955(2),@CC01671                                0196
*                 END;              /*                       @G55CP2P*/
*               ELSE                                               0198
*                 IF EVENTID=IV     /* TEST FOR IV           @G55CP2P*/
*                   THEN                                           0198
         B     @RC00193                                            0198
@RF00193 CLI   EVENTID,6                                           0198
         BNE   @RF00198                                            0198
*                   DO;             /*                       @G55CP2P*/
*                     THBPTR=ADDR(PWAIVTHB);/*               @G55CP2P*/
         L     @07,PWAPTR(,PCCAPTR)                                0200
         LA    @07,PWAIVTHB(,@07)                                  0200
         ST    @07,THBPTR                                          0200
*                     EVENT955='IV';/*                       @G55CP2P*/
         MVC   EVENT955(2),@CC01675                                0201
*                   END;            /*                       @G55CP2P*/
*                 ELSE                                             0203
*                   IF EVENTID=TC   /* TEST FOR TC           @G55CP2P*/
*                     THEN                                         0203
         B     @RC00198                                            0203
@RF00198 CLI   EVENTID,7                                           0203
         BNE   @RF00203                                            0203
*                     DO;           /*                       @G55CP2P*/
*                       THBPTR=ADDR(PWATCTHB);/*             @G55CP2P*/
         L     @07,PWAPTR(,PCCAPTR)                                0205
         LA    @07,PWATCTHB(,@07)                                  0205
         ST    @07,THBPTR                                          0205
