         LA    I,1                                                 0570
         B     @DE00570                                            0570
@DL00570 L     @10,TEXTPTR-1                                       0570
         LR    @07,I                                               0570
         MH    @07,@CH02040                                        0570
         LR    @06,TABADDR                                         0570
         AL    @06,@CF02051                                        0570
         SLR   @05,@05                                             0570
         IC    @05,ELEMSIZE-3(@07,@06)                             0570
         BCTR  @05,0                                               0570
         ALR   @07,TABADDR                                         0570
         AL    @07,@CF02052                                        0570
         EX    @05,@SC02053                                        0570
         BE    @DC00570                                            0570
*     END;                          /* END OF SINGLE TABLE SEARCH  0571
*                                      LOOP                          */
*                                                                  0571
         AL    I,@CF00050                                          0571
@DE00570 CH    I,TABLENGH(,TABADDR)                                0571
         BNH   @DL00570                                            0571
@DC00570 DS    0H                                                  0572
*     /***************************************************************/
*     /*                                                             */
