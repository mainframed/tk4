*               /* INTERVAL SPECIFIED WITH RECORD=ALL. 6. EVENT NOT  */
*               /* SPECIFIED, BUT SOMETHING ELSE IS. 7. ENABLE       */
*               /* SPECIFIED AND EVENT IS NOT DG.            @G55CP2P*/
*               /*                                                   */
*               /*****************************************************/
*                                                                  0319
*                 ELSE                                             0319
*                   IF              /*                       @G55CP2P*/
*                       ((EVENTID=PD×EVENTID=SD×EVENTID=IV×/*      0319
*                                                            @G55CP2P*/
*                       EVENTID=TC×EVENTID=PT×EVENTID=CC)&/* @G55CP2P*/
*                       (REPTID^=0×MODEID=QUIET×ENABID^=0))×/*     0319
*                                                            @G55CP2P
*                                                            @G55CP2P*/
*                       (MODEID^=RECDALL&REPTID^=0)×/*       @G55CP2P
*                                                            @G55CP2P*/
*                       (ENABID=ENABLEN&MODEID=QUIET)×/*     @G55CP2P
*                                                            @G55CP2P*/
*                       (INTVID^=0&(EVENTID=RECVRY×EVENTID=DG))×/* 0319
*                                                            @G55CP2P
*                                                            @G55CP2P*/
*                       (INTVID^=0&MODEID=RECDALL)×/*        @G55CP2P
*                                                            @G55CP2P*/
*                       (EVENTID=0&(MODEID^=0×CPUID^=0×/*    @G55CP2P*/
*                       REPTID^=0×INTVID^=0×/*               @G55CP2P*/
*                       ENABID^=0))×/*                       @G55CP2P
*                                                            @G55CP2P*/
*                       (ENABID^=0&EVENTID^=DG)/*            @G55CP2P
*                                                            @G55CP2P*/
*                     THEN                                         0319
         B     @RC00307                                            0319
@RF00307 CLI   EVENTID,4                                           0319
         BE    @GL00014                                            0319
         CLI   EVENTID,5                                           0319
         BE    @GL00014                                            0319
         CLI   EVENTID,6                                           0319
         BE    @GL00014                                            0319
         CLI   EVENTID,7                                           0319
         BE    @GL00014                                            0319
         CLI   EVENTID,8                                           0319
         BE    @GL00014                                            0319
         CLI   EVENTID,9                                           0319
         BNE   @GL00013                                            0319
@GL00014 CLI   REPTID,0                                            0319
