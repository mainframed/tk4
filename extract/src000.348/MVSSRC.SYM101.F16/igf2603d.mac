         BE    @RT00327                                            0327
         CLC   PCCACPID+8(4,PCCAPTR),M3033                         0327
         BNE   @RF00327                                            0327
@RT00327 DS    0H                                                  0328
*                           RETCODE=0;/* YES SET ZERO RETURN CODE  0328
*                                                            @G556P2P*/
         SLR   @10,@10                                             0328
         STH   @10,RETCODE                                         0328
*                         ELSE                                     0329
*                           DO;     /* NO - ISSUE INVALID CPU MESSAGE
*                                                            @G556P2P*/
         B     @RC00327                                            0329
@RF00327 DS    0H                                                  0330
*                             RETCODE=4;/*                   @G556P2P*/
         MVC   RETCODE(2),@CH00038                                 0330
*                             CALL WTO959I;/*                @G556P2P*/
         BAL   @14,WTO959I                                         0331
*                             RETCODE=24;/*                  @G556P2P*/
         MVC   RETCODE(2),@CH00144                                 0332
*                           END;    /*                       @G556P2P*/
*                                                                  0333
