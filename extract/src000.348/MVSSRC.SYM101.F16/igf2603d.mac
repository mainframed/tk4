@GL00013 CLI   MODEID,1                                            0319
         BE    @GL00012                                            0319
         CLI   REPTID,0                                            0319
         BNE   @RT00319                                            0319
@GL00012 CLI   ENABID,1                                            0319
         BNE   @GL00011                                            0319
         CLI   MODEID,4                                            0319
         BE    @RT00319                                            0319
@GL00011 CLI   INTVID,0                                            0319
         BE    @GL00010                                            0319
         CLI   EVENTID,2                                           0319
         BE    @RT00319                                            0319
         CLI   EVENTID,3                                           0319
         BE    @RT00319                                            0319
@GL00010 CLI   INTVID,0                                            0319
         BE    @GL00009                                            0319
         CLI   MODEID,1                                            0319
         BE    @RT00319                                            0319
@GL00009 CLI   EVENTID,0                                           0319
         BNE   @GL00008                                            0319
         CLI   MODEID,0                                            0319
         BNE   @RT00319                                            0319
         CLI   CPUID,0                                             0319
         BNE   @RT00319                                            0319
         CLI   REPTID,0                                            0319
         BNE   @RT00319                                            0319
         CLI   INTVID,0                                            0319
         BNE   @RT00319                                            0319
         CLI   ENABID,0                                            0319
         BNE   @RT00319                                            0319
@GL00008 CLI   ENABID,0                                            0319
         BE    @RF00319                                            0319
         CLI   EVENTID,3                                           0319
         BE    @RF00319                                            0319
@RT00319 DS    0H                                                  0320
*                     DO;           /*                       @G55CP2P*/
*                       XAE=CONFLICT;/*                      @G55CP2P*/
         MVI   XAE(XSAPTR),X'0C'                                   0321
*                       RETCODE=16; /*                       @G55CP2P*/
         MVC   RETCODE(2),@CH00040                                 0322
*                     END;          /*                       @G55CP2P*/
*                   ELSE                                           0324
*                     DO;           /* NO CONFLICT           @G55CP2P*/
         B     @RC00319                                            0324
@RF00319 DS    0H                                                  0325
*                       PCCAPTR=PCCAT00P(PSACPUPA+1);/*      @G556P2P*/
         LH    @10,PSACPUPA                                        0325
         SLA   @10,2                                               0325
         L     @03,CVTPTR                                          0325
         L     @01,CVTPCCAT(,@03)                                  0325
         L     PCCAPTR,PCCAT00P(@10,@01)                           0325
*                       IF ENABID^=0 THEN/* ENABLE SPECIFIED @G556P2P*/
         CLI   ENABID,0                                            0326
         BE    @RF00326                                            0326
*                         IF PCCACPID(9:12)=MOD168×/*        @G556P2P*/
*                             PCCACPID(9:12)=MOD165×/*       @G556P2P*/
*                             PCCACPID(9:12)=M168AP×/*       @G556P2P*/
*                             PCCACPID(9:12)=M3032×/*        @ZA30079*/
*                             PCCACPID(9:12)=M3033/* DOES PROCESSOR
*                                      SUPPORT ENABLE        @ZM45593*/
*                           THEN                                   0327
         CLC   PCCACPID+8(4,PCCAPTR),MOD168                        0327
         BE    @RT00327                                            0327
         CLC   PCCACPID+8(4,PCCAPTR),MOD165                        0327
         BE    @RT00327                                            0327
         CLC   PCCACPID+8(4,PCCAPTR),M168AP                        0327
         BE    @RT00327                                            0327
