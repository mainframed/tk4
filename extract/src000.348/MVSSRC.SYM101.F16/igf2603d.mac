*                   RETCODE=12;                                    0338
         B     @RC00336                                            0338
@RF00336 MVC   RETCODE(2),@CH00033                                 0338
*             END;                                                 0339
@RC00336 DS    0H                                                  0339
@RC00306 DS    0H                                                  0340
*       END;                        /* END OF SYNTAX CHECK LOOP      */
@RC00302 DS    0H                                                  0340
@RC00297 DS    0H                                                  0340
@DE00294 TM    ENDBIT,B'10000000'                                  0340
         BZ    @DL00294                                            0340
*     END;                                                         0341
*   END SYNTAX;                                                    0342
@EL00003 DS    0H                                                  0342
@EF00003 DS    0H                                                  0342
@ER00003 LM    @14,@07,@SA00003                                    0342
