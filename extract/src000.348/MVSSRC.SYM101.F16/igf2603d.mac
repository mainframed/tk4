*   IF INPUTEXT(1)=',' THEN                                        0374
         L     @10,TEXTPTR-1                                       0374
         LA    @10,0(,@10)                                         0374
         CLI   INPUTEXT(@10),C','                                  0374
         BNE   @RF00374                                            0374
*     TEXTPTR=TEXTPTR+1;                                           0375
*                                                                  0375
         LA    @10,1(,@10)                                         0375
         STCM  @10,7,TEXTPTR                                       0375
*   /*****************************************************************/
*   /*                                                               */
*   /* IF NOT A COMMA, CHECK FOR A BLANK. IF NON-BLANK THEN SET XAE =*/
*   /* '07'X, SET THE RETURN CODE TO 8 AND RETURN                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0376
*   ELSE                                                           0376
*     IF INPUTEXT(1)^=' ' THEN                                     0376
         B     @RC00374                                            0376
@RF00374 L     @01,TEXTPTR-1                                       0376
         CLI   INPUTEXT(@01),C' '                                  0376
         BE    @RF00376                                            0376
*       DO;                                                        0377
*         XAE=DELIMERR;             /* INDICATE INVALID DELIMITOR    */
         MVI   XAE(XSAPTR),X'07'                                   0378
*         RETCODE=8;                                               0379
         MVC   RETCODE(2),@CH00048                                 0379
*       END;                                                       0380
*                                                                  0380
*   /*****************************************************************/
*   /*                                                               */
*   /* IF BLANK THEN THE RETURN CODE IS SET TO 0 AND CONTROL IS      */
*   /* RETURNED TO THE CALLER.                                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0381
*     ELSE                          /*                       @ZM45592*/
*       RETCODE=0;                  /*                       @ZM45592*/
         B     @RC00376                                            0381
@RF00376 SLR   @10,@10                                             0381
         STH   @10,RETCODE                                         0381
*   END ENDTEXT;                                                   0382
@EL00005 DS    0H                                                  0382
