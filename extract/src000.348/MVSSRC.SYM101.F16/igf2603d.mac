*ELBAIRAV:                                                         0344
*   PROC;                                                          0344
         B     @PB00004                                            0344
ELBAIRAV STM   @14,@12,@SA00004                                    0344
*   RESPECIFY                                                      0345
*     I RSTD;                       /* RESTRICT LOOP CONTROL         */
*                                                                  0345
*   /*****************************************************************/
*   /*                                                               */
*   /* DETERMINE LENGTH OF VARIABLE FIELD IN VERB UP TO A MAXIMUM OF */
*   /* SIX CHARACTERS                                                */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0346
*   DO I=1 TO 6 WHILE INPUTEXT(I)^=','&INPUTEXT(I)^=' ';           0346
         LA    I,1                                                 0346
@DL00346 L     @08,TEXTPTR-1                                       0346
         LA    @08,0(,@08)                                         0346
         LR    @01,@08                                             0346
         ALR   @01,I                                               0346
         BCTR  @01,0                                               0346
         CLI   INPUTEXT(@01),C','                                  0346
         BE    @DC00346                                            0346
         ALR   @08,I                                               0346
         BCTR  @08,0                                               0346
         CLI   INPUTEXT(@08),C' '                                  0346
         BE    @DC00346                                            0346
*   END;                            /* END OF LENGTH DETERMINATION 0347
*                                      LOOP                          */
*                                                                  0347
         AL    I,@CF00050                                          0347
         C     I,@CF00808                                          0347
         BNH   @DL00346                                            0347
@DC00346 DS    0H                                                  0348
*   /*****************************************************************/
