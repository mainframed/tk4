         LTR   @10,@10                                             0362
         BZ    @RF00362                                            0362
*         DO;                                                      0363
*           XAE=INVALNUM;           /* INDICATE INVALID NUMERICS     */
         MVI   XAE(XSAPTR),X'06'                                   0364
*           RETCODE=4;                                             0365
         MVC   RETCODE(2),@CH00038                                 0365
*         END;                                                     0366
*       ELSE                                                       0367
*         ;                                                        0367
@RF00362 DS    0H                                                  0368
*     END;                                                         0368
