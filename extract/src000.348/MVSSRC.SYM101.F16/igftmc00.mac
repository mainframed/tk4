*/********************************************************************/
*/*                                                                  */
*/*   IGFTMC01: CHECK UCBS FOR MISSING INTERRUPTION                  */
*/*                                                                  */
*/*   INPUT:    REGISTER 0 WILL CONTAIN THE ADDRESS OF SRB           */
*/*             REGISTER 1 WILL CONTAIN THE ADDRESS OF PARAMETER     */
*/*             LIST:                                                */
*/*                 1.PTR TO DDRCOM                                  */
*/*                 2.PTR TO TQE OR IF INITIALIZATION        @G24LNLC*/
*/*                 3.PTR TO ECB                                     */
*/*                                                                  */
*/*   FUNCTION: IF INITIALIZATION, CHECK EXIT TABLES FOR     @G24LNLC*/
*/*             ZERO ENTRIES AND SET THEM TO BR14 ADDRESS OTHERWISE  */
*/*                                                          @G24LNLC*/
*/*             1.CHECK THE UCB LIST FOR MISSING INTERRUPTION        */
*/*             2.SCHEDULE  I/O RESTART TO CLEAR MISSING             */
*/*               INTERRUPTION FOR CHANNEL END,DEVICE END,CHANNEL    */
*/*               END AND DEVICE END, MOUNT,SWAP,OR DEVICE END       */
*/*               ON A PAGE PACK.                                    */
*/*             3.BUILD MESSAGE QUEUE FOR PENDING CONDITIONS         */
*/*                                                                  */
*/*   OUTPUT:   POST IGFTMCHK FOR OUTAGE RECORD                      */
*/*             MESSAGE QUEUE WILL BE BUILT IF PENDING CONDITION     */
*/*             OCCURRED                                             */
*/*                                                                  */
*/********************************************************************/
*                                                                  0267
*IGFTMC01:                                                         0267
*   ENTRY;                                                         0267
@EP00267 DS    0H                                                  0268
*   RFY                                                            0268
*    (R1,                                                          0268
*     R0,                                                          0268
*     SAVEPTR,                                                     0268
*     R14) RSTD;                    /* RESTRICT REG 1 AND 14         */
*   RFY                                                            0269
*     INPARM BASED(R1);             /* GET INPUT PARAM               */
*   TQEPTR=INTQE;                   /* SET TQE ADDRESSABILITY        */
         L     TQEPTR,INTQE(,R1)                                   0270
*   DDRPTR=INDDRCM;                 /* SET DDRCOM ADDRESSABILITY     */
         L     DDRPTR,INDDRCM(,R1)                                 0271
*   ECBPTR=INECB;                   /* SET ECB ADDRESSABILITY        */
         L     ECBPTR,INECB(,R1)                                   0272
*   RFY                                                            0273
*     R1 UNRSTD;                    /* RELEASE REG 1                 */
*   R14SAVE=R14;                    /* SAVE RETURN ADDRESS           */
         ST    R14,R14SAVE(,DDRPTR)                                0274
*   RFY                                                            0275
*     R14 UNRSTD;                   /* RELEASE REG 14                */
*   SAVEPTR=ADDR(TMC01SAV);         /* REGISTER 13 PTR TO SAVEAREA   */
         LA    SAVEPTR,TMC01SAV(,DDRPTR)                           0276
*   IF TQEPTR=0 THEN                                               0277
         LTR   TQEPTR,TQEPTR                                       0277
         BNZ   @RF00277                                            0277
*     DO;                           /* INITIALIZATION CALL IF NO   0278
*                                      TQEPTR                @G24LNLC*/
*       DO I=1 TO DIM(TMCEXT1);     /* INITIALIZE ONCE FOR MIH EXIT
*                                      TABLE                 @G24LNLC*/
         LA    I,1                                                 0279
@DL00279 DS    0H                                                  0280
*         IF TMCEX1(I)=ZERO THEN                                   0280
         LR    @12,I                                               0280
         SLA   @12,3                                               0280
         L     @11,TMCEX1-8(@12)                                   0280
         LTR   @11,@11                                             0280
         BNZ   @RF00280                                            0280
*           TMCEX1(I)=ADDR(TMCBR14);                               0281
         LA    @11,TMCBR14                                         0281
         ST    @11,TMCEX1-8(@12)                                   0281
*       END;                                                       0282
@RF00280 AL    I,@CF00046                                          0282
         C     I,@CF00046                                          0282
         BNH   @DL00279                                            0282
*       DO I=1 TO DIM(TMCEXT2);     /* INITIALIZE UCB EXIT TABLE   0283
*                                                            @G24LNLC*/
         LA    I,1                                                 0283
@DL00283 DS    0H                                                  0284
*         IF TMCEX2(I)=ZERO THEN                                   0284
         LR    @12,I                                               0284
         SLA   @12,3                                               0284
         L     @11,TMCEX2-8(@12)                                   0284
         LTR   @11,@11                                             0284
         BNZ   @RF00284                                            0284
*           TMCEX2(I)=ADDR(TMCBR14);/* G24LNLC                       */
         LA    @11,TMCBR14                                         0285
         ST    @11,TMCEX2-8(@12)                                   0285
*       END;                                                       0286
*                                                                  0286
@RF00284 AL    I,@CF00046                                          0286
         C     I,@CF00046                                          0286
         BNH   @DL00283                                            0286
*/* RESUME MUST BE ISSUED TO RETURN CONTROL TO IGFTMCHK WHICH IS   0287
*   SUSPENDED @G24LNLC                                               */
*                                                                  0287
*       RFY                                                        0287
*        (R4,                                                      0287
*         R5) RSTD;                 /* RESTRICT REGS FOR INTERFCE TO
*                                      RESUME                @G24LNLC*/
*       R4=R0->SRBPTCB;             /* SET UP TCB ADDRESS    @G24LNLC*/
         LR    @12,R0                                              0288
         L     R4,SRBPTCB(,@12)                                    0288
*       R5=R4->TCBRBP;              /* SET UP RB POINTER     @G24LNLC*/
         L     R5,TCBRBP(,R4)                                      0289
*       GEN REFS(CVTRSUME) EXIT NOSEQFLOW( RESUME RETURN=N);/* RESUME
*                                      IGFTMCHK              @G24LNLC*/
          RESUME RETURN=N
*       RFY                                                        0291
*        (R4,                                                      0291
*         R0,                                                      0291
*         R5) UNRSTD;                                              0291
*     END;                                                         0292
*                                                                  0293
*/********************************************************************/
*/*                                                                  */
*/*   ESTABLISH ERROR RECOVERY: SETFRR                               */
*/*                                                                  */
*/*   INPUT: PTR TO DDRCOM                                           */
*/*          PTR TO TQE                                              */
*/*          PTR TO ECB                                              */
*/*                                                                  */
*/*   FUNCTION: SET UP ERROR RECOVERY FOR IGFTMC01                   */
*/*             RETRY IS ATTEMPTED FOR THREE TIMES.                  */
*/*             IF THE ERROR STILL EXISTS THE MIH FUNCTION           */
*/*             WILL BE TERMINATED                                   */
*/*                                                                  */
*/*   OUTPUT: FRR WILL BE ESTABLISHED                                */
*/*                                                                  */
*/*                                                                  */
*/********************************************************************/
*                                                                  0293
*   RFY                                                            0293
*    (R1,                                                          0293
*     R4,                                                          0293
*     R5) RSTD;                     /* RESTRICT REG1,4,5             */
@RF00277 DS    0H                                                  0294
*   R1=ADDR(IGFTMFRR);              /* ADDRESS OF RECOVERY PROGRAM   */
         LA    R1,IGFTMFRR                                         0294
*   GEN REFS(R1,R4,R5,FRRPARM,FRRSCURR,FRRS,FRRSLAST,PSA,PSALSFCC, 0295
*       FRRSELEN,FRRSFRRA,FRRSENTR,FRRSPARM);                      0295
*                                            /*
 SETFRR  A,FRRAD=(R1),PARMAD=(FRRPARM),WRKREGS=(R4,R5),               **
                RELATED=('IGFTMFRR-FRR EXIT ROUTINE')
*   RFY                                                            0296
*    (R1,                                                          0296
*     R4,                                                          0296
*     R5) UNRSTD;                   /* RELEASE REG 1,4 AND 5         */
*   FRRTQE=TQEPTR;                  /* SET UP TQE ADDRESSABILITY IN
*                                      FRR                           */
         ST    TQEPTR,FRRTQE(,FRRPARM)                             0297
*   FRRECB=ECBPTR;                  /* SET UP ECB ADDRESSABILITY IN
*                                      FRR                           */
         ST    ECBPTR,FRRECB(,FRRPARM)                             0298
*   FRRDDRCM=DDRPTR;                /* SET UP DDRCOM ADDRESSABILITY
*                                      IN FRR                        */
         ST    DDRPTR,FRRDDRCM(,FRRPARM)                           0299
*   TMC01RTY=OFF;                   /* INIT FRR RETRY FLAG           */
         NI    TMC01RTY(DDRPTR),B'11011111'                        0300
*   RFY                                                            0301
*    (R1,                                                          0301
*     R14,                                                         0301
*     R15) RSTD;                                                   0301
*   R1=ADDR(MIHINTVL);              /* SET UP PARM FOR EXIT  @G24LNLC*/
         LA    R1,MIHINTVL(,DDRPTR)                                0302
*   FRROPTI=ON;                     /* INDICATE ONCE PER MIX EXIT TO
*                                      FRR                   @G24LNLC*/
         OI    FRROPTI(FRRPARM),B'10000000'                        0303
*   DO I=1 TO DIM(TMCEXT1);         /* CALL EACH EXIT        @G24LNLC*/
         LA    I,1                                                 0304
@DL00304 DS    0H                                                  0305
*     FRREXITI=I;                   /* SET UP EXIT INDEX FOR FRR   0305
*                                                            @G24LNLC*/
         STCM  I,7,FRREXITI(FRRPARM)                               0305
*     CALL TMCEX1(I);               /* GO TO EXIT            @G24LNLC*/
         LR    @12,I                                               0306
         SLA   @12,3                                               0306
         L     @15,TMCEX1-8(@12)                                   0306
         BALR  @14,@15                                             0306
*   END;                                                           0307
         AL    I,@CF00046                                          0307
         C     I,@CF00046                                          0307
         BNH   @DL00304                                            0307
*   FRREXITI=ZERO;                  /* INDICATE NO EXIT ACTIVE     0308
*                                                            @G24LNLC*/
         SLR   @12,@12                                             0308
         STCM  @12,7,FRREXITI(FRRPARM)                             0308
*   FRROPTI=OFF;                    /* SHOW UCB EXIT TYPE    @G24LNLC*/
         NI    FRROPTI(FRRPARM),B'01111111'                        0309
*   RFY                                                            0310
*    (R1,                                                          0310
*     R14,                                                         0310
*     R15) UNRSTD;                                                 0310
*                                                                  0311
*/********************************************************************/
*/*                                                                  */
*/*   SCAN UCB: CHECK UCBS FOR PENDING CONDITIONS                    */
*/*                                                                  */
*/*   INPUT:  PTR TO DDRCOM                                          */
*/*           PTR TO CVT                                             */
*/*                                                                  */
*/*                                                                  */
*/*   FUNCTION: SCAN UCB IN THE SYSTEM FOR MISSING INTERRUPTIONS     */
*/*             I.E.MOUNTS,SWAPS,CHANNEL ENDS OR DEVICE ENDS.        */
*/*             WHEN ONE IS FOUND THE NECESSARY INFORMATION IS       */
*/*             PUT INTO MIHCODE(LOCAL WORKAREA)TO INDICATE THE TYPE */
*/*             OF PENDING CONDITION. GO TO UCB SPECIFIED    @G24LNLC*/
*/*             MIHEXITS.                                    @G24LNLC*/
*/*                                                                  */
*/*   OUTPUT: MIHCODE IS SET FOR PENDING CONDITION                   */
*/*                                                                  */
*/*                                                                  */
*/********************************************************************/
*                                                                  0311
*   RFY                                                            0311
*     INDEXUCB RSTD;                /* RESTRICT REG FOR UCB INDEX    */
*   INDEXUCB=CVTILK2;               /* GET UCB LOOK UP TAB ADDRESS   */
         L     @12,CVTPTR                                          0312
         L     INDEXUCB,CVTILK2(,@12)                              0312
*   UCBPTR=TMCUCB;                  /* SET FIRST UCB PTR             */
         LH    UCBPTR,TMCUCB(,INDEXUCB)                            0313
         N     UCBPTR,@CF03319                                     0313
*   DO WHILE UCBPTR^=ENDUCB;        /* ALL UCBS ARE SEARCHED? NO - 0314
*                                      CHECK FOR PENDING             */
         B     @DE00314                                            0314
@DL00314 DS    0H                                                  0315
*     IF UCBPTR=ZERO×               /* UCB NOT EXIST?                */
*         UCBDVCLS=TP40×            /* TELEPROCESSING?       @ZA14026*/
*         (UCBONLI=OFF&UCBMTPXP=OFF)×/* OFFLINE AND NOT            0315
*                                      MULTI-EXPOSURE DEVICE?      0315
*                                                            @ZA30660*/
*         (UCBDVCLS=UR3HEX08&UCBUNTYP=UR4HEX1D)/* DON'T TIME 1419 PCU
*                                                            @ZA30660*/
*       THEN                                                       0315
         LTR   UCBPTR,UCBPTR                                       0315
         BZ    @RT00315                                            0315
         CLI   UCBDVCLS(UCBPTR),X'40'                              0315
         BE    @RT00315                                            0315
         TM    UCBONLI(UCBPTR),B'10000000'                         0315
         BNZ   @GL00001                                            0315
         TM    UCBMTPXP(UCBPTR),B'00001000'                        0315
         BZ    @RT00315                                            0315
@GL00001 CLI   UCBDVCLS(UCBPTR),X'08'                              0315
         BNE   @RF00315                                            0315
         CLI   UCBUNTYP(UCBPTR),X'1D'                              0315
         BE    @RT00315                                            0315
*       GOTO NEXTUCB;               /* YES-IGNORE CURRENT UCB        */
*     IF UCBALTPH=ON&(UCBTICBT=ON×UCBMIHTI^=ZERO) THEN/* DEVICE HAS
*                                      ALTERNATE PATHS AND HAS     0317
*                                      PREVIOUS CONDITION FOUND    0317
*                                                            @G24LNLC*/
@RF00315 TM    UCBALTPH(UCBPTR),B'00000001'                        0317
         BNO   @RF00317                                            0317
         TM    UCBTICBT(UCBPTR),B'00000010'                        0317
         BO    @RT00317                                            0317
         L     @12,UCBEXTPT(,UCBPTR)                               0317
         CLI   UCBMIHTI(@12),0                                     0317
         BE    @RF00317                                            0317
@RT00317 DS    0H                                                  0318
*       DO;                         /* YES                   @G24LNLC*/
*                                                                  0318
*         /***********************************************************/
*         /*                                                         */
*         /* IGNORE THE DUPLICATE MIH MESSAGE FOR THE ALTERNATE      */
*         /* PATH@G24LNLC                                            */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0319
*         RFY                                                      0319
*           INDEXALT RSTD;          /* RESTRICT REG 5        @G24LNLC*/
*         INDEXALT=CVTILK2;         /* PTR TO FIRST ENTRY IN UCB LIST
*                                                            @G24LNLC*/
         L     @12,CVTPTR                                          0320
         L     INDEXALT,CVTILK2(,@12)                              0320
*         DO WHILE(INDEXALT^=INDEXUCB);/* SCAN FOR THE ALTERNATH PATH
*                                                            @G24LNLC*/
         B     @DE00321                                            0321
@DL00321 DS    0H                                                  0322
*           IF ALTPHUCB=UCBPTR THEN /* FOUND ALTERNATE PATH? @G24LNLC*/
         LH    @12,ALTPHUCB(,INDEXALT)                             0322
         N     @12,@CF03319                                        0322
         CR    UCBPTR,@12                                          0322
         BE    @RT00322                                            0322
*             GOTO NEXTUCB;         /* CHECK NEXT UCB ON UCB LIST  0323
*                                                            @G24LNLC*/
*           ELSE                                                   0324
*             INDEXALT=INDEXALT+TWO;/* UPDATE TO NEXT UCB    @G24LNLC*/
         AL    INDEXALT,@CF00086                                   0324
*         END;                      /* END OF SEARCH UCB LIST      0325
*                                                            @G24LNLC*/
@DE00321 CR    INDEXALT,INDEXUCB                                   0325
         BNE   @DL00321                                            0325
*         RFY                                                      0326
*           INDEXALT UNRSTD;        /* RELEASE REG 5         @G24LNLC*/
*       END;                        /* END OF PROCESSING ALTERNATE 0327
*                                      PATH                  @G24LNLC*/
*     IF UCBMOUNT=ON&UCBALOC=ON&UCBMMSGP=OFF THEN/* MOUNT AND MESSAGE
*                                      PENDING & DEVICE ALLOCATED  0328
*                                                            @ZA15343*/
@RF00317 TM    UCBMOUNT(UCBPTR),B'10000000'                        0328
         BNO   @RF00328                                            0328
         TM    UCBALOC(UCBPTR),B'00001000'                         0328
         BNO   @RF00328                                            0328
         TM    UCBMMSGP(UCBPTR),B'00000100'                        0328
         BNZ   @RF00328                                            0328
*       IF UCB3DACC=ON×UCBNRY=ON&UCB3TAPE=ON THEN/* DIRECT ACCESS OR
*                                      TAPE IS ALLOCATED BUT NOT   0329
*                                      READY?                        */
         TM    UCB3DACC(UCBPTR),B'00100000'                        0329
         BO    @RT00329                                            0329
         TM    UCBNRY(UCBPTR),B'01000000'                          0329
         BNO   @RF00329                                            0329
         TM    UCB3TAPE(UCBPTR),B'10000000'                        0329
         BNO   @RF00329                                            0329
@RT00329 DS    0H                                                  0330
*         MIHMT=ON;                 /* YES-SET MOUNTING              */
         OI    MIHMT(DDRPTR),B'00000100'                           0330
*     IF UCBDDRSW=ON THEN           /* CHECK FOR SWAP PENDING        */
@RF00329 DS    0H                                                  0331
@RF00328 TM    UCBDDRSW(UCBPTR),B'00000001'                        0331
         BNO   @RF00331                                            0331
*       MIHSW=ON;                   /* YES-SET SWAP PENDING          */
         OI    MIHSW(DDRPTR),B'00001000'                           0332
*     IF UCBBSY=ON THEN             /* DEVICE BUSY?                  */
@RF00331 TM    UCBBSY(UCBPTR),B'10000000'                          0333
         BNO   @RF00333                                            0333
*       MIHDE=ON;                   /* YES-SET DEVICE PENDING        */
         OI    MIHDE(DDRPTR),B'00000001'                           0334
*     IF UCBACTV=ON THEN            /* CHANNEL STILL IS ACTIVE?      */
@RF00333 TM    UCBACTV(UCBPTR),B'00000010'                         0335
         BNO   @RF00335                                            0335
*       MIHCE=ON;                   /* YES-SET CHANNEL PENDING       */
         OI    MIHCE(DDRPTR),B'00000010'                           0336
*     MIHCODE=MIHTAB(MIHCOND+1);    /* SET MIHCODE FOR CONDITION   0337
*                                                            @G24LNLC*/
@RF00335 SLR   @12,@12                                             0337
         IC    @12,MIHCOND(,DDRPTR)                                0337
         LA    @15,MIHTAB(@12)                                     0337
         MVC   MIHCODE(1,DDRPTR),0(@15)                            0337
*     IF MIHCODE=DDRMIHC×MIHCODE=DDRMIHCD×(MIHCODE=DDRMIHD&UCB3DACC=ON&
*         (UCBPGFL=ON×UCBSYSR=ON)) THEN/* DEVICE END ON PAGE PACK OR
*                                      SYSRES DEVICE         @ZA28603*/
         CLI   MIHCODE(DDRPTR),X'03'                               0338
         BE    @RT00338                                            0338
         CLI   MIHCODE(DDRPTR),X'04'                               0338
         BE    @RT00338                                            0338
         CLI   MIHCODE(DDRPTR),X'02'                               0338
         BNE   @RF00338                                            0338
         TM    UCB3DACC(UCBPTR),B'00100000'                        0338
         BNO   @RF00338                                            0338
         TM    UCBPGFL(UCBPTR),B'01000000'                         0338
         BO    @RT00338                                            0338
         TM    UCBSYSR(UCBPTR),B'00000010'                         0338
         BNO   @RF00338                                            0338
@RT00338 DS    0H                                                  0339
*       MIHIOR=ON;                                                 0339
         OI    MIHIOR(DDRPTR),B'10000000'                          0339
*     ELSE                                                         0340
*       MIHIOR=OFF;                 /* INDICATE IF IOE RESTART WOULD
*                                      BE CALLED             @G24LNLC*/
         B     @RC00338                                            0340
@RF00338 NI    MIHIOR(DDRPTR),B'01111111'                          0340
*     IF UCBMIHTI^=ZERO THEN                                       0341
@RC00338 L     @12,UCBEXTPT(,UCBPTR)                               0341
         CLI   UCBMIHTI(@12),0                                     0341
         BE    @RF00341                                            0341
*       DO;                         /* IF EXIT SPECIFIED TOTO IT   0342
*                                                            @G24LNLC*/
*         I=UCBMIHTI;               /* GET INDEX             @G24LNLC*/
         SLR   I,I                                                 0343
         IC    I,UCBMIHTI(,@12)                                    0343
*         RFY                                                      0344
*          (R1,                                                    0344
*           R14,                                                   0344
*           R15) RSTD;              /*                       @G24LNLC*/
*         SAVEPTR=ADDR(TMC01SAV);   /* GIVE EXIT A SAVE AREA @G24LNLC*/
         LA    SAVEPTR,TMC01SAV(,DDRPTR)                           0345
*         R1=ADDR(TMCEXITP);        /* SET UP PARM LIST POINTER IN R1
*                                                            @G24LNLC*/
         LA    R1,TMCEXITP(,DDRPTR)                                0346
*         MIHINTVA=ADDR(MIHINTVL);  /* GET ADDR OF INTVL INFO      0347
*                                                            @G24LNLC*/
         LA    @12,MIHINTVL(,DDRPTR)                               0347
         ST    @12,MIHINTVA(,DDRPTR)                               0347
*         FRREXITI=I;               /* INDICATE EXIT INDEX TO FRR  0348
*                                                            @G24LNLC*/
*                                                                  0348
         STCM  I,7,FRREXITI(FRRPARM)                               0348
*         /***********************************************************/
*         /*                                                         */
*         /* R2 ALREADY POINTS TO UCB                        @G24LNLC*/
*         /*                                                         */
*         /***********************************************************/
*                                                                  0349
*         CALL TMCEX2(I);           /* GO TO EXIT ROUTINE    @G24LNLC*/
         LR    @12,I                                               0349
         SLA   @12,3                                               0349
         L     @15,TMCEX2-8(@12)                                   0349
         BALR  @14,@15                                             0349
*         FRREXITI=ZERO;            /* ZERO EXIT ACTIVE INDICATOR  0350
*                                                            @G24LNLC*/
         SLR   @12,@12                                             0350
         STCM  @12,7,FRREXITI(FRRPARM)                             0350
*         RFY                                                      0351
*          (R1,                                                    0351
*           R14,                                                   0351
*           R15) UNRSTD;                                           0351
*       END;                                                       0352
*     MIHCODE=MIHTAB(MIHCOND+ONE);  /* SET MIH CODE FOR PENDING    0353
*                                      CONDITION                     */
@RF00341 SLR   @12,@12                                             0353
         IC    @12,MIHCOND(,DDRPTR)                                0353
         LA    @15,MIHTAB(@12)                                     0353
         MVC   MIHCODE(1,DDRPTR),0(@15)                            0353
*     IF MIHCODE=NO THEN            /* PENDING CONDITION OCCURRED?   */
         CLI   MIHCODE(DDRPTR),X'00'                               0354
         BE    @RT00354                                            0354
*       GOTO NEXTUCB;               /* NO-CHECK NEXT UCB             */
*     IF UCBIOQ=ZERO×UCBPST=OFF THEN/* ANY I/O REQUEST               */
         LR    @12,UCBPTR                                          0356
         AL    @12,@CF03324                                        0356
         L     @12,UCBIOQ-508(,@12)                                0356
         LTR   @12,@12                                             0356
         BZ    @RT00356                                            0356
         TM    UCBPST(UCBPTR),B'00100000'                          0356
         BNZ   @RF00356                                            0356
@RT00356 DS    0H                                                  0357
*       USERASID=UCBASID;           /* NO I/O REQUEST-SET DEVICE USER
*                                      ASID                          */
         L     @12,UCBEXTPT(,UCBPTR)                               0357
         LH    @12,UCBASID(,@12)                                   0357
         STH   @12,USERASID(,DDRPTR)                               0357
*     ELSE                          /* I/O REQUESTS IS QUEUED        */
*       IF IOSDVRID=IOSOLTID THEN   /* OLTEP?                        */
         B     @RC00356                                            0358
@RF00356 LR    @12,UCBPTR                                          0358
         AL    @12,@CF03324                                        0358
