         TITLE 'IGFPTCON - MCH CONSOLE WRITE ROUTINE                   *
                       '
IGFPTCON CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'IGFPTCON  74.028'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @12,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@12                                         0001
*                                                                  0047
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK MCS PREFIX TO UCM                                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0047
*   WTOPARM=R1;                     /* SAVE INPUT PARAMETER          */
         LR    WTOPARM,R1                                          0047
*   RFY                                                            0048
*     R1 UNRSTD;                    /* UNRESTRICT R1                 */
*   IF UCMPRFXP=0 THEN                                             0049
         L     @07,CVTPTR                                          0049
         L     @07,UCMBASPT(,@07)                                  0049
         LR    @15,@07                                             0049
         SH    @15,@CH00143                                        0049
         ICM   @14,15,UCMPRFXP(@15)                                0049
         BZ    @RT00049                                            0049
*     RETURN;                       /* RETURN TO CALLER IF MCS PREFIX
*                                      NOT AVAILABLE                 */
*   UCMDVEPT=UCMMCENT;              /* GET PTR TO MASTER CONSOLE UCM
*                                      ENTRY                         */
*                                                                  0051
         L     @01,UCMMCSPT(,@15)                                  0051
         L     UCMDVEPT,UCMMCENT(,@01)                             0051
*   /*****************************************************************/
*   /*                                                               */
*   /* ATTEMPT TO LOCATE AN ACTIVE GRAPHICS OR UNIT RECORD CONSOLE   */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0052
*   DO CONSCTR=(UCMVEL->UCMID)-1 BY-1 TO 0 WHILE UCMDVEPT^=0;/*    0052
*                                      SEARCH UCM CHAIN UNTIL END OF
*                                      CHAIN REACHED OR MAXIMUM    0052
*                                      NUMBER OF CONSOLES EXAMINED 0052
*                                                            @YM06150*/
         L     @03,CVTPTR                                          0052
         L     @03,UCMBASPT(,@03)                                  0052
         L     @03,UCMVEL(,@03)                                    0052
         SR    CONSCTR,CONSCTR                                     0052
         IC    CONSCTR,UCMID(,@03)                                 0052
         BCTR  CONSCTR,0                                           0052
         B     @DE00052                                            0052
@DL00052 SR    @03,@03                                             0052
         CR    UCMDVEPT,@03                                        0052
         BE    @DC00052                                            0052
*     IF UCMCOMPC^=0 THEN           /* CHECK FOR COMPOSITE CONSOLE   */
         L     @15,UCMCOMPC(,UCMDVEPT)                             0053
         CR    @15,@03                                             0053
         BE    @RF00053                                            0053
*       UCMDVEPT=UCMCOMPC;          /* GET PTR TO UCM ENTRY FOR    0054
*                                      OUTPUT DEVICE IF CONSOLE IS 0054
*                                      COMPOSITE                     */
         LR    UCMDVEPT,@15                                        0054
*     IF UCMUF='1'B THEN            /* CHECK FOR CONSOLE ACTIVE      */
@RF00053 TM    UCMUF(UCMDVEPT),B'00010000'                         0055
         BNO   @RF00055                                            0055
*       DO;                         /* ACTIVE CONSOLE FOUND          */
*         IF UCMXB^=0 THEN          /* CHECK FOR GRAPHICS DEVICE     */
*                                                                  0057
         ICM   @03,15,UCMXB(UCMDVEPT)                              0057
         BZ    @RF00057                                            0057
*           /*********************************************************/
*           /*                                                       */
*           /* ACTIVE GRAPHICS CONSOLE FOUND                         */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0058
*           DO;                                                    0058
*             CCWCHPTR=DCMADRMS;    /* LOCATE GRAPHICS CCW CHAIN   0059
*                                                            @YM06150*/
         L     @15,DCMTSPTR(,UCMDVEPT)                             0059
         SR    @14,@14                                             0059
         ICM   @14,7,DCMADRMS(@15)                                 0059
         LR    CCWCHPTR,@14                                        0059
*             CCWWTPTR=DCMADRMS+(DCMRMS-1)*8;/* POINT TO CCW TO BE 0060
*                                      FILLED IN BY IGFPTIOS @YM06150*/
         SR    CCWWTPTR,CCWWTPTR                                   0060
         IC    CCWWTPTR,DCMRMS(,@15)                               0060
         BCTR  CCWWTPTR,0                                          0060
         SLA   CCWWTPTR,3                                          0060
         AR    @14,CCWWTPTR                                        0060
         LR    CCWWTPTR,@14                                        0060
*             CALL IGFPTIOS;        /* ATTEMPT TO WRITE MSG          */
         BAL   @14,IGFPTIOS                                        0061
*             IF RETCODE=SIOGOOD THEN/* CHECK RETURN CODE            */
         LTR   RETCODE,RETCODE                                     0062
         BZ    @RT00062                                            0062
*               GOTO CKHRDCPY;      /* ATTEMPT TO WRITE SAME MSG TO
*                                      HARDCOPY IF SIO SUCCESSFUL    */
*           END;                    /* OTHERWISE CONTINUE CONSOLE  0064
*                                      SEARCH                        */
*         ELSE                                                     0065
*           IF UCBTBYT3=UNITREC THEN/* CHECK FOR UNIT RECORD DEVICE  */
*                                                                  0065
         B     @RC00057                                            0065
@RF00057 L     @03,UCBPTR(,UCMDVEPT)                               0065
         CLI   UCBTBYT3(@03),X'08'                                 0065
         BNE   @RF00065                                            0065
*             /*******************************************************/
*             /*                                                     */
*             /* ACTIVE UNIT RECORD CONSOLE FOUND                    */
*             /*                                                     */
*             /*******************************************************/
*                                                                  0066
*             DO;                                                  0066
*               CCWCHPTR=ADDR(URECCCW);/* POINT TO UNIT RECORD CCW 0067
*                                                            @YM06150*/
         LA    @15,URECCCW                                         0067
         LR    CCWCHPTR,@15                                        0067
*               CCWWTPTR=ADDR(URECCCW);/* POINT TO CCW TO BE FILLED
*                                      IN BY IGFPTIOS        @YM06150*/
         LR    CCWWTPTR,@15                                        0068
*               CALL IGFPTIOS;      /* ATTEMPT TO WRITE MSG          */
         BAL   @14,IGFPTIOS                                        0069
*               IF RETCODE=SIOGOOD THEN/* CHECK RETURN CODE          */
         LTR   RETCODE,RETCODE                                     0070
         BZ    @RT00070                                            0070
*                 RETURN;           /* RETURN TO CALLER IF SIO     0071
*                                      SUCCESSFUL                    */
*             END;                  /* OTHERWISE CONTINUE CONSOLE  0072
*                                      SEARCH                        */
*       END;                                                       0073
@RF00065 DS    0H                                                  0073
@RC00057 DS    0H                                                  0074
*     UCMDVEPT=UCMALTEN;            /* GET PTR TO UCM ENTRY OF     0074
*                                      ALTERNATE                     */
@RF00055 L     UCMDVEPT,UCMALTEN(,UCMDVEPT)                        0074
*   END;                            /* CONTINUE CONSOLE SEARCH LOOP
*                                      IF EXITING CONDITIONS NOT MET */
*                                                                  0075
         BCTR  CONSCTR,0                                           0075
@DE00052 LTR   CONSCTR,CONSCTR                                     0075
         BNM   @DL00052                                            0075
@DC00052 DS    0H                                                  0076
*   /*****************************************************************/
*   /*                                                               */
*   /* IF THE MESSAGE COULD NOT BE WRITTEN TO ONE OF THE CONSOLES OR */
*   /* IF IT WAS WRITTEN TO A GRAPHICS DEVICE, AN ATTEMPT WILL BE    */
*   /* MADE TO WRITE IT TO THE HARDCOPY DEVICE                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0076
*CKHRDCPY:                                                         0076
*   UCMDVEPT=UCMHCUCM;              /* GET PTR TO UCM ENTRY OF     0076
*                                      HARDCOPY DEVICE               */
CKHRDCPY L     @07,CVTPTR                                          0076
         L     @07,UCMBASPT(,@07)                                  0076
         AL    @07,@CF01797                                        0076
         L     @07,UCMMCSPT(,@07)                                  0076
         L     UCMDVEPT,UCMHCUCM(,@07)                             0076
*   IF UCMSYSB='1'B                 /* CHECK FOR HARDCOPY REQUIRED   */
*       &UCMSYSG^='1'B              /* CHECK FOR HARDCOPY NOT SYSLOG */
*       &UCMDVEPT^=0                /* CHECK FOR NON-ZERO UCM ENTRY
*                                      PTR                           */
*       &UCMUF='1'B                 /* CHECK FOR ACTIVE              */
*       &UCBTBYT3=UNITREC THEN      /* CHECK FOR UNIT RECORD DEVICE  */
         TM    UCMSYSB(@07),B'01000000'                            0077
         BNO   @RF00077                                            0077
         TM    UCMSYSG(@07),B'00000010'                            0077
         BNZ   @RF00077                                            0077
         LTR   UCMDVEPT,UCMDVEPT                                   0077
         BZ    @RF00077                                            0077
         TM    UCMUF(UCMDVEPT),B'00010000'                         0077
         BNO   @RF00077                                            0077
         L     @07,UCBPTR(,UCMDVEPT)                               0077
         CLI   UCBTBYT3(@07),X'08'                                 0077
         BNE   @RF00077                                            0077
*     DO;                                                          0078
*       CCWCHPTR=ADDR(URECCCW);     /* POINT TO UNIT RECORD CCW    0079
*                                                            @YM06150*/
         LA    @07,URECCCW                                         0079
         LR    CCWCHPTR,@07                                        0079
*       CCWWTPTR=ADDR(URECCCW);     /* POINT TO CCW TO BE FILLED IN
*                                      BY IGFPTIOS           @YM06150*/
         LR    CCWWTPTR,@07                                        0080
*       CALL IGFPTIOS;              /* ATTEMPT TO WRITE MSG TO     0081
*                                      HARDCOPY                      */
         BAL   @14,IGFPTIOS                                        0081
*     END;                                                         0082
*   RETURN;                         /* RETURN TO CALLER              */
@EL00001 DS    0H                                                  0083
@EF00001 DS    0H                                                  0083
@ER00001 LM    @14,@12,12(@13)                                     0083
         BR    @14                                                 0083
*                                                                  0084
*   /*****************************************************************/
*   /*                                                               */
*   /* SUBROUTINE TO DO I/O                                          */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0084
*IGFPTIOS:                                                         0084
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0084
*                                                                  0084
IGFPTIOS DS    0H                                                  0085
*   /*****************************************************************/
*   /*                                                               */
*   /* SAVE CURRENT I/O STATUS AND PREPARE FOR SIO                   */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0085
*   IONEWSAV=FLCINPSW;              /* SAVE I/O NEW PSW              */
         MVC   IONEWSAV(8),FLCINPSW                                0085
*   IOOLDSAV=FLCIOPSW;              /* SAVE I/O OLD PSW              */
         MVC   IOOLDSAV(8),FLCIOPSW                                0086
*   IOADSAVE=FLCIOA;                /* SAVE I/O ADDRESS              */
         MVC   IOADSAVE(4),FLCIOA                                  0087
*   CAWSAVE=FLCCAW;                 /* SAVE CAW                      */
         MVC   CAWSAVE(4),FLCCAW                                   0088
*   CSWSAVE=FLCCSW;                 /* SAVE CSW                      */
         MVC   CSWSAVE(8),FLCCSW                                   0089
*   GEN REFS(CHMSKSAV,CHMSKNEW)SETS(CHMSKSAV);                     0090
         STCTL 2,2,CHMSKSAV           SAVE CURRENT CHANNEL MASKS
         LCTL  2,2,CHMSKNEW           INSURE ALL CHANNELS MASKED ON
*   FLCINPSW=MYIONPSW;              /* CHANGE I/O NEW SO THAT THIS 0091
*                                      MODULE WILL HANDLE ALL I/O  0091
*                                      INTERRUPTS                    */
         MVC   FLCINPSW(8),MYIONPSW                                0091
*   FLCCAW=CCWCHPTR;                /* SET UP CAW WITH CCW CHAIN ADDR
*                                                            @YM06150*/
         ST    CCWCHPTR,FLCCAW                                     0092
*   CCWDATAD=ADDR(WTOMSG);          /* SET UP 'WRITE' CCW DATA ADDR
*                                      FIELD                         */
         LA    @04,WTOMSG(,WTOPARM)                                0093
         STCM  @04,7,CCWDATAD(CCWWTPTR)                            0093
*   CCWCOUNT=WTOLEN-(LENGTH(WTOLEN)+LENGTH(WTOJUNK));/* SET UP     0094
*                                      'WRITE' CCW BYTE COUNT FIELD  */
         SR    @04,@04                                             0094
         ICM   @04,3,WTOLEN(WTOPARM)                               0094
         SH    @04,@CH00143                                        0094
         STH   @04,CCWCOUNT(,CCWWTPTR)                             0094
*   RETCODE=SIOGOOD;                /* INITIALIZE RETURN CODE        */
*                                                                  0095
         SR    RETCODE,RETCODE                                     0095
*   /*****************************************************************/
*   /*                                                               */
*   /* ATTEMPT TO WRITE MESSAGE                                      */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0096
*   DO SIOCTR=SIOLIMIT BY-1 TO 0;                                  0096
         L     SIOCTR,@CF01759                                     0096
@DL00096 DS    0H                                                  0097
*     SIO(UCBCHAN);                 /* ISSUE SIO AND CHECK CONDITION
*                                      CODE                          */
         L     @04,UCBPTR(,UCMDVEPT)                               0097
         SR    @15,@15                                             0097
         ICM   @15,3,UCBCHAN(@04)                                  0097
         SIO   0(@15)                                              0097
*     BC(8,RESTORE);                /* CC=0, I/O INITIATED           */
         BC    8,RESTORE                                           0098
*     BC(2,IGFPTINT);               /* CC=2, CHANNEL BUSY            */
         BC    2,IGFPTINT                                          0099
*     BC(1,NOGOOD);                 /* CC=3, HARDWARE NOT OPERATIONAL*/
*                                                                  0100
         BC    1,NOGOOD                                            0100
*/*                                    CC=1, CSW STORED              */
*                                                                  0101
*     IF CSWCUE^='1'B               /* CHECK FOR CONTROL UNIT END    */
*         &CSWBUSY^='1'B THEN       /* CHECK FOR DEVICE BUSY         */
         TM    CSWCUE,B'00110000'                                  0101
         BZ    @RT00101                                            0101
*       GOTO NOGOOD;                /* IF NEITHER, BRANCH            */
*IGFPTINT:                                                         0103
*     LPSW(ENABLE);                 /* ENABLE I/O INTERRUPTIONS      */
@RF00101 DS    0H                                                  0103
IGFPTINT LPSW  ENABLE                                              0103
*ENABLNSI:                                                         0104
*     LPSW(DISABLE);                /* DISABLE I/O INTERRUPTIONS     */
ENABLNSI LPSW  DISABLE                                             0104
*DISABNSI:                                                         0105
*   END;                            /* CONTINUE SIO LOOP IF LIMIT NOT
*                                      EXCEEDED                      */
DISABNSI BCTR  SIOCTR,0                                            0105
         LTR   SIOCTR,SIOCTR                                       0105
         BNM   @DL00096                                            0105
*NOGOOD:                                                           0106
*   RETCODE=SIOFAIL;                /* INDICATE I/O NOT INITIATED    */
*                                                                  0106
NOGOOD   LA    RETCODE,4                                           0106
*   /*****************************************************************/
*   /*                                                               */
*   /* RESTORE SAVED I/O STATUS AND RETURN TO MAINLINE               */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0107
*RESTORE:                                                          0107
*   FLCINPSW=IONEWSAV;              /* RESTORE I/O NEW PSW           */
RESTORE  MVC   FLCINPSW(8),IONEWSAV                                0107
*   FLCIOPSW=IOOLDSAV;              /* RESTORE I/O OLD PSW           */
         MVC   FLCIOPSW(8),IOOLDSAV                                0108
*   FLCIOA=IOADSAVE;                /* RESTORE I/O ADDRESS           */
         MVC   FLCIOA(4),IOADSAVE                                  0109
*   FLCCAW=CAWSAVE;                 /* RESTORE CAW                   */
         MVC   FLCCAW(4),CAWSAVE                                   0110
*   FLCCSW=CSWSAVE;                 /* RESTORE CSW                   */
         MVC   FLCCSW(8),CSWSAVE                                   0111
*   GEN(LCTL  2,2,CHMSKSAV)                                        0112
*   REFS(CHMSKSAV)NOSETS;           /* RESTORE CHANNEL MASKS         */
         LCTL  2,2,CHMSKSAV
*   END IGFPTIOS;                   /* RETURN TO IGFPTCON MAINLINE   */
@EL00002 DS    0H                                                  0113
@EF00002 DS    0H                                                  0113
@ER00002 BR    @14                                                 0113
*   END IGFPTCON                                                   0114
