         TITLE 'IGFTMCHK- MVS MISSING INTERRUPT HANDLER                *
                        '
IGFTMCHK CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'IGFTMCHK  77.313'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @08,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@08                                         0001
         L     @00,@SIZDATD                                        0001
         GETMAIN  R,LV=(0)
         LR    @09,@01                                             0001
         USING @DATD,@09                                           0001
         ST    @13,@SA00001+4                                      0001
         LM    @00,@01,20(@13)                                     0001
         ST    @09,8(,@13)                                         0001
         LR    @13,@09                                             0001
RBPRFXLN EQU 64
RBPRFLNA EQU 32
PRBLEN   EQU 136
SIRBLEN  EQU 200
TIRBLEN  EQU 136
IRBLEN   EQU 128
SVRBLEN  EQU 224
*                                                                  0198
*/********************************************************************/
*/*                                                                  */
*/*  INITIALIZATION: IGFTMCHK INITIALIZATION ROUTINE                 */
*/*                                                                  */
*/*  INPUT: NONE                                                     */
*/*                                                                  */
*/*  FUNCTION:                                                       */
*/*           1.OBTAIN SQA FOR BUILDING TQE,DDRCOM,ECB AND SRB       */
*/*             FOR SCHEDULE IGFTMC01                                */
*/*           2.ISSUE ESTAE FOR ABNORMAL TERMINATION                 */
*/*           3.INITIALIZE DDRCOM                                    */
*/*           4.WRITE IPL RECORD TO SYS1.LOGREC                      */
*/*                                                                  */
*/*  OUTPUT: WORK AREA FOR TQE, DDRCOM,ECB AND SRB ARE OBTAINED      */
*/*          IPL RECORD ARE WRITTEN TO SYS1.LOGREC                   */
*/*          ESTABLISH ESTAE EXIT ROUTINE TO HANDLE ABNORMAL         */
*/*          TERMINATION                                             */
*/*                                                                  */
*/*                                                                  */
*/********************************************************************/
*                                                                  0198
*   TQELEN=(LENGTH(TQE)+SEVEN)/EIGHT*EIGHT;/* ROUND TQE TO DOUBLE  0198
*                                      WORD                          */
         LA    TQELEN,128                                          0198
*   DDRCMLEN=(LENGTH(DDRCOMWK)+SEVEN)/EIGHT*EIGHT;/* SETUP TOTAL   0199
*                                      DDRCOM AND MIH LOCAL WORK AREA*/
         LA    @12,224                                             0199
         ST    @12,DDRCMLEN                                        0199
*   SRBLEN=(LENGTH(SRB)+SEVEN)/EIGHT*EIGHT;/* ROUND SRB TO DOUBLE  0200
*                                      WORD                          */
         LA    SRBLEN,48                                           0200
*   SQASIZE=TQELEN+SRBLEN+DDRCMLEN+LENGTH(ECB)+LENGTH(PARMLIST);/* 0201
*                                      SET UP FOR GETMAIN            */
         LR    @03,TQELEN                                          0201
         ALR   @03,SRBLEN                                          0201
         ALR   @03,@12                                             0201
         AL    @03,@CF00790                                        0201
         ST    @03,SQASIZE                                         0201
*   RFY                                                            0202
*     RTNCODE RSTD;                 /* RESTRICT REG 15               */
*   DO;                             /* GETMAIN (RC) LV(SQASIZE)    0203
*                                      SP(245)                       */
*     RESPECIFY                                                    0204
*      (GPR01F,                                                    0204
*       GPR15F,                                                    0204
*       GPR00F) RESTRICTED;                                        0204
*     GPR01F=0;                     /* REG 1 MUST BE ZERO            */
         SLR   GPR01F,GPR01F                                       0205
*     GPR15F=0;                     /* RC-TYPE GETMAIN               */
         SLR   GPR15F,GPR15F                                       0206
*     GPR00F=SQASIZE;               /* LENGTH REQUESTED              */
         LR    GPR00F,@03                                          0207
*     GPR15F=GPR15F×((245)*256);    /* SP IN BYTE 2                  */
         O     GPR15F,@CF02958                                     0208
*     SVC(120);                     /* RC/RU-FORM OF GETMAIN         */
         SVC   120                                                 0209
*     RESPECIFY                                                    0210
*      (GPR01F,                                                    0210
*       GPR15F,                                                    0210
*       GPR00F) UNRESTRICTED;                                      0210
*   END;                            /* GETMAIN (RC) LV(SQASIZE)    0211
*                                      SP(245)                       */
*   RFY                                                            0212
*    (R0,                                                          0212
*     R1) RSTD;                     /* RESTRICT REG 0,1              */
*   IF RTNCODE^=ZERO THEN           /* GETMAIN SUCCESSFUL?           */
         LTR   RTNCODE,RTNCODE                                     0213
         BNZ   @RT00213                                            0213
*     GOTO MIHEXIT;                 /* TERMINATE MIH FOR GETMAIN   0214
*                                      FAILS                         */
*   TQEPTR=R1;                      /* GET PTR FOR TQE               */
         ST    R1,TQEPTR                                           0215
*   SRBPTR=TQEPTR+TQELEN;           /* GET PTR FOR SRB               */
         L     @14,TQEPTR                                          0216
         ALR   @14,TQELEN                                          0216
         LR    SRBPTR,@14                                          0216
*   DDRPTR=SRBPTR+SRBLEN;           /* SET UP PTR FOR DDRCOM         */
         LR    DDRPTR,SRBPTR                                       0217
         ALR   DDRPTR,SRBLEN                                       0217
*   ECBPTR=DDRPTR+DDRCMLEN;         /* GET PTR FOR ECB               */
         LR    @14,DDRPTR                                          0218
         AL    @14,DDRCMLEN                                        0218
         ST    @14,ECBPTR                                          0218
*   PARMPTR=ECBPTR+LENGTH(ECB);     /* GET PTR TO PARM LIST          */
         AL    @14,@CF00041                                        0219
         ST    @14,PARMPTR                                         0219
*   R0=R1;                          /* GET GETMAIN ADDRESS           */
         LR    R0,R1                                               0220
*   RFY                                                            0221
*     R14 RSTD;                     /* RESTRICT REG 14               */
*   R1=SQASIZE;                     /* GET GETMAIN LENGTH            */
         L     R1,SQASIZE                                          0222
*   MVCL(R0,R14);                   /* CLEAR GETMAIN AREA ZERO MVCL
*                                      BASED ON REG 15 ZERO          */
         MVCL  R0,R14                                              0223
*   RFY                                                            0224
*    (R0,                                                          0224
*     R1,                                                          0224
*     R14,                                                         0224
*     RTNCODE) UNRSTD;              /* RELEASE REG 0,1,14,15         */
*   DO;                             /* ?IGFDERR INIT                 */
*                                                                  0225
*     /***************************************************************/
*     /*                                                             */
*     /* IGFDERR - INITIALIZE RECOVERY ENVIRONMENT                   */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0226
*     DERPLIST=DERPLIST&&DERPLIST;  /* CLEAR PARAMETER LIST          */
         XC    DERPLIST(48),DERPLIST                               0226
*     DERFUNK=DERINIT;              /* INDICATE INIT FUNCTION        */
         MVI   DERFUNK,X'01'                                       0227
*     DERREC=ADDR(DERRECPM);        /* POINT TO REC PARMS            */
         LA    @12,DERRECPM                                        0228
         ST    @12,DERREC                                          0228
*     DERSPEC=ADDR(IGFTMCHS);       /* POINT TO SPECIAL EXIT         */
         LA    @12,IGFTMCHS                                        0229
         ST    @12,DERSPEC                                         0229
*     DERRETRY=ADDR(IGFTMCHR);      /* POINT TO RETRY ROUTINE        */
         LA    @12,IGFTMCHR                                        0230
         ST    @12,DERRETRY                                        0230
*     GEN SETS(DERDATD)(MVC DERDATD,@SIZDATD);/* SET UP SPN, SIZE    */
         MVC DERDATD,@SIZDATD
*     DERPFX=DERRMASK;              /* SET UP RUB MASK               */
         MVC   DERPFX(2),@CB02791                                  0232
*     DERRCODE=CODEBASE;            /* SAVE CODEREG                  */
         ST    CODEBASE,DERRCODE                                   0233
*     DERRDATA=DATABASE;            /* SAVE DATAREG                  */
         ST    DATABASE,DERRDATA                                   0234
*     DERRDDR=DDRPTR;               /* SAVE DDRCOM POINTER           */
         ST    DDRPTR,DERRDDR                                      0235
*     DERRSAVE=SAVEPTR;             /* SAVE REGISTER 13              */
         ST    SAVEPTR,DERRSAVE                                    0236
*     CALL IGFDE0(DERPLIST);        /* ISSUE ESTAE                   */
         LA    @12,DERPLIST                                        0237
         ST    @12,@AL00001                                        0237
         L     @15,@CV02814                                        0237
         LA    @01,@AL00001                                        0237
         BALR  @14,@15                                             0237
*   END;                            /* ?IGFDERR INIT ESTABLISH ESTAE */
*   DDRUTCB=TMCTCBP;                /* GET PTR TO MIH TCB            */
         L     @12,CVTPTR                                          0239
         L     @12,CVTTCBP(,@12)                                   0239
         L     @12,TMCTCBP+4(,@12)                                 0239
         ST    @12,DDRUTCB(,DDRPTR)                                0239
*   DERPTR=ADDR(DERPLIST);          /* GET ADDR OF DERPLIST          */
         LA    DERPTR,DERPLIST                                     0240
*   DDRREC1=DDRRIPL;                /* INDICATE TO WRITE IPL REC     */
         MVI   DDRREC1(DDRPTR),X'04'                               0241
*   CALL IGFDR0;                    /* CALL DDR RECORDER             */
         L     @15,@CV02812                                        0242
         BALR  @14,@15                                             0242
*   DDRPTR=DERRDDR;                 /* RESET DDRCOM PTR              */
         L     DDRPTR,DERRDDR                                      0243
*                                                                  0244
*/********************************************************************/
*/*                                                                  */
*/*  SET MIH TIME INTERVAL: SET USER PROVIDED TIME INTERVAL          */
*/*                                                                  */
*/*  INPUT: DDRPTR- PTR TO DDRCOM                                    */
*/*                                                                  */
*/*  FUNCTION: CHECK TO SEE IF THE USER PROVIDED TIME IS A VALID     */
*/*            ONE. IF SO, MOVE IT INTO THE DDRCOM TO BE USED        */
*/*            BY SETDIE. IF INVALID,SET TIME INTERVAL AS 3 MINUTES  */
*/*            FOR DEVICES OTHER THAN MSS.THE TIME INTERVAL FOR MSS  */
*/*            WILL BE 4 TIMES(USER MAY ALSO DEFINE) OVER THE REGULAR*/
*/*            TIME INTERVAL. IN THIS CASE IT WILL BE 12 MINUTES     */
*/*                                                                  */
*/*                                                                  */
*/*  OUTPUT:   DDRMIHTD- MIH TIME INTERVAL IN HHMMSSTH               */
*/*            MIHSECND- MIH TIME INTERVAL IN SECONDS                */
*/*                      THE HUNDREDTH OF SECONDS ARE IGNORED        */
*/*            MSSCOUNT- 3330V MSS TIME LIMIT COUNTER                */
*/*            MIHINTVL- COPY OF IGFINTVL FOR EXIT ROUTINES  @G24LNLC*/
*/*                                                                  */
*/*                                                                  */
*/********************************************************************/
*                                                                  0244
*   DO I=ONE TO LENGTH(INTVL) WHILE(TIMERR=OFF);/* CHECK USER TIME   */
         LA    I,1                                                 0244
@DL00244 TM    TIMERR(DDRPTR),B'10000000'                          0244
         BNZ   @DC00244                                            0244
*     IF INTVL(I)<ZERODIG×INTVL(I)>NINE THEN/* VALUE IS 0-9 ?        */
         L     @12,@CV02815                                        0245
         ALR   @12,I                                               0245
         BCTR  @12,0                                               0245
         CLI   INTVL(@12),C'0'                                     0245
         BL    @RT00245                                            0245
         L     @12,@CV02815                                        0245
         ALR   @12,I                                               0245
         BCTR  @12,0                                               0245
         CLI   INTVL(@12),C'9'                                     0245
         BNH   @RF00245                                            0245
@RT00245 DS    0H                                                  0246
*       TIMERR=ON;                  /* NO-SET TIME ERROR             */
         OI    TIMERR(DDRPTR),B'10000000'                          0246
*   END;                            /* END OF SCAN SPECIFIED USER  0247
*                                      TIME INTERVAL                 */
@RF00245 AL    I,@CF00047                                          0247
         C     I,@CF00074                                          0247
         BNH   @DL00244                                            0247
@DC00244 DS    0H                                                  0248
*   IF TIMERR=OFF&INTVL^=DZERO THEN /* ALL CHARACTERS VALID?         */
         TM    TIMERR(DDRPTR),B'10000000'                          0248
         BNZ   @RF00248                                            0248
         L     @12,@CV02815                                        0248
         CLC   INTVL(8,@12),@CC02842                               0248
         BE    @RF00248                                            0248
*     DDRMIHT=INTVL;                /* YES-SET USER SPECIFIED TIME   */
         L     @12,@CV02815                                        0249
         MVC   DDRMIHT(8,DDRPTR),INTVL(@12)                        0249
*   ELSE                            /* NO                            */
*     DDRMIHT=TMCTIME;              /* SET DEFAULT TIME-3 MINUTES    */
         B     @RC00248                                            0250
@RF00248 MVC   DDRMIHT(8,DDRPTR),@CC02828                          0250
*   PACK(MIHOUR,TMCHR);             /* PACK HOUR TO DECIMAL          */
@RC00248 PACK  MIHOUR(2),TMCHR(2,DDRPTR)                           0251
*   CVB(HR,MIHOUR);                 /* CONVERT HOUR TO BINARY        */
         XC    @TS00001(8),@TS00001                                0252
         MVC   @TS00001+6(2),MIHOUR                                0252
         CVB   @12,@TS00001                                        0252
         ST    @12,HR                                              0252
*   PACK(MIHMIM,TMCMM);             /* PACK MINUTES TO DECIMAL       */
         PACK  MIHMIM(2),TMCMM(2,DDRPTR)                           0253
*   CVB(MM,MIHMIM);                 /* CONVERT MINUTES TO BINARY     */
         XC    @TS00001(8),@TS00001                                0254
         MVC   @TS00001+6(2),MIHMIM                                0254
         CVB   @11,@TS00001                                        0254
         ST    @11,MM                                              0254
*   PACK(MIHSEC,TMCSS);             /* PACK SECONDES TO DECIMAL      */
         PACK  MIHSEC(2),TMCSS(2,DDRPTR)                           0255
*   CVB(SS,MIHSEC);                 /* CONVERT SECONDS TO BINARY     */
         XC    @TS00001(8),@TS00001                                0256
         MVC   @TS00001+6(2),MIHSEC                                0256
         CVB   @06,@TS00001                                        0256
         ST    @06,SS                                              0256
*   MIHSECND=(HR*SIXTY+MM)*SIXTY+SS;/* STORE TIME INTERVAL IN SECONDS*/
         LA    @05,60                                              0257
         LR    @01,@12                                             0257
         MR    @00,@05                                             0257
         ALR   @01,@11                                             0257
         MR    @00,@05                                             0257
         ALR   @01,@06                                             0257
         ST    @01,MIHSECND(,DDRPTR)                               0257
*   MIHINTVL=INDATA;                /* COPY INTVL INTO WORKAREA FOR
*                                      USE BY EXITS          @G24LNLC*/
         L     @12,@CV02815                                        0258
         MVC   MIHINTVL(12,DDRPTR),INDATA(@12)                     0258
*   MIHINTVL(1:8)=DDRMIHT;          /* PUT IN CORRECTED TIME INTERVAL
*                                      IN CASE OF INPUT ERROR      0259
*                                                            @G24LNLC*/
         MVC   MIHINTVL(8,DDRPTR),DDRMIHT(DDRPTR)                  0259
*   MIHINTVA=ADDR(MIHINTVL);        /* SET UP FOR EXITS      @G24LNLC*/
         LA    @12,MIHINTVL(,DDRPTR)                               0260
         ST    @12,MIHINTVA(,DDRPTR)                               0260
*                                                                  0261
*/********************************************************************/
*/*                                                                  */
*/*  INIT CONTROL BLOCKS: TQE AND SRB WILL BE INTILIZED              */
*/*                                                                  */
*/*  INPUT: TQEPTR WILL POINT TO TQE                                 */
*/*         SRBPTR WILL POINT TO SRB                                 */
*/*                                                                  */
*/*  FUNCTION:TQE AND SRB WILL BE INITIALIZED TO CALL SETDIE         */
*/*           TO  SCHEDULE A SRB FOR CHECKING UCBS(IGFTMC01)         */
*/*           MIH WILL BE TERMINATED IF SETDIE FAILS                 */
*/*                                                                  */
*/*  OUTPUT: TQE AND SRB ARE INITIALIZED                             */
*/*                                                                  */
*/********************************************************************/
*/********************************************************************/
*/*                                                                  */
*/*  INITIALIZE SRB FOR CHECK UCB LIST IN THE SYSTEM                 */
*/*  THE SRB HAS NO RELATIONSHIP WITH THE TCB                        */
*/*                                                                  */
*/********************************************************************/
*                                                                  0261
*   SRBID=CSRBID;                   /* SET SRB ID                    */
         MVC   SRBID(4,SRBPTR),@CC02846                            0261
*   SRBASCB=ASVTENTY(ONE);          /* SET SRB ADDRESS SPACE         */
         L     @12,CVTPTR                                          0262
         L     @11,ASVTPTR(,@12)                                   0262
         L     @11,ASVTENTY(,@11)                                  0262
         ST    @11,SRBASCB(,SRBPTR)                                0262
*   PARMTQE=0;                      /* STORE TQE PTR TO ZERO TO    0263
*                                      INDICATE INITIALIZATION     0263
*                                                            @G24LNLC*/
         L     @11,PARMPTR                                         0263
         SLR   @06,@06                                             0263
         ST    @06,PARMTQE(,@11)                                   0263
*   PARMDCM=DDRPTR;                 /* STORE DDRCOM PTR AS PARAM     */
         ST    DDRPTR,PARMDCM(,@11)                                0264
*   PARMECB=ECBPTR;                 /* STORE ECB PTR AS PARAM        */
         L     @06,ECBPTR                                          0265
         ST    @06,PARMECB(,@11)                                   0265
*   SRBPARM=ADDR(PARMLIST);         /* SET PARM LIST ADDR            */
         ST    @11,SRBPARM(,SRBPTR)                                0266
*   SRBEP=RVTMIH2;                  /* SET IGFTMC01 ENTRY POINT      */
         L     @12,RVTPTR(,@12)                                    0267
         L     @12,RVTMIH2(,@12)                                   0267
         ST    @12,SRBEP(,SRBPTR)                                  0267
*/********************************************************************/
*/*                                                                  */
*/*  IGFTMC01 (IN MODULE IGFTMC00) IS SCHEDULED AS AN SRB TO PERFORM */
*/*  INITIALIZATION OF EXIT ROUTINE TABLES.  SUSPEND AND CALLDISP    */
*/*  ARE USED TO CAUSE SERIALIZATION OF THE SRB.  IT WILL ISSUE      */
*/*  RESUME TO CAUSE THIS CODE TO REGAIN CONTROL AFTER THE TABLE     */
*/*  INITIALIZATION IS COMPLETE.  THE LOCAL LOCK IS OBTAINED PRIOR   */
*/*  TO THE SCHEDULE TO ENSURE THAT IT IS COMPLETED PRIOR TO EXITING */
*/*  TO THE DISPATCHER.                                              */
*/*                                                          @G24LNLC*/
*/********************************************************************/
*                                                                  0268
*   SRBPTCB=PSATOLD;                /* ASSOCIATE SRB WITH TCB      0268
*                                                            @G24LNLC*/
         L     @12,PSATOLD                                         0268
         ST    @12,SRBPTCB(,SRBPTR)                                0268
*   RFY                                                            0269
*    (R0,                                                          0269
*     R1,                                                          0269
*     R11,                                                         0269
*     R12,                                                         0269
*     R14,                                                         0269
*     R15) RSTD;                    /*                       @ZA26986*/
*/* GET THE LOCAL LOCK IN ORDER TO USE SUSPEND               @G24LNLC*/
*                                                                  0270
*   GEN;                                                           0270
*                                                        /* @G24LNLC*/
        SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,REGS=USE,       @G24LNLC**
               RELATED=('FREED AFTER SCHEDULE OR IN ESTAE')   @G24LNLC
*   LCLLOCK=ON;                     /* INDICATE FOR ESTAE THAT LOCAL
*                                      LOCK IS HELD          @G24LNLC*/
         OI    LCLLOCK(DDRPTR),B'00010000'                         0271
*   R13SAVE=SAVEPTR;                /* SAVE R13 ACROSS SUSPEND     0272
*                                                            @G24LNLC*/
         ST    SAVEPTR,R13SAVE                                     0272
*   GEN REFS(CVTSUSP,CVT)(SUSPEND RB=CURRENT);/*             @G24LNLC*/
         SUSPEND RB=CURRENT
*   SAVEPTR=R13SAVE;                /* RESTORE R13           @G24LNLC*/
         L     SAVEPTR,R13SAVE                                     0274
*   R1=ADDR(SRB);                   /* SET UP SRB ADDR       @G24LNLC*/
         LR    R1,SRBPTR                                           0275
*   GEN REFS(CVTLSMQ,SRBFLNK)(SCHEDULE SRB=(1),SCOPE=LOCAL);/*     0276
*                                                            @G24LNLC*/
         SCHEDULE SRB=(1),SCOPE=LOCAL
*   GEN REFS(PSALITA);                                             0277
*                                                        /* @G24LNLC*/
        SETLOCK RELEASE,TYPE=LOCAL,REGS=USE,RELATED=('GOTTEN BEFORE SU**
               SPEND')                                   /* @G24LNLC*/
*   RFY                                                            0278
*    (R0,                                                          0278
*     R1,                                                          0278
*     R11,                                                         0278
*     R12,                                                         0278
*     R14,                                                         0278
*     R15) UNRSTD;                  /*                       @ZA26986*/
*   LCLLOCK=OFF;                    /* TURN OFF LOCAL LOCK HELD    0279
*                                      INDICATOR             @G24LNLC*/
         NI    LCLLOCK(DDRPTR),B'11101111'                         0279
*   GEN(CALLDISP);                  /*                       @G24LNLC*/
         CALLDISP
*   SRBPTCB=0;                      /* INDICATE SRB NOT RELATED TO 0281
*                                      TCB                   @G24LNLC*/
         SLR   @12,@12                                             0281
         ST    @12,SRBPTCB(,SRBPTR)                                0281
*   PARMTQE=TQEPTR;                 /* PASS PTR TO TQE TO SRB AS PARM
*                                                            @G24LNLC*/
         L     @12,TQEPTR                                          0282
         L     @11,PARMPTR                                         0282
         ST    @12,PARMTQE(,@11)                                   0282
*/********************************************************************/
*/*                                                                  */
*/*  INITIALIZE TQE FOR SETDIE AND TQE ENQUEUE                       */
*/*                                                                  */
*/********************************************************************/
*                                                                  0283
*   TQEAID=ONE;                     /* SET ASID AS MASTER SCHEDULE   */
         MVC   TQEAID(2,@12),@CH00047                              0283
*   TQEEXIT=RVTMIH;                 /* ENTRY POINT OF IGFTMC00       */
         L     @11,CVTPTR                                          0284
         L     @05,RVTPTR(,@11)                                    0284
         L     @05,RVTMIH(,@05)                                    0284
         ST    @05,TQEEXIT(,@12)                                   0284
*   TQEVAL=MIHTIMSD;                /* SET TIME INTERVAL FOR   SETDIE*/
         MVC   TQEVAL(8,@12),MIHTIMSD(DDRPTR)                      0285
*                                                                  0286
*/********************************************************************/
*/*                                                                  */
*/* SETDIE: CALL SETDIE AND WAIT                                     */
*/*                                                                  */
*/* INPUT: TQE CONTAINS REAL TIME INTERVAL AND DIE ADDRESS           */
*/*                                                                  */
*/* FUNCTION: CALL SETDIE TO INVOKE DIE ROUTINE TO CHECK             */
*/*           ALL UCBS FOR ACTIVE I/O INCLUDING A PENDING            */
*/*           MOUNT AND DDR SWAP. AFTER CALL DIE,PUT TASK IN WAIT    */
*/*           IT WILL BE WAKED UP BY IGFTMC01. AN OUTAGE RECORD IS   */
*/*           WRITTEN TO SYS1.LOGREC                                 */
*/*                                                                  */
*/* OUTPUT: R15=0 SUCCESSFUL                                         */
*/*         OUTAGE RECORD WILL BE WRITTEN TO STS1.LOGREC             */
*/********************************************************************/
*                                                                  0286
*   R13SAVE=SAVEPTR;                /* SAVE SAVEAREA ADDR            */
         ST    SAVEPTR,R13SAVE                                     0286
*   RFY                                                            0287
*    (R11,                                                         0287
*     R12,                                                         0287
*     R0,                                                          0287
*     R1,                                                          0287
*     R15) RSTD;                    /* RESTRICT REG0,1,11,12 AND 15  */
*   R1=TQEPTR;                      /* SET PARM REG                  */
         L     @14,TQEPTR                                          0288
         LR    R1,@14                                              0288
*   CALL SETDIE;                    /* ENQ TQE                       */
         L     @05,CVTPTR                                          0289
         L     @05,TPCPTR(,@05)                                    0289
         L     @15,TPCSDIE(,@05)                                   0289
         BALR  @14,@15                                             0289
*   RFY                                                            0290
*    (R11,                                                         0290
*     R12,                                                         0290
*     R0,                                                          0290
*     R1) UNRSTD;                   /* RELEASE REG11,12,0 AND 1      */
*   SAVEPTR=R13SAVE;                /* RESTORE REG 13                */
         L     SAVEPTR,R13SAVE                                     0291
*   IF RTNCODE^=ZERO THEN           /* SET DIE SUCCESS?              */
         LTR   RTNCODE,RTNCODE                                     0292
         BZ    @RF00292                                            0292
*     DO;                           /* NO                            */
*       DO;                         /* FREEMAIN(RC) LV(SQASIZE)A(TQE)
*                                      SP(245)                       */
*         RESPECIFY                                                0295
*          (GPR01F,                                                0295
*           GPR15F,                                                0295
*           GPR00F) RESTRICTED;                                    0295
*         GPR01F=0;                 /* REG 1 MUST BE ZERO            */
         SLR   GPR01F,GPR01F                                       0296
*         GPR15F=1;                 /* SET TYPE                      */
         LA    GPR15F,1                                            0297
*         GPR00F=SQASIZE;           /* LENGTH REQUESTED              */
         L     GPR00F,SQASIZE                                      0298
*         GPR01F=ADDR(TQE);         /* SET REGISTER ONE              */
         L     GPR01F,TQEPTR                                       0299
*         GPR15F=GPR15F+((245)*256);/* SUBPOOL VALUE                 */
         AL    GPR15F,@CF02958                                     0300
*         SVC(120);                 /* RC/RU-FORM OF GETMAIN         */
         SVC   120                                                 0301
