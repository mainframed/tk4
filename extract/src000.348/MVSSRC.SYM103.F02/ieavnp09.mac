@EL00001 DS    0H                                                  0206
@EF00001 DS    0H                                                  0206
@ER00001 LM    @14,@12,12(@13)                                     0206
         BR    @14                                                 0206
@DATA    DS    0H
@CH01109 DC    H'-16'
@CH00046 DC    H'1'
@CH00044 DC    H'4'
@CH00249 DC    H'6'
@CH01001 DC    H'256'
@CH01012 DC    H'512'
@CH01081 DC    H'32767'
@SX01112 XC    INPUT(0,ADDRREG),INPUT(ADDRREG)
         DS    0F
         DS    0F
@CF01100 DC    F'-1'
         DS    0D
         DS    0D
@TS00001 DS    CL8
@CC01092 DC    C'256'
@CC01084 DC    C'ASVT'
@CC01091 DC    C'000000'
SW       DS    BL1
BUFFER   DS    CL6
