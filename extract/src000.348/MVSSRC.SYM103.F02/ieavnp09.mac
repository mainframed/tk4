         TITLE 'IEAVNP09- NIP09- ASVT CREATION                         *
                       '
IEAVNP09 CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'IEAVNP09  73.274'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @12,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@12                                         0001
*
** REFS ARE GENERATED FOR NVT FIELDS THAT CONTAIN ADDRESSES OF
** NIP SERVICE ROUTINES, SO IEAPMNIP MACRO CALLS TO THESE ROUTINES
** WILL ASSEMBLE PROPERLY.
*                                                                  0087
*/*                                                                  */
*/*                    INITIALIZATION                                */
*/*                                                                  */
*                                                                  0088
*INIT:                                                             0088
*   ;                                                              0088
INIT     DS    0H                                                  0089
*   MAXVALUE=0;                     /* MAXUSER VALUE FOR THIS IPL    */
         SR    MAXVALUE,MAXVALUE                                   0089
*   BUFFER='000000';                /* INITIALIZE BUFFER TO C'0'     */
         MVC   BUFFER(6),@CC01091                                  0090
*                                                                  0091
*/*                                                                  */
*/*                    VALIDATE MAXUSER VALUE                        */
*/*                                                                  */
*                                                                  0091
*   DO WHILE MAXVALUE=0;            /* IF MAXVALUE NOT ZERO, VALID 0091
*                                      MAXUSER VALUE HAS BEEN FOUND  */
         B     @DE00091                                            0091
@DL00091 DS    0H                                                  0092
*     IF MAXUSER=0                  /* MAXUSER VALUE SPECIFIED?      */
*       THEN                                                       0092
         L     @04,NVTPTAB(,RNVT)                                  0092
         ICM   @15,15,MAXUSER(@04)                                 0092
         BNZ   @RF00092                                            0092
*       DO;                         /* NO MAXUSER VALUE SPECIFIED    */
*         MAXVALUE=DEFAULT;         /* NO VALUE SPECIFIED, USE       */
         LA    MAXVALUE,256                                        0094
*         BUFFER(4:6)='256';        /* DEFAULT VALUE OF 256          */
         MVC   BUFFER+3(3),@CC01092                                0095
*         GENERATE REFS(NVTWTO);                                   0096
*                                     /* ISSUE DEFAULT MESSAGE       */
         IEAPMNIP TYPE=WTO,PARAM=DFLTMSG
*       END;                                                       0097
*     ELSE                                                         0098
*       DO;                         /* VALUE SPECIFIED- VALIDATE     */
         B     @RC00092                                            0098
@RF00092 DS    0H                                                  0099
*         SCANPTR=MAXUSER;          /* POINT TO INPUT DATA           */
         L     @07,NVTPTAB(,RNVT)                                  0099
         L     SCANPTR,MAXUSER(,@07)                               0099
*         CTR=0;                    /* SET # DIGITS = 0              */
         SR    CTR,CTR                                             0100
*         SW='0'B;                  /* INDICATE NO ERROR FOUND YET   */
*                                                                  0101
         NI    SW,B'01111111'                                      0101
*/*                                                                  */
*/*  LOOP UNTIL AN ERROR IS FOUND, QUOTE IS FOUND, OR C')' IS FOUND  */
*/*                                                                  */
*                                                                  0102
*         DO WHILE SW='0'B&INPUT(1)^=''''&INPUT(1)^=')';           0102
         B     @DE00102                                            0102
@DL00102 DS    0H                                                  0103
*           IF INPUT(1)='('         /* VALUE IN PARENTHESES- VALID   */
*             THEN                                                 0103
         CLI   INPUT(SCANPTR),C'('                                 0103
         BNE   @RF00103                                            0103
*             SCANPTR=SCANPTR+1;    /* UPDATE TO NEXT DIGIT          */
         AH    SCANPTR,@CH00046                                    0104
*           ELSE                                                   0105
*             DO;                   /* NO PARENTHESES FOUND          */
*                                                                  0105
         B     @RC00103                                            0105
@RF00103 DS    0H                                                  0106
*/*                                                                  */
*/*  MAXUSER VALUE MUST BE DECIMAL CHARACTERS AND < 7 DIGITS         */
*/*                                                                  */
*                                                                  0106
*               IF INPUT(1)<'0'×INPUT(1)>'9'×CTR>6/* VALID CHARACTER
*                                      IN MAXUSER ?                  */
*                 THEN              /* NO                            */
         CLI   INPUT(SCANPTR),C'0'                                 0106
         BL    @RT00106                                            0106
         CLI   INPUT(SCANPTR),C'9'                                 0106
         BH    @RT00106                                            0106
         CH    CTR,@CH00249                                        0106
         BNH   @RF00106                                            0106
@RT00106 DS    0H                                                  0107
*                 DO;               /* INVALID MAXUSER VALUE GENERATE
*                                      ERROR MESSAGE                 */
*                   MAXUSER=0;      /* ZERO PTR IN PARMTAB           */
         L     @07,NVTPTAB(,RNVT)                                  0108
         SR    @15,@15                                             0108
         ST    @15,MAXUSER(,@07)                                   0108
*                   GENERATE REFS(PARMNAME,NVTPRMPT);              0109
         IEAPMNIP TYPE=PRMPT,PARAM=PARMNAME,OPT=ERROR
*                   SW='1'B;        /* INDICATE ERROR CONDITION      */
         OI    SW,B'10000000'                                      0110
*                 END;                                             0111
*               ELSE                /* CHARACTER VALID               */
*                 DO;               /* VALID CHARACTER, CHECK NEXT   */
         B     @RC00106                                            0112
@RF00106 DS    0H                                                  0113
*                   SCANPTR=SCANPTR+1;/* UPDATE PTR                  */
         LA    @07,1                                               0113
         AR    SCANPTR,@07                                         0113
*                   CTR=CTR+1;      /* INCREMENT # DIGITS            */
         AR    CTR,@07                                             0114
*                 END;                                             0115
*             END;                                                 0116
@RC00106 DS    0H                                                  0117
*         END;                                                     0117
@RC00103 DS    0H                                                  0117
@DE00102 TM    SW,B'10000000'                                      0117
         BNZ   @DC00102                                            0117
         CLI   INPUT(SCANPTR),C''''                                0117
         BE    @DC00102                                            0117
         CLI   INPUT(SCANPTR),C')'                                 0117
         BNE   @DL00102                                            0117
@DC00102 DS    0H                                                  0118
*         IF SW='0'B                /* IF NO ERROR FOUND             */
*           THEN                                                   0118
         TM    SW,B'10000000'                                      0118
         BNZ   @RF00118                                            0118
*           DO;                     /* AND                           */
*             IF CTR=0              /* IF NO VALUE SPECIFIED         */
*               THEN                                               0120
         LTR   CTR,CTR                                             0120
         BNZ   @RF00120                                            0120
*               DO;                 /* DEFAULT TO BE USED            */
*                 MAXVALUE=DEFAULT; /* NO VALUE SPECIFIED,           */
         LA    MAXVALUE,256                                        0122
*                 BUFFER(4:6)='256';/* USE DEFAULT OF 256            */
         MVC   BUFFER+3(3),@CC01092                                0123
*                 GENERATE REFS(NVTWTO);                           0124
*                                                 /* ISSUE MSG       */
         IEAPMNIP TYPE=WTO,PARAM=DFLTMSG
*               END;                                               0125
*             ELSE                  /* VALID VALUE FOUND             */
*               DO;                                                0126
*                                                                  0126
         B     @RC00120                                            0126
@RF00120 DS    0H                                                  0127
*/*                                                                  */
*/*  MOVE MAXUSER VALUE TO BUFFER AND CONVERT TO BINARY              */
*/*                                                                  */
*                                                                  0127
*                 SCANPTR=SCANPTR-CTR;/* UPDATE SCANPTR              */
         SR    SCANPTR,CTR                                         0127
*                 N=6;              /* MAXIMUM NUMBER OF DIGITS      */
*                                                                  0128
         LA    N,6                                                 0128
*/*                                                                  */
*/*  START AT BEGINING OF MAXUSER VALUE AND MOVE TO BUFFER FOR CVB   */
*/*                                                                  */
*                                                                  0129
*                 DO I=CTR BY-1 TO 1;/* MOVE ALL DIGITS              */
         LR    I,CTR                                               0129
         B     @DE00129                                            0129
@DL00129 DS    0H                                                  0130
*                   BUFFER(N)=INPUT(I);/* MOVE TO BUFFER             */
         LA    @09,BUFFER-1(N)                                     0130
         LR    @01,SCANPTR                                         0130
         ALR   @01,I                                               0130
         AL    @01,@CF01100                                        0130
         MVC   0(1,@09),INPUT(@01)                                 0130
*                   N=N-1;          /* UPDATE PLACE IN BUFFER        */
         BCTR  N,0                                                 0131
*                 END;                                             0132
         BCTR  I,0                                                 0132
@DE00129 LTR   I,I                                                 0132
         BP    @DL00129                                            0132
*                 PACK(TEMP,BUFFER);/* PACK MAXUSER VALUE            */
         PACK  TEMP(4),BUFFER(6)                                   0133
*                 CVB(CONVERT,TEMP);/* CONVERT TO BINARY             */
         XC    @TS00001(8),@TS00001                                0134
         MVC   @TS00001+4(4),TEMP                                  0134
         CVB   CONVERT,@TS00001                                    0134
*                                                                  0135
*/*                                                                  */
*/*            MAXUSER VALUE MUST BE <= X'7FFF'                      */
*/*   BUT AT LEAST = NUMBER OF PERMANENT ADDRESS SPACES              */
*/*                                                                  */
*                                                                  0135
*                 IF CONVERT>LIMIT×CONVERT<CVTASVT->ASVTMAXU THEN  0135
         CH    CONVERT,@CH01081                                    0135
         BH    @RT00135                                            0135
         L     @01,CVTASVT(,CVTPTR)                                0135
         C     CONVERT,ASVTMAXU(,@01)                              0135
         BNL   @RF00135                                            0135
@RT00135 DS    0H                                                  0136
*                   DO;             /* INVALID VALUE ISSUE MSG       */
*                     MAXUSER=0;    /* ZERO PTR IN PARMTAB           */
         L     @04,NVTPTAB(,RNVT)                                  0137
         SR    @15,@15                                             0137
         ST    @15,MAXUSER(,@04)                                   0137
*                     BUFFER='000000';/* CLEAR BUFFER                */
         MVC   BUFFER(6),@CC01091                                  0138
*                     GENERATE REFS(NVTPRMPT,PARMNAME);            0139
         IEAPMNIP TYPE=PRMPT,PARAM=PARMNAME,OPT=ERROR
*                   END;                                           0140
*                 ELSE                                             0141
*                   DO;             /* VALID VALUE IN BINARY FORM    */
         B     @RC00135                                            0141
@RF00135 DS    0H                                                  0142
*                     MAXVALUE=CONVERT;/* SET UP VALUE               */
         LR    MAXVALUE,CONVERT                                    0142
*                   END;                                           0143
*               END;                                               0144
@RC00135 DS    0H                                                  0145
*           END;                                                   0145
@RC00120 DS    0H                                                  0146
*       END;                                                       0146
@RF00118 DS    0H                                                  0147
*   END;                                                           0147
@RC00092 DS    0H                                                  0147
@DE00091 SR    @04,@04                                             0147
         CR    MAXVALUE,@04                                        0147
         BE    @DL00091                                            0147
*                                                                  0148
*/*                                                                  */
*/*                      GET CORE FOR ASVT                           */
*/*                                                                  */
*                                                                  0148
*   RESPECIFY                                                      0148
*    (LENASVT,                                                     0148
*     ADDRREG,                                                     0148
*     RTNCDRG)RSTD;                                                0148
*   ASVTPTR=0;                      /* POINTER NOT REALLY USED       */
*                                                                  0149
         SR    @14,@14                                             0149
         LR    ASVTPTR,@14                                         0149
*/*  NOTE: ASVT CAN BE USED FOR CALCULATION WITH A POINTER OF ZERO   */
*/*        ONLY THE LENGTH OF THE ASVT HEADER IS NEEDED              */
*                                                                  0150
*   LENASVT=(MAXVALUE*4)+LENGTH(ASVT)-508-4;/* CALCULATE LEN OF ASVT */
         LR    LENASVT,MAXVALUE                                    0150
         SLA   LENASVT,2                                           0150
         SH    LENASVT,@CH01109                                    0150
*   GENERATE;                                                      0151
*                          /* ISSUE GETMAIN FOR SQA */
         GETMAIN RC,LV=(LENASVT),SP=245,                               *
               RELATED=(SP245-(MAXUSER*4+16),'CORE IS NOT FREED')
*   IF RTNCDRG^=SUCCESS             /* GETMAIN SUCCESSFUL ?          */
*     THEN                                                         0152
         SR    @14,@14                                             0152
         CR    RTNCDRG,@14                                         0152
         BE    @RF00152                                            0152
*     DO;                           /* IF GETMAIN FAILED, ISSUE MSG  */
*       MAXERR(2:7)=BUFFER;         /* PUT VALUE SPECIFIED INTO MSG  */
         MVC   MAXERR+1(6),BUFFER                                  0154
*       MAXUSER=0;                  /* ZERO PTR IN PARMTAB           */
         L     @11,NVTPTAB(,RNVT)                                  0155
         ST    @14,MAXUSER(,@11)                                   0155
*       GENERATE REFS(NVTWTO,NVTPRMPT,PARMNAME);                   0156
*                                                      /* ISSUE MSGS */
         IEAPMNIP TYPE=WTO,PARAM=SQAMSG
         IEAPMNIP TYPE=PRMPT,PARAM=PARMNAME,OPT=ERROR
*       GOTO INIT;                  /* START PROCESS OVER AGAIN      */
         B     INIT                                                0157
*     END;                                                         0158
*   ASVTPTR=ADDRREG-PREFASVT;       /* ADDRESS MUST BE ADJUSTED FOR
*                                      RESERVED PREFIX AREA          */
@RF00152 LR    ASVTPTR,ADDRREG                                     0159
         SH    ASVTPTR,@CH01012                                    0159
*                                                                  0160
*/*                                                                  */
*/*                    ZERO ASVT                                     */
*/*                                                                  */
*                                                                  0160
*   RESPECIFY                                                      0160
*     INPUT BASED(ADDRREG);                                        0160
*   DO WHILE LENASVT>256;           /* ZERO 256 BYTES AT A TIME      */
         B     @DE00161                                            0161
@DL00161 DS    0H                                                  0162
*     INPUT(1:256)=''B;             /* ZERO 256 BYTES                */
         XC    INPUT(256,ADDRREG),INPUT(ADDRREG)                   0162
*     ADDRREG=ADDRREG+256;          /* UPDATE ADDR BY 256            */
         LA    @14,256                                             0163
         AR    ADDRREG,@14                                         0163
*     LENASVT=LENASVT-256;          /* UPDATE LEN TO DO BY 256       */
         SR    LENASVT,@14                                         0164
*   END;                                                           0165
*                                                                  0165
@DE00161 CH    LENASVT,@CH01001                                    0165
         BH    @DL00161                                            0165
*/*                THEN ZERO OUT THE REMAINING BYTES                 */
*                                                                  0166
*   INPUT(1:LENASVT)=INPUT(1:LENASVT)&&INPUT(1:LENASVT);           0166
         LR    @14,LENASVT                                         0166
         BCTR  @14,0                                               0166
         EX    @14,@SX01112                                        0166
*                                                                  0167
*/*                                                                  */
*/*                      INITIALIZE ASVT                             */
*/*                                                                  */
*                                                                  0167
*   ASVTASVT=IDASVT;                /* INITIALIZE ID FIELD           */
         MVC   ASVTASVT(4,ASVTPTR),@CC01084                        0167
*   ASVTMAXU=MAXVALUE;              /* INITIALIZE ASVT MAXUSER VALUE */
         ST    MAXVALUE,ASVTMAXU(,ASVTPTR)                         0168
*   CTR=CVTASVT->ASVTMAXU;          /* NUMBER OF PERMANENTLY ASSIGNED
*                                      ADDRESS SPACES                */
         L     @14,CVTASVT(,CVTPTR)                                0169
         L     @11,ASVTMAXU(,@14)                                  0169
         LR    CTR,@11                                             0169
*   TEMPPTR=CVTASVT;                /* SET UP TO INITIALIZE ENTRIES  */
         LR    TEMPPTR,@14                                         0170
*   PERMPTR=ASVTPTR;                /* SET UP TO INITIALIZE ENTRIES  */
*                                                                  0171
         LR    PERMPTR,ASVTPTR                                     0171
*/*  INITIALIZE PERMANENT ADDRESS SPACE ENTRIES IN ASVT              */
*                                                                  0172
*   DO CTR=CVTASVT->ASVTMAXU BY-1 TO 1;                            0172
         LR    CTR,@11                                             0172
         B     @DE00172                                            0172
@DL00172 DS    0H                                                  0173
*     PERMPTR->ASVTENTY=TEMPPTR->ASVTENTY;                         0173
         MVC   ASVTENTY(4,PERMPTR),ASVTENTY(TEMPPTR)               0173
*     PERMPTR=PERMPTR+4;            /* UPDATE TO NEXT ENTRY          */
         LA    @14,4                                               0174
         AR    PERMPTR,@14                                         0174
*     TEMPPTR=TEMPPTR+4;            /* UPDATE TO NEXT ENTRY          */
         AR    TEMPPTR,@14                                         0175
*   END;                                                           0176
         BCTR  CTR,0                                               0176
@DE00172 LTR   CTR,CTR                                             0176
         BP    @DL00172                                            0176
*   IF ASVTMAXU=CVTASVT->ASVTMAXU   /* ALL ASID'S ASSIGNED ?         */
*     THEN                                                         0177
         L     @14,CVTASVT(,CVTPTR)                                0177
         CLC   ASVTMAXU(4,ASVTPTR),ASVTMAXU(@14)                   0177
         BNE   @RF00177                                            0177
*     DO;                           /* NO AVAILABLE ENTRIES IN ASVT  */
*       ASVTFRST=0;                 /* INDICATE NO AVAILABLE ENTRIES */
         SR    @14,@14                                             0179
         ST    @14,ASVTFRST(,ASVTPTR)                              0179
*       ASVTAVAI='1'B;              /* INDICATE UNASSIGNED           */
         OI    ASVTAVAI(ASVTPTR),B'10000000'                       0180
*     END;                                                         0181
*   ELSE                                                           0182
*     DO;                           /* INITIALIZE ASVTFRST           */
         B     @RC00177                                            0182
@RF00177 DS    0H                                                  0183
*       PERMPTR=ADDR(PERMPTR->ASVTENTY);                           0183
         LA    @14,ASVTENTY(,PERMPTR)                              0183
         LR    PERMPTR,@14                                         0183
*       ASVTFRST=PERMPTR;           /* POINT TO NEXT AVAILABLE ENTRY */
         ST    PERMPTR,ASVTFRST(,ASVTPTR)                          0184
*       ASVTAVAI='1'B;              /* INDICATE UNASSIGNED           */
         OI    ASVTAVAI(ASVTPTR),B'10000000'                       0185
*       CTR=ASVTMAXU-CVTASVT->ASVTMAXU;/* NUMBER AVAILABLE           */
         L     @14,CVTASVT(,CVTPTR)                                0186
         L     CTR,ASVTMAXU(,ASVTPTR)                              0186
         S     CTR,ASVTMAXU(,@14)                                  0186
*       TEMPPTR=PERMPTR+4;          /* UPDATE PTR TO NEXT AVAILABLE  */
         LA    TEMPPTR,4                                           0187
         AR    TEMPPTR,PERMPTR                                     0187
*       DO WHILE CTR^=1;            /* LAST AVAILABLE ENTRY   SPECIAL*/
         B     @DE00188                                            0188
@DL00188 DS    0H                                                  0189
*         ENTY=TEMPPTR;             /* ENTRY POINTS TO NEXT AVAIL    */
         ST    TEMPPTR,ENTY(,PERMPTR)                              0189
*         AVAL='1'B;                /* INDICATE ENTRY AVAILABLE      */
         OI    AVAL(PERMPTR),B'10000000'                           0190
*         PERMPTR=TEMPPTR;          /* UPDATE POINTER TO NEXT ENTRY  */
         LR    PERMPTR,TEMPPTR                                     0191
*         TEMPPTR=TEMPPTR+4;        /* UPDATE NEXT POINTER           */
         AH    TEMPPTR,@CH00044                                    0192
*         CTR=CTR-1;                /* DECREMENT CTR                 */
         BCTR  CTR,0                                               0193
*       END;                                                       0194
@DE00188 CH    CTR,@CH00046                                        0194
         BNE   @DL00188                                            0194
*       ENTY=0;                     /* INDICATE LAST ENTRY           */
         SR    @14,@14                                             0195
         ST    @14,ENTY(,PERMPTR)                                  0195
*       AVAL='1'B;                  /* INDICATE ENTRY AVAILABLE      */
         OI    AVAL(PERMPTR),B'10000000'                           0196
*     END;                                                         0197
*   NVTFASVT=ASVTPTR;               /* NVT POINTS TO PERMANENT ASVT  */
@RC00177 ST    ASVTPTR,NVTFASVT(,RNVT)                             0198
*   IF CVTGSDAB='1'B                /* IF VALID VALUE IN CVTGSDA     */
*     THEN                                                         0199
         TM    CVTGSDAB(CVTPTR),B'10000000'                        0199
         BNO   @RF00199                                            0199
*     DO;                           /* THEN DUPLEX ASVT PTR & MAXUSER
*                                      VALUE                         */
*       GSDAASVT=ASVTPTR;           /* SET UP DUPLEX ASVT ADDRESS    */
         L     @14,CVTGSDA(,CVTPTR)                                0201
         ST    ASVTPTR,GSDAASVT(,@14)                              0201
*       GSDAASVB='1'B;              /* INDICATE DUPLEX VALUE VALID   */
         OI    GSDAASVB(@14),B'10000000'                           0202
*       GSDAMAXU=ASVTMAXU;          /* SET UP DUPLEX MAXUSER VALUE   */
         MVC   GSDAMAXU(2,@14),ASVTMAXU+2(ASVTPTR)                 0203
*       GSDAMAXB='1'B;              /* INDICATE DUPLEX VALUE VALID   */
         OI    GSDAMAXB(@14),B'10000000'                           0204
*     END;                                                         0205
*   END IEAVNP09                                                   0206
