         MVI   SDWARCDE(GPR01P),X'00'                              0344
*           GPR01P->SDWARCRD='1'B;  /* TURN ON RECORD INDICATOR      */
         OI    SDWARCRD(GPR01P),B'10000000'                        0345
*           GPR01P->SDWAREQ='0'B;   /* TURN OFF DUMP INDICATOR       */
         NI    SDWAREQ(GPR01P),B'01111111'                         0346
*           GEN(RETURN (14,12));                                   0347
         RETURN (14,12)
*           RESPECIFY                                              0348
*            (GPR00P,                                              0348
*             GPR01P,                                              0348
*             GPR14P,                                              0348
*             GPR15P) UNRSTD;                                      0348
*         END;                                                     0349
*       END;                                                       0350
*     ELSE                                                         0351
*       DO;                         /* NO SDWA SUPPLIED              */
         B     @RC00338                                            0351
@RF00338 DS    0H                                                  0352
