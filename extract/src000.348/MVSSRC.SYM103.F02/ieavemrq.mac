*    (RTNREG) UNRSTD;                                              0335
*RECOV:                                                            0336
*   ;                               /* START OF RECOVERY CODE        */
RECOV    DS    0H                                                  0337
*   IF RECURSOR=BITON               /* IF RECURSIVE ABEND,           */
*     THEN                                                         0337
         L     @12,PRMPTR                                          0337
         TM    RECURSOR(@12),B'10000000'                           0337
         BNO   @RF00337                                            0337
*     IF NOWKAREA=BITOFF            /* SDWA SUPPLIED                 */
*       THEN                                                       0338
         TM    NOWKAREA(@12),B'00000010'                           0338
         BNZ   @RF00338                                            0338
*       DO;                         /* CONTINUE WITH TERMINATION     */
*         SAVPTR=ESTAESA(2);        /* RESTORE REGISTER 13           */
         L     SAVPTR,ESTAESA+4                                    0340
*         DO;                       /* SETRP RC(0)RECORD(YES)WKAREA(S
*                                      DWAPTR)DUMP(NO)REGS(14,12)    */
*           RESPECIFY                                              0342
*            (GPR00P,                                              0342
*             GPR01P,                                              0342
