         TITLE 'IEAVEMRQ- MEMORY REQUEST                               *
                        '
IEAVEMRQ CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'IEAVEMRQ  75.324'                                 0001
         DROP  @15
@PROLOG  ST    @14,12(,@13)                                        0001
         STM   @00,@12,20(@13)                                     0001
         BALR  @09,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@09                                         0001
         L     @00,@SIZDATD                                        0001
         GETMAIN  R,LV=(0)
         LR    @10,@01                                             0001
         USING @DATD,@10                                           0001
         LM    @00,@01,20(@13)                                     0001
*   PRMPTR=ADDR(ESTPARM);           /* INITIALIZE PTR TO ESTAE PARM  */
         LA    @12,ESTPARM                                         0105
         ST    @12,PRMPTR                                          0105
*   ASVTPTR=CVTASVT;                /* SET UP ASVT ADDRESS           */
         L     @11,CVTPTR                                          0106
         L     ASVTPTR,CVTASVT(,@11)                               0106
*   CSCBPTR=ADDRCSCB;               /* PICK UP CSCB PTR              */
         MVC   CSCBPTR(4),ADDRCSCB(PARMRG)                         0107
*   RESPECIFY                                                      0108
*    (PARMRG) UNRSTD;                                              0108
*                                                                  0109
*/*                                                                  */
*/*                        SET UP ESTAE ENVIRONMENT                  */
*/*                                                                  */
*                                                                  0109
*   ESTAPRM=''B;                    /* INITIALIZE PARM AREA TO 0     */
         XC    ESTAPRM(28,@12),ESTAPRM(@12)                        0109
*   CODESAV=REG9;                   /* SAVE CODE REGISTER            */
         ST    REG9,CODESAV(,@12)                                  0110
*   DATASAV=REG10;                  /* SAVE DATA REGISTER            */
         ST    REG10,DATASAV(,@12)                                 0111
*   SAVADDR=SAVPTR;                 /* SAVE SAVE AREA ADDRESS IN   0112
*                                      ESTAE PARAMETER AREA          */
         ST    SAVPTR,SAVADDR(,@12)                                0112
*   RESPECIFY                                                      0113
*    (ESTAPTR) RSTD;                                               0113
*   ESTAPTR=ADDR(PRMLST);           /* SET UP ADDRESS OF PARM AREA   */
         LA    ESTAPTR,PRMLST                                      0114
*   ESTA=ESTAELST;                  /* MOVE LIST FORM OF MACRO TO  0115
*                                      GOTTEN CORE                   */
         MVC   ESTA(16,ESTAPTR),ESTAELST                           0115
*   RESPECIFY                                                      0116
*     WRKRG1 RSTD;                                                 0116
*   WRKRG1=ADDR(ESTAPRM);           /* ADDRESS OF PARM AREA          */
         LR    WRKRG1,@12                                          0117
*   GENERATE REFS(ESTAPTR,WRKRG1);                                 0118
*                                 /* ISSUE ESTAE MACRO               */
         ESTAE PARAM=(WRKRG1),MF=(E,(ESTAPTR)),                        *
               RELATED=(MRQESTAE,IEAVEMRQ)
*   RESPECIFY                                                      0119
*    (ESTAPTR,                                                     0119
*     WRKRG1) UNRSTD;                                              0119
*                                                                  0120
*/*                                                                  */
*/*                      GET CORE FOR ASCB AND SPL                   */
*/*                                                                  */
*/*     ASCB AND LSPL MUST BE CONTIGUOUS AS OF PTM VS06241   @YM06241*/
*/*     LSPL ADDRESS MUST BE ADDRESS OF ASCB + LENGTH(ASCB)  @YM06241*/
*/*                                                                  */
*                                                                  0120
*   RESPECIFY                                                      0120
*    (LENREG,                                                      0120
*     ADDRREG,                                                     0120
*     RTNCDERG) RSTD;                                              0120
*   LENREG=LENGTH(ASCB)+LENGTH(LSPL)+LENGTH(ASCRLIST);             0121
*                                                                  0121
         LA    LENREG,232                                          0121
*   /*****************************************************************/
*   /*                                                               */
*   /* LENGTH FOR GETMAIN                                    @ZA04120*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0122
*   LENASCB=LENREG;                 /* SAVE LENGTH IN PARM AREA      */
         L     @14,PRMPTR                                          0122
         STH   LENREG,LENASCB(,@14)                                0122
*   GENERATE;                                                      0123
          GETMAIN RC,LV=(LENREG),SP=245,                               *
               RELATED=(SP245-216,IEAVEMDL,IEAVEMRQ)
*                                   /* GET CORE FOR ASCB AND SPL FROM
*                                      SQA - SUBPOOL 245             */
*   IF RTNCDERG=RTNCDE4 THEN        /* ERROR EXIT IF GETMAIN FAILED  */
         CH    RTNCDERG,@CH00042                                   0124
         BNE   @RF00124                                            0124
*     DO;                           /* EXIT                          */
*       GENERATE;                                                  0126
*                                    /* CANCEL ESTAE                 */
          ESTAE 0,RELATED=(MRQESTAE,IEAVEMRQ)
*                                   /* SET RETURN CODE TO 8          */
*       RETURN CODE(RTNCDE8);       /* RETURN TO CALLER              */
         L     @00,@SIZDATD                                        0127
         LR    @01,@10                                             0127
         FREEMAIN R,LV=(0),A=(1)
         LA    @15,8                                               0127
         L     @14,12(,@13)                                        0127
         LM    @00,@12,20(@13)                                     0127
         BR    @14                                                 0127
*     END;                                                         0128
*   RESPECIFY                                                      0129
*    (LENREG,                                                      0129
*     RTNCDERG) UNRSTD;                                            0129
@RF00124 DS    0H                                                  0130
*                                                                  0130
*/*                                                                  */
*/*                      INITIALIZE ASCB & SPL                       */
*/*                                                                  */
*                                                                  0130
*   ADDRASCB=ADDRREG;               /* INDICATE GETMAIN DONE         */
         L     @12,PRMPTR                                          0130
         ST    ADDRREG,ADDRASCB(,@12)                              0130
*   ASCBPTR=ADDRREG;                /* SET UP ADDRESS OF ASCB        */
         ST    ADDRREG,ASCBPTR                                     0131
*   LSPLPTR=ADDRREG+LENGTH(ASCB);   /* CALCULATE ADDR OF SPL         */
         LA    @12,208                                             0132
         ALR   @12,ADDRREG                                         0132
         ST    @12,LSPLPTR                                         0132
*   RESPECIFY                                                      0133
*    (ADDRREG) UNRSTD;                                             0133
*   ASCB=''B;                       /* ZERO OUT ASCB                 */
         L     @11,ASCBPTR                                         0134
         XC    ASCB(208,@11),ASCB(@11)                             0134
*   ASCBASCB=IDASCB;                /* INITIALIZE ACRONYM - ASCB-    */
         MVC   ASCBASCB(4,@11),@CC01654                            0135
*   ASCBSSRB=STOPSRB;               /* INDICATE STATUS STOP SRBS     */
         LA    @15,1                                               0136
         STH   @15,ASCBSSRB(,@11)                                  0136
*   ASCBCSCB=CSCBPTR;               /* INITIALIZE CSCB ADDRESS       */
         MVC   ASCBCSCB(4,@11),CSCBPTR                             0137
*   ASCBSPL=LSPLPTR;                /* INITIALIZE SPL ADDRESS        */
         ST    @12,ASCBSPL(,@11)                                   0138
*   ASCBAFFN=AFFNBITS;              /* INITIALIZE CPU AFFINITY       */
         MVC   ASCBAFFN(2,@11),@CB01660                            0139
*   ASCBNOQ=BITON;                  /* ASCB NOT ON READY QUEUE       */
         OI    ASCBNOQ(@11),B'10000000'                            0140
*   ASCBTOFF=BITON;                 /* INDICATE NO JOB STEP TIMING   */
         OI    ASCBTOFF(@11),B'10000000'                           0141
*   ASCBDP=252;                     /* DISPATCHING PRIORITY  @ZA07475*/
         MVI   ASCBDP(@11),X'FC'                                   0142
*   LSPL=''B;                       /* ZERO OUT LSPL                 */
         XC    LSPL(16,@12),LSPL(@12)                              0143
*                                                                  0144
*/*                                                                  */
*/*                SET UP LOCKED ENVIRONMENT                         */
*/*                                                                  */
*/* MEMORY REQUEST MUST BE FIXED WHILE HOLDING THE GLOBAL DISPATCHER */
*/* LOCK PAGE FAULTS CANNOT BE HANDLED. THEREFORE, THE PAGE IS       */
*/* FIXED, THE LOCK IS OBTAINED, AND AN FRR IS ESTABLISHED WHILE     */
*/* HOLDING THE LOCK                                                 */
*/*                                                                  */
*                                                                  0144
*   RESPECIFY                                                      0144
*    (ECBRG,                                                       0144
*     RTNCDERG,                                                    0144
*     WRKRG1,                                                      0144
*     WRKRG2) RSTD;                                                0144
*   ECBRG=ADDR(ASCBMECB);           /* ADDRESS OF ECB TO BE POSTED   */
         LA    @14,ASCBMECB(,@11)                                  0145
         LR    ECBRG,@14                                           0145
*   ECBLIST=ADDR(ASCBMECB);         /* SET UP ECB ADDRESS            */
         ST    @14,ECBLIST                                         0146
*   LASTBIT=BITON;                  /* INDICATE LAST IN LIST         */
         OI    LASTBIT,B'10000000'                                 0147
*   WRKRG1=ADDR(IEAVEMRQ);          /* BEGINNING OF AREA TO BE FIXED */
         L     WRKRG1,@CA00028                                     0148
*   WRKRG2=1+ADDR(@ENDDATA);        /* ADDR OF END OF RTN            */
         LA    @14,1                                               0149
         LA    WRKRG2,@ENDDATA                                     0149
         ALR   WRKRG2,@14                                          0149
*   GENERATE REFS(ECBRG,WRKRG1,WRKRG2);                            0150
*                                      /* FIX MODULE */
*                   FIX ENTIRE MODULE- IEAVEMRQ
         PGFIX R,A=(WRKRG1),EA=(WRKRG2),ECB=(ECBRG),LONG=N,            *
               RELATED=(IEAVEMRQ-@ENDDATA,IEAVEMRQ)
*   IF RTNCDERG=RTNCDE8             /* PGFIX INDICATE WAIT ?         */
*     THEN                                                         0151
         CH    RTNCDERG,@CH00069                                   0151
         BNE   @RF00151                                            0151
*     DO;                           /* YES, ISSUE WAIT               */
*       GENERATE REFS(ECBLIST);                                    0153
*                                 /* LIST FORM OF WAIT MACRO */
         WAIT  ECBLIST=ECBLIST,   WAIT UNTIL PAGE IS FIXED             *
               RELATED=(ASCBMECB,PGFIX)
*     END;                                                         0154
*   ASCBMECB=0;                     /* CLEAR ECB                     */
@RF00151 L     @14,ASCBPTR                                         0155
         SLR   @12,@12                                             0155
         ST    @12,ASCBMECB(,@14)                                  0155
*   WRKRG1=ADDR(@DATD);             /* GET ADDRESS OF DATA AREA      */
         LA    @12,@DATD                                           0156
         LR    WRKRG1,@12                                          0156
*   WRKRG2=ADDR(@DATD)+@SIZDATD;    /* ENDING ADDRESS OF DATA      0157
*                                                            @YM02231*/
         AL    @12,@SIZDATD                                        0157
         LR    WRKRG2,@12                                          0157
*   ECBRG=ADDR(ASCBMECB);           /* ADDRESS OF ECB TO BE POSTED   */
         LA    ECBRG,ASCBMECB(,@14)                                0158
*   GENERATE REFS(ECBRG,WRKRG1,WRKRG2);                            0159
*                                      /* FIX DATA AREA */
         PGFIX R,A=(WRKRG1),EA=(WRKRG2),ECB=(ECBRG),LONG=N,            *
               RELATED=(@DATD,IEAVEMRQ)
*   IF RTNCDERG=RTNCDE8             /* PGFIX INDICATE WAIT ?         */
*     THEN                                                         0160
         CH    RTNCDERG,@CH00069                                   0160
         BNE   @RF00160                                            0160
*     DO;                           /* YES, ISSUE WAIT               */
*       GENERATE REFS(ECBLIST);                                    0162
*                                 /* LIST FORM OF WAIT MACRO */
         WAIT  ECBLIST=ECBLIST,   WAIT UNTIL PAGE IS FIXED             *
               RELATED=(ASCBMECB,PGFIX)
*     END;                                                         0163
*   RTNFIXED=BITON;                 /* INDICATE ROUTINE FIXED        */
@RF00160 L     @14,PRMPTR                                          0164
         OI    RTNFIXED(@14),B'01000000'                           0164
*   RESPECIFY                                                      0165
*    (REGFRR,                                                      0165
*     EXITADDR,                                                    0165
*     REG11,                                                       0165
*     REG12,                                                       0165
*     REG14) RSTD;                                                 0165
*   EXITADDR=ADDR(MRQFRR);          /* ADDRESS OF FRR ROUTINE        */
*                                                                  0166
         LA    EXITADDR,MRQFRR                                     0166
*   /*****************************************************************/
*   /*                                                               */
*   /* GET GLOBAL DISPATCHER LOCK AND ESTABLISH FRR                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0167
*   GENERATE REFS(PSALITA,PSACSTK,WRKRG1,WRKRG2,FRRS,FRRSCURR,PSALSFCC,
*       FRRSLAST,FRRSELEN,FRRSPARM,EXITADDR,FRRSFRRA,REGFRR);      0167
*                                 OBTAIN GLOBAL DISPATCHER LOCK
         SETLOCK OBTAIN,TYPE=DISP,MODE=UNCOND,                         *
               RELATED=(DISPATCHER,IEAVEMRQ)
*                                 ESTABLISH FRR WHILE HOLDING LOCK
         SETFRR A,FRRAD=(EXITADDR),PARMAD=(REGFRR),                    *
               WRKREGS=(WRKRG1,WRKRG2),RELATED=(MRQFRR,IEAVEMRQ)
*   RESPECIFY                                                      0168
*    (RTNCDERG,                                                    0168
*     ECBRG,                                                       0168
*     WRKRG2,                                                      0168
*     EXITADDR,                                                    0168
*     REG11,                                                       0168
*     REG12,                                                       0168
*     REG14) UNRSTD;                                               0168
*   ADDRESTA=ADDR(ESTAPRM);         /* INITIALIZE ADDR OF ESTAE PARM */
         L     @12,PRMPTR                                          0169
         ST    @12,ADDRESTA(,REGFRR)                               0169
*   SAVPTR=SAVADDR;                 /* RESTORE SAVE AREA ADDRESS     */
         L     SAVPTR,SAVADDR(,@12)                                0170
*   ASCBMECB=0;                     /* CLEAR ECB                     */
         L     @12,ASCBPTR                                         0171
         SLR   @11,@11                                             0171
         ST    @11,ASCBMECB(,@12)                                  0171
*                                                                  0172
*/*                                                                  */
*/*                           ASSIGN AN ASID                         */
*/*                                                                  */
*                                                                  0172
*   IF ASVTFRST=NOASIDS THEN        /* IF NO AVAILABLE ENTRIES, THEN */
         CLC   ASVTFRST(4,ASVTPTR),NOASIDS                         0172
         BNE   @RF00172                                            0172
*     DO;                           /* RELEASE LOCK, SET A RETURN  0173
*                                      CODE OF 4 AND RETURN          */
*/*   ERROR EXIT FOR CASE WHEN ASID NOT ASSIGNED, PAGE FIXED,      0174
*         LOCK HELD AND ASCB GOTTEN                                  */
*                                                                  0174
*ERREXIT1:                                                         0174
*       ;                                                          0174
ERREXIT1 DS    0H                                                  0175
*       RESPECIFY                                                  0175
*        (WRKRG1,                                                  0175
*         WRKRG2,                                                  0175
*         REG11,                                                   0175
*         REG12,                                                   0175
*         REG14) RSTD;                                             0175
*       GENERATE REFS(PSALITA,PSACSTK,WRKRG1,WRKRG2,FRRS,FRRSCURR, 0176
*           FRRSEMP,FRRSELEN);                                     0176
*                                 DELETE FRR
         SETFRR D,WRKREGS=(WRKRG1,WRKRG2),RELATED=(MRQFRR,IEAVEMRQ)
*                                 RELEASE DISPATCHER LOCK
         SETLOCK RELEASE,TYPE=DISP,RELATED=(DISPATCHER,IEAVEMRQ)
*       RESPECIFY                                                  0177
*        (REG11,                                                   0177
*         REG12,                                                   0177
*         REG14) UNRSTD;                                           0177
*       WRKRG1=ADDR(IEAVEMRQ);      /* BEGINNING OF AREA TO BE FIXED */
         L     WRKRG1,@CA00028                                     0178
*       WRKRG2=1+ADDR(@ENDDATA);    /* ADDR OF END OF RTN            */
         LA    WRKRG2,@ENDDATA                                     0179
         AH    WRKRG2,@CH00048                                     0179
*       GENERATE REFS(WRKRG1,WRKRG2);                              0180
*                                 RELEASE PAGE FIX
         PGFREE R,A=(WRKRG1),EA=(WRKRG2),                              *
               RELATED=(IEAVEMRQ-@ENDDATA,IEAVEMRQ)
*       WRKRG1=ADDR(@DATD);         /* GET ADDRESS OF DATA AREA      */
         LA    @12,@DATD                                           0181
         LR    WRKRG1,@12                                          0181
*       WRKRG2=ADDR(@DATD)+@SIZDATD;/* ENDING ADDRESS OF DATA      0182
*                                                            @YM02231*/
         AL    @12,@SIZDATD                                        0182
         LR    WRKRG2,@12                                          0182
*       GENERATE REFS(WRKRG1,WRKRG2);                              0183
*                                  /* UNFIX DATA AREA */
         PGFREE R,A=(WRKRG1),EA=(WRKRG2),                              *
               RELATED=(@DATD,IEAVEMRQ)
*       RESPECIFY                                                  0184
*        (WRKRG1,                                                  0184
*         WRKRG2) UNRSTD;                                          0184
*       RTNFIXED=BITOFF;            /* INDICATE RTN NO LONGER FIXED  */
         L     @12,PRMPTR                                          0185
         NI    RTNFIXED(@12),B'10111111'                           0185
*       RESPECIFY                                                  0186
*        (LENREG,                                                  0186
*         ADDRREG,                                                 0186
*         RTNCDERG) RSTD;                                          0186
*       LSPL=''B;                   /* ZERO LSPL BEFORE FREEING      */
         L     @14,LSPLPTR                                         0187
         XC    LSPL(16,@14),LSPL(@14)                              0187
*       ASCB=''B;                   /* ZERO ASCB BEFORE FREEING      */
         L     @14,ASCBPTR                                         0188
         XC    ASCB(208,@14),ASCB(@14)                             0188
*       LENREG=LENASCB;             /* LENGTH OF GOTTEN CORE         */
         LH    LENREG,LENASCB(,@12)                                0189
*       ADDRREG=ASCBPTR;            /* BEGINING ADDRESS OF GOTTEN  0190
*                                      CORE                          */
         LR    ADDRREG,@14                                         0190
*       GENERATE REFS(ADDRREG);                                    0191
*                                 /* FREE ASCB & LSPL */
*                                 FREE ASCB & SPL
         FREEMAIN RC,LV=(LENREG),A=(ADDRREG),SP=245,                   *
               RELATED=(SP245-216,IEAVEMRQ)
*       ADDRASCB=0;                 /* INDICATE FREEMIAN DONE        */
         L     @14,PRMPTR                                          0192
         SLR   @12,@12                                             0192
         ST    @12,ADDRASCB(,@14)                                  0192
*       GENERATE;                                                  0193
*                                   /* CANCEL ESTAE */
         ESTAE 0,RELATED=(MRQESTAE,IEAVEMRQ)      CANCEL ESTAE
*       RESPECIFY                                                  0194
*        (LENREG,                                                  0194
*         ADDRREG,                                                 0194
*         RTNCDERG) UNRSTD;                                        0194
*       SAVPTR=SAVADDR;             /* RESTORE SAVE AREA ADDRESS SET
*                                      RETURN CODE EQ 8              */
         L     @11,PRMPTR                                          0195
         L     SAVPTR,SAVADDR(,@11)                                0195
*       RETURN CODE(RTNCDE8);       /* RETURN TO CALLER              */
         L     @00,@SIZDATD                                        0196
         LR    @01,@10                                             0196
         FREEMAIN R,LV=(0),A=(1)
         LA    @15,8                                               0196
         L     @14,12(,@13)                                        0196
         LM    @00,@12,20(@13)                                     0196
         BR    @14                                                 0196
*     END;                                                         0197
*                                                                  0197
*/*                                                                  */
*/*              ASID'S AVAILABLE                                    */
*/*                                                                  */
*                                                                  0198
*ASSIGNID:                          /* RETRY ADDRESS FROM FRR      0198
*                                                            @ZA04120*/
*   ENTYADDR=ASVTFRST;              /* SAVE ADDRESS OF ENTRY TO BE 0198
*                                      ASSIGNED                      */
@RF00172 DS    0H                                                  0198
ASSIGNID L     @12,ASVTFRST(,ASVTPTR)                              0198
         L     @11,PRMPTR                                          0198
         ST    @12,ENTYADDR(,@11)                                  0198
*/* CHECK TO SEE IF ASVTFRST CONTAINS A VALID ADDRESS                */
*                                                                  0199
*   IF ASVTAVAI=BITOFF×             /* AVAILABLE BIT ON?             */
*                                                                  0199
*   /*****************************************************************/
*   /*                                                               */
*   /* ENTRY ADDRESS WITHIN RANGE OF ASVT?                           */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0199
*       (ENTYADDR&'7FFFFFFF'X)<ADDR(ASVTENTY(1))×(ENTYADDR&'7FFFFFFF'X)
*       >ADDR(ASVTENTY(ASVTMAXU)) THEN                             0199
         TM    ASVTAVAI(ASVTPTR),B'10000000'                       0199
         BZ    @RT00199                                            0199
         N     @12,@CF01745                                        0199
         LA    @11,ASVTENTY(,ASVTPTR)                              0199
         CR    @12,@11                                             0199
         BL    @RT00199                                            0199
         L     @11,ASVTMAXU(,ASVTPTR)                              0199
         SLA   @11,2                                               0199
         LA    @11,ASVTENTY-4(@11,ASVTPTR)                         0199
         CR    @12,@11                                             0199
         BNH   @RF00199                                            0199
@RT00199 DS    0H                                                  0200
*     DO;                           /* ERROR IN ASVTFRST             */
*       GOTO ERREXIT1;              /* NO, CONTINUE WITH ERROR EXIT  */
         B     ERREXIT1                                            0201
*     END;                                                         0202
*                                                                  0203
*/*                                                                  */
*/*                       READY TO ASSIGN ASID                       */
*/*                                                                  */
*                                                                  0203
*   ASVTFRST=AVLENTRY;              /* FIRST AVAILABLE ENTRY HAS BEEN
*                                      DELETED FROM CHAIN            */
@RF00199 L     @12,PRMPTR                                          0203
         L     @12,ENTYADDR(,@12)                                  0203
         MVC   ASVTFRST(4,ASVTPTR),AVLENTRY(@12)                   0203
*   BALTASVT=BITON;                 /* INDICATE ASVT BEEN ALTERED    */
         OI    BALTASVT(REGFRR),B'01000000'                        0204
*   ASGNBIT=BITOFF;                 /* HIGH ORDER BIT 0 INDICATES  0205
*                                      ASID ASSIGNED                 */
         NI    ASGNBIT(@12),B'01111111'                            0205
*   ASID=(ENTYADDR-ADDR(ASVTFRST))/4;/* CALCULATE ASID               */
         LA    @11,ASVTFRST(,ASVTPTR)                              0206
         LR    @00,@12                                             0206
         SLR   @00,@11                                             0206
         SRDA  @00,32                                              0206
         D     @00,@CF00042                                        0206
         STH   @01,ASID                                            0206
*   ASCBASID=ASID;                  /* INITIALIZE ASID IN ASCB       */
         L     @12,ASCBPTR                                         0207
         STH   @01,ASCBASID(,@12)                                  0207
*   CHASID=ASID;                    /* PUT ASID IN CSCB              */
         L     @11,CSCBPTR                                         0208
         STH   @01,CHASID(,@11)                                    0208
*   RFY                                                            0209
*    (WRKRG1,                                                      0209
*     WRKRG2) RSTD;                                                0209
*   WRKRG1=CVTASCRL;                /* LAST ASCR ELT         @ZA04120*/
         L     @11,CVTPTR                                          0210
         L     WRKRG1,CVTASCRL(,@11)                               0210
*   WRKRG2=ASCBPTR+LENGTH(ASCB)+LENGTH(LSPL);/* CALCULATE THIS ASCR
*                                      ADDRESS               @ZA04120*/
         AH    @12,@CH01785                                        0211
         LR    WRKRG2,@12                                          0211
*   WRKRG2->ASCRFWDP=NULL;          /* ZERO FORWARD PTR      @ZA04120*/
         SLR   @12,@12                                             0212
         ST    @12,ASCRFWDP(,WRKRG2)                               0212
*   WRKRG2->ASCRBWDP=WRKRG1;        /* BKWD PTR=LAST ELT     @ZA04120*/
         ST    WRKRG1,ASCRBWDP(,WRKRG2)                            0213
*   IF WRKRG1^=NULL THEN                                           0214
         CR    WRKRG1,@12                                          0214
         BE    @RF00214                                            0214
*     WRKRG1->ASCRFWDP=WRKRG2;      /* CHAIN NEW ELT         @ZA04120*/
         ST    WRKRG2,ASCRFWDP(,WRKRG1)                            0215
*   ELSE                                                           0216
*     CVTASCRF=WRKRG2;              /* QUEUE WAS EMPTY       @ZA04120*/
         B     @RC00214                                            0216
@RF00214 L     @12,CVTPTR                                          0216
         ST    WRKRG2,CVTASCRF(,@12)                               0216
*   CVTASCRL=WRKRG2;                /* ADJUST TRAILER        @ZA04120*/
@RC00214 L     @12,CVTPTR                                          0217
         ST    WRKRG2,CVTASCRL(,@12)                               0217
*   AVLENTRY=ASCBPTR;               /* PUT ADDRESS OF ASCB IN ENTRY
*                                      BEING ASSIGNED                */
         L     @12,PRMPTR                                          0218
         L     @11,ENTYADDR(,@12)                                  0218
         MVC   AVLENTRY(4,@11),ASCBPTR                             0218
*   ASSIGN=BITON;                   /* INDICATE ASSIGNING OF ASID  0219
*                                      COMPLETE                      */
         OI    ASSIGN(@12),B'00010000'                             0219
*   RFY                                                            0220
*    (WRKRG1,                                                      0220
*     WRKRG2) UNRSTD;                                              0220
*                                                                  0221
