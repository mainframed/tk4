*/*              MEMORY REQUEST ESTAE ROUTINE                        */
*/*                                                                  */
*                                                                  0301
*MRQESTAE:                                                         0301
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);/* ESTAE EXIT ROUTINE            */
MRQESTAE DS    0H                                                  0302
*   DCL                                                            0302
*     RETRYRG REG(0) PTR(31);       /* RETRY ADDRESS IF NO SDWA GIVEN*/
*   RESPECIFY                                                      0303
*    (ADDRREG,                                                     0303
*     RTNREG) RSTD;                                                0303
*   DO;                             /* ENTESTAE (REG9,REG10)SAVEAREA(
*                                      ESTAESA)ERREXIT(NOSDWA)       */
*                                                                  0304
*     /***************************************************************/
*     /*                                                             */
*     /* ENTRY CODE FOR ESTAE ROUTINE. IF RTCA ALLOCATED THEN RESTORE*/
*     /* CODE AND DATA REGS FROM START OF USERS PARM LIST, AND       */
*     /* ESTABLISH ADDR TO ESTAE SAVE AREA IF REQUIRED. IF NO RTCA   */
*     /* AVAILABLE THEN BRANCH TO ERROR CODE AFTER ESTABLISHING      */
*     /* ADDRESSABILITY                                              */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0305
*     GEN(USING *,GPR15P);                                         0305
         USING *,GPR15P
*     GEN(CH    GPR00F,*+26);       /* CHECK IF RTCA ALLOC FAILED    */
         CH    GPR00F,*+26
*     GEN(BNE   *+24);              /* NO,SAVE RTM REGS              */
         BNE   *+24
*     GEN(DROP  GPR15P);            /* DROP LOCAL ADDR               */
         DROP  GPR15P
*     GEN(LR    GPR15P,GPR02P);     /* PTR TO USER PARM LIST         */
         LR    GPR15P,GPR02P
*     GEN(L     REG9,00(GPR15P));                                  0310
         L     REG9,00(GPR15P)
*     GEN(L     REG10,04(GPR15P));                                 0311
         L     REG10,04(GPR15P)
*     GEN(LA    GPR13P,ESTAESA);    /* SET PTR TO ESTAE SA           */
         LA    GPR13P,ESTAESA
*     GEN(B     NOSDWA);            /* BRANCH TO ERROR EXIT          */
         B     NOSDWA
*     GEN(DC    H'12');             /* NO RTCA AVAIL RC              */
         DC    H'12'
*     GEN(STM   GPR14P,GPR12F,12(GPR13P));/* GOOD R13, SAVE RTM'S  0315
*                                      REGS                          */
         STM   GPR14P,GPR12F,12(GPR13P)
*     GEN(L     GPR15P,0(GPR01P));  /* PTR TO USER PARM LIST         */
         L     GPR15P,0(GPR01P)
*     GEN(L     REG9,00(GPR15P));                                  0317
         L     REG9,00(GPR15P)
*     GEN(L     REG10,04(GPR15P));                                 0318
         L     REG10,04(GPR15P)
*     GEN(ST    GPR13P,ESTAESA+4);  /* SET PTR TO RTM SA             */
         ST    GPR13P,ESTAESA+4
*     GEN(LA    GPR13P,ESTAESA);    /* SET PTR TO ESTAE SA           */
         LA    GPR13P,ESTAESA
*   END;                            /* ENTESTAE (REG9,REG10)SAVEAREA(
*                                      ESTAESA)ERREXIT(NOSDWA)       */
*   RESPECIFY                                                      0322
*     SDWAPTR RSTD;                                                0322
*   SDWAPTR=ADDRREG;                /* SET UP SDWA ADDRESS           */
         LR    SDWAPTR,ADDRREG                                     0323
*   RESPECIFY                                                      0324
*    (ADDRREG,                                                     0324
*     RTNREG) UNRSTD;                                              0324
*   SDWAMODN=MODNAME;               /* MODULE NAME                   */
