*   SDWACSCT=MODNAME;               /* CSECT NAME                    */
         MVC   SDWACSCT(8,SDWAPTR),@CC01663                        0326
*   SDWAREXN=RECNAME2;              /* RECOVERY ROUTINE NAME         */
         MVC   SDWAREXN(8,SDWAPTR),@CC01667                        0327
*   SDWASR09=REG9;                  /* SET UP REG 9 FOR RESTORE AT 0328
*                                      RETRY                         */
         ST    REG9,SDWASR09(,SDWAPTR)                             0328
*   SDWASR10=REG10;                 /* SET UP REG 10 FOR RESTORE AT
*                                      RETRY                         */
         ST    REG10,SDWASR10(,SDWAPTR)                            0329
*   GO TO RECOV;                    /* START RECOVERY PROCESS        */
         B     RECOV                                               0330
*NOSDWA:                                                           0331
*   ;                               /* NO SDWA SUPPLIED              */
NOSDWA   DS    0H                                                  0332
*   NOWKAREA=BITON;                 /* INDICATE NO SDWA GIVEN        */
         L     @12,PRMPTR                                          0332
         OI    NOWKAREA(@12),B'00000010'                           0332
*   RESPECIFY                                                      0333
