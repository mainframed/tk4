         TITLE 'IEEMB815 - CHNGDUMP COMMAND PROCESSOR                  *
                        '
IEEMB815 CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(19)                                             0001
         DC    C'IEEMB815R0300077047'                              0001
         DROP  @15
@PROLOG  BALR  @12,0                                               0001
@PSTART  LA    @10,4095(,@12)                                      0001
         USING @PSTART,@12                                         0001
         USING @PSTART+4095,@10                                    0001
         L     @00,@SIZDATD                                        0001
         GETMAIN  R,LV=(0)
         LR    @08,@01                                             0001
         USING @DATD,@08                                           0001
         ST    @13,@SA00001+4                                      0001
         LR    @13,@08                                             0001
*                                                                  0082
*   /*****************************************************************/
*   /*                                                               */
*   /* GET AND INITIALIZE WORKAREA                                   */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0082
*   PART1=''B;                      /* CLEAR FIRST PART OF WORKAREA  */
         XC    PART1(88),PART1                                     0082
*   XSASAVE=XSAPTR;                 /* SAVE PTR TO XSA               */
         ST    XSAPTR,XSASAVE                                      0083
*   START=XAL;                      /* INITIALIZE PTR TO OPERAND     */
*                                                                  0084
         SLR   START,START                                         0084
         ICM   START,7,XAL(XSAPTR)                                 0084
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE TRANSLATE TABLE FOR FIRST SCAN INITIALIZE TRANSLATE*/
*   /* TABLE TO ALL INVALIDS                                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0085
*   TRANTBL(1)=INVLDCDE;            /* INITIALIZE FIRST BYTE OF    0085
*                                      TRANSLATE TABLE TO FUNCTION 0085
*                                      CODE FOR 'INVALIDS'   @G33CPHE*/
         MVI   TRANTBL,X'07'                                       0085
*   MVC(TRANTBL(2:256),TRANTBL(1:255));/* PROPAGATE INVALIDS @G33CPHE*/
         MVC   TRANTBL+1(255),TRANTBL                              0086
*   TRANTBL(','+1)=COMMACDE;        /* STOPPER CHARACTER AFTER 'SET'
*                                      OR 'DEL' CAN BE COMMA @G33CPHE*/
         MVI   TRANTBL+107,X'02'                                   0087
*   TRANTBL(' '+1)=EOCMDCDE;        /* STOPPER CHARACTER CAN ALSO BE
*                                      A BLANK AFTER 'DEL'   @G33CPHE*/
         MVI   TRANTBL+64,X'03'                                    0088
*   TRANTBL('='+1)=UNEXPCDE;        /* A VALID BUT CURRENTLY       0089
*                                      UNEXPECTED DELIMITER  @G33CPHE*/
         MVI   TRANTBL+126,X'06'                                   0089
*   TRANTBL('('+1)=UNEXPCDE;        /* A VALID BUT CURRENTLY       0090
*                                      UNEXPECTED DELIMITER  @G33CPHE*/
         MVI   TRANTBL+77,X'06'                                    0090
*   TRANTBL(')'+1)=UNEXPCDE;        /* A VALID BUT CURRENTLY       0091
*                                      UNEXPECTED DELIMITER  @G33CPHE*/
*                                                                  0091
         MVI   TRANTBL+93,X'06'                                    0091
*   /*****************************************************************/
*   /*                                                               */
*   /* DON'T STOP TRANSLATE ON ALPHABETIC CHARACTERS                 */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0092
*   TRANTBL('A'+1:'I'+1)=TRANTBL('A'+1:'I'+1)&&TRANTBL('A'+1:'I'+1);
         XC    TRANTBL+193(9),TRANTBL+193                          0092
*   TRANTBL('J'+1:'R'+1)=TRANTBL('J'+1:'R'+1)&&TRANTBL('J'+1:'R'+1);
         XC    TRANTBL+209(9),TRANTBL+209                          0093
*   TRANTBL('S'+1:'Z'+1)=TRANTBL('S'+1:'Z'+1)&&TRANTBL('S'+1:'Z'+1);
         XC    TRANTBL+226(8),TRANTBL+226                          0094
*   TRANTBL('0'+1:'9'+1)=TRANTBL('0'+1:'9'+1)&&TRANTBL('0'+1:'9'+1);
         XC    TRANTBL+240(10),TRANTBL+240                         0095
*   ENP=0;                          /* INITIALIZE PTR TO LAST CHAR 0096
*                                      TRANSLATED                    */
         SLR   ENP,ENP                                             0096
*   PKEY1='1'B;                     /* INDICATE 'SET', 'DEL', OR   0097
*                                      'RESET' IS EXPECTED (FIRST  0097
*                                      POSITION KEYWORD)     @G33CPHE*/
         OI    PKEY1,B'10000000'                                   0097
*   DT=0;                           /* CLEAR DUMP TYPE INDICATOR   0098
*                                                            @G33CPHE*/
         SLR   DT,DT                                               0098
*   MSGID='00'X;                    /* CLEAR MESSAGE INDEX   @G33CPHE*/
         MVI   MSGID,X'00'                                         0099
*   R14SVC34=R14;                   /* RET ADDRESS OF SVC 34 ROUTER  */
         ST    R14,R14SVC34                                        0100
*   IF START=0                      /* IS THERE ANY OPERAND ?        */
*     THEN                          /* NO                            */
         LTR   START,START                                         0101
         BNZ   @RF00101                                            0101
*     MSGID=IEE305;                 /* MSG INDEX FOR KEYWORD MISSING
*                                                            @G33CPHE*/
         MVI   MSGID,X'05'                                         0102
*   ELSE                                                           0103
*     DO;                                                          0103
         B     @RC00101                                            0103
@RF00101 DS    0H                                                  0104
*       OPLNTH=10;                  /* INITIALIZE TO ONE MORE THAN 0104
*                                      MAXIMUM KEYWORD LENGTH        */
         LA    OPLNTH,10                                           0104
*       STOPPER=0;                  /* INITIALIZE REG USED TO HOLD 0105
*                                      TRANSLATE STOPPER VALUE       */
         SLR   STOPPER,STOPPER                                     0105
*                                                                  0106
*       /*************************************************************/
*       /*                                                           */
*       /* THIS IS THE CONTROLLER SEGMENT OF THE CHNGDUMP COMMAND    */
*       /* PROCESSOR. IT CALCULATES THE LENGTH OF THE KEYWORD OR     */
*       /* PARAMETER ENCOUNTERED, THE LENGTH OF THE OPERAND REMAINING*/
*       /* TO BE TRANSLATED. IT MAINTAINS POINTERS TO THE CURRENT    */
*       /* KEYWORD, CALLS TRANSLATE AND CALLS INTERNAL PROCEDURES TO */
*       /* PROCESS THE DELIMITERS ENCOUNTERED.                       */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0106
*       DO                          /* CONTROLS TRT OF OPERAND       */
*             WHILE(MSGID='00'X);   /* TRT UNTIL END OF OPERAND OR 0106
*                                      ERROR                 @G33CPHE*/
         B     @DE00106                                            0106
@DL00106 DS    0H                                                  0107
*         ERRTXTP=START;            /* POINT TO THE TEXT TO BE     0107
*                                      SCANNED NEXT AS THE TEXT TO BE
*                                      PUT INTO AN ERROR MSG @G33CPHE*/
         ST    START,ERRTXTP                                       0107
*         GEN REFS(OPLNTH,TRTIN,TRANTBL,OPERAND) SETS(ENP,STOPPER);
        EX   OPLNTH,TRTIN          EXECUTE TRT INSTRUCTION FOR FULL
*                                  LENGTH OF OPERAND NOT YET
*                                  TRANSLATED
*         BC(8,NOSTOPER);           /* UNINTERUPTED TRT SCAN         */
         BC    8,NOSTOPER                                          0109
*/* AFTER THE TRT ENP=ADDR OF CHAR THAT CAUSED TRT TO STOP AND     0110
*   STOPPER=FUNCTION CODE FROM TRANTBL THAT CAUSED THE STOP   @G33CPHE
*                                                                    */
*                                                                  0110
*         LEN=ENP-START;            /* DETERMINE LENGTH OF KEYWORD OR
*                                      PARAMETER JUST ISOLATED       */
         LR    LEN,ENP                                             0110
         SLR   LEN,START                                           0110
*         CALL BRTABLE(STOPPER);    /* GO TO PROPER DELIMITER      0111
*                                      SUBROUTINE TO PROCESS THIS  0111
*                                      PART OF OPERAND               */
         LR    @06,STOPPER                                         0111
         SLA   @06,2                                               0111
         L     @15,BRTABLE-4(@06)                                  0111
         BALR  @14,@15                                             0111
*         STOPSAVE=STOPPER;         /* SAVE TO DETERMINE PREVIOUS  0112
*                                      STOPPER CHARACTER             */
         STC   STOPPER,STOPSAVE                                    0112
*         START=ENP+1;                                             0113
         LA    START,1                                             0113
         ALR   START,ENP                                           0113
*       END;                                                       0114
@DE00106 CLI   MSGID,X'00'                                         0114
         BE    @DL00106                                            0114
*       GO TO CONTINUE;             /* BRANCH AROUND MAX LENGTH PARM
*                                      ERROR HANDLING                */
         B     CONTINUE                                            0115
*NOSTOPER:                                                         0116
*       MSGID=IEE308;               /* MSG INDEX FOR TERM LENGTH   0116
*                                      ERROR -- ISOLATED PARM EXCEEDS
*                                      MAXIMUM ALLOWABLE     @G33CPHE*/
NOSTOPER MVI   MSGID,X'08'                                         0116
*CONTINUE:                          /* RESUME NORMAL PROCESSING      */
*     END;                                                         0117
CONTINUE DS    0H                                                  0118
*   RESPECIFY                                                      0118
*    (OPSETPTR,                                                    0118
*     STOPPER,                                                     0118
*     ENP) UNRESTRICTED;                                           0118
*                                                                  0118
@RC00101 DS    0H                                                  0119
*   /*****************************************************************/
*   /*                                                               */
*   /* UPDATE THE RTCT WITH THE NEW EFFECTIVE DUMP OPTIONS IF THE    */
*   /* MSGID IS NOT AN ERROR ONE.                            @G33CPHE*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0119
*   IF MSGID=IEE712                 /* IS THIS THE PROCESSING      0119
*                                      COMPLETE MSG?         @G33CPHE*/
*     THEN                          /* YES SO GO AHEAD WITH THE RTCT
*                                      UPDATE                @G33CPHE*/
         CLI   MSGID,X'03'                                         0119
         BNE   @RF00119                                            0119
*     IF DT=ALL                     /* CHECK TO SEE IF ALL THE DUMP
*                                      TYPES MUST BE UPDATED OR JUST
*                                      ONE                   @G33CPHE*/
*       THEN                                                       0120
         LTR   DT,DT                                               0120
         BNZ   @RF00120                                            0120
*       DO DT=1 TO DIM(RTCTOPTS);   /* IF ALL THE DUMP TYPES ARE   0121
*                                      INVOLVED LOOP DOING EACH ONE
*                                      AT A TIME             @G33CPHE*/
         LA    DT,1                                                0121
@DL00121 DS    0H                                                  0122
*         CALL UPDTERTN;            /* THIS SUBROUTINE WILL UPDATE 0122
*                                      THE RTCT EFFECTIVE DUMP OPTION
*                                      CORRESPONDING TO THE DT     0122
*                                                            @G33CPHE*/
         BAL   @14,UPDTERTN                                        0122
*       END;                        /*                       @G33CPHE*/
         AH    DT,@CH00065                                         0123
         CH    DT,@CH00057                                         0123
         BNH   @DL00121                                            0123
*     ELSE                                                         0124
*       CALL UPDTERTN;              /* UPDATE THE EFFECTIVE RTCT   0124
*                                      OPTIONS FOR THE DUMP TYPE   0124
*                                      SPECIFIED (DT)        @G33CPHE*/
         B     @RC00120                                            0124
@RF00120 BAL   @14,UPDTERTN                                        0124
*   XSAPTR=XSASAVE;                 /* RESTORE PTR TO XSA            */
@RC00120 DS    0H                                                  0125
@RF00119 L     XSAPTR,XSASAVE                                      0125
*   XAE=MSGID;                      /* PUT MSG INDEX INTO XSA      0126
*                                                            @G33CPHE*/
         MVC   XAE(1,XSAPTR),MSGID                                 0126
*   IF MSGID^=IEE712&MSGID^=IEE305  /* IS THE MSG ONE THAT NEEDS THE
*                                      ERROR TEXT INSERTED   @G33CPHE*/
*       &START^=0                   /* AND IS THERE TEXT TO BE     0127
*                                      INSERTED INTO THE MSG TEXT  0127
*                                                            @G33CPHE*/
*     THEN                          /* YES                   @G33CPHE*/
         CLI   MSGID,X'03'                                         0127
         BE    @RF00127                                            0127
         CLI   MSGID,X'05'                                         0127
         BE    @RF00127                                            0127
         LTR   START,START                                         0127
         BZ    @RF00127                                            0127
*     DO;                           /*                       @G33CPHE*/
*       IF LEN<LENGTH(XAV)          /* IS THE LENGTH OF THE ERROR  0129
*                                      PARM TEXT LESS THAN THE     0129
*                                      VARIABLE SECTION OF THE MSG 0129
*                                                            @G33CPHE*/
*         THEN                      /* YES IT IS             @G33CPHE*/
         CH    LEN,@CH00063                                        0129
         BNL   @RF00129                                            0129
*         LEN=LEN+1;                /* SO INCREASE IT BY ONE TO    0130
*                                      INCLUDE THE DELIMITER THAT  0130
*                                      FOLLOWS IT            @G33CPHE*/
         AH    LEN,@CH00065                                        0130
*       ELSE                        /* OTHERWISE IF IT IS TOO LONG 0131
*                                                            @G33CPHE*/
*         LEN=LENGTH(XAV);          /* TRUNCATE IT           @G33CPHE*/
         B     @RC00129                                            0131
@RF00129 LA    LEN,8                                               0131
*       XAV='        ';             /* INIT THE VARIABLE AREA TO   0132
*                                      BLANKS                @G33CPHE*/
@RC00129 MVC   XAV(8,XSAPTR),@CC02081                              0132
*       XAV(1:LEN)=ERRTXT(1:LEN);   /* AND MOVE IN THE OFFENDING PARM
*                                                            @G33CPHE*/
         LR    @15,LEN                                             0133
         BCTR  @15,0                                               0133
         L     @05,ERRTXTP                                         0133
         EX    @15,@SM02259                                        0133
*     END;                          /* OR ERROR TEXT INSERTION     0134
*                                                            @G33CPHE*/
*   CALL IEE2103D;                  /* ISSUE MESSAGE TO INDICATE   0135
*                                      SUCCESS OR FAILURE OF COMMAND */
@RF00127 L     @15,@CV02052                                        0135
         BALR  @14,@15                                             0135
*   R14=R14SVC34;                   /* RESTORE RETURN ADDRESS OF   0136
*                                      CHNGDUMP CALLER       @G33CPHE*/
         L     R14,R14SVC34                                        0136
*   RETURN;                                                        0137
@EL00001 L     @13,4(,@13)                                         0137
@EF00001 L     @00,@SIZDATD                                        0137
         LR    @01,@08                                             0137
         FREEMAIN R,LV=(0),A=(1)
@ER00001 BR    @14                                                 0137
*   RESPECIFY                                                      0138
*    (OPSETPTR,                                                    0138
