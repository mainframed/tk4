*                 THEN              /* YES                   @G33CPHE*/
         TM    OVERMODE,B'00000010'                                0242
         BNZ   @RF00242                                            0242
*                 ADDMODE=ON;       /* SET FLAG FOR CHDSAQ SEGMENT 0243
*                                      WHICH WILL SET BITS IN THE  0243
*                                      UPDATE AREA           @G33CPHE*/
         OI    ADDMODE,B'00000100'                                 0243
*               ELSE                                               0244
*                 MSGID=IEE312;     /* INDICATE THE ERROR    @G33CPHE*/
         B     @RC00242                                            0244
@RF00242 MVI   MSGID,X'0C'                                         0244
*             ELSE                  /* NOT ADD               @G33CPHE*/
*               IF LEN=4&OPERAND(1:4)='OVER'/* IS IT OVER?   @G33CPHE*/
*                 THEN                                             0245
         B     @RC00241                                            0245
@RF00241 CH    LEN,@CH00057                                        0245
         BNE   @RF00245                                            0245
         CLC   OPERAND(4,START),@CC02106                           0245
         BNE   @RF00245                                            0245
*                 IF ADDMODE=OFF    /* CHECK FOR A CONFLICTING VALUE
*                                      ALREADY SPECIFIED     @G33CPHE*/
*                   THEN            /* YES IT IS             @G33CPHE*/
         TM    ADDMODE,B'00000100'                                 0246
         BNZ   @RF00246                                            0246
*                   OVERMODE=ON;    /* SET FLAG FOR CHDSAQ SEGMENT 0247
*                                      WHICH WILL SET BITS IN THE  0247
*                                      UPDATE AREA           @G33CPHE*/
         OI    OVERMODE,B'00000010'                                0247
*                 ELSE                                             0248
*                   MSGID=IEE312;   /* INDICATE THE ERROR    @G33CPHE*/
         B     @RC00246                                            0248
@RF00246 MVI   MSGID,X'0C'                                         0248
*               ELSE                /* THERE ARE NO MORE VALID     0249
*                                      POSSIBILITIES         @G33CPHE*/
*                 MSGID=IEE309;     /* INDICATE THE ERROR    @G33CPHE*/
         B     @RC00245                                            0249
@RF00245 MVI   MSGID,X'09'                                         0249
*     END;                                                         0250
*   ELSE                                                           0251
*     IF PKEY2='1'B                 /* IS THE SECOND POSITION KEYWORD
*                                      EXPECTED ??                   */
*       THEN                        /* THEN THIS MUST BE IT.         */
         B     @RC00212                                            0251
@RF00212 TM    PKEY2,B'01000000'                                   0251
         BNO   @RF00251                                            0251
*       DO;                                                        0252
*         IF CDRESET=ON             /* WAS RESET SPECIFIED?  @G33CPHE*/
*           THEN                    /* YES IT WAS            @G33CPHE*/
         TM    CDRESET,B'01000000'                                 0253
         BNO   @RF00253                                            0253
*           DO;                     /*                       @G33CPHE*/
*             MSGID=IEE535;         /* INDICATE THE ERROR. RESET MAY
*                                      NOT HAVE ANY 3RD POS PARMS  0255
*                                      HENCE THE 2ND POS PARM MAY NOT
*                                      BE FOLLOWED BY A COMMA      0255
*                                                            @G33CPHE*/
         MVI   MSGID,X'3E'                                         0255
*             ERRTXTP=START+LEN;    /* CHANGE THE POINTER TO THE   0256
*                                      ERROR TEXT TO INCLUDE THE   0256
*                                      COMMA THAT FOLLOWS THE PORTION
*                                      JUST SCANNED AND WHATEVER   0256
*                                      KEYWORD FOLLOWS. THE REASON 0256
*                                      FOR THIS IS THAT WE KNOW IT IS
*                                      WRONG BUT WE HAVEN'T CHECKED
*                                      THE PART JUST SCANNED, SO IT
*                                      MAY BE RIGHT.         @G33CPHE*/
         LR    @15,START                                           0256
         ALR   @15,LEN                                             0256
         ST    @15,ERRTXTP                                         0256
*             LEN=LENGTH(XAV);      /* ALSO CHANGE THE LEN TO THE MAX
*                                      SINCE WHAT FOLLOWS IS UNKNOWN
*                                                            @G33CPHE*/
         LA    LEN,8                                               0257
*           END;                                                   0258
*         ELSE                                                     0259
*           DO;                     /* RESET NOT SPECIFIED   @G33CPHE*/
         B     @RC00253                                            0259
@RF00253 DS    0H                                                  0260
*             PKEY2='0'B;           /* NO LONGER EXPECTING THE SECOND
*                                      POSITION KEYWORD              */
*             KEY3='1'B;            /* NOW EXPECTING 3RD POSITION  0261
