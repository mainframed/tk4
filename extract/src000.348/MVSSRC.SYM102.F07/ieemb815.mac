         CH    LEN,@CH00770                                        0280
         BNE   @RF00280                                            0280
         CLC   OPERAND(5,START),@CC02109                           0280
         BNE   @RF00280                                            0280
*             CDRESET=ON;           /*                       @G33CPHE*/
         OI    CDRESET,B'01000000'                                 0281
*           ELSE                    /*                       @G33CPHE*/
*             IF LEN=3&OPERAND(1:3)='SET' THEN                     0282
         B     @RC00280                                            0282
@RF00280 CH    LEN,@CH00290                                        0282
         BNE   @RF00282                                            0282
         CLC   OPERAND(3,START),@CC02110                           0282
         BNE   @RF00282                                            0282
