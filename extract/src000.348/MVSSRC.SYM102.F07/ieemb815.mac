CHDCOMMA DS    0H                                                  0212
*/*    *****  THE COMMA SUBROUTINE  ****                             */
*/*    IF OPTIONS ARE EXPECTED, THIS ROUTINE CALLS THE OPTION        */
*/*    HANDLER FOR SDATA OR PDATA OR SDUMP.                          */
*/*    IF THE SECOND POSITION KEYWORD IS EXPECTED, THEN A CHECK IS   */
*/*    MADE FOR SYSABEND, SYSUDUMP, OR SDUMP, AND THE PROPER         */
*/*    INDICATORS ARE SET ACCORDINGLY.                               */
*/*                                                                  */
*/*    IF THE FIRST POSITION KEYWORD IS EXPECTED, THEN A CHECK IS    */
*/*    MADE FOR SET OR DEL.                                          */
*/*                                                                  */
*/*    FINALLY IF NONE OF THE ABOVE CONDITIONS ARE FOUND, A CHECK IS */
*/*    MADE TO SEE IF THIS IS A COMMA FOLLOWING THE RIGHT PARAN. IF  */
*/*    THIS IS THE CASE,WE DO NOT HAVE A KEYWORD OR SUBPARAMETER     */
*/*    ISOLATED, GO TO CONTINUE THE SCAN.                            */
*/*                                                                  */
*/*    A MESSAGE INDEX FOR AN APPROPRIATE ERROR MESSAGE IS SET       */
*/*    WHEN AN UNEXPECTED CONDITION IS FOUND.                        */
*/*                                                                  */
*                                                                  0212
*   IF KEY3=ON                      /* CHECK TO SEE IF 3RD POSITION
*                                      KEYWORDS ARE IN PROCESS       */
*     THEN                                                         0212
         TM    KEY3,B'00100000'                                    0212
         BNO   @RF00212                                            0212
*     DO;                           /*                       @G33CPHE*/
*       KEY31=OFF;                  /* YES. THIS MEANS THAT MORE THAN
*                                      ONE 3RD POS IS BEING        0214
*                                      SPECIFIED, HENCE THE EXCUSIVE
*                                      ONES CAN NO LONGER POSSIBLY BE
*                                      VALID                 @G33CPHE*/
         NI    KEY31,B'11101111'                                   0214
*       IF OPTIONX='1'B             /* EXPECTING OPTIONS ??          */
*         THEN                                                     0215
         TM    OPTIONX,B'00000100'                                 0215
         BNO   @RF00215                                            0215
*         DO;                                                      0216
*           R14SAVE=R14;            /* SAVE RETURN ADDRESS           */
         ST    R14,R14SAVE                                         0217
*           CALL OPSET;             /* GO TO OPTION HANDLER          */
         LR    @15,OPSETPTR                                        0218
         BALR  @14,@15                                             0218
*           R14=R14SAVE;            /* RESTORE R14                   */
         L     R14,R14SAVE                                         0219
*           IF LPARAN^='1'B         /* IF A LEFT PARAN HAS NOT BEEN
*                                      ENCOUNTERED, THEN ALLOW ONLY
*                                      ONE                           */
*             THEN                                                 0220
         TM    LPARAN,B'00001000'                                  0220
         BO    @RF00220                                            0220
*             OPTIONX='0'B;         /* NO MORE DUMP OPTIONS ARE    0221
*                                      EXPECTED FOR THIS KEYWORD   0221
*                                      BECAUSE PARENTHESIS WERE NOT
*                                      USED                          */
         NI    OPTIONX,B'11111011'                                 0221
*         END;                                                     0222
*       ELSE                        /*                       @G33CPHE*/
*         IF STOPSAVE=RPARNCDE&LEN=0/* IF THIS COMMA FOLLOWS A RIGHT
*                                      PAREN WITH NO KEYWORD @G33CPHE*/
*           THEN                                                   0223
         B     @RC00215                                            0223
@RF00215 CLI   STOPSAVE,X'05'                                      0223
         BNE   @RF00223                                            0223
         LTR   LEN,LEN                                             0223
         BZ    @RT00223                                            0223
*           ;                       /* THERE IS NOTHING TO PROCESS 0224
*                                      YET                   @G33CPHE*/
*         ELSE                      /* CHECK FOR VALID 3RD POSITION
*                                      NONEXCLUSIVE KEYWORDS @G33CPHE*/
*           IF YESNO=ON             /* IS THE YES OR NO OPTION FOR Q
*                                      EXPECTED?             @G33CPHE*/
*             THEN                  /* YES IT IS             @G33CPHE*/
@RF00223 TM    YESNO,B'00001000'                                   0225
         BNO   @RF00225                                            0225
*             IF LEN=2&OPERAND(1:2)='NO'/* IS IT NO          @G33CPHE*/
*               THEN                                               0226
         CH    LEN,@CH00077                                        0226
         BNE   @RF00226                                            0226
         CLC   OPERAND(2,START),@CC02103                           0226
         BNE   @RF00226                                            0226
*               IF ADDR(CHNGWORK)->RTCTSDPG=OFF/* CHECK FOR A      0227
*                                      CONFLICTING VALUE ALREADY   0227
*                                      SPECIFIED             @G33CPHE*/
*                 THEN              /* Q=NO SPECIFIED        @G33CPHE*/
         LA    @15,CHNGWORK                                        0227
         TM    RTCTSDPG(@15),B'10000000'                           0227
         BNZ   @RF00227                                            0227
*                 DO;                                              0228
*                   YESNO=OFF;      /* SHOW THAT THE YESNO OPTION IS
*                                      NO LONGER BEING EXPECTED      */
         NI    YESNO,B'11110111'                                   0229
*                   ADDR(CHNGWORK)->RTCTSDPH=ON;/* TURN ON THE OPTION
*                                      IN THE WORK AREA      @G33CPHE*/
         OI    RTCTSDPH(@15),B'01000000'                           0230
*                 END;                                             0231
*               ELSE                                               0232
*                 MSGID=IEE312;     /* INDICATE THE ERROR    @G33CPHE*/
         B     @RC00227                                            0232
@RF00227 MVI   MSGID,X'0C'                                         0232
*             ELSE                                                 0233
*               IF LEN=3&OPERAND(1:3)='YES'/* IS IT Q=YES    @G33CPHE*/
*                 THEN                                             0233
         B     @RC00226                                            0233
@RF00226 CH    LEN,@CH00290                                        0233
         BNE   @RF00233                                            0233
         CLC   OPERAND(3,START),@CC02104                           0233
         BNE   @RF00233                                            0233
*                 IF ADDR(CHNGWORK)->RTCTSDPH=OFF/* CHECK FOR A    0234
*                                      CONFLICTING VALUE ALREADY   0234
*                                      SPECIFIED             @G33CPHE*/
*                   THEN            /* YES                   @G33CPHE*/
         LA    @15,CHNGWORK                                        0234
         TM    RTCTSDPH(@15),B'01000000'                           0234
         BNZ   @RF00234                                            0234
*                   DO;                                            0235
*                     YESNO=OFF;    /* SHOW THAT THE YESNO OPTION IS
*                                      NO LONGER BEING EXPECTED      */
         NI    YESNO,B'11110111'                                   0236
*                     ADDR(CHNGWORK)->RTCTSDPG=ON;/* TURN ON THE   0237
*                                      OPTION IN THE WORK AREA     0237
*                                                            @G33CPHE*/
         OI    RTCTSDPG(@15),B'10000000'                           0237
*                   END;                                           0238
*                 ELSE                                             0239
*                   MSGID=IEE312;   /* INDICATE THE ERROR    @G33CPHE*/
         B     @RC00234                                            0239
@RF00234 MVI   MSGID,X'0C'                                         0239
*               ELSE                /* NOT A VALID VALUE     @G33CPHE*/
*                 MSGID=IEE708;     /* INDICATE THE ERROR    @G33CPHE*/
         B     @RC00233                                            0240
@RF00233 MVI   MSGID,X'3D'                                         0240
*           ELSE                    /* YES NO NOT EXPECTED   @G33CPHE*/
*             IF LEN=3&OPERAND(1:3)='ADD'/* IS IT ADD?       @G33CPHE*/
*               THEN                                               0241
         B     @RC00225                                            0241
@RF00225 CH    LEN,@CH00290                                        0241
         BNE   @RF00241                                            0241
         CLC   OPERAND(3,START),@CC02105                           0241
         BNE   @RF00241                                            0241
*               IF OVERMODE=OFF     /* CHECK FOR A CONFLICTING VALUE
