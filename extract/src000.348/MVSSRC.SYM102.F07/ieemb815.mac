*     ENP) RESTRICTED;                                             0138
*CHDLPARN:                                                         0139
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0139
*                                                                  0139
CHDLPARN DS    0H                                                  0140
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS PROC HANDLES PROCESSING OF THE LEFT PARANTHESIS DELIMITER*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0140
*   LPARAN='1'B;                    /* INDICATE LEFT PARAN         0140
*                                      ENCOUNTERED                   */
*                                                                  0140
         OI    LPARAN,B'00001000'                                  0140
*   /*****************************************************************/
*   /*                                                               */
*   /* RESET TRANSLATE TABLE FOR NEXT SCAN                           */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0141
*   TRANTBL(')'+1)=RPARNCDE;        /* STOP ON RIGHT PARAN   @G33CPHE*/
         MVI   TRANTBL+93,X'05'                                    0141
*   TRANTBL(','+1)=COMMACDE;        /* STOP ON COMMA         @G33CPHE*/
         MVI   TRANTBL+107,X'02'                                   0142
*   TRANTBL('='+1)=UNEXPCDE;        /* EQUAL SIGN UNEXPECTED @G33CPHE*/
         MVI   TRANTBL+126,X'06'                                   0143
*   TRANTBL(' '+1)=UNEXPCDE;        /* BLANK IS UNEXPECTED   @G33CPHE*/
         MVI   TRANTBL+64,X'06'                                    0144
*   TRANTBL('('+1)=UNEXPCDE;        /* LEFT PARAN IS UNEXPECTED    0145
*                                                            @G33CPHE*/
         MVI   TRANTBL+77,X'06'                                    0145
*   RETURN;                                                        0146
@EL00002 DS    0H                                                  0146
@EF00002 DS    0H                                                  0146
@ER00002 BR    @14                                                 0146
*   END CHDLPARN;                                                  0147
*CHDRPARN:                                                         0148
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0148
*                                                                  0148
CHDRPARN DS    0H                                                  0149
*   /*****************************************************************/
*   /*                                                               */
*   /* HANDLES PROCESSING OF RIGHT PARANTHESIS. VALIDITY CHECKS FOR  */
*   /* PREVIOUS LEFT PARANTHESIS AND OPTIONS EXPECTED. CALLS OPTION  */
*   /* HANDLER TO PROCESS THE OPTION                                 */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0149
*   R14SAVE=R14;                    /* SAVE RETURN ADDRESS TO      0149
*                                      MAINLINE PROCESSING BEFORE  0149
*                                      CALLING OPTION HANDLING     0149
*                                      SUBROUTINE                    */
         ST    R14,R14SAVE                                         0149
*   CALL OPSET;                     /* GO TO OPTION HANDLER          */
         LR    @15,OPSETPTR                                        0150
         BALR  @14,@15                                             0150
*   R14=R14SAVE;                    /* RESTORE RETURN ADDRESS TO   0151
*                                      MAINLINE PROCESSING           */
         L     R14,R14SAVE                                         0151
*   LPARAN='0'B;                    /* REINITIALIZE LEFT PARANTHESIS
*                                      INDICATOR SINCE WE HAVE     0152
*                                      ENCOUNTERED THE MATCHING RIGHT
*                                      PARAN.                        */
*   OPTIONX='0'B;                   /* NO MORE OPTIONS EXPECTED      */
*                                                                  0153
         NI    LPARAN,B'11110011'                                  0153
*   /*****************************************************************/
*   /*                                                               */
*   /* RIGHT PARAN PROCESSOR - RESET TRANSLATE TABLE FOR NEXT SCAN   */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0154
*   TRANTBL(','+1)=COMMACDE;        /* STOP ON COMMA         @G33CPHE*/
         MVI   TRANTBL+107,X'02'                                   0154
*   TRANTBL(' '+1)=EOCMDCDE;        /* STOP ON BLANK         @G33CPHE*/
         MVI   TRANTBL+64,X'03'                                    0155
*   TRANTBL('='+1)=UNEXPCDE;        /* EQUAL SIGN IS UNEXPECTED    0156
*                                                            @G33CPHE*/
         MVI   TRANTBL+126,X'06'                                   0156
*   TRANTBL('('+1)=UNEXPCDE;        /* LEFT PARAN IS UNEXPECTED    0157
*                                                            @G33CPHE*/
         MVI   TRANTBL+77,X'06'                                    0157
*   TRANTBL(')'+1)=UNEXPCDE;        /* RIGHT PARAN IS UNEXPECTED   0158
*                                                            @G33CPHE*/
         MVI   TRANTBL+93,X'06'                                    0158
*   RETURN;                                                        0159
@EL00003 DS    0H                                                  0159
@EF00003 DS    0H                                                  0159
@ER00003 BR    @14                                                 0159
*   END CHDRPARN;                                                  0160
*CHDEQUAL:                                                         0161
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0161
*                                                                  0161
CHDEQUAL DS    0H                                                  0162
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS PROCESSES THE KEYWORDS WHICH ARE FOLLOWED BY THE EQUAL   */
*   /* SIGN DELIMITER. POSSIBLE KEYWORDS IN THIS POSITION ARE        */
*   /* PDATA,SDATA,Q OR SDUMP. THE OPTIONS EXPECTED FLAG IS TURNED ON*/
*   /* AND THE KEYWORDS ARE SYNTAX CHECKED. IF AN ERROR IS DETECTED, */
*   /* IT WILL BE FLAGGED AS UNIDENTIFIABLE KEYWORD.                 */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0162
*   IF OPTIONX='0'B THEN                                           0162
         TM    OPTIONX,B'00000100'                                 0162
         BNZ   @RF00162                                            0162
*     IF KEY3='1'B                  /* EXPECTING 3RD POSITION      0163
*                                      KEYWORD?                      */
*       THEN                                                       0163
         TM    KEY3,B'00100000'                                    0163
         BNO   @RF00163                                            0163
*       DO;                         /* YES- MUST BE PDATA,SDATA OR Q
*                                                             @G33CPH*/
*         KEY31=OFF;                /* AN EXCLUSIVE 3RD POS KEYWORD
*                                      IS NO LONGER POSSIBLE @G33CPHE*/
         NI    KEY31,B'11101111'                                   0165
*         IF DT=SADINDEX×DT=SUDINDEX/* SYSABEND OR SYSUDUMP SPECIFIED
*                                                            @G33CPHE*/
*             &LEN=5                /*                       @G33CPHE*/
*           THEN                    /* YES                           */
         CH    DT,@CH00065                                         0166
         BE    @RT00166                                            0166
         CH    DT,@CH00077                                         0166
         BNE   @RF00166                                            0166
         CH    LEN,@CH00770                                        0166
         BNE   @RF00166                                            0166
@RT00166 DS    0H                                                  0167
*           DO;                     /*                       @G33CPHE*/
*             OPTIONX=ON;           /*                       @G33CPHE*/
         OI    OPTIONX,B'00000100'                                 0168
*             IF OPERAND(1:5)='PDATA'/*                      @G33CPHE*/
*               THEN                                               0169
         CLC   OPERAND(5,START),@CC02091                           0169
         BNE   @RF00169                                            0169
*               DO;                                                0170
*                 PDATAKEY='1'B;                                   0171
         OI    PDATAKEY,B'00010000'                                0171
*                 OPSETPTR=ADDR(CHDPDATA);/* SET PTR TO OPTION     0172
*                                      HANDLER FOR PDATA OPTIONS     */
         LA    OPSETPTR,CHDPDATA                                   0172
*               END;                                               0173
*             ELSE                                                 0174
*               IF OPERAND(1:5)='SDATA'/* SDATA KEYWORD ?    @G33CPHE*/
*                 THEN              /* YES                           */
         B     @RC00169                                            0174
@RF00169 CLC   OPERAND(5,START),@CC02093                           0174
         BNE   @RF00174                                            0174
*                 DO;                                              0175
*                   SDATAKEY='1'B;  /* INDICATE SDATA SPECIFIED      */
         OI    SDATAKEY,B'00100000'                                0176
*                   OPSETPTR=ADDR(CHDSDATA);/* SET PTR TO OPTION   0177
*                                      HANDLER FOR SDATA OPTIONS     */
         LA    OPSETPTR,CHDSDATA                                   0177
*                 END;                                             0178
*               ELSE                                               0179
*                 MSGID=IEE309;     /* KEYWORD NOT VALID FOR 3ND   0179
*                                      POSITIONAL FOLLOWED BY AN = 0179
*                                                            @G33CPHE*/
         B     @RC00174                                            0179
@RF00174 MVI   MSGID,X'09'                                         0179
*           END;                    /*                       @G33CPHE*/
*         ELSE                                                     0181
*           IF DT=SVDINDEX&LEN=1&OPERAND(1)='Q'/* Q= KEYWORD @G33CPHE*/
*             THEN                                                 0181
         B     @RC00166                                            0181
@RF00166 CH    DT,@CH00057                                         0181
         BNE   @RF00181                                            0181
         CH    LEN,@CH00065                                        0181
         BNE   @RF00181                                            0181
         CLI   OPERAND(START),C'Q'                                 0181
         BNE   @RF00181                                            0181
*             YESNO='1'B;           /* EXPECTING 'YES' OR'NO'        */
         OI    YESNO,B'00001000'                                   0182
*           ELSE                                                   0183
*             MSGID=IEE309;         /* KEYWORD NOT VALID FOR 3ND   0183
*                                      POSITIONAL FOLLOWED BY AN = 0183
*                                                            @G33CPHE*/
         B     @RC00181                                            0183
@RF00181 MVI   MSGID,X'09'                                         0183
*       END;                        /*                       @G33CPHE*/
*     ELSE                                                         0185
*       IF PKEY2='1'B&CDRESET=OFF   /* EXPECTING 2ND POS. PARM     0185
*                                                            @G33CPHE*/
*         THEN                      /* MUST BE SDUMP OR SYSMDUMP   0185
*                                                            @G33CPHE*/
         B     @RC00163                                            0185
@RF00163 TM    PKEY2,B'01000000'                                   0185
         BNO   @RF00185                                            0185
         TM    CDRESET,B'01000000'                                 0185
         BNZ   @RF00185                                            0185
*         DO;                       /*                       @G33CPHE*/
*           PKEY2='0'B;             /* NO LONGER EXPECTING PKEY2     */
*           KEY3='1'B;              /* INDICATE -NOW EXPECTING     0188
*                                      3RD-4TH POSITION PARAMETERS   */
*           OPTIONX='1'B;           /* OPTIONS NOW EXPECTED          */
         OI    KEY3,B'00100100'                                    0189
         NI    PKEY2,B'10111111'                                   0189
*           IF LEN=5&OPERAND(1:5)='SDUMP' THEN                     0190
         CH    LEN,@CH00770                                        0190
         BNE   @RF00190                                            0190
         CLC   OPERAND(5,START),@CC02096                           0190
         BNE   @RF00190                                            0190
*             DO;                                                  0191
*               OPSETPTR=ADDR(CHDSDUMP);/* SET PTR TO OPTION HANDLER
*                                      FOR SDUMP OPTIONS             */
         LA    OPSETPTR,CHDSDUMP                                   0192
*               DT=SVDINDEX;        /* SET DUMP TYPE         @G33CPHE*/
         LA    DT,4                                                0193
*             END;                                                 0194
*           ELSE                                                   0195
*             IF LEN=8&OPERAND(1:8)='SYSMDUMP'/*             @G33CPHE*/
*               THEN                /*                       @G33CPHE*/
         B     @RC00190                                            0195
@RF00190 CH    LEN,@CH00063                                        0195
