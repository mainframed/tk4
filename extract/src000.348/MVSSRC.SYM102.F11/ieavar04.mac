         TITLE 'IEAVAR04 - ATTENTION EXIT SCHEDULER ROUTINE            *
                        '
IEAVAR04 CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'IEAVAR04  76.030'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @08,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@08                                         0001
RBPRFXLN EQU 64
RBPRFLNA EQU 32
PRBLEN   EQU 136
SIRBLEN  EQU 200
TIRBLEN  EQU 136
IRBLEN   EQU 128
SVRBLEN  EQU 200
DECREG   DS    0H                                                  0051
*                                                                  0097
*/********************************************************************/
*/*                                                                  */
*/*  START  OF EXECUTABLE CODE                                       */
*/*                                                                  */
*/********************************************************************/
*                                                                  0097
*   RCTDRCTR='08000000'X;           /* SET MODULE FLAG(RCTDATTN)   0097
*                                      WHILE RESETTING ALL OTHER   0097
*                                      FLAGS IN THE RCTDRCTR         */
         L     @15,ASXBRCTD(,ASXBPTR)                              0097
         MVC   RCTDRCTR(4,@15),@CB02427                            0097
*/********************************************************************/
*/*                                                                  */
*/*  THE FOLLOWING SEGMENT WILL OBTAIN THE LOCAL LOCK                */
*/*  UNCONDITIONALLY. IF A NON-ZERO RETURN CODE IS RETURNED          */
*/*  AN ABEND WILL BE ISSUED. THE ABEND WILL BE PROCESSED            */
*/*  BY RCT'S ESTAE ROUTINE.                                         */
*/*                                                                  */
*/********************************************************************/
*                                                                  0098
*   RESPECIFY                                                      0098
*     SETLK11 RESTRICTED;           /* REGISTERS 11,12,13            */
*   RESPECIFY                                                      0099
*     SETLK12 RESTRICTED;           /* AND 14 ARE USED BY            */
*   RESPECIFY                                                      0100
*     SETLK13 RESTRICTED;           /* THE SETLOCK MACRO.            */
*   RESPECIFY                                                      0101
*     SETLK14 RESTRICTED;           /* SETLOCK DOES NOT SAVE THE   0101
*                                      REGISTERS                     */
*SETLKO1:                                                          0102
*   GENERATE CODE NOSETS NOFLOWS NODEFS REFS(PSALITA);             0102
SETLKO1  DS    0H                                                  0102
        SETLOCK  OBTAIN,TYPE=LOCAL,MODE=UNCOND,                        X
               RELATED=(TAXE-TCB-QUEUE,IEAVAR04(SETLKR1))
*   IF SETLK13^=ZERO THEN           /* WAS SETLOCK SUCCESSFUL        */
         LTR   SETLK13,SETLK13                                     0103
         BZ    @RF00103                                            0103
*     DO;                                                          0104
*R04ABEND:                                                         0105
*       RCTDRC=SETLK13;             /* SAVE THE RETURN CODE FROM   0105
*                                      SETLOCK FOR LOGREC            */
R04ABEND L     @15,ASXBRCTD(,ASXBPTR)                              0105
         ST    SETLK13,RCTDRC(,@15)                                0105
*       GENERATE CODE NOFLOWS NOREFS NODEFS NOSETS;                0106
                 ABEND  X'079',,,SYSTEM
*     END;                                                         0107
*   RESPECIFY                                                      0108
*     SETLK11 UNRESTRICTED;                                        0108
@RF00103 DS    0H                                                  0109
*   RESPECIFY                                                      0109
*     SETLK12 UNRESTRICTED;                                        0109
*   RESPECIFY                                                      0110
*     SETLK13 UNRESTRICTED;                                        0110
*   RESPECIFY                                                      0111
*     SETLK14 UNRESTRICTED;                                        0111
*                                                                  0112
*/********************************************************************/
*/*                                                                  */
*/*  THE FOLLOWING SEGMENT WILL ESTABLISH A RECOVERY                 */
*/*  ENVIRONMENT FOR THE LOCKED CODE OF THIS ROUTINE                 */
*/*                                                                  */
*/********************************************************************/
*                                                                  0112
*   RESPECIFY                                                      0112
*     FRRSPTR RESTRICTED;                                          0112
*   RESPECIFY                                                      0113
*     FRREPTR RESTRICTED;                                          0113
*   FRRSPTR=ADDR(IEAVAFR4);         /* ENTRY POINT TO FRR            */
         LA    FRRSPTR,IEAVAFR4                                    0114
*   GENERATE CODE NODEFS NOSETS NOFLOWS REFS(FRRWKAP,FRRSPTR,FRREPTR,
*       PSACSTK,FRRSCURR,FRRSLAST,FRRSELEN,FRRSFRRA,FRRSPARM,PSALSFCC);
        SETFRR  A,FRRAD=(FRRSPTR),PARMAD=(FRRWKAP),                    X
               WRKREGS=(FRRWKAP,FRREPTR)
*   RESPECIFY                                                      0116
*     FRRSPTR UNRESTRICTED;                                        0116
*   RESPECIFY                                                      0117
*     FRREPTR UNRESTRICTED;                                        0117
*   RCTDAFPE='1'B;                  /* SET FOOTPRINT INDICATING THAT
*                                      THE FRR HAS BEEN ESTABLISHED  */
         L     @15,ASXBRCTD(,ASXBPTR)                              0118
         OI    RCTDAFPE(@15),B'10000000'                           0118
*                                                                  0119
*/********************************************************************/
*/*                                                                  */
*/* THE FOLLOWING SEGMENT WILL SEARCH FOR ANY PREVIOUSLY             */
*/* SCHEDULED ATTENTION EXITS THAT HAVE NOT YET ENTERED THE          */
*/* ATTENTION EXIT. IF ANY ARE FOUND THE ATTENTION EXIT              */
*/* WILL BE CANCELLED.                                               */
*/*                                                                  */
*/********************************************************************/
*                                                                  0119
*   RESPECIFY                                                      0119
*     TSBPTR RESTRICTED;                                           0119
*   RESPECIFY                                                      0120
*     PTAXETBS RESTRICTED;                                         0120
*   RESPECIFY                                                      0121
*     TAXEPTR RESTRICTED;                                          0121
*   RESPECIFY                                                      0122
*     SWAPVAL RESTRICTED;                                          0122
*   RESPECIFY                                                      0123
*     ATTNCNT RESTRICTED;                                          0123
*   RESPECIFY                                                      0124
*     STAXCNT RESTRICTED;                                          0124
*   TSBPTR=ASCBTSB;                 /* LOAD BASE REG OF TSB          */
         L     TSBPTR,ASCBTSB(,ASCBPTR)                            0125
*   GENERATE CODE NOSETS NOFLOWS NODEFS REFS(TSBPTR);              0126
        USING  TSB,TSBPTR
*   PTAXETBS=PTAXETBS-PTAXETBS;     /* INITIALIZE PTR OF TAXE TO BE
*                                      SCHED                         */
         SLR   PTAXETBS,PTAXETBS                                   0127
*   SWAPVAL=TSBATTNC;               /* VALUE IN TSB AT START OF    0128
*                                      COMPARE AND SWAP LOOP         */
         L     SWAPVAL,TSBATTNC                                    0128
*   STAXCNT=SWAPVAL&'00FF0000'X;    /* AVAILABLE TAXE CNT            */
         LR    STAXCNT,SWAPVAL                                     0129
         N     STAXCNT,@CF02435                                    0129
*   ATTNCNT=SWAPVAL&'FF000000'X;    /* REQUESTED ATTN LEVEL          */
         LR    ATTNCNT,SWAPVAL                                     0130
         N     ATTNCNT,@CF02436                                    0130
*   IF RCTDCLST>OFF THEN                                           0131
         L     @15,ASXBRCTD(,ASXBPTR)                              0131
         CLC   RCTDCLST(2,@15),@CH00036                            0131
         BNH   @RF00131                                            0131
*     DO;                           /* IF > 0 THEN SET ATTNCNT=    0132
*                                      AVAILABLE TAXE COUNT FOR THE
*                                      TMP EXIT              @ZA08100*/
*       ATTNCNT=STAXCNT;            /* WILL FORCE THE TMP ATTN EXIT
*                                                            @ZA08100*/
         LR    ATTNCNT,STAXCNT                                     0133
*       SLL(ATTNCNT,8);             /* SHIFT COUNT TO CORRECT BYTE 0134
*                                                            @ZA08100*/
         SLL   ATTNCNT,8                                           0134
*     END;                          /*                       @ZA08100*/
*   TAXEPTR=RCTDTAXE;               /* INITIALIZE TAXE SEARCH      0136
*                                      ARGUMENT                      */
@RF00131 L     @15,ASXBRCTD(,ASXBPTR)                              0136
         L     TAXEPTR,RCTDTAXE(,@15)                              0136
*LOOP:                                                             0137
*   DO WHILE TAXEPTR^=ZERO;         /* ANY MORE TAXES?               */
LOOP     B     @DE00137                                            0137
@DL00137 DS    0H                                                  0138
*     IF ATTNCNT=ZERO THEN          /* HIGHER LEVEL REQ?             */
         LTR   ATTNCNT,ATTNCNT                                     0138
         BNZ   @RF00138                                            0138
*       DO;                         /* NO                            */
*         GO TO UPDCNT;             /* UPDATE THE COUNTS             */
         B     UPDCNT                                              0140
*       END;                                                       0141
*     IF TAXEFREQ='0'B THEN         /* IS TAXE AVAILABLE             */
@RF00138 TM    TAXEFREQ(TAXEPTR),B'00100000'                       0142
         BNZ   @RF00142                                            0142
*       DO;                                                        0143
*         TAXEFREQ='1'B;            /* MARK TAXE UNAVAIL             */
         OI    TAXEFREQ(TAXEPTR),B'00100000'                       0144
*         ATTNCNT=ATTNCNT-'01000000'X;/* DECREMENT THE ATTENTION CNT */
         SL    ATTNCNT,@CF02442                                    0145
*         STAXCNT=STAXCNT-'00010000'X;/* DECREMENT THE AVAIL TAXE CNT*/
         SL    STAXCNT,@CF02443                                    0146
*         PTAXETBS=TAXEPTR;         /* SAVE TAXE ADDRESS             */
         LR    PTAXETBS,TAXEPTR                                    0147
*       END;                                                       0148
*     ELSE                          /* NO                            */
*       IF TAXELNK=ZERO THEN        /* LAST TAXE                     */
         B     @RC00142                                            0149
@RF00142 L     @15,TAXELNK(,TAXEPTR)                               0149
         LTR   @15,@15                                             0149
         BNZ   @RF00149                                            0149
*         DO;                                                      0150
*           PTAXETBS=PTAXETBS-PTAXETBS;/* NO TAXE TO BE SCHED.       */
         SLR   PTAXETBS,PTAXETBS                                   0151
*           ATTNCNT=ATTNCNT-ATTNCNT;/* CLEAR ATTN COUNT              */
         SLR   ATTNCNT,ATTNCNT                                     0152
*           STAXCNT=STAXCNT-STAXCNT;/* CLEAR STAX COUNT              */
         SLR   STAXCNT,STAXCNT                                     0153
*         END;                                                     0154
*       ELSE                        /* NO                            */
*         IF TAXESCHD='1'B THEN     /* TAXE SCHEDULED                */
         B     @RC00149                                            0155
@RF00149 TM    TAXESCHD(TAXEPTR),B'00001000'                       0155
         BNO   @RF00155                                            0155
*           DO;                                                    0156
*             IF TAXERESM='0'B THEN /* WAS TAXE CANCELLED            */
         TM    TAXERESM(TAXEPTR),B'00010000'                       0157
         BNZ   @RF00157                                            0157
*               DO;                                                0158
*                 FRRSTRF='1'B;     /* SET CANCEL PRINT              */
         OI    FRRSTRF(FRRWKAP),B'10000000'                        0159
*                 FRRTRESM=TAXEPTR; /* PTR TO CANCELLED TAXE         */
         ST    TAXEPTR,FRRTRESM(,FRRWKAP)                          0160
*                 TAXERESM='1'B;    /* CANCEL THE TAXE               */
         OI    TAXERESM(TAXEPTR),B'00010000'                       0161
*               END;                                               0162
*           END;                                                   0163
@RF00157 DS    0H                                                  0164
*     TAXEPTR=TAXELNK;              /* UPDATE SEARCH ARGU            */
@RF00155 DS    0H                                                  0164
@RC00149 DS    0H                                                  0164
@RC00142 L     TAXEPTR,TAXELNK(,TAXEPTR)                           0164
*   END;                                                           0165
@DE00137 LTR   TAXEPTR,TAXEPTR                                     0165
         BNZ   @DL00137                                            0165
*   STAXCNT=STAXCNT-STAXCNT;        /* CLEAR THE STAX CNT            */
         SLR   STAXCNT,STAXCNT                                     0166
*   ATTNCNT=ATTNCNT-ATTNCNT;        /* CLEAR THE ATTN CNT            */
         SLR   ATTNCNT,ATTNCNT                                     0167
*/********************************************************************/
*/*                                                                  */
*/* SET UP THE COMPARE AND SWAP LOOP TO UPDATE THE TSB WITH          */
*/* THE NEW VALUES FOR THE TSBSTAX AND THE TSBATTNC FIELDS           */
*/*                                                                  */
*/********************************************************************/
*                                                                  0168
*   RESPECIFY                                                      0168
*     NSWAPVAL RESTRICTED;                                         0168
*UPDCNT:                                                           0169
*   NSWAPVAL=SWAPVAL&'0000FFFF'X;   /* CLEAR ORIGINAL COUNTS         */
UPDCNT   LR    NSWAPVAL,SWAPVAL                                    0169
         N     NSWAPVAL,@CF02444                                   0169
*   NSWAPVAL=NSWAPVAL×STAXCNT;      /* INSERT NEW STAX COUNT         */
         OR    NSWAPVAL,STAXCNT                                    0170
*   NSWAPVAL=NSWAPVAL×ATTNCNT;      /* INSERT NEW ATTN COUNT         */
         OR    NSWAPVAL,ATTNCNT                                    0171
*   ATTNCNT=SWAPVAL;                /* SAVE ORIGINAL VALUE FROM THE
*                                      TSB                           */
         LR    ATTNCNT,SWAPVAL                                     0172
*   CS(SWAPVAL,NSWAPVAL,TSBATTNC);                                 0173
*                                                                  0173
         CS    SWAPVAL,@09,TSBATTNC                                0173
*   /*****************************************************************/
*   /*                                                               */
*   /* PERFORM ACTUAL UPDATE OF THE VALUES                           */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0174
*   BC(8,STSTOP);                   /* DID THE UPDATE WORK? IF IT  0174
*                                      DID,BRANCH                    */
         BC    8,STSTOP                                            0174
*/* THE UPDATE WAS NOT SUCCESSFUL. THE COUNTS MUST BE SET            */
*/* UP SO THAT THE LOOP CAN BE RE-ENTERED SO THAT THE MORE           */
*/* CURRENT TAXE CAN BE SCHEDULED.                                   */
*                                                                  0175
*   NSWAPVAL=ATTNCNT;               /* MOVE THE ORIGINAL TSB VALUE 0175
*                                      FOR FURTHER USE               */
         LR    NSWAPVAL,ATTNCNT                                    0175
*   STAXCNT=SWAPVAL&'00FF0000'X;    /* NEW AVAILABLE TAXE CNT FROM 0176
*                                      THE TSB                       */
         L     @15,@CF02435                                        0176
         LR    STAXCNT,SWAPVAL                                     0176
         NR    STAXCNT,@15                                         0176
*   STAXCNT=STAXCNT-(NSWAPVAL&'00FF0000'X);                        0177
*                                                                  0177
         NR    @15,NSWAPVAL                                        0177
         SLR   STAXCNT,@15                                         0177
*   /*****************************************************************/
*   /*                                                               */
*   /* DECREMENT THE NEW AVAIL TAXE COUNT BY THE VALUE FROM THE      */
*   /* PREVIOUS TIME THROUGH THE LOOP                                */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0178
*   ATTNCNT=SWAPVAL&'FF000000'X;    /* NEW ATTN COUNT FROM TSB       */
         L     @15,@CF02436                                        0178
         LR    ATTNCNT,SWAPVAL                                     0178
         NR    ATTNCNT,@15                                         0178
*   ATTNCNT=ATTNCNT-(NSWAPVAL&'FF000000'X);                        0179
*                                                                  0179
         NR    @15,NSWAPVAL                                        0179
         SLR   ATTNCNT,@15                                         0179
*   /*****************************************************************/
*   /*                                                               */
*   /* DECREMENT NEW ATTN CNT BY THE NUMBER OF ATTNS FROM THE        */
*   /* PREVIOUS TIME THROUGH THE LOOP                                */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0180
*   GO TO LOOP;                     /* CONTINUE SEARCH               */
         B     LOOP                                                0180
*STSTOP:                            /* COMPLETED                     */
*   GENERATE CODE NOSETS NODEFS NOFLOWS REFS(TSBPTR);              0181
STSTOP   DS    0H                                                  0181
        DROP  TSBPTR
*   RESPECIFY                                                      0182
*     TSBPTR UNRESTRICTED;                                         0182
*   RESPECIFY                                                      0183
*     ATTNCNT UNRESTRICTED;                                        0183
*   RESPECIFY                                                      0184
*     STAXCNT UNRESTRICTED;                                        0184
*   RESPECIFY                                                      0185
*     SWAPVAL UNRESTRICTED;                                        0185
*   RESPECIFY                                                      0186
*     NSWAPVAL UNRESTRICTED;                                       0186
*                                                                  0187
*/********************************************************************/
*/*                                                                  */
*/* THE FOLLOWING SEGMENT OF CODE WILL STATUS STOP THE SUB-          */
*/* TASKS OF THE TCB THAT IS HAVING THE ATTENTION EXIT SCHD.         */
*/* IF THERE IS NO TAXE TO BE SCHEDULED A BRANCH AROUND THIS         */
*/* SEGMENT WILL OCCUR. THE FIRST TEST WILL DETERMINE THE            */
*/* ACTION THAT WILL BE TAKEN                                        */
*/*                                                                  */
*/********************************************************************/
*                                                                  0187
*   IF PTAXETBS^=ZERO THEN          /* IF THERE IS AN ATTN EXIT TO BE
*                                      SCHEDULED THE SUBTASKS OF THE
*                                      TCB THAT IS HAVING THE ATTN 0187
*                                      EXIT SCHEDULED WILL BE STATUS
*                                      STOPPED                       */
         LTR   PTAXETBS,PTAXETBS                                   0187
         BZ    @RF00187                                            0187
*     DO;                                                          0188
*       FRRATTS='1'B;               /* SET FOOTPRINT THAT AN ATTN  0189
*                                      EXIT WILL BE SCHD             */
         OI    FRRATTS(FRRWKAP),B'01000000'                        0189
*       FRRSTAXE=PTAXETBS;          /* SAVE THE ADDRESS OF THE TAXE
*                                      TO BE SCHEDULED               */
         ST    PTAXETBS,FRRSTAXE(,FRRWKAP)                         0190
*       TAXEPTR=PTAXETBS;           /* INITIALIZE BASE REG FOR THE 0191
*                                      TAXE                          */
         LR    TAXEPTR,PTAXETBS                                    0191
*       IF RCTDCLST>OFF THEN                                       0192
         L     @15,ASXBRCTD(,ASXBPTR)                              0192
         CLC   RCTDCLST(2,@15),@CH00036                            0192
         BNH   @RF00192                                            0192
*         TAXEATTN='1'B;            /* IF >0 THEN SET FLAG FOR ATTN
*                                      PROLOGUE(IEAVAR05)    @ZA08100*/
         OI    TAXEATTN(TAXEPTR),B'00000100'                       0193
*       TCBPTR=TAXETCB;             /* INITIALIZE THE TCB BASE     0194
*                                      REGISTER                      */
@RF00192 L     TCBPTR,TAXETCB(,TAXEPTR)                            0194
*       IF TCBLTC^=ZERO THEN        /* IF A DAUGHER EXITS SEARCH   0195
*                                      SUBTASK QUEUE                 */
         L     @15,TCBLTC(,TCBPTR)                                 0195
         LTR   @15,@15                                             0195
         BZ    @RF00195                                            0195
*         DO;                                                      0196
*           RESPECIFY                                              0197
*             PTCBPTR RESTRICTED;                                  0197
*           RESPECIFY                                              0198
*             STATST0 RESTRICTED;                                  0198
*           RESPECIFY                                              0199
*             STATST1 RESTRICTED;                                  0199
*           RESPECIFY                                              0200
*             STATST13 RESTRICTED;                                 0200
*           TCBPTR=TCBLTC;          /* ESTABLISH TCB BASE            */
         LR    TCBPTR,@15                                          0201
*STSTOP1:                                                          0202
*           DO WHILE TCBPTR^=ZERO;  /* TCB BASE ZERO                 */
STSTOP1  B     @DE00202                                            0202
@DL00202 DS    0H                                                  0203
*/* SET UP THE INTERFACE FOR STATUS STOP TCB                         */
*                                                                  0203
*             FRRGTSS='1'B;         /* SET FOOTPRINT INDICATING GOING
*                                      TO STATUS STOP                */
         OI    FRRGTSS(FRRWKAP),B'00100000'                        0203
*             FRRSSTCB=TCBPTR;      /* SAVE THE ADDRESS OF THE TCB TO
*                                      BE STOPPED                    */
         ST    TCBPTR,FRRSSTCB(,FRRWKAP)                           0204
*             STATST0='00000006'X;  /* PLACE A SIX IN PARM REG       */
         LA    STATST0,6                                           0205
*             STATST1=TCBPTR;       /* PLACE ADDRESS OF TCB TO BE  0206
*                                      STOPPED IN PARM REG           */
         LR    STATST1,TCBPTR                                      0206
*             STATST13=ZERO;        /* CLEAR PARM REG FOR MASK       */
         SLR   STATST13,STATST13                                   0207
*             CALL STATENT;         /* GO TO STATUS STOP TCB         */
         L     @10,SCVTSPTR(,CVTPTR)                               0208
         L     @15,SCVTSTAT(,@10)                                  0208
         BALR  @14,@15                                             0208
*             FRRGTSS='0'B;         /* RESET THE FOOTPRINT IND THAT
*                                      STATUS HAS BEEN ENTERED       */
         NI    FRRGTSS(FRRWKAP),B'11011111'                        0209
*             PTCBPTR=TCBPTR;       /* SAVE PREV TCB PTR             */
         LR    PTCBPTR,TCBPTR                                      0210
*             TCBPTR=TCBLTC;        /* UPDATE TCB SEARCH ARG         */
         L     TCBPTR,TCBLTC(,TCBPTR)                              0211
*           END;                                                   0212
@DE00202 LTR   TCBPTR,TCBPTR                                       0212
         BNZ   @DL00202                                            0212
*           TCBPTR=PTCBPTR;         /* REESTABLISH TCB BASE          */
         LR    TCBPTR,PTCBPTR                                      0213
*           DO WHILE TCBPTR^=TAXETCB;/* CONTINUE SEARCHING TCB CHAIN
*                                      UNTIL THE ORIGINAL TCB        */
         B     @DE00214                                            0214
@DL00214 DS    0H                                                  0215
*             IF TCBNTC^=ZERO THEN  /* IF THERE IS A SISTER TCB USE
*                                      IT AS NEW SEARCH ARGUMENT TCB */
         L     @15,TCBNTC(,TCBPTR)                                 0215
         LTR   @15,@15                                             0215
         BZ    @RF00215                                            0215
*               DO;                                                0216
*                 TCBPTR=TCBNTC;    /* UPDATE NEW SEARCH ARG TCB   0217
*                                      ADDRESS TO SISTER OF CURRENT
*                                      SEARCH TCB                    */
         LR    TCBPTR,@15                                          0217
*                 GO TO STSTOP1;    /* GO STOP SISTER                */
         B     STSTOP1                                             0218
*               END;                                               0219
*             ELSE                  /* NO                            */
*               DO;                                                0220
@RF00215 DS    0H                                                  0221
*                 TCBPTR=TCBOTC;    /* UPDATE NEW SEARCH ARG TCB   0221
*                                      ADDRESS TO MOTHER OF CURRENT
*                                      SEARCH TCB                    */
         L     TCBPTR,TCBOTC(,TCBPTR)                              0221
*               END;                                               0222
*           END;                                                   0223
@DE00214 C     TCBPTR,TAXETCB(,TAXEPTR)                            0223
         BNE   @DL00214                                            0223
*           RESPECIFY                                              0224
*             STATST0 UNRESTRICTED;                                0224
*           RESPECIFY                                              0225
*             STATST1 UNRESTRICTED;                                0225
*           RESPECIFY                                              0226
*             STATST13 UNRESTRICTED;                               0226
*           RESPECIFY                                              0227
*             PTCBPTR UNRESTRICTED;                                0227
*         END;                                                     0228
*       FRRGTST='1'B;               /* SET FOOTPRINT INDICATING GOING
*                                      TO STAGE TWO EXIT EFFECTOR    */
@RF00195 OI    FRRGTST(FRRWKAP),B'00010000'                        0229
*       TAXESCHD='1'B;              /* SET FOOTPRINT INDICATING TAXE
*                                      HAS BEEN SCHEDULED            */
         OI    TAXESCHD(TAXEPTR),B'00001000'                       0230
*       RBEP=ADDR(IEAVAR05);        /* PLACE ENTRY POINT OF R05 IN 0231
*                                      THE RB (TAXE)                 */
         L     @15,@CV02426                                        0231
         ST    @15,RBEP(,RBSECPTR)                                 0231
*       RPSWFLD='070C0000'X;        /* INITIALIZE THE RESUME PSW TO
*                                      KEY 0 AND SUPERVISOR STATE  0232
*                                                            @YM05190*/
         MVC   RPSWFLD(4,RBSECPTR),@CF02453                        0232
*/* SET UP THE INTERFACE FOR THE STAGE TWO EXIT EFFECTOR             */
*/* THIS WILL PLACE THE ATTN EXIT ON THE IQE QUEUE                   */
*                                                                  0233
*       RESPECIFY                                                  0233
*         STGTWO1 RESTRICTED;                                      0233
*       RESPECIFY                                                  0234
*         STGTWO10 RESTRICTED;                                     0234
*       STGTWO1=ADDR(TIQELNK);      /* PLACE THE ADDRESS OF THE IQE
*                                      IN THE PARAMETER REGISTER     */
         LA    STGTWO1,TIQELNK(,TAXEPTR)                           0235
*       STGTWO1=-(STGTWO1);         /* COMPLIMENT THE IQE ADDR       */
         LCR   STGTWO1,STGTWO1                                     0236
*       CALL STGTENT;               /* GO TO STAGE TWO EXIT EFFECTOR */
         L     @15,CVT0EF00(,CVTPTR)                               0237
         BALR  @14,@15                                             0237
*       RESPECIFY                                                  0238
*         STGTWO1 UNRESTRICTED;                                    0238
*       RESPECIFY                                                  0239
*         STGTWO10 UNRESTRICTED;                                   0239
*       FRRCFST='1'B;               /* SET THE COMING FROM THE STAGE
*                                      TWO EXIT EFFECTOR FOOTPRINT   */
         OI    FRRCFST(FRRWKAP),B'00001000'                        0240
*     END;                                                         0241
*   RESPECIFY                                                      0242
*     PTAXETBS UNRESTRICTED;                                       0242
@RF00187 DS    0H                                                  0243
*   RESPECIFY                                                      0243
*     TAXEPTR UNRESTRICTED;                                        0243
*                                                                  0244
*/********************************************************************/
*/*                                                                  */
*/* THIS SEGMENT WILL SCAN THE ENTIRE TCB RB QUEUE SEARCHING         */
*/* FOR ANY TGET/TPUT SVRBS THAT ARE IN IWAIT OR OWAIT. IF           */
*/* AN SVRB IS FOUND THE TCBTIOTG FLAG IS SET AND THE IWAIT          */
*/* OR OWAIT CONDITION IS RESET USING STATUS RESET.                  */
*/*                                                                  */
*/********************************************************************/
*                                                                  0244
*   RESPECIFY                                                      0244
