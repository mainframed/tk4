         TITLE 'IEAVAR07 - ATTENTION EXIT PURGE ROUTINE                *
                        '
IEAVAR07 CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'IEAVAR07  77.133'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @08,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@08                                         0001
RBPRFXLN EQU 64
RBPRFLNA EQU 32
PRBLEN   EQU 136
SIRBLEN  EQU 200
TIRBLEN  EQU 136
IRBLEN   EQU 128
SVRBLEN  EQU 200
DECREG   DS    0H                                                  0043
*   ENTREG13=RTNREG13;              /* SAVE REG SAVE PTR             */
         LR    ENTREG13,RTNREG13                                   0075
*   RESPECIFY                                                      0076
*     RTNREG13 UNRESTRICTED;                                       0076
*   RESPECIFY                                                      0077
*     ASCBPTR RESTRICTED;                                          0077
*   ASCBPTR=PSAAOLD;                /* INITIALIZE ASCB BASE          */
         L     ASCBPTR,PSAAOLD                                     0078
*   IF ASCBTSB^='00000000'X THEN    /* IS THIS A TSO USER            */
         L     @12,ASCBTSB(,ASCBPTR)                               0079
         LTR   @12,@12                                             0079
         BZ    @RF00079                                            0079
*     DO;                                                          0080
*                                                                  0080
*/* THE SETLOCK MACRO USES AND DOES NOT SAVE REGISTERS               */
*/* 11,12,13 AND 14.                                                 */
*                                                                  0081
*       RESPECIFY                                                  0081
*         SETLK11 RESTRICTED;                                      0081
*       RESPECIFY                                                  0082
*         SETLK12 RESTRICTED;                                      0082
*       RESPECIFY                                                  0083
*         SETLK13 RESTRICTED;                                      0083
*       RESPECIFY                                                  0084
*         SETLK14 RESTRICTED;                                      0084
*SETLKO1:                           /* GET THE LOCAL LOCK            */
*       GENERATE CODE NODEFS NOSETS NOFLOWS REFS(PSALITA);         0085
SETLKO1  DS    0H                                                  0085
       SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,                          X
               RELATED=(TAXE-QUEUE,IEAVAR07(SETLKR1))
*       RESPECIFY                                                  0086
*         SETLK11 UNRESTRICTED;                                    0086
*       RESPECIFY                                                  0087
*         SETLK12 UNRESTRICTED;                                    0087
*       RESPECIFY                                                  0088
*         SETLK13 UNRESTRICTED;                                    0088
*       RESPECIFY                                                  0089
*         SETLK14 UNRESTRICTED;                                    0089
*                                                                  0090
*/********************************************************************/
*/*                                                                  */
*/* THE FOLLOWING SEGMENT OF CODE WILL ESTABLISH A RECOVERY          */
*/* ENVIRONMENT FOR THE LOCKED CODE OF THIS ROUTINE                  */
*/*                                                                  */
*/********************************************************************/
*                                                                  0090
*       RESPECIFY                                                  0090
*         FRREPTR RESTRICTED;                                      0090
*       RESPECIFY                                                  0091
*         FRRSPTR RESTRICTED;                                      0091
*       FRRSPTR=ADDR(IEAVAFR7);     /* ENTRY POINT TO FRR            */
         LA    FRRSPTR,IEAVAFR7                                    0092
*       GENERATE CODE NOFLOWS NODEFS NOSETS REFS(FRRSPTR,FRREPTR,  0093
*           FRRWKAP,PSACSTK,FRRSCURR,PSALSFCC,FRRSLAST,FRRSELEN,   0093
*           FRRSFRRA,FRRSPARM);                                    0093
       SETFRR A,FRRAD=(FRRSPTR),PARMAD=(FRRWKAP),                      X
               WRKREGS=(FRRWKAP,FRREPTR)
*       RESPECIFY                                                  0094
*         FRRSPTR UNRESTRICTED;                                    0094
*       RESPECIFY                                                  0095
*         FRREPTR UNRESTRICTED;                                    0095
*                                                                  0096
*/********************************************************************/
*/*                                                                  */
*/* THE FOLLOWING SEGMENT OF CODE WILL SEARCH THE TAXE               */
*/* QUEUE LOOKING FOR TAXES THAT ARE FOR THE TERMINATING             */
*/* TASK. IF A TAXE IS FOUND FOR THE TCB THE FOLLOWING               */
*/* ACTIONS ARE TAKEN.                                               */
*/*                                                                  */
*/* IF THE TAXE IS AN ACTIVE RB THE RBFDYN FLAG WILL BE              */
*/* SET SO THAT THE RB WILL BE FREED BY EXIT. ALL OTHER              */
*/* PROCESSING THAT IS NECESSARY TO HOUSEKEEP THE TAXE               */
*/* RESOURCE IS PERFORMED AT EXIT TIME BY IEAVAR06.                  */
*/*                                                                  */
*/* IF THE TAXE IS SCHEDULED BUT IS NOT AN ACTIVE RB                 */
*/* THE TAXE IS DEQUEUED AND THE RBFDYN FLAG IS SET.                 */
*/* THE TAXE IS DEQUEUED BECAUSE THE TAXE IS SCHEDULED               */
*/* AS AN IQE THAT WILL BE DEQUEUED FROM THE IQE QUEUE               */
*/* AND NEVER SCHEDULED. THE RBFDYN FLAG IS SET SO THAT              */
*/* IF FOR ANY REASON THE TAXE(IQE) DOES BECOME AN ACTIVE            */
*/* RB IT WILL BE FREED BY EXIT.                                     */
*/*                                                                  */
*/* IF THE TAXE IS DORMANT THE TAXE IS DEQUEUED AND THE              */
*/* RBFDYN FLAG IS SET ONLY FOR CONVIENCE.                           */
*/*                                                                  */
*/* THE TAXES ARE NOT FREED BY THIS ROUTINE. THE TAXES               */
*/* THAT ARE ACTIVE RB'S WILL BE FREED BY EXIT, THE IQES             */
*/* AND DORMANT TAXES ARE FREED WHEN THE SUBPOOL STORAGE             */
*/* FOR THIS TCB IS FREED. MOREOVER THE TAXES THAT ARE               */
*/* ON THE IQE QUEUE CANNOT BE FREED BECAUSE THEY HAVE               */
*/* NOT YET BEEN DEQUEUED FROM THE STAGE TWO EXIT                    */
*/* EFFECTORS IQE QUEUE.                                             */
*/*                                                                  */
*/********************************************************************/
*                                                                  0096
*       RESPECIFY                                                  0096
*         TAXEPTR RESTRICTED;                                      0096
*       RESPECIFY                                                  0097
*         PTAXEPTR RESTRICTED;                                     0097
*       RESPECIFY                                                  0098
*         NTAXEPTR RESTRICTED;                                     0098
*       TAXEPTR=RCTDTAXE;           /* INIT TAXE BASE REG            */
         L     @12,ASXBPTR(,ASCBPTR)                               0099
         L     @12,ASXBRCTD(,@12)                                  0099
         L     TAXEPTR,RCTDTAXE(,@12)                              0099
*       PTAXEPTR=PTAXEPTR-PTAXEPTR; /* CLEAR PREV TAXE PTR           */
         SLR   PTAXEPTR,PTAXEPTR                                   0100
*       NTAXEPTR=NTAXEPTR-NTAXEPTR; /* CLEAR NEXT TAXE PTR           */
         SLR   NTAXEPTR,NTAXEPTR                                   0101
*       DO WHILE TAXEPTR^='00000000'X;/* SCAN ENTIRE TAXE QUEUE      */
         B     @DE00102                                            0102
@DL00102 DS    0H                                                  0103
*         IF TAXETCB=RMPLTCBA THEN  /* IS TAXE FOR TERM TCB          */
         L     @12,RMPLPTR(,PTRRMPL)                               0103
         CLC   TAXETCB(4,TAXEPTR),RMPLTCBA(@12)                    0103
         BNE   @RF00103                                            0103
*           DO;                     /* YES                           */
*             RBFDYN='1'B;          /* SET FREE AT EXIT FLAG         */
         OI    RBFDYN(RBSECPTR),B'00000010'                        0105
*             IF RBFACTV='0'B THEN  /* IS TAXE AN ACTIVE RB          */
         TM    RBFACTV(RBSECPTR),B'01000000'                       0106
         BNZ   @RF00106                                            0106
*               DO;                 /* NO                            */
*                 FRRTTBDQ='1'B;    /* SET FOOTPRINT FOR FRR         */
         OI    FRRTTBDQ(FRRWKAP),B'10000000'                       0108
*                 FRRTAXE=TAXEPTR;  /* SAVE TAXE ADDR                */
         ST    TAXEPTR,FRRTAXE(,FRRWKAP)                           0109
*                 IF PTAXEPTR='00000000'X THEN/* FIRST TAXE ON QUEUE */
         LTR   PTAXEPTR,PTAXEPTR                                   0110
         BNZ   @RF00110                                            0110
*                   DO;             /* YES                           */
*                     RCTDTAXE=TAXELNK;/* DEQUEUE FIRST TAXE         */
         L     @12,ASXBPTR(,ASCBPTR)                               0112
         L     @12,ASXBRCTD(,@12)                                  0112
         L     @11,TAXELNK-1(,TAXEPTR)                             0112
         LA    @11,0(,@11)                                         0112
         ST    @11,RCTDTAXE(,@12)                                  0112
*                   END;                                           0113
*                 ELSE              /* NO                            */
*                   DO;             /* NO                            */
         B     @RC00110                                            0114
@RF00110 DS    0H                                                  0115
*                     NTAXEPTR=TAXELNK;/* POINTER TO NEXT TAXE       */
         L     NTAXEPTR,TAXELNK-1(,TAXEPTR)                        0115
         LA    NTAXEPTR,0(,NTAXEPTR)                               0115
*                     TAXEPTR=PTAXEPTR;/* UPDATE BASE PTR            */
         LR    TAXEPTR,PTAXEPTR                                    0116
*                     TAXELNK=NTAXEPTR;/* DEQUEUE THE TAXE           */
         STCM  NTAXEPTR,7,TAXELNK(TAXEPTR)                         0117
*                     PTAXEPTR=TAXEPTR;/* SAVE PREVIOUS TAXE PTR     */
         LR    PTAXEPTR,TAXEPTR                                    0118
*                   END;                                           0119
*                 FRRTTBDQ='0'B;    /* RESET FOOTPRINT               */
@RC00110 NI    FRRTTBDQ(FRRWKAP),B'01111111'                       0120
*                 FRRTAXE='00000000'X;/* RESET TAXE PTR              */
         SLR   @12,@12                                             0121
         ST    @12,FRRTAXE(,FRRWKAP)                               0121
*               END;                                               0122
*             ELSE                  /* YES                           */
*               DO;                                                0123
         B     @RC00106                                            0123
@RF00106 DS    0H                                                  0124
*                 PTAXEPTR=TAXEPTR; /* UPDATE PREVIOUS PTR           */
         LR    PTAXEPTR,TAXEPTR                                    0124
*               END;                                               0125
*           END;                                                   0126
*         ELSE                      /* NO                            */
*           DO;                                                    0127
         B     @RC00103                                            0127
@RF00103 DS    0H                                                  0128
*             PTAXEPTR=TAXEPTR;     /* UPDATE PREVIOUS PTR           */
         LR    PTAXEPTR,TAXEPTR                                    0128
*           END;                                                   0129
*         TAXEPTR=TAXELNK;          /* UPDATE TAXE BASE              */
@RC00103 L     @12,TAXELNK-1(,TAXEPTR)                             0130
         LA    @12,0(,@12)                                         0130
         LR    TAXEPTR,@12                                         0130
*       END;                                                       0131
@DE00102 LTR   TAXEPTR,TAXEPTR                                     0131
         BNZ   @DL00102                                            0131
*       RESPECIFY                                                  0132
*         NTAXEPTR UNRESTRICTED;                                   0132
*                                                                  0133
*/********************************************************************/
*/*                                                                  */
*/* THE FOLLOWING SEGMENT OF CODE WILL CHECK TO SEE IF               */
*/* THERE ARE ANY TAXES ON THE QUEUE. IF THERE ARE THE               */
*/* ENTIRE TAXE QUEUE WILL BE SEARCHED LOOKING FOR THE               */
*/* HIGHEST LEVEL TAXE THAT HAS BEEN SCHEDULED.                      */
*/*                                                                  */
*/********************************************************************/
*                                                                  0133
*       TAXEPTR=RCTDTAXE;           /* INITIALIZE SEARCH ARG         */
         L     @12,ASXBPTR(,ASCBPTR)                               0133
         L     @12,ASXBRCTD(,@12)                                  0133
         L     TAXEPTR,RCTDTAXE(,@12)                              0133
*       PTAXEPTR=PTAXEPTR-PTAXEPTR; /* CLEAR ACTIVE TAXE PTR         */
         SLR   PTAXEPTR,PTAXEPTR                                   0134
*       DO WHILE TAXEPTR^='00000000'X;/* ANY MORE TAXES              */
         B     @DE00135                                            0135
@DL00135 DS    0H                                                  0136
*         IF RBFACTV='1'B×TAXESCHD='1'B THEN/* IS THE TAXE ACT       */
         TM    RBFACTV(RBSECPTR),B'01000000'                       0136
         BO    @RT00136                                            0136
         TM    TAXESCHD(TAXEPTR),B'00001000'                       0136
         BNO   @RF00136                                            0136
@RT00136 DS    0H                                                  0137
*           DO;                     /* YES                           */
*             PTAXEPTR=TAXEPTR;     /* SAVE ACTIVE TAXE PTR          */
         LR    PTAXEPTR,TAXEPTR                                    0138
*           END;                                                   0139
*         TAXEPTR=TAXELNK;          /* UPDATE SEARCH ARGUMENT        */
@RF00136 L     @12,TAXELNK-1(,TAXEPTR)                             0140
         LA    @12,0(,@12)                                         0140
         LR    TAXEPTR,@12                                         0140
*       END;                                                       0141
@DE00135 SLR   @12,@12                                             0141
         CR    TAXEPTR,@12                                         0141
         BNE   @DL00135                                            0141
*                                                                  0142
*/********************************************************************/
*/*                                                                  */
*/* THE TAXES FOLLOWING THE HIGHEST ACTIVE TAXE MUST BE              */
*/* MARKED AVAILABLE FOR USE AND THEY MUST BE REFLECTED              */
*/* IN THE TSBSTAX COUNT. THIS SEGMENT OF CODE WILL                  */
*/* SEARCH THE ENTIRE TAXE QUEUE STARTING WITH THE HIGHEST           */
*/* SCHEDULED TAXE AND PERFORM THE OPERATION.                        */
*/*                                                                  */
*/********************************************************************/
*                                                                  0142
*       RESPECIFY                                                  0142
*         STAXCNT RESTRICTED;                                      0142
*       STAXCNT=STAXCNT-STAXCNT;    /* CLEAR NEW STAX CNT            */
         SLR   STAXCNT,STAXCNT                                     0143
*       IF RCTDTAXE^='00000000'X THEN/* ARE THERE ANY TAXES          */
         L     @11,ASXBPTR(,ASCBPTR)                               0144
         L     @11,ASXBRCTD(,@11)                                  0144
         C     @12,RCTDTAXE(,@11)                                  0144
         BE    @RF00144                                            0144
*         DO;                                                      0145
*           IF PTAXEPTR^='00000000'X THEN/* IS THIS HEAD OF QUEUE    */
         CR    PTAXEPTR,@12                                        0146
         BE    @RF00146                                            0146
*             DO;                   /* NO                            */
*               TAXEPTR=PTAXEPTR;   /* ESTABLISH BASE FOR TAXE       */
         LR    TAXEPTR,PTAXEPTR                                    0148
*               TAXEPTR=TAXELNK;    /* START WITH NEXT TAXE          */
         L     @12,TAXELNK-1(,TAXEPTR)                             0149
         LA    @12,0(,@12)                                         0149
         LR    TAXEPTR,@12                                         0149
*             END;                                                 0150
*           ELSE                    /* YES                           */
*             DO;                                                  0151
         B     @RC00146                                            0151
@RF00146 DS    0H                                                  0152
*               TAXEPTR=RCTDTAXE;   /* SET BASE TO LOWEST            */
         L     @12,ASXBPTR(,ASCBPTR)                               0152
         L     @12,ASXBRCTD(,@12)                                  0152
         L     TAXEPTR,RCTDTAXE(,@12)                              0152
*             END;                                                 0153
*           DO WHILE TAXEPTR^='00000000'X;/* SEARCH THE REMAINDER OF
*                                      THE TAXE QUEUE                */
@RC00146 B     @DE00154                                            0154
@DL00154 DS    0H                                                  0155
*             TAXEFREQ='0'B;        /* MARK TAXE AVAILABLE           */
         NI    TAXEFREQ(TAXEPTR),B'11011111'                       0155
*             STAXCNT=STAXCNT+'00010000'X;/* INCREMENT COUNT BY ONE  */
         AL    STAXCNT,@CF01363                                    0156
*             TAXEPTR=TAXELNK;      /* UPDATE SEARCH ARGU            */
         L     @12,TAXELNK-1(,TAXEPTR)                             0157
         LA    @12,0(,@12)                                         0157
         LR    TAXEPTR,@12                                         0157
*           END;                                                   0158
@DE00154 LTR   TAXEPTR,TAXEPTR                                     0158
         BNZ   @DL00154                                            0158
*         END;                                                     0159
*       RESPECIFY                                                  0160
*         PTAXEPTR UNRESTRICTED;                                   0160
@RF00144 DS    0H                                                  0161
*       RESPECIFY                                                  0161
*         TAXEPTR UNRESTRICTED;                                    0161
*                                                                  0162
*/********************************************************************/
*/*                                                                  */
*/* UPDATE THE STAX COUNT IN THE TSB                                 */
*/*                                                                  */
*/* THE COUNT IS UPDATED USING COMPARE AND SWAP                      */
*/*                                                                  */
*/* THE COUNT WILL NOT BE ALLOWED TO BE GREATER THAN 255             */
*/* EVEN IF MORE TAXES EXIST.                                        */
*/*                                                                  */
*/********************************************************************/
*                                                                  0162
*       RESPECIFY                                                  0162
*         TSBPTR RESTRICTED;                                       0162
*       RESPECIFY                                                  0163
*         SWAPVAL RESTRICTED;                                      0163
*       RESPECIFY                                                  0164
*         NSWAPVAL RESTRICTED;                                     0164
*       TSBPTR=ASCBTSB;             /* SET BASE REG FOR TSB          */
         L     TSBPTR,ASCBTSB(,ASCBPTR)                            0165
*       GENERATE CODE NODEFS NOSETS NOFLOWS REFS(TSBPTR);          0166
         USING  TSB,TSBPTR
*       IF STAXCNT>'00FF0000'X THEN /* IS STAX COUNT >255            */
         L     @05,@CF01364                                        0167
         CR    STAXCNT,@05                                         0167
         BNH   @RF00167                                            0167
*         DO;                       /* YES                           */
*           STAXCNT='00FF0000'X;    /* SET TO 255                    */
         LR    STAXCNT,@05                                         0169
*         END;                                                     0170
*       SWAPVAL=TSBATTNC;           /* LOAD ORIG COUNT               */
@RF00167 L     SWAPVAL,TSBATTNC                                    0171
*LOOP:                                                             0172
*       NSWAPVAL=SWAPVAL;           /* LOAD ORIG COUNT               */
LOOP     LR    NSWAPVAL,SWAPVAL                                    0172
*       NSWAPVAL=NSWAPVAL&'FF00FFFF'X;/* CLEAR STAX CNT              */
         N     NSWAPVAL,@CF01367                                   0173
*       NSWAPVAL=NSWAPVAL×STAXCNT;  /* INSERT NEW COUNT              */
         OR    NSWAPVAL,STAXCNT                                    0174
*       CS(SWAPVAL,NSWAPVAL,TSBATTNC);/* UPDATE THE COUNT            */
         CS    SWAPVAL,@12,TSBATTNC                                0175
*       BC(7,LOOP);                 /* DID THE UPDATE WORK CONTINUE
*                                      IF IT DID                     */
         BC    7,LOOP                                              0176
*       GENERATE CODE NODEFS NOSETS NOFLOWS REFS(TSBPTR);          0177
         DROP  TSBPTR
*       RESPECIFY                                                  0178
*         SWAPVAL UNRESTRICTED;                                    0178
*       RESPECIFY                                                  0179
*         NSWAPVAL UNRESTRICTED;                                   0179
*       RESPECIFY                                                  0180
*         STAXCNT UNRESTRICTED;                                    0180
*       RESPECIFY                                                  0181
*         TSBPTR UNRESTRICTED;                                     0181
*                                                                  0182
*/********************************************************************/
*/*                                                                  */
*/* THE FOLLOWING SEGMENT OF CODE WILL DELETE THE RECOVERY           */
*/*                                                                  */
*/********************************************************************/
*                                                                  0182
*       RESPECIFY                                                  0182
*         FRRSPTR RESTRICTED;                                      0182
*       RESPECIFY                                                  0183
*         FRREPTR RESTRICTED;                                      0183
*       GENERATE CODE NOSETS NODEFS NOFLOWS REFS(FRRSPTR,FRREPTR,  0184
*           PSACSTK,FRRSCURR,FRRSEMP,FRRSELEN);                    0184
        SETFRR D,WRKREGS=(FRRSPTR,FRREPTR)
*       RESPECIFY                                                  0185
*         FRRSPTR RESTRICTED;                                      0185
*       RESPECIFY                                                  0186
*         FRREPTR RESTRICTED;                                      0186
*                                                                  0187
*/********************************************************************/
*/*                                                                  */
*/* THIS ROUTINE WILL RELEASE THE LOCAL LOCK. THE RETURN             */
*/* CODE FROM SETLOCK IS NOT CHECKED TO SEE IF THE                   */
*/* RELEASE WAS SUCCESSFUL                                           */
*/*                                                                  */
*/********************************************************************/
*                                                                  0187
*       RESPECIFY                                                  0187
*         SETLK11 RESTRICTED;                                      0187
*       RESPECIFY                                                  0188
*         SETLK12 RESTRICTED;                                      0188
*       RESPECIFY                                                  0189
*         SETLK13 RESTRICTED;                                      0189
*       RESPECIFY                                                  0190
*         SETLK14 RESTRICTED;                                      0190
*SETLKR1:                           /* RELEASE THE LOCAL LOCK        */
*       GENERATE CODE NOFLOWS NOSETS NODEFS REFS(PSALITA);         0191
SETLKR1  DS    0H                                                  0191
       SETLOCK  RELEASE,TYPE=LOCAL,                                    X
               RELATED=(TAXE-QUEUE,IEAVAR07(SETLKO1))
*       RESPECIFY                                                  0192
*         SETLK11 UNRESTRICTED;                                    0192
*       RESPECIFY                                                  0193
*         SETLK12 UNRESTRICTED;                                    0193
*       RESPECIFY                                                  0194
*         SETLK13 UNRESTRICTED;                                    0194
*       RESPECIFY                                                  0195
*         SETLK14 UNRESTRICTED;                                    0195
*     END;                                                         0196
*                                                                  0197
*/********************************************************************/
*/*                                                                  */
*/* THIS CODE WILL RE-ESTABLISH THE SAVE AREA POINTER                */
*/* AND WILL SET THE RETURN CODE IN REGISTER 15                      */
*/*                                                                  */
*/********************************************************************/
*                                                                  0197
*   RESPECIFY                                                      0197
*     RTNREG13 RESTRICTED;                                         0197
@RF00079 DS    0H                                                  0198
*   RTNREG13=ENTREG13;              /* RELOAD SAVE AREA ADDR         */
         LR    RTNREG13,ENTREG13                                   0198
*   RESPECIFY                                                      0199
*     ENTREG13 UNRESTRICTED;                                       0199
*   RETURN CODE(0);                 /* RETURN TO END OF TASK         */
         SLR   @15,@15                                             0200
         L     @14,12(,@13)                                        0200
         LM    @00,@12,20(@13)                                     0200
         BR    @14                                                 0200
*                                                                  0201
*/********************************************************************/
*/*                                                                  */
*/* THIS ROUTINE WILL RECEIVE CONTROL IN THE EVENT OF AN ERROR IN    */
*/* THE ATTENTION EXIT PURGE ROUTINE. CONTROL WILL BE RECEIVED       */
*/* FROM RTM VIA A LOAD PSW. REGISTERS UPON ENTRY ARE AS FOLLOWS:    */
*/*             REGISTER  0  UNPREDICTABLE                           */
*/*                       1  ADDRESS OF SDWA                         */
*/*                    2-13  UNPREDICTABLE                           */
*/*                      14  RETURN ADDRESS                          */
*/*                      15  ENTRY POINT ADDRESS                     */
*/*                                                                  */
*/********************************************************************/
*                                                                  0201
*IEAVAFR7:                                                         0201
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA,NOPARMREG,RETREG);         0201
@EL00001 DS    0H                                                  0201
@EF00001 DS    0H                                                  0201
@ER00001 LM    @14,@12,12(@13)                                     0201
         BR    @14                                                 0201
IEAVAFR7 DS    0H                                                  0202
*   GENERATE CODE NOSETS NODEFS NOFLOWS REFS(IEAVAR07);            0202
       USING  IEAVAFR7,8
       LR     8,15
*   RESPECIFY                                                      0203
*     SDWAPTR RESTRICTED;                                          0203
*   RESPECIFY                                                      0204
*     RTNREG14 RESTRICTED;                                         0204
*   FRRWKAP=SDWAPARM;               /* INITIALIZE FRR WORK AREA BASE */
         L     FRRWKAP,SDWAPARM(,SDWAPTR)                          0205
*   IF SDWAFMID='00000000'X THEN    /* ERROR IN THIS ADDRESS SPACE   */
         CLC   SDWAFMID(2,SDWAPTR),@CB01356                        0206
         BNE   @RF00206                                            0206
*     DO;                           /* YES                           */
*       ASCBPTR=PSAAOLD;            /* INITIALIZE ASCB BASE REGISTER */
         L     ASCBPTR,PSAAOLD                                     0208
*       RCTDTAXE='00000000'X;       /* CLEAR TAXE QUEUE POINTER      */
         L     @15,ASXBPTR(,ASCBPTR)                               0209
         L     @15,ASXBRCTD(,@15)                                  0209
         SLR   @09,@09                                             0209
         ST    @09,RCTDTAXE(,@15)                                  0209
*       RESPECIFY                                                  0210
*         TSBPTR RESTRICTED;                                       0210
*       RESPECIFY                                                  0211
*         SWAPVAL RESTRICTED;                                      0211
*       RESPECIFY                                                  0212
*         NSWAPVAL RESTRICTED;                                     0212
*       TSBPTR=ASCBTSB;             /* INITIALIZE TSB BASE REG       */
         L     TSBPTR,ASCBTSB(,ASCBPTR)                            0213
*       GENERATE CODE NOSETS NODEFS NOFLOWS REFS(TSBPTR);          0214
        USING  TSB,TSBPTR
*       SWAPVAL=TSBATTNC;           /* LOAD CURRENT TSB VALUE        */
         L     SWAPVAL,TSBATTNC                                    0215
*LOOP1:                                                            0216
*       NSWAPVAL=SWAPVAL;           /* LOAD CURRENT TSB VALUE        */
LOOP1    LR    NSWAPVAL,SWAPVAL                                    0216
*       NSWAPVAL=NSWAPVAL&'0000FFFF'X;/* CLEAR COUNTS                */
         N     NSWAPVAL,@CF01377                                   0217
*       CS(SWAPVAL,NSWAPVAL,TSBATTNC);/* UPDATE TSB                  */
         CS    SWAPVAL,@12,TSBATTNC                                0218
*       BC(7,LOOP1);                /* TRY AGAIN IF NECESSARY        */
         BC    7,LOOP1                                             0219
*       GENERATE CODE NOSETS NODEFS NOFLOWS REFS(TSBPTR);          0220
        DROP   TSBPTR
*       RESPECIFY                                                  0221
*         TSBPTR UNRESTRICTED;                                     0221
*       RESPECIFY                                                  0222
