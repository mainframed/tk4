*LOOP0:                             /*                       @YA00058*/
*       R8=R6&FQUMASK;              /*                       @YA00058*/
LOOP0    LR    R8,R6                                               0130
         N     R8,@CF00100                                         0130
*       CS(R6,R8,CSFIELD);          /*                       @YA00058*/
         CS    R6,@08,CSFIELD(R7)                                  0131
*       BC(7,LOOP0);                /*                       @YA00058*/
         BC    7,LOOP0                                             0132
*       RFY                                                        0133
*        (R6,                                                      0133
*         R8) UNRSTD;               /*                       @YA00058*/
