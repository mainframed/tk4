         L     R0,LBLPTR                LOAD POINTER TO LABEL
         L     R1,LPPTR                 LOAD POINTER TO LP
         ST    R0,AD0(R1)               STORE LABEL POINTER
         L     R13,SVR13                RESTORE REG 13
         MVI   AD0(R3),X09
         LM    R14,R12,12(R13)          RESTORE REGISTERS
         BR    R14                      RETURN
