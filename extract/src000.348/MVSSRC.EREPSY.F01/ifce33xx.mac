*                                                                  0207
*/********************************************************************
*                                                                  0207
*   THIS PROC WILL CONVERT A NUMBER (4 BYTES) FROM BINARY TO PRINTABLE
*   CHARACTERS. THE RESULT IS RETURNED WITH LEADING 0'S BLANKED OUT.
*   A MAXIMUM OF 8 CHARACTERS IS OUTPUT. IF THE NUMBER OF SIGNIFICANT
*   DIGITS IS GREATER THAN THE SPACE ALLOWED FOR OUTPUT, '*' S WILL
*   BE PRINTED.                                                    0207
*      PARAMETERS:                                                 0207
*        NUMBER = NUMBER OF CHARACTER TO BE PRINTED (MAX 8)        0207
*        BINWORD = NUMBER TO BE CONVERTED                          0207
*        ADDNXT = ADDRESS OF OUTPUT                                0207
*                                                                  0207
******************************************************************** */
*                                                                  0207
*   RESPECIFY                                                      0207
*    (R6,                                                          0207
*     R8,                                                          0207
*     R9) RESTRICTED;               /*                       @D35UGGB*/
*CONVBIN:                                                          0208
*   PROC;                           /*                       @D35UGGB*/
CONVBIN  STM   @14,@12,@SA00002                                    0208
*   R8=BINWORD;                     /* GET NUMBER TO CONVERT @D35UGGB*/
         L     R8,BINWORD                                          0209
*   GEN(CVD R8,DBLWORK);            /* CONVERT FROM ABINARY TO PACKED
*                                      DECIMAL                       */
         CVD R8,DBLWORK
*   UNPK(WORK(1:8),DBLWORK);        /* FROM PACKED TO UNPACKED     0211
*                                                            @D35UGGB*/
         UNPK  WORK(8),DBLWORK(8)                                  0211
*   WORK(8)=WORK(8)×FOXZERO;        /* CHANGE THE SIGN BIT   @D35UGGB*/
         OI    WORK+7,X'F0'                                        0212
*   SWITCH=OFF;                     /*                       @D35UGGB*/
         NI    SWITCH,B'01111111'                                  0213
*   DO TEMP1=1 BY 1 TO 7 WHILE(SWITCH=OFF);/* DON'T TEST LAST CHAR   */
         LA    @10,1                                               0214
         ST    @10,TEMP1                                           0214
@DL00214 TM    SWITCH,B'10000000'                                  0214
         BNZ   @DC00214                                            0214
*     IF WORKOVER(TEMP1)=FOXZERO THEN/* TEST FOR LEADING 0'S         */
         LA    @03,WORKOVER-1(@10)                                 0215
         CLI   0(@03),X'F0'                                        0215
         BNE   @RF00215                                            0215
*       WORKOVER(TEMP1)=BLANK;      /* MOVE IN BLANK         @D35UGGB*/
         LA    @03,WORKOVER-1(@10)                                 0216
         MVI   0(@03),X'40'                                        0216
*     ELSE                                                         0217
*       SWITCH=ON;                  /* END OF LEADING 0'S    @D35UGGB*/
         B     @RC00215                                            0217
@RF00215 OI    SWITCH,B'10000000'                                  0217
*   END;                            /*                       @D35UGGB*/
@RC00215 LA    @10,1                                               0218
         AL    @10,TEMP1                                           0218
         ST    @10,TEMP1                                           0218
         C     @10,@CF00081                                        0218
         BNH   @DL00214                                            0218
@DC00214 DS    0H                                                  0219
*   IF SWITCH=ON THEN               /* POINT INDEX TEMP1 TO  @D35UGGB*/
         TM    SWITCH,B'10000000'                                  0219
         BNO   @RF00219                                            0219
*     TEMP1=TEMP1-1;                /* FIRST NON - ZERO DIGIT      0220
*                                                            @D35UGGB*/
         L     @10,TEMP1                                           0220
         BCTR  @10,0                                               0220
         ST    @10,TEMP1                                           0220
*   ELSE                                                           0221
*     ;                             /*                       @D35UGGB*/
@RF00219 DS    0H                                                  0222
*   IF(NINE-TEMP1)>NUMBER THEN      /* NO. TO BIG FOR OUTPUT SPACE   */
         LA    @10,9                                               0222
         SL    @10,TEMP1                                           0222
         C     @10,NUMBER                                          0222
         BNH   @RF00222                                            0222
*     WORK='********';              /* PUT OUT * FOR OVERFLOW      0223
*                                                            @D35UGGB*/
         MVC   WORK(8),@CC01399                                    0223
*   ELSE                                                           0224
*     ;                             /*                       @D35UGGB*/
@RF00222 DS    0H                                                  0225
*   R8=ADDR(WORK)+(8-NUMBER);       /* STARTING ADDRESS OF MOVE    0225
*                                                            @D35UGGB*/
         LA    R8,WORK                                             0225
         L     @10,NUMBER                                          0225
         LA    @03,8                                               0225
         SLR   @03,@10                                             0225
         ALR   R8,@03                                              0225
*   R9=NUMBER-1;                    /* LENGTH FOR MVC INSTRUCTION  0226
*                                                            @D35UGGB*/
         BCTR  @10,0                                               0226
         LR    R9,@10                                              0226
*   R6=ADDNXT;                      /* ADDRESS OF OUTPUT     @D35UGGB*/
         L     R6,ADDNXT                                           0227
*   GEN(EX R9,MVCINS);              /* PERFORM MOVE OF OUTPUT      0228
*                                                            @D35UGGB*/
         EX R9,MVCINS
*   GENERATE DATA;                                                 0229
*   RESPECIFY                                                      0230
*    (R6,                                                          0230
*     R8,                                                          0230
*     R9) UNRESTRICTED;             /*                       @D35UGGB*/
*   END CONVBIN;                    /*                       @D35UGGB*/
@EL00002 DS    0H                                                  0231
@EF00002 DS    0H                                                  0231
@ER00002 LM    @14,@12,@SA00002                                    0231
         BR    @14                                                 0231
