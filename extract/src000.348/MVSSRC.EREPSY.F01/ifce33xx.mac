*                                                                  0249
*/*********************************************************************
*                                                                  0249
*   THIS PROC WILL CONVERT HEX NUMBERS TO CHARACTER FORMAT.        0249
*   NUMBER = NUMBER OF BYTES TO CONVERT                            0249
*   NEXTBY = ADDRESS OF DATA TO BE CONVERTED                       0249
*   ADDNXT = ADDRESS OF RESULT                                     0249
*                                                                  0249
**********************************************************************
*                                                                    */
*                                                                  0249
*   RESPECIFY                                                      0249
*    (R8,                                                          0249
*     R9) RESTRICTED;               /*                       @D35UGGB*/
*HEX:                                                              0250
*   PROC;                           /*                       @D35UGGB*/
HEX      STM   @14,@12,12(@13)                                     0250
*   DO Z=NUMBER BY-1 TO 1;          /*                       @D35UGGB*/
         L     @10,NUMBER                                          0251
         ST    @10,Z                                               0251
         B     @DE00251                                            0251
@DL00251 DS    0H                                                  0252
*     R8=NEXTBYTE(Z);               /* PICK UP NEXT BYTE TO CONVERT
*                                                            @D35UGGB*/
         L     @06,NEXTBY                                          0252
         BCTR  @06,0                                               0252
         SLR   R8,R8                                               0252
         IC    R8,NEXTBYTE(@10,@06)                                0252
*     X=2*Z;                        /* OUTPUT INDEX MUST BE TWICE  0253
*                                      INPUT                 @D35UGGB*/
         LR    @06,@10                                             0253
         ALR   @06,@06                                             0253
         ST    @06,X                                               0253
*     SRDL(R8,4);                   /* SPLIT THE BYTE BETWEEN 2    0254
*                                      REGISTERS             @D35UGGB*/
         SRDL  R8,4                                                0254
*     SRL(R9,28);                   /* RIGHT JUSTIFY DATA    @D35UGGB*/
         SRL   R9,28                                               0255
*     ADDNUM(X-1)=CHARS(R8+1);      /* MOVE CHAR TO PRINT BUFFER     */
         L     @03,ADDNXT                                          0256
         ALR   @06,@03                                             0256
         BCTR  @06,0                                               0256
         BCTR  @06,0                                               0256
         LA    @01,CHARS(R8)                                       0256
         MVC   ADDNUM(1,@06),0(@01)                                0256
*     ADDNUM(X)=CHARS(R9+1);        /* DO NOT USE INDEX OF 0 @D35UGGB*/
         L     @06,X                                               0257
         ALR   @03,@06                                             0257
         BCTR  @03,0                                               0257
         LA    @06,CHARS(R9)                                       0257
         MVC   ADDNUM(1,@03),0(@06)                                0257
*   END;                            /*                       @D35UGGB*/
         BCTR  @10,0                                               0258
         ST    @10,Z                                               0258
@DE00251 LTR   @10,@10                                             0258
         BP    @DL00251                                            0258
*   END HEX;                        /*                       @D35UGGB*/
@EL00004 DS    0H                                                  0259
@EF00004 DS    0H                                                  0259
