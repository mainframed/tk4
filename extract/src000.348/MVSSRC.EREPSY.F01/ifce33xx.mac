*EDITTBLE:                                                         0233
*   PROC OPTIONS(SAVE(14));         /*                       @D35UGGB*/
EDITTBLE ST    @14,12(,@13)                                        0233
*   IF OBRDEV=DEV3350×(EMULATE=ON&OBRDEV=DEV3330)×(EMULATE=ON&OBRDEV=
*       DEV33301) THEN              /* 3350,E 3330,E 3330 MOD 11     */
         CLC   OBRDEV(2,R7),DEV3350                                0234
         BE    @RT00234                                            0234
         TM    EMULATE,B'00001000'                                 0234
         BNO   @GL00001                                            0234
         CLC   OBRDEV(2,R7),DEV3330                                0234
         BE    @RT00234                                            0234
@GL00001 TM    EMULATE,B'00001000'                                 0234
         BNO   @RF00234                                            0234
         CLC   OBRDEV(2,R7),DEV33301                               0234
         BNE   @RF00234                                            0234
@RT00234 DS    0H                                                  0235
*     R15=ADDR(F3350)-4;            /*                       @D35UGGB*/
         LA    R15,F3350                                           0235
         SL    R15,@CF00062                                        0235
*   ELSE                                                           0236
*     IF OBRDEV=DEV3340&EMULATE=ON THEN/* DEVICE IS 3344     @D35UGGB*/
         B     @RC00234                                            0236
@RF00234 CLC   OBRDEV(2,R7),DEV3340                                0236
         BNE   @RF00236                                            0236
         TM    EMULATE,B'00001000'                                 0236
         BNO   @RF00236                                            0236
*       R15=ADDR(F3344)-4;          /*                       @D35UGGB*/
         LA    R15,F3344                                           0237
         SL    R15,@CF00062                                        0237
*     ELSE                                                         0238
*       IF OBRDEV=DEV3340 THEN      /* DEVICE IS 3340        @D35UGGB*/
         B     @RC00236                                            0238
@RF00236 CLC   OBRDEV(2,R7),DEV3340                                0238
         BNE   @RF00238                                            0238
*         R15=ADDR(F3340)-4;        /*                       @D35UGGB*/
         LA    R15,F3340                                           0239
         SL    R15,@CF00062                                        0239
*       ELSE                                                       0240
*         IF OBRDEV=DEV3330×OBRDEV=DEV33301 THEN                   0240
         B     @RC00238                                            0240
@RF00238 CLC   OBRDEV(2,R7),DEV3330                                0240
         BE    @RT00240                                            0240
         CLC   OBRDEV(2,R7),DEV33301                               0240
         BNE   @RF00240                                            0240
@RT00240 DS    0H                                                  0241
*           R15=ADDR(F3330)-4;      /* DEVICE IS 3330 ANY MOD      0241
*                                                            @D35UGGB*/
         LA    R15,F3330                                           0241
         SL    R15,@CF00062                                        0241
*         ELSE                                                     0242
*           IF OBRDEV=DEVWOOD THEN                                 0242
         B     @RC00240                                            0242
@RF00240 CLC   OBRDEV(2,R7),DEVWOOD                                0242
         BNE   @RF00242                                            0242
*             R15=ADDR(WOODY)-4;    /* DEVICE IS 88--                */
         LA    R15,WOODY                                           0243
         SL    R15,@CF00062                                        0243
*           ELSE                                                   0244
*             IF OBRDEV=DEV3370 THEN                               0244
         B     @RC00242                                            0244
@RF00242 CLC   OBRDEV(2,R7),DEV3370                                0244
         BNE   @RF00244                                            0244
*               R15=ADDR(D3370)-4;  /*                       @D3506GB*/
         LA    R15,D3370                                           0245
         SL    R15,@CF00062                                        0245
*             ELSE                                                 0246
*               R15=0;              /* DEVICE IS UNKNOWN     @D35UGGB*/
         B     @RC00244                                            0246
@RF00244 SLR   R15,R15                                             0246
*   END EDITTBLE;                   /*                       @D35UGGB*/
@EL00003 DS    0H                                                  0247
@EF00003 DS    0H                                                  0247
@ER00003 L     @14,12(,@13)                                        0247
