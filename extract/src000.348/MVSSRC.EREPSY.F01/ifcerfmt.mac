*/* FUNCTION-                                                      0404
*       ROUTINE TO CLEAN UP RESOURCES AND PRINT AN ABEND MESSAGE FOR
*     ANY ABEND. THIS ALLOWS AMDPRDMP TO CONTINUE PROCESSING EVEN IF
*     THE LOGDATA FORMATTER ABENDS.                                0404
*                                                                  0404
*    CALLS-                                                        0404
*         MAKES NO OUTSIDE CALLS                                   0404
*                                                                  0404
*    CALLED BY -                                                   0404
*         RTM IN AN ABEND SITUATION                                0404
*                                                                  0404
*    RETURNS TO -                                                  0404
*         CALLER                                                   0404
*                                                                    */
*/********************************************************************/
*                                                                  0404
*   /*****************************************************************/
*   /*                                                               */
*   /* NO RESTORE REGS SPECIFIED                                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0404
*   DO;                             /* ENTESTAE PARMREG(DREG)        */
*/*  ENTRY CODE FOR ESTAE ROUTINE.                                   */
*/*  IF RTCA ALLOCATED THEN RESTORE CODE AND                         */
*/*  DATA REGS FROM START OF USERS PARM LIST,                        */
*/*  AND ESTABLISH ADDR TO ESTAE SAVE AREA                           */
*/*  IF REQUIRED.                                                    */
*/*  IF NO RTCA IS AVAILABLE THEN JUST RETURN                        */
*/*  TO RTM TO CONTINUE TERMINATION                                  */
*                                                                  0405
*     GEN(USING *,GPR15P);                                         0405
         USING *,GPR15P
*     GEN(CH    GPR00F,*+14);       /* CHECK IF RTCA ALLOC FAILED    */
         CH    GPR00F,*+14
*     GEN(BNE   *+12);              /* NO,SAVE RTM REGS              */
         BNE   *+12
*     GEN(DROP  GPR15P);            /* DROP LOCAL ADDR               */
         DROP  GPR15P
*     GEN(LA    GPR15P,0);          /* YES, SET CONT TERM RC         */
         LA    GPR15P,0
*     GEN(BR    GPR14P);            /* RETURN TO RTM                 */
         BR    GPR14P
*     GEN(DC    H'12');             /* NO RTCA AVAIL RC              */
         DC    H'12'
*     GEN(STM   GPR14P,GPR12F,12(GPR13P));/* GOOD R13, SAVE RTM'S  0412
*                                      REGS                          */
         STM   GPR14P,GPR12F,12(GPR13P)
*     GEN(L     DREG,0(GPR01P));    /* PTR TO USER PARM LIST         */
         L     DREG,0(GPR01P)
*   END;                            /* ENTESTAE PARMREG(DREG)        */
*                                                                  0414
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS CODE SETS DREG TO BE THE POINTER TO ALL THE AUTOMATIC    */
*   /* DATA FOR THIS MODULE                                          */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0415
*   LM(GPR02P,GPR12F,REG2TO12);     /* RESTORE THE REGS SAVED FROM 0415
*                                      THE MAINLINE                  */
         LM    GPR02P,GPR12F,REG2TO12                              0415
*   SDWAPTR=GPR01P;                 /* SDWA ADDR PASSED TO THIS    0416
*                                      ROUTINE                       */
*                                                                  0416
         ST    GPR01P,SDWAPTR                                      0416
*   /*****************************************************************/
*   /*                                                               */
*   /* TO PREVENT MORE THAN ONE DUMP FROM BEING TAKEN FOR AN X37 TYPE*/
*   /* ABEND, PERCOLATE TO THE CALLERS RECOVERY. THIS WILL PREVENT   */
*   /* THIS ESTAE FROM TAKING A DUMP AND WILL LET THE CALLERS        */
*   /* RECOVERY TAKE IT.                                     @ZM43805*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0417
*   IF ABNDTYPE^='37'X THEN         /* PERCOLATE FOR ALL X37 TYPE  0417
*                                      ABEND CODES           @ZM43805*/
         L     @14,SDWAPTR                                         0417
         MVC   @TS00001(2),ABNDTYPE(@14)                           0417
         NC    @TS00001(2),@CB02579                                0417
         CLC   @TS00001(2),@CB02581                                0417
         BE    @RF00417                                            0417
*     DO;                           /* NOT AN X37 TYPE ABEND @ZM43805*/
*       IF UNEXP2=OFF THEN          /* THE FIRST UNEXPECTED ERROR    */
         TM    UNEXP2,B'10000000'                                  0419
         BNZ   @RF00419                                            0419
*         DO;                                                      0420
*           UNEXP2=ON;              /* RETRY ONLY ONCE - PREVENT LOOP*/
         OI    UNEXP2,B'10000000'                                  0421
*           RTYADR=ADDR(TROUBLE);   /* RETRY POINT IN MAIN LINE FOR
*                                      UNEXPECTED ERRORS             */
         LA    @14,TROUBLE                                         0422
         ST    @14,RTYADR                                          0422
*           SDWASRSV=RTYREGS;       /* SET RETRY REGS FOR MAINLINE 0423
*                                      RETRY                         */
         L     @02,SDWAPTR                                         0423
         MVC   SDWASRSV(64,@02),RTYREGS                            0423
*           ABDPLPTR=RTYREG1;       /* RESTORE PTR TO PRINT DUMP   0424
*                                      PARMLIST-REG1                 */
         L     ABDPLPTR,RTYREG1                                    0424
*           BUFFER(1:LENGTH(ENDMSG))=ENDMSG;/* ABEND MESSAGE         */
         L     @09,ADPLBUF(,ABDPLPTR)                              0425
         MVC   BUFFER(45,@09),ENDMSG                               0425
*           PACKAREA=ABNDCMPC;      /* MOVE COMP CODE TO CHAR AREA   */
         MVC   PACKAREA(2),ABNDCMPC(@02)                           0426
*           GARBAGE='04'X;          /* GARBAGE BYTE FOR THE UNPK     */
         MVI   GARBAGE,X'04'                                       0427
*           UNPK(BUFFER(46:50),UNPACK);/* UNPACK INTO THE BUFFER     */
         UNPK  BUFFER+45(5,@09),UNPACK(3)                          0428
*           TR(BUFFER(46:50),TRANS);/* TRANSLATE TO PRINTABLE HEX    */
         TR    BUFFER+45(5,@09),TRANS                              0429
*           BUFFER(49:49)=' ';      /* BLANK OUT SECOND BYTE         */
         MVI   BUFFER+48(@09),C' '                                 0430
*           GPR01P=SDWAPTR;         /* RESTORE REGISTER 1 FOR SETRP  */
         LR    GPR01P,@02                                          0431
*           DO;                     /* SETRP REGS(14,12)DUMP(YES)RC(4
*                                      )RETADDR(RETRYPT)RETREGS(YES)F
*                                      RESDWA(YES)                   */
*             RESPECIFY                                            0433
*              (GPR00P,                                            0433
*               GPR01P,                                            0433
*               GPR14P,                                            0433
*               GPR15P) RSTD;                                      0433
*             GPR01P->SDWARCDE=4;   /* STORE RC INTO SDWA            */
         MVI   SDWARCDE(GPR01P),X'04'                              0434
*             GPR01P->SDWARTYA=ADDR(RETRYPT);/* SAVE RETRY ADDRESS   */
         L     @02,RTYADR                                          0435
         ST    @02,SDWARTYA(,GPR01P)                               0435
