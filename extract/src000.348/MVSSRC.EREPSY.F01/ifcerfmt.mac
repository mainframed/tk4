         TITLE 'IFCERFMT IN-STORAGE LOGREC BUFFER FORMAT MODULE'
IFCERFMT CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'IFCERFMT  79.010'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @12,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@12                                         0001
         L     @00,@SIZDATD                                        0001
         GETMAIN  R,LV=(0)
         LR    @03,@01                                             0001
         LA    @10,4095(,@03)                                      0001
         LA    @11,4095(,@10)                                      0001
         USING @DATD,@03                                           0001
         USING @DATD+4095,@10                                      0001
         USING @DATD+8190,@11                                      0001
         ST    @13,@SA00001+4                                      0001
         LM    @00,@01,20(@13)                                     0001
         ST    @03,8(,@13)                                         0001
         LR    @13,@03                                             0001
