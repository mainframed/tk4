*/********************************************************************/
*/* FUNCTION-                                                      0139
*       ROUTINE TO SET UP THE ESTAE, DETERMINES IF THE RUNNING SYSTEM
*     HAS THE EREP ENHANCEMENT SU, FILLS IN THE EREP COMMUNICATIONS
*     TABLE, AND PASSES CONTROL TO EREP TO EDIT THE RECORDS, IF ALL
*     THE RECORDS WERE FOUND WITHOUT PROBLEM.                      0139
*                                                                  0139
*       MESSAGES ISSUED BY THIS ROUTINE ARE:                       0139
*                                                                  0139
*               *** IN STORAGE LOGREC BUFFER NOT FORMATTED, EREP   0139
*                   ENHANCEMENT SU NOT AVAILABLE ON SYSTEM ***     0139
*          (IF SU 27 IS NOT ON THE SYSTEM)                         0139
*                                                                  0139
*               *** IN STORAGE LOGREC BUFFER NOT FORMATTED, PARAMETER
*                   LIST DID NOT CONTAIN NECESSARY INFORMATION *** 0139
*          (IF THE CVT POINTER IS ZERO)                            0139
*                                                                  0139
*               *** LOGDATA FORMATTING OMITTED, DUMP IS NOT FROM AN
*                   MVS SYSTEM ***                                 0139
*          (IF THE OPERATING SYSTEM FLAGS COULD NOT BE ACCESSED OR IF
*           THE SYSTEM IS NOT MVS)                                 0139
*                                                                  0139
*               *** IN-STORAGE LOGREC BUFFER NOT FORMATTED, DATA   0139
*                   NECESSARY TO LOCATE THE BUFFER UNAVAILABLE *** 0139
*          (IF THE RTCT COULD NOT BE ACCESSED OR THE ADDRESS IS    0139
*           NOT ON A 4K BOUNDARY)                                  0139
*                                                                  0139
*                                                                  0139
*    CALLS-                                                        0139
*         ACCESRTN, CPUID, DEBLOCK, IFCEDSUM, AND PRINT            0139
*                                                                  0139
*    CALLED BY -                                                   0139
*         AMDPRDMP WHEN THE LOGDATA VERB IS ENCOUNTERED            0139
*                                                                  0139
*    RETURNS TO -                                                  0139
*         CALLER WITH A RETURN CODE OF ZERO                        0139
*                                                                    */
*/********************************************************************/
*                                                                  0139
*   /*****************************************************************/
*   /*                                                               */
*   /* ESTABLISH THE ESTAE                                           */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0139
*   ESTAERR=ZERO;                   /* TURN OFF ESTAE BITS   @ZM45067*/
         SLR   @14,@14                                             0139
         ST    @14,ESTAERR                                         0139
*   STM(VADR,RCREG,RTYREGS);        /* SAVE REGS TO BE USED IN THE 0140
*                                      ESTAE                 @ZM45067*/
         STM   VADR,RCREG,RTYREGS                                  0140
*   SAVEREG1=ABDPLPTR;              /* SAVE REGISTER 1 ACCESS THE  0141
*                                      ESTAE                         */
         LR    SAVEREG1,ABDPLPTR                                   0141
*   ESTAEXEC(1:ESTALEN)=ESTALIST(1:ESTALEN);/* MOVE LIST INTO      0142
*                                      AUTOMATIC AREA                */
         L     @14,ESTALEN                                         0142
         BCTR  @14,0                                               0142
         EX    @14,@SM02572                                        0142
*   GEN FLOWS(ESTAERTN)(ESTAE ESTAERTN,PARAM=(DREG),MF=(E,ESTAEXEC));
         ESTAE ESTAERTN,PARAM=(DREG),MF=(E,ESTAEXEC)
*                                   /* THE PARM, FOR THIS ESTAE,   0144
*                                      POINTS TO ALL THE AUTOMATIC 0144
*                                      DATA FOR THIS MODULE          */
*   IF RCREG=ZERO THEN              /* ESTAE RESOLVED FINE           */
         LTR   RCREG,RCREG                                         0144
         BNZ   @RF00144                                            0144
*     DO;                                                          0145
*       ABDPLPTR=SAVEREG1;          /* RESTORE REGISTER 1            */
         LR    ABDPLPTR,SAVEREG1                                   0146
*       EREPBUF=BUFFER;             /* USE PRINT DUMPS BLANK BUFFER
*                                      TO BLANK OUT EREPS OUTPUT   0147
*                                      BUFFER                        */
*                                                                  0147
         MVI   EREPBUF+120,C' '                                    0147
         MVC   EREPBUF+121(13),EREPBUF+120                         0147
         L     @14,ADPLBUF(,ABDPLPTR)                              0147
         MVC   EREPBUF(120),BUFFER(@14)                            0147
*       /*************************************************************/
*       /*                                                           */
*       /* PUT OUT HEADER                                            */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0148
*       CALL PRINT;                 /* BLANK LINE                    */
         L     @15,ADPLPRNT(,ABDPLPTR)                             0148
         BALR  @14,@15                                             0148
*       BUFFER(51:LENGTH(HDRLINE)+50)=HDRLINE;/* LOGDATA HDR         */
         L     @14,ADPLBUF(,ABDPLPTR)                              0149
         MVC   BUFFER+50(31,@14),HDRLINE                           0149
*       CALL PRINT;                 /* PRINT IT                      */
         L     @15,ADPLPRNT(,ABDPLPTR)                             0150
         BALR  @14,@15                                             0150
*       CALL PRINT;                 /* SPACE A LINE                  */
         L     @15,ADPLPRNT(,ABDPLPTR)                             0151
         BALR  @14,@15                                             0151
*       CALL PRINT;                 /* SPACE A LINE                  */
*                                                                  0152
         L     @15,ADPLPRNT(,ABDPLPTR)                             0152
         BALR  @14,@15                                             0152
*       /*************************************************************/
*       /*                                                           */
*       /* CHECK IF EREP ENHANCEMENT SU IS ON THE RUNNING SYSTEM,    */
*       /* (NOT THE SYSTEM ON WHICH THE DUMP WAS TAKEN               */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0153
*       CVTPTR=LOC16;               /* REAL RUNNING SYSTEM CVT ADDR  */
         L     CVTPTR,LOC16                                        0153
*       IF SU27BIT=OFF THEN         /* EREP ENHANCEMENT SU NOT ON  0154
*                                                              SYSTEM*/
         L     @14,CVTIHASU(,CVTPTR)                               0154
         TM    SU27BIT(@14),B'00010000'                            0154
         BNZ   @RF00154                                            0154
*         BUFFER(1:LENGTH(NOSU27))=NOSU27;/* ERROR MESSAGE           */
         L     @14,ADPLBUF(,ABDPLPTR)                              0155
         MVC   BUFFER(91,@14),NOSU27                               0155
*       ELSE                        /* EREP ENHANCEMENT IS ON SYSTEM */
*         DO;                                                      0156
*                                                                  0156
         B     @RC00154                                            0156
@RF00154 DS    0H                                                  0157
*           /*********************************************************/
*           /*                                                       */
*           /* CHECK IF A CVT ADDRESS WAS PASSED IN THE PARMLIST     */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0157
*           IF ADPLCVT=ZERO THEN    /* NO CVT ADDRESS PASSED         */
         ICM   @14,15,ADPLCVT(ABDPLPTR)                            0157
         BNZ   @RF00157                                            0157
*             BUFFER(1:LENGTH(NOINF))=NOINF;/* ERROR MESSAGE         */
         L     @14,ADPLBUF(,ABDPLPTR)                              0158
         MVC   BUFFER(100,@14),NOINF                               0158
*           ELSE                    /* A CVT IS PASSED               */
*             DO;                                                  0159
*                                                                  0159
         B     @RC00157                                            0159
@RF00157 DS    0H                                                  0160
*               /*****************************************************/
*               /*                                                   */
*               /* DETERMINE IF THE SYSTEM ON WHICH THE DUMP WAS     */
*               /* TAKEN WAS AN MVS SYSTEM                           */
*               /*                                                   */
*               /*****************************************************/
*                                                                  0160
*               CVTPTR=ADPLCVT;     /* REFERENCED CVT IS NOW THE DUMP
*                                      CVT                           */
         L     CVTPTR,ADPLCVT(,ABDPLPTR)                           0160
*               VADR=ADDR(CVTDCB);  /* OPERATING SYSTEM FLAGS        */
         LA    VADR,CVTDCB(,CVTPTR)                                0161
*               CALL ACCESRTN;      /* ACCESS IT                     */
         BAL   @14,ACCESRTN                                        0162
*               IF RCREG^=ZERO THEN /* ACCESS ERROR                  */
         LTR   RCREG,RCREG                                         0163
         BZ    @RF00163                                            0163
*                 BUFFER(1:LENGTH(INCOMP))=INCOMP;/* ERROR MSG       */
         L     @14,ADPLBUF(,ABDPLPTR)                              0164
         MVC   BUFFER(66,@14),INCOMP                               0164
*               ELSE                /* FOUND OPERATING SYSTEM FLAGS  */
*                 DO;                                              0165
         B     @RC00163                                            0165
@RF00163 DS    0H                                                  0166
*                   IF ADPLSYTM=ON×(DATABYTE&LOWBIT)^=LOWBIT THEN/*
*                                      NOT AN MVS SYSTEM             */
         TM    ADPLSYTM(ABDPLPTR),B'01000000'                      0166
         BO    @RT00166                                            0166
         SLR   @14,@14                                             0166
         IC    @14,LOWBIT                                          0166
         SLR   @08,@08                                             0166
         IC    @08,DATABYTE(,DATAPTR)                              0166
         NR    @08,@14                                             0166
         CR    @08,@14                                             0166
         BE    @RF00166                                            0166
@RT00166 DS    0H                                                  0167
*                     BUFFER(1:LENGTH(INCOMP))=INCOMP;/* ERROR     0167
*                                      MESSAGE                       */
         L     @14,ADPLBUF(,ABDPLPTR)                              0167
         MVC   BUFFER(66,@14),INCOMP                               0167
*                   ELSE            /* IS AN MVS SYSTEM              */
*                     DO;                                          0168
*                                                                  0168
         B     @RC00166                                            0168
@RF00166 DS    0H                                                  0169
*                       /*********************************************/
*                       /*                                           */
*                       /* LOCATE THE IN-STORAGE LOGREC BUFFER THE   */
*                       /* BUFFER CONSISTS OF THREE PARTS, THE RCB   */
*                       /* WHICH IS THE HEADER FOR THE BUFFER. IT    */
*                       /* INDICATES THE BEGINNING ADDRESS OF THE    */
*                       /* BUFFER, THE END ADDRESS, AND THE POINTER  */
*                       /* TO THE FIRST FREE ADDRESS. BEFORE EACH    */
*                       /* ENTRY IN THE BUFFER, THERE ARE TWO        */
*                       /* HEADERS. THE FIRST, THE BEHDR, HAS CONTROL*/
*                       /* INFORMATION ABOUT THE RECORD. THE         */
*                       /* SECOND,THE LRB, HAS INFORMATION ABOUT THE */
*                       /* LOGREC ENTRY ITSELF.                      */
*                       /*                                           */
*                       /*********************************************/
*                                                                  0169
*                       VADR=ADDR(CVTRTMCT);/* PTR TO RTCT           */
         LA    VADR,CVTRTMCT(,CVTPTR)                              0169
*                       CALL ACCESRTN;/* ACCESS IT                   */
         BAL   @14,ACCESRTN                                        0170
*                       IF RCREG^=ZERO THEN/* COULD NOT ACCESS CVT   */
         LTR   RCREG,RCREG                                         0171
         BZ    @RF00171                                            0171
*                         BUFFER(1:LENGTH(LOCERR))=LOCERR;/* ERROR 0172
*                                      MESSAGE                       */
         L     @14,ADPLBUF(,ABDPLPTR)                              0172
         MVC   BUFFER(95,@14),LOCERR                               0172
*                       ELSE        /* FOUND RTCT PTR                */
*                         DO;                                      0173
         B     @RC00171                                            0173
@RF00171 DS    0H                                                  0174
*                           RTCTPTR=DATAWORD;/* SAVE THE RTCT ADDRESS*/
         L     @14,DATAWORD(,DATAPTR)                              0174
         ST    @14,RTCTPTR                                         0174
*                           VADR=ADDR(RTCTRCB);/* PTR TO IN-STORAGE
*                                      LOGREC BUFFER                 */
         LA    VADR,RTCTRCB(,@14)                                  0175
*                           CALL ACCESRTN;/* ACCESS IT               */
         BAL   @14,ACCESRTN                                        0176
*                           IF(RCREG^=ZERO)×((DATAWORD&FOURKBDY)^= 0177
*                               DATAWORD) THEN/* EITHER THE RTCT   0177
*                                      COULD NOT BE ACCESSED OR THE
*                                      POINTER TO THE BUFFER IS BAD  */
         LTR   RCREG,RCREG                                         0177
         BNZ   @RT00177                                            0177
         L     @14,DATAWORD(,DATAPTR)                              0177
         LR    @08,@14                                             0177
         N     @08,FOURKBDY                                        0177
         CLR   @08,@14                                             0177
         BE    @RF00177                                            0177
@RT00177 DS    0H                                                  0178
*                             BUFFER(1:LENGTH(LOCERR))=LOCERR;/*   0178
*                                      ERROR MESSAGE                 */
         L     @14,ADPLBUF(,ABDPLPTR)                              0178
         MVC   BUFFER(95,@14),LOCERR                               0178
*                           ELSE    /* FOUND IN-STORAGE LOGREC BUFFER*/
*                             DO;                                  0179
         B     @RC00177                                            0179
@RF00177 DS    0H                                                  0180
*                               RECPTR=DATAWORD;/* SAVE BUFFER     0180
*                                      ADDRESS                       */
         L     RECPTR,DATAWORD(,DATAPTR)                           0180
*                               CALL CPUID;/* DETERMINE THE VALID CPU
*                                      IDS                           */
         BAL   @14,CPUID                                           0181
*                               IF RCREG=ZERO THEN/* FOUND CPU IDS OK*/
         LTR   RCREG,RCREG                                         0182
         BNZ   @RF00182                                            0182
*                                 DO;                              0183
*                                   CALL DEBLOCK;/* ROUTINE TO     0184
*                                      DEBLOCK THE BUFFER AND FILL IN
*                                      THE SORT TABLE                */
         BAL   @14,DEBLOCK                                         0184
*                                   IF SORTINDX^=ZERO THEN/* THERE 0185
*                                      ARE RECORDS TO BE EDITED      */
         L     @14,SORTINDX                                        0185
         LTR   @14,@14                                             0185
         BZ    @RF00185                                            0185
*                                    DO;                           0186
*                                                                  0186
*                                     /*******************************/
*                                     /*                             */
*                                     /* FILL IN COMMUNICATIONS TABLE*/
*                                     /* FOR USE BY THE EREP MODULES */
*                                     /*                             */
*                                     /*******************************/
*                                                                  0187
*                                     PARMP1=''B;/* ZERO OUT FIRST 0187
*                                      PART OF TABLE                 */
         XC    PARMP1(255),PARMP1                                  0187
*                                     RFY                          0188
*                                      (PARMPTR) RSTD;/*     @G33DGVS*/
*                                     PARMPTR=LENGTH(PARM)-256;/*  0189
*                                                            @G33DGVS*/
         LA    PARMPTR,92                                          0189
*                                     PARM(PARMPTR:LENGTH(PARM))=PARM(
*                                         PARMPTR:LENGTH(PARM))&&PARM(
*                                         PARMPTR:LENGTH(PARM));/* 0190
*                                                            @G33DGVS*/
         LA    @08,PARM-1(PARMPTR)                                 0190
         LA    @07,348                                             0190
         SLR   @07,PARMPTR                                         0190
         EX    @07,@SX02574                                        0190
*                                     RFY                          0191
*                                      (PARMPTR) UNRSTD;/*   @G33DGVS*/
*                                     RECTYPE=ALLON;/* ALL RECORD  0192
*                                      TYPES WANTED                  */
         MVC   RECTYPE(2),ALLON                                    0192
*                                     DEBUG19=ON;/* BREAK FOR NEW  0193
*                                      EDIT TABLE AFTER EACH RECORD  */
         OI    DEBUG19,B'00010000'                                 0193
*                                     SHORTOBR=ON;/* SHORT OBR     0194
*                                      RECORDS WANTED                */
*                                     NOSDR=ON;/* DUMP OF SDR NOT  0195
*                                      WANTED                        */
         OI    SHORTOBR,B'00100001'                                0195
*                                     PEDIT=ON;/* RECORDS ARE TO BE
*                                      EDITED                        */
         OI    PEDIT,B'00010000'                                   0196
*                                     TABSIZE=LENGTH(SORTABLE);/*  0197
*                                      FILL IN SIZE OF THE SORT TABLE*/
         MVC   TABSIZE(4),@CF02569                                 0197
*                                     SORTNUM=SORTINDX;/* NUMBER OF
*                                      RECORDS FOR THE EDITOR TO   0198
*                                      FORMAT                        */
         ST    @14,SORTNUM                                         0198
*                                     ADIOHND=ADDR(IFCFMTIO);/* I/O
*                                      ROUTINE USED BY EREP          */
         L     @14,@CV02051                                        0199
         ST    @14,ADIOHND                                         0199
*                                     ADIFCMSG=ADDR(IFCFMTIO);/* MSG
*                                      HANDLER USED BY EREP          */
         L     @14,@CV02051                                        0200
         ST    @14,ADIFCMSG                                        0200
*                                     SORTADR=ADDR(SORTABLE);/* PTR
*                                      TO SORT TABLE                 */
         LA    @14,SORTABLE                                        0201
         ST    @14,SORTADR                                         0201
*                                     ADEDITB=ADDR(EDITLOAD);/* PTR
*                                      TO TABLE OF LOADS             */
         LA    @14,EDITLOAD                                        0202
         ST    @14,ADEDITB                                         0202
*                                     PRINTADR=ADDR(EREPBUF);/*    0203
*                                      OUTPUT BUFFER USED BY THE EREP
*                                      ROUTINES                      */
         LA    @14,EREPBUF                                         0203
         ST    @14,PRINTADR                                        0203
*                                     LINECT=PAGEMAX;/* SET NUM LINES
*                                      FOR PAGE                      */
         MVC   LINECT(2),@CH02077                                  0204
*                                     PDMPLIST=ABDPLPTR;/* SAVE PTR
*                                      TO IHAABDPL                   */
         ST    ABDPLPTR,PDMPLIST                                   0205
*                                     UNWRPBUF=ADDR(NEWBUF);/* SAVE
*                                      PTR TO READ BUFFER            */
         LA    @14,NEWBUF                                          0206
         ST    @14,UNWRPBUF                                        0206
*                                     ENDADR=BUFEND;/* END OF PRINT
*                                      DUMPS COPY OF THE IN-STORAGE
*                                      LOGREC BUFFER                 */
         MVC   ENDADR(4),BUFEND                                    0207
*                                     BEGINADR=BUFSTART;/* START OF
*                                      PRINT DUMPS COPY OF THE     0208
*                                      IN-STORAGE LOGREC BUFFER      */
         MVC   BEGINADR(4),BUFSTART                                0208
*                                     RFY                          0209
*                                      (PARMPTR) RSTD;             0209
*                                     PARMPTR=ADDR(PARM);/* PASS THE
*                                      COMMUNICATIONS TABLE ADDRESS
*                                      IN REG 2                      */
*                                                                  0210
         LA    PARMPTR,PARM                                        0210
*                                     /*******************************/
*                                     /*                             */
*                                     /* FORMAT ALL VALID ENTRIES IN */
*                                     /* THE LOGREC BUFFER. (NOTE    */
*                                     /* THAT IFCFMTIO WILL BE CALLED*/
*                                     /* BY THE EREP EDITOR TO PUT   */
*                                     /* EACH FORMATTED LINE AND     */
*                                     /* ERROR MESSAGE INTO THE DUMP */
*                                     /* OUTPUT                      */
*                                     /*                             */
*                                     /*******************************/
*                                                                  0211
*                                     GEN FLOWS(IFCEDSUM)          0211
*                                         (LINK EP=IFCEDSUM);      0211
         LINK EP=IFCEDSUM
*                                    END;/* END THERE ARE RECORDS FOR
*                                      EDIT                          */
*                                   ELSE                           0213
*                                    ;/* NO RECORDS TO BE EDITED     */
@RF00185 DS    0H                                                  0214
*                                 END;/* END FOUND CPU IDS OK        */
*                               ELSE                               0215
*                                 ; /* COULD NOT DETERMINE VALID CPU
*                                      IDS                           */
@RF00182 DS    0H                                                  0216
*                             END;  /* END FOUND IN STORAGE BUFFER   */
*                         END;      /* END FOUND RTCT PTR            */
@RC00177 DS    0H                                                  0218
*                     END;          /* END IS AN MVS SYSTEM          */
@RC00171 DS    0H                                                  0219
*                 END;              /* END FOUND OPERATING SYSTEM  0219
*                                      FLAGS                         */
@RC00166 DS    0H                                                  0220
*             END;                  /* END A CVT ADDRESS IS PASSED   */
@RC00163 DS    0H                                                  0221
*         END;                      /* END EREP ENHANCEMENT SU IS ON
*                                                              SYSTEM*/
@RC00157 DS    0H                                                  0222
*TROUBLE:                           /* PLACE TO RETRY AT             */
*       CALL PRINT;                 /* BLANK LINE OR ERROR MESSAGE   */
@RC00154 DS    0H                                                  0222
TROUBLE  L     @15,ADPLPRNT(,ABDPLPTR)                             0222
         BALR  @14,@15                                             0222
*       GEN(ESTAE 0);               /* DROP THE ESTAE                */
         ESTAE 0
*     END;                          /* END ESTAE RESOLVED FINE       */
*   ELSE                                                           0225
*     ;                             /* ERROR ESTABLISHING THE ESTAE  */
@RF00144 DS    0H                                                  0226
*   RFY                                                            0226
*    (PARMPTR) UNRSTD;                                             0226
*   RETURN CODE(0);                                                0227
         L     @13,4(,@13)                                         0227
         L     @00,@SIZDATD                                        0227
         LR    @01,@03                                             0227
         FREEMAIN R,LV=(0),A=(1)
         SLR   @15,@15                                             0227
         L     @14,12(,@13)                                        0227
         LM    @00,@12,20(@13)                                     0227
         BR    @14                                                 0227
*CPUID:                                                            0228
*   PROC OPTIONS(SAVE(14));                                        0228
*                                                                  0228
@EL00001 L     @13,4(,@13)                                         0228
@EF00001 L     @00,@SIZDATD                                        0228
         LR    @01,@03                                             0228
         FREEMAIN R,LV=(0),A=(1)
@ER00001 LM    @14,@12,12(@13)                                     0228
         BR    @14                                                 0228
CPUID    ST    @14,@SA00002                                        0228
*/********************************************************************/
*/* FUNCTION-                                                      0229
*        TO DETERMINE WHAT THE VALID CPU IDS ARE FOR THE DUMP SYSTEM,
*      IN ORDER TO LOCATE THE FIRST VALID RECORD IN THE IN-STORAGE 0229
*      LOGREC BUFFER. THIS ROUTINE IS CURRENTLY SET UP TO HANDLE ONLY
*      2 RUNNING CPUS.                                             0229
*                                                                  0229
*       MESSAGES ISSUED BY THIS ROUTINE ARE:                       0229
*                                                                  0229
*               *** IN STORAGE LOGREC BUFFER NOT FORMATTED, UNABLE 0229
*                   TO DETERMINE VALID CPU ID ***                  0229
*          (COULD NOT ACCESS PCCAVT OR THE PCCA)                   0229
*                                                                  0229
*                                                                  0229
*    CALLS-                                                        0229
*         ACCESRTN AND PRINT                                       0229
*                                                                  0229
*    CALLED BY -                                                   0229
*         MAINLINE TO DETERMINE THE VALID CPU IDS                  0229
*                                                                  0229
*    RETURNS TO -                                                  0229
*         CALLER WITH A RETURN CODE SET BY THE ACCESS SERVICE ROUTINE
*         OR BY THIS ROUTINE, IF THE PCCAVT PTR IS ZERO.             */
*/********************************************************************/
*                                                                  0229
*   /*****************************************************************/
*   /*                                                               */
*   /* ACCESS THE PCCAVT                                             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0229
*   VADR=ADDR(CVTPCCAT);            /* POINTER TO PCCAVT TO OBTAIN 0229
*                                      POINTERS TO ALL VALID PCCA    */
         LA    VADR,CVTPCCAT(,CVTPTR)                              0229
*   CALL ACCESRTN;                  /* ACCESS IT                     */
         BAL   @14,ACCESRTN                                        0230
*   IF(RCREG^=ZERO)×(DATAWORD=ZERO) THEN/* COULD NOT ACCESS IT       */
         SLR   @14,@14                                             0231
         CR    RCREG,@14                                           0231
         BNE   @RT00231                                            0231
         C     @14,DATAWORD(,DATAPTR)                              0231
         BNE   @RF00231                                            0231
@RT00231 DS    0H                                                  0232
*     DO;                                                          0232
*       BUFFER(1:LENGTH(IDERR))=IDERR;/* ERROR MESSAGE               */
         L     @14,ADPLBUF(,ABDPLPTR)                              0233
         MVC   BUFFER(80,@14),IDERR                                0233
*       RCCODE=4;                   /* BAD RETURN CODE               */
         MVC   RCCODE(4),@CF00081                                  0234
*       CALL PRINT;                 /* PRINT MESSAGE                 */
         L     @15,ADPLPRNT(,ABDPLPTR)                             0235
         BALR  @14,@15                                             0235
*     END;                          /* END CULD NOT ACCESS PCCAVT    */
*   ELSE                            /* COULD ACCESS PCCAVT           */
*     DO;                                                          0237
         B     @RC00231                                            0237
@RF00231 DS    0H                                                  0238
*       PCCATPTR=DATAWORD;          /* SAVE PCCAVT ADDRESS           */
*                                                                  0238
         L     PCCATPTR,DATAWORD(,DATAPTR)                         0238
*       /*************************************************************/
*       /*                                                           */
*       /* LOOP THROUGH THE PCCAVT FOR ALL PCCAS                     */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0239
*       DO INDEX=1 TO MP;           /* LOOP TO FIND CPUIDS           */
         LA    INDEX,1                                             0239
@DL00239 DS    0H                                                  0240
*         VADR=PCCATPTR+(4*(INDEX-1));/* PTR TO PCCA                 */
         LR    VADR,INDEX                                          0240
