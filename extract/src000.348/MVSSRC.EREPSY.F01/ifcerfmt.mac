*/********************************************************************/
*/* FUNCTION-                                                      0374
*        ROUTINE TO SPACE FORWARD IN THE BUFFER TO THE NEXT DOUBLE WORD
*      THIS IS IMPORTANT SINCE ALL RECORDS AND CPU IDS START ON DOUBLE
*      WORD BOUNDRIES. IF THE NEXT DOUBLE WORD IS BEYOND THE BUFFER END
*      THE POINTER IS WRAPPED TO THE APPROPRIATE PLACE IN THE BEGINNING
*      OF THE BUFFER.                                              0374
*                                                                  0374
*    CALLS-                                                        0374
*         MAKES NO OUTSIDE CALLS                                   0374
*                                                                  0374
*    CALLED BY -                                                   0374
*         DEBLOCK AND KEYBUILD TO ADVANCE PTR TO NEXT DOUBLE WORD  0374
*                                                                  0374
*    RETURNS TO -                                                  0374
*         CALLER WITHOUT SETTING A RETURN CODE                     0374
*                                                                    */
*/********************************************************************/
*                                                                  0374
*   IF(RECPTR&DOUBLEWD)^=RECPTR THEN/* RECPTR IS NOT ON A DOUBLE WORD
*                                      BOUNDARY                      */
         LR    @14,RECPTR                                          0374
         N     @14,DOUBLEWD                                        0374
         CLR   @14,RECPTR                                          0374
         BE    @RF00374                                            0374
*     RECPTR=(RECPTR&DOUBLEWD)+NEXTDWRD;/* ROUND RECPTR DOWN TO A  0375
*                                      DOUBLE WORD AND INCREMENT 8 TO
*                                      POINT TO NEXT DOUBLE WORD     */
         AL    @14,@CF00087                                        0375
         LR    RECPTR,@14                                          0375
*   ELSE                                                           0376
*     ;                             /* RECPTR ALREADY ON A DOUBLE  0376
*                                      WORD BDY                      */
@RF00374 DS    0H                                                  0377
*   IF RECPTR>=BUFEND THEN          /* PTR TO NEXT DOUBLE WORD WENT
*                                      BEYOND END OF BUFFER          */
         L     @14,BUFEND                                          0377
         CR    RECPTR,@14                                          0377
         BL    @RF00377                                            0377
*     DO;                                                          0378
*       RECPTR=RECPTR-BUFEND;       /* HOW FAR BEYOND END            */
         SLR   RECPTR,@14                                          0379
*       RECPTR=RECPTR+BUFSTART;     /* WRAP TO BEGINNING OF BUFFER   */
         AL    RECPTR,BUFSTART                                     0380
*     END;                          /* PTR WENT BEYOND END OF BUFFER */
*   ELSE                                                           0382
*     ;                             /* POINTER IS NOT BEYOND END OF
*                                      BUFFER                        */
@RF00377 DS    0H                                                  0383
*   END DWORDBDY;                                                  0383
@EL00004 DS    0H                                                  0383
@EF00004 DS    0H                                                  0383
@ER00004 L     @14,12(,@13)                                        0383
         BR    @14                                                 0383
*KEYBUILD:                                                         0384
*   PROC OPTIONS(SAVE(14));                                        0384
*                                                                  0384
KEYBUILD ST    @14,@SA00005                                        0384
*/********************************************************************/
*/* FUNCTION-                                                      0385
*        ROUTINE TO BUILD THE SORT KEY USED BY THE EREP EDIT ROUTINES
*      TO CHOOSE THE PROPER EDIT TABLES. THE INFORMATION THAT IS SAVED
*      IN THE SORT KEY IS THE RECORD TYPE AND THE CPU MODEL NUMBER.
*                                                                  0385
*    CALLS-                                                        0385
*         DWORDBDY TO ENSURE PROPER BOUNDRY AND PROPER WRAPPING OF THE
*         BUFFER.                                                  0385
*                                                                  0385
*    CALLED BY -                                                   0385
*         DEBLOCK TO BUILD THE SORT KEY.                           0385
*                                                                  0385
*    RETURNS TO -                                                  0385
*         CALLER WITHOUT SETTING A RETURN CODE                     0385
*                                                                    */
*/********************************************************************/
*                                                                  0385
*   SAVEPTR=RECPTR;                 /* SAVE RECPTR ACROSS THE ROUTINE*/
         LR    SAVEPTR,RECPTR                                      0385
*   RECPTR=RECPTR+LENGTH(BEHDR);    /* POINT NOW TO THE LRB HEADER   */
         AH    RECPTR,@CH00777                                     0386
*   CALL DWORDBDY;                  /* FIND PROPER BOUNDRY           */
         BAL   @14,DWORDBDY                                        0387
*   SORTYPE(SORTINDX)=LRBHTYPE;     /* SAVE RECORD TYPE              */
         L     @14,SORTINDX                                        0388
         MH    @14,@CH00034                                        0388
         LA    @02,SORTYPE-11(@14)                                 0388
         MVC   0(1,@02),LRBHTYPE(RECPTR)                           0388
*   RECPTR=RECPTR+HDRLNGTH;         /* POINT NOW TO THE CPUID        */
         AL    RECPTR,HDRLNGTH                                     0389
*   CALL DWORDBDY;                  /* FIND PROPER BOUNDRY           */
         BAL   @14,DWORDBDY                                        0390
*   SORTCPU(SORTINDX)=CPID(5:6);    /* SAVE CPU MODEL NUMBER         */
         L     @14,SORTINDX                                        0391
         MH    @14,@CH00034                                        0391
         LA    @02,SORTCPU-11(@14)                                 0391
         MVC   0(2,@02),CPID+4(RECPTR)                             0391
*   RECPTR=SAVEPTR;                 /* RESTORE RECPTR                */
         LR    RECPTR,SAVEPTR                                      0392
*   END KEYBUILD;                                                  0393
@EL00005 DS    0H                                                  0393
@EF00005 DS    0H                                                  0393
@ER00005 L     @14,@SA00005                                        0393
         BR    @14                                                 0393
*   END DEBLOCK;                                                   0394
@EL00003 DS    0H                                                  0394
@EF00003 DS    0H                                                  0394
@ER00003 L     @14,@SA00003                                        0394
         BR    @14                                                 0394
