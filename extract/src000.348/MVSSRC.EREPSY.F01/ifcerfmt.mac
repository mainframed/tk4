         L     @15,ADPLMEMA(,ABDPLPTR)                             0396
         BALR  @14,@15                                             0396
*   IF RCREG=ZERO THEN                                             0397
         LTR   RCREG,RCREG                                         0397
         BNZ   @RF00397                                            0397
