         OI    SDWAFREE(GPR01P),B'00000100'                        0436
*             GPR01P->SDWAREQ='1'B; /* TURN ON DUMP INDICATOR        */
         OI    SDWAREQ(GPR01P),B'10000000'                         0437
*             GPR01P->SDWAUPRG='1'B;/* TURN ON RETREGS INDICATOR     */
         OI    SDWAUPRG(GPR01P),B'00001000'                        0438
*             GEN(RETURN (14,12));                                 0439
         RETURN (14,12)
*             RESPECIFY                                            0440
*              (GPR00P,                                            0440
*               GPR01P,                                            0440
*               GPR14P,                                            0440
*               GPR15P) UNRSTD;                                    0440
*           END;                                                   0441
*         END;                      /* END FIRST UNEXPECTED ERROR    */
*       ELSE                        /* PERCOLATE FOR SECOND        0443
*                                      UNEXPECTED ERROR              */
*         DO;                       /* SETRP                       0443
*                                      REGS(14,12)DUMP(YES)RC(0)     */
         B     @RC00419                                            0443
@RF00419 DS    0H                                                  0444
*           RESPECIFY                                              0444
*            (GPR00P,                                              0444
*             GPR01P,                                              0444
*             GPR14P,                                              0444
*             GPR15P) RSTD;                                        0444
*           GPR01P->SDWARCDE=0;     /* SAVE RC VALUE                 */
         MVI   SDWARCDE(GPR01P),X'00'                              0445
*           GPR01P->SDWAREQ='1'B;   /* TURN ON DUMP INDICATOR        */
         OI    SDWAREQ(GPR01P),B'10000000'                         0446
*           GEN(RETURN (14,12));                                   0447
         RETURN (14,12)
*           RESPECIFY                                              0448
*            (GPR00P,                                              0448
*             GPR01P,                                              0448
*             GPR14P,                                              0448
*             GPR15P) UNRSTD;                                      0448
*         END;                      /*                       @ZM43805*/
*     END;                          /* END NOT AN X37 TYPE ABEND   0450
*                                                            @ZM43805*/
*   ELSE                            /* THIS IS AN X37 ABEND -      0451
*                                      PERCOLATE             @ZM43805*/
*     DO;                           /* SETRP                       0451
*                                      REGS(14,12)DUMP(YES)RC(0)     */
         B     @RC00417                                            0451
@RF00417 DS    0H                                                  0452
*       RESPECIFY                                                  0452
*        (GPR00P,                                                  0452
*         GPR01P,                                                  0452
*         GPR14P,                                                  0452
*         GPR15P) RSTD;                                            0452
*       GPR01P->SDWARCDE=0;         /* SAVE RC VALUE                 */
         MVI   SDWARCDE(GPR01P),X'00'                              0453
*       GPR01P->SDWAREQ='1'B;       /* TURN ON DUMP INDICATOR        */
         OI    SDWAREQ(GPR01P),B'10000000'                         0454
*       GEN(RETURN (14,12));                                       0455
         RETURN (14,12)
*       RESPECIFY                                                  0456
*        (GPR00P,                                                  0456
*         GPR01P,                                                  0456
*         GPR14P,                                                  0456
*         GPR15P) UNRSTD;                                          0456
*     END;                          /*                       @ZM43805*/
