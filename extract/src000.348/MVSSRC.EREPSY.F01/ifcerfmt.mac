*/********************************************************************/
*/* FUNCTION-                                                      0283
*        ROUTINE TO DEBLOCK THE IN-STORAGE LOGREC BUFFER AND SAVE THE
*      POINTERS TO ALL THE LOGREC RECORDS IN THE SORT TABLE. IT FINDS
*      THE FIRST RECORD BY THE CPUID AND BACKS UP TO THE HEADER    0283
*      INFORMATION ABOUT LENGTHS TO FIND THE LATER RECORDS. THE RECORDS
*      ARE SAVED IN OLDEST TO NEWEST ORDER.                        0283
*                                                                  0283
*       MESSAGES ISSUED BY THIS ROUTINE ARE:                       0283
*                                                                  0283
*               *** IN-STORAGE LOGREC BUFFER COULD NOT BE ACCESSED,
*                   POSSIBLE CAUSE (DATA NOT IN DUMP) ***          0283
*          (COULD NOT ACCESS LOGREC BUFFER)                        0283
*                                                                  0283
*               *** IN-STORAGE LOGREC BUFFER NOT FORMATTED, INVALID
*                   HEADER INFORMATION FOUND ***                   0283
*          (EITHER THE BUFFER WAS NOT 4K IN LENGTH, OR THE FIRST FREE
*           POINTER WAS NOT BETWEEN THE BENNING AND END ADDRESSES) 0283
*                                                                  0283
*               *** THERE ARE NO VALID LOGREC RECORDS IN THE IN-STORAGE
*                   LOGREC BUFFER ***                              0283
*          (NO LOGREC TYPE RECORDS WITH DATA IN BUFFER FOUND)      0283
*                                                                  0283
*               *** IN-STORAGE LOGREC BUFFER NOT FORMATTED, DATA   0283
*                   NECESSARY TO LOCATE THE BUFFER UNAVAILABLE *** 0283
*          (THE RTCT RECORDING FLAGS COULD NOT BE ACCESSED)        0283
*                                                                  0283
*                                                                  0283
*    CALLS-                                                        0283
*         ACCESRTN, DWORDBDY, KEYBUILD, AND PRINT                  0283
*                                                                  0283
*    CALLED BY -                                                   0283
*         MAINLINE TO DEBLOCK THE BUFFER                           0283
*                                                                  0283
*    RETURNS TO -                                                  0283
*         CALLER WITHOUT SETTING A RETURN CODE                     0283
*                                                                    */
*/********************************************************************/
*                                                                  0283
*   SORTINDX=ZERO;                  /* RECORD COUNTER FOR SORT TABLE */
         SLR   @14,@14                                             0283
         ST    @14,SORTINDX                                        0283
*   VADR=RECPTR;                    /* ADDRESS OF IN-STORAGE LOGREC
*                                      BUFFER                        */
         LR    VADR,RECPTR                                         0284
*   CALL ACCESRTN;                  /* ACCESS BUFFER-IT IS ON A 4K 0285
*                                      BDY SO ALL REFERENCES IN THE
*                                      BUFFER CAN BE MADE DIRECTLY 0285
*                                      WITHOUT THE USE OF THE ACCESS
*                                      SERVICE ROUTINE               */
         BAL   @14,ACCESRTN                                        0285
*   IF RCREG^=ZERO THEN             /* ACCESS ERROR                  */
         LTR   RCREG,RCREG                                         0286
         BZ    @RF00286                                            0286
*     DO;                                                          0287
*       BUFFER(1:LENGTH(NOBUF))=NOBUF;/* ERROR MESSAGE               */
         L     @14,ADPLBUF(,ABDPLPTR)                              0288
         MVC   BUFFER(89,@14),NOBUF                                0288
*       CALL PRINT;                 /* PRINT IT                      */
         L     @15,ADPLPRNT(,ABDPLPTR)                             0289
         BALR  @14,@15                                             0289
*     END;                          /* END ACCESS ERROR              */
*   ELSE                            /* FOUND BUFFER                  */
*     DO;                                                          0291
*                                                                  0291
         B     @RC00286                                            0291
@RF00286 DS    0H                                                  0292
*       /*************************************************************/
*       /*                                                           */
*       /* MOVE THE PRINT DUMP COPY INTO AN INTERNAL BUFFER TO ENSURE*/
*       /* THE BUFFER STARTS ON A DOUBLE WORD                        */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0292
*       RFY                                                        0292
*        (COPYREG,                                                 0292
*         COPYSIZE,                                                0292
*         BUFREG,                                                  0292
*         BUFSIZE) RSTD;                                           0292
*       COPYREG=ADDR(COPYBUF);      /* WHERE TO COPY THE LOGREC    0293
*                                      BUFFER TO                     */
         LA    COPYREG,COPYBUF                                     0293
*       COPYSIZE=LENGTH(COPYBUF);   /* SIZE OF THE LOGREC BUFFER     */
         LH    @14,@CH01275                                        0294
         LR    COPYSIZE,@14                                        0294
*       BUFREG=DATAPTR;             /* POINTER TO THE PRINT DUMP COPY*/
         LR    BUFREG,DATAPTR                                      0295
*       BUFSIZE=LENGTH(COPYBUF);    /* LENGTH OF LOGREC BUFFER       */
         LR    BUFSIZE,@14                                         0296
*       MVCL(COPYREG,BUFREG);       /* MOVE INTO INTERNAL BUFFER     */
         MVCL  COPYREG,BUFREG                                      0297
*       RFY                                                        0298
*        (COPYREG,                                                 0298
*         COPYSIZE,                                                0298
*         BUFREG,                                                  0298
*         BUFSIZE) UNRSTD;                                         0298
*                                                                  0298
*       /*************************************************************/
*       /*                                                           */
*       /* DETERMINE IF THE LOGREC HEADER INFORMATION IS IN REAL     */
*       /* ADDRESSES INSTEAD OF VIRTUAL ADDRESSES. THIS HAPPENS WHEN */
*       /* THE EMERGENCY RECORDING IS DONE WITHOUT THE VIRTUAL       */
*       /* TRANSLATION FEATURE. THE WAY TO DETERMINE THIS IS THE RSTF*/
*       /* BIT IN THE RTCT. THE BIT IS ON WHEN THE EMERGENCY         */
*       /* RECORDING IS WRITING ITS RECORDS AND IS TRANSLATING THE   */
*       /* HEADER TO REAL ADDRESSES.                                 */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0299
*       REALADR=RECPTR;             /* SAVE VIRTUAL LOGREC BUFFER  0299
*                                      ADDR TO CALCULATE START AND 0299
*                                      END OFFSETS FOR THE COPY OF 0299
*                                      THE BUFFER                    */
         LR    REALADR,RECPTR                                      0299
*       RECPTR=ADDR(COPYBUF);       /* ADDRESS OF THE COPY OF THE  0300
*                                      BUFFER                        */
         LA    RECPTR,COPYBUF                                      0300
*       VALIDCPU=OFF;               /* INITIALIZE FOUND FIRST RECORD
*                                      BY CPU ID OFF                 */
         NI    VALIDCPU,B'01111111'                                0301
*       VADR=ADDR(RTCTRFLG);        /* RTCT RECORDING FLAGS          */
         L     @14,RTCTPTR                                         0302
         LA    VADR,RTCTRFLG(,@14)                                 0302
*       CALL ACCESRTN;              /* ACCESS IT                     */
         BAL   @14,ACCESRTN                                        0303
*       IF RCREG^=ZERO THEN         /* ERROR ACCESSING THE RTCT      */
         LTR   RCREG,RCREG                                         0304
         BZ    @RF00304                                            0304
*         DO;                                                      0305
*           BUFFER(1:LENGTH(LOCERR))=LOCERR;/* ERROR MESSAGE         */
         L     @14,ADPLBUF(,ABDPLPTR)                              0306
         MVC   BUFFER(95,@14),LOCERR                               0306
*           CALL PRINT;             /* PRINT IT                      */
         L     @15,ADPLPRNT(,ABDPLPTR)                             0307
         BALR  @14,@15                                             0307
*         END;                      /* END ACCESS ERROR              */
*       ELSE                        /* FOUND RECORDING FLAGS         */
*         DO;                                                      0309
         B     @RC00304                                            0309
@RF00304 DS    0H                                                  0310
*           IF STFBIT=ON THEN       /* THE BUFFER POINTERS ARE REAL
*                                      ADDRESSES NOT VIRTUAL ADDRS   */
         TM    STFBIT(DATAPTR),B'00100000'                         0310
         BNO   @RF00310                                            0310
*             REALADR=RCBBUFB&FOURKBDY;/* MASK OUT LOW ORDER BITS TO
*                                      DETERMINE THE BUFFERS       0311
*                                      BEGINNING REAL ADDR TO BE USED
*                                      FOR CALCULATING START AND END
*                                      ADDRESS FOR THE INTERNAL COPY
*                                      OF THE BUFFER                 */
         L     REALADR,RCBBUFB(,RECPTR)                            0311
         N     REALADR,FOURKBDY                                    0311
*           ELSE                                                   0312
*             ;                     /* BUFFERS POINTERS ARE VIRTUAL
*                                      AND THE VIRTUAL ADDRESS HAS 0312
*                                      ALREADY BEEN SAVED IN REALADR */
*                                                                  0312
@RF00310 DS    0H                                                  0313
*           /*********************************************************/
*           /*                                                       */
*           /* DETERMINE THE FIRST AND LAST POINTERS FOR THE COPIED  */
*           /* BUFFER THAT THE IN-STORAGE BUFFER HAS BEEN COPIED     */
*           /* INTO, SINCE THAT IS THE BUFFER BEING REFERENCED, NOT  */
*           /* THE ACTUAL BUFFER                                     */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0313
*           BUFSTART=RCBBUFB-REALADR;/* OFFSET OF BEGINNING ADDR OF
*                                      REAL BUFFER                   */
         L     @14,RCBBUFB(,RECPTR)                                0313
         SLR   @14,REALADR                                         0313
         ST    @14,BUFSTART                                        0313
*           BUFSTART=BUFSTART+RECPTR;/* BEGINNING ADDR IN COPIED   0314
*                                      BUFFER                        */
         ALR   @14,RECPTR                                          0314
         ST    @14,BUFSTART                                        0314
*           BUFEND=RCBBUFE-REALADR; /* OFFSET OF END ADDR OF REAL  0315
*                                      BUFFER                        */
         L     @04,RCBBUFE(,RECPTR)                                0315
         SLR   @04,REALADR                                         0315
         ST    @04,BUFEND                                          0315
*           BUFEND=BUFEND+RECPTR;   /* END ADDRESS OF COPIED BUFFER  */
         ALR   @04,RECPTR                                          0316
         ST    @04,BUFEND                                          0316
*           FRSTFREE=RCBFREE-REALADR;/* OFFSET OF FIRST FREE AREA IN
*                                      REAL BUF                      */
         L     FRSTFREE,RCBFREE(,RECPTR)                           0317
         SLR   FRSTFREE,REALADR                                    0317
*           FRSTFREE=FRSTFREE+RECPTR;/* PTR TO FIRST FREE AREA IN  0318
*                                      COPIED BUF                    */
         ALR   FRSTFREE,RECPTR                                     0318
*           IF((RECPTR+FOURK)^=BUFEND)×/* BUFFER IS NOT 4K         0319
*                                      LONG(IMPROPER LENGTH)         */
*               (FRSTFREE<BUFSTART)×/* BAD FIRST FREE POINTER        */
*               (FRSTFREE>BUFEND) THEN/* BAD FIRST FREE ADDRESS      */
         LR    @02,RECPTR                                          0319
         AH    @02,@CH01275                                        0319
         CR    @02,@04                                             0319
         BNE   @RT00319                                            0319
         CR    FRSTFREE,@14                                        0319
         BL    @RT00319                                            0319
         CR    FRSTFREE,@04                                        0319
         BNH   @RF00319                                            0319
@RT00319 DS    0H                                                  0320
*             DO;                                                  0320
*               BUFFER(1:LENGTH(BADBUF))=BADBUF;/* ERROR MESSAGE     */
         L     @14,ADPLBUF(,ABDPLPTR)                              0321
         MVC   BUFFER(80,@14),BADBUF                               0321
*               CALL PRINT;         /* PRINT IT                      */
         L     @15,ADPLPRNT(,ABDPLPTR)                             0322
         BALR  @14,@15                                             0322
*             END;                  /* END BAD BUFFER ENCOUNTERED    */
*           ELSE                    /* FOUND A VALID BUFFER          */
*             DO;                                                  0324
         B     @RC00319                                            0324
@RF00319 DS    0H                                                  0325
*               RECPTR=FRSTFREE+LENGTH(BEHDR)+HDRLNGTH;/* POINTER TO
*                                      THE CPUID FOR THE FIRST     0325
*                                      POSSIBLE WHOLE RECORD AFTER 0325
*                                      THE FIRST FREE POINTER IN THE
*                                      BUFFER                        */
         LA    RECPTR,16                                           0325
         ALR   RECPTR,FRSTFREE                                     0325
         AL    RECPTR,HDRLNGTH                                     0325
*               CALL DWORDBDY;      /* MOVE PTR TO NEXT DOUBLEWORD   */
*                                                                  0326
         BAL   @14,DWORDBDY                                        0326
*               /*****************************************************/
*               /*                                                   */
*               /* LOOP TO FIND THE FIRST RECORD BY CPUID            */
*               /*                                                   */
*               /*****************************************************/
*                                                                  0327
*               DO UNTIL((RECPTR=FRSTFREE)×(VALIDCPU=ON));/* LOOP  0327
*                                      UNTIL THE BUFFER IS EXHAUSTED
*                                      OR A RECORD IS FOUND          */
@DL00327 DS    0H                                                  0328
*                 DO INDEX=1 TO MP; /* INNER LOOP-SCAN ALL POSSIBLE
*                                      VALID CPU IDS                 */
         LA    INDEX,1                                             0328
@DL00328 DS    0H                                                  0329
*                   IF(IDTABLE(INDEX)^=BLANKS)&/* CPUID NOT BLANKS 0329
*                                      (ONE OF THE ENTRIES IS BLANK
*                                      IN A UP                 SYSTEM*/
*                       (CPID=IDTABLE(INDEX)) THEN/* VALID CPU ID  0329
*                                      FOUND IN RECORD               */
         LR    @14,INDEX                                           0329
         MH    @14,@CH00879                                        0329
         LA    @02,IDTABLE-6(@14)                                  0329
         CLC   0(6,@02),BLANKS                                     0329
         BE    @RF00329                                            0329
         LA    @02,IDTABLE-6(@14)                                  0329
         CLC   CPID(6,RECPTR),0(@02)                               0329
         BNE   @RF00329                                            0329
*                     VALIDCPU=ON;  /* INDICATE A RECORD FOUND       */
         OI    VALIDCPU,B'10000000'                                0330
*                   ELSE                                           0331
*                     ;             /* A RECORD NOT FOUND            */
@RF00329 DS    0H                                                  0332
*                 END;              /* END INNER LOOP TO SCAN FOR  0332
*                                      VALID CPU ID                  */
         AH    INDEX,@CH00042                                      0332
         CH    INDEX,@CH00100                                      0332
         BNH   @DL00328                                            0332
*                 IF VALIDCPU=OFF THEN/* DID NOT FIND A RECORD       */
         TM    VALIDCPU,B'10000000'                                0333
         BNZ   @RF00333                                            0333
*                   DO;                                            0334
*                     RECPTR=RECPTR+NEXTDWRD;/* INCREMENT TO NEXT  0335
*                                      DOUBLE WORD                   */
         AH    RECPTR,@CH00087                                     0335
*                     CALL DWORDBDY;/* CALL TO CHECK IF NEXT DWORD IS
*                                      BEYOND END OF BUFFER          */
         BAL   @14,DWORDBDY                                        0336
*                   END;            /* END DID NOT FIND A RECORD     */
*                 ELSE                                             0338
*                   ;               /* RECORD WAS FOUND- NO NEED TO
*                                      INCR PTR                      */
@RF00333 DS    0H                                                  0339
*               END;                /* END LOOP LOOKING FOR A RECORD */
*                                                                  0339
@DE00327 CR    RECPTR,FRSTFREE                                     0339
         BE    @DC00327                                            0339
         TM    VALIDCPU,B'10000000'                                0339
         BNO   @DL00327                                            0339
@DC00327 DS    0H                                                  0340
*               /*****************************************************/
*               /*                                                   */
*               /* CHECK IF ANY VALID RECORDS WERE FOUND             */
*               /*                                                   */
*               /*****************************************************/
*                                                                  0340
*               IF VALIDCPU=OFF THEN/* BUFFER EXHAUSTED WITHOUT    0340
*                                      FINDING A RECORD              */
         TM    VALIDCPU,B'10000000'                                0340
         BNZ   @RF00340                                            0340
*                 DO;                                              0341
*                   BUFFER(1:LENGTH(NORECORD))=NORECORD;/* ERROR   0342
*                                      MESSAGE                       */
         L     @14,ADPLBUF(,ABDPLPTR)                              0342
         MVC   BUFFER(73,@14),NORECORD                             0342
*                   CALL PRINT;     /* PRINT IT                      */
         L     @15,ADPLPRNT(,ABDPLPTR)                             0343
         BALR  @14,@15                                             0343
*                 END;              /* END NO RECORDS FOUND          */
*               ELSE                /* FOUND A VALID FIRST RECORD    */
*                 DO;                                              0345
         B     @RC00340                                            0345
@RF00340 DS    0H                                                  0346
*                   RECPTR=RECPTR-(HDRLNGTH+LENGTH(BEHDR));/* POINT
*                                      BEFORE BOTH THE CONTROL HEADER
*                                      AND THE RECORD HEADER         */
         LA    @14,16                                              0346
         AL    @14,HDRLNGTH                                        0346
         SLR   RECPTR,@14                                          0346
*                   IF RECPTR<BUFSTART THEN/* RECORD WRAPS THE BUFFER*/
         L     @14,BUFSTART                                        0347
         CR    RECPTR,@14                                          0347
         BNL   @RF00347                                            0347
*                     DO;                                          0348
*                       RECPTR=BUFSTART-RECPTR;/* HOW MUCH HAS     0349
*                                      WRAPPED                       */
         SLR   @14,RECPTR                                          0349
         LR    RECPTR,@14                                          0349
*                       RECPTR=BUFEND-RECPTR;/* PROPER PLACE AT END
*                                      OF BUFFER                     */
         L     @14,BUFEND                                          0350
         SLR   @14,RECPTR                                          0350
         LR    RECPTR,@14                                          0350
*                     END;          /* END RECORD WRAPS BUFFER       */
*                   ELSE                                           0352
*                     ;             /* RECORD HAS NOT WRAPPED        */
*                                                                  0352
@RF00347 DS    0H                                                  0353
*                   /*************************************************/
*                   /*                                               */
*                   /* DEBLOCK THE REST OF THE BUFFER USING THE      */
*                   /* RECORD HEADERS FOR LENGTHS                    */
*                   /*                                               */
*                   /*************************************************/
*                                                                  0353
*                   DO UNTIL(RECPTR=FRSTFREE);/* DEBLOCK LOOP        */
@DL00353 DS    0H                                                  0354
*                     IF(DATAP=OFF)&(LOG=ON) THEN/* THIS RECORD IS TO
*                                      BE PRINTED, IT IS A LOGREC  0354
*                                      RECORD AND THE ENTRY IS IN THE
*                                      BUFFER                        */
         TM    DATAP(RECPTR),B'00001000'                           0354
         BNZ   @RF00354                                            0354
         TM    LOG(RECPTR),B'01000000'                             0354
         BNO   @RF00354                                            0354
*                       DO;                                        0355
*                         SORTINDX=SORTINDX+1;/* INCREMENT NO. OF  0356
