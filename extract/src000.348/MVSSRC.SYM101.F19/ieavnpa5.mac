         TITLE 'IEAVNPA5, APF TABLE INITIALIZATION ROUTINE             *
                        '
IEAVNPA5 CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL2(@EP00001-@MAINENT)
         DC    AL1(16)                                             0001
         DC    C'IEAVNPA5  73.324'                                 0001
NPA5APF  DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL2(@EP00166-NPA5APF)
         ENTRY NPA5APF
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @10,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@10                                         0001
         ST    @13,@SA00001+4                                      0001
         LA    @14,@SA00001                                        0001
         ST    @14,8(,@13)                                         0001
         LR    @13,@14                                             0001
         AH    @15,4(,@15)                                         0001
         BR    @15                                                 0001
@EP00001 DS    0H                                                  0002
*
** REFS ARE GENERATED FOR NVT FIELDS THAT CONTAIN ADDRESSES OF
** NIP SERVICE ROUTINES, SO IEAPMNIP MACRO CALLS TO THESE ROUTINES
** WILL ASSEMBLE PROPERLY.
*/*                    END OF CVT MACRO                              */
*                                                                  0166
*                                                                  0166
*/********************************************************************/
*/*                                                                  */
*/*        SUBROUTINE NPA5APF                                        */
*/*                                                                  */
*/*        FUNCTION - BUILD THE SYSTEM APF TABLE                     */
*/*                                                                  */
*/*        INPUT    - (1) REG 3 (POINTER TO CVT)                     */
*/*                   (2) APF PARMLIB MEMBER, IEAPAKXX               */
*/*                                                                  */
*/*        OUTPUT   - (1) CVTAUTHL - POINTER TO THE SYSTEM APF TABLE */
*/*                   (2) SYSTEM APF TABLE COMPLETE                  */
*/*                                                                  */
*/********************************************************************/
*                                                                  0166
*NPA5APF:                                                          0166
*   ENTRY;                                                         0166
@EP00166 DS    0H                                                  0167
*   RESPECIFY                                                      0167
*    (RCVT,                                                        0167
*     RPARM0,                                                      0167
*     TEXTPTR,                                                     0167
*     APFPTR,                                                      0167
*     RNVT) RESTRICTED;                                            0167
*   RPARM0=APFTABSZ;                /* SETTUP FOR GETMAIN OF APF   0168
*                                      TABLE                         */
         L     RPARM0,APFTABSZ                                     0168
*   GENERATE REFS(RPARM0) SETS(RPARM)(GETMAIN R,LV=(RPARM0));/* ISSUE
*                                      GETMAIN FOR 2040 BYTES FROM 0169
*                                      SP245                         */
         GETMAIN R,LV=(RPARM0)
*   RESPECIFY                                                      0170
*    (RPARM) RESTRICTED;                                           0170
*   APFSAVE=RPARM;                  /* SAVE PTR TO START OF TABLE    */
         ST    RPARM,APFSAVE                                       0171
*   APFPTR=RPARM+2;                 /* INIT APF TABLE POINTER        */
         LA    @12,2                                               0172
         LR    APFPTR,RPARM                                        0172
         ALR   APFPTR,@12                                          0172
*   RESPECIFY                                                      0173
*    (RPARM,                                                       0173
*     RPARM0) UNRESTRICTED;                                        0173
*                                                                  0173
*   /*****************************************************************/
*   /*                                                               */
*   /* THE SYSTEM APF TABLE WILL ALWAYS CONTAIN SYS1.LINKLIB AND     */
*   /* SYS1.SVCLIB AS ENTRIES.                                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0174
*   RESPECIFY                                                      0174
*    (DEBPTR,                                                      0174
*     DEBDAPTR) RESTRICTED;                                        0174
*   APFTAB=LINKLIB;                 /* INIT LINKLIB ENTRY            */
         MVI   APFTAB+19(APFPTR),C' '                              0175
         MVC   APFTAB+20(31,APFPTR),APFTAB+19(APFPTR)              0175
         MVC   APFTAB(19,APFPTR),LINKLIB                           0175
*   DEBPTR=CVTLINK->DCBDEBAD;       /* POINT TO DEB BASIC SECTION    */
         L     @11,CVTLINK(,CVTPTR)                                0176
         SLR   DEBPTR,DEBPTR                                       0176
         ICM   DEBPTR,7,DCBDEBAD(@11)                              0176
*   DEBDAPTR=ADDR(DEBBASND);        /* POINT TO DEB DA SECTION       */
         LA    DEBDAPTR,DEBBASND(,DEBPTR)                          0177
*   APFTVOL=DEBUCBAD->UCBVOLI;      /* INIT LINKLIB VOLID IN TAB     */
         L     @11,DEBUCBAD(,DEBDAPTR)                             0178
         MVC   APFTVOL(6,APFPTR),UCBVOLI(@11)                      0178
*   APFPTR=APFPTR+19;               /* POINT TO NXT ENTRY-SVCLIB     */
         AH    APFPTR,@CH00079                                     0179
*   APFTAB=SVCLIB;                  /* INIT SVCLIB ENTRY             */
         MVI   APFTAB+18(APFPTR),C' '                              0180
         MVC   APFTAB+19(32,APFPTR),APFTAB+18(APFPTR)              0180
         MVC   APFTAB(18,APFPTR),SVCLIB                            0180
*   DEBPTR=CVTSVDCB->DCBDEBAD;      /* POINT TO DEB BASIC SECTION    */
         L     @11,CVTSVDCB(,CVTPTR)                               0181
         SLR   DEBPTR,DEBPTR                                       0181
         ICM   DEBPTR,7,DCBDEBAD(@11)                              0181
*   DEBDAPTR=ADDR(DEBBASND);        /* POINT TO DEB DA SECTION       */
         LA    DEBDAPTR,DEBBASND(,DEBPTR)                          0182
*   APFTVOL=DEBUCBAD->UCBVOLI;      /* INIT SVCLIB VOLID IN TAB      */
         L     @11,DEBUCBAD(,DEBDAPTR)                             0183
         MVC   APFTVOL(6,APFPTR),UCBVOLI(@11)                      0183
*   RESPECIFY                                                      0184
*    (DEBPTR,                                                      0184
*     DEBDAPTR) UNRESTRICTED;                                      0184
*   APFPTR=APFPTR+18;               /* POINT TO NEXT ENTRY           */
         AH    APFPTR,@CH00090                                     0185
*   APFSVCLK=APFPTR;                /* SAVE PTR TO END OF LAST ENT   */
         ST    APFPTR,APFSVCLK                                     0186
*   ENTRYCTR=2;                     /* UPDATE COUNTER WITH NO. ENT   */
*                                                                  0187
         STH   @12,ENTRYCTR                                        0187
*   /*****************************************************************/
*   /*                                                               */
*   /* IF APF=XX HAS BEEN SPECIFIED BY OPERATOR, PROCESS IEAAPFXX    */
*   /* MEMBER OF SYS1.PARMLIB.                                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0188
*   RESPECIFY                                                      0188
*     PARMTAB BASED(NVTPTAB);                                      0188
*   RESPECIFY                                                      0189
*    (SCNPTR) RESTRICTED;                                          0189
*   SCNPTR=APF;                     /* GET POINTER TO SUBSTRING      */
         L     @12,NVTPTAB(,RNVT)                                  0190
         L     SCNPTR,APF(,@12)                                    0190
*   IF SCNPTR=0 THEN                /* WAS APF SPECIFIED?            */
         LTR   SCNPTR,SCNPTR                                       0191
         BNZ   @RF00191                                            0191
*     DO;                           /* APF NOT SPECIFIED             */
*       CALL NPA5COMP;              /* COMPLETE TABLE BUILD          */
         BAL   @14,NPA5COMP                                        0193
*       RETURN;                     /* RETURN TO CALLER              */
@EL00001 L     @13,4(,@13)                                         0194
@EF00001 DS    0H                                                  0194
@ER00001 LM    @14,@12,12(@13)                                     0194
         BR    @14                                                 0194
*     END;                                                         0195
*   ELSE                                                           0196
*     ;                             /* APF SPECIFIED                 */
@RF00191 DS    0H                                                  0197
*NPA5SCHK:                          /* CHECK SUBSTRING               */
*   CALL NPA5APRM;                  /* SYNTAX CHECK APF SUBSTRING    */
NPA5SCHK BAL   @14,NPA5APRM                                        0197
*   IF PRMRC^='00'X THEN            /* WAS THERE A PARAMETER ERROR   */
         CLI   PRMRC,X'00'                                         0198
         BE    @RF00198                                            0198
*     DO;                           /* YES                           */
*       ERRORCD=PINV;               /* SET ERROR CAUSE CODE          */
         MVI   ERRORCD,X'04'                                       0200
*       GO TO NPA5EROR;             /* PROCESS ERROR                 */
         B     NPA5EROR                                            0201
*     END;                                                         0202
*   ELSE                                                           0203
*     ;                             /* PARAMETER SPECIFICATION OK    */
*                                                                  0203
@RF00198 DS    0H                                                  0204
*   /*****************************************************************/
*   /*                                                               */
*   /* READ PARMLIB (IEAAPFXX) TEXT RECORDS UNTIL ALL ENTRIES ARE    */
*   /* PROCESSED.                                                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0204
*   DO WHILE NPA5AEND='0'B;         /* LOOP TILL ALL TXT RECORDS HAVE
*                                      BEEN READ                     */
         B     @DE00204                                            0204
@DL00204 DS    0H                                                  0205
*NPA5TEXT:                                                         0205
*     CALL NPA5TXTR;                /* READ A TEXT RECORD            */
NPA5TEXT BAL   @14,NPA5TXTR                                        0205
*     IF TXTRC^='00'X THEN          /* WAS READ SUCCESSFUL?          */
         CLI   TXTRC,X'00'                                         0206
         BNE   @RT00206                                            0206
*       GO TO NPA5EROR;             /* NO - PROCESS ERROR            */
*     ELSE                                                         0208
*       ;                           /* YES - CONTINUE                */
*     APFLIBSZ=0;                   /* INIT LIBRARY SIZE TO 0        */
         SLR   @12,@12                                             0209
         STH   @12,APFLIBSZ                                        0209
*NPA5SCAN:                                                         0210
*     LIBPTR=TEXTPTR;               /* SAVE PTR TO LIBRARY NAME      */
NPA5SCAN ST    TEXTPTR,LIBPTR                                      0210
*     DO WHILE SCAN(1)^=' ';        /* DETERMINE LIBNAME SIZE        */
         B     @DE00211                                            0211
@DL00211 DS    0H                                                  0212
*       APFLIBSZ=APFLIBSZ+1;        /* INCREMENT LIBNAME COUNTER     */
         LA    @12,1                                               0212
         LH    @11,APFLIBSZ                                        0212
         ALR   @11,@12                                             0212
         STH   @11,APFLIBSZ                                        0212
*       TEXTPTR=TEXTPTR+1;          /* POINT TO NEXT CHAR OF LIBNM   */
         ALR   TEXTPTR,@12                                         0213
*     END;                          /* END OF SCAN LOOP              */
@DE00211 CLI   SCAN(TEXTPTR),C' '                                  0214
         BNE   @DL00211                                            0214
*     IF APFLIBSZ>44 THEN           /* IS LIBNAME INVALID            */
         LH    @12,APFLIBSZ                                        0215
         CH    @12,@CH00137                                        0215
         BNH   @RF00215                                            0215
*       DO;                         /* YES - PROCESS ERROR           */
*         ERRORCD=LINV;             /* SET ERROR CAUSE CODE          */
         MVI   ERRORCD,X'08'                                       0217
*         GO TO NPA5EROR;           /* PROCESS ERROR                 */
         B     NPA5EROR                                            0218
*       END;                        /* END ERROR PROCESSING          */
*     ELSE                                                         0220
*       ;                           /* VALID LIBNAME                 */
@RF00215 DS    0H                                                  0221
*     IF APFLIBSZ=0 THEN            /* IS TEXT RECORD BLANK          */
         ICM   @12,3,APFLIBSZ                                      0221
         BZ    @RT00221                                            0221
*       GO TO NPA5TEND;             /* YES - GET NEXT RECORD         */
*     ELSE                                                         0223
*       ;                           /* NO - PROCESS LIBNAME IF     0223
*                                      STORAGE IS AVAILABLE          */
*     IF((APFPTR+LENGTH(APFTENTR))-APFSAVE)>2040 THEN/* APF TABLE  0224
*                                      SIZE (2040) EXCEEDED          */
         LA    @12,51                                              0224
         ALR   @12,APFPTR                                          0224
         SL    @12,APFSAVE                                         0224
         CH    @12,@CH02061                                        0224
         BNH   @RF00224                                            0224
*       DO;                         /* YES - PROCESS ERROR           */
*         ERRORCD=STOR;             /* INDICATE CAUSE OF ERROR       */
         MVI   ERRORCD,X'10'                                       0226
*         GO TO NPA5EROR;           /* PROCESS ERROR                 */
         B     NPA5EROR                                            0227
*       END;                                                       0228
*     ELSE                                                         0229
*       ;                           /* STORAGE AVAILABLE             */
@RF00224 DS    0H                                                  0230
*     APFTDSN=LIBPTR->SCAN(1:APFLIBSZ);/* MOVE DSSN TO TABLE         */
         MVI   APFTDSN+1(APFPTR),C' '                              0230
         MVC   APFTDSN+2(42,APFPTR),APFTDSN+1(APFPTR)              0230
         LH    @12,APFLIBSZ                                        0230
         BCTR  @12,0                                               0230
         L     @11,LIBPTR                                          0230
         EX    @12,@SM02174                                        0230
*     DO WHILE SCAN(1)=' ';         /* SKIP BLANKS BEFORE VOLID      */
         B     @DE00231                                            0231
@DL00231 DS    0H                                                  0232
*       TEXTPTR=TEXTPTR+1;          /* INCREMENT TO NXT CHAR         */
         AH    TEXTPTR,@CH00034                                    0232
*     END;                          /* END OF SCAN-FOR-BLANKS LOOP   */
*                                                                  0233
@DE00231 CLI   SCAN(TEXTPTR),C' '                                  0233
         BE    @DL00231                                            0233
*     /***************************************************************/
*     /*                                                             */
*     /* PROCESS VOLID FIELD                                         */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0234
*     APFVOLSZ=0;                   /* SET VOLID CHAR COUNTER TO 0   */
         SLR   @12,@12                                             0234
         STH   @12,APFVOLSZ                                        0234
*     VOLPTR=TEXTPTR;               /* SAVE PTR TO VOLID             */
*                                                                  0235
         ST    TEXTPTR,VOLPTR                                      0235
*     /***************************************************************/
*     /*                                                             */
*     /* DETERMINE DELIMITER FOLLOWING VOLID                         */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0236
*     DO WHILE SCAN(1)^=' '&SCAN(1)^=','&SCAN(1)^='FF'X;/* CHARACTER
*                                      A DELIMITER?                  */
         B     @DE00236                                            0236
@DL00236 DS    0H                                                  0237
*       APFVOLSZ=APFVOLSZ+1;        /* INCREMENT VOLID SIZE          */
         LA    @12,1                                               0237
         LH    @11,APFVOLSZ                                        0237
         ALR   @11,@12                                             0237
         STH   @11,APFVOLSZ                                        0237
*       TEXTPTR=TEXTPTR+1;          /* POINT TO NXT CHARACTER        */
         ALR   TEXTPTR,@12                                         0238
*     END;                          /* END OF VOLID SCAN LOOP        */
@DE00236 CLI   SCAN(TEXTPTR),C' '                                  0239
         BE    @DC00236                                            0239
         CLI   SCAN(TEXTPTR),C','                                  0239
         BE    @DC00236                                            0239
         CLI   SCAN(TEXTPTR),X'FF'                                 0239
         BNE   @DL00236                                            0239
@DC00236 DS    0H                                                  0240
*     IF SCAN(1)='FF'X×APFVOLSZ>6 THEN/* VOLID ERROR                 */
         CLI   SCAN(TEXTPTR),X'FF'                                 0240
         BE    @RT00240                                            0240
         LH    @12,APFVOLSZ                                        0240
         CH    @12,@CH00083                                        0240
         BNH   @RF00240                                            0240
@RT00240 DS    0H                                                  0241
*       DO;                         /* YES- EITHER VOLID MISSING   0241
*                                      (SCAN=FF) OR LIBSIZE TOO LARGE*/
*         ERRORCD=VINV;             /* SET ERROR-CAUSE CODE          */
         MVI   ERRORCD,X'0C'                                       0242
*         GO TO NPA5EROR;           /* PROCESS ERROR                 */
         B     NPA5EROR                                            0243
*       END;                                                       0244
*     ELSE                                                         0245
*       ;                           /* NO- PROCESS VOLID             */
@RF00240 DS    0H                                                  0246
*     APFWORK=APFLIBSZ+6;           /* SIZE OF LIBRARY AND VOLID     */
         LA    @12,6                                               0246
         AH    @12,APFLIBSZ                                        0246
         STH   @12,APFWORK                                         0246
*     APFTLEN=AWORK2;               /* UPDATE LENGTH FIELD FOR THIS
*                                      ENTRY                         */
         MVC   APFTLEN(1,APFPTR),AWORK2                            0247
*     APFTVOL=APFTVOL&&APFTVOL;     /* BLANK VOLID TABLE ENTRY       */
         XC    APFTVOL(6,APFPTR),APFTVOL(APFPTR)                   0248
*     APFTVOL=VOLPTR->SCAN(1:APFVOLSZ);/* MOVE VOLID TO TABLE        */
         MVI   APFTVOL+1(APFPTR),C' '                              0249
         MVC   APFTVOL+2(4,APFPTR),APFTVOL+1(APFPTR)               0249
         LH    @12,APFVOLSZ                                        0249
         BCTR  @12,0                                               0249
         L     @11,VOLPTR                                          0249
         EX    @12,@SM02176                                        0249
*     IF SCAN(1)=' ' THEN           /* LAST TEXT RECORD              */
         CLI   SCAN(TEXTPTR),C' '                                  0250
         BNE   @RF00250                                            0250
*       NPA5AEND='1'B;              /* YES - SET SWITCH TO TERM      */
         OI    NPA5AEND,B'01000000'                                0251
*     ELSE                                                         0252
*       ;                           /* NO - SCAN=,                   */
@RF00250 DS    0H                                                  0253
*     APFPTR=APFPTR+APFWORK+1;      /* POINT TO NEXT TABLE ENTRY     */
         LA    @12,1                                               0253
         LR    @11,APFPTR                                          0253
         AH    @11,APFWORK                                         0253
         ALR   @11,@12                                             0253
         LR    APFPTR,@11                                          0253
*     ENTRYCTR=ENTRYCTR+1;          /* UPDATE NO. ENTRIES COUNTER    */
         AH    @12,ENTRYCTR                                        0254
         STH   @12,ENTRYCTR                                        0254
*NPA5TEND:                                                         0255
*   END;                            /* END ENTRY PROCESSING LOOP     */
NPA5TEND DS    0H                                                  0255
@DE00204 TM    NPA5AEND,B'01000000'                                0255
         BZ    @DL00204                                            0255
*   CALL NPA5COMP;                  /* COMPLETE APF TABLE BUILD      */
         BAL   @14,NPA5COMP                                        0256
*   RETURN;                         /* RETURN TO CALLER              */
*                                                                  0257
         B     @EL00001                                            0257
*   /*****************************************************************/
*   /*                                                               */
*   /* BEGIN ERROR PROCESSING                                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0258
*NPA5EROR:                                                         0258
*   CALL NPA5ERTN;                  /* CALL ERROR PROCESSING RTN     */
NPA5EROR BAL   @14,NPA5ERTN                                        0258
*   IF PRMERRC^=RETRY THEN          /* WAS RECOVERY SUCESSFUL        */
         CLI   PRMERRC,X'00'                                       0259
         BE    @RF00259                                            0259
*     DO;                           /* NO - COMPLETE TABLE TO POINT
*                                      OF ERROR                      */
*       CALL NPA5COMP;              /* COMPLETE TABLE BUILD          */
         BAL   @14,NPA5COMP                                        0261
*       RETURN;                     /* RETURN TO CALLER              */
         B     @EL00001                                            0262
*     END;                                                         0263
*   ELSE                                                           0264
*     ;                             /* RECOVERY SUCCESSFUL-REBUILD 0264
*                                      TABLE                         */
@RF00259 DS    0H                                                  0265
*   APFPTR=APFSVCLK;                /* RESTORE PTR TO FIRST ENTRY  0265
*                                      FOLLOWING LINKLIB-SVCLIB    0265
*                                      ENTRIES                       */
         L     APFPTR,APFSVCLK                                     0265
*   NPA5PARM='0'B;                  /* SET PDS READ FLAG             */
*   NPA5AEND='0'B;                  /* SET PROCESS TXT RECORD FLAG   */
         NI    NPA5PARM,B'00111111'                                0267
*   ENTRYCTR=2;                     /* SET COUNTER FOR NO. ENTRIES TO
*                                      2 (LINKLIB=SVCLIB)            */
         MVC   ENTRYCTR(2),@CH00102                                0268
*   GO TO NPA5SCHK;                 /* CHECK SUBSTRING               */
         B     NPA5SCHK                                            0269
*   RESPECIFY                                                      0270
*    (TEXTPTR,                                                     0270
*     APFPTR) UNRESTRICTED;                                        0270
*   GENERATE NOREFS NOSETS(EJECT);                                 0271
*                                                                  0271
         EJECT
*/********************************************************************/
*/*                                                                  */
*/*        SUBROUTINE NPA5COMP                                       */
*/*                                                                  */
*/*        FUNCTION - COMPLETE APF TABLE BUILD PROCESSING            */
*/*                                                                  */
*/*        INPUT    - (1) RCVT  - POINTER TO CVT                     */
*/*                   (2) APFPTR- POINTER TO END OF LAST ENTRY IN    */
*/*                               APF TABLE                          */
*/*                   (3) APFSAVE-POINTER TO START OF APF TABLE      */
*/*                   (4) ENTRYCTR- NO. ENTRIES IN TABLE             */
*/*                                                                  */
*/*        OUTPUT   - (1) CVTAUTHL - POINTER TO THE SYSTEM APF TABLE */
*/*                   (2) APF TABLE HEADER UPDATED WITH NO. ENTRIES  */
*/*                       AND SIZE OF TABLE IN DOUBLEWORDS           */
*/*                                                                  */
*/********************************************************************/
*                                                                  0272
*NPA5COMP:                                                         0272
*   PROC OPTIONS(NOSAVEAREA);                                      0272
         B     @PB00002                                            0272
NPA5COMP STM   @14,@12,12(@13)                                     0272
*   RESPECIFY                                                      0273
*    (RCVT,                                                        0273
*     RNVT,                                                        0273
*     APFPTR,                                                      0273
*     RPARM0) RESTRICTED;                                          0273
*   APFPTR=(APFPTR+7)&ROUNDDWD;     /* ROUND TO DWD BOUNDARY         */
         LA    @12,7                                               0274
         ALR   @12,APFPTR                                          0274
         N     @12,@CF00110                                        0274
         LR    APFPTR,@12                                          0274
*   APFWORK=(APFPTR-APFSAVE)/8;     /* COMPUTE APF TABLE SIZE IN   0275
*                                      DOUBLEWORDS                   */
         L     @12,APFSAVE                                         0275
         LR    @08,APFPTR                                          0275
         SLR   @08,@12                                             0275
         SRDA  @08,32                                              0275
         D     @08,@CF00040                                        0275
         STH   @09,APFWORK                                         0275
*   APFTABLN=AWORK2;                /* UPDATE TOTAL APF TABLE SIZE IN
*                                      APF HEADER                    */
         MVC   APFTABLN(1,@12),AWORK2                              0276
*   APFTNENT=ECNTR2;                /* UPDATE NO. ENTRIES IN TABLE   */
         MVC   APFTNENT(1,@12),ECNTR2                              0277
*   FMSIZE=2040-(APFWORK*8);        /* COMPUTE AMOUNT OF STORAGE NOT
*                                      USED                          */
         SLA   @09,3                                               0278
         LCR   @09,@09                                             0278
         AH    @09,@CH02061                                        0278
         ST    @09,FMSIZE                                          0278
*   IF FMSIZE^=0 THEN               /* IS A FREEMAIN NECESSARY       */
         LTR   @09,@09                                             0279
         BZ    @RF00279                                            0279
*     DO;                           /* YES                           */
*       FMSIZESP=SPF5;              /* SET SUBPOOL TO 245            */
         MVI   FMSIZESP,X'F5'                                      0281
*       RPARM0=FMSIZE;              /* SETTUP REG 0 FOR FREEMAIN     */
*                                                                  0282
         L     RPARM0,FMSIZE                                       0282
*       /*************************************************************/
*       /*                                                           */
*       /* ISSUE FREEMAIN TO FREE AREA NOT USED FROM ADDRESS APFPTR  */
*       /* FOR RPARM0 BYTES                                          */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0283
*       GENERATE REFS(RPARM0,APFPTR) SETS(RPARM)                   0283
*           (FREEMAIN R,LV=(0),A=(APFPTR));/* FREE SPACE             */
         FREEMAIN R,LV=(0),A=(APFPTR)
*     END;                                                         0284
*   ELSE                                                           0285
*     ;                             /* 2040 BYTES USED - NO FRMAIN   */
@RF00279 DS    0H                                                  0286
*   CVTAUTHL=APFSAVE;               /* INSERT APF TAB ADDR IN CVT    */
         MVC   CVTAUTHL(4,CVTPTR),APFSAVE                          0286
*   RETURN;                         /* RETURN TO CALLER              */
@EL00002 DS    0H                                                  0287
@EF00002 DS    0H                                                  0287
@ER00002 LM    @14,@12,12(@13)                                     0287
         BR    @14                                                 0287
*   RESPECIFY                                                      0288
*    (RPARM0) UNRESTRICTED;                                        0288
*   END NPA5COMP;                                                  0289
@PB00002 DS    0H                                                  0290
*   GENERATE NOREFS NOSETS(EJECT);                                 0290
*                                                                  0290
         EJECT
*/********************************************************************/
*/*                                                                  */
*/*        SUBROUTINE NPA5APRM                                       */
*/*                                                                  */
*/*        INPUT    - (1) SCNPTR - POINTER TO SUBSTRING INPUT        */
*/*                                                                  */
*/*        OUTPUT   - (1) PLISTNAM - NAME OF PARMLIB MEMBER, I.E.,   */
*/*                                  IEAAPFXX                        */
*/*                   (2) NPA5PARM - REQUEST TO READ PARMLIB MEMBER  */
*/*                                  IEAAPFXX                        */
*/*                                                                  */
*/********************************************************************/
*                                                                  0291
*NPA5APRM:                                                         0291
*   PROC OPTIONS(NOSAVEAREA);                                      0291
         B     @PB00003                                            0291
NPA5APRM STM   @14,@12,12(@13)                                     0291
*   RESPECIFY                                                      0292
*    (RCVT,                                                        0292
*     RNVT,                                                        0292
*     SCNPTR) RESTRICTED;                                          0292
*NPA5ALFT:                                                         0293
*   DO WHILE PARMCHAR(1)='(';       /* IF FIRST CHAR IS ( SKIP TO  0293
*                                      NEXT CHAR                     */
NPA5ALFT B     @DE00293                                            0293
@DL00293 DS    0H                                                  0294
*     SCNPTR=SCNPTR+1;              /* INCREMENT TXT PTR             */
         AH    SCNPTR,@CH00034                                     0294
*   END;                                                           0295
*                                                                  0295
@DE00293 CLI   PARMCHAR(SCNPTR),C'('                               0295
         BE    @DL00293                                            0295
*   /*****************************************************************/
*   /*                                                               */
*   /* INSURE NEXT 2 CHARACTERS ARE ALPHANUMERIC                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0296
*   RESPECIFY                                                      0296
*    (RI) RESTRICTED;                                              0296
*   DO RI=0 TO 1 BY 1 WHILE PARMCHAR(1)>='0'&PARMCHAR(1)<='9'×/*   0297
*                                      CHECK FOR ALPHANUM            */
*         PARMCHAR(1)>='A'&PARMCHAR(1)<='Z';/* CHARACTERS            */
         SLR   RI,RI                                               0297
@DL00297 CLI   PARMCHAR(SCNPTR),C'0'                               0297
         BL    @GL00004                                            0297
         CLI   PARMCHAR(SCNPTR),C'9'                               0297
         BNH   @DB00297                                            0297
@GL00004 CLI   PARMCHAR(SCNPTR),C'A'                               0297
         BL    @DC00297                                            0297
         CLI   PARMCHAR(SCNPTR),C'Z'                               0297
         BH    @DC00297                                            0297
@DB00297 DS    0H                                                  0298
*     SCNPTR=SCNPTR+1;              /* INCREMENT TO NEXT CHAR        */
         AH    SCNPTR,@CH00034                                     0298
*   END;                                                           0299
         AH    RI,@CH00034                                         0299
         CH    RI,@CH00034                                         0299
         BNH   @DL00297                                            0299
@DC00297 DS    0H                                                  0300
*   IF RI^=2×PARMCHAR(1)^=''''&PARMCHAR(1)^=')' THEN               0300
         CH    RI,@CH00102                                         0300
         BNE   @RT00300                                            0300
         CLI   PARMCHAR(SCNPTR),C''''                              0300
         BE    @RF00300                                            0300
         CLI   PARMCHAR(SCNPTR),C')'                               0300
         BE    @RF00300                                            0300
@RT00300 DS    0H                                                  0301
*     DO;                           /* IF 2 CHAR NOT CHECKED ABOVE OR
*                                      PARMCHAR IS NOT A VALID     0301
*                                      DELIMITER, THEN RETURN WITH 0301
*                                      ERROR CONDITION               */
*       PRMRC='04'X;                /* ERROR CODE TO CALLER          */
         MVI   PRMRC,X'04'                                         0302
*       RETURN;                                                    0303
@EL00003 DS    0H                                                  0303
@EF00003 DS    0H                                                  0303
@ER00003 LM    @14,@12,12(@13)                                     0303
         BR    @14                                                 0303
*     END;                                                         0304
*   ELSE                                                           0305
*     ;                             /* SUBSTRING OK                  */
@RF00300 DS    0H                                                  0306
*   RESPECIFY                                                      0306
*    (RI) UNRESTRICTED;                                            0306
*   SCNPTR=SCNPTR-2;                /* POINT TO FIRST VALID CHAR     */
         BCTR  SCNPTR,0                                            0307
         BCTR  SCNPTR,0                                            0307
*   PLISTEL=PARMCHAR(1:2);          /* SUFFIX FOR IEAAPF MEMBER      */
         MVC   PLISTEL(2),PARMCHAR(SCNPTR)                         0308
*   NPA5PARM='1'B;                  /* REQUEST TO READ PARMLIB MEMBER
*                                      IEAAPFXX                      */
         OI    NPA5PARM,B'10000000'                                0309
*   PRMRC='00'X;                    /* SET GOOD RC                   */
         MVI   PRMRC,X'00'                                         0310
*   RETURN;                         /* RETURN TO CALLER              */
         B     @EL00003                                            0311
*   END NPA5APRM;                   /* END OF SUBROUTINE           0312
*                                                                    */
@PB00003 DS    0H                                                  0313
*   GENERATE NOREFS NOSETS(EJECT);                                 0313
*                                                                  0313
         EJECT
*/********************************************************************/
*/*                                                                  */
*/*        SUBROUTINE NPA5TXTR                                       */
*/*                                                                  */
*/*        INPUT    - (1) NPA5PARM- WHEN SET TO 1 INDICATES PARMLIB  */
*/*                                 MEMBER IEAAPFXX SHOULD BE READ   */
*/*                                 WHEN SET TO 0  INDICATES ONLY    */
*/*                                 TXT READS FROM IEAAPFXX ARE      */
*/*                                 REQUIRED                         */
*/*                   (2) PLISTNAM- PARMLIB LIST NAME,I.E., IEAAPFXX */
*/*                                                                  */
*/*        OUTPUT   - (1) NPA5PARM- ON FIRST CALL TO THIS ROUTINE,IS */
*/*                                 SET TO 0                         */
*/*                   (2) TEXTPTR - POINTER TO FIRST NON-BLANK CHAR  */
*/*                                 OF PARMLIB RECORD                */
*/*                                                                  */
*/********************************************************************/
*                                                                  0314
*NPA5TXTR:                                                         0314
*   PROC OPTIONS(NOSAVE(TEXTPTR));                                 0314
         B     @PB00004                                            0314
NPA5TXTR STM   @14,@03,@SA00004                                    0314
         STM   @05,@12,@SA00004+24                                 0314
*NPA5TPRM:                                                         0315
*   RESPECIFY                                                      0315
*    (RCVT,                                                        0315
*     RNVT,                                                        0315
*     TEXTPTR) RESTRICTED;                                         0315
NPA5TPRM DS    0H                                                  0316
*   IF NPA5PARM='1'B THEN           /* REQUEST TO READ IEAAPFXX      */
         TM    NPA5PARM,B'10000000'                                0316
         BNO   @RF00316                                            0316
*     DO;                           /* YES                           */
*       RESPECIFY                                                  0318
*         NIPPAREA BASED(NVTPAREA);                                0318
*       NIPPANAM=PLISTNAM;          /* PASS IEAAPFXX TO NIPPMPDS     */
         L     @12,NVTPAREA(,RNVT)                                 0319
         MVC   NIPPANAM(8,@12),PLISTNAM                            0319
*       CALL NIPPMPDS;              /* READ PARMLIB MEM IEAAPFXX     */
         BAL   @14,NIPPMPDS                                        0320
*       GENERATE NOREFS NOSETS FLOWS(NPA5TGRC,NPA5TERR);           0321
               B   NPA5TGRC            READ SUCCESSFUL
               B   NPA5TERR            READ ERROR
*NPA5TGRC:                                                         0322
*       NPA5PARM='0'B;              /* ON SUBSEQUENT CALLS TO THIS 0322
*                                      RTN WILL READ TEXT RECORDS    */
NPA5TGRC NI    NPA5PARM,B'01111111'                                0322
*     END;                                                         0323
*   ELSE                                                           0324
*     ;                             /* READ TEXT RECORD REGARDLESS OF
*                                      SWITCH SETTING                */
@RF00316 DS    0H                                                  0325
*   CALL NIPPMTXT;                  /* READ TXT RCD                  */
         BAL   @14,NIPPMTXT                                        0325
*   GENERATE NOREFS NOSETS FLOWS(NPA5TRTR,NPA5TERR);               0326
           B   NPA5TRTR                READ SUCCESSFUL
           B   NPA5TERR                READ ERROR
*NPA5TRTR:                                                         0327
*   RESPECIFY                                                      0327
*    (RPARM) RESTRICTED;                                           0327
NPA5TRTR DS    0H                                                  0328
*   TEXTPTR=RPARM;                  /* RETURN PTR TO TXT RECORD      */
         LR    TEXTPTR,RPARM                                       0328
*   TXTRC='00'X;                    /* SET RC=0                      */
         MVI   TXTRC,X'00'                                         0329
*   RETURN;                         /* RETURN TO CALLER              */
@EL00004 DS    0H                                                  0330
@EF00004 DS    0H                                                  0330
@ER00004 LM    @14,@03,@SA00004                                    0330
         LM    @05,@12,@SA00004+24                                 0330
         BR    @14                                                 0330
*   RESPECIFY                                                      0331
*    (RPARM) UNRESTRICTED;                                         0331
*NPA5TERR:                                                         0332
*                                                                  0332
*   /*****************************************************************/
*   /*                                                               */
*   /* CALLING RTN WILL PROCESS READ ERROR                           */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0332
*   NPA5RTRY='1'B;                  /* INDICATE DIAGNOSTIC ISSUED    */
NPA5TERR OI    NPA5RTRY,B'00100000'                                0332
*   TXTRC='04'X;                    /* INDICATE ERROR                */
         MVI   TXTRC,X'04'                                         0333
*   RETURN;                         /* CALLING ROUTINE WILL PROCES 0334
*                                      ERROR                         */
         B     @EL00004                                            0334
*   END NPA5TXTR;                                                  0335
@PB00004 DS    0H                                                  0336
*   GENERATE NOREFS NOSETS(EJECT);                                 0336
*                                                                  0336
         EJECT
*/********************************************************************/
*/*                                                                  */
*/*        SUBROUTINE NPA5ERTN                                       */
*/*                                                                  */
*/*        FUNCTION - WRITE INDICATIVE ERROR MESSAGE FOR ERROR DURING*/
*/*                   APF TABLE PROCESSING                           */
*/*                                                                  */
*/*        INPUT    -(1) NPA5RTRY - WHEN SET TO 1 INDICATES THAT     */
*/*                                 ERROR CAUSE (I/O ERROR) MESSAGE  */
*/*                                 HAS PREVIOUSLY BEEN ISSUED AND   */
*/*                                 THAT ONLY A RETRY MESSAGE IS     */
*/*                                 REQUIRED.                        */
*/*                  (2) ERRORCD  - INDICATES CAUSE OF ERROR, EITHER:*/
*/*                                                                  */
*/*                                PINV (X'04') - APF PARAMETER ERROR*/
*/*                                                                  */
*/*                                LINV (X'08') - APF LIBRARY INVALID*/
*/*                                                                  */
*/*                                VINV (X'0C') - VOLID IS MISSING   */
*/*                                                                  */
*/*                                STOR (X'10') - EXCEED MAX APF     */
*/*                                               TABLE SIZE         */
*/*                                                                  */
*/*        OUTPUT   -(1) ERROR MESSAGE AND PROMPT TO RESPECIFY       */
*/*                      PARAMETER                                   */
*/*                                                                  */
*/********************************************************************/
*                                                                  0337
*NPA5ERTN:                                                         0337
*   PROC OPTIONS(NOSAVE(SCNPTR));                                  0337
         B     @PB00005                                            0337
NPA5ERTN STM   @14,@04,@SA00005                                    0337
         STM   @06,@12,@SA00005+28                                 0337
*   RESPECIFY                                                      0338
*    (RCVT,                                                        0338
*     RNVT,                                                        0338
*     SCNPTR) RESTRICTED;                                          0338
*                                                                  0338
*   /*****************************************************************/
*   /*                                                               */
*   /* SEE IF ONLY A RETRY IS REQUESTED                              */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0339
*   IF NPA5RTRY='1'B×ERRORCD=PINV THEN/* RETRY ONLY ON I/O           */
         TM    NPA5RTRY,B'00100000'                                0339
         BO    @RT00339                                            0339
         CLI   ERRORCD,X'04'                                       0339
         BE    @RT00339                                            0339
*     GO TO NPA5PRPT;               /* ERROR OR INVALID PARAMETER    */
*                                                                  0340
*   /*****************************************************************/
*   /*                                                               */
*   /* WAS THE APF LIBRARY INVALID FORMAT                            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0341
*   IF ERRORCD=LINV THEN            /* LIBRARY INVALID               */
         CLI   ERRORCD,X'08'                                       0341
         BNE   @RF00341                                            0341
*     MSG9I3=CAUSELIB;              /* YES - SET ERROR CAUSE         */
         MVC   MSG9I3(24),CAUSELIB                                 0342
*   ELSE                                                           0343
*     ;                                                            0343
*                                                                  0343
@RF00341 DS    0H                                                  0344
*   /*****************************************************************/
*   /*                                                               */
*   /* WAS THE VOLID MISSING OR INCORRECT FORMAT                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0344
*   IF ERRORCD=VINV THEN            /* VOLID INVALID ?               */
         CLI   ERRORCD,X'0C'                                       0344
         BNE   @RF00344                                            0344
*     MSG9I3=CAUSEVOL;              /* YES - SET ERROR CAUSE         */
         MVC   MSG9I3(24),CAUSEVOL                                 0345
*   ELSE                                                           0346
*     ;                                                            0346
*                                                                  0346
@RF00344 DS    0H                                                  0347
*   /*****************************************************************/
*   /*                                                               */
*   /* WAS MAX APF TABLE SIZE OF 2040 BYTES EXCEEDED                 */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0347
*   IF ERRORCD=STOR THEN            /* STOR UNAVAILABLE              */
         CLI   ERRORCD,X'10'                                       0347
         BNE   @RF00347                                            0347
*     MSG9I3=CAUSESTR;              /* YES - SET ERROR CAUSE         */
         MVC   MSG9I3(24),CAUSESTR                                 0348
*   ELSE                                                           0349
*     ;                                                            0349
@RF00347 DS    0H                                                  0350
*   MSG9I1=PLISTNAM;                /* PARMLIB LIST NAME             */
         MVI   MSG9I1+8,C' '                                       0350
         MVC   MSG9I1(8),PLISTNAM                                  0350
*   MSG9I2=APF1;                    /* MOVE PARAMETER TO MESSAGE     */
*                                                                  0351
         MVI   MSG9I2+3,C' '                                       0351
         MVC   MSG9I2+4(2),MSG9I2+3                                0351
         MVC   MSG9I2(3),APF1                                      0351
*   /*****************************************************************/
*   /*                                                               */
*   /* ISSUE MESSAGE IEA352I LIST LIS FOR PRM INVALID- CAUSE         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0352
*   GENERATE REFS(RNVT,NVTWTO,MSG9S,MSG9I1,MSG9I2) SETS(RPARM,REXIT,
*       RENTRY)(IEAPMNIP TYPE=WTO,PARAM=MSG9S);/* ISSUE WTO          */
         IEAPMNIP TYPE=WTO,PARAM=MSG9S
*NPA5PRPT:                                                         0353
*                                                                  0353
*   /*****************************************************************/
*   /*                                                               */
*   /* ISSUE PROMPT TO RESPECIFY OR CANCEL THE APF PARAMETER         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0353
*   PASSPARM=APF1;                  /* PASS APF PARAM TO PROMPT      */
NPA5PRPT MVI   PASSPARM+3,C' '                                     0353
         MVC   PASSPARM+4(4),PASSPARM+3                            0353
         MVC   PASSPARM(3),APF1                                    0353
*   GENERATE REFS(PASSPARM,NVTPRMPT,RNVT) SETS(RPARM,REXIT,RENTRY) 0354
*       (IEAPMNIP TYPE=PRMPT,PARAM=PASSPARM,OPT=ERROR);            0354
*                                                                  0354
         IEAPMNIP TYPE=PRMPT,PARAM=PASSPARM,OPT=ERROR
*   /*****************************************************************/
*   /*                                                               */
*   /* DID OPERATOR CANCEL THE MESSAGE                               */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0355
*   RESPECIFY                                                      0355
*    (NPTEPTR) RESTRICTED;                                         0355
*   RESPECIFY                                                      0356
*     NIPPTE BASED(NPTEPTR);                                       0356
*   NPTEPTR=ADDR(APF);              /* GET PARMTAB ENTRY FOR APF     */
         L     NPTEPTR,NVTPTAB(,RNVT)                              0357
         LA    NPTEPTR,APF(,NPTEPTR)                               0357
*   IF NIPPTCAN='1'B THEN                                          0358
         TM    NIPPTCAN(NPTEPTR),B'00100000'                       0358
         BNO   @RF00358                                            0358
*     DO;                                                          0359
*       PRMERRC=CANCEL;             /* OPERATOR CANCELLED RC=4       */
         MVI   PRMERRC,X'04'                                       0360
*       NPA5RTRY='0'B;              /* RESET RETRY-ONLY FLAG         */
         NI    NPA5RTRY,B'11011111'                                0361
*       RETURN;                     /* RETURN TO CALLER              */
@EL00005 DS    0H                                                  0362
@EF00005 DS    0H                                                  0362
@ER00005 LM    @14,@04,@SA00005                                    0362
         LM    @06,@12,@SA00005+28                                 0362
         BR    @14                                                 0362
*     END;                                                         0363
*   ELSE                                                           0364
*     ;                             /* RETRY WITH NEW PARAM          */
@RF00358 DS    0H                                                  0365
*   PRMERRC=RETRY;                  /* SET RC TO 0                   */
         MVI   PRMERRC,X'00'                                       0365
*   SCNPTR=NIPPTADR;                /* RESTORE PTR TO PARMT ENTRY    */
         L     SCNPTR,NIPPTADR(,NPTEPTR)                           0366
*   NPA5RTRY='0'B;                  /* RESET RETRY-ONLY FLAG         */
         NI    NPA5RTRY,B'11011111'                                0367
*   RESPECIFY                                                      0368
*    (NPTEPTR) UNRESTRICTED;                                       0368
*   RETURN;                         /* RETURN TO CALLER              */
         B     @EL00005                                            0369
*   END NPA5ERTN;                   /* END SUBROUTINE              0370
*                                                                    */
@PB00005 DS    0H                                                  0371
*   GENERATE NOREFS NOSETS(EJECT);                                 0371
         EJECT
*NP5PSPIO:                                                         0372
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);/* DUMMY PROCEDURE               */
         B     @PB00006                                            0372
NP5PSPIO DS    0H                                                  0373
*   RESPECIFY                                                      0373
*    (RNVT,                                                        0373
*     RCVT) RESTRICTED;                                            0373
*   RESPECIFY                                                      0374
*    (NIPPMR00,                                                    0374
*     NIPPMR14,                                                    0374
*     NIPPMR15) RESTRICTED;                                        0374
*                                                                  0374
*/********************************************************************/
*/*                                                                  */
*/* MACRO-      IEAPSPIO                                             */
*/*                                                                  */
*/* STATUS-     VERSION DATE=10/24/73, VS/2 RELEASE 2                */
*/*                                                                  */
*/* FUNCTION-   IEAPSPIO IS PART OF THE  SUPERVISOR                  */
*/*             INITIALIZATION PROCESS FOR VS2.                      */
*/*                                                                  */
*/*             THIS MACRO GENERATES INLINE PL/S STATEMENTS          */
*/*             WHICH FORM A SUBROUTINE TO FIND AND READ TEXT        */
*/*             RECORDS FROM SYS1.PARMLIB.                           */
*/*                                                                  */
*/*  REGISTER DEPENDENCIES-                                          */
*/*                                                                  */
*/*                 1. IEAPSPIO DEPENDS ON THE INCLUDING ROUTINES    */
*/*                    BASE REGISTERS FOR ADDRESSABILITY             */
*/*                                                                  */
*/*                 2. REGISTER 3 MUST CONTAIN THE ADDRESS OF        */
*/*                    THE CVT ON ENTRY                              */
*/*                                                                  */
*/*                 3. REGISTER 2 MUST CONTAIN THE ADDRESS OF        */
*/*                    THE NVT ON ENTRY                              */
*/*                                                                  */
*/*                 4. REGISTER 14 MUST CONTAIN THE ADDRESS OF       */
*/*                    THE CALLERS RETURN POINT ON ENTRY             */
*/*                                                                  */
*/* ENTRY POINTS-                                                    */
*/*                                                                  */
*/*    TWO CALLING SEQUENCRS ARE AVAILABLE.  ONE PROVIDES            */
*/*    COMPATIBILITY WITH THE VS2-1 VERSION OF THIS MACRO,           */
*/*    THE OTHER CALLING SEQUENCE WAS DEVELOPED EXPRESSLY            */
*/*    FOR VS2-2.                                                    */
*/*                                                                  */
*/*        VS2-1 COMPATIBLE VERSION-                                 */
*/*                                                                  */
*/*            1. CALL NIPPMPDS TO LOCATE (VIA BLDL) THE MEMBER      */
*/*               OF SYS1.PARMLIB                                    */
*/*                                                                  */
*/*            2. CALL NIPPMTXT TO READ A TEXT RECORD FROM           */
*/*               SYS1.PARMLIB                                       */
*/*                                                                  */
*/*        VS2-2 VERSION-                                            */
*/*                                                                  */
*/*            1. CALL NIPPMMEM TO LOCATE THE MEMBER AND RETURN      */
*/*               A TEXT RECORD                                      */
*/*                                                                  */
*/*            2. REPEAT CALL TO NIPPMMEM TO READ ADDITIONAL         */
*/*               TEXT RECORDS                                       */
*/*                                                                  */
*/* OUTPUT (NORMAL) -                                                */
*/*                                                                  */
*/*     THE ROUTINE WILL SAVE AND RESTORE ALL GENERAL REGISTERS      */
*/*     WITH THE EXCEPTION OF RPARM.  ON A SUCCESSFUL READ,          */
*/*     RPARM (REGISTER 1) WILL BE SET TO POINT TO THE FIRST NON-    */
*/*     BLANK CHARACTER IN THE TEXT RECORD.  IF THE RECORD           */
*/*     COLUMNS 1 THRU 72 ALL CONTAIN BLANKS, RPARM WILL             */
*/*     POINT TO A BLANK IN COLUMN 72.  IF COLUMNS 1 THRU 71         */
*/*     ARE BLANK BUT COLUMN 72 IS NON-BLANK, THE ROUTINE            */
*/*     WILL AUTOMATICALLY ATTEMPT TO READ THE NEXT RECORD.  IN      */
*/*     ALL CASES, COLUMN 72 WILL BE SET TO BLANK AND COLUMN 73 WILL */
*/*     BE SET TO X'FF'.                                             */
*/*                                                                  */
*/* EXIT (NORMAL)-                                                   */
*/*                                                                  */
*/*     VS2-1 AND VS2-2 VERSIONS RETURN TO THE ADDRESS FOUND         */
*/*     IN REGISTER 14 ON ENTRY.                                     */
*/*                                                                  */
*/* EXIT (ERROR)-                                                    */
*/*                                                                  */
*/*     VS2-1 COMPATIBLE VERSION-                                    */
*/*                                                                  */
*/*         RETURN TO THE ADDRESS IN REGISTER 14 +4                  */
*/*                                                                  */
*/*         NOTE- WHEN AN ERROR OCCURS ON AN ATTEMP TO READ A RECORD */
*/*               THAT DOES NOT EXIST (E-O-F),                       */
*/*               IT IS CONSIDERED A PERMANENT I/O ERROR             */
*/*                                                                  */
*/*     VS2-2 VERSION-                                               */
*/*                                                                  */
*/*        RETURN TO THE ADDRESS FOUND IN REGISTER 14 ON ENTRY       */
*/*        WITH REGISTER 1 (RPARM) SET TO A NEGATIVE VALUE.  IN THE  */
*/*        CASE WHERE RPARM IS NEGATIVE ON RETURN, THE NIPPARC FIELD */
*/*        OF THE NIP PATAMETER AREA WILL INDICATE THE REASON FOR    */
*/*        FAILURE AS FOLLOWS-                                       */
*/*                            4 - MEMBER NOT FOUND                  */
*/*                            8 - I/O ERROR DURING BLDL             */
*/*                            12- I/O ERROR DURING READ             */
*/*                            16- END-OF-FILE                       */
*/*                                                                  */
*/*     COMMON TO BOTH VERSIONS-                                     */
*/*                                                                  */
*/*        AN INTERPRETIVE MESSAGE WILL BE WRITTEN TO THE OPERATOR   */
*/*        FOR EACH OF THE FIRST THREE ERROR CONDITIONS LISTED       */
*/*        ABOVE.  THE CALLER MAY SUPPRESS THE 'MEMBER NOT FOUND'    */
*/*        MESSAGE BY SETTING THE NIPPAFL1 FLAG FIELD TO BINARY      */
*/*        ONE IN THE NIP PARAMETER AREA BEFORE CALLING THIS         */
*/*        ROUTINE.  NO MESSAGE WILL BE WRITTEN FOR THE E-O-F        */
*/*        CONDITION IF THE VS2-2 VERSION IS USED.                   */
*/*                                                                  */
*/* MACROS-                                                          */
*/*     THE FOLLOWING MAPPING MACROS MUST BE                         */
*/*     INCLUDED IN THE MODULE WHICH INCLUDES                        */
*/*     IEAPSPIO-                                                    */
*/*                                       IEZIOB                     */
*/*                                       IHANVT                     */
*/*                                       IHAECB                     */
*/*                                       IEAPPNIP                   */
*/*                                       IEFUCBOB                   */
*/*                                       CVT                        */
*/*                                                                  */
*/* NOTES-                                                           */
*/*        THE FIRST FOUR LETTERS OF ALL IEAPSPIO SYMBOLES ARE:      */
*/*        NIPP OR IPPM.                                             */
*/*                                                                  */
*/*        REGISTERS 0, 1, 14, AND 15 WILL BE UNRESTRICTED AT THE    */
*/*        END OF THE SEGMENT GENERATED BY IEAPSPIO.                 */
*/*                                                                  */
*/*        NIPPAREA WILL BE BASED ON NVTPAREA AND IOBSTDRD WILL BE   */
*/*        BASED ON THE ADDR OF NIPPAIOB AT THE END OF THE SEGMENT   */
*/*        GENERATED BY IEAPSPIO                                     */
*/*                                                                  */
*/*        IEAPSPIO MUST BE INCLUDED IN A CODE SEGMENT,              */
*/*        NOT AT THE TOP OF THE PROCEDURE WITH THE                  */
*/*        MAPPING MACROS LISTED ABOVE.                              */
*/*                                                            Y02752*/
