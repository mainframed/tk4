         B     @DE00775                                            0775
@DL00775 DS    0H                                                  0776
*     IF INFIND='0'B&INFALIS='1'B THEN/* TEST FOR UNPROCESSED ALIAS  */
*                                                                  0776
         L     @08,INFOPTR                                         0776
         TM    INFIND(@08),B'10000000'                             0776
         BNZ   @RF00776                                            0776
         L     @08,INFOPTR                                         0776
         TM    INFALIS(@08),B'01000000'                            0776
         BNO   @RF00776                                            0776
*       /*************************************************************/
*       /*                                                           */
*       /* BUILD THE LPDE FOR THIS ALIAS NAME                        */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0777
*       CALL NPA5ALPD;              /* BUILD ALIAS LPDE              */
         BAL   @14,NPA5ALPD                                        0777
*   END;                            /* END OF SCAN LOOP              */
@RF00776 LA    @08,40                                              0778
         AL    @08,INFOPTR                                         0778
         ST    @08,INFOPTR                                         0778
@DE00775 L     @08,INFOPTR                                         0778
         CLC   INFNAME(8,@08),@CB00041                             0778
         BNE   @DL00775                                            0778
*   RETURN;                         /* RETURN TO CALLER              */
         B     @EL00009                                            0779
*   GENERATE NOREFS NOSETS(EJECT);                                 0780
*                                                                  0780
         EJECT
*/********************************************************************/
*/*                                                                  */
*/*         SUBROUTINE NPA5BDIR                                      */
*/*                                                                  */
*/*         FUNCTION - BUILD THE LINK PACK AREA DIRECTORY            */
*/*                                                                  */
*/*         INPUT - THE LPDES FOR ALL THE LPA MODULES                */
*/*            NVTLVIRT - THE LOWEST LPA ADDRESS USED                */
*/*            LPDECT - THE NUMBER OF ENTRIES NEEDED                 */
*/*                                                                  */
*/*         OUTPUT - THE LPA DIRECTORY IS BUILT                      */
*/*            NVTLVIRT IS UPDATED TO THE DIRECTORY START ADDRESS    */
*/*            THE DIRECTORY ADDR IS IN THE NVT                      */
*/*                                                                  */
*/********************************************************************/
*                                                                  0781
*NPA5BDIR:                          /* NPA5BDIR ENTRY PT             */
*   ENTRY;                          /* BEGINNING OF SUBR             */
         B     @EC00781                                            0781
NPA5BDIR STM   @14,@12,@SA00009                                    0781
@EC00781 DS    0H                                                  0782
*   RESPECIFY                                                      0782
*    (RLDADDR,                                                     0782
*     LPDEPTR) RESTRICTED;                                         0782
*   RLDADDR=(NVTLVIRT-LPDECT*LENGTH(LPDE))&ROUNDPAG;/* CALCULATE THE
*                                      ADDRESS OF THE LPA DIRECTORY  */
         L     RLDADDR,LPDECT                                      0783
         MH    RLDADDR,@CH02665                                    0783
         LCR   RLDADDR,RLDADDR                                     0783
         AL    RLDADDR,NVTLVIRT(,RNVT)                             0783
         N     RLDADDR,@CF00123                                    0783
*   IF RLDADDR<NVTVVPG1 THEN        /* CHECK AVAILABLE VIRT STOR     */
         C     RLDADDR,NVTVVPG1(,RNVT)                             0784
         BNL   @RF00784                                            0784
*     CALL NPA5CERR(STUNAV);        /* INDICATE STORAGE UNAVAL       */
         LA    @01,@AL00785                                        0785
         BAL   @14,NPA5CERR                                        0785
*   NVTLVIRT=RLDADDR;               /* RESET LOW PTR TO DIRECTORY  0786
*                                      START ADDRESS.                */
@RF00784 ST    RLDADDR,NVTLVIRT(,RNVT)                             0786
*   NVTLPALO=RLDADDR;               /* PUT INTO NVT          @Z40WPYB*/
*                                                                  0787
         ST    RLDADDR,NVTLPALO(,RNVT)                             0787
*   /*****************************************************************/
*   /*                                                               */
*   /* BUILD THE DUMMY ENTRY AT THE END OF THE DIRECTORY             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0788
*   LPDEPTR=RLDADDR+(LPDECT-1)*LENGTH(LPDE);/* CALCULATE ADDRESS OF
*                                      THE DUMMY ENTRY               */
         L     LPDEPTR,LPDECT                                      0788
         BCTR  LPDEPTR,0                                           0788
         MH    LPDEPTR,@CH02665                                    0788
         ALR   LPDEPTR,RLDADDR                                     0788
*   LPDE=LPDE&&LPDE;                /* CLEAR DUMMY ENTRY             */
         XC    LPDE(40,LPDEPTR),LPDE(LPDEPTR)                      0789
*   LPDENAME=DUMMY;                 /* SET NAME TO FFF...            */
*                                                                  0790
         MVC   LPDENAME(8,LPDEPTR),@CB00041                        0790
*   /*****************************************************************/
*   /*                                                               */
*   /* BUILD THE DIRECTORY BY COPYING ONE SYNONYM QUEUE AT A TIME    */
*   /* FROM THE BLDL INFORMATION TABLE AREA                          */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0791
*   DO SLOTADDR=NA5BLDIR TO BLDIREND BY 4;/* LOOP THROUGH THE BLDL 0791
*                                      INDEX TABLE                   */
         L     @08,NA5BLDIR                                        0791
         ST    @08,SLOTADDR                                        0791
         B     @DE00791                                            0791
@DL00791 DS    0H                                                  0792
*     IF SLOTADDR->NPA5BPTR>1 THEN  /* TEST FOR SYNONYM QUEUE      0792
*                                      STARTING HERE                 */
*                                                                  0792
         CLC   NPA5BPTR(4,@08),@CF00264                            0792
         BNH   @RF00792                                            0792
*       /*************************************************************/
*       /*                                                           */
*       /* COPY LPDES FOR THIS SYNONYM QUEUE TO DIRECTORY            */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0793
*       CALL NPA5RHSH;              /* COPY LPDES                    */
         BAL   @14,NPA5RHSH                                        0793
*   END;                            /* END OF SCAN LOOP              */
@RF00792 LA    @08,4                                               0794
         AL    @08,SLOTADDR                                        0794
         ST    @08,SLOTADDR                                        0794
@DE00791 C     @08,BLDIREND                                        0794
         BNH   @DL00791                                            0794
*   NVTVVPG1=INFOSTRT;              /* RESTORE NVTVVPG1              */
*                                                                  0795
         MVC   NVTVVPG1(4,RNVT),INFOSTRT                           0795
*   /*****************************************************************/
*   /*                                                               */
*   /* MUST PAGE OUT ALL VIRTUAL STORAGE USED TO INSURE ALL REAL PAGE*/
*   /* FRAMES ARE PLACED IN THE PAGE DATA SET. THIS WILL PERMIT NIP  */
*   /* TO QUICK START THE PLPA ON SUBSEQUENT IPLS.                   */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0796
*   RESPECIFY                                                      0796
*    (PGOUTST,                                                     0796
*     PGOUTEND,                                                    0796
*     RENTRY) RESTRICTED;                                          0796
*   PGOUTST=NVTLVIRT;               /* SETTUP START (LOW) ADDRESS    */
         L     PGOUTST,NVTLVIRT(,RNVT)                             0797
*   PGOUTEND=NVTLPAND;              /* SETTUP END (HIGH)     @Z40WPYB*/
         L     PGOUTEND,NVTLPAND(,RNVT)                            0798
*   GENERATE REFS(PGOUTST,PGOUTEND) SETS(RENTRY)(PGOUT R,A=(1),EA=(15))
*       ;                           /* R1=START R15=END              */
         PGOUT R,A=(1),EA=(15)
*   IF RENTRY^=0 THEN               /* WAS THERE A PAGING ERROR      */
         LTR   RENTRY,RENTRY                                       0800
         BZ    @RF00800                                            0800
*     CALL NPA5CERR(PGOUTERR);      /* YES - LOAD WAIT CODE          */
         LA    @01,@AL00801                                        0801
         BAL   @14,NPA5CERR                                        0801
*   ELSE                                                           0802
*     ;                                                            0802
@RF00800 DS    0H                                                  0803
*   RESPECIFY                                                      0803
*    (PGOUTST,                                                     0803
*     PGOUTEND,                                                    0803
*     RENTRY) UNRESTRICTED;                                        0803
*   CALL NP5WTPAG;                  /* WAIT FOR PAGE I/O TO COMPLETE
*                                                            @Z40WPYB*/
         BAL   @14,NP5WTPAG                                        0804
*   CALL ILRQSRIT;                  /* INITIALIZE THE QSR    @Z40WPYB*/
         L     @15,@CV00149                                        0805
         BALR  @14,@15                                             0805
*   RETURN;                         /* RETURN TO CALLER              */
         B     @EL00009                                            0806
*   RESPECIFY                                                      0807
*    (RLDADDR,                                                     0807
*     LPDEPTR) UNRESTRICTED;                                       0807
*   END NPA5LIND;                   /* END OF SUBROUTINE             */
@PB00009 DS    0H                                                  0809
*   GENERATE NOREFS NOSETS(EJECT);                                 0809
*                                                                  0809
         EJECT
*/********************************************************************/
*/*                                                                  */
*/*         SUBROUTINE NPA5RBLK                                      */
*/*                                                                  */
*/*         FUNCTION - READS A DIRECTORY BLOCK OF SYS1.LPALIB        */
*/*                                                                  */
*/*         INPUT - NONE                                             */
*/*                                                                  */
*/*         OUTPUT - THE NEXT DIRECTORY BLOCK IS READ INTO           */
*/*            PDSDATA AND ITS KEY IS IN PDSKEY                      */
*/*            WHEN THE LAST BLOCK HAS BEEN READ IN, LASTBLOK        */
*/*            IS SET TO '1'B                                        */
*/*                                                                  */
*/********************************************************************/
*                                                                  0810
*NPA5RBLK:                          /* NPA5RBLK ENTRY PT             */
*   PROCEDURE OPTIONS(NOSAVE(RLDADDR));/* NOTE - RLDADDR IS PASSED 0810
*                                      BETWEEN SUBROUTINES CONTAINED
*                                      IN THIS AND OTHER PROCEDURES  */
*                                                                  0810
         B     @PB00010                                            0810
NPA5RBLK STM   @14,@03,@SA00010                                    0810
         STM   @05,@12,@SA00010+24                                 0810
*   /*****************************************************************/
*   /*                                                               */
*   /* READ THE NEXT RECORD FROM SYS1.LPALIB                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0811
*   CALL NPA5EXCP;                  /* READ A RECORD                 */
*                                                                  0811
         BAL   @14,NPA5EXCP                                        0811
*   /*****************************************************************/
*   /*                                                               */
*   /* WAIT FOR THE I/O TO COMPLETE                                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0812
*   CALL NPA5IOND;                  /* WAIT TILL POSTED              */
         BAL   @14,NPA5IOND                                        0812
*   IF IONDRC^=IONSUC THEN          /* TEST FOR GOOD I/O             */
         CLI   IONDRC,X'04'                                        0813
         BE    @RF00813                                            0813
*     IF PDSKEY=DUMMY THEN          /* GOOD I/O, TEST FOR LAST     0814
*                                      DIRECTORY BLOCK               */
         CLC   PDSKEY(8),@CB00041                                  0814
         BNE   @RF00814                                            0814
*       LASTBLOK='1'B;              /* LAST BLOCK - SET SW           */
         OI    LASTBLOK,B'10000000'                                0815
*     ELSE                                                         0816
*       ;                           /* NOT LAST BLOCK                */
@RF00814 DS    0H                                                  0817
*   ELSE                            /* I/O ERROR                     */
*     DO;                           /* INDICATE ERROR                */
         B     @RC00813                                            0817
@RF00813 DS    0H                                                  0818
*       NVTCSIOB=ADDR(NIPIOBSN);    /* PUT IOB ADDRESS INTO NVT FOR
*                                      NP05                          */
         LA    @08,NIPIOBSN                                        0818
         ST    @08,NVTCSIOB(,RNVT)                                 0818
*       CALL NPA5CERR(IOERR);       /* PROCESS ERROR                 */
         LA    @01,@AL00819                                        0819
         BAL   @14,NPA5CERR                                        0819
*     END;                                                         0820
*   RETURN;                         /* RETURN TO CALLER              */
@EL00010 DS    0H                                                  0821
@EF00010 DS    0H                                                  0821
@ER00010 LM    @14,@03,@SA00010                                    0821
         LM    @05,@12,@SA00010+24                                 0821
         BR    @14                                                 0821
*   GENERATE NOREFS NOSETS(EJECT);                                 0822
*                                                                  0822
         EJECT
*/********************************************************************/
*/*                                                                  */
*/*         SUBROUTINE NPA5INFO                                      */
*/*                                                                  */
*/*         FUNCTION - BUILD BLDL INFORMATION TABLE ENTRIES FOR THE  */
*/*            MODULES SPECIFIED IN THE DIRECTORY BLOCK JUST READ    */
*/*                                                                  */
*/*         INPUT - DIRECTORY BLOCK IN PDSDATA AND KEY IN PDSKEY     */
*/*            POINTER TO CURRENT INFORMATION TABLE ENTRY (INFOPTR)  */
*/*                                                                  */
*/*         OUTPUT - INFORMATION TABLE ENTRIES FOR THE MODULES       */
*/*            INFOPTR IS SET TO POINT TO THE NEXT AVAILABLE SLOT    */
*/*                                                                  */
*/********************************************************************/
*                                                                  0823
*NPA5INFO:                          /* NPA5INFO ENTRY PT             */
*   ENTRY;                          /* BEGINNING OF SUBR             */
         B     @EC00823                                            0823
NPA5INFO STM   @14,@03,@SA00010                                    0823
         STM   @05,@12,@SA00010+24                                 0823
@EC00823 DS    0H                                                  0824
*   RESPECIFY                                                      0824
*    (PDS2PTR,                                                     0824
*     PDSPTRV) RESTRICTED;          /*                               */
*   PDS2PTR=ADDR(PDSDATA);          /* POINT TO FIRST DE             */
         LA    PDS2PTR,PDSDATA                                     0825
*   LASTENT='0'B;                   /* INDICATE NOT LAST ENTRY IN  0826
*                                      BLOCK                         */
*                                                                  0826
         NI    LASTENT,B'10111111'                                 0826
*   /*****************************************************************/
*   /*                                                               */
*   /* LOOP THROUGH THE DIRECTORY BLOCK, BUILDING A BLDL INFORMATION */
*   /* TABLE ENTRY FOR EACH MODULE                                   */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0827
*   DO WHILE(LASTENT='0'B);         /* PROCESS MODULES               */
*                                                                  0827
         B     @DE00827                                            0827
@DL00827 DS    0H                                                  0828
*     /***************************************************************/
*     /*                                                             */
*     /* INSURE VIRTUAL ADDRESS SPACE FOR NEXT ENTRY                 */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0828
*     IF INFOPTR>NVTLVIRT THEN      /* CHECK FOR AVAIL STORAGE       */
         CLC   INFOPTR(4),NVTLVIRT(RNVT)                           0828
         BNH   @RF00828                                            0828
*       CALL NPA5CERR(STUNAV);      /* INDICATE STOR UNAVAILABLE     */
         LA    @01,@AL00829                                        0829
         BAL   @14,NPA5CERR                                        0829
*     ELSE                                                         0830
*       ;                                                          0830
@RF00828 DS    0H                                                  0831
*     INFODATA=INFODATA&&INFODATA;  /* ZERO DIRECTRY ENTRY           */
*                                                                  0831
         L     @06,INFOPTR                                         0831
         XC    INFODATA(40,@06),INFODATA(@06)                      0831
*     /***************************************************************/
*     /*                                                             */
*     /* SET UP BLDL INFORMATION TABLE ENTRY                         */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0832
*     INFNAME=PDS2NAME;             /* MODULE NAME                   */
         L     @06,INFOPTR                                         0832
         MVC   INFNAME(8,@06),PDS2NAME(PDS2PTR)                    0832
*     IF PDS2NAME=PDSKEY THEN       /* TEST FOR LAST ENTRY IN THIS 0833
*                                      DIRECTORY BLOCK               */
         CLC   PDS2NAME(8,PDS2PTR),PDSKEY                          0833
         BNE   @RF00833                                            0833
*       LASTENT='1'B;               /* IF SO, SET SWITCH             */
         OI    LASTENT,B'01000000'                                 0834
*     IF PDS2NAME^=DUMMY THEN       /* TEST FOR LAST ENTRY           */
@RF00833 CLC   PDS2NAME(8,PDS2PTR),@CB00041                        0835
         BE    @RF00835                                            0835
*       DO;                         /* PROCESS NEXT ENTRY            */
*         INFTTRP=PDS2TTRP;         /* TTR OF FIRST BLOCK            */
         L     @06,INFOPTR                                         0837
         MVC   INFTTRP(3,@06),PDS2TTRP(PDS2PTR)                    0837
*         PDS2PTR=PDS2PTR-2;        /* ADJUST POINTER TO ACCOUNT FOR
*                                      THE DIFFERENCE IN SIZE BETWEEN
*                                      THE ACTUAL DATA (PDS1 FORMAT)
*                                      AND THE DSECT (PDS2 FORMAT)   */
         BCTR  PDS2PTR,0                                           0838
         BCTR  PDS2PTR,0                                           0838
*         INFEPA=PDS2EPA;           /* ENTRY POINT ADDRESS           */
         L     @06,INFOPTR                                         0839
         MVC   INFEPA(3,@06),PDS2EPA(PDS2PTR)                      0839
*         IF PDSAOSLE='0'B          /* OS LINKAGE EDITOR             */
*             &PDS2ALIS='1'B        /* ALIAS                         */
*             &PDS2RENT='0'B        /* NOT REENTRANT                 */
*             &PDS2REUS='0'B THEN   /* NOT REUSABLE - THIS IS A DUMMY
*                                      ALIAS ENTRY FOR WHICH THERE IS
*                                      NO ALIAS SECTION IN THE PDS   */
         TM    PDSAOSLE(PDS2PTR),B'10000000'                       0840
         BNZ   @RF00840                                            0840
         TM    PDS2ALIS(PDS2PTR),B'10000000'                       0840
         BNO   @RF00840                                            0840
         TM    PDS2RENT(PDS2PTR),B'11000000'                       0840
         BNZ   @RF00840                                            0840
*           PDS2ALIS='0'B;          /* RESET ALIAS FLAG              */
         NI    PDS2ALIS(PDS2PTR),B'01111111'                       0841
*         IF PDS2ALIS='1'B THEN     /* TEST FOR ALIAS                */
@RF00840 TM    PDS2ALIS(PDS2PTR),B'10000000'                       0842
         BNO   @RF00842                                            0842
*           DO;                     /* BUILD ALIAS INFO              */
*             INFALIS='1'B;         /* SET ALIAS FLAG                */
         L     @06,INFOPTR                                         0844
         OI    INFALIS(@06),B'01000000'                            0844
*             PDSPTRV=ADDR(PDSBCEND);/* POINT PAST BASIC SECTION     */
         LA    PDSPTRV,PDSBCEND(,PDS2PTR)                          0845
*             IF PDS2SCTR='1'B THEN /* SCTR DATA IN PDS              */
         TM    PDS2SCTR(PDS2PTR),B'00000100'                       0846
         BNO   @RF00846                                            0846
*               PDSPTRV=PDSPTRV+LENGTH(PDSS01);/* YES, BYPASS SCTR 0847
*                                      DATA AND POINT TO ALIAS DATA  */
         LA    PDSPTRV,8(,PDSPTRV)                                 0847
*             INFMNM=PDS2MNM;       /* MAJOR NAME                    */
@RF00846 L     @06,INFOPTR                                         0848
         MVC   INFMNM(8,@06),PDS2MNM(PDSPTRV)                      0848
*           END;                                                   0849
*         ELSE                      /* NOT ALIAS ENTRY               */
*           DO;                     /* BUILD MAJOR ENTRY             */
         B     @RC00842                                            0850
@RF00842 DS    0H                                                  0851
*             IF PDS2LOAD='1'B THEN /* TEST LOAD FLAG                */
         TM    PDS2LOAD(PDS2PTR),B'00001000'                       0851
         BNO   @RF00851                                            0851
*               INFLOAD='1'B;       /* LOADABLE ONLY                 */
         L     @06,INFOPTR                                         0852
         OI    INFLOAD(@06),B'00100000'                            0852
*             IF PDS2EXEC='1'B THEN /* TEST EXEC FLAG                */
@RF00851 TM    PDS2EXEC(PDS2PTR),B'00000010'                       0853
         BNO   @RF00853                                            0853
*               INFEXEC='1'B;       /* EXECUTABLE                    */
         L     @06,INFOPTR                                         0854
         OI    INFEXEC(@06),B'00010000'                            0854
*             IF PDS21BLK='1'B THEN /* TEST 1BLK FLAG                */
@RF00853 TM    PDS21BLK(PDS2PTR),B'00000001'                       0855
         BNO   @RF00855                                            0855
*               INF1BLK='1'B;       /* ONLY 1 TXT RECORD             */
         L     @06,INFOPTR                                         0856
         OI    INF1BLK(@06),B'00001000'                            0856
*             IF PDS2NRLD='1'B THEN /* TEST NRLD FLAG                */
@RF00855 TM    PDS2NRLD(PDS2PTR),B'00010000'                       0857
         BNO   @RF00857                                            0857
*               INFNRLD='1'B;       /* NO RLD ITEMS                  */
         L     @06,INFOPTR                                         0858
         OI    INFNRLD(@06),B'00000100'                            0858
*             IF PDS2REFR='1'B THEN /* TEST REFR FLAG                */
@RF00857 TM    PDS2REFR(PDS2PTR),B'00000001'                       0859
         BNO   @RF00859                                            0859
*               INFREFR='1'B;       /* REFRESHABLE                   */
         L     @06,INFOPTR                                         0860
         OI    INFREFR(@06),B'00000010'                            0860
*             IF PDS2ORG0='1'B THEN /* TEST ORG0 FLAG                */
@RF00859 TM    PDS2ORG0(PDS2PTR),B'01000000'                       0861
         BNO   @RF00861                                            0861
*               INFORG0='1'B;       /* ZERO ORIGIN TXT BLK           */
         L     @06,INFOPTR                                         0862
         OI    INFORG0(@06),B'00010000'                            0862
*             INFTTRT=PDS2TTRT;     /* TTR OF 1ST TEXT BLK           */
@RF00861 L     @06,INFOPTR                                         0863
         MVC   INFTTRT(3,@06),PDS2TTRT(PDS2PTR)                    0863
*             INFFTBL=PDS2FTBL;     /* FIRST TXT BLK LEN             */
         L     @06,INFOPTR                                         0864
         MVC   INFFTBL(2,@06),PDS2FTBL(PDS2PTR)                    0864
*             INFSTOR=PDS2STOR;     /* MODULE SIZE                   */
         L     @06,INFOPTR                                         0865
         MVC   INFSTOR(3,@06),PDS2STOR(PDS2PTR)                    0865
*             IF PDSAOSLE='1'B THEN /* AOS LINK EDIT                 */
         TM    PDSAOSLE(PDS2PTR),B'10000000'                       0866
         BNO   @RF00866                                            0866
*               DO;                 /* YES, SPECIAL PROC             */
*                 INFAOSLE='1'B;    /* AOS LINKAGE EDIT              */
         L     @06,INFOPTR                                         0868
         OI    INFAOSLE(@06),B'00000001'                           0868
*                 IF PDS2PAGA='1'B THEN/* TEST PAGA FLG              */
         TM    PDS2PAGA(PDS2PTR),B'00100000'                       0869
         BNO   @RF00869                                            0869
*                   INFPAGA='1'B;   /* PAGE ALIGNED MODULE           */
         L     @06,INFOPTR                                         0870
         OI    INFPAGA(@06),B'01000000'                            0870
*                 IF PDSAPFLG='1'B THEN/* TEST APFLG                 */
@RF00869 TM    PDSAPFLG(PDS2PTR),B'00001000'                       0871
         BNO   @RF00871                                            0871
*                   DO;             /* APF INFO EXISTS               */
*                     INFAPFLG='1'B;/* APF IS VALID                  */
         L     @06,INFOPTR                                         0873
         OI    INFAPFLG(@06),B'00100000'                           0873
*                     PDSPTRV=ADDR(PDSBCEND);/* POINT PAST BASIC   0874
*                                      SECTION                       */
         LA    PDSPTRV,PDSBCEND(,PDS2PTR)                          0874
*                     IF PDS2SCTR='1'B THEN/* SCATTER?               */
         TM    PDS2SCTR(PDS2PTR),B'00000100'                       0875
         BNO   @RF00875                                            0875
*                       PDSPTRV=PDSPTRV+LENGTH(PDSS01);/* YES, POINT
*                                      PAST SCATTER SECTION          */
         LA    PDSPTRV,8(,PDSPTRV)                                 0876
*                     IF PDS2SSI='1'B THEN/* SSI?                    */
@RF00875 TM    PDS2SSI(PDS2PTR),B'00010000'                        0877
         BNO   @RF00877                                            0877
*                       PDSPTRV=PDSPTRV+LENGTH(PDSS03)+1;/* YES,   0878
*                                      POINT PAST SSI SECTION - NOTE
*                                      - THE ADDRESS MUST BE ROUNDED
*                                      UP BY 1 BYTE BECAUSE THE SSI
*                                      SECTION STARTS ON A HALFWORD
*                                      BOUNDARY AND THERE IS NO ALIAS
*                                      SECTION                       */
         LA    PDSPTRV,5(,PDSPTRV)                                 0878
*                     INFAPF=PDSAPF;/* APF DATA                      */
@RF00877 L     @06,INFOPTR                                         0879
         MVC   INFAPF(2,@06),PDSAPF(PDSPTRV)                       0879
*                   END;                                           0880
*               END;                                               0881
@RF00871 DS    0H                                                  0882
*           END;                                                   0882
@RF00866 DS    0H                                                  0883
*         IF PDS2EXEC='0'B THEN     /* IS MOD NON-EXEC?      @ZD04022*/
@RC00842 TM    PDS2EXEC(PDS2PTR),B'00000010'                       0883
         BNZ   @RF00883                                            0883
*           DO;                     /*                       @ZD04022*/
*             GEN REFS(NVTREAL,RNVT) SETS(RENTRY,REXIT)            0885
*                 (IEAPMNIP TYPE=REAL);/* SWITCH TO V=R      @ZA14202*/
         IEAPMNIP TYPE=REAL
*             MSGNXMD=INFNAME;      /* SEND MSG IEA358 I IF YES    0886
*                                                            @ZD04022*/
         L     @06,INFOPTR                                         0886
         MVC   MSGNXMD(8),INFNAME(@06)                             0886
*             GEN REFS(RNVT,NVTWTO,MSGNXS) SETS(RPARM,REXIT,RENTRY)
*                 (IEAPMNIP TYPE=WTO,PARAM=MSGNXS);/*        @ZD04022*/
         IEAPMNIP TYPE=WTO,PARAM=MSGNXS
*             GEN REFS(NVTVIRT,RNVT) SETS(RENTRY,REXIT)            0888
*                 (IEAPMNIP TYPE=VIRT);/* SWITCH TO V=V      @ZA14202*/
         IEAPMNIP TYPE=VIRT
*             NP5NONEX='1'B;        /* SET NON-EXECUTABLE MODULE   0889
*                                      FOUND FLAG            @ZD04022*/
         OI    NP5NONEX,B'00010000'                                0889
*           END;                    /*                       @ZD04022*/
*         LPDECT=LPDECT+1;          /* STEP MODULE COUNT             */
@RF00883 LA    @06,1                                               0891
         AL    @06,LPDECT                                          0891
         ST    @06,LPDECT                                          0891
*         PDS2PTR=PDS2PTR+14+(PDS2INDC&'1F'X)*2;/* POINT TO NEXT PDS
*                                      ENTRY                         */
         LA    @06,14                                              0892
         ALR   @06,PDS2PTR                                         0892
         LA    @00,31                                              0892
         SLR   @04,@04                                             0892
         IC    @04,PDS2INDC(,PDS2PTR)                              0892
         NR    @00,@04                                             0892
         ALR   @00,@00                                             0892
         ALR   @06,@00                                             0892
         LR    PDS2PTR,@06                                         0892
*         INFOPTR=INFOPTR+LENGTH(INFODATA);/* POINT TO NEXT INFO   0893
*                                      TABLE ENTRY                   */
         LA    @06,40                                              0893
         AL    @06,INFOPTR                                         0893
         ST    @06,INFOPTR                                         0893
*       END;                        /* IF DUMMY - SKIP ABOVE         */
*   END;                            /* END OF DIRECTORY SCAN LOOP    */
@RF00835 DS    0H                                                  0895
@DE00827 TM    LASTENT,B'01000000'                                 0895
         BZ    @DL00827                                            0895
*   RETURN;                         /* RETURN TO CALLER              */
         B     @EL00010                                            0896
*   RESPECIFY                                                      0897
*    (PDS2PTR,                                                     0897
*     PDSPTRV) UNRESTRICTED;                                       0897
*   GENERATE NOREFS NOSETS(EJECT);                                 0898
*                                                                  0898
         EJECT
