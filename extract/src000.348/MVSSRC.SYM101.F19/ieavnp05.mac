*/*                                                                  */
*/*         SUBROUTINE NPA5HSHA                                      */
*/*                                                                  */
*/*         FUNCTION - CALCULATES THE PROPER HASH TABLE INDEX AND    */
*/*            BUILDS THE HASHING ALGORITHM POINTERS IN THE BLDL     */
*/*            INFORMATION TABLE ENTRIES                             */
*/*                                                                  */
*/*         INPUT - NUMBER OF MODULES IN DIRECTORY (LPDECT),PTR TO   */
*/*                 LAST BLDL INFO TABLE ENTRY, I.E., DUMMY(INFOPTR) */
*/*                                                                  */
*/*         OUTPUT - HASH TABLE INDEX (NPA5HASH,NVTLPAHA)            */
*/*            NVTVVPG1 POINTS TO THE SEGMENT ABOVE THE END OF       */
*/*            THE BLDL INFORMATION TABLE                            */
*/*                                                                  */
*/********************************************************************/
*                                                                  0899
*NPA5HSHA:                          /* NPA5HSHA ENTRY PT             */
*   ENTRY;                          /* BEGINNING OF SUBR             */
         B     @EC00899                                            0899
NPA5HSHA STM   @14,@03,@SA00010                                    0899
         STM   @05,@12,@SA00010+24                                 0899
@EC00899 DS    0H                                                  0900
*   RESPECIFY                                                      0900
*    (RLDADDR,                                                     0900
*     BLDLTSZ1,                                                    0900
*     BLDLTSZ2,                                                    0900
*     RBLDL) RESTRICTED;            /*                               */
*   INFNAME=DUMMY;                  /* SET UP DUMMY ENTRY            */
         L     @08,INFOPTR                                         0901
         MVC   INFNAME(8,@08),@CB00041                             0901
*   LPDECT=LPDECT+1;                /* STEP MODULE COUNT             */
         LA    @08,1                                               0902
         L     @00,LPDECT                                          0902
         ALR   @00,@08                                             0902
         ST    @00,LPDECT                                          0902
*   NPA5HASH=NIPDIRHT(MIN(LPDECT/100+1,DIM(NIPDIRHT)));/* PICK UP  0903
*                                      PROPER HASH INDEX FROM THE  0903
*                                      HASH INDEX TABLE (NIPDIRHT)   */
         LR    @14,@00                                             0903
         SRDA  @14,32                                              0903
         D     @14,@CF03665                                        0903
         ALR   @15,@08                                             0903
         LA    @08,31                                              0903
         CR    @15,@08                                             0903
         BNH   *+6
         LR    @15,@08                                             0903
         SLA   @15,2                                               0903
         L     @08,NIPDIRHT-4(@15)                                 0903
         ST    @08,NPA5HASH                                        0903
*   NVTLPAHA=NPA5HASH;              /* PUT HASH VALUE INTO NVT     0904
*                                                            @Z40WPYB*/
         ST    @08,NVTLPAHA(,RNVT)                                 0904
*   NA5BLDIR=INFOPTR+LENGTH(INFODATA);/* POINT TO BEGINNING OF INDEX
*                                      TABLE                         */
         LA    @08,40                                              0905
         AL    @08,INFOPTR                                         0905
         ST    @08,NA5BLDIR                                        0905
*   BLDIREND=NA5BLDIR+(LPDECT-1)*4; /* CALCULATE ADDRESS OF END OF 0906
*                                      TABLE                         */
         BCTR  @00,0                                               0906
         SLA   @00,2                                               0906
         ALR   @08,@00                                             0906
         ST    @08,BLDIREND                                        0906
*   NPA5NWST=BLDIREND+4;            /* SET UP BASE FOR WASTED AREA 0907
*                                      CONTROL BLOCK SPACE           */
         AH    @08,@CH00058                                        0907
         ST    @08,NPA5NWST                                        0907
*   NPA5LWST=(NPA5NWST+WASTTBLS+PAGEM1)&ROUNDPAG;/* CALC ADDRESS OF
*                                      END OF WASTED AREA CONTROL  0908
*                                      BLOCK SPACE                   */
         AH    @08,@CH04111                                        0908
         N     @08,@CF00123                                        0908
         ST    @08,NPA5LWST                                        0908
*   IF NPA5LWST>NVTLVIRT THEN       /* CHECK FOR AVAIL STORAGE       */
         C     @08,NVTLVIRT(,RNVT)                                 0909
         BNH   @RF00909                                            0909
*     CALL NPA5CERR(STUNAV);        /* INDICATE STORAGE UNAVAIL      */
         LA    @01,@AL00910                                        0910
         BAL   @14,NPA5CERR                                        0910
*   ELSE                                                           0911
*     ;                             /* STORAGE AVAILABLE             */
@RF00909 DS    0H                                                  0912
*   RLDADDR=NA5BLDIR;               /* SET UP LOW ADDRESS            */
*                                                                  0912
         L     @08,NA5BLDIR                                        0912
         LR    RLDADDR,@08                                         0912
*   /*****************************************************************/
*   /*                                                               */
*   /* CLEAR THE AREA FOR THE BLDL INDEX TABLE TO ZEROS              */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0913
*   BLDLTSZ1=BLDIREND-NA5BLDIR+4;   /* CALCULATE LENGTH OF BLDL INFO
*                                      INDEX TABLE                   */
         LCR   @08,@08                                             0913
         AL    @08,BLDIREND                                        0913
         LA    @08,4(,@08)                                         0913
         LR    BLDLTSZ1,@08                                        0913
*   RBLDL=ADDR(ZEROQSR);            /* POINT TO ZEROS                */
         LA    RBLDL,ZEROQSR                                       0914
*   BLDLTSZ2=0;                     /* LENGTH OF SOURCE FIELD - THIS
*                                      FORCES PADDING OF ZEROS TO  0915
*                                      CLEAR AREA                    */
         SLR   BLDLTSZ2,BLDLTSZ2                                   0915
*   GENERATE REFS(RLDADDR,BLDLTSZ1,RBLDL,BLDLTSZ2) SETS(RLDADDR,   0916
*       BLDLTSZ1,RBLDL,BLDLTSZ2)(MVCL  RLDADDR,RBLDL);/* CLEAR THE 0916
*                                      SPACE                         */
         MVCL  RLDADDR,RBLDL
*   RESPECIFY                                                      0917
*    (RLDADDR,                                                     0917
*     BLDLTSZ1,                                                    0917
*     BLDLTSZ2,                                                    0917
*     RBLDL) UNRESTRICTED;                                         0917
*   BLDIREND->NPA5BPTR=1;           /* MARK DUMMY ENTRY INDEX TAB  0918
*                                      ENTRY AS 'MOVED TO DIREC'     */
*                                                                  0918
         L     @08,BLDIREND                                        0918
         MVC   NPA5BPTR(4,@08),@CF00264                            0918
*   /*****************************************************************/
*   /*                                                               */
*   /* LOOP THROUGH ALL BLDL INFORMATION TABLE ENTRIES, PUTTING THE  */
*   /* PROPER CHAIN POINTERS INTO THE SYNONYM CHAIN FIELDS NOTE -    */
*   /* RPARM0 WILL BE RESTRICTED BEFORE FOLLOWING LOOP DUE TO BUG IN */
*   /* PLS, AND UNRESTRICTED THEREAFTER                              */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0919
*   RESPECIFY                                                      0919
*    (RPARM0) RESTRICTED;                                          0919
*   DO INFOPTR=NVTVVPG1 BY LENGTH(INFODATA) WHILE(INFNAME^=DUMMY);/*
*                                      LOOP THRU TABLE               */
         L     @08,NVTVVPG1(,RNVT)                                 0920
         ST    @08,INFOPTR                                         0920
         B     @DE00920                                            0920
@DL00920 DS    0H                                                  0921
*     MODULE=INFNAME;               /* PICK UP MODULE NAME           */
*                                                                  0921
         L     @08,INFOPTR                                         0921
         MVC   MODULE(8),INFNAME(@08)                              0921
*     /***************************************************************/
*     /*                                                             */
*     /* FIND INDEX TABLE ENTRY FOR THIS MODULE NAME                 */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0922
*     CALL NPA5FSLT;                /* FIND SLOT                     */
         BAL   @14,NPA5FSLT                                        0922
*     INFSYNM=SLOTADDR->NPA5BPTR;   /* COPY Q PTR                    */
         L     @08,INFOPTR                                         0923
         L     @15,SLOTADDR                                        0923
         MVC   INFSYNM(3,@08),NPA5BPTR+1(@15)                      0923
*     SLOTADDR->NPA5BPTR=INFOPTR;   /* ADD TO TOP OF Q               */
         L     @08,SLOTADDR                                        0924
         MVC   NPA5BPTR(4,@08),INFOPTR                             0924
*   END;                            /* END OF TABLE SCAN             */
         LA    @08,40                                              0925
         AL    @08,INFOPTR                                         0925
         ST    @08,INFOPTR                                         0925
@DE00920 L     @08,INFOPTR                                         0925
         CLC   INFNAME(8,@08),@CB00041                             0925
         BNE   @DL00920                                            0925
*   RESPECIFY                                                      0926
*    (RPARM0) UNRESTRICTED;         /* RESTORE STATUS OF R0          */
*   NVTVVPG1=(NPA5LWST+SEGMNTM1)&ROUNDSEG;/* STEP NVTVVPG1 PAST END
*                                      OF BLDL INFO TABLE AND ROUND
*                                      TO THE NEXT SEGMENT BOUNDARY  */
         L     @08,@CF00048                                        0927
         AL    @08,NPA5LWST                                        0927
         N     @08,@CF00125                                        0927
         ST    @08,NVTVVPG1(,RNVT)                                 0927
*   RETURN;                         /* RETURN TO CALLER              */
         B     @EL00010                                            0928
*   GENERATE NOREFS NOSETS(EJECT);                                 0929
*                                                                  0929
         EJECT
*/********************************************************************/
*/*                                                                  */
*/*         SUBROUTINE NPA5VCHK                                      */
*/*                                                                  */
*/*         FUNCTION - VALIDITY CHECK ALL ALIAS ENTRIES AND REPORT   */
*/*            ALL ENTRIES WHOSE MAJOR NAMES DO NOT EXIST IN THE     */
*/*            BLDL TABLE AS MAJOR ENTRIES                           */
*/*                                                                  */
*/*         INPUT - BLDL INFORMATION TABLE                           */
*/*                                                                  */
*/*         OUTPUT - INVALID ENTRIES ARE REMOVED FROM THE SYNONYM    */
*/*            QUEUES AND ARE MARKED AS ALREADY LOADED               */
*/*                                                                  */
*/********************************************************************/
*                                                                  0930
*NPA5VCHK:                          /* NPA5VCHK ENTRY PT             */
*   ENTRY;                          /* BEGINNING OF SUBR             */
         B     @EC00930                                            0930
NPA5VCHK STM   @14,@03,@SA00010                                    0930
         STM   @05,@12,@SA00010+24                                 0930
@EC00930 DS    0H                                                  0931
*   RESPECIFY                                                      0931
*    (LPDEPTR) RESTRICTED;          /*                               */
*   DO INFOPTR=INFOSTRT BY LENGTH(INFODATA) WHILE(INFNAME^=DUMMY);/*
*                                      SCAN ENTIRE TABLE             */
         L     @08,INFOSTRT                                        0932
         ST    @08,INFOPTR                                         0932
         B     @DE00932                                            0932
@DL00932 DS    0H                                                  0933
*     IF INFALIS='1'B THEN          /* LOOK FOR ALIAS ENTRIES        */
         L     @08,INFOPTR                                         0933
         TM    INFALIS(@08),B'01000000'                            0933
         BNO   @RF00933                                            0933
*       DO;                         /* ALIAS ENTRY - TEST IT FOR   0934
*                                      VALIDITY                      */
*         MODULE=INFMNM;            /* PICK UP MAJOR NAME            */
*                                                                  0935
         L     @08,INFOPTR                                         0935
         MVC   MODULE(8),INFMNM(@08)                               0935
*         /***********************************************************/
*         /*                                                         */
*         /* FIND START OF SYNONYM QUEUE FOR THIS NAME               */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0936
*         CALL NPA5FSLT;            /* FIND QUEUE HEADER             */
         BAL   @14,NPA5FSLT                                        0936
*         LPDEPTR=SLOTADDR->NPA5BPTR;/* PICK UP ADDRESS OF FIRST   0937
*                                      ENTRY IN Q                    */
         L     @08,SLOTADDR                                        0937
         L     LPDEPTR,NPA5BPTR(,@08)                              0937
*         DO WHILE(LPDEPTR^=0&LPDEPTR->INFNAME^=MODULE);/* SEARCH THE
*                                      QUEUE FOR THE MODULE          */
         B     @DE00938                                            0938
@DL00938 DS    0H                                                  0939
*           LPDEPTR=LPDEPTR->INFSYNM;/* PICK UP ADDRESS OF NEXT ENTRY
*                                      IN QUEUE                      */
         SLR   @08,@08                                             0939
         ICM   @08,7,INFSYNM(LPDEPTR)                              0939
         LR    LPDEPTR,@08                                         0939
*         END;                      /* END OF SEARCH LOOP            */
@DE00938 LTR   LPDEPTR,LPDEPTR                                     0940
         BZ    @DC00938                                            0940
         CLC   INFNAME(8,LPDEPTR),MODULE                           0940
         BNE   @DL00938                                            0940
@DC00938 DS    0H                                                  0941
*         IF LPDEPTR=0 THEN         /* WAS MODULE FOUND              */
         LTR   LPDEPTR,LPDEPTR                                     0941
         BNZ   @RF00941                                            0941
*           DO;                     /* NO, ALIAS ENTRY IS INVALID    */
*             M301FI1=MODULE;       /* PUT MODULE NAME INTO MESSAGE
*                                      IEA301I                       */
         MVC   M301FI1(8),MODULE                                   0943
*             M356FI1=INFNAME;      /* PUT ALIAS NAME INTO MESSAGE 0944
*                                      IEA356I                       */
         L     @08,INFOPTR                                         0944
         MVC   M356FI1(8),INFNAME(@08)                             0944
*             M356FI2=MODULE;       /* PUT MODULE NAME INTO MESSAGE
*                                      IEA356I                       */
*                                                                  0945
         MVC   M356FI2(8),MODULE                                   0945
*             /*******************************************************/
*             /*                                                     */
*             /* SWITCH TO V=R TCB FOR WTO                           */
*             /*                                                     */
*             /*******************************************************/
*                                                                  0946
*             GENERATE REFS(NVTREAL,RNVT) SETS(RENTRY,REXIT)       0946
*                 (IEAPMNIP TYPE=REAL);/* SWITCH TO V=R              */
         IEAPMNIP TYPE=REAL
*/********************************************************************/
*/*                                                                  */
*/*         IEA301I 'MODNAME' NOT FOUND IN SYS1.LPALIB               */
*/*         IEA356I ALIAS 'ALIASNAME' OF MODULE 'MODNAME' IGNORED    */
*/*                                                                  */
*/********************************************************************/
*                                                                  0947
*             GENERATE REFS(RNVT,NVTWTO,MSG301I,M301FI1,MSG356I,M356FI1
*                 ,M356FI2) SETS(RPARM,REXIT,RENTRY);              0947
*                                                                  0947
*                                                 /* WTO MSGS        */
            IEAPMNIP TYPE=WTO,PARAM=MSG301I  WTO MSG IEA301I
            IEAPMNIP TYPE=WTO,PARAM=MSG356I  WTO MSG IEA356I
*             /*******************************************************/
*             /*                                                     */
*             /* SWITCH TCB BACK TO V=V                              */
*             /*                                                     */
*             /*******************************************************/
*                                                                  0948
*             GENERATE REFS(NVTVIRT,RNVT) SETS(RENTRY,REXIT)       0948
*                 (IEAPMNIP TYPE=VIRT);/* SWITCH TO V=V              */
         IEAPMNIP TYPE=VIRT
*             INFIND='1'B;          /* INDICATE MODULE ALREADY LOADED*/
         L     @08,INFOPTR                                         0949
         OI    INFIND(@08),B'10000000'                             0949
*             LPDECT=LPDECT-1;      /* DECREMENT MODULE COUNT BY 1   */
         L     @08,LPDECT                                          0950
         BCTR  @08,0                                               0950
         ST    @08,LPDECT                                          0950
*             MODULE=INFNAME;       /* PICK UP ALIAS NAME            */
*                                                                  0951
         L     @08,INFOPTR                                         0951
         MVC   MODULE(8),INFNAME(@08)                              0951
*             /*******************************************************/
*             /*                                                     */
*             /* FIND START OF SYNONYM QUEUE CONTAINING ALIAS        */
*             /*                                                     */
*             /*******************************************************/
*                                                                  0952
*             CALL NPA5FSLT;        /* FIND QUEUE HEADER             */
         BAL   @14,NPA5FSLT                                        0952
*             LPDEPTR=SLOTADDR->NPA5BPTR;/* FIND FIRST ENTRY ON QUEUE*/
         L     @08,SLOTADDR                                        0953
         L     LPDEPTR,NPA5BPTR(,@08)                              0953
*             IF LPDEPTR=INFOPTR THEN/* IS THIS ALIAS ENTRY          */
         C     LPDEPTR,INFOPTR                                     0954
         BNE   @RF00954                                            0954
*               SLOTADDR->NPA5BPTR=INFSYNM;/* YES, REMOVE ALIAS ENTRY
*                                      FROM Q                        */
         L     @07,INFOPTR                                         0955
         SLR   @00,@00                                             0955
         ICM   @00,7,INFSYNM(@07)                                  0955
         ST    @00,NPA5BPTR(,@08)                                  0955
*             ELSE                  /* NO, NOT START OF Q            */
*               DO;                 /* FIND ALIAS ENTRY              */
         B     @RC00954                                            0956
@RF00954 DS    0H                                                  0957
*                 DO WHILE(LPDEPTR->INFSYNM^=INFOPTR);/* SCAN FOR  0957
*                                      ALIAS ENTRY                   */
         B     @DE00957                                            0957
@DL00957 DS    0H                                                  0958
*                   LPDEPTR=LPDEPTR->INFSYNM;/* PICK UP NEXT ENTRY 0958
*                                      ADDRESS                       */
         SLR   @08,@08                                             0958
         ICM   @08,7,INFSYNM(LPDEPTR)                              0958
         LR    LPDEPTR,@08                                         0958
*                 END;              /* END OF SCAN LOOP              */
@DE00957 SLR   @08,@08                                             0959
         ICM   @08,7,INFSYNM(LPDEPTR)                              0959
         C     @08,INFOPTR                                         0959
         BNE   @DL00957                                            0959
*                 LPDEPTR->INFSYNM=INFSYNM;/* REMOVE ALIAS ENTRY FROM
*                                      QUEUE                         */
         L     @08,INFOPTR                                         0960
         MVC   INFSYNM(3,LPDEPTR),INFSYNM(@08)                     0960
*               END;                                               0961
*           END;                    /* END OF ERROR PROCESSING       */
@RC00954 DS    0H                                                  0963
*       END;                        /* END OF PROCESSING FOR THIS  0963
*                                      ALIAS ENTRY                   */
@RF00941 DS    0H                                                  0964
*   END;                            /* END OF SCAN LOOP              */
@RF00933 LA    @08,40                                              0964
         AL    @08,INFOPTR                                         0964
         ST    @08,INFOPTR                                         0964
@DE00932 L     @08,INFOPTR                                         0964
         CLC   INFNAME(8,@08),@CB00041                             0964
         BNE   @DL00932                                            0964
*   RETURN;                         /* RETURN TO CALLER              */
         B     @EL00010                                            0965
*   RESPECIFY                                                      0966
*    (LPDEPTR) UNRESTRICTED;                                       0966
*   GENERATE NOREFS NOSETS(EJECT);                                 0967
*                                                                  0967
         EJECT
*/********************************************************************/
*/*                                                                  */
*/*         SUBROUTINE NPA5GLOD                                      */
*/*                                                                  */
*/*         FUNCTION - LOAD THE MODULES SPECIFIED IN A PACKED GROUP  */
*/*                                                                  */
*/*         INPUT - NA5CSGRP POINTS TO THE BLDL INFO ENTRY FOR THE   */
*/*            FIRST MODULE IN THE GROUP                             */
*/*         OUTPUT - THE SPECIFIED MODULES ARE LOADED INTO THE       */
*/*            LINK PACK AREA, THE BLDL INFORMATION TABLE ENTRIES    */
*/*            FOR THOSE MODULES ARE REPLACED BY LPDES, AND          */
*/*            CVTSHRVM IS UPDATED TO POINT TO THE BEGINNING OF      */
*/*            THE GROUP JUST LOADED                                 */
*/*                                                                  */
*/********************************************************************/
*                                                                  0968
*NPA5GLOD:                          /* NPA5GLOD ENTRY PT             */
*   ENTRY;                          /* BEGINNING OF SUBR             */
         B     @EC00968                                            0968
NPA5GLOD STM   @14,@03,@SA00010                                    0968
         STM   @05,@12,@SA00010+24                                 0968
@EC00968 DS    0H                                                  0969
*   RESPECIFY                                                      0969
*    (RLDADDR) RESTRICTED;                                         0969
*   IF NA5CSGRP=0 THEN              /* THIS CONDITION WILL RESULT    */
         L     @08,NA5CSGRP                                        0970
         LTR   @08,@08                                             0970
         BZ    @RT00970                                            0970
*     RETURN;                       /* DURING ERROR PROCESSING IF  0971
*                                      FIRST MODULE IN GROUP IS    0971
*                                      INVALID.                      */
*   RLDADDR=0;                      /* LOGICAL LOAD ADDR             */
         SLR   RLDADDR,RLDADDR                                     0972
*   INFOPTR=NA5CSGRP;               /* POINT TO FIRST ENT            */
*                                                                  0973
         ST    @08,INFOPTR                                         0973
*   /*****************************************************************/
*   /*                                                               */
*   /* CALCULATE THE TOTAL SIZE OF THE GROUP OF MODULES TO BE LOADED */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0974
*   DO WHILE(INFOPTR^=0);           /* SCAN ENTIRE GROUP             */
         B     @DE00974                                            0974
@DL00974 DS    0H                                                  0975
*     NEXTADDR=RLDADDR+(INFSTOR+7)&ROUNDDWD;/* CALCULATE LOGICAL   0975
*                                      ADDRESS OF END OF THIS MODULE */
         LA    @08,7                                               0975
         L     @05,INFOPTR                                         0975
         SLR   @00,@00                                             0975
         ICM   @00,7,INFSTOR(@05)                                  0975
         ALR   @08,@00                                             0975
         ALR   @08,RLDADDR                                         0975
         N     @08,@CF00121                                        0975
         ST    @08,NEXTADDR                                        0975
*     IF((NEXTADDR-1)&ROUNDPAG)^=(RLDADDR&ROUNDPAG) THEN/* TEST FOR
*                                      END OF MODULE IN A DIFFERENT
*                                      PAGE FROM THE START OF THE  0976
*                                      MODULE                        */
         LH    @05,@CH00123                                        0976
         LR    @00,@08                                             0976
         BCTR  @00,0                                               0976
         NR    @00,@05                                             0976
         LR    @15,RLDADDR                                         0976
         NR    @15,@05                                             0976
         CLR   @00,@15                                             0976
         BE    @RF00976                                            0976
*       DO;                         /* PAGE BOUNDARY CUTS THROUGH  0977
*                                      MODULE - AJUST LOAD ADDRESS IF
*                                      NECESSARY                     */
*         WASTE=((RLDADDR+PAGEM1)&ROUNDPAG)-RLDADDR;/* CALC SPACE TO
*                                      BE WASTED                     */
         LA    @00,4095                                            0978
         ALR   @00,RLDADDR                                         0978
         NR    @00,@05                                             0978
         SLR   @00,RLDADDR                                         0978
         ST    @00,WASTE                                           0978
*         IF WASTE^=PAGE THEN       /* DOES ROUNDING WASTE AN ENTIRE
*                                      PAGE                          */
         CH    @00,@CH00044                                        0979
         BE    @RF00979                                            0979
*           NEXTADDR=WASTE+NEXTADDR;/* NO, ROUND UP                  */
         ALR   @00,@08                                             0980
         ST    @00,NEXTADDR                                        0980
*       END;                                                       0981
@RF00979 DS    0H                                                  0982
*     RLDADDR=NEXTADDR;             /* STEP TO NEXT ADDR             */
@RF00976 L     RLDADDR,NEXTADDR                                    0982
*     INFOPTR=INFCHAIN;             /* FIND NEXT ENTRY               */
         L     @08,INFOPTR                                         0983
         SLR   @05,@05                                             0983
         ICM   @05,7,INFCHAIN(@08)                                 0983
         ST    @05,INFOPTR                                         0983
*   END;                            /* END OF SCAN LOOP              */
@DE00974 ICM   @08,15,INFOPTR                                      0984
         BNZ   @DL00974                                            0984
*   RLDADDR=(NVTLVIRT-RLDADDR)&ROUNDPAG;/* CALCULATE LOW ADDRESS OF
*                                      GROUP                         */
         L     @08,NVTLVIRT(,RNVT)                                 0985
         SLR   @08,RLDADDR                                         0985
         N     @08,@CF00123                                        0985
         LR    RLDADDR,@08                                         0985
*   IF RLDADDR<NVTVVPG1 THEN        /* IS ADDRESS VALID              */
         C     RLDADDR,NVTVVPG1(,RNVT)                             0986
         BNL   @RF00986                                            0986
*     CALL NPA5CERR(STUNAV);        /* NO, PROCESS ERROR             */
         LA    @01,@AL00987                                        0987
         BAL   @14,NPA5CERR                                        0987
*   ELSE                                                           0988
*     ;                                                            0988
@RF00986 DS    0H                                                  0989
*   NVTLVIRT=RLDADDR;               /* UPDATE LOW V=V POINTER        */
         ST    RLDADDR,NVTLVIRT(,RNVT)                             0989
*   INFOPTR=NA5CSGRP;               /* POINT TO FIRST MOD            */
*                                                                  0990
         MVC   INFOPTR(4),NA5CSGRP                                 0990
*   /*****************************************************************/
*   /*                                                               */
*   /* LOAD THE MODULES INTO THE SPACE JUST CALCULATED               */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0991
*   DO WHILE(INFOPTR^=0);           /* PROCESS LIST                  */
         B     @DE00991                                            0991
@DL00991 DS    0H                                                  0992
*     NEXTINFO=INFCHAIN;            /* SAVE CHAIN POINTER            */
         L     @08,INFOPTR                                         0992
         SLR   @05,@05                                             0992
         ICM   @05,7,INFCHAIN(@08)                                 0992
         ST    @05,NEXTINFO                                        0992
*     INFPAGA='0'B;                 /* FORCE NO PAGE ALIGN           */
*                                                                  0993
         L     @08,INFOPTR                                         0993
         NI    INFPAGA(@08),B'10111111'                            0993
*     /***************************************************************/
*     /*                                                             */
*     /* LOAD REQUESTED MODULE AND BUILD LPDE IN PLACE OF THE BLDL   */
*     /* INFORMATION TABLE ENTRY                                     */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0994
*     CALL NPA5MLOD;                /* LOAD MODULE                   */
         BAL   @14,NPA5MLOD                                        0994
*     INFOPTR=NEXTINFO;             /* POINT TO NEXT MOD             */
         MVC   INFOPTR(4),NEXTINFO                                 0995
*   END;                            /* END OF LOAD LOOP              */
@DE00991 SLR   @08,@08                                             0996
         C     @08,INFOPTR                                         0996
         BNE   @DL00991                                            0996
*   WASTE=PAGE-RLDADDR&OVERPAG;     /* CALCULATE SPACE LOST ON LAST
*                                      PAGE OF GROUP                 */
         LH    @05,@CH00044                                        0997
         SLR   @05,RLDADDR                                         0997
         N     @05,@CF00127                                        0997
         ST    @05,WASTE                                           0997
*   IF WASTE>0 THEN                 /* IS SPACE WASTED               */
         CR    @05,@08                                             0998
         BNH   @RF00998                                            0998
*     DO;                           /* YES, PROCESS WASTE            */
*       WASTEADR=RLDADDR;           /* START ADDR OF WASTE           */
*                                                                  1000
         ST    RLDADDR,WASTEADR                                    1000
*       /*************************************************************/
*       /*                                                           */
*       /* ADD WASTED SPACE TO QUEUE TO BE RECLAIMED LATER           */
*       /*                                                           */
*       /*************************************************************/
*                                                                  1001
*       CALL NPA5WAST;              /* ADD SPACE TO QUEUE            */
         BAL   @14,NPA5WAST                                        1001
*     END;                                                         1002
*   RETURN;                         /* RETURN TO CALLER              */
         B     @EL00010                                            1003
*   RESPECIFY                                                      1004
*    (RLDADDR) UNRESTRICTED;                                       1004
*   GENERATE NOREFS NOSETS(EJECT);                                 1005
*                                                                  1005
         EJECT
*/********************************************************************/
*/*                                                                  */
*/*         SUBROUTINE NPA5ILOD                                      */
*/*                                                                  */
*/*         FUNCTION - LOAD AN INDEPENDENT MODULE INTO THE LPA       */
*/*                                                                  */
*/*         INPUT - THE BLDL INFORMATION TABLE ENTRY FOR THE MODULE  */
*/*            (POINTED TO BY INFOPTR)                               */
*/*                                                                  */
*/*         OUTPUT - THE MODULES IS LOADED IN THE LPA AND THE        */
*/*            BLDL INFO TABLE ENTRY IS REPLACED BY THE LPDE         */
*/*                                                                  */
*/********************************************************************/
*                                                                  1006
*NPA5ILOD:                          /* NPA5ILOD ENTRY PT             */
*   ENTRY;                          /* BEGINNING OF SUBR             */
         B     @EC01006                                            1006
NPA5ILOD STM   @14,@03,@SA00010                                    1006
         STM   @05,@12,@SA00010+24                                 1006
@EC01006 DS    0H                                                  1007
*   RESPECIFY                                                      1007
*    (RLDADDR) RESTRICTED;                                         1007
*   FMSIZE=(INFSTOR+7)&ROUNDDWD;    /* ROUND MODULE SIZE UP TO     1008
*                                      DOUBLEWORD MULTIPLE           */
         LA    @08,7                                               1008
         L     @05,INFOPTR                                         1008
         SLR   @00,@00                                             1008
         ICM   @00,7,INFSTOR(@05)                                  1008
         ALR   @08,@00                                             1008
         N     @08,@CF00121                                        1008
         ST    @08,FMSIZE                                          1008
*   RLDADDR=0;                      /* ZERO LOAD ADDRESS             */
         SLR   RLDADDR,RLDADDR                                     1009
*   WASTE=0;                        /* CLEAR WASTE COUNT             */
         SLR   @05,@05                                             1010
         ST    @05,WASTE                                           1010
*   IF FMSIZE<PAGE&INFPAGA='0'B THEN/* TEST FOR A MODULE WHICH CAN 1011
*                                      FIT ON AN EXISTING PAGE - NOTE
*                                      - NO SPACE WILL BE RECLAIMED
*                                      IF A MODULE PACKING LIST WAS
*                                      NOT FOUND OR WAS NULL         */
*                                                                  1011
         CH    @08,@CH00044                                        1011
         BNL   @RF01011                                            1011
         L     @08,INFOPTR                                         1011
         TM    INFPAGA(@08),B'01000000'                            1011
         BNZ   @RF01011                                            1011
*     /***************************************************************/
*     /*                                                             */
*     /* TRY TO FIND SOME WASTED SPACE WHICH CAN CONTAIN IT          */
*     /*                                                             */
*     /***************************************************************/
*                                                                  1012
*     CALL NPA5RCLM;                /* RECLAIM SPACE                 */
         BAL   @14,NPA5RCLM                                        1012
*   IF RLDADDR=0 THEN               /* TEST FOR SPACE NOT FOUND FOR
*                                      MODULE                        */
@RF01011 SLR   @08,@08                                             1013
         CR    RLDADDR,@08                                         1013
         BNE   @RF01013                                            1013
*     DO;                           /* MODULE MUST BE LOADED BELOW 1014
*                                      CVTSHRVM                      */
*       AVAIL=NVTLVIRT&OVERPAG;     /* CALC SPACE WHICH IS AVAILABLE
*                                      IN LAST PAGE USED             */
         L     @05,NVTLVIRT(,RNVT)                                 1015
         LR    @00,@05                                             1015
         N     @00,@CF00127                                        1015
         ST    @00,AVAIL                                           1015
*       IF FMSIZE>AVAIL THEN        /* DOES MODULE FIT               */
         C     @00,FMSIZE                                          1016
         BNL   @RF01016                                            1016
*         DO;                       /* NO, MUST DROP DOWN TO NEXT  1017
*                                      PAGE BOUNDARY                 */
*           NVTLVIRT=NVTLVIRT&ROUNDPAG;/* ROUND DOWN TO PAGE BOUNDARY*/
         N     @05,@CF00123                                        1018
         ST    @05,NVTLVIRT(,RNVT)                                 1018
*           WASTEADR=NVTLVIRT;      /* SET UP ADDRESS OF WASTED SPACE*/
         ST    @05,WASTEADR                                        1019
*           WASTE=AVAIL;            /* SET UP LENGTH OF WASTED SPACE */
         ST    @00,WASTE                                           1020
*           IF WASTE^=0 THEN        /* TEST FOR NO WASTE             */
*                                                                  1021
         CR    @00,@08                                             1021
         BE    @RF01021                                            1021
*             /*******************************************************/
*             /*                                                     */
*             /* ADD WASTED SPACE TO QUEUE                           */
*             /*                                                     */
*             /*******************************************************/
*                                                                  1022
*             CALL NPA5WAST;        /* QUEUE SPACE                   */
         BAL   @14,NPA5WAST                                        1022
*         END;                                                     1023
@RF01021 DS    0H                                                  1024
*       IF FMSIZE>=PAGEINFPAGA='1'B THEN/* TEST FOR MODULE TO BE  1024
*                                      FORCED TO A PAGE BOUNDARY     */
@RF01016 L     @08,FMSIZE                                          1024
         CH    @08,@CH00044                                        1024
         BNL   @RT01024                                            1024
         L     @08,INFOPTR                                         1024
         TM    INFPAGA(@08),B'01000000'                            1024
         BNO   @RF01024                                            1024
@RT01024 DS    0H                                                  1025
*         DO;                       /* MODULE MUST BE LOADED ON A  1025
*                                      PAGE BOUNDARY                 */
*           RLDADDR=(NVTLVIRT-FMSIZE)&ROUNDPAG;/* CALCULATE LOAD   1026
*                                      ADDRESS                       */
         L     @08,FMSIZE                                          1026
         L     @15,NVTLVIRT(,RNVT)                                 1026
         LR    RLDADDR,@15                                         1026
         SLR   RLDADDR,@08                                         1026
         N     RLDADDR,@CF00123                                    1026
*           WASTEADR=RLDADDR+FMSIZE;/* CALCULATE MODULE END ADDRESS -
*                                      IF THERE IS ANY WASTED SPACE,
*                                      IT WILL START HERE            */
         LR    @05,RLDADDR                                         1027
         ALR   @05,@08                                             1027
         ST    @05,WASTEADR                                        1027
*           WASTE=NVTLVIRT-WASTEADR;/* CALCULATE SIZE OF WASTED SPACE*/
         SLR   @15,@05                                             1028
         ST    @15,WASTE                                           1028
*           IF FMSIZE<PAGE THEN     /* TEST FOR PAGE ALIGNED MODULE
*                                      SMALLER THAN A PAGE           */
         CH    @08,@CH00044                                        1029
         BNL   @RF01029                                            1029
*             INFPAGA='0'B;         /* YES, RESET PAGE ALIGN BIT TO
*                                      AVOID GETMAIN PROBLEM         */
         L     @08,INFOPTR                                         1030
         NI    INFPAGA(@08),B'10111111'                            1030
*           ELSE                    /* NO, NOT SMALL MOD             */
*             INFPAGA='1'B;         /* FORCE PAGE ALIGN              */
         B     @RC01029                                            1031
@RF01029 L     @08,INFOPTR                                         1031
         OI    INFPAGA(@08),B'01000000'                            1031
*         END;                                                     1032
*       ELSE                        /* MODULE CAN BE LOADED ON A   1033
*                                      DOUBLEWORD BOUNDARY           */
*         RLDADDR=NVTLVIRT-FMSIZE;  /* CALC LOAD ADDR                */
         B     @RC01024                                            1033
@RF01024 L     RLDADDR,NVTLVIRT(,RNVT)                             1033
         SL    RLDADDR,FMSIZE                                      1033
*     END;                                                         1034
@RC01024 DS    0H                                                  1035
*   IF RLDADDR<NVTVVPG1 THEN        /* TEST FOR INV ADDR             */
@RF01013 C     RLDADDR,NVTVVPG1(,RNVT)                             1035
         BNL   @RF01035                                            1035
*     CALL NPA5CERR(STUNAV);        /* PROCESS ERROR                 */
         LA    @01,@AL01036                                        1036
         BAL   @14,NPA5CERR                                        1036
*   IF RLDADDR<NVTLVIRT THEN        /* TEST FOR MODULE LOADED BELOW
*                                      VALIDATED ADDRESS SPACE       */
@RF01035 C     RLDADDR,NVTLVIRT(,RNVT)                             1037
         BNL   @RF01037                                            1037
*     NVTLVIRT=RLDADDR;             /* UPDATE LOW V=V POINTER        */
         ST    RLDADDR,NVTLVIRT(,RNVT)                             1038
*   SAVEWAST=WASTE;                 /* SAVE WASTE COUNT OVER CALL TO
*                                      NPA5MLOD                      */
@RF01037 MVC   SAVEWAST(4),WASTE                                   1039
*   SAVEADR=WASTEADR;               /* SAVE WASTE ADDRESS OVER CALL
*                                      TO NPA5MLOD                   */
*                                                                  1040
         MVC   SAVEADR(4),WASTEADR                                 1040
*   /*****************************************************************/
*   /*                                                               */
*   /* LOAD THE MODULE INTO THE LPA AND CONVERT THE BLDL INFO TABLE  */
*   /* ENTRY INTO AN LPDE                                            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  1041
*   CALL NPA5MLOD;                  /* LOAD THE MODULE               */
         BAL   @14,NPA5MLOD                                        1041
*   WASTE=SAVEWAST;                 /* RESTORE WASTE COUNT           */
         L     @08,SAVEWAST                                        1042
         ST    @08,WASTE                                           1042
*   WASTEADR=SAVEADR;               /* RESTORE WASTE ADDR            */
         MVC   WASTEADR(4),SAVEADR                                 1043
*   IF WASTE>0 THEN                 /* IS THERE ANY WASTE            */
*                                                                  1044
         LTR   @08,@08                                             1044
         BNP   @RF01044                                            1044
*     /***************************************************************/
*     /*                                                             */
*     /* ADD WASTED SPACE TO QUEUE                                   */
*     /*                                                             */
*     /***************************************************************/
*                                                                  1045
*     CALL NPA5WAST;                /* QUEUE SPACE                   */
         BAL   @14,NPA5WAST                                        1045
*   RETURN;                         /* RETURN TO CALLER              */
         B     @EL00010                                            1046
*   RESPECIFY                                                      1047
*    (RLDADDR) UNRESTRICTED;                                       1047
*   GENERATE NOREFS NOSETS(EJECT);                                 1048
*                                                                  1048
         EJECT
*/********************************************************************/
*/*                                                                  */
*/*         SUBROUTINE NPA5ALPD                                      */
*/*                                                                  */
*/*         FUNCTION - BUILD AN LPDE FOR AN ALIAS NAME               */
*/*                                                                  */
*/*         INPUT - THE INFO TABLE ENTRY FOR THE ALIAS NAME          */
*/*            THE BLDL INFO TABLE WITH LPDES FOR ALL MAJOR NAMES    */
*/*                                                                  */
*/*         OUTPUT - THE LPDE FOR THE ALIAS NAME IN PLACE OF THE     */
*/*            BLDL INFO TABLE ENTRY                                 */
*/*                                                                  */
*/********************************************************************/
*                                                                  1049
*NPA5ALPD:                          /* NPA5ALPD ENTRY PT             */
*   ENTRY;                          /* BEGINNING OF SUBR             */
         B     @EC01049                                            1049
NPA5ALPD STM   @14,@03,@SA00010                                    1049
         STM   @05,@12,@SA00010+24                                 1049
@EC01049 DS    0H                                                  1050
*   RESPECIFY                                                      1050
*    (LPDEPTR) RESTRICTED;                                         1050
*   MODULE=INFMNM;                  /* PICK UP MAJOR NAME            */
*                                                                  1051
         L     @08,INFOPTR                                         1051
         MVC   MODULE(8),INFMNM(@08)                               1051
*   /*****************************************************************/
*   /*                                                               */
*   /* LOCATE LPDE FOR THE MAJOR NAME                                */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  1052
*   CALL NPA5FSLT;                  /* FIND INDEX ENTRY              */
         BAL   @14,NPA5FSLT                                        1052
*   LPDEPTR=SLOTADDR->NPA5BPTR;     /* PICK UP FIRST LPDE ADDRESS    */
         L     @08,SLOTADDR                                        1053
         L     LPDEPTR,NPA5BPTR(,@08)                              1053
*   DO WHILE(LPDENIP='0'BLPDENAME^=MODULE);/* SEARCH SYNONYM QUEUE
*                                      FOR THE LPDE FOR THE MAJOR  1054
*                                      NAME                          */
         B     @DE01054                                            1054
@DL01054 DS    0H                                                  1055
*     LPDEPTR=LPDECHN;              /* STEP TO NEXT LPDE             */
         L     LPDEPTR,LPDECHN(,LPDEPTR)                           1055
*   END;                            /* END OF SCAN LOOP              */
*                                                                  1056
@DE01054 TM    LPDENIP(LPDEPTR),B'10000000'                        1056
         BZ    @DL01054                                            1056
         CLC   LPDENAME(8,LPDEPTR),MODULE                          1056
         BNE   @DL01054                                            1056
*   /*****************************************************************/
*   /*                                                               */
*   /* BUILD THE LPDE FOR THE ALIAS NAME                             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  1057
*   NPA5LPDE=LPDE;                  /* COPY MAJOR LPDE               */
         MVC   NPA5LPDE(40),LPDE(LPDEPTR)                          1057
*   LPDEPTR=ADDR(NPA5LPDE);         /* SET UP DSECT BASE             */
         LA    LPDEPTR,NPA5LPDE                                    1058
*   LPDENTP=LPDEXTAD+INFEPA;        /* RELOCATE EPA                  */
         L     @08,INFOPTR                                         1059
         SLR   @07,@07                                             1059
         ICM   @07,7,INFEPA(@08)                                   1059
         AL    @07,LPDEXTAD(,LPDEPTR)                              1059
         ST    @07,LPDENTP(,LPDEPTR)                               1059
*   LPDENAME=INFNAME;               /* COPY ALIAS NAME               */
         L     @08,INFOPTR                                         1060
         MVC   LPDENAME(8,LPDEPTR),INFNAME(@08)                    1060
*   LPDEMJNM=INFMNM;                /* COPY MAJOR NAME               */
         L     @08,INFOPTR                                         1061
         MVC   LPDEMJNM(8,LPDEPTR),INFMNM(@08)                     1061
*   LPDEMIN='1'B;                   /* SET MINOR LPDE FLAG           */
         OI    LPDEMIN(LPDEPTR),B'00000100'                        1062
*   LPDERLC='1'B;                   /* SET ALIAS LPDE FLAG           */
*   LPDEXLE='0'B;                   /* RESET XLST FLAG               */
         OI    LPDERLC(LPDEPTR),B'00010000'                        1064
         NI    LPDEXLE(LPDEPTR),B'11011111'                        1064
*   LPDECHN=INFSYNM;                /* COPY SYNONYM CHAIN            */
         L     @08,INFOPTR                                         1065
         SLR   @07,@07                                             1065
         ICM   @07,7,INFSYNM(@08)                                  1065
         ST    @07,LPDECHN(,LPDEPTR)                               1065
*   INFODATA=LPDE;                  /* REPLACE INFODATA WITH ALIAS 1066
*                                      LPDE                          */
         L     @08,INFOPTR                                         1066
         MVC   INFODATA(40,@08),LPDE(LPDEPTR)                      1066
*   RETURN;                         /* RETURN TO CALLER              */
         B     @EL00010                                            1067
*   RESPECIFY                                                      1068
*    (LPDEPTR) UNRESTRICTED;                                       1068
*   GENERATE NOREFS NOSETS(EJECT);                                 1069
*                                                                  1069
         EJECT
*/********************************************************************/
*/*                                                                  */
*/*         SUBROUTINE NPA5RHSH                                      */
*/*                                                                  */
*/*         FUNCTION - REHASH LPDES TO THEIR PROPER PLACE IN THE     */
*/*            LPA DIRECTORY                                         */
*/*                                                                  */
*/*         INPUT - SLOTADDR - POINTS TO INDEX TABLE ENTRY FOR       */
*/*               THE SYNONYM QUEUE TO BE PROCESSED                  */
*/*            BLDL INFORMATION TABLE CONTAINING LPDES TO BE RESASHED*/
*/*                                                                  */
*/*         OUTPUT - THE LPDES ON THE SYNONYM QUEUE ARE IN THEIR     */
*/*            PROPER PLACES IN THE LPA DIRECTORY AND THE SYNONYM    */
*/*            QUEUE IS MAINTAINED IN THE DIRECTORY                  */
*/*            NOTE - THE BLDL INDEX TABLE ENTRY CORRESPONDING TO    */
*/*            ALL LPDES IN USE NOW CONTAIN A NON-ZERO VALUE         */
*/*                                                                  */
*/********************************************************************/
*                                                                  1070
*NPA5RHSH:                          /* NPA5RHSH ENTRY PT             */
*   ENTRY;                          /* BEGINNING OF SUBR             */
         B     @EC01070                                            1070
NPA5RHSH STM   @14,@03,@SA00010                                    1070
         STM   @05,@12,@SA00010+24                                 1070
@EC01070 DS    0H                                                  1071
*   RESPECIFY                                                      1071
*    (LPDEPTR) RESTRICTED;                                         1071
*   INDEX=SLOTADDR-NA5BLDIR;        /* CALCULATE OFFSET INTO INDEX 1072
*                                      TABLE                         */
         L     @08,SLOTADDR                                        1072
         LR    @07,@08                                             1072
         SL    @07,NA5BLDIR                                        1072
         ST    @07,INDEX                                           1072
*   INFOPTR=SLOTADDR->NPA5BPTR;     /* PICK UP ADDRESS OF FIRST LPDE
*                                      ON QUEUE                      */
*                                                                  1073
         MVC   INFOPTR(4),NPA5BPTR(@08)                            1073
*   /*****************************************************************/
*   /*                                                               */
*   /* PUT FIRST LPDE INTO DIRECTORY IN ITS PRIMARY SLOT             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  1074
*   LPDEPTR=NVTLVIRT+INDEX*10;      /* CALCULATE ADDRESS OF PRIMARY
*                                      SLOT                          */
         L     LPDEPTR,INDEX                                       1074
         MH    LPDEPTR,@CH03109                                    1074
         AL    LPDEPTR,NVTLVIRT(,RNVT)                             1074
*   LPDE=INFOPTR->LPDE;             /* COPY LPDE TO SLOT             */
*                                                                  1075
         L     @08,INFOPTR                                         1075
         MVC   LPDE(40,LPDEPTR),LPDE(@08)                          1075
*   /*****************************************************************/
*   /*                                                               */
*   /* LOOP THROUGH THE SYNONYM CHAIN AND COPY ALL OTHER LPDES IN THE*/
*   /* CHAIN INTO THE DIRECTORY                                      */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  1076
*   DO WHILE((LPDECHN&ZEROHIGH)^=0);/* LOOP TILL END SYN CHAIN       */
         B     @DE01076                                            1076
@DL01076 DS    0H                                                  1077
*     INFOPTR=LPDECHN;              /* POINT TO NEXT LPDE            */
         MVC   INFOPTR(4),LPDECHN(LPDEPTR)                         1077
*     WORKLPDE=LPDEPTR;             /* MOVE LPDEPTR TO WORKAREA      */
         ST    LPDEPTR,WORKLPDE                                    1078
*     LPDEPAGE=WORKLPDE&ROUNDPAG;   /* ROUND LPDE TO PAGE            */
         L     @08,WORKLPDE                                        1079
         LR    @07,@08                                             1079
         N     @07,@CF00123                                        1079
         ST    @07,LPDEPAGE                                        1079
*     LPDERMDR=(WORKLPDE-LPDEPAGE)//40;/* COMPUTE AMOUNT OVER PAGE 1080
*                                      BOUNDARY THAT A PAGE        1080
*                                      OVERLAPPED LPDE OCCUPIES ON 1080
*                                      CURRENT PAGE                  */
         LA    @15,40                                              1080
         LR    @04,@08                                             1080
         SLR   @04,@07                                             1080
         SRDA  @04,32                                              1080
         DR    @04,@15                                             1080
         ST    @04,LPDERMDR                                        1080
*     LPDEFRST=LPDEPAGE+LPDERMDR;   /* POINT TO FIRST LPDE ON THIS 1081
*                                      PAGE                          */
         ALR   @07,@04                                             1081
         ST    @07,LPDEFRST                                        1081
*     INDEX=((LPDEFRST-NVTLVIRT)/40)*4;/* COMPUTE CORRESPONDING INDEX
*                                      TABLE ENTRY                   */
         LR    @04,@07                                             1082
         SL    @04,NVTLVIRT(,RNVT)                                 1082
         SRDA  @04,32                                              1082
         DR    @04,@15                                             1082
         SLA   @05,2                                               1082
         ST    @05,INDEX                                           1082
*     IF LPDERMDR>16 THEN           /* DETERMINE THE NUMBER OF LPDES
*                                      THAT CAN FIT ON CURRENT PAGE -
*                                      THIS NUMBER X 4 IS LAST INDEX
*                                      TABLE ENTRY THAT MAPS TO LAST
*                                      LPDE ON THIS PAGE             */
         L     @08,LPDERMDR                                        1083
         CH    @08,@CH00067                                        1083
         BNH   @RF01083                                            1083
*       IXPAG=400;                  /* 100 INDEX TABLE ENTRIES       */
         MVC   IXPAG(4),@CF03684                                   1084
*     ELSE                                                         1085
*       IXPAG=404;                  /* 101 INDEX TABLE ENTRIES       */
         B     @RC01083                                            1085
@RF01083 MVC   IXPAG(4),@CF03685                                   1085
*     IXLAST=MIN(BLDIREND,NA5BLDIR+INDEX+IXPAG);/* CALC LAST INDEX 1086
*                                      FOR THAT PAGE OF DIRECTORY    */
@RC01083 L     @08,NA5BLDIR                                        1086
         AL    @08,INDEX                                           1086
         LR    @07,@08                                             1086
         AL    @07,IXPAG                                           1086
         L     @00,BLDIREND                                        1086
         CR    @00,@07                                             1086
         BNH   *+6
         LR    @00,@07                                             1086
         ST    @00,IXLAST                                          1086
*     DO IXAD=NA5BLDIR+INDEX TO IXLAST BY 4 WHILE(IXAD->NPA5BPTR>0);
         ST    @08,IXAD                                            1087
         B     @DE01087                                            1087
@DL01087 ICM   @07,15,NPA5BPTR(@08)                                1087
         BNP   @DC01087                                            1087
*                                   /* SCAN THIS PAGE FOR AN UNUSED
*                                      ENTRY                         */
*     END;                          /* END OF SCAN LOOP              */
         AH    @08,@CH00058                                        1088
         ST    @08,IXAD                                            1088
@DE01087 C     @08,IXLAST                                          1088
         BNH   @DL01087                                            1088
@DC01087 DS    0H                                                  1089
*     IF IXAD>IXLAST THEN           /* WAS ENTRY FOUND               */
         CLC   IXAD(4),IXLAST                                      1089
         BNH   @RF01089                                            1089
*       DO IXAD=NA5BLDIR TO BLDIREND BY 4 WHILE(IXAD->NPA5BPTR>0);/*
*                                      NO, SCAN ENTIRE DIRECTORY FOR
*                                      AN UNUSED ENTRY               */
         L     @08,NA5BLDIR                                        1090
         ST    @08,IXAD                                            1090
         B     @DE01090                                            1090
@DL01090 ICM   @07,15,NPA5BPTR(@08)                                1090
         BNP   @DC01090                                            1090
*       END;                        /* END OF FULL SCAN              */
         AH    @08,@CH00058                                        1091
         ST    @08,IXAD                                            1091
@DE01090 C     @08,BLDIREND                                        1091
         BNH   @DL01090                                            1091
@DC01090 DS    0H                                                  1092
*     INDEX=IXAD-NA5BLDIR;          /* CALCULATE OFFSET INTO INDEX 1092
*                                      TABLE                         */
@RF01089 L     @08,IXAD                                            1092
         LR    @07,@08                                             1092
         SL    @07,NA5BLDIR                                        1092
         ST    @07,INDEX                                           1092
*     LPDECHN=NVTLVIRT+INDEX*10;    /* CALCULATE ADDRESS OF        1093
*                                      CORRESPONDING SLOT            */
         MH    @07,@CH03109                                        1093
         AL    @07,NVTLVIRT(,RNVT)                                 1093
         ST    @07,LPDECHN(,LPDEPTR)                               1093
*     LPDEPTR=LPDECHN;              /* SET UP DSECT BASE             */
         LR    LPDEPTR,@07                                         1094
*     LPDE=INFOPTR->LPDE;           /* COPY LPDE TO SLOT             */
         L     @07,INFOPTR                                         1095
         MVC   LPDE(40,LPDEPTR),LPDE(@07)                          1095
*     IXAD->NPA5BPTR=1;             /* SET INDEX VALUE TO NON-ZERO 1096
*                                      VALUE                         */
         MVC   NPA5BPTR(4,@08),@CF00264                            1096
*   END;                            /* END OF PROCESSING FOR THIS  1097
*                                      LPDE - LOOP TILL END OF     1097
*                                      SYNONYM QUEUE                 */
@DE01076 L     @08,LPDECHN(,LPDEPTR)                               1097
         N     @08,@CF00134                                        1097
         LTR   @08,@08                                             1097
         BNZ   @DL01076                                            1097
*   RETURN;                         /* RETURN TO CALLER              */
         B     @EL00010                                            1098
*   RESPECIFY                                                      1099
*    (LPDEPTR) UNRESTRICTED;                                       1099
*   END NPA5RBLK;                                                  1100
         B     @EL00010                                            1100
@PB00010 DS    0H                                                  1101
*   GENERATE NOREFS NOSETS(EJECT);                                 1101
*                                                                  1101
         EJECT
*/********************************************************************/
*/*                                                                  */
*/*         SUBROUTINE NPA5MLOD                                      */
*/*                                                                  */
*/*         FUNCTION - SET UP TO LOAD A MODULE INTO THE LINK PACK    */
*/*            AREA BY RECONSTRUCTING THE BLDL ENTRY DESCRIBING      */
*/*            IT FROM THE BLDL INFORMATION TABLE                    */
*/*                                                                  */
*/*         INPUT - POINTER TO INFO TABLE ENTRY (INFOPTR)            */
*/*                                                                  */
*/*         OUTPUT - THE MODULE LOADED IN THE LPA, AND ITS BLDL      */
*/*            INFO TABLE ENTRY REPLACED WITH ITS LPDE               */
*/*                                                                  */
*/********************************************************************/
*                                                                  1102
*NPA5MLOD:                          /* NPA5MLOD ENTRY PT             */
*   PROCEDURE OPTIONS(NOSAVE(RLDADDR));                            1102
         B     @PB00011                                            1102
NPA5MLOD STM   @14,@03,@SA00011                                    1102
         STM   @05,@12,@SA00011+24                                 1102
*   RESPECIFY                                                      1103
*    (PDS2PTR,                                                     1103
*     PDSPTRV,                                                     1103
*     RLDADDR) RESTRICTED;                                         1103
*   PDS2PTR=ADDR(BLDLPDS);          /* POINT TO BLDL AREA            */
         L     @06,BLDLSAV                                         1104
         LA    PDS2PTR,BLDLPDS(,@06)                               1104
*   NVTCSLNM=PDS2PTR;               /* POINT NVT TO BLDL ENTRY SO  1105
*                                      NAME OF MODULE BEING LOADED 1105
*                                      CAN BE FOUND                  */
         ST    PDS2PTR,NVTCSLNM(,RNVT)                             1105
*   BLDLPDS=BLDLPDS&&BLDLPDS;       /* CLEAR BLDL AREA               */
*                                                                  1106
         XC    BLDLPDS(60,@06),BLDLPDS(@06)                        1106
*   /*****************************************************************/
*   /*                                                               */
*   /* RECONSTRUCT THE BLDL ENTRY                                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  1107
*   PDS2NAME=INFNAME;               /* MODULE NAME                   */
         L     @06,INFOPTR                                         1107
         MVC   PDS2NAME(8,PDS2PTR),INFNAME(@06)                    1107
*   PDS2TTRP=INFTTRP;               /* TTR OF FIRST BLOCK            */
         L     @06,INFOPTR                                         1108
         MVC   PDS2TTRP(3,PDS2PTR),INFTTRP(@06)                    1108
*   PDS2INDC='2C'X;                 /* LENGTH OF USER DATA AND FLAGS */
         MVI   PDS2INDC(PDS2PTR),X'2C'                             1109
*   PDS2RENT='1'B;                  /* REENTRANT                     */
*   PDS2REUS='1'B;                  /* REUSABLE                      */
         OI    PDS2RENT(PDS2PTR),B'11000000'                       1111
*   PDS2TTRT=INFTTRT;               /* TTR OF FIRST TXT              */
         L     @06,INFOPTR                                         1112
         MVC   PDS2TTRT(3,PDS2PTR),INFTTRT(@06)                    1112
*   IF INFLOAD='1'B THEN            /* TEST LOAD FLAG                */
         L     @06,INFOPTR                                         1113
         TM    INFLOAD(@06),B'00100000'                            1113
         BNO   @RF01113                                            1113
*     PDS2LOAD='1'B;                /* ONLY LOADABLE                 */
         OI    PDS2LOAD(PDS2PTR),B'00001000'                       1114
*   IF INFEXEC='1'B THEN            /* TEST EXEC FLAG                */
@RF01113 L     @06,INFOPTR                                         1115
         TM    INFEXEC(@06),B'00010000'                            1115
         BNO   @RF01115                                            1115
*     PDS2EXEC='1'B;                /* EXECUTABLE MODULE             */
         OI    PDS2EXEC(PDS2PTR),B'00000010'                       1116
*   IF INF1BLK='1'B THEN            /* TEST 1BLK FLAG                */
@RF01115 L     @06,INFOPTR                                         1117
         TM    INF1BLK(@06),B'00001000'                            1117
         BNO   @RF01117                                            1117
*     PDS21BLK='1'B;                /* ONLY ONE TXT BLOCK            */
         OI    PDS21BLK(PDS2PTR),B'00000001'                       1118
*   IF INFORG0='1'B THEN            /* TEST ORG0 FLAG                */
@RF01117 L     @06,INFOPTR                                         1119
         TM    INFORG0(@06),B'00010000'                            1119
         BNO   @RF01119                                            1119
*     PDS2ORG0='1'B;                /* 1ST TXT ORIGIN = 0            */
         OI    PDS2ORG0(PDS2PTR),B'01000000'                       1120
*   IF INFEPA=0 THEN                /* TEST FOR 0 ORIGIN             */
@RF01119 L     @06,INFOPTR                                         1121
         ICM   @05,7,INFEPA(@06)                                   1121
         BNZ   @RF01121                                            1121
*     PDS2EP0='1'B;                 /* MOD ORIGIN = 0                */
         OI    PDS2EP0(PDS2PTR),B'00100000'                        1122
*   IF INFNRLD='1'B THEN            /* TEST NRLD FLAG                */
@RF01121 L     @06,INFOPTR                                         1123
         TM    INFNRLD(@06),B'00000100'                            1123
         BNO   @RF01123                                            1123
*     PDS2NRLD='1'B;                /* NO RLD ITEMS                  */
         OI    PDS2NRLD(PDS2PTR),B'00010000'                       1124
*   IF INFREFR='1'B THEN            /* TEST REFR FLAG                */
@RF01123 L     @06,INFOPTR                                         1125
         TM    INFREFR(@06),B'00000010'                            1125
         BNO   @RF01125                                            1125
*     PDS2REFR='1'B;                /* REFRESHABLE MODULE            */
         OI    PDS2REFR(PDS2PTR),B'00000001'                       1126
*   PDS2STOR=INFSTOR;               /* MODULE SIZE                   */
@RF01125 L     @06,INFOPTR                                         1127
         MVC   PDS2STOR(3,PDS2PTR),INFSTOR(@06)                    1127
*   PDS2FTBL=INFFTBL;               /* 1ST BLOCK LENGTH              */
         L     @06,INFOPTR                                         1128
         MVC   PDS2FTBL(2,PDS2PTR),INFFTBL(@06)                    1128
*   PDS2EPA=INFEPA;                 /* ENTRY POINT ADDRESS           */
         L     @06,INFOPTR                                         1129
         MVC   PDS2EPA(3,PDS2PTR),INFEPA(@06)                      1129
*   IF INFAOSLE='1'B THEN           /* TEST AOSLE FLAG               */
         L     @06,INFOPTR                                         1130
         TM    INFAOSLE(@06),B'00000001'                           1130
         BNO   @RF01130                                            1130
*     DO;                           /* AOS LE - SPECIAL              */
*       PDSAOSLE='1'B;              /* AOS LINKAGE EDITOR            */
         OI    PDSAOSLE(PDS2PTR),B'10000000'                       1132
*       IF INFAPFLG='1'B THEN       /* TEST FOR APF INFO             */
         L     @06,INFOPTR                                         1133
         TM    INFAPFLG(@06),B'00100000'                           1133
         BNO   @RF01133                                            1133
*         DO;                       /* PUT IN APF DATA               */
*           PDSAPFLG='1'B;          /* SET APF FLAG                  */
         OI    PDSAPFLG(PDS2PTR),B'00001000'                       1135
*           PDSPTRV=ADDR(PDSBCEND); /* POINT PAST BASIC SECTION      */
         LA    PDSPTRV,PDSBCEND(,PDS2PTR)                          1136
*           PDSAPF=INFAPF;          /* PUT IN APF DATA               */
         L     @06,INFOPTR                                         1137
         MVC   PDSAPF(2,PDSPTRV),INFAPF(@06)                       1137
*         END;                                                     1138
*       IF INFPAGA='1'B THEN        /* TEST PAGA FLAG                */
@RF01133 L     @06,INFOPTR                                         1139
         TM    INFPAGA(@06),B'01000000'                            1139
         BNO   @RF01139                                            1139
*         PDS2PAGA='1'B;            /* PAGE ALIGNED MODULE           */
         OI    PDS2PAGA(PDS2PTR),B'00100000'                       1140
*     END;                                                         1141
*                                                                  1141
@RF01139 DS    0H                                                  1142
*   /*****************************************************************/
*   /*                                                               */
*   /* CALCULATE SIZE TO BE FREEMAINED (AND RECLAIMED, IF NECESSARY) */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  1142
*   FMSIZE=(PDS2STOR+7)&ROUNDDWD;   /* ROUND SIZE UP TO DOUBLEWORD 1142
*                                      MULT                          */
@RF01130 LA    @06,7                                               1142
         SLR   @05,@05                                             1142
         ICM   @05,7,PDS2STOR(PDS2PTR)                             1142
         ALR   @06,@05                                             1142
         N     @06,@CF00121                                        1142
         ST    @06,FMSIZE                                          1142
*   SIZESAVE=0;                     /* NOTHING TO RECLAIM            */
         SLR   @05,@05                                             1143
         ST    @05,SIZESAVE                                        1143
*   NEXTADDR=RLDADDR+FMSIZE;        /* ADDRESS PAST MOD              */
         ALR   @06,RLDADDR                                         1144
         ST    @06,NEXTADDR                                        1144
*   IF((NEXTADDR-1)&ROUNDPAG)^=(RLDADDR&ROUNDPAG) THEN/* DOES MODULE
*                                      CROSS A PAGE BOUNDARY         */
         LH    @15,@CH00123                                        1145
         BCTR  @06,0                                               1145
         NR    @06,@15                                             1145
         LR    @00,RLDADDR                                         1145
         NR    @00,@15                                             1145
         CLR   @06,@00                                             1145
         BE    @RF01145                                            1145
*     DO;                           /* YES, MUST ADJUST              */
*       NEXTADDR=(RLDADDR+PAGEM1)&ROUNDPAG;/* CALCULATE ADDRESS OF 1147
*                                      NXT PAGE                      */
         LA    @06,4095                                            1147
         ALR   @06,RLDADDR                                         1147
         NR    @06,@15                                             1147
         ST    @06,NEXTADDR                                        1147
*       WASTE=NEXTADDR-RLDADDR;     /* CALCULATE WASTED SPACE IF   1148
*                                      MODULE MOVED                  */
         SLR   @06,RLDADDR                                         1148
         ST    @06,WASTE                                           1148
*       IF WASTE^=PAGE&WASTE^=0 THEN/* TEST FOR USELESS MOVE OF    1149
*                                      MODULE                        */
         CH    @06,@CH00044                                        1149
         BE    @RF01149                                            1149
         CR    @06,@05                                             1149
         BE    @RF01149                                            1149
*         DO;                       /* MODULE MUST MOVE              */
*           WASTEADR=RLDADDR;       /* ADDRESS OF WASTED SPACE       */
         ST    RLDADDR,WASTEADR                                    1151
*           CALL NPA5WAST;          /* ADD SPACE TO QUEUE            */
         BAL   @14,NPA5WAST                                        1152
*           RLDADDR=NEXTADDR;       /* CHANGE LOAD ADDRESS           */
         L     RLDADDR,NEXTADDR                                    1153
*         END;                      /*                               */
*       IF FMSIZE>=PAGE THEN        /* IS MODULE AT LEAST A PAGE IN
*                                      LENGTH                        */
@RF01149 L     @06,FMSIZE                                          1155
         LH    @05,@CH00044                                        1155
         CR    @06,@05                                             1155
         BL    @RF01155                                            1155
*         DO;                       /* YES, SPECIAL LOAD             */
*           SIZESAVE=PAGE-FMSIZE&OVERPAG;/* CALCULATE EXCESS IN LAST
*                                      PAGE                          */
         LCR   @06,@06                                             1157
         ALR   @06,@05                                             1157
         N     @06,@CF00127                                        1157
         ST    @06,SIZESAVE                                        1157
*           IF SIZESAVE=PAGE THEN   /* TEST FOR WASTE OF A FULL PAGE */
         CR    @06,@05                                             1158
         BNE   @RF01158                                            1158
*             SIZESAVE=0;           /* YES, NO WASTE                 */
         SLR   @06,@06                                             1159
         ST    @06,SIZESAVE                                        1159
*           FMSIZE=FMSIZE+SIZESAVE; /* CALCULATE TOTAL SPACE TO BE 1160
*                                      FREEMAINED - THE AMOUNT IN  1160
*                                      SIZESAVE WILL BE RECLAIMED  1160
*                                      IMMEDIATELY AFTER THE MODULE
*                                      IS LOADED                     */
@RF01158 L     @06,FMSIZE                                          1160
         AL    @06,SIZESAVE                                        1160
         ST    @06,FMSIZE                                          1160
*           PDS2PAGA='1'B;          /* SET PAGE ALIGN FLAG           */
         OI    PDS2PAGA(PDS2PTR),B'00100000'                       1161
*         END;                                                     1162
*       NEXTADDR=RLDADDR+FMSIZE-SIZESAVE;/* CALC NEXT MOD STARTING 1163
*                                      ADDRESS                       */
@RF01155 LR    @06,RLDADDR                                         1163
         AL    @06,FMSIZE                                          1163
         SL    @06,SIZESAVE                                        1163
         ST    @06,NEXTADDR                                        1163
*     END;                                                         1164
*                                                                  1164
*   /*****************************************************************/
*   /*                                                               */
*   /* LOAD SPECIFIED MODULE AND CONVERT BLDL INFORMATION TABLE ENTRY*/
*   /* TO A LINK PACK DIRECTORY ENTRY                                */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  1165
*   CALL NPA5LODM;                  /* LOAD MODULE                   */
@RF01145 BAL   @14,NPA5LODM                                        1165
*   RLDADDR=NEXTADDR;               /* UPDATE LOAD ADDRESS           */
         L     RLDADDR,NEXTADDR                                    1166
*   RETURN;                         /* RETURN TO CALLER              */
@EL00011 DS    0H                                                  1167
@EF00011 DS    0H                                                  1167
@ER00011 LM    @14,@03,@SA00011                                    1167
         LM    @05,@12,@SA00011+24                                 1167
         BR    @14                                                 1167
*   RESPECIFY                                                      1168
*    (PDS2PTR,                                                     1168
*     PDSPTRV,                                                     1168
*     RLDADDR) UNRESTRICTED;                                       1168
*   GENERATE NOREFS NOSETS(EJECT);                                 1169
*                                                                  1169
         EJECT
*/********************************************************************/
*/*                                                                  */
*/*         SUBROUTINE NPA5RCLM                                      */
*/*                                                                  */
*/*         FUNCTION - RECLAIM WASTED SPACE TO LOAD A MODULE         */
*/*                                                                  */
*/*         INPUT - SIZE OF MODULE TO BE LOADED (FMSIZE)             */
*/*            WASTED SPACE TABLE (NPA5WLST)                         */
*/*                                                                  */
*/*         OUTPUT - ADDRESS OF SPACE TO BE USED (RLDADDR) IF FOUND  */
*/*                                                                  */
*/********************************************************************/
*                                                                  1170
*NPA5RCLM:                          /* NPA5RCLM ENTRY PT             */
*   ENTRY;                          /* BEGINNING OF SUBR             */
         B     @EC01170                                            1170
NPA5RCLM STM   @14,@03,@SA00011                                    1170
         STM   @05,@12,@SA00011+24                                 1170
@EC01170 DS    0H                                                  1171
*   RESPECIFY                                                      1171
*    (RLDADDR) RESTRICTED;          /*                               */
*                                                                  1171
*   /*****************************************************************/
*   /*                                                               */
*   /* SEARCH THE QUEUES OF WASTED SPACE FOR A PLACE TO PUT THE      */
*   /* MODULE                                                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  1172
*   DO INDEX=1 TO DIM(NPA5WLST)-1 WHILE(RLDADDR=0);/* LOOP UNTIL   1172
*                                      FOUND                         */
         LA    @08,1                                               1172
         ST    @08,INDEX                                           1172
@DL01172 LTR   RLDADDR,RLDADDR                                     1172
         BNZ   @DC01172                                            1172
*     IF FMSIZE<NPA5WSIZ(INDEX+1) THEN/* WOULD THE MODULE FIT IN THE
*                                      SPACE REPRESENTED BY THIS   1173
*                                      QUEUE                         */
         SLA   @08,3                                               1173
         L     @05,FMSIZE                                          1173
         C     @05,NPA5WSIZ(@08)                                   1173
         BNL   @RF01173                                            1173
*       DO;                         /* YES, SEE IF THERE ARE ANY   1174
*                                      USEABLE ENTRIES IN THIS QUEUE */
*         LASTPTR=ADDR(NPA5WQNE(INDEX));/* POINT TO QUEUE HEADER     */
         LA    @08,NPA5WQNE-8(@08)                                 1175
         ST    @08,LASTPTR                                         1175
*         WASTPTR=LASTPTR->WASTNEXT;/* PICK UP ADDRESS OF FIRST BLOCK
*                                      ON Q                          */
         MVC   WASTPTR(4),WASTNEXT(@08)                            1176
*         DO WHILE(WASTPTR^=0&WASTSIZE<FMSIZE);/* SEARCH THE QUEUE 1177
*                                      FOR AN AREA LARGE ENOUGH TO 1177
*                                      CONTAIN THE MODULE            */
         B     @DE01177                                            1177
@DL01177 DS    0H                                                  1178
*           LASTPTR=WASTPTR;        /* SAVE ADDRESS OF CURRENT BLOCK */
         L     @08,WASTPTR                                         1178
         ST    @08,LASTPTR                                         1178
*           WASTPTR=WASTNEXT;       /* PICK UP ADDRESS OF NEXT BLOCK */
         L     @08,WASTNEXT(,@08)                                  1179
         ST    @08,WASTPTR                                         1179
*         END;                      /* END OF SEARCH LOOP            */
@DE01177 L     @08,WASTPTR                                         1180
         LTR   @08,@08                                             1180
         BZ    @DC01177                                            1180
         L     @08,WASTSIZE(,@08)                                  1180
         C     @08,FMSIZE                                          1180
         BL    @DL01177                                            1180
@DC01177 DS    0H                                                  1181
*         IF WASTPTR^=0 THEN        /* WAS AN AREA FOUND             */
         L     @08,WASTPTR                                         1181
         LTR   @08,@08                                             1181
         BZ    @RF01181                                            1181
*           DO;                     /* YES, USE IT                   */
*             LASTPTR->WASTNEXT=WASTNEXT;/* REMOVE THE BLOCK FROM THE
*                                      QUEUE                         */
         L     @05,LASTPTR                                         1183
         MVC   WASTNEXT(4,@05),WASTNEXT(@08)                       1183
*             WASTE=WASTSIZE-FMSIZE;/* CALCULATE THE AMOUNT OF     1184
*                                      UNNEEDED SPACE IN THE AREA    */
         L     @05,WASTSIZE(,@08)                                  1184
         SL    @05,FMSIZE                                          1184
         ST    @05,WASTE                                           1184
*             WASTEADR=WASTAREA;    /* CALCULATE THE ADDRESS OF THE
*                                      WASTED SPACE                  */
         L     @15,WASTAREA(,@08)                                  1185
         ST    @15,WASTEADR                                        1185
*             RLDADDR=WASTAREA+WASTE;/* CALCULATE THE ADDRESS OF   1186
