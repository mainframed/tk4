*/*                                                                  */
*/*         SUBROUTINE NP5PKERR                                      */
*/*                                                                  */
*/*         FUNCTION - PROCESSES AN ERROR ENCOUNTERED WHILE          */
*/*            ATTEMPTING TO BUILD A LOAD MODULE QUEUE TO SUPPORT    */
*/*            THE LPA LOAD MODULE PACKING FUNCTION                  */
*/*                                                                  */
*/*         INPUT - REGISTER 1 POINTS TO THE ADDRESS OF A HALFWORD   */
*/*            ERROR CODE                                            */
*/*                                                                  */
*/*         OUTPUT - TEXTPTR IS UPDATED TO POINT PAST THE ERROR      */
*/*                                                                  */
*/********************************************************************/
*                                                                  0690
*NP5PKERR:                          /* NP5PKERR ENTRY PT             */
*   PROC;                           /* BEGINNING OF SUBR             */
         B     @PB00008                                            0690
NP5PKERR STM   @14,@12,@SA00008                                    0690
*   CODE=ERRCOD;                    /* COPY ERROR CODE               */
         L     @08,ERRPTR(,RPARM)                                  0691
         LH    @08,ERRCOD(,@08)                                    0691
         STH   @08,CODE                                            0691
*   IF CODE=DELERRCODE=DELNG THEN  /* TEST FOR DELIMITER ERROR LINK
*                                      MISSING (                     */
         CH    @08,@CH00058                                        0692
         BE    @RT00692                                            0692
         CH    @08,@CH00067                                        0692
         BNE   @RF00692                                            0692
@RT00692 DS    0H                                                  0693
*     DO;                           /* DELIMITER ERROR               */
*                                                                  0693
*       /*************************************************************/
*       /*                                                           */
*       /* IEA357I DELIMITER FOLLOWING MODNAME INVALID - SKIPPING TO */
*       /* NEXT GROUP                                                */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0694
*       DELMDNM=LASTMODN;           /* PUT LAST VALID MODULE NAME  0694
*                                      INTO CAUSE OF ERROR           */
         MVC   DELMDNM(8),LASTMODN                                 0694
*       M357FI1=CSEDELNG;           /* PUT CAUSE INTO MSG            */
         MVC   M357FI1(36),CSEDELNG                                0695
*       M357FI2=GRPSKIP;            /* PUT ACTION INTO MSG           */
         MVC   M357FI2(22),GRPSKIP                                 0696
*       IF CODE=DELNG THEN          /* IF DELIMITER IS NO GOOD       */
         CLC   CODE(2),@CH00067                                    0697
         BE    @RT00697                                            0697
*         GOTO NP5SCANR;            /* SCAN FOR RIGHT PAREND         */
*       ELSE                                                       0699
*         ;                                                        0699
*                                                                  0699
*       /*************************************************************/
*       /*                                                           */
*       /* SCAN FOR THE START OF THE NEXT GROUP SO PROCESSING CAN    */
*       /* RESUME AFTER THE GROUP IN ERROR                           */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0700
*       DO WHILE(SCAN^='(');        /* SCAN FOR LEFT PAREN           */
         B     @DE00700                                            0700
@DL00700 DS    0H                                                  0701
*         IF SCAN=' ' THEN          /* TEST FOR BLANK                */
         L     @08,TEXTPTR                                         0701
         CLI   SCAN(@08),C' '                                      0701
         BNE   @RF00701                                            0701
*           DO;                     /* BLANK - END OF RECORD - MUST
*                                      READ NEXT RECORD              */
*                                                                  0702
*             /*******************************************************/
*             /*                                                     */
*             /* READ THE NEXT RECORD OF THE PACKING LIST            */
*             /*                                                     */
*             /*******************************************************/
*                                                                  0703
*             GENERATE REFS(NVTREAL,RNVT) SETS(RENTRY,REXIT)       0703
*                 (IEAPMNIP TYPE=REAL);/* SWITCH TCB TO V=R          */
         IEAPMNIP TYPE=REAL
*             CALL NIPPMTXT;        /* READ A RECORD                 */
         BAL   @14,NIPPMTXT                                        0704
*             BC(15,NP5PKER1);      /* I/O OK                        */
         BC    15,NP5PKER1                                         0705
*             BC(15,NP5PKER2);      /* I/O ERROR - EOF               */
         BC    15,NP5PKER2                                         0706
*NP5PKER1:                                                         0707
*             GENERATE REFS(NVTVIRT,RNVT) SETS(RENTRY,REXIT)       0707
*                 (IEAPMNIP TYPE=VIRT);/* SWITCH TCB TO V=V READ OK  */
NP5PKER1 IEAPMNIP TYPE=VIRT
*             TEXTPTR=RPARM;        /* POINT TO FIRST CHARACTER OF 0708
*                                      NEW RECORD                    */
         ST    RPARM,TEXTPTR                                       0708
*           END;                    /*                               */
*         ELSE                      /* NOT BLANK - MORE DATA TO    0710
*                                      PROCESS                       */
*           TEXTPTR=TEXTPTR+1;      /* STEP POINTER                  */
         B     @RC00701                                            0710
@RF00701 LA    @08,1                                               0710
         AL    @08,TEXTPTR                                         0710
         ST    @08,TEXTPTR                                         0710
*       END;                        /* END OF SCAN LOOP              */
@RC00701 DS    0H                                                  0711
@DE00700 L     @08,TEXTPTR                                         0711
         CLI   SCAN(@08),C'('                                      0711
         BNE   @DL00700                                            0711
*     END;                          /* END OF DELIMITER ERROR      0712
*                                      PROCESSING                    */
*   IF CODE=PKIOERR THEN            /* TEST FOR I/O ERROR            */
@RF00692 CLC   CODE(2),@CH00050                                    0713
         BNE   @RF00713                                            0713
*     DO;                           /* YES, I/O ERROR                */
*NP5PKER5:                          /* PROCESS I/O ERROR             */
*                                                                  0715
*       /*************************************************************/
*       /*                                                           */
*       /* IEA357I I/O ERROR FOLLOWING MODNAME - LIST TERMINATED     */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0715
*       IOERMDN=LASTMODN;           /* PUT LAST VALID MODULE NAME  0715
*                                      INTO TEXT                     */
NP5PKER5 MVC   IOERMDN(8),LASTMODN                                 0715
*       M357FI1=CSEIOERR;           /* PUT CAUSE OF ERROR INTO     0716
*                                      MESSAGE                       */
         MVI   M357FI1+28,C' '                                     0716
         MVC   M357FI1+29(7),M357FI1+28                            0716
         MVC   M357FI1(28),CSEIOERR                                0716
*       M357FI2=LISTTERM;           /* PUT ACTION TO MSG             */
*                                                                  0717
         MVI   M357FI2+15,C' '                                     0717
         MVC   M357FI2+16(6),M357FI2+15                            0717
         MVC   M357FI2(15),LISTTERM                                0717
*       /*************************************************************/
*       /*                                                           */
*       /* FORCE TERMINATION OF LIST PROCESSING                      */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0718
*       TEXTPTR=ADDR('FF'X);        /* POINT TO END OF LIST INDICATOR*/
         LA    @08,@CB00105                                        0718
         ST    @08,TEXTPTR                                         0718
*       ENDOFQUE='1'B;              /* INDICATE END OF CURRENT LOAD
*                                      MODULE QUEUE                  */
*       PKERIOF='0'B;               /* RESET I/O ERROR DURING      0720
*                                      RECOVERY FLAG                 */
         OI    ENDOFQUE,B'00100000'                                0720
         NI    PKERIOF,B'11111011'                                 0720
*     END;                          /* END OF I/O ERROR PROCESSING   */
*   IF CODE=PKNAMNG THEN            /* TEST FOR NAME NO GOOD         */
@RF00713 CLC   CODE(2),@CH00075                                    0722
         BNE   @RF00722                                            0722
*     DO;                           /* YES, NAME SYNTACTILLY INVALID */
*                                                                  0723
*       /*************************************************************/
*       /*                                                           */
*       /* IEA357I NAME FOLLOWING MODNAME IMPROPER - SKIPPING TO NEXT*/
*       /* GROUP                                                     */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0724
*       NMMODNM=LASTMODN;           /* PUT LAST VALID MODULE NAME  0724
*                                      INTO TEXT                     */
         MVC   NMMODNM(8),LASTMODN                                 0724
*       M357FI1=CSENAMNG;           /* PUT ERROR CAUSE INTO MESSAGE  */
         MVI   M357FI1+32,C' '                                     0725
         MVC   M357FI1+33(3),M357FI1+32                            0725
         MVC   M357FI1(32),CSENAMNG                                0725
*       M357FI2=GRPSKIP;            /* PUT ACTION INTO MSG           */
         MVC   M357FI2(22),GRPSKIP                                 0726
*NP5SCANR:                                                         0727
*       INDEX=0;                    /* INDICATE NO NAME FOUND        */
*                                                                  0727
NP5SCANR SLR   @08,@08                                             0727
         ST    @08,INDEX                                           0727
*       /*************************************************************/
*       /*                                                           */
*       /* SCAN FOR THE LEFT PARENTHESIS AT THE END OF THE GROUP SO  */
*       /* PROCESSING CAN RESUME AFTER THE GROUP IN ERROR            */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0728
*       DO WHILE(SCAN^=')');        /* SCAN FOR )                    */
         B     @DE00728                                            0728
@DL00728 DS    0H                                                  0729
*         IF SCAN='FF'X THEN        /* TEST FOR END OF RECORD        */
         L     @08,TEXTPTR                                         0729
         CLI   SCAN(@08),X'FF'                                     0729
         BNE   @RF00729                                            0729
*           DO;                     /* YES, END OF RECORD            */
*                                                                  0730
*             /*******************************************************/
*             /*                                                     */
*             /* READ THE NEXT RECORD OF THE PACKING LIST            */
*             /*                                                     */
*             /*******************************************************/
*                                                                  0731
*             GENERATE REFS(NVTREAL,RNVT) SETS(RENTRY,REXIT)       0731
*                 (IEAPMNIP TYPE=REAL);/* SWITCH TCB TO V=R          */
         IEAPMNIP TYPE=REAL
*             CALL NIPPMTXT;        /* READ A RECORD                 */
         BAL   @14,NIPPMTXT                                        0732
*             BC(15,NP5PKER4);      /* I/O OK                        */
         BC    15,NP5PKER4                                         0733
*             BC(15,NP5PKER2);      /* I/O ERROR                     */
         BC    15,NP5PKER2                                         0734
*NP5PKER4:                                                         0735
*             GENERATE REFS(NVTVIRT,RNVT) SETS(RENTRY,REXIT)       0735
*                 (IEAPMNIP TYPE=VIRT);/* SWITCH TCB TO V=V I/O OK -
*                                      CONTINUE                      */
NP5PKER4 IEAPMNIP TYPE=VIRT
*             TEXTPTR=RPARM;        /* POINT TO FIRST BYTE OF RECORD */
         ST    RPARM,TEXTPTR                                       0736
*           END;                    /*                               */
*         ELSE                      /* NOT BLANK - MORE DATA ON    0738
*                                      RECORD                        */
*           TEXTPTR=TEXTPTR+1;      /* POINT PAST CHAR               */
         B     @RC00729                                            0738
@RF00729 LA    @08,1                                               0738
         AL    @08,TEXTPTR                                         0738
         ST    @08,TEXTPTR                                         0738
*       END;                        /* END OF SCAN LOOP              */
@RC00729 DS    0H                                                  0739
@DE00728 L     @08,TEXTPTR                                         0739
         CLI   SCAN(@08),C')'                                      0739
         BNE   @DL00728                                            0739
*     END;                          /* END OF INVALID NAME PROCESSING*/
*   IF CODE=NAMENG THEN             /* TEST FOR MODULE NAME        0741
*                                      IMPROPERLY SPECIFIED          */
@RF00722 CLC   CODE(2),@CH00061                                    0741
         BNE   @RF00741                                            0741
*     DO;                           /* YES, NAME NO GOOD             */
*                                                                  0742
*       /*************************************************************/
*       /*                                                           */
*       /* IEA357I MODNAME INVALID - MODULE IGNORED                  */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0743
*       MNINVN=MODULE;              /* PUT MODULE NAME INTO TEXT     */
         MVC   MNINVN(8),MODULE                                    0743
*       M357FI1=CSEMINV;            /* PUT ERROR CAUSE INTO MESSAGE  */
         MVI   M357FI1+16,C' '                                     0744
         MVC   M357FI1+17(19),M357FI1+16                           0744
         MVC   M357FI1(16),CSEMINV                                 0744
*       M357FI2=MODIGND;            /* PUT ACTION INTO MSG           */
         MVI   M357FI2+14,C' '                                     0745
         MVC   M357FI2+15(7),M357FI2+14                            0745
         MVC   M357FI2(14),MODIGND                                 0745
*     END;                          /* END OF INVALID MODULE NAME  0746
*                                      PROCESSING                    */
*NP5PKER3:                          /* PUT OUT MESSAGE(S)            */
*                                                                  0747
*   /*****************************************************************/
*   /*                                                               */
*   /* SWITCH TO V=R TCB FOR WTO                                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0747
*   GENERATE REFS(NVTREAL,RNVT) SETS(RENTRY,REXIT)(IEAPMNIP TYPE=REAL);
@RF00741 DS    0H                                                  0747
NP5PKER3 IEAPMNIP TYPE=REAL
*                                   /* SWITCH TO V=R                 */
*   IF PKERIND='0'B THEN            /* WAS HEADER MESSAGE PRINTED  0748
*                                      BEFORE                        */
         TM    PKERIND,B'00001000'                                 0748
         BNZ   @RF00748                                            0748
*     DO;                           /* NO, PRINT IT OUT              */
*       PKERIND='1'B;               /* INDICATE HEADER MESSAGE ISSUED*/
*                                                                  0750
         OI    PKERIND,B'00001000'                                 0750
*       /*************************************************************/
*       /*                                                           */
*       /* IEA357I ERRORS IN IEAPAK00 LIST                           */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0751
*       GENERATE REFS(RNVT,NVTWTO,MSG357I1) SETS(RPARM,REXIT,RENTRY)
*           (IEAPMNIP TYPE=WTO,PARAM=MSG357I1);/* WTO                */
         IEAPMNIP TYPE=WTO,PARAM=MSG357I1
*     END;                                                         0752
*   GENERATE REFS(RNVT,NVTWTO,MSG357I2,M357FI1,M357FI2) SETS(RPARM,
*       REXIT,RENTRY)(IEAPMNIP TYPE=WTO,PARAM=MSG357I2);/* WTO ERROR
*                                      MESSAGE                       */
*                                                                  0753
@RF00748 DS    0H                                                  0753
         IEAPMNIP TYPE=WTO,PARAM=MSG357I2
*   /*****************************************************************/
*   /*                                                               */
*   /* SWITCH TCB BACK TO V=V                                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0754
*   GENERATE REFS(NVTVIRT,RNVT) SETS(RENTRY,REXIT)(IEAPMNIP TYPE=VIRT);
         IEAPMNIP TYPE=VIRT
*                                   /* SWITCH TO V=V                 */
*   IF PKERIOF='1'B THEN            /* WAS AN I/O ERROR DETECTED   0755
*                                      DURING ATTEMPTED ERROR      0755
*                                      RECOVERY                      */
         TM    PKERIOF,B'00000100'                                 0755
         BO    @RT00755                                            0755
*     GOTO NP5PKER5;                /* YES, PRINT I/O ERROR MESSAGE  */
*   RETURN;                         /* RETURN TO CALLER              */
@EL00008 DS    0H                                                  0757
@EF00008 DS    0H                                                  0757
@ER00008 LM    @14,@12,@SA00008                                    0757
         BR    @14                                                 0757
*NP5PKER2:                          /* I/O ERROR DURING ERROR      0758
*                                      RECOVERY                      */
*   PKERIOF='1'B;                   /* INDICATE I/O ERROR            */
NP5PKER2 OI    PKERIOF,B'00000100'                                 0758
*   CODE=PKIOERR;                   /* INDICATE I/O ERROR            */
         MVC   CODE(2),@CH00050                                    0759
*   GOTO NP5PKER3;                  /* PRINT ORIGINAL ERROR AND    0760
*                                      PROCESS I/O ERROR             */
         B     NP5PKER3                                            0760
*   END NP5PKERR;                   /* END OF SUBROUTINE             */
@PB00008 DS    0H                                                  0762
*   GENERATE NOREFS NOSETS(EJECT);                                 0762
*                                                                  0762
         EJECT
*/********************************************************************/
*/*                                                                  */
*/*         SUBROUTINE NPA5LIND                                      */
*/*                                                                  */
*/*         FUNCTION - CONTROLS THE LOADING OF THE LPA MODULES WHICH */
*/*            WERE NOT SPECIFIED AS PART OF ORDERED GROUPS          */
*/*                                                                  */
*/*         INPUT - BLDL INFORMATION TABLE                           */
*/*                                                                  */
*/*         OUTPUT - THE MODULES ARE LOADED INTO THE LPA             */
*/*                                                                  */
*/********************************************************************/
*                                                                  0763
*NPA5LIND:                          /* NPA5LIND ENTRY PT             */
*   PROCEDURE;                      /* BEGINNING OF SUBR             */
         B     @PB00009                                            0763
NPA5LIND STM   @14,@12,@SA00009                                    0763
*   DO INDEX2=DIM(NPA5WLST) TO 1 BY-1;/* LOAD MODULES IN MULTIPLE  0764
*                                      PASSES - ON EACH PASS, ONLY 0764
*                                      LOAD MODULES LARGER THAN THE
*                                      GIVEN CRITERION VALUE         */
         LA    @08,12                                              0764
         ST    @08,INDEX2                                          0764
@DL00764 DS    0H                                                  0765
*     NPA5CRIT=NPA5WSIZ(INDEX2);    /* ESTABLISH CRITERION FOR THIS
*                                      LOADING PASS                  */
         SLA   @08,3                                               0765
         L     @08,NPA5WSIZ-8(@08)                                 0765
         ST    @08,NPA5CRIT                                        0765
*     DO INFOPTR=INFOSTRT BY LENGTH(INFODATA) WHILE(INFNAME^=DUMMY);
         L     @08,INFOSTRT                                        0766
         ST    @08,INFOPTR                                         0766
         B     @DE00766                                            0766
@DL00766 DS    0H                                                  0767
*                                   /* LOOP THROUGH THE ENTIRE TABLE */
*       IF INFIND='0'B&INFALIS='0'B THEN/* TEST FOR BLDL INFO ENTRY
*                                      FOR A MAJOR NAME              */
         L     @08,INFOPTR                                         0767
         TM    INFIND(@08),B'10000000'                             0767
         BNZ   @RF00767                                            0767
         L     @08,INFOPTR                                         0767
         TM    INFALIS(@08),B'01000000'                            0767
         BNZ   @RF00767                                            0767
*         IF INFSTOR>=NPA5CRITINFPAGA='1'B THEN/* TEST WHETHER THIS
*                                      MODULE IS TO BE LOADED IN THIS
*                                      PASS OR NOT                   */
*                                                                  0768
         L     @08,INFOPTR                                         0768
         SLR   @00,@00                                             0768
         ICM   @00,7,INFSTOR(@08)                                  0768
         C     @00,NPA5CRIT                                        0768
         BNL   @RT00768                                            0768
         L     @08,INFOPTR                                         0768
         TM    INFPAGA(@08),B'01000000'                            0768
         BNO   @RF00768                                            0768
@RT00768 DS    0H                                                  0769
*           /*********************************************************/
*           /*                                                       */
*           /* LOAD THIS MODULE INTO THE LINK PACK AREA              */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0769
*           CALL NPA5ILOD;          /* LOAD THE MODULE               */
         BAL   @14,NPA5ILOD                                        0769
*     END;                          /* END OF SCAN LOOP              */
@RF00768 DS    0H                                                  0770
@RF00767 LA    @08,40                                              0770
         AL    @08,INFOPTR                                         0770
         ST    @08,INFOPTR                                         0770
@DE00766 L     @08,INFOPTR                                         0770
         CLC   INFNAME(8,@08),@CB00041                             0770
         BNE   @DL00766                                            0770
*   END;                            /* END OF LOADING PASS - LOOP  0771
*                                      TILL ALL MODULES LOADED       */
         SLR   @08,@08                                             0771
         BCTR  @08,0                                               0771
         AL    @08,INDEX2                                          0771
         ST    @08,INDEX2                                          0771
         LTR   @08,@08                                             0771
         BP    @DL00764                                            0771
*   RETURN;                         /* RETURN TO CALLER              */
@EL00009 DS    0H                                                  0772
@EF00009 DS    0H                                                  0772
@ER00009 LM    @14,@12,@SA00009                                    0772
         BR    @14                                                 0772
*   GENERATE NOREFS NOSETS(EJECT);                                 0773
*                                                                  0773
         EJECT
