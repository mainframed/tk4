*/*                                                                  */
*/*         SUBROUTINE NP5FINDE                                      */
*/*                                                                  */
*/*         FUNCTION - TO FIND AND VALIDATE THE BLDL INFORMATION     */
*/*            TABLE ENTRY FOR A GIVEN MODULE TO BE ADDED TO THE     */
*/*            LOAD MODULE QUEUE                                     */
*/*                                                                  */
*/*         INPUT - THE NAME OF THE MODULE TO BE FOUND               */
*/*            THE ADDRESS OF THE START OF THE BLDL INFO INDEX TABLE */
*/*            THE LPA DIRECTORY HASH VALUE                          */
*/*                                                                  */
*/*         OUTPUT - THE ADDRESS OF THE BLDL INFO TABLE ENTRY        */
*/*                                                                  */
*/********************************************************************/
*                                                                  0664
*NP5FINDE:                          /* NP5FINDE ENTRY PT             */
*   PROC;                           /* BEGINNING OF SUBR             */
         B     @PB00007                                            0664
NP5FINDE STM   @14,@12,@SA00007                                    0664
*   DIRNTNO=(MOD1&&MOD2)//NVTLPAHA; /* CALCULATE THE LPA DIRECTORY 0665
*                                      INDEX FOR THE MODULE  @Z40WPYB*/
         L     @08,MOD1                                            0665
         X     @08,MOD2                                            0665
         LR    @05,@08                                             0665
         SLR   @04,@04                                             0665
         D     @04,NVTLPAHA(,RNVT)                                 0665
         ST    @04,DIRNTNO                                         0665
*   SLOTADDR=(DIRNTNO*4)+NA5BLDIR;  /* CALCULATE THE ADDRESS OF THE
*                                      BLDL INFO INDEX TABLE ENTRY   */
         SLA   @04,2                                               0666
         AL    @04,NA5BLDIR                                        0666
         ST    @04,SLOTADDR                                        0666
*   INFOPTR=SLOTADDR->NP5BPTR;      /* PICK UP ADDRESS OF FIRST BLDL
*                                      INFO TABLE ENTRY IN THE     0667
*                                      SYNONYM QUEUE                 */
         MVC   INFOPTR(4),NP5BPTR(@04)                             0667
*   IF INFOPTR^=0 THEN              /* TEST FOR NON-ZERO QUEUE HEADER*/
*                                                                  0668
         ICM   @08,15,INFOPTR                                      0668
         BZ    @RF00668                                            0668
*     /***************************************************************/
*     /*                                                             */
*     /* SEARCH THE SYNONYM CHAIN FOR THE SPECIFIED MODULE           */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0669
*     DO WHILE(MODERR='0'B&INFNAME^=MODULE);/* SCAN UNTIL THE GIVEN
*                                      MODULE NAME IS FOUND OR THE 0669
*                                      QUEUE IS FINISHED             */
         B     @DE00669                                            0669
@DL00669 DS    0H                                                  0670
*       IF INFSYNM^=0 THEN          /* ARE THERE ANY MORE MODULES IN
*                                      THIS SYNONYM CHAIN            */
         L     @08,INFOPTR                                         0670
         ICM   @00,7,INFSYNM(@08)                                  0670
         BZ    @RF00670                                            0670
*         INFOPTR=INFSYNM;          /* YES, KEEP GOING               */
         L     @08,INFOPTR                                         0671
         SLR   @00,@00                                             0671
         ICM   @00,7,INFSYNM(@08)                                  0671
         ST    @00,INFOPTR                                         0671
*       ELSE                        /* NO, END OF CHAIN              */
*         MODERR='1'B;              /* MODULE NOT FOUND              */
         B     @RC00670                                            0672
@RF00670 OI    MODERR,B'00010000'                                  0672
*     END;                          /* END OF SCAN LOOP              */
@RC00670 DS    0H                                                  0673
@DE00669 TM    MODERR,B'00010000'                                  0673
         BNZ   @DC00669                                            0673
         L     @08,INFOPTR                                         0673
         CLC   INFNAME(8,@08),MODULE                               0673
         BNE   @DL00669                                            0673
@DC00669 DS    0H                                                  0674
*   ELSE                            /* NO ELEMENTS ON Q              */
*     MODERR='1'B;                  /* INDICATE MODULE NOT FOUND     */
         B     @RC00668                                            0674
@RF00668 OI    MODERR,B'00010000'                                  0674
*   IF MODERR='0'B THEN             /* WAS MODULE FOUND              */
@RC00668 TM    MODERR,B'00010000'                                  0675
         BNZ   @RF00675                                            0675
*     IF INFIND='1'B THEN           /* YES, TEST FOR MODULE ALREADY
*                                      LOADED                        */
         L     @08,INFOPTR                                         0676
         TM    INFIND(@08),B'10000000'                             0676
         BNO   @RF00676                                            0676
*       MODERR='1'B;                /* YES, INDICATE INVALID       0677
*                                      SPECIFICATION                 */
         OI    MODERR,B'00010000'                                  0677
*     ELSE                          /* NO, CONTINUE CHECK            */
*       IF INFALIS='1'BINFCHAIN^=0INFOPTR=LASTMOD THEN/* TEST FOR A
*                                      MODULE WHICH IS AN ALIAS NAME
*                                      OR IS ALREADY ON THIS QUEUE   */
         B     @RC00676                                            0678
@RF00676 L     @08,INFOPTR                                         0678
         TM    INFALIS(@08),B'01000000'                            0678
         BO    @RT00678                                            0678
         L     @08,INFOPTR                                         0678
         ICM   @00,7,INFCHAIN(@08)                                 0678
         BNZ   @RT00678                                            0678
         CLC   INFOPTR(4),LASTMOD                                  0678
         BNE   @RF00678                                            0678
@RT00678 DS    0H                                                  0679
*         MODERR='1'B;              /* INDICATE ERROR                */
         OI    MODERR,B'00010000'                                  0679
*   IF MODERR='1'B THEN             /* WAS THERE AN ERROR            */
*                                                                  0680
@RF00678 DS    0H                                                  0680
@RC00676 DS    0H                                                  0680
@RF00675 TM    MODERR,B'00010000'                                  0680
         BNO   @RF00680                                            0680
*     /***************************************************************/
*     /*                                                             */
*     /* PROCESS INVALID MODULE NAME SPECIFICATION                   */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0681
*     CALL NP5PKERR(NAMENG);        /* PROCESS ERROR                 */
         LA    @01,@AL00681                                        0681
         BAL   @14,NP5PKERR                                        0681
*   RETURN;                         /* RETURN TO CALLER              */
@EL00007 DS    0H                                                  0682
@EF00007 DS    0H                                                  0682
@ER00007 LM    @14,@12,@SA00007                                    0682
         BR    @14                                                 0682
*   GENERATE NOREFS NOSETS(EJECT);                                 0683
*                                                                  0683
         EJECT
