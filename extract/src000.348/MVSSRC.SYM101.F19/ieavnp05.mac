*/*                                                                  */
*/*         SUBROUTINE NPA5CLIN                                      */
*/*                                                                  */
*/*         FUNCTION - CONTROLS THE BUILDING OF THE BLDL INFORMATION */
*/*            TABLE FOR ALL THE MODULES WHICH ARE ON THE            */
*/*            SYS1.LPALIB DATA SET                                  */
*/*                                                                  */
*/*         INPUT - NONE                                             */
*/*                                                                  */
*/*         OUTPUT - THE BLDL INFORMATION TABLE IS BUILD STARTING    */
*/*            AT THE LOW END OF NIPS V=V REGION                     */
*/*                                                                  */
*/********************************************************************/
*                                                                  0513
*NPA5CLIN:                          /* NPA5CLIN ENTRY PT             */
*   PROCEDURE;                      /* BEGINNING OF SUBR             */
         B     @PB00004                                            0513
NPA5CLIN STM   @14,@12,@SA00004                                    0513
*   RESPECIFY                                                      0514
*    (DEBPTR,                                                      0514
*     DEBDAPTR) RESTRICTED;                                        0514
*   NPA5INFP=NVTVVPG1;              /* SAVE STARTING ADDR            */
         L     @08,NVTVVPG1(,RNVT)                                 0515
         ST    @08,NPA5INFP                                        0515
*   INFOPTR=NVTVVPG1;               /* SET UP POINTER TO FIRST INFO
*                                      TABLE ENTRY                   */
         ST    @08,INFOPTR                                         0516
*   INFOSTRT=NVTVVPG1;              /* SAVE ADDRESS OF FIRST ENTRY   */
         MVC   INFOSTRT(4),NVTVVPG1(RNVT)                          0517
*   LPDECT=0;                       /* SET NUMBER OF MODULES       0518
*                                      PROCESSED TO 0                */
*                                                                  0518
         SLR   @08,@08                                             0518
         ST    @08,LPDECT                                          0518
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE THE I/O CONTROL BLOCKS FOR SYS1.LPALIB I/O         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0519
*   IOBSTPTR=ADDR(NIPIOBSN);        /* POINT TO IOB                  */
         LA    @08,NIPIOBSN                                        0519
         ST    @08,IOBSTPTR                                        0519
*   IOBDCBPB=NVTCSLIB;              /* POINT TO LPALIB DCB           */
         L     @07,NVTCSLIB(,RNVT)                                 0520
         STCM  @07,7,IOBDCBPB(@08)                                 0520
*   DCBPTR=NVTCSLIB;                /* SAVE DCB POINTER              */
         ST    @07,DCBPTR                                          0521
*   DEBPTR=DCBDEBAD;                /* POINT TO LPALIB DEB           */
         SLR   DEBPTR,DEBPTR                                       0522
         ICM   DEBPTR,7,DCBDEBAD(@07)                              0522
*   DEBDAPTR=ADDR(DEBBASND);        /* POINT TO FIRST EXTENT       0523
*                                      DESCRIPTION                   */
         LA    DEBDAPTR,DEBBASND(,DEBPTR)                          0523
*   IOBCC=DEBSTRCC;                 /* SET UP SEEK CC                */
         MVC   IOBCC(2,@08),DEBSTRCC(DEBDAPTR)                     0524
*   IOBHH=DEBSTRHH;                 /* SET UP SEEK HH                */
         MVC   IOBHH(2,@08),DEBSTRHH(DEBDAPTR)                     0525
*   NPA5SRCC(1:2)=DEBSTRCC;         /* SET UP SEARCH CC              */
         MVC   NPA5SRCC(2),DEBSTRCC(DEBDAPTR)                      0526
*   NPA5SRCC(3:4)=DEBSTRHH;         /* SET UP SEARCH HH              */
         MVC   NPA5SRCC+2(2),DEBSTRHH(DEBDAPTR)                    0527
*   NVTLPAND=NVTLVIRT;              /* UPDATE NVT WITH HIGH LPA    0528
*                                      ADDRESS               @Z40WPYB*/
*                                                                  0528
         MVC   NVTLPAND(4,RNVT),NVTLVIRT(RNVT)                     0528
*   /*****************************************************************/
*   /*                                                               */
*   /* BUILD THE BLDL INFORMATION TABLE FOR EXECUTABLE MODS          */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0529
*   LASTBLOK='0'B;                  /* INDICATE NOT END OF LPALIB  0529
*                                      DIRECTORY                     */
         NI    LASTBLOK,B'01111111'                                0529
*   NP5NONEX='0'B;                  /* INITIALIZE NON-EXEC FLAG    0530
*                                                            @ZD04022*/
         NI    NP5NONEX,B'11101111'                                0530
*   DO WHILE(LASTBLOK='0'B);        /* LOOP THROUGH LPALIB DIRECTORY
*                                      UNTIL LAST BLOCK IS PROCESSED */
*                                                                  0531
         B     @DE00531                                            0531
@DL00531 DS    0H                                                  0532
*     /***************************************************************/
*     /*                                                             */
*     /* READ THE NEXT DIRECTORY BLOCK FROM SYS1.LPALIB              */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0532
*     CALL NPA5RBLK;                /* READ A BLOCK - NOTE THAT    0532
*                                      LASTBLOK WILL BE SET TO A 1 0532
*                                      WHEN THE LAST BLOCK HAS BEEN
*                                      READ IN                       */
*                                                                  0532
         BAL   @14,NPA5RBLK                                        0532
*     /***************************************************************/
*     /*                                                             */
*     /* BUILD THE INFORMATION TABLE ENTRIES FOR THE MODULES LISTED  */
*     /* IN THE DIRECTORY BLOCK JUST READ                            */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0533
*     CALL NPA5INFO;                /* BUILD TABLE ENTRIES           */
         BAL   @14,NPA5INFO                                        0533
*   END;                            /* END OF SCAN LOOP              */
@DE00531 TM    LASTBLOK,B'10000000'                                0534
         BZ    @DL00531                                            0534
*   IF NP5NONEX^='0'B THEN          /* IF NON EXEC MODS      @ZD04022*/
         TM    NP5NONEX,B'00010000'                                0535
         BZ    @RF00535                                            0535
*     CALL NPA5CERR(NONEX);         /* INDICATE ERROR        @ZD04022*/
         LA    @01,@AL00536                                        0536
         BAL   @14,NPA5CERR                                        0536
*   IF LPDECT=0 THEN                /* TEST FOR EMPTY SYS1.LPALIB  0537
*                                      DATA SET                      */
@RF00535 ICM   @08,15,LPDECT                                       0537
         BNZ   @RF00537                                            0537
*     CALL NPA5CERR(LPAMT);         /* YES, INDICATE ERROR           */
*                                                                  0538
         LA    @01,@AL00538                                        0538
         BAL   @14,NPA5CERR                                        0538
*   /*****************************************************************/
*   /*                                                               */
*   /* CALCULATE THE HASH TABLE INDEX REQUIRED FOR THE LPA DIRECTORY */
*   /* AND SET UP THE HASH POINTERS FOR THE BLDL INFORMATION TABLE   */
*   /* ENTRIES                                                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0539
*   CALL NPA5HSHA;                  /* INITIALIZE HASHING ALGORITHM  */
*                                                                  0539
@RF00537 BAL   @14,NPA5HSHA                                        0539
*   /*****************************************************************/
*   /*                                                               */
*   /* VALIDITY CHECK THE ALIAS ENTRIES IN THE BLDL INFO TABLE AND   */
*   /* REPORT ANY WHICH ARE INVALID                                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0540
*   CALL NPA5VCHK;                  /* CHECK ALIASES                 */
         BAL   @14,NPA5VCHK                                        0540
*   RETURN;                         /* RETURN TO CALLER              */
@EL00004 DS    0H                                                  0541
@EF00004 DS    0H                                                  0541
@ER00004 LM    @14,@12,@SA00004                                    0541
         BR    @14                                                 0541
*   RESPECIFY                                                      0542
*    (DEBPTR,                                                      0542
*     DEBDAPTR) UNRESTRICTED;                                      0542
*   END NPA5CLIN;                                                  0543
@PB00004 DS    0H                                                  0544
*   GENERATE NOREFS NOSETS(EJECT);                                 0544
*                                                                  0544
         EJECT
*/********************************************************************/
*/*                                                                  */
*/*         SUBROUTINE NPA5BLMQ                                      */
*/*                                                                  */
*/*         FUNCTION - BUILD A LOAD MODULE QUEUE TO CONTROL THE      */
*/*            LOADING OF ORDERED LPA MODULES                        */
*/*                                                                  */
*/*         INPUT - THE LPA PACKING LIST MEMBER OF SYS1.PARMLIB      */
*/*            THE ADDRESS OF THE LAST BYTE OF THE LIST EXAMINED     */
*/*                                                                  */
*/*         OUTPUT - A QUEUE OF BLDL INFORMATION TABLE ENTRIES       */
*/*            POINTED TO BY NA5CSGRP                                */
*/*                                                                  */
*/********************************************************************/
*                                                                  0545
*NPA5BLMQ:                                                         0545
*   PROC;                           /* BEGINNING OF SUBR             */
         B     @PB00005                                            0545
NPA5BLMQ STM   @14,@12,@SA00005                                    0545
*   RESPECIFY                                                      0546
*     NIPPAREA BASED(NVTPAREA);     /*                               */
*                                                                  0546
*   /*****************************************************************/
*   /*                                                               */
*   /* IF THE LPA PACKING LIST HAS NOT ALREADY BEEN LOCATED FIND IT  */
*   /* NOW AND INITIALIZE FOR BUILDING QUEUES                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0547
*   IF PAKFOUND='0'B THEN           /* WAS LIST FOUND                */
         TM    PAKFOUND,B'01000000'                                0547
         BNZ   @RF00547                                            0547
*     DO;                           /* NO, FIND PACKING LIST MEMBER
*                                      OF PARMLIB                    */
*       NIPPANAM=PAKLSTNM;          /* SET UP MEMBER NAME            */
*                                                                  0549
         L     @08,NVTPAREA(,RNVT)                                 0549
         MVC   NIPPANAM(8,@08),@CC00071                            0549
*       /*************************************************************/
*       /*                                                           */
*       /* LOCATE SYS1.PARMLIB MEMBER FOR PACK LIST                  */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0550
*       GENERATE REFS(NVTREAL,RNVT) SETS(RENTRY,REXIT)             0550
*           (IEAPMNIP TYPE=REAL);   /* SWITCH TCB TO V=R             */
         IEAPMNIP TYPE=REAL
*       CALL NIPPMPDS;              /* LOCATE MEMBER                 */
         BAL   @14,NIPPMPDS                                        0551
*       BC(15,NP5LMQ01);            /* MEMBER FOUND - OK             */
         BC    15,NP5LMQ01                                         0552
*       BC(15,NP5LMQ02);            /* MEMBER NOT FOUND              */
         BC    15,NP5LMQ02                                         0553
*NP5LMQ01:                                                         0554
*       GENERATE REFS(NVTVIRT,RNVT) SETS(RENTRY,REXIT)             0554
*           (IEAPMNIP TYPE=VIRT);   /* SWITCH TCB TO V=V PACK LIST 0554
*                                      MEMBER FOUND ON SYS1.PARMLIB -
*                                      CONTINUE PROCESSING           */
NP5LMQ01 IEAPMNIP TYPE=VIRT
*       PAKFOUND='1'B;              /* INDICATE MEMBER FOUND         */
         OI    PAKFOUND,B'01000000'                                0555
*       TEXTPTR=ADDR(' ');          /* POINT TO BLANK - THIS FORCES A
*                                      READ OF THE NEXT TEXT RECORD
*                                      OF THE PACK LIST MEMBER       */
         LA    @08,@CC03588                                        0556
         ST    @08,TEXTPTR                                         0556
*     END;                                                         0557
*                                                                  0557
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE FOR START OF LOAD MODULE QUEUE                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0558
*   DO WHILE LASTREQ='0'B;          /* SETTUP GROUPS UNTIL LAST GROUP
*                                      IS PROCESSED                  */
@RF00547 B     @DE00558                                            0558
@DL00558 DS    0H                                                  0559
*     CALL NP5STRTQ;                /* SETTUP FOR QUEUE              */
*                                                                  0559
         BAL   @14,NP5STRTQ                                        0559
*     /***************************************************************/
*     /*                                                             */
*     /* ADD MODULES TO QUEUE UNTIL END IS FOUND                     */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0560
*     DO WHILE(ENDOFQUE='0'B);      /* LOOP UNTIL END                */
*                                                                  0560
         B     @DE00560                                            0560
@DL00560 DS    0H                                                  0561
*       /*************************************************************/
*       /*                                                           */
*       /* ADD THE NEXT MODULE TO THE QUEUE                          */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0561
*       CALL NP5NXTMD;              /* PROCESS NEXT NAME             */
         BAL   @14,NP5NXTMD                                        0561
*     END;                          /* END OF NAME PROCESS LOOP      */
@DE00560 TM    ENDOFQUE,B'00100000'                                0562
         BZ    @DL00560                                            0562
*     IF STGRP='0'B THEN            /* TEST FOR NO GROUP - IF SO,  0563
*                                      THIS IS THE END OF THE PACK 0563
*                                      LIST                          */
         TM    STGRP,B'00000001'                                   0563
         BNZ   @RF00563                                            0563
*NP5LMQ03:                                                         0564
*       LASTREQ='1'B;               /* PACK LIST NOT FOUND           */
NP5LMQ03 OI    LASTREQ,B'10000000'                                 0564
*     ELSE                                                         0565
*                                                                  0565
*       /*************************************************************/
*       /*                                                           */
*       /* LOAD MODULES SPECIFIED ON THIS QUEUE                      */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0565
*       CALL NPA5GLOD;              /* LOAD MODULES                  */
         B     @RC00563                                            0565
@RF00563 BAL   @14,NPA5GLOD                                        0565
*   END;                            /* LIST FINISHED ?               */
@RC00563 DS    0H                                                  0566
@DE00558 TM    LASTREQ,B'10000000'                                 0566
         BZ    @DL00558                                            0566
*   RETURN;                         /* YES - RETURN                  */
@EL00005 DS    0H                                                  0567
@EF00005 DS    0H                                                  0567
@ER00005 LM    @14,@12,@SA00005                                    0567
         BR    @14                                                 0567
*NP5LMQ02:                          /* LPA PACKING LIST NOT FOUND -
*                                      OUTPUT MESSAGE                */
*                                                                  0568
*   /*****************************************************************/
*   /*                                                               */
*   /* SWITCH TO V=R TCB FOR WTO                                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0568
*   GENERATE REFS(NVTREAL,RNVT) SETS(RENTRY,REXIT)(IEAPMNIP TYPE=REAL);
NP5LMQ02 IEAPMNIP TYPE=REAL
*                                   /* SWITCH TO V=R                 */
*                                                                  0569
*   /*****************************************************************/
*   /*                                                               */
*   /* IEA208I LPA PACKING FUNCTION INOPERATIVE                      */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0569
*   GENERATE REFS(RNVT,NVTWTO,MSG208I) SETS(RPARM,RENTRY,REXIT)    0569
*       (IEAPMNIP TYPE=WTO,PARAM=MSG208I);/* WTO MSG                 */
*                                                                  0569
         IEAPMNIP TYPE=WTO,PARAM=MSG208I
*   /*****************************************************************/
*   /*                                                               */
*   /* SWITCH TCB BACK TO V=V                                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0570
*   GENERATE REFS(NVTVIRT,RNVT) SETS(RENTRY,REXIT)(IEAPMNIP TYPE=VIRT);
         IEAPMNIP TYPE=VIRT
*                                   /* SWITCH TO V=V                 */
*   GOTO NP5LMQ03;                  /* INDICATE END OF LIST          */
         B     NP5LMQ03                                            0571
*   END NPA5BLMQ;                   /* END OF SUBROUTINE             */
@PB00005 DS    0H                                                  0573
*   GENERATE NOREFS NOSETS(EJECT);                                 0573
*                                                                  0573
         EJECT
*/********************************************************************/
*/*                                                                  */
*/*         SUBROUTINE NP5STRTQ                                      */
*/*                                                                  */
*/*         FUNCTION - LOCATE THE BEGINNING OF A LOAD MODULE         */
*/*            QUEUE SPECIFICATION AND INITIALIZE TO BUILD THE QUEUE */
*/*                                                                  */
*/*         INPUT - THE LPA PACKING LIST MEMBER OF SYS1.PARMLIB      */
*/*            THE ADDRESS OF THE NEXT CHARACTER TO EXAMINE IN THE   */
*/*            CURRENT INPUT RECORD                                  */
*/*                                                                  */
*/*         OUTPUT - LOAD MODULE QUEUE PROCESSING IS INITIALIZED     */
*/*                                                                  */
*/********************************************************************/
*                                                                  0574
*NP5STRTQ:                          /* NP5STRTQ ENTRY PT             */
*   PROC;                           /* BEGINNING OF SUBR             */
         B     @PB00006                                            0574
NP5STRTQ STM   @14,@12,@SA00006                                    0574
*   NA5CSGRP=0;                     /* ZERO QUEUE HEADER             */
         SLR   @08,@08                                             0575
         ST    @08,NA5CSGRP                                        0575
*   LASTMOD=0;                      /* INIT LAST MODULE POINTER      */
         ST    @08,LASTMOD                                         0576
*   ENDOFQUE='0'B;                  /* INITIALIZE END OF QUEUE     0577
*                                      INDICATOR                     */
*   STGRP='0'B;                     /* INDICATE START OF GROUP NOT 0578
*                                      FOUND YET                     */
         NI    ENDOFQUE,B'11011110'                                0578
*   IF SCAN=' ' THEN                /* TEST FOR END OF CURRENT INPUT
*                                      RECORD                        */
         L     @08,TEXTPTR                                         0579
         CLI   SCAN(@08),C' '                                      0579
         BNE   @RF00579                                            0579
*     DO;                           /* YES, MUST READ NEXT RECORD OF
*                                      PACK LIST                     */
*                                                                  0580
*       /*************************************************************/
*       /*                                                           */
*       /* READ THE NEXT RECORD FROM THE LPA PACKING LIST MEMBER OF  */
*       /* SYS1.PARMLIB                                              */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0581
*       GENERATE REFS(NVTREAL,RNVT) SETS(RENTRY,REXIT)             0581
*           (IEAPMNIP TYPE=REAL);   /* SWITCH TCB TO V=R             */
         IEAPMNIP TYPE=REAL
*       CALL NIPPMTXT;              /* READ A TEXT RECORD            */
         BAL   @14,NIPPMTXT                                        0582
*       BC(15,NP5STRT1);            /* RECORD READ OK                */
         BC    15,NP5STRT1                                         0583
*       BC(15,NP5STRT2);            /* RECORD NOT READ               */
         BC    15,NP5STRT2                                         0584
*NP5STRT1:                                                         0585
*       GENERATE REFS(NVTVIRT,RNVT) SETS(RENTRY,REXIT)             0585
*           (IEAPMNIP TYPE=VIRT);   /* SWITCH TCB TO V=V PROCESSING  */
NP5STRT1 IEAPMNIP TYPE=VIRT
*       TEXTPTR=RPARM;              /* POINT TO FIRST BYTE IN THE  0586
*                                      RECORD                        */
         ST    RPARM,TEXTPTR                                       0586
*     END;                                                         0587
*                                                                  0587
*   /*****************************************************************/
*   /*                                                               */
*   /* VALIDITY CHECK THE BEGINNING OF THE LIST                      */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0588
*   IF SCAN='(' THEN                /* TEST FOR START OF LIST      0588
*                                      SPECIFICATION                 */
@RF00579 L     @08,TEXTPTR                                         0588
         CLI   SCAN(@08),C'('                                      0588
         BNE   @RF00588                                            0588
*     DO;                           /* START OF GROUP                */
*       STGRP='1'B;                 /* INDICATE GROUP STRT           */
         OI    STGRP,B'00000001'                                   0590
*       TEXTPTR=TEXTPTR+1;          /* POINT PAST (                  */
         AH    @08,@CH00264                                        0591
         ST    @08,TEXTPTR                                         0591
*     END;                          /*                               */
*   ELSE                            /* NOT LEFT PAREN                */
*     IF SCAN=' 'SCAN='FF'X THEN   /* TEST FOR END OF PACKING LIST
*                                      MEMBER                        */
         B     @RC00588                                            0593
@RF00588 L     @08,TEXTPTR                                         0593
         CLI   SCAN(@08),C' '                                      0593
         BE    @RT00593                                            0593
         CLI   SCAN(@08),X'FF'                                     0593
         BNE   @RF00593                                            0593
@RT00593 DS    0H                                                  0594
*       ENDOFQUE='1'B;              /* END OF LIST                   */
         OI    ENDOFQUE,B'00100000'                                0594
*     ELSE                          /* NOT END OF LIST               */
*       DO;                         /* ATTEMPT RECOVERY              */
*                                                                  0595
         B     @RC00593                                            0595
@RF00593 DS    0H                                                  0596
*         /***********************************************************/
*         /*                                                         */
*         /* PROCESS INVALID DELIMITER IN LIST                       */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0596
*         CALL NP5PKERR(DELERR);    /* PROCESS ERROR                 */
         LA    @01,@AL00596                                        0596
         BAL   @14,NP5PKERR                                        0596
*         IF SCAN='(' THEN          /* WAS RECOVERY SUCCESSFUL       */
         L     @08,TEXTPTR                                         0597
         CLI   SCAN(@08),C'('                                      0597
         BNE   @RF00597                                            0597
*           DO;                     /* YES, CAN CONTINUE             */
*             STGRP='1'B;           /* INDICATE GROUP STRT           */
         OI    STGRP,B'00000001'                                   0599
*             TEXTPTR=TEXTPTR+1;    /* POINT PAST (                  */
         AH    @08,@CH00264                                        0600
         ST    @08,TEXTPTR                                         0600
*           END;                                                   0601
*       END;                                                       0602
*   RETURN;                         /* RETURN TO CALLER              */
@EL00006 DS    0H                                                  0603
@EF00006 DS    0H                                                  0603
@ER00006 LM    @14,@12,@SA00006                                    0603
         BR    @14                                                 0603
*NP5STRT2:                          /* I/O ERROR OCCURRED WHILE    0604
*                                      READING RECORD                */
*   CALL NP5PKERR(PKIOERR);         /* PROCESS ERROR                 */
NP5STRT2 LA    @01,@AL00604                                        0604
         BAL   @14,NP5PKERR                                        0604
*   RETURN;                         /* RETURN TO CALLER              */
         B     @EL00006                                            0605
*   GENERATE NOREFS NOSETS(EJECT);                                 0606
*                                                                  0606
         EJECT
*/********************************************************************/
*/*                                                                  */
*/*         SUBROUTINE NP5NXTMD                                      */
*/*                                                                  */
*/*         FUNCTION - PROCESS NEXT MODULE SPECIFICATION IN THE      */
*/*            LPA PACKING LIST                                      */
*/*                                                                  */
*/*         INPUT - LPA MODULE PACKING LIST MEMBER                   */
*/*            ADDRESS OF THE NEXT BYTE IN THE CURRENT INPUT RECORD  */
*/*            ADDRESS OF THE LAST MODULE ON THE QUEUE               */
*/*            ADDRESS OF THE START OF THE QUEUE                     */
*/*                                                                  */
*/*         OUTPUT - THE NEXT MODULE SPECIFIED IS ADDED TO THE QUEUE */
*/*                                                                  */
*/********************************************************************/
*                                                                  0607
*NP5NXTMD:                          /* NP5NXTMD ENTRY PT             */
*   ENTRY;                          /* BEGINNING OF SUBR             */
*                                                                  0607
         B     @EC00607                                            0607
NP5NXTMD STM   @14,@12,@SA00006                                    0607
@EC00607 DS    0H                                                  0608
*   /*****************************************************************/
*   /*                                                               */
*   /* IF THERE IS NO USEABLE DATA IN THE BUFFER, READ THE NEXT      */
*   /* RECORD FROM THE LPA PACKING LIST MEMBER                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0608
*   IF SCAN=' ' THEN                /* IS THERE ANY DATA             */
         L     @08,TEXTPTR                                         0608
         CLI   SCAN(@08),C' '                                      0608
         BNE   @RF00608                                            0608
*     DO;                           /* NO, READ A RECORD             */
*                                                                  0609
*       /*************************************************************/
*       /*                                                           */
*       /* READ THE NEXT RECORD FROM THE PARMLIB MEMBER              */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0610
*       GENERATE REFS(NVTREAL,RNVT) SETS(RENTRY,REXIT)             0610
*           (IEAPMNIP TYPE=REAL);   /* SWITCH TCB TO V=R             */
         IEAPMNIP TYPE=REAL
*       CALL NIPPMTXT;              /* READ A RECORD                 */
         BAL   @14,NIPPMTXT                                        0611
*       BC(15,NP5NXTM1);            /* RECORD READ OK                */
         BC    15,NP5NXTM1                                         0612
*       BC(15,NP5NXTM2);            /* ERROR ON READ                 */
         BC    15,NP5NXTM2                                         0613
*NP5NXTM1:                                                         0614
*       GENERATE REFS(NVTVIRT,RNVT) SETS(RENTRY,REXIT)             0614
*           (IEAPMNIP TYPE=VIRT);   /* SWITCH TCB TO V=V PROCESSING  */
NP5NXTM1 IEAPMNIP TYPE=VIRT
*       TEXTPTR=RPARM;              /* SAVE ADDRESS OF FIRST BYTE OF
*                                      RECORD                        */
         ST    RPARM,TEXTPTR                                       0615
*     END;                          /*                               */
*   MODULE='';                      /* BLANK OUT MODULE NAME         */
*                                                                  0617
@RF00608 MVI   MODULE,C' '                                         0617
         MVC   MODULE+1(7),MODULE                                  0617
*   /*****************************************************************/
*   /*                                                               */
*   /* SCAN THE INPUT RECORD FOR A DELIMITER                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0618
*   DO INDEX=1 TO 9                 /* 1-8 BYTE NAME VALID           */
*         WHILE(SCAN^=','&SCAN^=')'&SCAN^=' ');/* SCAN TILL DELIMITER
*                                      FOUND IN THE RECORD           */
         LA    @08,1                                               0618
         ST    @08,INDEX                                           0618
@DL00618 L     @15,TEXTPTR                                         0618
         CLI   SCAN(@15),C','                                      0618
         BE    @DC00618                                            0618
         CLI   SCAN(@15),C')'                                      0618
         BE    @DC00618                                            0618
         CLI   SCAN(@15),C' '                                      0618
         BE    @DC00618                                            0618
*     IF INDEX<=8 THEN              /* TEST FOR VALID NUMBER OF    0619
*                                      CHARACTERS                    */
         CH    @08,@CH00050                                        0619
         BH    @RF00619                                            0619
*       DO;                         /* VALID                         */
*         MODULE(INDEX)=SCAN;       /* COPY CHAR JUST EXAMINED       */
         LA    @14,MODULE-1(@08)                                   0621
         MVC   0(1,@14),SCAN(@15)                                  0621
*         TEXTPTR=TEXTPTR+1;        /* POINT TO NEXT CHARACTER       */
         AH    @15,@CH00264                                        0622
         ST    @15,TEXTPTR                                         0622
*       END;                        /* CONTINUE SCAN                 */
*     ELSE                          /* LONGER THAN 8 CHARACTERS      */
*       CALL NP5PKERR(PKNAMNG);     /* WRITE ERROR MSG               */
         B     @RC00619                                            0624
@RF00619 LA    @01,@AL00624                                        0624
         BAL   @14,NP5PKERR                                        0624
*   END;                            /* END OF SCAN LOOP - CONTINUE 0625
*                                      SCANNING UNTIL 8 BYTES HAVE 0625
*                                      BEEN COPIED OR A VALID      0625
*                                      DELIMITER HAS BEEN FOUND      */
@RC00619 LA    @08,1                                               0625
         AL    @08,INDEX                                           0625
         ST    @08,INDEX                                           0625
         CH    @08,@CH00827                                        0625
         BNH   @DL00618                                            0625
@DC00618 DS    0H                                                  0626
*   IF SCAN=' ' THEN                /* WAS THE DELIMITER A BLANK     */
         L     @08,TEXTPTR                                         0626
         CLI   SCAN(@08),C' '                                      0626
         BNE   @RF00626                                            0626
*     DO;                                                          0627
*       LASTMODN=MODULE;            /* SAVE NAME BEFORE BLANK        */
         MVC   LASTMODN(8),MODULE                                  0628
*       CALL NP5PKERR(DELNG);       /* YES, PROCESS THE DELIMTER   0629
*                                      ERROR                         */
         LA    @01,@AL00629                                        0629
         BAL   @14,NP5PKERR                                        0629
*     END;                                                         0630
*                                                                  0630
*   /*****************************************************************/
*   /*                                                               */
*   /* IF A MODULE NAME WAS SPECIFIED, ADD IT TO THE LOAD MODULE     */
*   /* QUEUE BEING BUILT                                             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0631
*   IF INDEX>1 THEN                 /* WAS A NAME SPECIFIED IN THE 0631
*                                      LIST                          */
@RF00626 L     @08,INDEX                                           0631
         CH    @08,@CH00264                                        0631
         BNH   @RF00631                                            0631
*     DO;                           /* YES, PROCESS IT               */
*       LASTMODN=MODULE;            /* SAVE VALID NAME               */
         MVC   LASTMODN(8),MODULE                                  0633
*       MODERR='0'B;                /* RESET ERROR FLAG              */
*                                                                  0634
         NI    MODERR,B'11101111'                                  0634
*       /*************************************************************/
*       /*                                                           */
*       /* FIND THE BLDL INFORMATION TABLE ENTRY FOR THE SPECIFIED   */
*       /* MODULE AND VALIDITY CHECK THE ENTRY                       */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0635
*       CALL NP5FINDE;              /* FIND ENTRY                    */
         BAL   @14,NP5FINDE                                        0635
*       IF MODERR='0'B THEN         /* WAS MODULE FOUND OK           */
         TM    MODERR,B'00010000'                                  0636
         BNZ   @RF00636                                            0636
*         DO;                       /* YES, ADD IT TO QUEUE          */
*           IF NA5CSGRP=0 THEN      /* IS THE QUEUE EMPTY            */
         ICM   @08,15,NA5CSGRP                                     0638
         BNZ   @RF00638                                            0638
*             NA5CSGRP=INFOPTR;     /* YES, START QUEUE              */
         MVC   NA5CSGRP(4),INFOPTR                                 0639
*           ELSE                    /* NO, QUEUE STARTED             */
*             LASTMOD->INFCHAIN=INFOPTR;/* ADD TO THE END OF THE   0640
*                                      QUEUE                         */
         B     @RC00638                                            0640
@RF00638 L     @08,LASTMOD                                         0640
         MVC   INFCHAIN(3,@08),INFOPTR+1                           0640
*           LASTMOD=INFOPTR;        /* UPDATE END OF QUEUE POINTER   */
@RC00638 MVC   LASTMOD(4),INFOPTR                                  0641
*         END;                                                     0642
*     END;                                                         0643
*                                                                  0643
@RF00636 DS    0H                                                  0644
*   /*****************************************************************/
*   /*                                                               */
*   /* PROCESS DELIMITER FOLLOWING NAME                              */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0644
*   IF SCAN=')' THEN                /* IS IT END OF LIST             */
@RF00631 L     @08,TEXTPTR                                         0644
         CLI   SCAN(@08),C')'                                      0644
         BNE   @RF00644                                            0644
*     DO;                           /* YES, END OF QUEUE             */
*       ENDOFQUE='1'B;              /* INDICATE END OF QUEUE         */
         OI    ENDOFQUE,B'00100000'                                0646
*       TEXTPTR=TEXTPTR+1;          /* POINT PAST RT PAREN           */
         LA    @15,1                                               0647
         ALR   @08,@15                                             0647
         ST    @08,TEXTPTR                                         0647
*       IF SCAN=',' THEN            /* IS THERE A COMMA              */
         CLI   SCAN(@08),C','                                      0648
         BNE   @RF00648                                            0648
*         TEXTPTR=TEXTPTR+1;        /* YES, POINT TO START OF THE  0649
*                                      NEXT LIST                     */
         ALR   @08,@15                                             0649
         ST    @08,TEXTPTR                                         0649
*       ELSE                        /* NO, MUST BE END               */
*         IF SCAN=' ' THEN          /* IS THERE A BLANK              */
         B     @RC00648                                            0650
@RF00648 L     @08,TEXTPTR                                         0650
         CLI   SCAN(@08),C' '                                      0650
         BNE   @RF00650                                            0650
*           SCAN='FF'X;             /* INDICATE END OF LIST          */
         MVI   SCAN(@08),X'FF'                                     0651
*         ELSE                      /* NO, INVALID DELIMITER         */
*           DO;                                                    0652
*                                                                  0652
         B     @RC00650                                            0652
@RF00650 DS    0H                                                  0653
*             /*******************************************************/
*             /*                                                     */
*             /* PROCESS INVALID DELIMITER                           */
*             /*                                                     */
*             /*******************************************************/
*                                                                  0653
*             CALL NP5PKERR(DELERR);/* PROCESS ERROR                 */
         LA    @01,@AL00653                                        0653
         BAL   @14,NP5PKERR                                        0653
*             ENDOFQUE='1'B;        /* INDICATE END OF QUEUE         */
         OI    ENDOFQUE,B'00100000'                                0654
*           END;                                                   0655
*     END;                                                         0656
*   ELSE                            /* NO, NOT END OF Q              */
*     IF SCAN^='FF'X THEN           /* TEST FOR END OF FILE DURING 0657
*                                      ERROR PROCESSING              */
         B     @RC00644                                            0657
@RF00644 L     @08,TEXTPTR                                         0657
         CLI   SCAN(@08),X'FF'                                     0657
         BE    @RF00657                                            0657
*       TEXTPTR=TEXTPTR+1;          /* POINT PAST COMMA              */
         AH    @08,@CH00264                                        0658
         ST    @08,TEXTPTR                                         0658
*   RETURN;                         /* RETURN TO CALLER              */
         B     @EL00006                                            0659
*NP5NXTM2:                          /* I/O ERROR WHILE READING     0660
*                                      PARMLIB RECORD                */
*                                                                  0660
*   /*****************************************************************/
*   /*                                                               */
*   /* PROCESS I/O ERROR ON PARMLIB                                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0660
*   CALL NP5PKERR(PKIOERR);         /* PROCESS ERROR                 */
NP5NXTM2 LA    @01,@AL00660                                        0660
         BAL   @14,NP5PKERR                                        0660
