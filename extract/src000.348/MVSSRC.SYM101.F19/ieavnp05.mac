*   DCL                                                            0443
*     SAVEADR FIXED;                /* SAVED WASTE ADDRESS           */
*   DCL                                                            0444
*     NPA5CRIT FIXED;               /* LOADING CRITERION VALUE       */
*   DCL                                                            0445
*     INDEX2 FIXED;                 /* 2ND TABLE INDEX               */
*   DCL                                                            0446
*     NPA5FQHD PTR(31) INIT(0);     /* HEADER OF QUEUE OF WASTED   0446
*                                      SPACE CONTROL BLOCKS          */
*   DCL                                                            0447
*     NPA5NWST PTR(31);             /* ADDRESS OF FIRST AVAILABLE  0447
*                                      BYTE IN WASTED SPACE CONTROL
*                                      BLOCK AREA                    */
*   DCL                                                            0448
*     NPA5LWST PTR(31);             /* ADDRESS OF END OF WASTED SPACE
*                                      CONTROL BLOCK AREA            */
*   DCL                                                            0449
*     WASTTBLS FIXED CONSTANT(8192);/* SIZE OF WASTED SPACE CONTROL
*                                      BLOCK AREA                    */
*   DCL                                                            0450
*     WASTPTR PTR(31);              /* ADDRESS OF CURRENT WASTED   0450
*                                      SPACE CONTROL BLOCK           */
*   DCL                                                            0451
*     1 WASTBLOK BASED(WASTPTR),    /* WASTED SPACE CONTROL BLOCK    */
*      2 WASTNEXT PTR(31),          /* ADDRESS OF NEXT WASTBLOK IN 0451
*                                      QUEUE                         */
*      2 WASTSIZE FIXED,            /* SIZE OF AREA REPRESENTED BY 0451
*                                      THIS BLOCK                    */
*      2 WASTAREA PTR(31);          /* ADDRESS OF AREA REPRESENTED BY
*                                      THIS BLOCK                    */
*   DCL                                                            0452
*     LASTPTR PTR(31);              /* POINTER TO PREVIOUS BLOCK ON
*                                      QUEUE                         */
*   DCL                                                            0453
*     CURRPTR PTR(31);              /* POINTER TO CURRENT BLOCK ON 0453
*                                      QUEUE                         */
*   DCL                                                            0454
*     1 * CHAR(1),                  /* FLAG BYTE                     */
*      2 LASTBLOK BIT(1),           /* LAST DIR BLOCK                */
*      2 LASTENT BIT(1),            /* LAST PDS ENTRY                */
*      2 LASTLMQ BIT(1),            /* LAST MODULE QUEUE             */
*      2 GLOAD BIT(1),              /* GROUP LOAD SPEC               */
*      2 * BIT(4);                  /* UNUSED                        */
*                                                                  0455
*/* UPON ENTRY IEAVNP05 TESTS THE NIP VECTOR TABLE TO DETERMINE      */
*/* WHETHER A 'QUICK START' OF THE LPA HAS BEEN REQUESTED. IF NVTFLQS*/
*/* IS SET, IEAVNP05 ENTERS THE NPA5INIT ROUTINE, IF NVTFLQS IS NOT  */
*/* SET, NP5CSLPA IS ENTERED TO EFFECT A 'COLD START' CREATION OF    */
*/* THE LPA.                                                         */
*                                                                  0455
*   RESPECIFY                                                      0455
*     NVT BASED(RNVT);                                             0455
*   RESPECIFY                                                      0456
*    (RNVT) RESTRICTED;             /* NVT ADDRESS                   */
*   RESPECIFY                                                      0457
*    (RCVT) RESTRICTED;             /* CVT ADDRESS                   */
*   RESPECIFY                                                      0458
*    (RPARM) RESTRICTED;            /* R1 PARAMETER REG              */
*   IEAQLPA=IEAQLPA&&IEAQLPA;       /* CLEAR ACTIVE QUEUE POINTER    */
*                                                                  0459
         L     @08,CVTPTR                                          0459
         L     @08,CVTQLPAQ(,@08)                                  0459
         L     @15,IEAQLPA(,@08)                                   0459
         LR    @00,@15                                             0459
         XR    @00,@15                                             0459
         ST    @00,IEAQLPA(,@08)                                   0459
*   /*****************************************************************/
*   /*                                                               */
*   /* SEE IF QUICK START OPTION WAS SPECIFIED                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0460
*   IF NVTFLQS^='1'B THEN                                          0460
         TM    NVTFLQS(RNVT),B'00001000'                           0460
         BO    @RF00460                                            0460
*     DO;                                                          0461
*       CALL NP5CSLPA;              /* COLD START LPA                */
         BAL   @14,NP5CSLPA                                        0462
*       GO TO NP5COMMN;             /* START COMMON PROCESSING       */
         B     NP5COMMN                                            0463
*     END;                                                         0464
*   ELSE                                                           0465
*     ;                                                            0465
@RF00460 DS    0H                                                  0466
*   CALL NPA5INIT;                  /* SETTUP V=V AREA               */
         BAL   @14,NPA5INIT                                        0466
*NP5COMMN:                                                         0467
*   CALL NP5FIX;                    /* MODIFY LPA - FIX              */
NP5COMMN BAL   @14,NP5FIX                                          0467
*   CALL NP5BLDLF;                  /* DEFINE FIXED BLDL TABLE       */
         BAL   @14,NP5BLDLF                                        0468
*   CALL NP5MLPA;                   /* MODIFY LPA - MLPA             */
         BAL   @14,NP5MLPA                                         0469
*   CALL NP5BLDLP;                  /* DEFINE PAGED BLDL TABLE       */
         BAL   @14,NP5BLDLP                                        0470
*   CALL NP5LPDIR;                  /* SET UP LPA DIRECTORY          */
         BAL   @14,NP5LPDIR                                        0471
*   CALL IEAVNPS5;                  /* INIT SVC TABLE FOR 3/4 SVC AND
*                                      ESR TABLE FOR 3/4 SVC @Y30HPYC*/
         L     @15,@CV00148                                        0472
         BALR  @14,@15                                             0472
*   CALL NP5LPLOD;                  /* PROCESS IEALOD00              */
         BAL   @14,NP5LPLOD                                        0473
*   CALL NP5BRENT;                  /* INIT SYSTEM BRANCH ENTRIES    */
         BAL   @14,NP5BRENT                                        0474
*   CALL IEAVNPA5;                  /* BUILD APF TABLE               */
         L     @15,@CV00147                                        0475
         BALR  @14,@15                                             0475
*   CALL NPA5TERM;                  /* RELEASE BLDL SAVE IN SQA      */
         BAL   @14,NPA5TERM                                        0476
*   RETURN;                         /* RETURN TO IEAVNIPM            */
@EL00001 L     @13,4(,@13)                                         0477
@EF00001 DS    0H                                                  0477
@ER00001 LM    @14,@12,12(@13)                                     0477
         BR    @14                                                 0477
*   GENERATE NOREFS NOSETS(EJECT);                                 0478
*                                                                  0478
         EJECT
*   /*****************************************************************/
*   /*                                                               */
*   /* LPA COLD START ROUTINE                                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0479
*NP5CSLPA:                                                         0479
*   PROC;                                                          0479
         B     @PB00002                                            0479
NP5CSLPA STM   @14,@12,@SA00002                                    0479
*   CALL NP5LPLIB;                  /* OPEN SYS1.LPALIB              */
*                                                                  0480
         BAL   @14,NP5LPLIB                                        0480
*   /*****************************************************************/
*   /*                                                               */
*   /* SEE IF OPEN WAS SUCCESSFUL                                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0481
*   IF LPLIBRC=OPNOSUC4 THEN        /* OPEN WAS NOT SUCCESSFUL       */
         CLI   LPLIBRC,X'04'                                       0481
         BNE   @RF00481                                            0481
*     CALL NPA5CERR(OPNUNSUC);      /* WRITE ERROR MSG               */
         LA    @01,@AL00482                                        0482
         BAL   @14,NPA5CERR                                        0482
*   ELSE                                                           0483
*     ;                                                            0483
*                                                                  0483
@RF00481 DS    0H                                                  0484
*   /*****************************************************************/
*   /*                                                               */
*   /* GETMAIN SP252 FOR PLPA                                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0484
*   CALL NPA5INIT;                  /* SETTUP FOR V=V REGION         */
         BAL   @14,NPA5INIT                                        0484
*   LASTREQ='0'B;                   /* INITIALIZE FLAG               */
*   PAKFOUND='0'B;                  /* INDICATE PACK LIST NOT YET  0486
*                                      LOCATED                       */
         NI    LASTREQ,B'00111111'                                 0486
*   NXTPGOUT=NVTLVIRT-PGCRIT;       /* EST. START ADDRESS FOR FIRST
*                                      PGOUT OF LPA                  */
         L     @08,NVTLVIRT(,RNVT)                                 0487
         LR    @00,@08                                             0487
         SL    @00,PGCRIT                                          0487
         ST    @00,NXTPGOUT                                        0487
*   LSTPGOUT=NVTLVIRT;              /* EST. HIGH ADDR FOR PGOUT      */
*                                                                  0488
         ST    @08,LSTPGOUT                                        0488
*   /*****************************************************************/
*   /*                                                               */
*   /* BUILD BLDL INFORMATION TABLE FOR LPA MODULES                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0489
*   CALL NPA5CLIN;                  /* BUILD TABLE                   */
*                                                                  0489
         BAL   @14,NPA5CLIN                                        0489
*   /*****************************************************************/
*   /*                                                               */
*   /* LOAD MODULES SPECIFIED IN THE LPA PACK LIST                   */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0490
*   CALL NPA5BLMQ;                  /* LOAD MODULE GROUPS            */
*                                                                  0490
         BAL   @14,NPA5BLMQ                                        0490
*   /*****************************************************************/
*   /*                                                               */
*   /* LOAD MODULES NOT SPECIFIED IN THE LPA PACK LIST               */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0491
*   CALL NPA5LIND;                  /* LOAD INDEPENDENT MODULES      */
*                                                                  0491
         BAL   @14,NPA5LIND                                        0491
*   /*****************************************************************/
*   /*                                                               */
*   /* BUILD ALIAS ENTRIES FOR ALL MODULE ALIASES                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0492
*   CALL NPA5ALIS;                  /* PROCESS ALIASES               */
*                                                                  0492
         BAL   @14,NPA5ALIS                                        0492
*   /*****************************************************************/
*   /*                                                               */
*   /* BUILD LINK PACK AREA DIRECTORY                                */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0493
*   CALL NPA5BDIR;                  /* BUILD DIRECTORY               */
         BAL   @14,NPA5BDIR                                        0493
*   END NP5CSLPA;                   /* END OF SUBROUTINE             */
@EL00002 DS    0H                                                  0494
@EF00002 DS    0H                                                  0494
@ER00002 LM    @14,@12,@SA00002                                    0494
         BR    @14                                                 0494
@PB00002 DS    0H                                                  0495
*   GENERATE NOREFS NOSETS(EJECT);                                 0495
         EJECT
*NPA5INIT:                                                         0496
*   PROCEDURE;                      /* IEAVNP05 INIT ROUTINE         */
         B     @PB00003                                            0496
NPA5INIT STM   @14,@12,@SA00003                                    0496
*   GENERATE REFS(NVTVIRT,RNVT) SETS(RENTRY,REXIT)(IEAPMNIP TYPE=VIRT);
         IEAPMNIP TYPE=VIRT
*                                   /* SWITCH TCB TO V=V             */
*                                                                  0498
*   /*****************************************************************/
*   /*                                                               */
*   /* IEAVNP05 ALLOCATES ENTIRE REGION AS SP252 SPACE               */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0498
*   GENERATE REFS(LOWADDR,APARM) SETS(RPARM,RCODE)                 0498
*       (GETMAIN VC,LA=LOWADDR,A=APARM,SP=252);                    0498
*                                                                  0498
         GETMAIN VC,LA=LOWADDR,A=APARM,SP=252
*   /*****************************************************************/
*   /*                                                               */
*   /* IEAVNP05 MAY NOW USE FREEMAIN TO CONTROL START ADDR FOR LPA   */
*   /* LOADING GET 64 BYTES FOR BLDL ENTRY SAVE AREA - SP245         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0499
*   GENERATE NOREFS SETS(RPARM0,RPARM)(GETMAIN R,LV=BLDLSAVL,SP=SP245);
         GETMAIN R,LV=BLDLSAVL,SP=SP245
*   BLDLSAV=RPARM;                  /* BLDL ENTRY SAVE AREA ADDR     */
         ST    RPARM,BLDLSAV                                       0500
*   BLDLLL=LENGTH(BLDLPDS);         /* LENGTH OF BLDL ENTRY          */
         L     @08,BLDLSAV                                         0501
         MVC   BLDLLL(2,@08),@CH00635                              0501
*   BLDLFF=1;                       /* NUMBER OF BLDL ENTRIES        */
         MVC   BLDLFF(2,@08),@CH00264                              0502
*   RETURN;                         /* RETURN TO CALLER              */
@EL00003 DS    0H                                                  0503
@EF00003 DS    0H                                                  0503
@ER00003 LM    @14,@12,@SA00003                                    0503
         BR    @14                                                 0503
*   GENERATE NOREFS NOSETS(EJECT);                                 0504
         EJECT
*NPA5TERM:                                                         0505
*   ENTRY;                          /* IEAVNP05 TERM ROUTINE         */
         B     @EC00505                                            0505
NPA5TERM STM   @14,@12,@SA00003                                    0505
@EC00505 DS    0H                                                  0506
*   RESPECIFY                                                      0506
*    (RENTRY) RESTRICTED;                                          0506
*                                                                  0506
*   /*****************************************************************/
*   /*                                                               */
*   /* FREE BLDL ENTRY SAVE AREA                                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0507
*   GENERATE REFS(BLDLSAV) SETS(RPARM)                             0507
*       (FREEMAIN E,LV=BLDLSAVL,A=BLDLSAV,SP=SP245);               0507
*                                                                  0507
         FREEMAIN E,LV=BLDLSAVL,A=BLDLSAV,SP=SP245
*   /*****************************************************************/
*   /*                                                               */
*   /* INSURE TCB IS V=R MODE BEFORE RETURN                          */
