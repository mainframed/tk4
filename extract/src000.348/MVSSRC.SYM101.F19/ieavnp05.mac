*/*                                                                  */
*/*         SUBROUTINE NP5BRENT                                      */
*/*                                                                  */
*/*         FUNCTION - INITIALIZES SYSTEM BRANCH ENTRY POINTS FOR    */
*/*                    FETCH AND SPIE IN THE CVT.                    */
*/*                                                                  */
*/*         INPUT - (1) IEAQLPA - PTR TO ACTIVE LPA.                 */
*/*                 (2) CVTLPDSR - BRANCH ENTRY TO LPDE SEARCH RTN.  */
*/*                 (3) INITCVT - TABLE OF BRANCH ENTRY NAMES AND    */
*/*                               CORRESPONDING OFFSET IN CVT TO     */
*/*                               INSERT ENTRY ADDRESS.              */
*/*         OUTPUT - BRANCH ENTRY POINTS DEFINED IN INITCVT INSERTED */
*/*                  IN CORRECT CVT FIELDS.                          */
*/*                                                                  */
*/********************************************************************/
*                                                                  2386
*NP5BRENT:                                                         2386
*   PROC OPTIONS(NOSAVEAREA);                                      2386
         B     @PB00034                                            2386
NP5BRENT STM   @14,@12,12(@13)                                     2386
*   RESPECIFY                                                      2387
*    (RI) RESTRICTED;                                              2387
*   DO RI=DIM(INITCVT) TO 1 BY-1;   /* LOOP THRU INITCVT TAB         */
         LA    RI,5                                                2388
@DL02388 DS    0H                                                  2389
*     NP5ENTFD='0'B;                /* SET MODULE NOT FOUND    SWITCH*/
         NI    NP5ENTFD,B'11111011'                                2389
*     MODULE=EPNAME(RI);            /* GET BRANCH ENT NAME FRM TAB   */
*                                                                  2390
         LR    @14,RI                                              2390
         MH    @14,@CH00075                                        2390
         LA    @08,EPNAME-12(@14)                                  2390
         MVC   MODULE(8),0(@08)                                    2390
*     /***************************************************************/
*     /*                                                             */
*     /* SEARCH THE ACTIVE LPA FOR THE BRANCH ENTRY NAME.            */
*     /*                                                             */
*     /***************************************************************/
*                                                                  2391
*     RESPECIFY                                                    2391
*      (CDENTPTR,                                                  2391
*       PTRCVTEN) RESTRICTED;                                      2391
*     CDENTPTR=IEAQLPA;             /* SET PTR TO ACTIVE QUEUE       */
         L     @14,CVTQLPAQ(,RCVT)                                 2392
         L     CDENTPTR,IEAQLPA(,@14)                              2392
*     DO WHILE(CDENTPTR^=0&NP5ENTFD='0'B);                         2393
         B     @DE02393                                            2393
@DL02393 DS    0H                                                  2394
*       IF CDNAME=MODULE THEN       /* IS THIS CORRECT CDE ?         */
         CLC   CDNAME(8,CDENTPTR),MODULE                           2394
         BNE   @RF02394                                            2394
*         DO;                       /* YES - GET ENTRY POINT.        */
*           PTRCVTEN=OFSETCVT(RI)+RCVT;/* COMPUTE OFFST IN CVT TO  2396
*                                      PLACE E.P. ADDR               */
         LR    @14,RI                                              2396
         MH    @14,@CH00075                                        2396
         LR    PTRCVTEN,RCVT                                       2396
         AL    PTRCVTEN,OFSETCVT-12(@14)                           2396
*           PTRCVTEN->BRENTCVT=CDENTPT;/* INIT CVT FIELD.            */
         MVC   BRENTCVT(4,PTRCVTEN),CDENTPT(CDENTPTR)              2397
*           NP5ENTFD='1'B;          /* INDICATE MODULE FOUND         */
         OI    NP5ENTFD,B'00000100'                                2398
*         END;                                                     2399
*       ELSE                                                       2400
*         CDENTPTR=CDCHAIN;         /* GET NEXT CDE.                 */
         B     @RC02394                                            2400
@RF02394 L     CDENTPTR,CDCHAIN(,CDENTPTR)                         2400
*     END;                          /* END OF CDE SEARCH LOOP        */
@RC02394 DS    0H                                                  2401
@DE02393 LTR   CDENTPTR,CDENTPTR                                   2401
         BZ    @DC02393                                            2401
         TM    NP5ENTFD,B'00000100'                                2401
         BZ    @DL02393                                            2401
@DC02393 DS    0H                                                  2402
*     RESPECIFY                                                    2402
*      (CDENTPTR) UNRESTRICTED;                                    2402
*     RESPECIFY                                                    2403
*      (RPARM0,                                                    2403
*       RPARM) RESTRICTED;                                         2403
*                                                                  2403
*     /***************************************************************/
*     /*                                                             */
*     /* SEARCH THE LPA DIRECTORY VIA IEAVVMSR FOR MODNAME           */
*     /*                                                             */
*     /***************************************************************/
*                                                                  2404
*     DO WHILE NP5ENTFD='0'B;       /* IF NOT FOUND IN ACTIVE LPA  2404
*                                      SEARCH LPDE'S                 */
         B     @DE02404                                            2404
@DL02404 DS    0H                                                  2405
*       RPARM0=MOD1;                /* SET LEFT HALF OF NAME IN R0   */
         L     RPARM0,MOD1                                         2405
*       RPARM=MOD2;                 /* SET RIGHT HALF OF NME IN R1   */
         L     RPARM,MOD2                                          2406
*       RESPECIFY                                                  2407
*        (REXIT,                                                   2407
*         RENTRY) RESTRICTED;                                      2407
*       RENTRY=CVTLPDSR;            /* GET IEAVVMSR ENTRY ADDRESS    */
         L     RENTRY,CVTLPDSR(,RCVT)                              2408
*       GENERATE REFS(RPARM0,RPARM,RCVT,RENTRY) SETS(RPARM0,NP5R7,NP5R8
*           ,RBASE1,REXIT) FLOWS(NP5SVCFD,NP5NOTFD);               2409
          BALR  REXIT,RENTRY
          B     NP5SVCFD
          B     NP5NOTFD
*NP5SVCFD:                                                         2410
*       RESPECIFY                                                  2410
*        (REXIT,                                                   2410
*         RENTRY) UNRESTRICTED;                                    2410
NP5SVCFD DS    0H                                                  2411
*       RESPECIFY                                                  2411
*        (LPDEPTR) RESTRICTED;                                     2411
*       LPDEPTR=RPARM0;             /* LPDE ADDRESS                  */
         LR    LPDEPTR,RPARM0                                      2412
*       RESPECIFY                                                  2413
*        (RPARM0) UNRESTRICTED;                                    2413
*       PTRCVTEN=OFSETCVT(RI)+RCVT; /* COMPUTE OFFSET IN CVT TO PLACE
*                                      E.P. ADDRESS                  */
         LR    @14,RI                                              2414
         MH    @14,@CH00075                                        2414
         LR    PTRCVTEN,RCVT                                       2414
         AL    PTRCVTEN,OFSETCVT-12(@14)                           2414
*       PTRCVTEN->BRENTCVT=LPDENTP; /* MOVE EP ADDR TO CVT           */
         MVC   BRENTCVT(4,PTRCVTEN),LPDENTP(LPDEPTR)               2415
*       NP5ENTFD='1'B;              /* INDICATE FOUND CONDITION      */
         OI    NP5ENTFD,B'00000100'                                2416
*     END;                          /* END OF LPDE SEARCH            */
@DE02404 TM    NP5ENTFD,B'00000100'                                2417
         BZ    @DL02404                                            2417
*   END;                            /* END OF INITCVT LOOP           */
         BCTR  RI,0                                                2418
         LTR   RI,RI                                               2418
         BP    @DL02388                                            2418
*   RESPECIFY                                                      2419
*    (RI,                                                          2419
*     LPDEPTR,                                                     2419
*     PTRCVTEN) UNRESTRICTED;                                      2419
*   RETURN;                         /* PROCESSING COMPLETE-RETURN    */
@EL00034 DS    0H                                                  2420
@EF00034 DS    0H                                                  2420
@ER00034 LM    @14,@12,12(@13)                                     2420
         BR    @14                                                 2420
*NP5NOTFD:                                                         2421
*   MSGFI1=MODULE;                  /* MOVE MODULE NAME TO ERR MSG   */
*                                                                  2421
NP5NOTFD MVI   MSGFI1+8,C' '                                       2421
         MVC   MSGFI1(8),MODULE                                    2421
*   /*****************************************************************/
*   /*                                                               */
*   /* ISSUE MSG IEA363I MOD NOT FOUND IN LPA.                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  2422
*   GENERATE REFS(RNVT,NVTWTO,MSGFS,MSGFI1) SETS(RPARM,REXIT,RENTRY)
*       (IEAPMNIP TYPE=WTO,PARAM=MSGFS);/* MSG IEA363I               */
         IEAPMNIP TYPE=WTO,PARAM=MSGFS
*   NVTFLWSC='3B'X;                 /* SET WAIT STATE CODE IN NVT    */
         MVI   NVTFLWSC(RNVT),X'3B'                                2423
*   GENERATE REFS(RNVT,NVTSWAIT,NVTFLWSC) SETS(REXIT,RENTRY)       2424
*       (IEAPMNIP TYPE=SWAIT);      /* SYSTEM WAIT CODE- 3B          */
         IEAPMNIP TYPE=SWAIT
*   END NP5BRENT;                   /* END OF SUBROUTINE             */
         B     @EL00034                                            2425
@PB00034 DS    0H                                                  2426
*   GENERATE NOREFS NOSETS(EJECT);                                 2426
         EJECT
*NP5PSPIO:                                                         2427
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);/* DUMMY PROCEDURE               */
         B     @PB00035                                            2427
NP5PSPIO DS    0H                                                  2428
