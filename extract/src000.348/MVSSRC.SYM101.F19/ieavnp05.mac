*   DCL                                                            0437
*     NPA5BPTR PTR(31) BASED;       /* BASED POINTER                 */
*   DCL                                                            0438
*     1 NPA5WLST(12),               /* WASTED SPACE TABLE            */
*      2 NPA5WSIZ FIXED INIT(0,64,128,256,512,1024,1536,2048,2560,3072,
*            3584,4096),            /* SIZE OF EACH AREA IN THE QUEUE*/
*      2 NPA5WQNE PTR(31) INIT((DIM(NPA5WLST))0);/* ANY BOUNDARY   0438
*                                      QUEUES                        */
*   DCL                                                            0439
