         EJECT
*/********************************************************************/
*/*                                                                  */
*/*         SUBROUTINE NPA5CLPD                                      */
*/*                                                                  */
*/*         FUNCTION - BUILD AN LPDE IN PLACE OF A BLDL INFORMATION  */
*/*            TABLE ENTRY FOR THE MODULE JUST LOADED, AND RELEASE   */
*/*            THE CDE, LLE, AND EXTENT LIST FOR THE MODULE          */
*/*                                                                  */
*/*         INPUT - THE JOB PACK QUEUE ON THE V=V TCB                */
*/*                                                                  */
*/*         OUTPUT - THE LPDE IS BUILT IN PLACE OF THE INFO ENTRY    */
*/*                                                                  */
*/********************************************************************/
*                                                                  1278
*NPA5CLPD:                          /* NPA5CLPD ENTRY PT             */
*   PROCEDURE;                      /* BEGINNING OF SUBR             */
         B     @PB00015                                            1278
NPA5CLPD STM   @14,@12,@SA00015                                    1278
*   RESPECIFY                                                      1279
*    (CDENTPTR,                                                    1279
*     XTLSTPTR,                                                    1279
*     LLEPTR,                                                      1279
*     RPARM0,                                                      1279
*     RPARM,                                                       1279
*     LPDEPTR) RESTRICTED;                                         1279
*                                                                  1279
*   /*****************************************************************/
*   /*                                                               */
*   /* BUILD THE LPDE                                                */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  1280
*   CDENTPTR=NVTVVTCB->TCBJPQB;     /* PICK UP CDE ADR               */
         L     @14,NVTVVTCB(,RNVT)                                 1280
         SLR   CDENTPTR,CDENTPTR                                   1280
         ICM   CDENTPTR,7,TCBJPQB(@14)                             1280
*   NVTVVTCB->TCBJPQB=0;            /* CLEAR JPQ POINTER             */
         SLR   @05,@05                                             1281
         STCM  @05,7,TCBJPQB(@14)                                  1281
*   LPDEPTR=ADDR(NPA5LPDE);         /* LPDE BUILD AREA               */
         LA    LPDEPTR,NPA5LPDE                                    1282
*   LPDE=LPDE&&LPDE;                /* CLEAR LPDE AREA               */
         XC    LPDE(40,LPDEPTR),LPDE(LPDEPTR)                      1283
*   CDJPA='0'B;                     /* CLEAR JPA BIT                 */
         NI    CDJPA(CDENTPTR),B'11111101'                         1284
*   LPDEATTR=CDATTR;                /* COPY COMMON FLAGS             */
         MVC   LPDEATTR(1,LPDEPTR),CDATTR(CDENTPTR)                1285
*   LPDENTP=CDENTPT;                /* ENTRY POINT ADDRESS           */
         MVC   LPDENTP(4,LPDEPTR),CDENTPT(CDENTPTR)                1286
*   LPDEATT2=CDATTR2;               /* MORE COMMON FLAGS             */
         MVC   LPDEATT2(1,LPDEPTR),CDATTR2(CDENTPTR)               1287
*   LPDENAME=CDNAME;                /* MODULE NAME                   */
         MVC   LPDENAME(8,LPDEPTR),CDNAME(CDENTPTR)                1288
*   LPDENIP='1'B;                   /* MOD LOADED BY NIP             */
         OI    LPDENIP(LPDEPTR),B'10000000'                        1289
*   XTLSTPTR=CDXLMJP;               /* POINT TO XTLST                */
         L     XTLSTPTR,CDXLMJP(,CDENTPTR)                         1290
*   LPDEXTLN=XTLMSBLA;              /* MODULE LENGTH                 */
         MVC   LPDEXTLN(4,LPDEPTR),XTLMSBLA(XTLSTPTR)              1291
*   LPDEXTAD=XTLMSBAA;              /* MODULE LOAD ADDR              */
         MVC   LPDEXTAD(4,LPDEPTR),XTLMSBAA(XTLSTPTR)              1292
*   LPDECHN=INFSYNM;                /* COPY SYNONYM PTR              */
         L     @14,INFOPTR                                         1293
         SLR   @05,@05                                             1293
         ICM   @05,7,INFSYNM(@14)                                  1293
         ST    @05,LPDECHN(,LPDEPTR)                               1293
*   INFODATA=LPDE;                  /* REPLACE INFO TABLE ENTRY WITH
*                                      LPDE                          */
*                                                                  1294
         L     @14,INFOPTR                                         1294
         MVC   INFODATA(40,@14),LPDE(LPDEPTR)                      1294
*   /*****************************************************************/
*   /*                                                               */
*   /* FREE THE CONTENTS SUPERVISION CONTROL BLOCKS                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  1295
*   RPARM0=XLSIZEP;                 /* XTLST FM PARM                 */
         L     RPARM0,XLSIZEP                                      1295
*   GENERATE REFS(RPARM0,XTLSTPTR) SETS(RPARM)                     1296
*       (FREEMAIN R,LV=(0),A=(XTLSTPTR));/* FREE XL                  */
         FREEMAIN R,LV=(0),A=(XTLSTPTR)
*   RPARM0=CDESIZEP;                /* CDE FM PARM                   */
         L     RPARM0,CDESIZEP                                     1297
*   GENERATE REFS(RPARM0,CDENTPTR) SETS(RPARM)                     1298
*       (FREEMAIN R,LV=(0),A=(CDENTPTR));/* FREE CDE                 */
         FREEMAIN R,LV=(0),A=(CDENTPTR)
*   LLEPTR=NVTVVTCB->TCBLLS;        /* POINT TO LLE                  */
         L     @14,NVTVVTCB(,RNVT)                                 1299
         L     LLEPTR,TCBLLS(,@14)                                 1299
*   NVTVVTCB->TCBLLS=0;             /* CLEAR LLE QUEUE               */
         SLR   @05,@05                                             1300
         ST    @05,TCBLLS(,@14)                                    1300
*   RPARM0=LLESIZEP;                /* LLE FM PARM                   */
         L     RPARM0,LLESIZEP                                     1301
*   GENERATE REFS(RPARM0,LLEPTR) SETS(RPARM)                       1302
*       (FREEMAIN R,LV=(0),A=(LLEPTR));/* FREE LLE                   */
         FREEMAIN R,LV=(0),A=(LLEPTR)
*   RETURN;                         /* RETURN TO CALLER              */
@EL00015 DS    0H                                                  1303
@EF00015 DS    0H                                                  1303
@ER00015 LM    @14,@12,@SA00015                                    1303
         BR    @14                                                 1303
*   RESPECIFY                                                      1304
*    (CDENTPTR,                                                    1304
*     XTLSTPTR,                                                    1304
*     LLEPTR,                                                      1304
*     RPARM0,                                                      1304
*     LPDEPTR) UNRESTRICTED;                                       1304
*   END NPA5CLPD;                   /* END OF SUBROUTINE             */
@PB00015 DS    0H                                                  1306
