*/*                                                                  */
*/*         SUBROUTINE NPA5LODM                                      */
*/*                                                                  */
*/*         FUNCTION - LOAD A MODULE INTO THE LINK PACK AREA AND     */
*/*            CONVERT THE BLDL INFO ENTRY TO AN LPDE                */
*/*                                                                  */
*/*         INPUT - MODULE SIZE (FMSIZE), MODULE LOAD ADDRESS        */
*/*            (RLDADDR), ADDRESS OF SYS1.LPALIB DCB (DCBPTR),       */
*/*            BLDL ENTRY (BLDLPDS), SIZE TO BE RECLAIMED (SIZESAVE) */
*/*                                                                  */
*/*         OUTPUT - MODULE LOADED, LPDE IN PLACE OF INFO ENTRY      */
*/*                                                                  */
*/********************************************************************/
*                                                                  1197
*NPA5LODM:                          /* NPA5LODM ENTRY PT             */
*   PROCEDURE;                      /* BEGINNING OF SUBR             */
         B     @PB00012                                            1197
NPA5LODM STM   @14,@12,@SA00012                                    1197
*   RESPECIFY                                                      1198
*    (RPARM0,                                                      1198
*     DCBPTR1,                                                     1198
*     RLDADDR) RESTRICTED;                                         1198
*   FMSIZESP=SPFC;                  /* APPEND SUBPOOL ID TO SIZE OF
*                                      MODULE                        */
         MVI   FMSIZESP,X'FC'                                      1199
*   RPARM0=FMSIZE;                  /* SET UP REG 0                  */
         L     RPARM0,FMSIZE                                       1200
*   GENERATE REFS(RPARM0,RLDADDR) SETS(RPARM)                      1201
*       (FREEMAIN R,LV=(0),A=(RLDADDR));/* FREE SPACE                */
         FREEMAIN R,LV=(0),A=(RLDADDR)
*   DCBPTR1=DCBPTR;                 /* SET DCB PTR                   */
         L     DCBPTR1,DCBPTR                                      1202
*   RPARM=ADDR(BLDLPDS);            /* SET UP REG 1                  */
         L     RPARM,BLDLSAV                                       1203
         LA    RPARM,BLDLPDS(,RPARM)                               1203
*   GENERATE REFS(RPARM,DCBPTR1) SETS(RPARM0,RPARM)                1204
*       (LOAD  DE=(RPARM),DCB=(DCBPTR1));/* LOAD MODULE              */
         LOAD  DE=(RPARM),DCB=(DCBPTR1)
*   IF SIZESAVE>0 THEN              /* MUST SPACE BE RECLAIMED       */
         L     @08,SIZESAVE                                        1205
         LTR   @08,@08                                             1205
         BNP   @RF01205                                            1205
*     DO;                           /* YES, GET IT BACK              */
*       FMSIZE=SIZESAVE;            /* SET UP GM SIZE                */
         ST    @08,FMSIZE                                          1207
*       FMSIZESP=SPFC;              /* PUT IN SUBPOOL ID             */
         MVI   FMSIZESP,X'FC'                                      1208
*       RPARM0=FMSIZE;              /* SET UP REG 0                  */
         L     RPARM0,FMSIZE                                       1209
*       GENERATE REFS(RPARM0) SETS(RPARM)(GETMAIN R,LV=(0));/* GET 1210
*                                      BACK SPACE                    */
         GETMAIN R,LV=(0)
*     END;                                                         1211
*   RESPECIFY                                                      1212
*    (PGOUTST,                                                     1212
*     PGOUTEND) RESTRICTED;                                        1212
*                                                                  1212
@RF01205 DS    0H                                                  1213
*   /*****************************************************************/
*   /*                                                               */
*   /* NEXT INSTRUCTIONS DETERMINE IF THE PLPA SHOULD BE PAGED OUT.  */
*   /* TO CREATE OVERLAP BETWEEN PAGING AND LOADING IT IS DONE EVERY */
*   /* 18000 HEX BYTES.                                              */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  1213
*   IF NVTLVIRT<NXTPGOUT THEN                                      1213
         L     @14,NXTPGOUT                                        1213
         C     @14,NVTLVIRT(,RNVT)                                 1213
         BNH   @RF01213                                            1213
*     DO;                           /* 18000 HEX BYTES SINCE LAST  1214
*                                      PGOUT - PAGE OUT THIS AREA    */
*       PGOUTST=NXTPGOUT;           /* PGOUT START (LOW) ADDRESS     */
         LR    PGOUTST,@14                                         1215
*       PGOUTEND=LSTPGOUT;          /* PGOUT END (HIGH) ADDRESS      */
         L     PGOUTEND,LSTPGOUT                                   1216
*       LSTPGOUT=PGOUTST;           /* SETTUP HIGH ADDRESS FOR NEXT
*                                      PGOUT                         */
         ST    PGOUTST,LSTPGOUT                                    1217
*       NXTPGOUT=PGOUTST-PGCRIT;    /* SETTUP LOW ADDRESS OF NEXT  1218
*                                      PGOUT                         */
         LR    @14,PGOUTST                                         1218
         SL    @14,PGCRIT                                          1218
         ST    @14,NXTPGOUT                                        1218
*       GENERATE REFS(PGOUTST,PGOUTEND) SETS(RENTRY)               1219
*           (PGOUT R,A=(1),EA=(15));/* R1=START R15=END              */
         PGOUT R,A=(1),EA=(15)
*       IF RENTRY^=0 THEN           /* WAS THERE A PAGING ERROR      */
         LTR   RENTRY,RENTRY                                       1220
         BZ    @RF01220                                            1220
*         CALL NPA5CERR(PGOUTERR);  /* YES - LOAD WAIT CODE          */
         LA    @01,@AL01221                                        1221
         BAL   @14,NPA5CERR                                        1221
*       ELSE                                                       1222
*         ;                                                        1222
@RF01220 DS    0H                                                  1223
*       CALL NP5WTPAG;              /* WAIT FOR PAGE I/O TO COMP     */
         BAL   @14,NP5WTPAG                                        1223
*     END;                                                         1224
*                                                                  1224
*   /*****************************************************************/
*   /*                                                               */
*   /* CONVERT BLDL INFORMATION TABLE ENTRY TO LPDE                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  1225
*   CALL NPA5CLPD;                  /* CONVERT TO LPDE               */
@RF01213 BAL   @14,NPA5CLPD                                        1225
*   RETURN;                         /* RETURN TO CALLER              */
@EL00012 DS    0H                                                  1226
@EF00012 DS    0H                                                  1226
@ER00012 LM    @14,@12,@SA00012                                    1226
         BR    @14                                                 1226
*   RESPECIFY                                                      1227
*    (RPARM0,                                                      1227
*     DCBPTR1) UNRESTRICTED;                                       1227
*   END NPA5LODM;                   /* END OF SUBROUTINE             */
@PB00012 DS    0H                                                  1229
*   GENERATE NOREFS NOSETS(EJECT);                                 1229
         EJECT
*NP5WTPAG:                          /* WAIT FOR PGOUT TO COMPLTE     */
*   PROC;                                                          1230
         B     @PB00013                                            1230
NP5WTPAG STM   @14,@12,@SA00013                                    1230
*   RESPECIFY                                                      1231
*    (SRBPTR,                                                      1231
*     XSRBPTR,                                                     1231
*     ASMVTPTR,                                                    1231
*     RPARM0,                                                      1231
*     RPARM) RESTRICTED;                                           1231
*   ASMVTPTR=CVTASMVT;              /* GET PTR TO ASMVT              */
         L     @14,CVTPTR                                          1232
         L     ASMVTPTR,CVTASMVT(,@14)                             1232
*   RPARM0=NIPSRB;                  /* SET SP AND LEN FOR SRB        */
         L     RPARM0,NIPSRB                                       1233
*   GENERATE REFS(RPARM0) SETS(RPARM)(GETMAIN R,LV=(RPARM0));/* GET
*                                      AREA FOR SRB SP245            */
         GETMAIN R,LV=(RPARM0)
*   XSRBPTR=RPARM+LENGTH(SRBSECT);  /* SET PTR TO SRB EXT.           */
         LA    XSRBPTR,44                                          1235
         ALR   XSRBPTR,RPARM                                       1235
*   SRBPTR=RPARM;                   /* SET PTR TO SRB                */
         LR    SRBPTR,RPARM                                        1236
*   RESPECIFY                                                      1237
*    (RPARM0,                                                      1237
*     RPARM) UNRESTRICTED;                                         1237
*   SRB=SRB&&SRB;                   /* CLEAR SRB                     */
         XC    SRB(44,SRBPTR),SRB(SRBPTR)                          1238
*   XSRBEXT=XSRBEXT&&XSRBEXT;       /* CLEAR SRB EXTENTION           */
*                                                                  1239
         XC    XSRBEXT(72,XSRBPTR),XSRBEXT(XSRBPTR)                1239
*   /*****************************************************************/
*   /*                                                               */
*   /* INIT SRB                                                      */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  1240
*   SRBPARM=XSRBPTR;                /* SET R1 FOR ENTRY TO SRB       */
         ST    XSRBPTR,SRBPARM(,SRBPTR)                            1240
*   SRBASCB=NVTMASCB;               /* MASTER'S ASCB         @Z40WPYD*/
         MVC   SRBASCB(4,SRBPTR),NVTMASCB(RNVT)                    1241
*   SRBEP=ADDR(NIPSRBEP);           /* SRB ENTRY POINT IN NIP05      */
         LA    @14,NIPSRBEP                                        1242
         ST    @14,SRBEP(,SRBPTR)                                  1242
*NP5RESCH:                                                         1243
*   XECBXMP=0;                      /* CLEAR ECB FOR XMPOST          */
NP5RESCH SLR   @14,@14                                             1243
         ST    @14,XECBXMP(,XSRBPTR)                               1243
*   STM(RPARM0,RENTRY,XSAVEREG);    /* SAVE REG FOR SRB              */
         STM   RPARM0,RENTRY,XSAVEREG(XSRBPTR)                     1244
*   RESPECIFY                                                      1245
*    (RPARM) RESTRICTED;                                           1245
*   RPARM=SRBPTR;                   /* SET PTR TO SRB PARMS          */
         LR    RPARM,SRBPTR                                        1246
*   RESPECIFY                                                      1247
*     CVTMAP BASED(CVTPTR);         /* SCHED USES LOC 16             */
*   GENERATE REFS(CVTPTR,CVTGSMQ,CVTLSMQ,SRBFLNK,SRBSECT);         1248
            SCHEDULE SRB=(1),SCOPE=LOCAL  SCHED SRB TO ASM
*   RESPECIFY                                                      1249
*     CVTMAP BASED(RCVT);           /* BACK TO R3 FOR CVT BASE       */
*   RPARM=ADDR(XECBXMP);            /* SET ADDR OF ECB FOR WAIT      */
         LA    RPARM,XECBXMP(,XSRBPTR)                             1250
*   GENERATE REFS(RPARM) SETS(RPARM0,RPARM);                       1251
              WAIT ECB=(RPARM)         WAIT FOR XMPOST FRM SRB
*   IF ASMIORQR^=ASMIORQC THEN      /* HAS ALL PAGE I/O COMPLETD     */
         CLC   ASMIORQR(4,ASMVTPTR),ASMIORQC(ASMVTPTR)             1252
         BNE   @RT01252                                            1252
*     GOTO NP5RESCH;                /* NO - RESCHEDULE SRB           */
*   ELSE                                                           1254
*     ;                             /* YES - CONTINUE LOADING        */
*   RESPECIFY                                                      1255
*    (RPARM) UNRESTRICTED;                                         1255
*   RESPECIFY                                                      1256
*    (RPARM0) RESTRICTED;                                          1256
*   RPARM0=NIPSRB;                  /* SP AND SIZE FOR FREEMAIN      */
         L     RPARM0,NIPSRB                                       1257
*   GENERATE REFS(RPARM0,SRBPTR) SETS(RPARM);                      1258
              FREEMAIN R,LV=(0),A=(SRBPTR)   FREE SRB AREA SP245
*   RESPECIFY                                                      1259
*    (RPARM0,                                                      1259
*     SRBPTR,                                                      1259
*     XSRBPTR,                                                     1259
*     ASMVTPTR,                                                    1259
*     RPARM) UNRESTRICTED;                                         1259
*   RETURN;                         /* RETURN TO CALLER              */
@EL00013 DS    0H                                                  1260
@EF00013 DS    0H                                                  1260
@ER00013 LM    @14,@12,@SA00013                                    1260
         BR    @14                                                 1260
*NP5XMPER:                                                         1261
*   CALL NPA5CERR(XMPSTERR);        /* TERMINATE COLD START          */
NP5XMPER LA    @01,@AL01261                                        1261
         BAL   @14,NPA5CERR                                        1261
*   END NP5WTPAG;                   /* END OF SUBROUTINE             */
         B     @EL00013                                            1262
@PB00013 DS    0H                                                  1263
*   GENERATE NOREFS NOSETS(EJECT);                                 1263
         EJECT
*NIPSRBEP:                          /* SRB TO ALLOW ASM TO CONTNU  1264
*                                      PAGE I/O WHILE NIP05 TESTS  1264
*                                      ASMVT FOR COMPLETION.         */
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1264
         B     @PB00014                                            1264
NIPSRBEP DS    0H                                                  1265
*   RESPECIFY                                                      1265
*    (XSRBPTR,                                                     1265
*     RENTRY,                                                      1265
*     REXIT) RESTRICTED;                                           1265
*   XSRBPTR=RPARM;                  /* REG1 PASSED BY NIP05          */
         LR    XSRBPTR,RPARM                                       1266
*   XSAVE14=REXIT;                  /* SAVE CALLERS RETURN ADDR      */
         ST    REXIT,XSAVE14(,XSRBPTR)                             1267
*   GENERATE REFS(PSALITA,FLC) SETS(REG11,REG12,REG13,REG14);      1268
               SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,                  X
               RELATED=(ASMVT,IEAVNP05(NIPSRBEP))
*   LM(RPARM0,RENTRY,XSAVEREG);     /* RESTORE NIP05 REGS            */
         LM    RPARM0,RENTRY,XSAVEREG(XSRBPTR)                     1269
*   RENTRY=CVT0PT01;                /* BR ENTRY FOR XMPOST           */
         L     RENTRY,CVT0PT01(,RCVT)                              1270
*   GENERATE REFS(XECBXMP,XSRBEXT,XSRBPTR,NVTMASCB,NVT,RNVT,NP5XMPER)
*       SETS(REG1,REG10,REG11,REG12,REG13,REXIT,RENTRY);           1271
*                                                                  1271
            LA  REG12,NP5XMPER          SET ERET ROUTINE ADDR
            LA  REG11,XECBXMP-XSRBEXT(,XSRBPTR) PTR TO ECB FOR XMPOST
            LA  REG1,1                  SETUP TO TURN ON SIGN BIT
            SLL REG1,31                 TURN ON SIGN BIT
            OR  REG11,REG1              INDICATE XMPOST
            SR  REG10,REG10             ZERO COMPLETION CODE
            L   REG13,NVTMASCB-NVT(,RNVT) PTR TO MASTER ASCB
            BALR REXIT,RENTRY           BRANCH ENTER XMPOST
*   /*****************************************************************/
*   /*                                                               */
*   /* FREE THE LOCAL LOCK                                           */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  1272
*   GENERATE REFS(PSALITA,FLC) SETS(REG11,REG12,REG13,REG14);      1272
              SETLOCK RELEASE,TYPE=LOCAL,                              X
               RELATED=(ASMVT,IEAVNP05(NIPSRBEP))
*   REXIT=XSAVE14;                  /* RESTORE CALLERS RET ADDR      */
         L     REXIT,XSAVE14(,XSRBPTR)                             1273
*   GENERATE;                                                      1274
            BR  REXIT                   RETURN TO CALLER
*   RESPECIFY                                                      1275
*    (XSRBPTR,                                                     1275
*     RENTRY,                                                      1275
*     REXIT) UNRESTRICTED;                                         1275
*   END NIPSRBEP;                   /* END OF SUBROUTINE             */
@EL00014 DS    0H                                                  1276
@EF00014 DS    0H                                                  1276
@ER00014 BR    @14                                                 1276
@PB00014 DS    0H                                                  1277
