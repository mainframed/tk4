*                                      EFFECT AT CHKPT TIME.)        */
         MVC   SIOTDEVT(4,@12),DSDRUTYP(IOBUFAD)                   1161
*       SIOUCNVT=0;                 /* TELL HSKP'ING THAT CONVERSION
*                                      HAS ALREADY BEEN DONE         */
