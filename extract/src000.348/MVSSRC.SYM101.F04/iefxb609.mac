SIOTPROC STM   @14,@12,@SA00013                                    1051
*/*************************************************************      */
*/* THIS PROCEDURE PERFORMS THE NECESSARY UPDATES TO THE SIOT        */
*/*    AFTER A MATCH TO THE DSDR HAS BEEN MADE.  THIS PROC IS        */
*/*    CALLED BY MERGMAIN.  THE SVA OF THE SIOT TO BE UPDATED        */
*/*    IS THE VARIABLE PROCSIOT PRODUCED BY THE PROC SIOTSCAN.       */
*/*************************************************************      */
*                                                                  1052
*   SWVA=PROCSIOT;                  /* SVA FOR READ                  */
         L     @12,SWEPAPTR                                        1052
         MVC   SWVA(3,@12),PROCSIOT+1                              1052
*   CALL READSWA;                   /* GET SIOT VIA SWAMGR           */
         BAL   @14,READSWA                                         1053
*   SIOTPTR=SWBLKPTR;               /* ADDRESSABILITY SIOT           */
         L     @12,SWEPAPTR                                        1054
         L     @12,SWBLKPTR(,@12)                                  1054
         ST    @12,SIOTPTR                                         1054
*   IF RESTYPE=ON THEN              /* IF DEFERRED, TEST DDNAME FOR
*                                      AUTO AFTER DEFERRED SETUP   1055
*                                                            @YM08167*/
         TM    RESTYPE,B'10000000'                                 1055
         BNO   @RF01055                                            1055
*     IF SCTDDNAM=SAVEDD THEN       /* IS THE ENTRY FOR CKPT DS ?    */
         CLC   SCTDDNAM(8,@12),SAVEDD                              1056
         BNE   @RF01056                                            1056
*       JCTCKTTR=SCTPJFCB;          /* STORE JFCB ADDR               */
         L     @11,JCTPTR                                          1057
         MVC   JCTCKTTR(3,@11),SCTPJFCB(@12)                       1057
*   SWVA=SCTPJFCB;                  /* SET UP FOR JFCB READ          */
@RF01056 DS    0H                                                  1058
@RF01055 L     @12,SWEPAPTR                                        1058
         L     @11,SIOTPTR                                         1058
         MVC   SWVA(3,@12),SCTPJFCB(@11)                           1058
*   CALL READSWA;                   /* READ MATCHING JFCB            */
         BAL   @14,READSWA                                         1059
*   JFCBPTR=SWBLKPTR;               /* ADDRESSABILITY FOR JFCB       */
         L     @12,SWEPAPTR                                        1060
         L     @12,SWBLKPTR(,@12)                                  1060
         ST    @12,JFCBPTR                                         1060
*   LALOCCTR=LALOCCTR+1;            /* KEEP ACCURATE COUNT OF DD'S   */
         LA    @12,1                                               1061
         AH    @12,LALOCCTR                                        1061
         STH   @12,LALOCCTR                                        1061
*   SCTDDINO=LALOCCTR;              /* INSURE INCREMENTAL,ASCENDING
*                                      INTERNAL DD NUMBERS.          */
         L     @11,SIOTPTR                                         1062
         STH   @12,SCTDDINO(,@11)                                  1062
*   DJFCBPTR=ADDR(DSDRJFCB);        /* ADDRABILITY FOR DSDR JFCB     */
