         BAL   @14,UNCATLOG                                        1931
*                 IF(NEWPTR->SIOTSSDS^=ON/* IF NOT SUBSYSTEM @YM08190*/
*                     &NEWPTR->SIOUCBAD->UCB3DACC=ON) THEN/* & UCB 1932
*                                      SAYS DA               @YM08190*/
@RF01930 L     @12,NEWPTR-1                                        1932
         LA    @12,0(,@12)                                         1932
         TM    SIOTSSDS(@12),B'10000000'                           1932
         BO    @RF01932                                            1932
         L     @12,SIOUCBAD-1(,@12)                                1932
         TM    UCB3DACC(@12),B'00100000'                           1932
         BNO   @RF01932                                            1932
*                   CALL SCRATCH(NEWPTR);                          1933
         LA    @12,NEWPTR                                          1933
         ST    @12,@AL00001                                        1933
         LA    @01,@AL00001                                        1933
         BAL   @14,SCRATCH                                         1933
*               END;                                               1934
@RF01932 DS    0H                                                  1935
*           END;                                                   1935
@RF01928 DS    0H                                                  1935
@DE01923 TM    TEMPSW,B'10000000'                                  1935
         BO    @DL01923                                            1935
