FREECHEK STM   @14,@12,@SA00028                                    1940
*/**************************************************************     */
*/* FREECHEK WILL DYNAMICALLY UNALLOCATE THE CHECKPOINT DATASET      */
*/*     IT WILL USE THE SAME PARAMETER LIST THAT WAS USED TO         */
*/*     ALLOCATE IT.                                                 */
*/**************************************************************     */
*                                                                  1941
*   DO;                             /* MODESET SAVEKEY(SAVEKEYA)   1941
*                                      KEYADDR(ZEROPTR)              */
*     RESPECIFY                                                    1942
*      (GPR02F) RESTRICTED;                                        1942
*     IPK;                          /* SAVE CURRENT PSW KEY          */
         IPK                                                       1943
*     SAVEKEYA=GPR02F;                                             1944
         STC   GPR02F,SAVEKEYA                                     1944
*     SPKA(ZEROPTR);                /* SET PSW KEY                   */
         SLR   @12,@12                                             1945
         SPKA  0(@12)                                              1945
*     RESPECIFY                                                    1946
*      (GPR02F) UNRESTRICTED;                                      1946
*   END;                            /* MODESET SAVEKEY(SAVEKEYA)   1947
*                                      KEYADDR(ZEROPTR) KEY = 0      */
*   S99VERB=S99VRBUN;               /* PARM VERB = UNALLOCATE        */
         LA    @12,EDBRBPTR                                        1948
         L     @12,S99RBPTR(,@12)                                  1948
         MVI   S99VERB(@12),X'02'                                  1948
*   TUPLAST(1)=ON;                  /* DDNAME IS ALL THAT IS       1949
*                                      NECESSARY FOR AN UNALLOC SO 1949
*                                      USE ONLY ONE TEXT UNIT.       */
         OI    TUPLAST,B'10000000'                                 1949
*   SAVEJSCB=JSCBACT;               /* FORCE DYNALLOC TO USE INIT'S  */
         L     @12,TCBPTR                                          1950
         L     @12,TCBJSCB(,@12)                                   1950
         L     @12,JSCBACT(,@12)                                   1950
