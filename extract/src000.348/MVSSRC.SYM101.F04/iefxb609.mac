SCTXPROC STM   @14,@12,@SA00020                                    1513
*/*************************************************************      */
*/* THE SCTX, WHICH NORMALLY CONTAINS PARAMETER INFORMATION FROM     */
*/* THE EXEC CARD IS USED TO PASS THE RESTART PARAMETER LIST TO      */
*/* THE RESTART SVC.  A 'NOTE' IS DONE ON THE CKPT DS, AND THE       */
*/* 'NOTED' TTR IS PLACED IN THE PARM LIST.  THE REST OF THE         */
*/* LIST CONSISTS OF THE CHR INFORMATION LEFT BY THE CHECKPOINT      */
*/* SVC. IF THERE IS A AN OLD SCTX, IT WILL BE OVERLAYED.            */
*/*************************************************************      */
*                                                                  1514
*   DO;                             /* MODESET SAVEKEY(SAVEKEYA)   1514
*                                      KEYADDR(ZEROPTR)              */
*     RESPECIFY                                                    1515
*      (GPR02F) RESTRICTED;                                        1515
*     IPK;                          /* SAVE CURRENT PSW KEY          */
         IPK                                                       1516
*     SAVEKEYA=GPR02F;                                             1517
         STC   GPR02F,SAVEKEYA                                     1517
*     SPKA(ZEROPTR);                /* SET PSW KEY                   */
         SLR   @12,@12                                             1518
         SPKA  0(@12)                                              1518
*     RESPECIFY                                                    1519
*      (GPR02F) UNRESTRICTED;                                      1519
*   END;                            /* MODESET SAVEKEY(SAVEKEYA)   1520
*                                      KEYADDR(ZEROPTR) KEY = 0      */
*   CALL READCHEK;                  /* READ THE CIR FOR NOTE @YM04748*/
         BAL   @14,READCHEK                                        1521
*   R1=DCBPARM;                     /* SET R1 PTR TO DCB FOR NOTE    */
         L     R1,DCBPARM                                          1522
*   GEN(NOTE (1));                  /* GET TTR OR BLOCKNO FOR RESTART*/
         NOTE (1)
*   NOTEPARM=R1;                    /* PUT INTO RESTART PARAMETER  1524
*                                      LIST                          */
         ST    R1,NOTEPARM                                         1524
*/**************************************                             */
*/* XB609 IS NOW THROUGH WITH CKPT DS                                */
*/**************************************                             */
*                                                                  1525
*   SAVEJSCB=JSCBACT;               /* SAVE JSCBACT FIELD    @YM01981*/
         L     @12,TCBPTR                                          1525
         L     @12,TCBJSCB(,@12)                                   1525
         L     @12,JSCBACT(,@12)                                   1525
         ST    @12,SAVEJSCB                                        1525
*   JSCBACT=TCBJSCB;                /* FORCE CLOSE TO INIT JSCB      */
         L     @12,TCBPTR                                          1526
         L     @12,TCBJSCB(,@12)                                   1526
         ST    @12,JSCBACT(,@12)                                   1526
*   DO;                             /* POST (ECB610)                 */
*     RESPECIFY                                                    1528
*      (GPR00F,                                                    1528
*       GPR01P) RESTRICTED;                                        1528
*     GPR00F=0;                     /* POST CODE IN R0               */
         SLR   GPR00F,GPR00F                                       1529
*     GPR01P=ADDR(ECB610);          /* ECB ADDRESS                   */
         LA    GPR01P,ECB610                                       1530
*     SVC(2);                       /* ISSUE POST SVC                */
         SVC   2                                                   1531
*     RESPECIFY                                                    1532
*      (GPR00F,                                                    1532
*       GPR01P) UNRESTRICTED;                                      1532
*   END;                            /* POST (ECB610) POST 610 TO   1533
*                                      CLOSE AND TERM                */
*/*************************************************                  */
*/*NOW WAIT FOR XB610 TO COMPLETE THE CLOSE...                       */
*/* OTHERWISE 609 WILL TERM AND FREE THE DCB                         */
*/************************************************                   */
*                                                                  1534
*   HIBITC1=ON;                     /* SETUP WAIT ECBLIST END        */
         OI    HIBITC1,B'10000000'                                 1534
*   DO;                             /* WAIT ECBLIST(ADECBC1)         */
*     RESPECIFY                                                    1536
*      (GPR00P,                                                    1536
*       GPR01P) RESTRICTED;                                        1536
*     GPR00P=1;                     /* LOAD WAIT COUNT               */
         LA    GPR00P,1                                            1537
*     GPR01P=-ADDR(ADECBC1);        /* ADDRESS OF LIST OF ECB'S      */
         LA    GPR01P,ADECBC1                                      1538
         LCR   GPR01P,GPR01P                                       1538
*     SVC(1);                       /* WAIT SVC                      */
         SVC   1                                                   1539
*     RESPECIFY                                                    1540
*      (GPR00P,                                                    1540
*       GPR01P) UNRESTRICTED;                                      1540
*   END;                            /* WAIT ECBLIST(ADECBC1) ISSUE 1541
*                                      WAIT FOR CLOSE COMPLETE       */
*   JSCBACT=SAVEJSCB;               /* RESTORE PP JSCB       @YM01981*/
         L     @12,TCBPTR                                          1542
         L     @12,TCBJSCB(,@12)                                   1542
         L     @11,SAVEJSCB                                        1542
         ST    @11,JSCBACT(,@12)                                   1542
*   DO;                             /* MODESET KEYADDR(SAVEKEYA)     */
*     SPKA(SAVEKEYA);               /* SET PSW KEY                   */
         SLR   @12,@12                                             1544
         IC    @12,SAVEKEYA                                        1544
         SPKA  0(@12)                                              1544
*   END;                            /* MODESET KEYADDR(SAVEKEYA) KEY
*                                      BACK TO SCHED                 */
