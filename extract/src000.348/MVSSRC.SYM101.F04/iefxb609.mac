         ST    @12,JFCBXPTR                                        1461
*       END;                                                       1462
*     PREV=ADDR(JFCBXTTR);          /* RE-SET BACK-LINK PTR          */
@RC01440 L     @12,JFCBXPTR                                        1463
         STCM  @12,7,PREV                                          1463
*     IF(DJFCXPTR->JFCBXTTR=0       /* IF DSDR HAS NO JFCBX          */
