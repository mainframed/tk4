*             TEMPPTR=TEMPPTR->SCTPSIOT;/* PICK UP NEW 'NEXT' PTR    */
@RF01858 L     @12,TEMPPTR-1                                       1860
         SLR   @11,@11                                             1860
         ICM   @11,7,SCTPSIOT(@12)                                 1860
         STCM  @11,7,TEMPPTR                                       1860
*           END;                    /* CLOSE SEARCH LOOP             */
@DE01854 TM    TEMPSW,B'10000000'                                  1861
         BO    @DL01854                                            1861
