*/**************************************************************     */
*/* THIS PROCEDURE HANDLES ANY SIOTS THAT REMAIN ON THE CHAIN        */
*/*   AFTER ALL THE DSDRS HAVE BEEN PROCESSED. FOR DEFERRED          */
*/*   RESTARTS, THESE EXTRAS REPRESENT ADDED DD ENTRIES.  THEY       */
*/*   WILL BE LEFT ON THE CHAIN, AND THE NUMBER OF DD'S UPDATED      */
*/*   TO REFLECT THEM. FOR AUTO REATARTS, THEY MAY REPRESENT         */
*/*   EITHER GDG ALL GENERATIONS (IN WHICH CASE THEY ARE DELETED     */
*/*   FROM THE CHAIN) OR DYNAMIC ALLOCATIONS AFTER THE CHKPT         */
*/*   WAS TAKEN.  THESE DYNALLOCS WILL BE TESTED TO DETERMINE IF     */
*/*   THEY WERE NEW.  IF NEW,THEY ARE UNCATALOGED AND SCRATCHED      */
*/* SO THE USER MAY RE-ALLOCATE THEM.                                */
*/**************************************************************     */
*                                                                  1898
*   DCL                                                            1898
*     1 NULLSIOT CHAR(70),          /* DUMMY SIOT FOR SPECIAL CASE 1898
*                                                            @YM03604*/
*      2 * CHAR(28),                                               1898
*      2 NULPSIOT PTR(24) BDY(WORD),                               1898
*      2 * CHAR(39);                                               1898
*   IF PROCSIOT=ZEROPTR THEN                                       1899
         L     @12,PROCSIOT                                        1899
         LTR   @12,@12                                             1899
         BNZ   @RF01899                                            1899
*     DO;                           /* ALL SIOTS NOT PROCESSED     1900
*                                                            @YM03604*/
*       PROCSIOT=ADDR(NULLSIOT)-LENGTH(SWAPFX);/* TO PICK UP 1ST SIOT
*                                      WHEN LOOP STARTS      @YM03604*/
         LA    @12,NULLSIOT                                        1901
         SL    @12,@CF00052                                        1901
         ST    @12,PROCSIOT                                        1901
*       NULPSIOT=SCTFSIOT;          /* IF NONE OF THE SIOTS HAVE BEEN
*                                      PROCESSED, START LOOKING FROM
*                                      THE 1ST.              @YM03604*/
         L     @12,SCTPTR                                          1902
         MVC   NULPSIOT(3),SCTFSIOT(@12)                           1902
*     END;                          /* CLOSE SPECIAL DO LOOP @YM03604*/
*   IF RESTYPE=ON THEN              /* THIS IS DEFERRED...           */
@RF01899 TM    RESTYPE,B'10000000'                                 1904
         BNO   @RF01904                                            1904
*     DO;                           /* ADD THE LEFTOVERS             */
*       IF PROCSIOT^=SCTLSIOT THEN  /* ARE THERE ANY LEFTOVERS?      */
         L     @12,PROCSIOT                                        1906
         L     @11,SCTPTR                                          1906
         SLR   @09,@09                                             1906
         ICM   @09,7,SCTLSIOT(@11)                                 1906
         CR    @12,@09                                             1906
         BE    @RF01906                                            1906
*         DO;                       /* YES                           */
*           TEMPSW=ON;              /* LOOP CONTROL                  */
         OI    TEMPSW,B'10000000'                                  1908
*           DO WHILE TEMPSW=ON;                                    1909
         B     @DE01909                                            1909
@DL01909 DS    0H                                                  1910
*             PROCSIOT=(PROCSIOT+REALADD)->SCTPSIOT;/* PICK UP     1910
*                                      NEXTSIOT                      */
         L     @12,REALADD-1                                       1910
         LA    @12,0(,@12)                                         1910
         AL    @12,PROCSIOT                                        1910
         SLR   @11,@11                                             1910
         ICM   @11,7,SCTPSIOT(@12)                                 1910
         ST    @11,PROCSIOT                                        1910
*             IF PROCSIOT=SCTLSIOT THEN/* IF THIS IS LAST SIOT,      */
         L     @12,SCTPTR                                          1911
         SLR   @09,@09                                             1911
         ICM   @09,7,SCTLSIOT(@12)                                 1911
         CR    @11,@09                                             1911
         BNE   @RF01911                                            1911
*               TEMPSW=NOTON;       /* CLOSE THE LOOP                */
         NI    TEMPSW,B'01111111'                                  1912
*             LALOCCTR=LALOCCTR+ONEHEX;/* BUMP TOTAL NUM DD'S        */
@RF01911 LA    @12,1                                               1913
         AH    @12,LALOCCTR                                        1913
         STH   @12,LALOCCTR                                        1913
*             EVAL(PROCSIOT+REALADD)->SCTDDINO=/* ASSIGN NEW DDNUMBER*/
*                 LALOCCTR;                                        1914
         L     @11,REALADD-1                                       1914
         LA    @11,0(,@11)                                         1914
         AL    @11,PROCSIOT                                        1914
         STH   @12,SCTDDINO(,@11)                                  1914
*           END;                    /* CLOSE ADD-ON LOOP             */
@DE01909 TM    TEMPSW,B'10000000'                                  1915
         BO    @DL01909                                            1915
*         END;                      /* CLOSE SEARCH                  */
*     END;                          /* CLOSE DEFERRED SECTION        */
*   ELSE                                                           1918
*     DO;                           /* THIS IS AUTOMATIC... DELETE 1918
*                                      THE LEFTOVERS                 */
         B     @RC01904                                            1918
@RF01904 DS    0H                                                  1919
*       SAVELAST=PROCSIOT;          /* RETAIN ADDR OF LAST PROCESSED
*                                      SIOT. IT BECOMES SCTLSIOT WHEN
*                                      OTHER ARE PROCESSED OUT.      */
         L     @12,PROCSIOT                                        1919
