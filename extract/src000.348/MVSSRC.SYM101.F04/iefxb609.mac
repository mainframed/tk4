         BAL   @14,UTYPPROC                                        1072
*   IF(SCTVREF=ON                   /* ONLY LEAVE VOL REF VALID IF   */
*       &SCTVOLAF=ON                /* REQUESTED AND NO VOLUMES ARE  */
*       &(DJFCBPTR->JFCBVOLS(1)=FOXES/*                     @YM08115*/
*       DJFCBPTR->JFCBVOLS(1)=BLANKS))/* SHOWN IN THE JFCB (OR FFTTR
*                                      EXISTS)                       */
*     THEN                                                         1073
@RF01071 L     @12,SIOTPTR                                         1073
         TM    SCTVREF(@12),B'00010000'                            1073
         BNO   @RF01073                                            1073
         TM    SCTVOLAF(@12),B'00100000'                           1073
         BNO   @RF01073                                            1073
         L     @12,DJFCBPTR                                        1073
         CLI   JFCBVOLS(@12),X'FF'                                 1073
         BE    @RT01073                                            1073
         L     @12,DJFCBPTR                                        1073
         CLI   JFCBVOLS(@12),C' '                                  1073
         BNE   @RF01073                                            1073
@RT01073 DS    0H                                                  1074
*     SCTDSNRF=NOTON;                                              1074
