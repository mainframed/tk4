*                 DO;                                              0403
*                   CKPTSIOT=ZEROPTR;/* INDICATE NO CKPT SIOT        */
         SLR   @12,@12                                             0404
         ST    @12,CKPTSIOT                                        0404
*                   TTRSW=ON;       /* STOP THE LOOP                 */
         OI    TTRSW,B'10000000'                                   0405
*                   IF EVAL(JCTCKTTR+REALADD)->JFCVRDS=ON THEN     0406
