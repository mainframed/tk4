*       SWVA=SCTANSCT;              /* GET P/P SCT FOR ALOCAT      0421
*                                                            @YM06575*/
         L     @11,SCTPTR                                          0421
         MVC   SWVA(3,@12),SCTANSCT(@11)                           0421
*       CALL READSWA;               /* READ IT               @YM06575*/
         BAL   @14,READSWA                                         0422
*       SCTPTR=SWBLKPTR;            /* ADDRESSABILITY SCT    @YM06575*/
         L     @12,SWEPAPTR                                        0423
