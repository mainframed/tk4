         SLR   @11,@11                                             0406
         ICM   @11,7,JCTCKTTR(@12)                                 0406
         L     @12,REALADD-1                                       0406
         LA    @12,0(,@12)                                         0406
         ALR   @11,@12                                             0406
         TM    JFCVRDS(@11),B'00000100'                            0406
         BNO   @RF00406                                            0406
*                     CALL VAMERR;  /* IF VAM WAS UNALLOCATED, NO UCB
