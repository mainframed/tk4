SIOTSCAN STM   @14,@12,@SA00012                                    0977
*/**************************************************************     */
*/* THIS PROC SCANS THE SIOT CHAIN FOR A MATCH ON THE DDNAME         */
*/*   FOUND IN THE DSDR.  THE CHAIN IS SEARCHED IN SUCH A WAY        */
*/*   THAT ALREADY MATCHED AND PROCESSED SIOTS REMAIN AT THE         */
*/*   UPPER PORTION OF THE CHAIN, AND SIOTS THAT DO NOT MATCH        */
*/*   THE CURRENT SEARCH ARGUMENT ARE BUBBLED-DOWN TO THE            */
*/*   BOTTOM OF THE CHAIN.  THE SIOT THAT MATCHES WILL ALWAYS        */
*/*   BE CHAINED IN AFTER THE PREVIOUSLY MATCHED AND PROCESSED       */
*/*   SIOT ON THE CHAIN.                                             */
*/*   ON EXIT, PROCSIOT IS THE SVA OF THE MATCHED SIOT.              */
*/**************************************************************     */
*                                                                  0978
*   DCL                                                            0978
*     DOUBLDD BIT(1);               /* FLAG TO INSURE NO PROBLEMS  0978
*                                      WITH DOUBLE DDS       @YM07532*/
*   DCL                                                            0979
*     SAVEL PTR(24) BDY(WORD);      /* HOLDER FOR LAST SIOT PTR      */
*   DCL                                                            0980
*     NEWCCAT BIT(1);               /* FLAG FOR ADDED CONCATS AT   0980
*                                      DEFERRED                      */
*   REALADD=LENGTH(SWAPFX);         /* CONSTANT FOR ADRESS         0981
*                                      TRANSLATION                   */
         MVC   REALADD(3),@CF00052+1                               0981
*/***************************************************************    */
*/* IF THE RESTART TYPE IS DEFERRED, AND THE DSDR IS FOR A DATA      */
*/*    SET THAT WAS DYNAMICALLY ALLOCATED, THERE WILL NOT BE A       */
*/*    SIOT/JFCB TO MATCH AGAINST - AS THE INTERPRETER WAS NOT       */
*/*    AWARE OF ITS PRESENCE.  THE PROCEDURE NOMATCH WILL BE         */
*/*    CALLED TO BUILD A SIOT/JFCB AND CHAIN THEM INTO SWA.          */
*/* IF THE RESTART IS AUTOMATIC, SIOTS FOR DYNAMICALLY ALLOCATED     */
*/*    DATASETS WILL BE BUILT ONLY IF NO MATCH CAN BE MADE.          */
*/***************************************************************    */
*                                                                  0982
*   SAVEL=SCTLSIOT;                 /* SAVE PTR TO LAST SIOT ON ENTRY
*                                      TO PROC                       */
         L     @12,SCTPTR                                          0982
         MVC   SAVEL(3),SCTLSIOT(@12)                              0982
*   BUILDSW=NOTON;                  /* INIT SWITCH FOR NOMATCH     0983
*                                      FUNCTION                      */
         NI    BUILDSW,B'01111111'                                 0983
*   DOUBLDD=NOTON;                  /* SHOW NO MATCH SO FAR  @YM07532*/
         NI    DOUBLDD,B'01111111'                                 0984
*   NEWCCAT=NOTON;                  /* INITIALIZE FLAG               */
         NI    NEWCCAT,B'01111111'                                 0985
*   IF DSDRDALC=ON THEN                                            0986
         TM    DSDRDALC(IOBUFAD),B'10000000'                       0986
         BNO   @RF00986                                            0986
*     BUILDSW=ON;                   /* TURN ON BUILD SWITCH FOR ALL
*                                      DYNAMICALLY ALLOCATED DATASETS
*                                                            @YM07048*/
         OI    BUILDSW,B'10000000'                                 0987
*   IF(RESTYPE=ON                   /* IF DEFERRED...                */
*       &DSDRDALC=ON) THEN          /* AND D.S. WAS DYNALLOC         */
@RF00986 TM    RESTYPE,B'10000000'                                 0988
         BNO   @RF00988                                            0988
         TM    DSDRDALC(IOBUFAD),B'10000000'                       0988
         BNO   @RF00988                                            0988
*     DO;                                                          0989
*       CALL NOMATCH;               /* INVOKE BUILD ROUTINE          */
         BAL   @14,NOMATCH                                         0990
*       PROCSIOT=NEWSIOT;           /* PARM FOR SIOTPROC             */
         L     @12,NEWSIOT-1                                       0991
         LA    @12,0(,@12)                                         0991
         ST    @12,PROCSIOT                                        0991
*     END;                                                         0992
*   ELSE                            /* INSERT DECISION FOR   @YM06021*/
*     IF PROCSIOT=SAVEL THEN                                       0993
         B     @RC00988                                            0993
@RF00988 L     @12,PROCSIOT                                        0993
         SLR   @11,@11                                             0993
         ICM   @11,7,SAVEL                                         0993
         CR    @12,@11                                             0993
         BNE   @RF00993                                            0993
*       DO;                                                        0994
*         CALL NOMATCH;             /* IF SIOTSCAN IS BEING ENTERED
*                                      AGAIN AFTER ALL SIOTS HAVE  0995
*                                      BEEN PROCESSED, CALL THE ERROR
*                                      ROUTINE               @YM05442*/
         BAL   @14,NOMATCH                                         0995
*         PROCSIOT=NEWSIOT;         /* FOR SIOTPROC          @YM08139*/
         L     @12,NEWSIOT-1                                       0996
         LA    @12,0(,@12)                                         0996
         ST    @12,PROCSIOT                                        0996
*         RETURN;                   /* RETURN TO CALLER      @YM08139*/
@EL00012 DS    0H                                                  0997
@EF00012 DS    0H                                                  0997
@ER00012 LM    @14,@12,@SA00012                                    0997
         BR    @14                                                 0997
*       END;                                                       0998
*                                                                  0998
