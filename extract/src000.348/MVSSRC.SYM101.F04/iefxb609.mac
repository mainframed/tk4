*       SWVA=SCTPJFCB;              /* SET UP FOR COMMON READ        */
         L     @11,SIOTPTR                                         0424
         MVC   SWVA(3,@12),SCTPJFCB(@11)                           0424
*       JCTCKFT=ON;                 /* TURN ON CKPT TAKEN BIT        */
         L     @12,JCTPTR                                          0425
         OI    JCTCKFT(@12),B'00010000'                            0425
*     END;                                                         0426
*   CALL READSWA;                   /* READ JFCB                     */
@RC00386 BAL   @14,READSWA                                         0427
*   JFCBPTR=SWBLKPTR;               /* ADDRESSABILITY FOR JFCB       */
         L     @12,SWEPAPTR                                        0428
         L     @12,SWBLKPTR(,@12)                                  0428
         ST    @12,JFCBPTR                                         0428
*   CALL ALOCAT;                    /* GO ALLOC ANY PVT CATALOGS   0429
*                                      NEEDED                @YM06575*/
         BAL   @14,ALOCAT                                          0429
*   END FINDCHEK;                                                  0430
@EL00003 DS    0H                                                  0430
@EF00003 DS    0H                                                  0430
