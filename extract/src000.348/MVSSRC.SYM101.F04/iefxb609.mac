ALOCCHEK STM   @14,@12,@SA00004                                    0431
*/*************************************************************      */
*/* THE PURPOSE OF THE ROUTINE IS TO DYNAMICALLY ALLOCATE THE        */
*/* CHECKPOINT DATA SET.  SIOTPTR AND JFCBPTR WILL HAVE BEEN         */
*/* INITIALIZED BY THE FINDCHEK PROCEDURE..                          */
*/*************************************************************      */
*                                                                  0432
*   RESPECIFY                                                      0432
*     S99RBPTR BASED(ADDR(EDBRBPTR));                              0432
*   RESPECIFY                                                      0433
*     S99RB BASED(S99RBPTR);                                       0433
*   DCL                                                            0434
*     ARITFLAG FIXED(31) DEFINED(S99FLAG2) BDY(BYTE);              0434
*   DCL                                                            0435
*     ARITFLG2 FIXED(15) DEFINED(S99FLAG1) BDY(BYTE);              0435
*   DCL                                                            0436
*     USEJCL BIT(1);                                               0436
*   DCL                                                            0437
*     SAVEDEVT CHAR(4);             /* HOLDER FOR DEVT               */
*/****  GENERAL PARM LIST INFO  ***                                  */
*                                                                  0438
*   DO;                             /* MODESET SAVEKEY(SAVEKEYA)   0438
*                                      KEYADDR(ZEROPTR)              */
*     RESPECIFY                                                    0439
*      (GPR02F) RESTRICTED;                                        0439
*     IPK;                          /* SAVE CURRENT PSW KEY          */
         IPK                                                       0440
*     SAVEKEYA=GPR02F;                                             0441
         STC   GPR02F,SAVEKEYA                                     0441
*     SPKA(ZEROPTR);                /* SET PSW KEY                   */
         SLR   @12,@12                                             0442
         SPKA  0(@12)                                              0442
*     RESPECIFY                                                    0443
*      (GPR02F) UNRESTRICTED;                                      0443
*   END;                            /* MODESET SAVEKEY(SAVEKEYA)   0444
*                                      KEYADDR(ZEROPTR) SET KEY = 0  */
*   EDBRBPTR=ADDR(EDB99RB);         /* INITIALIZE BASE               */
         LA    @11,EDB99RB                                         0445
         ST    @11,EDBRBPTR                                        0445
*   S99RBPND=ON;                    /* INDICATE ONLY ONE RB          */
         LA    @11,EDBRBPTR                                        0446
         OI    S99RBPND(@11),B'10000000'                           0446
*   S99RBLN=LENGTH(EDB99RB);        /* SET LENGTH OF RB              */
         L     @11,S99RBPTR(,@11)                                  0447
         MVI   S99RBLN(@11),X'14'                                  0447
*   S99VERB=S99VRBAL;               /* SET VERB TO ALLOCATE          */
         MVI   S99VERB(@11),X'01'                                  0448
*   S99TXTPP=ADDR(TUPLEDB);         /* POINT TO TXT UNIT PTR LIST    */
