         ST    @12,SCTPTR                                          0391
*       TTRSW=NOTON;                                               0392
         NI    TTRSW,B'01111111'                                   0392
*       IF SCTFSIOT^=ZEROPTR THEN   /* IF NO SIOTS, DONT TRY         */
         SLR   @11,@11                                             0393
         ICM   @11,7,SCTFSIOT(@12)                                 0393
         LTR   @11,@11                                             0393
         BZ    @RF00393                                            0393
*         DO;                       /* TO READ               @YM08163*/
*           CKPTSIOT=SCTFSIOT;      /* PICK UP 1ST SIOT              */
         ST    @11,CKPTSIOT                                        0395
*           DO WHILE TTRSW=NOTON;   /* LOOP TIL FOUND OR END         */
         B     @DE00396                                            0396
@DL00396 DS    0H                                                  0397
*             IF EVAL(CKPTSIOT+REALADD)->SCTPJFCB=JCTCKTTR THEN    0397
