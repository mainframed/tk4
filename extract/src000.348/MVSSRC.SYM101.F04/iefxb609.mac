         BAL   @14,NOMATCH                                         1015
*                   PROCSIOT=NEWSIOT;/* IF RETURN WAS MADE FROM    1016
*                                      NOMATCH,SET UP PTR TO NEW SIOT*/
         L     @12,NEWSIOT-1                                       1016
         LA    @12,0(,@12)                                         1016
         ST    @12,PROCSIOT                                        1016
*                   IF NEWCCAT=ON THEN                             1017
         TM    NEWCCAT,B'10000000'                                 1017
         BNO   @RF01017                                            1017
*                     DO;                                          1018
*                       LALOCCTR=LALOCCTR+1;/*               @YM08159*/
         LA    @11,1                                               1019
         AH    @11,LALOCCTR                                        1019
         STH   @11,LALOCCTR                                        1019
*                       EVAL(PROCSIOT+REALADD)->SCTDDINO=LALOCCTR; 1020
         L     @09,REALADD-1                                       1020
         LA    @09,0(,@09)                                         1020
         ALR   @12,@09                                             1020
         STH   @11,SCTDDINO(,@12)                                  1020
*                       RETURN;                                    1021
         B     @EL00012                                            1021
*                     END;                                         1022
*                 END;              /* CLOSE ERROR DO        @YM08668*/
@RF01017 DS    0H                                                  1024
*               NEXTSIOT=(CURNSIOT+REALADD)->SCTPSIOT;/* PICK UP ADDR
*                                      OF THE NEXT SIOT ON THE CHAIN */
@RF01013 L     @12,REALADD-1                                       1024
         LA    @12,0(,@12)                                         1024
         L     @11,CURNSIOT                                        1024
         ALR   @11,@12                                             1024
         SLR   @09,@09                                             1024
         ICM   @09,7,SCTPSIOT(@11)                                 1024
         ST    @09,NEXTSIOT                                        1024
*               IF NEXTSIOT=SAVEL THEN/* IF THE NEXT SIOT ON THE   1025
*                                      CHAIN IS THE LAST,TEST FOR  1025
*                                      ERROR NOW....                 */
         SLR   @11,@11                                             1025
         ICM   @11,7,SAVEL                                         1025
         CR    @09,@11                                             1025
         BNE   @RF01025                                            1025
*                 IF EVAL(NEXTSIOT+REALADD)->SCTDDNAM^=DSDRDNAM THEN
         ALR   @09,@12                                             1026
         CLC   SCTDDNAM(8,@09),DSDRDNAM(IOBUFAD)                   1026
         BE    @RF01026                                            1026
*                   DO;                                            1027
*                     CALL NOMATCH; /* GO BUILD IF DYNAMIC, OR PUT 1028
*                                      OUT ERROR MSG IF NOT  @YM07048*/
         BAL   @14,NOMATCH                                         1028
*                     PROCSIOT=NEWSIOT;/* IF RETURN WAS MADE FROM  1029
*                                      NOMATCH,SET UP PTR TO NEW SIOT*/
         L     @12,NEWSIOT-1                                       1029
         LA    @12,0(,@12)                                         1029
         ST    @12,PROCSIOT                                        1029
*                     RETURN;                                      1030
         B     @EL00012                                            1030
*                   END;                                           1031
*                                                                  1031
*/* OTHERWISE, RECHAINING TAKES PLACE TO KEEP THE SIOT THAT          */
*/* MATCHES (WHEN IT IS FOUND) IN THE PROPER PLACE ON CHAIN          */
*                                                                  1032
*               EVAL(SCTLSIOT+REALADD)->SCTPSIOT=CURNSIOT;/* MAKE THE
