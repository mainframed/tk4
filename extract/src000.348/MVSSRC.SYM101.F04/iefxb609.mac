         B     @DE01000                                            1000
@DL01000 DS    0H                                                  1001
*           IF PROCSIOT=ZEROPTR THEN/* IS THIS FIRST ENTRY ?         */
         L     @12,PROCSIOT                                        1001
         LTR   @12,@12                                             1001
         BNZ   @RF01001                                            1001
*             DO;                                                  1002
*               PROCSIOT=SCTFSIOT;  /* PICK UP FIRST SIOT            */
         L     @12,SCTPTR                                          1003
         SLR   @11,@11                                             1003
         ICM   @11,7,SCTFSIOT(@12)                                 1003
         ST    @11,PROCSIOT                                        1003
*               CURNSIOT=PROCSIOT;  /* INIT. SEARCH SIOT ADDR        */
         ST    @11,CURNSIOT                                        1004
*             END;                                                 1005
*                                                                  1005
*/* ON OTHER THAN THE FIRST ENTRY, BEGIN SEARCH WITH THE NEXT        */
*/* SIOT AFTER THE LAST PROCESSED.                                   */
*                                                                  1006
*           ELSE                                                   1006
*             CURNSIOT=(PROCSIOT+REALADD)->SCTPSIOT;/* PICK UP NEXT  */
         B     @RC01001                                            1006
@RF01001 L     @12,REALADD-1                                       1006
         LA    @12,0(,@12)                                         1006
         AL    @12,PROCSIOT                                        1006
         SLR   @11,@11                                             1006
         ICM   @11,7,SCTPSIOT(@12)                                 1006
         ST    @11,CURNSIOT                                        1006
*           IF EVAL(CURNSIOT+REALADD)->SCTDDNAM=DSDRDNAM THEN      1007
@RC01001 L     @12,CURNSIOT                                        1007
