*       IF PROCSIOT^=SCTLSIOT THEN  /* ARE THER ANY LEFTOVER SIOTS/  */
         L     @11,SCTPTR                                          1920
         SLR   @09,@09                                             1920
         ICM   @09,7,SCTLSIOT(@11)                                 1920
         CR    @12,@09                                             1920
         BE    @RF01920                                            1920
*         DO;                                                      1921
*           TEMPSW=ON;                                             1922
         OI    TEMPSW,B'10000000'                                  1922
*           DO WHILE TEMPSW=ON;                                    1923
         B     @DE01923                                            1923
@DL01923 DS    0H                                                  1924
*             PROCSIOT=(PROCSIOT+REALADD)->SCTPSIOT;/* PICK UP     1924
*                                      NEXTSIOT                      */
         L     @12,REALADD-1                                       1924
         LA    @12,0(,@12)                                         1924
         AL    @12,PROCSIOT                                        1924
         SLR   @11,@11                                             1924
         ICM   @11,7,SCTPSIOT(@12)                                 1924
         ST    @11,PROCSIOT                                        1924
*             IF PROCSIOT=SCTLSIOT THEN/* IF THIS IS THE LAST,       */
         L     @12,SCTPTR                                          1925
         SLR   @09,@09                                             1925
         ICM   @09,7,SCTLSIOT(@12)                                 1925
         CR    @11,@09                                             1925
         BNE   @RF01925                                            1925
*               TEMPSW=NOTON;       /* MAKE THIS LAST TIMETHRU LOOP  */
         NI    TEMPSW,B'01111111'                                  1926
*             NEWPTR=PROCSIOT+REALADD;/* TEMPORARY ADDRESSABILITY    */
@RF01925 L     @12,REALADD-1                                       1927
         LA    @12,0(,@12)                                         1927
         AL    @12,PROCSIOT                                        1927
         STCM  @12,7,NEWPTR                                        1927
*             IF(NEWPTR->SCTSNEW=ON /* IS THE DATA SET NEW ?         */
*                 NEWPTR->SIOTCNEW=ON)/* OR CONVERTED FROM NEW?     */
*                 &NEWPTR->SIOTDYAL=ON THEN                        1928
         TM    SCTSNEW(@12),B'00000100'                            1928
         BO    @GL00053                                            1928
         TM    SIOTCNEW(@12),B'00001000'                           1928
         BNO   @RF01928                                            1928
@GL00053 L     @12,NEWPTR-1                                        1928
         LA    @12,0(,@12)                                         1928
         TM    SIOTDYAL(@12),B'01000000'                           1928
         BNO   @RF01928                                            1928
