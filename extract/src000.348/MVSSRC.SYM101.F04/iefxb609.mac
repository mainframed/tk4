*/**************************************************************     */
*/*  THE PURPOSE OF THIS PROC IS : 1) TO BUILD A SIOT/JFCB PAIR      */
*/*     FOR A DYN ALLOCED DATA SET.                     2) TO        */
*/* INVOKE THE ERROR PROC IF IT HAS BEEN FOUND THAT A DD ENTRY       */
*/* WAS MISSING FROM THE JCL SUBMITTED FOR THE RESTART.              */
*/**************************************************************     */
*                                                                  1807
*   DCL                                                            1807
*     UNITS FIXED(8) DEFINED(SCTNMBUT) BDY(BYTE);/* NUMERIC UNIT CT  */
*   DCL                                                            1808
*     CCATEND BIT(1);                                              1808
*   IF BUILDSW=ON THEN              /* SIOTSCAN DETERMINED THAT SIOT
*                                      WAS NEEDED BECAUSE OF A     1809
*                                      DYNALLOC AT DEFERRED RESTART  */
         TM    BUILDSW,B'10000000'                                 1809
         BNO   @RF01809                                            1809
*     DO;                                                          1810
*       DJFCBPTR=ADDR(DSDRJFCB);    /* INSURE ADDR'ABILITY DSDRJFCB  */
         LA    @12,DSDRJFCB(,IOBUFAD)                              1811
         ST    @12,DJFCBPTR                                        1811
*       IF DJFCBPTR->JFCVRDS=ON THEN/* IF DATA SET IS VAM....        */
         L     @12,DJFCBPTR                                        1812
         TM    JFCVRDS(@12),B'00000100'                            1812
         BNO   @RF01812                                            1812
*         CALL VAMERR;              /* THE UCBADD CANNOT BE RECOVERED
*                                                            @YM08170*/
         BAL   @14,VAMERR                                          1813
*       CALL ASGNPROC;              /* GET A NEW BLOCK ASSIGNED      */
@RF01812 BAL   @14,ASGNPROC                                        1814
*       NEWPTR=ASGNADD+REALADD;     /* SWA ADDRESSABILITY NEW SIOT   */
         L     @12,ASGNADD-1                                       1815
         LA    @12,0(,@12)                                         1815
         L     @11,REALADD-1                                       1815
         LA    @11,0(,@11)                                         1815
         ALR   @11,@12                                             1815
         STCM  @11,7,NEWPTR                                        1815
*       IF PROCSIOT=ZEROPTR THEN    /* THE ADDED SIOT MAY NEED TO BE
*                                      ADDED AS FIRST SIOT           */
         L     @09,PROCSIOT                                        1816
         LTR   @09,@09                                             1816
         BNZ   @RF01816                                            1816
*         DO;                                                      1817
*           NEWPTR->SCTPSIOT=SCTFSIOT;/* MAKE THE OLD FIRST THE NEW
*                                      FIRST'S NEXT.                 */
         L     @09,SCTPTR                                          1818
         MVC   SCTPSIOT(3,@11),SCTFSIOT(@09)                       1818
*           SCTFSIOT=ASGNADD;       /* PLUG IN NEW AS FIRST          */
         STCM  @12,7,SCTFSIOT(@09)                                 1819
*           IF SCTLSIOT=ZEROPTR THEN/* FOR ONLY DD SET               */
         ICM   @12,7,SCTLSIOT(@09)                                 1820
         BNZ   @RF01820                                            1820
*             SCTLSIOT=SCTFSIOT;    /* FIRST AND LAST =              */
         MVC   SCTLSIOT(3,@09),SCTFSIOT(@09)                       1821
*         END;                                                     1822
*       ELSE                                                       1823
*         DO;                       /* THIS IS NOT THE FIRST.        */
         B     @RC01816                                            1823
@RF01816 DS    0H                                                  1824
*           IF PROCSIOT=SCTLSIOT THEN/* IS IT THE LAST?              */
         L     @12,PROCSIOT                                        1824
         L     @11,SCTPTR                                          1824
         SLR   @09,@09                                             1824
         ICM   @09,7,SCTLSIOT(@11)                                 1824
         CR    @12,@09                                             1824
         BNE   @RF01824                                            1824
*             DO;                                                  1825
*               EVAL(PROCSIOT+REALADD)->SCTPSIOT=ASGNADD;/* MAKE THE
*                                      OLD LAST POINT TO THE NEW   1826
*                                      LAST.                         */
         L     @09,REALADD-1                                       1826
         LA    @09,0(,@09)                                         1826
         ALR   @12,@09                                             1826
         MVC   SCTPSIOT(3,@12),ASGNADD                             1826
*               SCTLSIOT=ASGNADD;   /* SET 'LAST' PTR IN SCT.        */
         MVC   SCTLSIOT(3,@11),ASGNADD                             1827
*             END;                                                 1828
*           ELSE                                                   1829
*             DO;                   /* NEW SIOT WON'T BE THE 1ST OR
*                                      THE LAST                      */
         B     @RC01824                                            1829
@RF01824 DS    0H                                                  1830
*               NEWPTR->SCTPSIOT=   /* PICK UP THE PREV SIOT'S       */
*                   (PROCSIOT+REALADD)->SCTPSIOT;/* LINK POINTER     */
         L     @12,NEWPTR-1                                        1830
         L     @11,PROCSIOT                                        1830
         L     @09,REALADD-1                                       1830
         LA    @09,0(,@09)                                         1830
         LR    @15,@11                                             1830
