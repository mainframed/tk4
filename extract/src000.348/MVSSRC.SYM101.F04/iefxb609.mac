*JFCBPROC:                                                         1205
*   PROC OPTIONS(NOSAVE(IOBUFAD));                                 1205
*                                                                  1205
JFCBPROC STM   @14,@06,@SA00015                                    1205
         STM   @08,@12,@SA00015+36                                 1205
*/**************************************************************     */
*/* THIS PROCEDURE RESTORES ALL FIELDS FROM THE JFCB ON THE CKPT     */
*/* EXCEPT THE VOLUME INFORMATION...WHICH IS DONE IN 'VOLSERS'       */
*/**************************************************************     */
*                                                                  1206
*   DCL                                                            1206
*     JFCBEFLG CHAR(1) BDY(BYTE);   /* TEMP JFCBEXTP FLAG    @Z40MPSJ*/
*   DCL                                                            1207
*     JFCBEON BIT(8) CONSTANT('80'X);/* JFCBEXTP VALUE       @Z40MPSJ*/
*   DCL                                                            1208
*     ARITXNAM FIXED(31) DEFINED(JFCBXNAM) BDY(BYTE);              1208
*   JFCBTVOL=DJFCBPTR->JFCBVOLS(1); /* SAVE ONE BYTE         @ZA07018*/
         L     @12,DJFCBPTR                                        1209
         IC    @11,JFCBVOLS(,@12)                                  1209
         STC   @11,JFCBTVOL                                        1209
*   JFCBTENQ=DJFCBPTR->JFCBIND2;    /* SAVE FOR ENQ          @ZA07018*/
         L     @12,DJFCBPTR                                        1210
         IC    @11,JFCBIND2(,@12)                                  1210
         STC   @11,JFCBTENQ                                        1210
*   IF SCTDUMMY=NOTON THEN          /* DO NOT PROCESS JFCB FOR DUMMY */
         L     @12,SIOTPTR                                         1211
         TM    SCTDUMMY(@12),B'10000000'                           1211
         BNZ   @RF01211                                            1211
*     IF(SCTSGDGS=ONDJFCBPTR->JFCGDG=NOTON) THEN/* ONLY MERGE JFCB
*                                      FOR GDG SINGLE OR NONGDG, NOT
*                                      FOR GDGALL            @ZA06230*/
         TM    SCTSGDGS(@12),B'10000000'                           1212
         BO    @RT01212                                            1212
         L     @12,DJFCBPTR                                        1212
         TM    JFCGDG(@12),B'00000010'                             1212
         BNZ   @RF01212                                            1212
@RT01212 DS    0H                                                  1213
*       DO;                                                        1213
*         IF(RESTYPE=ON             /* FOR DEFERRED RESTART,         */
*             &DJFCBPTR->JFCVRDS=ON) THEN/* AND DATASET IS VAM..   1214
*                                                            @YM07524*/
         TM    RESTYPE,B'10000000'                                 1214
         BNO   @RF01214                                            1214
         L     @12,DJFCBPTR                                        1214
         TM    JFCVRDS(@12),B'00000100'                            1214
         BNO   @RF01214                                            1214
*           CALL VAMERR;            /* DO NOT ALLOW THE RESTART.     */
         BAL   @14,VAMERR                                          1215
*         IF(RESTYPE=ON(RESTYPE=NOTON&JCTSYSCK=ON))/* FOR DEF. OR 1216
*                                      AUTO AFTER DEFERRED           */
*             &(SCTSYSIN=ONSCTSYSOU=ON) THEN/* & D.S. IS SYSIN OR 1216
*                                      OUT..                         */
@RF01214 TM    RESTYPE,B'10000000'                                 1216
         BO    @GL00028                                            1216
         TM    RESTYPE,B'10000000'                                 1216
         BNZ   @RF01216                                            1216
         L     @12,JCTPTR                                          1216
         TM    JCTSYSCK(@12),B'10000000'                           1216
         BNO   @RF01216                                            1216
@GL00028 L     @12,SIOTPTR                                         1216
         TM    SCTSYSIN(@12),B'01000000'                           1216
         BO    @RT01216                                            1216
         TM    SCTSYSOU(@12),B'00001000'                           1216
         BNO   @RF01216                                            1216
@RT01216 DS    0H                                                  1217
*           IF(RESTYPE=ON&SCTSYSOU=ON) THEN/* DEF. AND SYSOUT..      */
         TM    RESTYPE,B'10000000'                                 1217
         BNO   @RF01217                                            1217
         L     @12,SIOTPTR                                         1217
         TM    SCTSYSOU(@12),B'00001000'                           1217
         BNO   @RF01217                                            1217
*             JFCBNVOL=0;           /* NVOLS NOW ZERO.               */
         L     @12,JFCBPTR                                         1218
         MVI   JFCBNVOL(@12),X'00'                                 1218
*           ELSE                                                   1219
*             ;                                                    1219
@RF01217 DS    0H                                                  1220
*         ELSE                                                     1220
*           JFCBDSNM=DJFCBPTR->JFCBDSNM;/* MERGE DSNAME.             */
         B     @RC01216                                            1220
@RF01216 L     @12,JFCBPTR                                         1220
         L     @11,DJFCBPTR                                        1220
         MVC   JFCBDSNM(44,@12),JFCBDSNM(@11)                      1220
*/**************************************************************     */
*/* AT THIS POINT ALL DATASETS EXCEPT SYSIN,SYSOUT                   */
*/*         HAVE HAD DSNAME MERGED.                                  */
*/**************************************************************     */
*/**************************************************************     */
*/* FOLLOWING IS A FIELD BY FIELD MERGE OF MOST AREAS OF JFCB        */
*/**************************************************************     */
*                                                                  1221
*         JFCBELNM=DJFCBPTR->JFCBELNM;                             1221
@RC01216 L     @12,JFCBPTR                                         1221
         L     @11,DJFCBPTR                                        1221
         MVC   JFCBELNM(8,@12),JFCBELNM(@11)                       1221
*         JFCBTSDM=DJFCBPTR->JFCBTSDM;                             1222
         L     @11,DJFCBPTR                                        1222
         MVC   JFCBTSDM(1,@12),JFCBTSDM(@11)                       1222
*         JFCBDSCB=DJFCBPTR->JFCBDSCB;                             1223
         L     @11,DJFCBPTR                                        1223
         MVC   JFCBDSCB(3,@12),JFCBDSCB(@11)                       1223
*         IF JFCORGAM^=ON THEN      /* DO NOT MERGE VSAM RESTART   1224
*                                      OPTIONS               @ZA24441*/
         TM    JFCORGAM(@12),B'00001000'                           1224
         BO    @RF01224                                            1224
*           JFCFCBID=DJFCBPTR->JFCFCBID;                           1225
         L     @11,DJFCBPTR                                        1225
         MVC   JFCFCBID(4,@12),JFCFCBID(@11)                       1225
*         JFCBADBF=DJFCBPTR->JFCBADBF;                             1226
@RF01224 L     @12,JFCBPTR                                         1226
         L     @11,DJFCBPTR                                        1226
         LH    @11,JFCBADBF(,@11)                                  1226
         STH   @11,JFCBADBF(,@12)                                  1226
*         JFCNLREC=DJFCBPTR->JFCNLREC;                             1227
*                                                                  1227
         L     @11,DJFCBPTR                                        1227
         LH    @11,JFCNLREC(,@11)                                  1227
         STH   @11,JFCNLREC(,@12)                                  1227
*/*   JFCVINDX WILL NOT BE MERGED                       @ZM30408     */
*                                                                  1228
*         JFCBLTYP=DJFCBPTR->JFCBLTYP;                             1228
         L     @11,DJFCBPTR                                        1228
         MVC   JFCBLTYP(1,@12),JFCBLTYP(@11)                       1228
*         JFCBOTTR=DJFCBPTR->JFCBOTTR;                             1229
         L     @11,DJFCBPTR                                        1229
         MVC   JFCBOTTR(3,@12),JFCBOTTR(@11)                       1229
*         JFCBVLSQ=DJFCBPTR->JFCBVLSQ;                             1230
         L     @11,DJFCBPTR                                        1230
         LH    @11,JFCBVLSQ(,@11)                                  1230
         STH   @11,JFCBVLSQ(,@12)                                  1230
*         JFCBMASK=DJFCBPTR->JFCBMASK;                             1231
         L     @11,DJFCBPTR                                        1231
         MVC   JFCBMASK(8,@12),JFCBMASK(@11)                       1231
*         JFCBCRDT=DJFCBPTR->JFCBCRDT;                             1232
         L     @11,DJFCBPTR                                        1232
         MVC   JFCBCRDT(3,@12),JFCBCRDT(@11)                       1232
*         JFCBXPDT=DJFCBPTR->JFCBXPDT;                             1233
         L     @11,DJFCBPTR                                        1233
         MVC   JFCBXPDT(3,@12),JFCBXPDT(@11)                       1233
*         JFCBIND1=DJFCBPTR->JFCBIND1;                             1234
*                                                                  1234
         L     @11,DJFCBPTR                                        1234
         MVC   JFCBIND1(1,@12),JFCBIND1(@11)                       1234
*/* JFCBIND2 WILL BE MERGED LATER IN THIS PROC AFTER A TEST IS       */
*/*   MADE TO DETERMINE IF THE DISP HAS CHANGED FROM SHR TO OLD      */
*/*   VIA A DYNAMIC CONVERT.                                         */
*                                                                  1235
*         JFCAMPTR=DJFCBPTR->JFCAMPTR;                             1235
         L     @11,DJFCBPTR                                        1235
         L     @11,JFCAMPTR(,@11)                                  1235
         ST    @11,JFCAMPTR(,@12)                                  1235
*         JFCEROPT=DJFCBPTR->JFCEROPT;                             1236
         L     @11,DJFCBPTR                                        1236
         MVC   JFCEROPT(1,@12),JFCEROPT(@11)                       1236
*         JFCTRTCH=DJFCBPTR->JFCTRTCH;                             1237
         L     @11,DJFCBPTR                                        1237
         MVC   JFCTRTCH(1,@12),JFCTRTCH(@11)                       1237
*         JFCCODE=DJFCBPTR->JFCCODE;                               1238
         L     @11,DJFCBPTR                                        1238
         MVC   JFCCODE(1,@12),JFCCODE(@11)                         1238
*         JFCMODE=DJFCBPTR->JFCMODE;                               1239
         L     @11,DJFCBPTR                                        1239
         MVC   JFCMODE(1,@12),JFCMODE(@11)                         1239
*         JFCPRTSP=DJFCBPTR->JFCPRTSP;                             1240
         L     @11,DJFCBPTR                                        1240
         MVC   JFCPRTSP(1,@12),JFCPRTSP(@11)                       1240
*         JFCDEN=DJFCBPTR->JFCDEN;                                 1241
         L     @11,DJFCBPTR                                        1241
         MVC   JFCDEN(1,@12),JFCDEN(@11)                           1241
*         JFCBABFS=DJFCBPTR->JFCBABFS;                             1242
         L     @11,DJFCBPTR                                        1242
         MVC   JFCBABFS(3,@12),JFCBABFS(@11)                       1242
*         JFCDSORG=DJFCBPTR->JFCDSORG;                             1243
         L     @11,DJFCBPTR                                        1243
         MVC   JFCDSORG(2,@12),JFCDSORG(@11)                       1243
*         JFCRECFM=DJFCBPTR->JFCRECFM;                             1244
         L     @11,DJFCBPTR                                        1244
         MVC   JFCRECFM(1,@12),JFCRECFM(@11)                       1244
*         JFCOPTCD=DJFCBPTR->JFCOPTCD;                             1245
         L     @11,DJFCBPTR                                        1245
         MVC   JFCOPTCD(1,@12),JFCOPTCD(@11)                       1245
*         JFCBLKSI=DJFCBPTR->JFCBLKSI;                             1246
         L     @11,DJFCBPTR                                        1246
         LH    @11,JFCBLKSI(,@11)                                  1246
         STH   @11,JFCBLKSI(,@12)                                  1246
*         JFCLRECL=DJFCBPTR->JFCLRECL;                             1247
         L     @11,DJFCBPTR                                        1247
         LH    @11,JFCLRECL(,@11)                                  1247
         STH   @11,JFCLRECL(,@12)                                  1247
*         JFCNCP=DJFCBPTR->JFCNCP;                                 1248
         L     @11,DJFCBPTR                                        1248
         MVC   JFCNCP(1,@12),JFCNCP(@11)                           1248
*         JFCNTM=DJFCBPTR->JFCNTM;                                 1249
         L     @11,DJFCBPTR                                        1249
         MVC   JFCNTM(1,@12),JFCNTM(@11)                           1249
*         JFCRKP=DJFCBPTR->JFCRKP;                                 1250
         L     @11,DJFCBPTR                                        1250
         LH    @11,JFCRKP(,@11)                                    1250
         STH   @11,JFCRKP(,@12)                                    1250
*         JFCCYLOF=DJFCBPTR->JFCCYLOF;                             1251
         L     @11,DJFCBPTR                                        1251
         MVC   JFCCYLOF(1,@12),JFCCYLOF(@11)                       1251
*         JFCDBUFN=DJFCBPTR->JFCDBUFN;                             1252
         L     @11,DJFCBPTR                                        1252
         MVC   JFCDBUFN(1,@12),JFCDBUFN(@11)                       1252
*         JFCBEFLG=JFCBEFLG&&JFCBEFLG;/* CLEAR FLAG          @Z40MPSJ*/
         XC    JFCBEFLG(1),JFCBEFLG                                1253
*         IF JFCBEXTP=ON&JFCBEXAD^=ZEROPTR THEN                    1254
         TM    JFCBEXTP(@12),B'10000000'                           1254
         BNO   @RF01254                                            1254
         L     @12,JFCBEXAD-1(,@12)                                1254
         LA    @12,0(,@12)                                         1254
         LTR   @12,@12                                             1254
         BZ    @RF01254                                            1254
*           JFCBEFLG=JFCBEON;       /* RETAIN JFCBE INDICATOR      1255
*                                                            @Z40MPSJ*/
         MVI   JFCBEFLG,X'80'                                      1255
*         JFCINTVL=DJFCBPTR->JFCUCSOPJFCBEFLG;/* JFCUCSOP=JFCINTVL
*                                                            @Z40MPSJ*/
@RF01254 L     @12,JFCBPTR                                         1256
         L     @11,DJFCBPTR                                        1256
         MVC   JFCINTVL(1,@12),JFCUCSOP(@11)                       1256
         OC    JFCINTVL(1,@12),JFCBEFLG                            1256
*         JFCOUTLI=DJFCBPTR->JFCOUTLI;                             1257
         L     @11,DJFCBPTR                                        1257
         MVC   JFCOUTLI(3,@12),JFCOUTLI(@11)                       1257
*         JFCSOWA=DJFCBPTR->JFCSOWA;                               1258
         L     @11,DJFCBPTR                                        1258
         LH    @11,JFCSOWA(,@11)                                   1258
         STH   @11,JFCSOWA(,@12)                                   1258
*         JFCBNTCS=DJFCBPTR->JFCBNTCS;                             1259
*                                                                  1259
         L     @11,DJFCBPTR                                        1259
         MVC   JFCBNTCS(1,@12),JFCBNTCS(@11)                       1259
*/* JFCBNVOL AND JFCBVOLS WILL BE SELECTIVELY MERGED IN VOLSERS.     */
*                                                                  1260
*         JFCBEXTL=DJFCBPTR->JFCBEXTL;                             1260
*                                                                  1260
         L     @11,DJFCBPTR                                        1260
         MVC   JFCBEXTL(1,@12),JFCBEXTL(@11)                       1260
*/* JFCBEXAD WILL BE MERGED IN JFCBXJOB.                             */
*                                                                  1261
*         JFCBPQTY=DJFCBPTR->JFCBPQTY;                             1261
         L     @11,DJFCBPTR                                        1261
         MVC   JFCBPQTY(3,@12),JFCBPQTY(@11)                       1261
*         JFCBCTRI=DJFCBPTR->JFCBCTRI;                             1262
         L     @11,DJFCBPTR                                        1262
         MVC   JFCBCTRI(1,@12),JFCBCTRI(@11)                       1262
*         IF(RESTYPE=ON&JFCBSQTY^>DJFCBPTR->JFCBSQTY) THEN         1263
         TM    RESTYPE,B'10000000'                                 1263
         BNO   @RF01263                                            1263
         L     @11,DJFCBPTR                                        1263
         CLC   JFCBSQTY(3,@12),JFCBSQTY(@11)                       1263
         BH    @RF01263                                            1263
*           JFCBSQTY=DJFCBPTR->JFCBSQTY;/* DONT OVERLAY A REQUEST FOR
*                                      MORE SEC ALLOC FOR DEFERRED 1264
*                                      RESTART               @YA66585*/
         L     @11,DJFCBPTR                                        1264
         MVC   JFCBSQTY(3,@12),JFCBSQTY(@11)                       1264
*         JFCFLGS1=DJFCBPTR->JFCFLGS1;                             1265
@RF01263 L     @12,JFCBPTR                                         1265
         L     @11,DJFCBPTR                                        1265
         MVC   JFCFLGS1(1,@12),JFCFLGS1(@11)                       1265
*         JFCBFLG3=DJFCBPTR->JFCBFLG3;/* THIS FIELD UNUSED IN MVS  1266
*                                      PRIOR TO SU32         @ZA19250*/
         L     @11,DJFCBPTR                                        1266
         MVC   JFCBFLG3(1,@12),JFCBFLG3(@11)                       1266
*         JFCBABST=DJFCBPTR->JFCBABST;                             1267
         L     @11,DJFCBPTR                                        1267
         LH    @11,JFCBABST(,@11)                                  1267
         STH   @11,JFCBABST(,@12)                                  1267
*         JFCBDRLH=DJFCBPTR->JFCBDRLH;                             1268
*                                                                  1268
         L     @11,DJFCBPTR                                        1268
         MVC   JFCBDRLH(3,@12),JFCBDRLH(@11)                       1268
*/* JFCBVLCT WILL BE MERGED IN VOLSERS PROC                          */
*                                                                  1269
*         JFCBDQTY=DJFCBPTR->JFCBDQTY;                             1269
         L     @11,DJFCBPTR                                        1269
         MVC   JFCBDQTY(3,@12),JFCBDQTY(@11)                       1269
*         JFCBSPTN=DJFCBPTR->JFCBSPTN;                             1270
*                                                                  1270
         L     @11,DJFCBPTR                                        1270
         MVC   JFCBSPTN(1,@12),JFCBSPTN(@11)                       1270
*/********* END OF BASIC MERGE   ***********                         */
*/**************************************************************     */
*/* NOW TEST TO DETERMINE IF DISP WAS DYNAMICALLY CONVERTED          */
*/*   FROM SHR TO OLD.  IF THE TEST IS POSITIVE, THE DSENQ TABLE     */
*/*   MUST BE MODIFIED SO THE DATASET WILL BE ENQ'D EXCLUSIVE        */
*/*   WHEN THE JOB IS INITIATED.                                     */
*/**************************************************************     */
*                                                                  1271
*         IF DJFCBPTR->JFCBIND2^=JFCBIND2 THEN/* IF DISPS ^=         */
         L     @11,DJFCBPTR                                        1271
         CLC   JFCBIND2(1,@11),JFCBIND2(@12)                       1271
         BE    @RF01271                                            1271
*           IF(DJFCBPTR->JFCSHARE^=ON/* DSDR SAYS STAT WAS NON-SHR   */
*               &JFCSHARE=ON)       /* JCL SAYS STAT WAS SHR @YM04969*/
*             THEN                                                 1272
         L     @11,DJFCBPTR                                        1272
         TM    JFCSHARE(@11),B'00001000'                           1272
         BO    @RF01272                                            1272
         TM    JFCSHARE(@12),B'00001000'                           1272
         BNO   @RF01272                                            1272
*             DO;                   /* GO UPDATE THE DSENQ TABLE     */
*               DSN=JFCBDSNM;       /* SET DSNAME INTO PARM LIST     */
         MVC   DSN(44),JFCBDSNM(@12)                               1274
*               BITMAP=''B;         /* ZERO THE FUNCTION MAP         */
         XC    BITMAP(2),BITMAP                                    1275
*               ENQEXCL=ON;         /* FUNCTION = ENQ EXCL.          */
*               ADCHONLY=ON;        /* DONT'T DO THE ENQ, JUST MARK
*                                      THE ENTRY FOR INIIATOR'S USE  */
         OI    ENQEXCL,B'01000001'                                 1277
*               DO;                 /* LINK EP('IEFAB4DC') PARAM(BITM
*                                      AP,PASSJCT,PASSDSNM,PASSEPA,PA
*                                      SSTCB,STEPNO,PAD1,PAD2,RETPARM
*                                      )                             */
*                 I00601='00'X;     /* EP/DE FLAG                    */
         MVI   I00601,X'00'                                        1279
*                 I00602=ADDR('IEFAB4DC');/* ADDR OF EP/DE           */
         LA    @12,@CC04253                                        1280
         STCM  @12,7,I00602                                        1280
*                 I00604=0;         /* DCB PTR                       */
         SLR   @12,@12                                             1281
         STCM  @12,7,I00604                                        1281
*                 I00603='00'X;     /* ERRET FLAG                    */
         MVI   I00603,X'00'                                        1282
*                 IOPTLIST(1)=ADDR(BITMAP);/* ADDR OF PROB PROG PARM */
         LA    @12,BITMAP                                          1283
         ST    @12,IOPTLIST                                        1283
*                 IOPTLIST(2)=ADDR(PASSJCT);/* ADDR OF PROB PROG PARM*/
         LA    @12,PASSJCT                                         1284
         ST    @12,IOPTLIST+4                                      1284
*                 IOPTLIST(3)=ADDR(PASSDSNM);/* ADDR OF PROB PROG  1285
*                                      PARM                          */
         LA    @12,PASSDSNM                                        1285
         ST    @12,IOPTLIST+8                                      1285
*                 IOPTLIST(4)=ADDR(PASSEPA);/* ADDR OF PROB PROG PARM*/
         LA    @12,PASSEPA                                         1286
         ST    @12,IOPTLIST+12                                     1286
*                 IOPTLIST(5)=ADDR(PASSTCB);/* ADDR OF PROB PROG PARM*/
         LA    @12,PASSTCB                                         1287
         ST    @12,IOPTLIST+16                                     1287
*                 IOPTLIST(6)=ADDR(STEPNO);/* ADDR OF PROB PROG PARM */
         LA    @12,STEPNO                                          1288
         ST    @12,IOPTLIST+20                                     1288
*                 IOPTLIST(7)=ADDR(PAD1);/* ADDR OF PROB PROG PARM   */
         LA    @12,PAD1                                            1289
         ST    @12,IOPTLIST+24                                     1289
*                 IOPTLIST(8)=ADDR(PAD2);/* ADDR OF PROB PROG PARM   */
         LA    @12,PAD2                                            1290
         ST    @12,IOPTLIST+28                                     1290
*                 IOPTLIST(9)=ADDR(RETPARM);/* ADDR OF PROB PROG PARM*/
         LA    @12,RETPARM                                         1291
         ST    @12,IOPTLIST+32                                     1291
*                 IOPTLIST(9)=IOPTLIST(9)'80000000'X;/* SET VL BIT  */
         O     @12,@CF04255                                        1292
         ST    @12,IOPTLIST+32                                     1292
*                 RESPECIFY                                        1293
*                  (GPR01P) RESTRICTED;                            1293
*                 GPR01P=ADDR(IOPTLIST);/* ADDR OF OPTLIST           */
         LA    GPR01P,IOPTLIST                                     1294
*                 RESPECIFY                                        1295
*                  (GPR15P) RESTRICTED;                            1295
*                 GPR15P=ADDR(I006);/* ADDR OF LINK LIST             */
         LA    GPR15P,I006                                         1296
*                 SVC(6);           /* ISSUE LINK SVC                */
         SVC   6                                                   1297
*                 RESPECIFY                                        1298
*                  (GPR01P) UNRESTRICTED;                          1298
*                 RESPECIFY                                        1299
*                  (GPR15P) UNRESTRICTED;                          1299
*               END;                /* LINK EP('IEFAB4DC') PARAM(BITM
*                                      AP,PASSJCT,PASSDSNM,PASSEPA,PA
*                                      SSTCB,STEPNO,PAD1,PAD2,RETPARM
*                                      )                             */
*               CALL FREEEPA;       /* FREE EXTRA EPA        @YM08161*/
         BAL   @14,FREEEPA                                         1301
*             END;                                                 1302
*         JFCBIND2=DJFCBPTR->JFCBIND2;/* COMPLETE THE MERGE          */
@RF01272 DS    0H                                                  1303
@RF01271 L     @12,JFCBPTR                                         1303
         L     @11,DJFCBPTR                                        1303
         MVC   JFCBIND2(1,@12),JFCBIND2(@11)                       1303
*         IF JFCDEFER=ON THEN       /* IF CHKPT DETECTED A VOLUME    */
         TM    JFCDEFER(@12),B'00100000'                           1304
         BNO   @RF01304                                            1304
*           DO;                     /* SWITCH,CAUSE DEFERED MOUNT TO
*                                      TAKE PLACE.                   */
*             JFCDEFER=NOTON;       /* TURN OFF JFCB SWITCH          */
         NI    JFCDEFER(@12),B'11011111'                           1306
*             IF DJFCBPTR->JFCBNVOL>SCTNMBUT THEN/* TURN DEFER MOUNT
*                                      BIT ON IF MORE VOLS THAN    1307
*                                      DEVICES               @ZA16378*/
         L     @12,SIOTPTR                                         1307
         L     @11,DJFCBPTR                                        1307
         CLC   JFCBNVOL(1,@11),SCTNMBUT(@12)                       1307
         BNH   @RF01307                                            1307
*               SCTDEFER=ON;        /* TURN ON SWITCH FOR DATA-MANG. */
         OI    SCTDEFER(@12),B'00000010'                           1308
*           END;                                                   1309
@RF01307 DS    0H                                                  1310
*         IF(DJFCBPTR->JFCBVOLS(1)^=FOXES/*                  @YM08115*/
*             &DJFCBPTR->JFCORGAM^=ON) THEN/*                @YM08116*/
@RF01304 L     @12,DJFCBPTR                                        1310
         CLI   JFCBVOLS(@12),X'FF'                                 1310
         BE    @RF01310                                            1310
         L     @12,DJFCBPTR                                        1310
