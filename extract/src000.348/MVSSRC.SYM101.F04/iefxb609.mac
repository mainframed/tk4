         CLC   SCTDDNAM(8,@11),DSDRDNAM(IOBUFAD)                   1007
         BNE   @RF01007                                            1007
*             DO;                                                  1008
*               DOUBLDD=ON;         /* SHOW MATCH FOUND      @YM07532*/
         OI    DOUBLDD,B'10000000'                                 1009
*               PROCSIOT=CURNSIOT;  /* IF THE NEW NEXT MATCHES THE   */
         ST    @12,PROCSIOT                                        1010
