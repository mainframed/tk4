         B     @RC01007                                            1012
@RF01007 DS    0H                                                  1013
*               IF(CURNSIOT=SAVEL(RESTYPE=ON&EVAL(CURNSIOT+REALADD)->
*                   SCTDDNAM=BLANKS)) THEN                         1013
         L     @12,CURNSIOT                                        1013
         SLR   @11,@11                                             1013
         ICM   @11,7,SAVEL                                         1013
         CR    @12,@11                                             1013
         BE    @RT01013                                            1013
         TM    RESTYPE,B'10000000'                                 1013
         BNO   @RF01013                                            1013
         L     @11,REALADD-1                                       1013
         LA    @11,0(,@11)                                         1013
         ALR   @12,@11                                             1013
         CLC   SCTDDNAM(8,@12),@CC03776                            1013
         BNE   @RF01013                                            1013
@RT01013 DS    0H                                                  1014
*                 DO;                                              1014
*                   CALL NOMATCH;   /* GO BUILD IF DYNAMIC, OR PUT 1015
