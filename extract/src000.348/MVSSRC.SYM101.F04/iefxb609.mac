         ALR   @12,@09                                             1887
         CLC   SCTDDNAM(8,@12),@CC03776                            1887
         BNE   @RF01887                                            1887
*                     NEWSIOT=NEWPTR;/* UPDATE UNTIL END OF GRP      */
         STCM  @09,7,NEWSIOT                                       1888
