         L     @12,SWEPAPTR                                        0415
         L     @11,JCTPTR                                          0415
         MVC   SWVA(3,@12),JCTSSTR(@11)                            0415
*       CALL READSWA;               /* READ IT                       */
         BAL   @14,READSWA                                         0416
*       SCTPTR=SWBLKPTR;            /* ADDRESSABILITY SCT            */
         L     @12,SWEPAPTR                                        0417
         L     @11,SWBLKPTR(,@12)                                  0417
         ST    @11,SCTPTR                                          0417
*       SWVA=SCTFSIOT;              /* PICK UP ADDR SIOT FOR CKPT DS */
         MVC   SWVA(3,@12),SCTFSIOT(@11)                           0418
*       CALL READSWA;                                              0419
         BAL   @14,READSWA                                         0419
*       SIOTPTR=SWBLKPTR;           /* ADDRESSABILITY SIOT           */
         L     @12,SWEPAPTR                                        0420
