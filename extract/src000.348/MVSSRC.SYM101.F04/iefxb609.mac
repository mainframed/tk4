         L     @12,SIOTPTR                                         1065
         OI    SIOTCCAT(@12),B'00100000'                           1065
*   SCTSDISP=DSDRDISP;              /* RESTORE DISPOSITION FIELD     */
@RF01064 L     @12,SIOTPTR                                         1066
         MVC   SCTSDISP(1,@12),DSDRDISP(IOBUFAD)                   1066
*   SCTSBYT3=DSDRBYT3;              /* RESTORE STATUS FIELD          */
         MVC   SCTSBYT3(1,@12),DSDRBYT3(IOBUFAD)                   1067
*   SIOTALTD=DSDRALTD;              /* RESTORE ALTERNATE DISP FIELD  */
         MVC   SIOTALTD(1,@12),DSDRALTD(IOBUFAD)                   1068
*   IF DSDROCKP=ON THEN             /* RESTORE OPEN-AT-CHKPT BIT FOR */
         TM    DSDROCKP(IOBUFAD),B'00100000'                       1069
         BNO   @RF01069                                            1069
*     SIOTOCKP=ON;                  /* RESTART SVC IF NECESSARY.     */
         OI    SIOTOCKP(@12),B'10000000'                           1070
*   IF(SIOVAMDS=NOTON               /* UNITTYPE FIELD WILL NOT BE    */
*       &SIOTSSDS=NOTON)            /* OVERLAYED FROM THE DSDR FOR   */
*     THEN                                                         1071
@RF01069 L     @12,SIOTPTR                                         1071
         TM    SIOVAMDS(@12),B'00100000'                           1071
         BNZ   @RF01071                                            1071
         TM    SIOTSSDS(@12),B'10000000'                           1071
         BNZ   @RF01071                                            1071
