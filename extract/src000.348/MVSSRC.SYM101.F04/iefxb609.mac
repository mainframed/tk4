         B     @RC00402                                            0409
@RF00402 L     @12,REALADD-1                                       0409
         LA    @12,0(,@12)                                         0409
         AL    @12,CKPTSIOT                                        0409
         SLR   @11,@11                                             0409
         ICM   @11,7,SCTPSIOT(@12)                                 0409
         ST    @11,CKPTSIOT                                        0409
*           END;                                                   0410
@RC00402 DS    0H                                                  0410
@RC00397 DS    0H                                                  0410
@DE00396 TM    TTRSW,B'10000000'                                   0410
         BZ    @DL00396                                            0410
*         END;                      /*                       @YM08163*/
*       SWVA=JCTCKTTR;              /* INIT FOR COMMON READ          */
@RF00393 L     @12,SWEPAPTR                                        0412
         L     @11,JCTPTR                                          0412
