*                                      LAST                          */
         L     @12,SCTPTR                                          1855
         CLC   TEMPPTR(3),SCTLSIOT(@12)                            1855
         BNE   @RF01855                                            1855
*               TEMPSW=NOTON;       /* SIOT HAS BEEN EXAMINED        */
         NI    TEMPSW,B'01111111'                                  1856
*             TEMPPTR=TEMPPTR+REALADD;/* SWA ADDR'ABILITY            */
@RF01855 L     @12,TEMPPTR-1                                       1857
         LA    @12,0(,@12)                                         1857
         L     @11,REALADD-1                                       1857
         LA    @11,0(,@11)                                         1857
         ALR   @12,@11                                             1857
         STCM  @12,7,TEMPPTR                                       1857
*             IF TEMPPTR->SIOTUNAF  /* IF REQUESTED AFFINITY NUMBER  */
*                 >LALOCCTR THEN    /* IS GREATER THAN LAST DD NUMBER*/
         LH    @11,SIOTUNAF(,@12)                                  1858
         CH    @11,LALOCCTR                                        1858
         BNH   @RF01858                                            1858
*               TEMPPTR->SIOTUNAF=TEMPPTR->SIOTUNAF+ONEHEX;/* BUMP IT
