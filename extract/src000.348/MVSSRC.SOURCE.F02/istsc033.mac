*   /*****************************************************************/
*                                                                  0162
*S33C0600:                                                         0162
*   $RCVRFLG=OFF;                   /* SET RCM RECALL FLAG OFF       */
@RF00141 DS    0H                                                  0162
S33C0600 NI    $RCVRFLG(WAPTR),B'11111101'                         0162
*   IF RPLRTNCD>'04'X THEN          /* BAD RC FROM RECEIVE           */
         CLI   RPLRTNCD+1856(WAPTR),4                              0163
         BNH   @RF00163                                            0163
*     DO;                           /* YES                           */
*       R15=12;                                                    0165
         LA    R15,12                                              0165
*       GOTO S33C9999;                                             0166
         B     S33C9999                                            0166
*     END;                                                         0167
*   RFY                                                            0168
*     TECBMAPS BASED($RCVTECB);                                    0168
@RF00163 DS    0H                                                  0169
*   IF TECBSNOC<255 THEN            /* REACHED MAX COUNT?            */
         L     @14,RCVPTR                                          0169
         L     @14,$RCVTECB(,@14)                                  0169
         CLI   TECBSNOC(@14),255                                   0169
         BNL   @RF00169                                            0169
*     TECBSNCT=TECBSNCT+1;          /* INCREASE COUNT                */
         LA    @04,1                                               0170
         SLR   @03,@03                                             0170
         IC    @03,TECBSNCT(,@14)                                  0170
         ALR   @04,@03                                             0170
         STC   @04,TECBSNCT(,@14)                                  0170
*   IF TECBSNOC<=TECBSNCT THEN      /* ROOM LEFT IN TECB             */
@RF00169 L     @14,RCVPTR                                          0171
         L     @14,$RCVTECB(,@14)                                  0171
         CLC   TECBSNOC(1,@14),TECBSNCT(@14)                       0171
         BH    @RF00171                                            0171
*     DO;                           /* YES                           */
*       N=MIN(TECBSNLN-3,25);       /* FIND MIN LENGTH FOR MOVE      */
         LA    @04,3                                               0173
         LH    N,TECBSNLN(,@14)                                    0173
         SLR   N,@04                                               0173
         LA    @14,25                                              0173
         CR    N,@14                                               0173
         BNH   *+6
         LR    N,@14                                               0173
*       IF N>3 THEN                 /* LENGTH = 0?                   */
         CR    N,@04                                               0174
         BNH   @RF00174                                            0174
*         DO;                       /* NO                            */
*                                                                  0175
*           /*********************************************************/
*           /*                                                       */
*           /* MOVE RPL DATA TO TECB                                 */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0176
*           RFY                                                    0176
*             TECBSEND BASED(ADDR($INBUFR));                       0176
*           TCBSSEN=RPLSSEI;        /* YES,SYSTEM SENSE TO BUFFER    */
         LA    @14,2104                                            0177
         ALR   @14,WAPTR                                           0177
         LA    @09,1856                                            0177
         ALR   @09,WAPTR                                           0177
         MVC   TCBSSEN(1,@14),RPLSSEI(@09)                         0177
*           TCBSSENM=RPLSSMI;       /* YES, MODIFIER TO BUFFER       */
         IC    @04,RPLSSMI(,@09)                                   0178
         STC   @04,TCBSSENM(,@14)                                  0178
*           TCBUSEN=RPLUSNSI;       /* YES, USER SENSE TO BUF        */
         MVC   TCBUSEN(2,@14),RPLUSNSI(@09)                        0179
*           TCBRESP=RPLVTFL2;       /* YES, RESPOND TO BUF           */
         LA    @04,1872                                            0180
         ALR   @04,WAPTR                                           0180
         MVC   TCBRESP(1,@14),RPLVTFL2(@04)                        0180
*           TCBRECLN=RPLRLEN;       /* YES, RECLEN TO BUF            */
         MVC   TCBRECLN(2,@14),RPLRLEN+2(@09)                      0181
*           TCBRTYPE=RPLSRTYP;      /* YES, RTYPE TO BUF             */
         MVC   TCBRTYPE(1,@14),RPLSRTYP(@04)                       0182
*           TCBRKCHG=RPLRH3;        /* BRACKET FIELD TO BUF          */
         MVC   TCBRKCHG(1,@14),RPLRH3(@04)                         0183
*           TCBCHAIN=RPLCHN;        /* CHAIN FIELD TO BUF            */
         MVC   TCBCHAIN(1,@14),RPLCHN(@04)                         0184
*           TCBCNTRL=RPLCNTDF;      /* CONTROL TO BUF                */
         XC    TCBCNTRL+1(2,@14),TCBCNTRL+1(@14)                   0185
         MVC   TCBCNTRL(1,@14),RPLCNTDF(@04)                       0185
*           TCBSEQNO=RPLSEQNO;      /* SEQ NUM TO BUF                */
         LA    @04,1916                                            0186
         ALR   @04,WAPTR                                           0186
         MVC   TCBSEQNO(2,@14),RPLSEQNO(@04)                       0186
*           TCBSIGDA=RPLSIGDA;      /* SIGNAL DATA TO BUF            */
         MVC   TCBSIGDA(4,@14),RPLSIGDA(@09)                       0187
*           TCBIBSQ=RPLIBSQ;        /* IBSQAC TO BUF                 */
         MVC   TCBIBSQ(1,@14),RPLIBSQ(@04)                         0188
*           TCBIBSQL=RPLIBSQV;      /* IBSQVAL TO BUF                */
         MVC   TCBIBSQL(2,@14),RPLIBSQV(@04)                       0189
*           TCBOBSQ=RPLOBSQ;        /* OBSQAC TO BUF                 */
         MVC   TCBOBSQ(1,@14),RPLOBSQ(@04)                         0190
*           TCBOBSQL=RPLOBSQV;      /* OBSQVAL TO BUF                */
         MVC   TCBOBSQL(2,@14),RPLOBSQV(@04)                       0191
*           $INBUFR(1:25)=$INBUFR(1:25)&BUFMASK(1:25);/* N OUT UNUSED
*                                      FLAGS                         */
         NC    $INBUFR(25,WAPTR),BUFMASK                           0192
*           IF RPLFMHDR=ON THEN     /* FMHDR RECEIVED         @DM0911*/
         TM    RPLFMHDR(@09),B'00000001'                           0193
         BNO   @RF00193                                            0193
*             TCBRTYPE=TCBRTYPE×'80'X;/* YES, SET TECB BIT    @DM0911*/
         OI    TCBRTYPE(@14),X'80'                                 0194
*           TCBSENSE(1:N)=$INBUFR(1:N);/* MOVE SENSE INFO TO TECB    */
@RF00193 L     @14,RCVPTR                                          0195
         L     @14,$RCVTECB(,@14)                                  0195
         LA    @09,TECBEVT1(,@14)                                  0195
         SLR   @07,@07                                             0195
         IC    @07,TECBFDCT(,@14)                                  0195
         SLR   @04,@04                                             0195
         IC    @04,TECBFDLN(,@14)                                  0195
         MR    @06,@04                                             0195
         ALR   @09,@07                                             0195
         SLR   @04,@04                                             0195
         IC    @04,TECBSNOC(,@14)                                  0195
         MH    @04,TECBSNLN(,@14)                                  0195
         ALR   @09,@04                                             0195
         LR    @14,N                                               0195
         BCTR  @14,0                                               0195
         EX    @14,@SM01124                                        0195
*         END;                                                     0196
