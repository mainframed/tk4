*     DO;                           /* GIVE BACK BAD RETURN CODE   0116
*                                                            @ZD10889*/
*       R15=12;                     /* SET RETURN CODE       @ZD10889*/
         LA    R15,12                                              0117
*       GOTO S33C9999;              /* EXIT                  @ZD10889*/
         B     S33C9999                                            0118
*     END;                          /* END BAD RETURN CODE PROCESSING
