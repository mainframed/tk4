*   RPLKEEP=ON;                     /* TURN KEEP FLAG ON IN RPL      */
S33C0300 DS    0H                                                  0090
*   RPLTRUNC=OFF;                   /* TURN TRUNC FLAG OFF           */
*   RPLNIBTK=OFF;                   /* TURN NIBTK FLAG OFF   @AZ17856*/
         LA    @14,1856                                            0091
         ALR   @14,WAPTR                                           0091
         OI    RPLKEEP(@14),B'01000000'                            0091
         NI    RPLTRUNC(@14),B'11001111'                           0091
*   IF $RCVTRC=ON THEN              /* TRUNCATE EXCESS DATA          */
         L     @08,RCVPTR                                          0092
         TM    $RCVTRC(@08),B'10000000'                            0092
         BNO   @RF00092                                            0092
*     DO;                           /* YES                           */
*       RPLKEEP=OFF;                /* TURN KEEP OFF                 */
*       RPLTRUNC=ON;                /* TRUNC EXCESS DATA ON          */
         OI    RPLTRUNC(@14),B'00100000'                           0095
         NI    RPLKEEP(@14),B'10111111'                            0095
*     END;                                                         0096
*   RFY                                                            0097
*    (R1,                                                          0097
*     R3,                                                          0097
*     R4,                                                          0097
*     R5) RSTD;                                                    0097
@RF00092 DS    0H                                                  0098
*   R1=ADDR($TLTRPL3);              /* GET RPL ADDRESS               */
         LA    R1,$TLTRPL3(,WAPTR)                                 0098
*   R3=$RCVARA;                     /* OLT DATA AREA                 */
         L     @14,RCVPTR                                          0099
         L     R3,$RCVARA(,@14)                                    0099
*   R4=$RCVARL;                     /* DATA AREA LENGTH              */
         LH    R4,$RCVARL(,@14)                                    0100
*   R5=$CID(I);                     /* CID OF DEVICE FOR RECEIVE     */
         LR    @08,I                                               0101
         MH    @08,@CH00713                                        0101
         ST    @08,@TF00001                                        0101
         ALR   @08,WAPTR                                           0101
         ICM   R5,15,$CID-68(@08)                                  0101
*   IF $RCVSESS=ON THEN             /* SESSION PARMS REQUESTED       */
         TM    $RCVSESS(@14),B'01000000'                           0102
         BNO   @RF00102                                            0102
*     DO;                           /* YES                           */
*       N=MIN(R4,7);                /* GET BIND LENGTH        @DM0903*/
         LA    N,7                                                 0104
         CR    N,R4                                                0104
         BNH   *+6
         LR    N,R4                                                0104
