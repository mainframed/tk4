*   IF R15^=0 THEN                  /* BAD CID                       */
         LTR   R15,R15                                             0129
         BZ    @RF00129                                            0129
*     DO;                           /* YES                           */
*       R15=12;                                                    0131
         LA    R15,12                                              0131
