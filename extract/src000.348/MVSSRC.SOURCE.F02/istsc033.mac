         ST    R14,SVESC033+4(,WAPTR)                              0081
*   RFY                                                            0082
*    (R13,                                                         0082
*     R14) UNRSTD;                                                 0082
*   IF $RCVRFLG=ON THEN             /* RECALL FROM RCM               */
         TM    $RCVRFLG(WAPTR),B'00000010'                         0083
         BO    @RT00083                                            0083
