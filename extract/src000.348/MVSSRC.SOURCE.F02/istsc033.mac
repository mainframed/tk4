*                                      MOVE                   @DM0903*/
         BCTR  N,0                                                 0105
*       R15=12;                     /* UNRECOV ERROR                 */
         LA    R15,12                                              0106
*       IF N<=0 THEN                /* IF TO SHORT TO MOVE DATA    0107
*                                                             @DM0903*/
         LTR   N,N                                                 0107
         BNP   @RT00107                                            0107
*         GO TO S33C9999;           /* EXIT ON ERROR          @DM0903*/
*       R15=0;                      /* SET GOOD RETURN CODE   @DM0903*/
         SLR   R15,R15                                             0109
