         B     S33C9999                                            0132
*     END;                                                         0133
*   RFY                                                            0134
*    (R3,                                                          0134
*     R4) UNRSTD;                                                  0134
@RF00129 DS    0H                                                  0135
*   IF $RCVMSG=ON THEN              /* RECEIVE DONE?                 */
         TM    $RCVMSG(WAPTR),B'00000001'                          0135
         BNO   @RF00135                                            0135
*     DO;                           /* NO                            */
*       $RCVRFLG=ON;                /* TELL RCM TO RECALL            */
         OI    $RCVRFLG(WAPTR),B'00000010'                         0137
*       GOTO S33C9999;              /* RETURN                        */
         B     S33C9999                                            0138
*     END;                                                         0139
*   GOTO S33C0600;                  /* RECEIVE DONE CONTINUE         */
         B     S33C0600                                            0140
*S33C0500:                                                         0141
*   IF $RCVMSG=ON THEN              /* RECEIVE DONE?                 */
S33C0500 TM    $RCVMSG(WAPTR),B'00000001'                          0141
         BNO   @RF00141                                            0141
*     DO;                           /* NO, CHECK TIME                */
*       CALL ISTSC026;              /* GET CURRENT TIME              */
         L     @15,@CV01081                                        0143
         BALR  @14,@15                                             0143
*       R5=RPLARG;                  /* PICK UP CID                   */
         L     R5,RPLARG+1856(,WAPTR)                              0144
*       IF R1<$WAITIME THEN         /* TIME EXPIRED?                 */
         CL    R1,$WAITIME(,WAPTR)                                 0145
         BL    @RT00145                                            0145
*         GOTO S33C9999;            /* NO RETURN TO RCM              */
*       CALL S33C2000;              /* ISSUE A RESETSR               */
         BAL   @14,S33C2000                                        0147
*       IF R15=12 THEN              /* BAD RC FROM RESETSR           */
         C     R15,@CF00373                                        0148
         BNE   @RF00148                                            0148
*         DO;                       /* ERROR RETURN CODE FROM RESETSR*/
*           $RCVMSG=OFF;            /* RESET RECEIVE OUT FLAG        */
*           $RCVRFLG=OFF;           /* RESET RECALL FLAG             */
         NI    $RCVMSG(WAPTR),B'11111100'                          0151
*           GOTO S33C9999;                                         0152
         B     S33C9999                                            0152
*         END;                      /* END OF BAD RET CODE FROM    0153
*                                      RESETSR                       */
*       IF $RCVMSG=ON THEN          /* RECEIVE COMPLETE BEFOR RST    */
@RF00148 TM    $RCVMSG(WAPTR),B'00000001'                          0154
         BNO   @RF00154                                            0154
*         DO;                       /* NO, RESETSR DID IT            */
*           $RCVMSG=OFF;            /* RESET RECEIVE ISSUED FLAG     */
*           $RCVRFLG=OFF;           /* SET RCM RECALL FLAG OFF       */
         NI    $RCVMSG(WAPTR),B'11111100'                          0157
*           R15=16;                 /* SET TIMEOUT RC FOR OLT        */
         LA    R15,16                                              0158
*           GOTO S33C9999;          /* RETURN TO OLT                 */
         B     S33C9999                                            0159
*         END;                                                     0160
*     END;                                                         0161
*                                                                  0161
@RF00154 DS    0H                                                  0162
