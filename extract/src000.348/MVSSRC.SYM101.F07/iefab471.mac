         TITLE 'IEFAB471 - GENERIC ALLOCATION CONTROL                  *
                        '
IEFAB471 CSECT ,                                                01S0001
@MAINENT DS    0H                                               01S0001
         USING *,@15                                            01S0001
         B     @PROLOG                                          01S0001
         DC    AL1(24)                                          01S0001
         DC    C'IEFAB471 78124  UZ18622 '                      01S0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                  01S0001
         BALR  @12,0                                            01S0001
@PSTART  DS    0H                                               01S0001
         USING @PSTART,@12                                      01S0001
         L     @00,@SIZDATD                                     01S0001
         MODESET EXTKEY=SCHED,SAVEKEY=(2)
         GSPACE   R,LV=(0)
         LR    @11,@01                                          01S0001
         USING @DATD,@11                                        01S0001
         ST    @13,@SA00001+4                                   01S0001
         STC   @02,@OLDKEY                                      01S0001
         MODESET KEYADDR=(2)
         LM    @00,@02,20(@13)                                  01S0001
         ST    @11,8(,@13)                                      01S0001
         LR    @13,@11                                          01S0001
         MODESET EXTKEY=ZERO
         MVC   @PC00001(4),0(@01)                               01S0001
         MODESET EXTKEY=SCHED
*RTNCODE = SUCCESS;                    /*INITIALIZE RETURN CODE      */
         SLR   RTNCODE,RTNCODE                                  01S0085
*RFY AQMPARAM BASED(QMGRRBP);                                   01S0086
*RFY EDTHDR BASED(EDTABPTR);           /*BASE EDT ON ALCWA POINTER   */
*RFY GENENTRY BASED(LUVGENP);          /*BASE EDT LUV ENTRY ON LUV   */
*RFY GRPCOUNT BASED(GRPCTPTR);                                  01S0089
*NUMGROUP = GMTENTLN*EIGHT;            /*DETERMINE NUMBER OF GROUPS  */
         L     @07,@PC00001                                     01S0090
         L     @01,ALCWAPTR(,@07)                               01S0090
         L     @01,EDTABPTR(,@01)                               01S0090
         L     @01,EDTMSKTP(,@01)                               01S0090
         L     NUMGROUP,GMTENTLN(,@01)                          01S0090
         SLA   NUMGROUP,3                                       01S0090
*CALL IEFAB472(ALCWAPTR);              /*SETUP FOR GENERIC PROCESSING*/
         ST    @07,@AL00001                                     01S0091
         L     @15,@CV00129                                     01S0091
         LA    @01,@AL00001                                     01S0091
         BALR  @14,@15                                          01S0091
*RTNCODE=REG15;                                                 01S0092
         LR    RTNCODE,REG15                                    01S0092
*ENQLIST = NOWAITNQ;                   /*MOVE ENQ LIST FORM INTO
*                                        GOTTEN CORE                 */
         MVC   ENQLIST(12),NOWAITNQ                             01S0093
*LASTDONE = YES;                       /*ALL ALLOC'D BY GEN SO FAR   */
*PARTALG = OFF;                        /*ALG FULL COVER SO FAR       */
         OI    LASTDONE,B'10000000'                             01S0095
         NI    PARTALG,B'11101111'                              01S0095
*DEFPRESP = ADDR(PREFTAB);             /*POINT TO DEVICE PRES LIST   */
         L     @08,@CA00133                                     01S0096
         ST    @08,DEFPRESP                                     01S0096
*PRESENTP = DEFPRESP + LENGTH(PRESHDR);/*SET PTR TO FIRST GENERIC    */
         AH    @08,@CH00076                                     01S0097
         ST    @08,PRESENTP                                     01S0097
*DO GENCTR = ONE TO PRESNUM            /*PROCESS PRECEDENCE LIST     */
*  WHILE (TOTREQS ^= ZERO              /*UNTIL ALL REQS ARE SATISFIED*/
*  & INDRETRY = OFF                                             01S0098
*  & RTNCODE = SUCCESS);               /*OR UNTIL AN ERROR IS FOUND  */
         LA    @08,1                                            01S0098
         B     @DE00098                                         01S0098
@DL00098 SLR   @08,@08                                          01S0098
         L     @07,@PC00001                                     01S0098
         L     @07,ALCWAPTR(,@07)                               01S0098
         C     @08,TOTREQS(,@07)                                01S0098
         BE    @DC00098                                         01S0098
         TM    INDRETRY(@07),B'01000000'                        01S0098
         BNZ   @DC00098                                         01S0098
         CR    RTNCODE,@08                                      01S0098
         BNE   @DC00098                                         01S0098
*  LUVPTR = EDTLUVSP + LENGTH(LUVHDR); /*POINT TO FIRST EDTLUV ENTRY */
         L     @08,EDTABPTR(,@07)                               01S0099
         LA    LUVPTR,8                                         01S0099
         AL    LUVPTR,EDTLUVSP(,@08)                            01S0099
*  LUVFOUND = NO;                      /*INITIALIZE SWITCH TO OFF    */
         NI    LUVFOUND,B'01111111'                             01S0100
*  DO LUVCTR = ONE TO LUVENTNO         /*SCAN EDT LOOKUP VALUES      */
*    WHILE (LUVFOUND = NO);            /*UNTIL GENERICS ENTRY FOUND  */
         LA    LUVCTR,1                                         01S0101
         B     @DE00101                                         01S0101
@DL00101 TM    LUVFOUND,B'10000000'                             01S0101
         BNZ   @DC00101                                         01S0101
*    IF PRESUNAM = UNITNAME THEN       /*IS THIS THE GENERICS ENTRY  */
         L     @08,PRESENTP                                     01S0102
         CLC   PRESUNAM(8,@08),UNITNAME(LUVPTR)                 01S0102
         BNE   @RF00102                                         01S0102
*      DO;                             /*YES ...                     */
*        LUVFOUND = YES;               /*INDICATE LUV FOR GEN FOUND  */
         OI    LUVFOUND,B'10000000'                             01S0104
*        IF LUVAGMSK = YES THEN        /*IS THERE AN ALT GROUP MASK  */
         TM    LUVAGMSK(LUVPTR),B'01000000'                     01S0105
         BNO   @RF00105                                         01S0105
*          DO;                         /*YES AN ALT GROUP MASK EXISTS*/
*            GENMASKP = LUVAGMTP;      /*USE ALTERNATE MASK          */
         L     GENMASKP,LUVAGMTP(,LUVPTR)                       01S0107
*          END;                                                 01S0108
*        ELSE                          /*NO ALTERNATE MASK EXISTS    */
*          DO;                                                  01S0109
         B     @RC00105                                         01S0109
@RF00105 DS    0H                                               01S0110
*            GENMASKP = LUVGMTP;       /*USE REGULAR MASK            */
         L     GENMASKP,LUVGMTP(,LUVPTR)                        01S0110
*          END;                                                 01S0111
*      END;                                                     01S0112
*    ELSE                             /*NOT THE GENERIC LUV ENTRY   */
*    LUVPTR = LUVPTR + LUVENTLN;       /*POINT TO NEXT LUV ENTRY     */
         B     @RC00102                                         01S0113
@RF00102 L     @08,@PC00001                                     01S0113
         L     @08,ALCWAPTR(,@08)                               01S0113
         L     @08,EDTABPTR(,@08)                               01S0113
         L     @08,EDTLUVSP(,@08)                               01S0113
         AL    LUVPTR,LUVENTLN(,@08)                            01S0113
*  END;                                                         01S0114
@RC00102 AH    LUVCTR,@CH00052                                  01S0114
@DE00101 L     @08,@PC00001                                     01S0114
         L     @08,ALCWAPTR(,@08)                               01S0114
         L     @08,EDTABPTR(,@08)                               01S0114
         L     @08,EDTLUVSP(,@08)                               01S0114
         C     LUVCTR,LUVENTNO(,@08)                            01S0114
         BNH   @DL00101                                         01S0114
@DC00101 DS    0H                                               01S0115
*  IF LUVFOUND = YES THEN              /*WAS GENERIC IN EDT?         */
         TM    LUVFOUND,B'10000000'                             01S0115
         BNO   @RF00115                                         01S0115
*    DO;                               /*YES, PROCESS                */
*/********************************************************************/
*/*                                                                  */
*/* SEGMENT NAME - DETLOCK                                           */
*/*                                                                  */
*/* DESCRIPTIVE NAME - DETERMINE GROUPS TO LOCK FOR GENERIC          */
*/*                                                                  */
*/* FUNCTION -                                                       */
*/*     THIS ROUTINE DETERMINES WHICH DEVICE GROUPS, OF THE          */
*/*     GENERIC CHOSEN, MUST BE LOCKED.                              */
*/*                                                                  */
*/*   OPERATION -                                                    */
*/*       FIRST THIS ROUTINE DETERMINES IF AN UNALLOCATED SIOT,      */
*/*       WHICH IS ELIGIBLE TO THIS GENERIC, MUST LOCK ALL THE       */
*/*       GROUPS IN THE GENERIC (SIOTRTRY = 1).IF NO SIOT NEEDS THE  */
*/*       WHOLE GENERIC, THE REQUEST ID MASK TABLE (RIMT) IS         */
*/*       USED TO DETERMINE WHICH GROUPS ARE NEEDED. THE MASK OF     */
*/*       GROUPS NEEDED IS THEN 'AND'ED WITH THE GENERIC GROUP MASK. */
*/*       THE RESULTING MASK INDICATES THE GENERICS GROUP WHICH      */
*/*       MUST BE LOCKED.                                            */
*/* ENTRY = (INLINE SUBROUTINE)                                      */
*/*                                                                  */
*/*   PURPOSE - (SEE FUNCTION)                                       */
*/*                                                                  */
*/*   INPUT - GENLOKSW - ALLOCATION FUNCTION MAP 'RETRY' INDICATOR   */
*/*         - SIOT1P - PTR TO FIRST SIOT IN CHAIN                    */
*/*         - GENDEVT - GENERIC TYPE INDICATOR                       */
*/*         - GENMASKP - PTR TO GENERIC GROUP MASK                   */
*/*         - RIMTABP  - PTR TO REQUEST ID MASK TABLE                */
*/*                                                                  */
*/* EXIT - (INLINE SUBROUTINE)                                       */
*/*                                                                  */
*/*   CONDITION - MASK INDICATING THE GENERIC DEVICE GROUPS TO       */
*/*               BE LOCKED HAS BEEN SET UP.                         */
*/*                                                                  */
*/*   OUTPUT - NEEDLOCK - MASK OF DEVICE GROUPS TO LOCK              */
*/*                                                                  */
*/*   RETURN CODE - (NONE)                                           */
*/*                                                                  */
*/********************************************************************/
*USERIMSW = YES;                       /*ASSUME NO LOCK-WHOLE-GENERIC*/
         OI    USERIMSW,B'10000000'                             02S0117
*IF GENLOKSW = ON THEN                 /*IS THIS A RETRY ?           */
         L     @08,@PC00001                                     02S0118
         L     @08,ALCWAPTR(,@08)                               02S0118
         L     @07,FMAPPTR(,@08)                                02S0118
         TM    GENLOKSW(@07),B'00000001'                        02S0118
         BNO   @RF00118                                         02S0118
*  DO;                                 /*YES THIS IS A RETRY         */
*    SIOTPTR = SIOT1P;                 /*POINT TO FIRST SIOT         */
         L     SIOTPTR,SIOT1P(,@08)                             02S0120
*    DO WHILE (SIOTPTR^=ZERO & USERIMSW=YES); /*SCAN ALL SIOTS       */
         B     @DE00121                                         02S0121
@DL00121 DS    0H                                               02S0122
*      IF SIOTALCD=NO & SIOTRTRY=ON THEN /*IS SIOT UNALLOC'D + MARKED*/
         TM    SIOTALCD(SIOTPTR),B'00000010'                    02S0122
         BNZ   @RF00122                                         02S0122
         TM    SIOTRTRY(SIOTPTR),B'00000001'                    02S0122
         BNO   @RF00122                                         02S0122
*        DO;                           /*YES, PROCESS                */
*          EDLPTR = EDLFGENP;          /*POINT TO FIRST GEN IN EDL   */
         L     @08,SIOTEDLP(,SIOTPTR)                           02S0124
         L     EDLPTR,EDLFGENP(,@08)                            02S0124
*          DO EGENCTR = ONE TO EDLGENNO /*SCAN ALL GENERICS IN EDL   */
*               WHILE (USERIMSW = YES);                         02S0125
         LA    EGENCTR,1                                        02S0125
         B     @DE00125                                         02S0125
@DL00125 TM    USERIMSW,B'10000000'                             02S0125
         BNO   @DC00125                                         02S0125
*            IF (EINDEVTP=OFF & GENDEVT=EDLDEVT) THEN /*ELIGIBLE TO..
*                                       ..THIS GENERIC ?             */
         TM    EINDEVTP(EDLPTR),B'10000000'                     02S0126
         BNZ   @RF00126                                         02S0126
         L     @08,LUVGENP(,LUVPTR)                             02S0126
         CLC   GENDEVT(4,@08),EDLDEVT(EDLPTR)                   02S0126
         BNE   @RF00126                                         02S0126
*              DO;                     /*YES,MUST LOCK WHOLE GENERIC */
*                USERIMSW = NO;        /*INDICATE LOCKING WHOLE GEN  */
         NI    USERIMSW,B'01111111'                             02S0128
*                NEEDLOCK(ONE:GMTENTLN)=GENMASK(ONE:GMTENTLN);/*
*                                                  SET LOCK=WHOLE GEN*/
         L     @08,@PC00001                                     02S0129
         L     @08,ALCWAPTR(,@08)                               02S0129
         L     @07,WMASKPTR(,@08)                               02S0129
         L     @08,EDTABPTR(,@08)                               02S0129
         L     @08,EDTMSKTP(,@08)                               02S0129
         L     @08,GMTENTLN(,@08)                               02S0129
         BCTR  @08,0                                            02S0129
         EX    @08,@SM01970                                     02S0129
*              END;                                             02S0130
*            ELSE;                     /*NOT ELIGIBLE TO THIS GENERIC*/
@RF00126 DS    0H                                               02S0132
*            EDLPTR = EDLNXTNT;        /*POINT TO NEXT GENERIC IN EDL*/
         L     EDLPTR,EDLNXTNT(,EDLPTR)                         02S0132
*          END;                                                 02S0133
         AH    EGENCTR,@CH00052                                 02S0133
@DE00125 L     @08,SIOTEDLP(,SIOTPTR)                           02S0133
         C     EGENCTR,EDLGENNO(,@08)                           02S0133
         BNH   @DL00125                                         02S0133
@DC00125 DS    0H                                               02S0134
*        END;                                                   02S0134
*      ELSE;                           /*SIOT NOT UNALLOC'D + MARKED */
@RF00122 DS    0H                                               02S0136
*      SIOTPTR = SIOTNPTR;             /*POINT TO THE NEXT SIOT      */
         L     SIOTPTR,SIOTNPTR(,SIOTPTR)                       02S0136
*    END;                                                       02S0137
@DE00121 LTR   SIOTPTR,SIOTPTR                                  02S0137
         BZ    @DC00121                                         02S0137
         TM    USERIMSW,B'10000000'                             02S0137
         BO    @DL00121                                         02S0137
@DC00121 DS    0H                                               02S0138
*  END;                                                         02S0138
*ELSE;                                 /*NOT RETRY                   */
@RF00118 DS    0H                                               02S0140
*IF USERIMSW = YES THEN                /*IS WHOLE GENERIC TO BE LOCKD*/
         TM    USERIMSW,B'10000000'                             02S0140
         BNO   @RF00140                                         02S0140
*  DO;                                 /*NO,DETERMINE GROUPS TO LOCK */
*    RIMNEEDM(ONE:GMTENTLN)=RIMNEEDM(ONE:GMTENTLN) && RIMNEEDM(ONE:GMTE
*TLN);                                                  /*ZERO MASK  */
         L     @08,@PC00001                                     02S0142
         L     @08,ALCWAPTR(,@08)                               02S0142
         L     @07,WMASK2P(,@08)                                02S0142
         L     @01,EDTABPTR(,@08)                               02S0142
         L     @01,EDTMSKTP(,@01)                               02S0142
         L     @15,GMTENTLN(,@01)                               02S0142
         BCTR  @15,0                                            02S0142
         EX    @15,@SX01972                                     02S0142
*    RIMTENTP = RIMTABP;               /*POINT TO FIRST RIMT ENTRY   */
         L     RIMTENTP,RIMTABP(,@08)                           02S0143
*    DO RIMCTR = ONE TO RIMTABCT;      /*SCAN ALL RIM TABLE ENTRIES  */
         LA    RIMCTR,1                                         02S0144
         B     @DE00144                                         02S0144
@DL00144 DS    0H                                               02S0145
*      IF RIMUACT  ^= ZERO THEN        /*ARE ALL ASSOC. REQS ALLOC'D?*/
         ICM   @08,15,RIMUACT(RIMTENTP)                         02S0145
         BZ    @RF00145                                         02S0145
*        DO;                           /*NO,MUST LOCK ASSOC. MASK    */
*          RIMNEEDM(ONE:GMTENTLN)=RIMNEEDM(ONE:GMTENTLN)×RIMMASK(ONE:GM
*ENTLN);                                                   /*ADD MASK*/
         L     @08,@PC00001                                     02S0147
         L     @08,ALCWAPTR(,@08)                               02S0147
         L     @07,WMASK2P(,@08)                                02S0147
         L     @08,EDTABPTR(,@08)                               02S0147
         L     @08,EDTMSKTP(,@08)                               02S0147
         L     @08,GMTENTLN(,@08)                               02S0147
         BCTR  @08,0                                            02S0147
         EX    @08,@SO01974                                     02S0147
*        END;                                                   02S0148
*      ELSE;                           /*ALL ASSOC. REQS ARE ALLOC'D */
@RF00145 DS    0H                                               02S0150
*      RIMTENTP = RIMTENTP + RIMTABLN; /*POINT TO NEXT RIMT ENTRY    */
         L     @08,@PC00001                                     02S0150
         L     @08,ALCWAPTR(,@08)                               02S0150
         AL    RIMTENTP,RIMTABLN(,@08)                          02S0150
*    END;                                                       02S0151
         AH    RIMCTR,@CH00052                                  02S0151
@DE00144 L     @08,@PC00001                                     02S0151
         L     @08,ALCWAPTR(,@08)                               02S0151
         C     RIMCTR,RIMTABCT(,@08)                            02S0151
         BNH   @DL00144                                         02S0151
*    NEEDLOCK(ONE:GMTENTLN)=GENMASK(ONE:GMTENTLN)               02S0152
*    & RIMNEEDM(ONE:GMTENTLN);         /*ELIMINATE OTHER        02S0152
*                                        GENERICS GROUP FROM LOCKMASK*/
         L     @08,@PC00001                                     02S0152
         L     @08,ALCWAPTR(,@08)                               02S0152
         L     @07,WMASKPTR(,@08)                               02S0152
         L     @02,EDTABPTR(,@08)                               02S0152
         L     @02,EDTMSKTP(,@02)                               02S0152
         L     @02,GMTENTLN(,@02)                               02S0152
         BCTR  @02,0                                            02S0152
         L     @08,WMASK2P(,@08)                                02S0152
         EX    @02,@SM01970                                     02S0152
         EX    @02,@SN01976                                     02S0152
*  END;                                                         02S0153
*ELSE;                                 /*WHOLE GENERIC TO BE LOCKED  */
@RF00140 DS    0H                                               02S0155
*      VOLSWOK = NO;                   /*ASSUME WHOLE GEN NOT NEEDED */
         NI    VOLSWOK,B'10111111'                              01S0155
*      ZMASK(ONE:GMTENTLN) = ZMASK(ONE:GMTENTLN) && ZMASK(ONE:GMTENTLN)
         L     @08,@PC00001                                     01S0156
         L     @08,ALCWAPTR(,@08)                               01S0156
         L     @07,WMASK2P(,@08)                                01S0156
         L     @02,EDTABPTR(,@08)                               01S0156
         L     @02,EDTMSKTP(,@02)                               01S0156
         L     @02,GMTENTLN(,@02)                               01S0156
         BCTR  @02,0                                            01S0156
         EX    @02,@SX01979                                     01S0156
*      IF NEEDLOCK(ONE:GMTENTLN)^= ZMASK(ONE:GMTENTLN)  THEN/*  01S0157
*                                                NEED GEN            */
         L     @08,WMASKPTR(,@08)                               01S0157
         EX    @02,@SC01981                                     01S0157
         BE    @RF00157                                         01S0157
*        DO;                           /*YES,GENERIC NEEDED..CONTINUE*/
*          IF NEEDLOCK(ONE:GMTENTLN)=GENMASK(ONE:GMTENTLN) THEN/*
*                                                   WHOLE GEN LOCKED?*/
         EX    @02,@SC01983                                     01S0159
         BNE   @RF00159                                         01S0159
*            DO;                       /*YES,                        */
*              VOLSWOK = YES;          /*INDICATE VOL SWITCHING OK   */
         OI    VOLSWOK,B'01000000'                              01S0161
*            END;                                               01S0162
*          ELSE;                       /*WHOLE GEN NOT LOCKED        */
@RF00159 DS    0H                                               01S0164
*          GEN;                        /*ALLOW UNLOADS,ETC           */
            DEQ   (,SYSRNAME,8),MF=(E,ENQLIST)
            DEQ   (,DDRDAMIN,5),RET=HAVE,MF=(E,ENQLIST)       @ZA32215
            DEQ   (,DDRTUMIN,7),RET=HAVE,MF=(E,ENQLIST)       @ZA32215
*          AQMECODE = LOCKCODE;        /*INDICATE GROUPS TO BE LOCKED*/
         L     @08,@PC00001                                     01S0165
         L     @08,ALCWAPTR(,@08)                               01S0165
         L     @07,QMGRRBP(,@08)                                01S0165
         MVC   AQMECODE(2,@07),@CH00101                         01S0165
*          AQMMASKP = WMASKPTR;        /*POINT TO LOCKMASK           */
         MVC   AQMMASKP(4,@07),WMASKPTR(@08)                    01S0166
*          CALL IEFAB4FA(QMGRRBP,EXITPRMP); /*LOCK GROUPS OF DEVICES */
         LA    @07,QMGRRBP(,@08)                                01S0167
         ST    @07,@AL00001                                     01S0167
         LA    @08,EXITPRMP(,@08)                               01S0167
         ST    @08,@AL00001+4                                   01S0167
         L     @15,@CV00132                                     01S0167
         LA    @01,@AL00001                                     01S0167
         BALR  @14,@15                                          01S0167
*          RTNCODE=REG15;              /*SAVE THE RETURN CODE        */
         LR    RTNCODE,REG15                                    01S0168
*          ENQLIST = NOWAITNQ;         /*REINITIALIZE ENQ PARM  01S0169
*                                        LIST                @ZA32215*/
         MVC   ENQLIST(12),NOWAITNQ                             01S0169
*          GEN;                        /*SERIALIZE UNLOAD,ETC        */
            ENQ   (,SYSRNAME,,8),MF=(E,ENQLIST)
*          IF DDRTPUR = ON THEN        /*ANY TAPE×UR REQUESTS@ZA32215*/
         L     @08,@PC00001                                     01S0171
         L     @08,ALCWAPTR(,@08)                               01S0171
         TM    DDRTPUR(@08),B'00000100'                         01S0171
         BNO   @RF00171                                         01S0171
*            DO;                       /*YES, ENQ DDR TAPE×UR@ZA32215*/
*              GEN REFS(ENQLIST);      /*                    @ZA32215*/
               ENQ (,DDRTUMIN,,7),MF=(E,ENQLIST)              @ZA32215
*            END;                      /*END ENQ DDR TAPE×UR @ZA32215*/
*          IF DDRDA = ON THEN          /*ANY DA REQUESTS     @ZA32215*/
@RF00171 L     @08,@PC00001                                     01S0175
         L     @08,ALCWAPTR(,@08)                               01S0175
         TM    DDRDA(@08),B'00000010'                           01S0175
         BNO   @RF00175                                         01S0175
*            DO;                       /*YES, ENQ DDR DA     @ZA32215*/
*              GEN REFS(ENQLIST);      /*                    @ZA32215*/
               ENQ (,DDRDAMIN,,5),MF=(E,ENQLIST)              @ZA32215
*            END;                      /*END ENQ DDR DA      @ZA32215*/
*          IF RTNCODE = SUCCESS THEN   /*WERE LOCKS OBTAINED         */
@RF00175 SLR   @08,@08                                          01S0179
         CR    RTNCODE,@08                                      01S0179
         BNE   @RF00179                                         01S0179
*            DO;                       /*YES,LOCKS OBTAINED..CONTINUE*/
*/********************************************************************/
*/*                                                                  */
*/* SEGMENT NAME - LOCKDSS                                           */
*/*                                                                  */
*/* DESCRIPTIVE NAME - PREVENT DSS FROM CHANGING LOCKED UCBS         */
*/*                                                                  */
*/* FUNCTION -                                                       */
*/*     THIS ROUTINE INFORMS DYNAMIC SUPPORT SYSTEM (DSS) PROCESSING */
*/*     WHEN THIS ALLOCATION HAS LOCKED  UP  TAPE OR UNIT RECORD     */
*/*     DEVICES AND THEREFORE DSS SHOULD NOT ALLOCATE TO SUCH        */
*/*     DEVICES.                                                     */
*/*                                                                  */
*/*   OPERATION - THE OPERATION IS DOCUMENTED BY THE CODE ITSELF.    */
*/*                                                                  */
*/* ENTRY - (INLINE SUBROUTINE)                                      */
*/*   PURPOSE - (SEE FUNCTION)                                       */
*/*   INPUT -                                                        */
*/*      GENDEVT - DEVICE TYPE OF THE GENERIC JUST LOCKED            */
*/*                                                                  */
*/* EXIT - NORMAL - (INLINE SUBROUTINE)                              */
*/*                                                                  */
*/*   CONDITIONS - FUNCTION COMPLETED.                               */
*/*                                                                  */
*/*   OUTPUT -                                                       */
*/*     .IF THIS IS THE FIRST TAPE GENERIC LOCKED,                   */
*/*            DSSTAPE - SET TO 1                                    */
*/*            CSDTCNT - INCREMENTED BY 1                            */
*/*     .IF THIS IS THE FIRST UNIT RECORD GENERIC LOCKED,            */
*/*            DSSUNIT - SET TO 1                                    */
*/*            CSDUCNT - INCREMENTED BY 1                            */
*/*                                                                  */
*/*   RETURN CODES - (NONE)                                          */
*/*                                                                  */
*/********************************************************************/
*IF GENDEVT(3) = TAPETYPE              /*IS THIS THE 1ST TAPE GEN..  */
*    & DSSTAPE = OFF THEN              /*..LOCKED ?                  */
         L     @07,LUVGENP(,LUVPTR)                             02S0181
         CLI   GENDEVT+2(@07),X'80'                             02S0181
         BNE   @RF00181                                         02S0181
         L     @07,@PC00001                                     02S0181
         L     @07,ALCWAPTR(,@07)                               02S0181
         TM    DSSTAPE(@07),B'00001000'                         02S0181
         BNZ   @RF00181                                         02S0181
