         TITLE 'IEFAB49B - DEVICE END POST HANDLER                     *
                        '
IEFAB49B CSECT ,                                                01S0001
@MAINENT DS    0H                                               01S0001
         USING *,@15                                            01S0001
         B     @PROLOG                                          01S0001
         DC    AL1(24)                                          01S0001
         DC    C'IEFAB49B 76267  UZ06233 '                      01S0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                  01S0001
         BALR  @10,0                                            01S0001
@PSTART  DS    0H                                               01S0001
         USING @PSTART,@10                                      01S0001
         L     @00,@SIZDATD                                     01S0001
         GSPACE   R,LV=(0)
         LR    @09,@01                                          01S0001
         USING @DATD,@09                                        01S0001
         ST    @13,@SA00001+4                                   01S0001
         LM    @00,@01,20(@13)                                  01S0001
         ST    @09,8(,@13)                                      01S0001
         LR    @13,@09                                          01S0001
         MVC   @PC00001(16),0(@01)                              01S0001
*DECRCNT = OFF;                        /*INIT COUNT DECREMENT SWITCH */
         NI    DECRCNT,B'01111111'                              01S0057
*ENQDQSW = ZERO;                       /*INIT ENQ/DEQ SWITCH         */
         MVI   ENQDQSW,X'00'                                    01S0058
*RETCODE = NOERROR;                    /*INIT RETURN CODE            */
         SLR   RETCODE,RETCODE                                  01S0059
*MTENTPTR = MVENTRYP;                  /*POINT TO FIRST MOUNT ENTRY  */
         L     @12,@PC00001                                     01S0060
         L     @12,MVCAPTR(,@12)                                01S0060
         L     @12,MVCAXPTR(,@12)                               01S0060
         L     MTENTPTR,MVENTRYP(,@12)                          01S0060
*  DO WHILE(MTENTPTR ^= ZERO &         /*LOOP THRU ALL MOUNT ENTRIES */
*    RETCODE = NOERROR);                                        01S0061
         B     @DE00061                                         01S0061
@DL00061 DS    0H                                               01S0062
*    NODEVENT =(MTLENGTH - LENGTH(MTENBASE))/(LENGTH(MTDOMIDS)  01S0062
*      + LENGTH(MTDEVENT));            /*CALC NUMBER OF DEV ENTRIES  */
         LH    @00,MTLENGTH(,MTENTPTR)                          01S0062
         SH    @00,@CH01602                                     01S0062
         SRDA  @00,32                                           01S0062
         D     @00,@CF00046                                     01S0062
         ST    @01,NODEVENT                                     01S0062
*    DO DEVCTR = ONE TO NODEVENT BY ONE/*LOOP THRU ALL DEVICE ENTRIES*/
*      WHILE(RETCODE = NOERROR);                                01S0063
         LA    DEVCTR,1                                         01S0063
         B     @DE00063                                         01S0063
@DL00063 LTR   RETCODE,RETCODE                                  01S0063
         BNZ   @DC00063                                         01S0063
*      IF MTVRYPND(DEVCTR) = ON THEN   /*VERIFY TO BE DONE ?         */
         LR    @12,DEVCTR                                       01S0064
         MH    @12,@CH01603                                     01S0064
         L     @11,MTDEVPTR(,MTENTPTR)                          01S0064
         ST    @12,@TF00001                                     01S0064
         ALR   @12,@11                                          01S0064
         AL    @12,@CF01615                                     01S0064
         TM    MTVRYPND(@12),B'10000000'                        01S0064
         BNO   @RF00064                                         01S0064
*        DO;                                                    01S0065
*          UCBPTR = MTUCBPTR(DEVCTR);  /*SET UP UCB BASE PTR         */
         L     @12,@TF00001                                     01S0066
         AL    @12,@CF01615                                     01S0066
         L     @12,MTUCBPTR-1(@12,@11)                          01S0066
         LA    @12,0(,@12)                                      01S0066
         ST    @12,UCBPTR                                       01S0066
*          IF UCBMOUNT = ON THEN       /*VERIFY ALREADY COMPLETED ?  */
         TM    UCBMOUNT(@12),B'10000000'                        01S0067
         BNO   @RF00067                                         01S0067
*            DO;                       /*NO                          */
*              IF UCBNOTRD = OFF THEN  /*DEVICE READY                */
         TM    UCBNOTRD(@12),B'01000000'                        01S0069
         BNZ   @RF00069                                         01S0069
*                DO;                   /*YES                         */
*                  IF MTVRYLBL(DEVCTR) = ON THEN /* VERIFY LABEL ?   */
         AL    @11,@TF00001                                     01S0071
         AL    @11,@CF01615                                     01S0071
         TM    MTVRYLBL(@11),B'01000000'                        01S0071
         BNO   @RF00071                                         01S0071
*                    DO;               /*YES                         */
*/********************************************************************/
*/*                                                                  */
*/* SEGMENT NAME - VERLABEL                                          */
*/*                                                                  */
*/* DESCRIPTIVE NAME - VERIFY DIRECT ACCESS VOLUME LABEL             */
*/*                                                                  */
*/* FUNCTION - THE FUNCTION OF THIS ROUTINE IS TO VERIFY THAT AN     */
*/*   ACCEPTABLE VOLUME HAS BEEN MOUNTED. IF THE MOUNT WAS FOR A     */
*/*   SPECIFIC VOLUME THEN THE LABEL OF THE MOUNTED VOLUME MUST      */
*/*   MATCH THAT IN THE UCB. IF THE MOUNT WAS FOR A NONSPECIFIC      */
*/*   VOLUME THEN THE LABEL MUST NOT BE A DUPLICATE OF ANOTHER       */
*/*   MOUNTED VOLUME.                                                */
*/*                                                                  */
*/*   OPERATION - ENQ ON VM&V RESOURCE. IF VOLSER HAS BEEN VERIFIED  */
*/*     DEQ FROM VM&V RESOURCE, DECREMENT NUMBER OF WAITS            */
*/*     TO BE DONE. IF VERIFY STILL TO BE DONE, CALL                 */
*/*     IEFAB4F8 TO READ THE DA LABEL. IF THERE WAS NO               */
*/*     ERROR READING THE LABEL,DETERMINE IF MOUNT WAS FOR SPECIFIC  */
*/*     VOLUME. IF SO,LABEL READ MUST MATCH VOLSER IN UCB.           */
*/*     IF NOT SPECIFIC VOLUME, SCAN ALL OTHER MOUNTED VOLUMES.      */
*/*     VOLSER IS OK IF NO OTHER VOLUME HAS THE SAME VOLSER.         */
*/*     IF VOLSER IS OK, DECREMENT THE COUNT OF WAITS TO             */
*/*     BE DONE AND REMOVE VERIFY PENDING INDICATOR FROM DEVICE      */
*/*     ENTRY. ISSUE MODESET TO GET KEY ZERO,OBTAIN L0CAL            */
*/*     AND CMS LOCKS. INDICATE UCB VERIFIED. CLEAR ATTENTION TABLE  */
*/*     INDEX. RELEASE LOCAL AND CMS LOCKS. ISSUE MODESET TO GET     */
*/*     SCHEDULER KEY. DEQ FROM VM&V RESOURCE.                       */
*/*     IF THE VOLSER WAS NOT SUCCESSFULLY READ, THEN VOL. MOUNT     */
*/*     AND VERIFY IS CALLED TO UNLOAD THE CURRENT VOLUME AND MOUNT  */
*/*     THE REQUIRED ONE. IF VERIFY FAILS FOR AN MSS         @Y30LPPE*/
*/*     VOLUME, THE RETURN CODE WILL BE SET TO 24 AND MESSAGE@Y30LPPE*/
*/*     IEF712I WILL BE ISSUED TO INDICATE THIS FAILURE.     @Y30LPPE*/
*/*     VOLUME MOUNT AND VERIFY IS CALLED TO UNLOAD THE      @Y30LPPE*/
*/*     VOLUME SERIAL IN THE UCB. OTHERWISE VM&V WILL        @Y30LPPE*/
*/*     CONSTRUCT A NEW MOUNT ENTRY AND PLACE IT IN THE              */
*/*     DOMID FOR THE MOUNT MESSAGE IT ISSUES. ON RETURN TO          */
*/*     THIS ROUTINE THE NEW MOUNT ENTRY WILL BE THE LAST            */
*/*     ON THE MOUNT ENTRY CHAIN. THE NEW MOUNT ENTRY CAN NOW        */
*/*     BE MARKED AS PROCESSED BECAUSE THE ORIGINAL MOUNT ENTRY      */
*/*     (FIRST MOUNT MESSAGE) STILL REPRESENTS THIS MOUNT. IN ORDER  */
*/*     FOR THE FIRST MOUNT MESSAGE TO BE REMOVED, THE DOMIDS IN THE */
*/*     TWO MOUNT ENTRIES MUST BE SWITCHED.                          */
*/*                                                                  */
*/*                                                                  */
*/* ENTRY - INLINE SUBROUTINE                                        */
*/*                                                                  */
*/*   PURPOSE - SEE FUNCTION                                         */
*/*                                                                  */
*/*   INPUT -                                                        */
*/*     UCBPTR - PTR TO UCB OF MOUNTED VOLUME                        */
*/*     MOUNT ENTRY FOR DEVICE WHICH HAS COME READY                  */
*/*                                                                  */
*/*                                                                  */
*/* EXIT -                                                           */
*/*                                                                  */
*/*   CONDITION - VOLUME SERIAL VERIFIED                             */
*/*                                                                  */
*/*     OUTPUT -                                                     */
*/*       UCBATI - IN UCB SET TO ZERO                                */
*/*       UCBMOUNT - IN UCB,SET TO ZERO                              */
*/*       UCBVOLI - IN UCB.CONTAINS SERIAL OF VOLUME MOUNTED         */
*/*       MVNUMWTS - IN MOUNT-VERIFY COMM AREA(MVCA),DECREMENTED BY  */
*/*                   ONE FOR EACH VOLUME VERIFIED                   */
*/*                                                                  */
*/*     RETURN CODE  - 0 IN RETCODE                                  */
*/*                                                                  */
*/*                                                                  */
*/*   CONDITION - IEFAB493 GETMAIN FAILURE                           */
*/*                                                                  */
*/*     OUTPUT - MVNUMWTS SET TO ZERO                                */
*/*                                                                  */
*/*     RETURN CODE - 4 IN RETCODE                                   */
*/*                                                                  */
*/*   CONDITION - VERIFY FAILED FOR AN MSS VOLUME.           @Y30LPPE*/
*/*                                                          @Y30LPPE*/
*/*     OUTPUT -                                             @Y30LPPE*/
*/*       UCBVOLI - IN UCB, SET TO ZERO                      @Y30LPPE*/
*/*       MESSAGE - IEF712I IS ISSUED                        @Y30LPPE*/
*/*                                                          @Y30LPPE*/
*/*     RETURN CODE - 24 IN RETCODE                          @Y30LPPE*/
*/*                                                          @Y30LPPE*/
*/********************************************************************/
*DOENQ = ON;                           /*INDICATE ENQ REQUIRED       */
         OI    DOENQ,B'10000000'                                02S0073
*CALL VMVENQDQ;                                                 02S0074
         BAL   @14,VMVENQDQ                                     02S0074
*DOENQ = OFF;                          /*CLEAR ENQ INDIC             */
         NI    DOENQ,B'01111111'                                02S0075
*VIRTFAIL = OFF;                       /*INITIALIZE SWITCH THAT SHOWS
*                                        THAT VERIFY FAILED FOR AN
*                                        MSS VOLUME          @Y30LPPE*/
*DUPEFAIL = OFF;                       /*INITIALIZE DUPEFAIL SWITCH
*                                                            @Y30LPPE*/
*ERRTYPE = OFF;                        /*INIT ERRTYPE SWITCH @ZM34521*/
*LOCKSW = OFF;                         /*NO LOCKS YET        @YM07608*/
         NI    VIRTFAIL,B'10000111'                             02S0079
*IF UCBMOUNT = ON THEN                 /*VOLUME ALREADY VERIFIED ?   */
         L     @12,UCBPTR                                       02S0080
         TM    UCBMOUNT(@12),B'10000000'                        02S0080
         BNO   @RF00080                                         02S0080
*  DO;                                 /*NO                          */
*    CALL IEFAB4F8(UCBPTR,ADDR(RECORD));/*READ DIRECT ACCESS VOLSER  */
         LA    @12,UCBPTR                                       02S0082
         ST    @12,@AL00001                                     02S0082
         LA    @12,RECORD                                       02S0082
         ST    @12,@TF00001                                     02S0082
         LA    @12,@TF00001                                     02S0082
         ST    @12,@AL00001+4                                   02S0082
         L     @15,@CV01585                                     02S0082
         LA    @01,@AL00001                                     02S0082
         BALR  @14,@15                                          02S0082
*    RETCODE = REG15;                  /*SAVE RETURN CODE            */
         LR    RETCODE,REG15                                    02S0083
*    IF RETCODE = NOERROR THEN         /*ANY ERRORS ?                */
         LTR   RETCODE,RETCODE                                  02S0084
         BNZ   @RF00084                                         02S0084
*      DO;                             /*NO                          */
*        IF UCBVOLI ^= ZEROVLSR THEN   /*VOLUME SERIAL SPECIFIC ?    */
         L     @12,UCBPTR                                       02S0086
         CLC   UCBVOLI(6,@12),ZEROVLSR                          02S0086
         BE    @RF00086                                         02S0086
*          DO;                         /*YES                         */
*            IF UCBVOLI = VOLSER THEN  /*VOLSER IN UCB = VOLSER READ?*/
         CLC   UCBVOLI(6,@12),VOLSER                            02S0088
         BNE   @RF00088                                         02S0088
*              VOLSEROK = ON;          /*YES,INDICATE SERIAL IS OK   */
         OI    VOLSEROK,B'00000100'                             02S0089
*            ELSE                                               02S0090
*              VOLSEROK = OFF;         /*WRONG VOLUME MOUNTED        */
         B     @RC00088                                         02S0090
@RF00088 NI    VOLSEROK,B'11111011'                             02S0090
*          END;                                                 02S0091
*        ELSE                          /*NON-SPECIFIC VOLUME REQUEST */
*          DO;                                                  02S0092
         B     @RC00086                                         02S0092
@RF00086 DS    0H                                               02S0093
*             DO; /*MODESET  KEYADDR(ZERO)*/                    02S0093
*               SPKA (ZERO); /*SET PSW KEY*/                    02S0094
         SLR   @12,@12                                          02S0094
         SPKA  0(@12)                                           02S0094
*             END; /*MODESET  KEYADDR(ZERO)*/   /*KEY 0 FOR LOCKING   @
*M07608*/                                                       02S0095
*             DO; /* SETLOCK (OBTAIN) MODE(UNCOND) TYPE(LOCAL) REGS(USE
* RELATED(LOCAL LOCK,IEFAB49B(VERLABEL)) */                     02S0096
*               RFY (GPR11P,GPR12P,GPR13P,GPR14P) RSTD;         02S0097
*               RFY (GPR15P,GPR00P,GPR01P) RSTD;                02S0098
*               GPR15P = GPR11P; /* SAVE REG. 11 IN REG. 15. */ 02S0099
         LR    GPR15P,GPR11P                                    02S0099
*               GPR00P = GPR12P; /* SAVE REG. 12 IN REG. 0. */  02S0100
         LR    GPR00P,GPR12P                                    02S0100
*               GPR01P = GPR13P; /* SAVE REG. 13 IN REG. 1. */  02S0101
         LR    GPR01P,GPR13P                                    02S0101
*               GPR13P = PSALITA; /* ADDRESS OF LOCK INTERFACE TABLE. *
         L     GPR13P,PSALITA                                   02S0102
*               GEN(LM GPR11P,GPR13P,576(GPR13P)); /* LOAD LOCKWORD, CL
*T AND ENTRY ADRS. */                                           02S0103
         LM GPR11P,GPR13P,576(GPR13P)
*               GEN(BALR GPR14P,GPR13P); /* BALR TO SETLOCK RTN. */
         BALR GPR14P,GPR13P
*               GPR11P = GPR15P; /* RESTORE REG. 11. */         02S0105
         LR    GPR11P,GPR15P                                    02S0105
*               GPR15P = GPR13P; /* SAVE RETURN CODE. */        02S0106
         LR    GPR15P,GPR13P                                    02S0106
*               GPR12P = GPR00P; /* RESTORE REG. 12 */          02S0107
         LR    GPR12P,GPR00P                                    02S0107
*               GPR13P = GPR01P; /* RESTORE REG. 13. */         02S0108
         LR    GPR13P,GPR01P                                    02S0108
*               RFY(GPR15P,GPR00P,GPR01P) UNRSTD;               02S0109
*               RFY (GPR11P,GPR12P,GPR13P,GPR14P) UNRSTD;       02S0110
*             END; /* SETLOCK (OBTAIN) MODE(UNCOND) TYPE(LOCAL) REGS(US
*) RELATED(LOCAL LOCK,IEFAB49B(VERLABEL)) */ /*GET LOCAL LOCK      @YM0
*608*/                                                          02S0111
*             DO; /* SETLOCK (OBTAIN) MODE(UNCOND) TYPE(CMS) REGS(USE)
*ELATED(CMS LOCK,IEFAB49B(VERLABEL)) */                         02S0112
*               RFY (GPR11P,GPR12P,GPR13P,GPR14P) RSTD;         02S0113
*               RFY (GPR15P,GPR00P,GPR01P) RSTD;                02S0114
*               GPR15P = GPR11P; /* SAVE REG. 11 IN REG. 15. */ 02S0115
         LR    GPR15P,GPR11P                                    02S0115
*               GPR00P = GPR12P; /* SAVE REG. 12 IN REG. 0. */  02S0116
         LR    GPR00P,GPR12P                                    02S0116
*               GPR01P = GPR13P; /* SAVE REG. 13 IN REG. 1. */  02S0117
         LR    GPR01P,GPR13P                                    02S0117
*               GPR13P = PSALITA; /* ADDRESS OF LOCK INTERFACE TABLE. *
         L     GPR13P,PSALITA                                   02S0118
*               GEN(LM GPR11P,GPR13P,540(GPR13P)); /* LOAD LOCKWORD, CL
*T AND ENTRY ADRS. */                                           02S0119
         LM GPR11P,GPR13P,540(GPR13P)
*               GEN(BALR GPR14P,GPR13P); /* BALR TO SETLOCK RTN. */
         BALR GPR14P,GPR13P
*               GPR11P = GPR15P; /* RESTORE REG. 11. */         02S0121
         LR    GPR11P,GPR15P                                    02S0121
*               GPR15P = GPR13P; /* SAVE RETURN CODE. */        02S0122
         LR    GPR15P,GPR13P                                    02S0122
*               GPR12P = GPR00P; /* RESTORE REG. 12 */          02S0123
         LR    GPR12P,GPR00P                                    02S0123
*               GPR13P = GPR01P; /* RESTORE REG. 13. */         02S0124
         LR    GPR13P,GPR01P                                    02S0124
*               RFY(GPR15P,GPR00P,GPR01P) UNRSTD;               02S0125
*               RFY (GPR11P,GPR12P,GPR13P,GPR14P) UNRSTD;       02S0126
*             END; /* SETLOCK (OBTAIN) MODE(UNCOND) TYPE(CMS) REGS(USE)
*RELATED(CMS LOCK,IEFAB49B(VERLABEL)) */ /*GET CMS LOCK        @YM07608
*/                                                              02S0127
*            LOCKSW = ON;              /*LOCK IS NOW HELD    @YM07608*/
*/********************************************************************/
*/*                                                                  */
*/* SEGMENT NAME - VLSRDPCK                                          */
*/*                                                                  */
*/* DESCRIPTIVE NAME - CHECK FOR DUPLICATE VOLUME SERIAL             */
*/*                                                                  */
*/* FUNCTION - WHEN A VOLUME IS MOUNTED IN RESPONSE TO A NON-        */
*/*   SPECIFIC MOUNT REQUEST, THIS ROUTINE SCANS ALL OTHER DA AND    */
*/*   TAPE UCB'S TO DETERMINE IF A VOLUME WITH THE SAME SERIAL IS    */
*/*   MOUNTED ELSEWHERE.                                             */
*/*                                                                  */
*/*   OPERATION - THE OPERATION IS DOCUMENTED BY THE CODE ITSELF.    */
*/*                                                                  */
*/*                                                                  */
*/* ENTRY - INLINE SUBROUTINE                                        */
*/*                                                                  */
*/*   PURPOSE - SEE FUNCTION.                                        */
*/*                                                                  */
*/*   INPUT -                                                        */
*/*     VOLSER - VOLUME SERIAL OF VOLUME JUST READ                   */
*/*     UCBPTR - ADDR OF UCB ON WHICH VOLUME WAS MOUNTED.            */
*/*                                                                  */
*/* EXIT -                                                           */
*/*                                                                  */
*/*   CONDITION - VOLSER NOT DUPLICATE                               */
*/*                                                                  */
*/*     OUTPUT - VOLSEROK ON TO INDICATE VOLUME SERIAL NOT A         */
*/*       DUPLICATE.                                                 */
*/*                                                                  */
*/*     RETURN CODE -NONE.                                           */
*/*                                                                  */
*/*  CONDITION - VOLSER IS DUPLICATE                                 */
*/*                                                                  */
*/*     OUTPUT - VOLSEROK OFF INDICATING VOLUME IS A DUPLICATE       */
*/*              DUPEFAIL ON INDICATING SPECIFICALLY THAT VOLUME     */
*/*                          IS A DUPLICATE                  @Y30LPPE*/
*/*                                                                  */
*/*                                                                  */
*/*     RETURN CODE - NONE                                           */
*/*                                                                  */
*/*                                                                  */
*/********************************************************************/
*VOLSEROK = ON;                        /*ASSUME VOLSER IS NOT A DUP  */
*DUPEFAIL = OFF;                       /*AND INITIALIZE DUPEFAIL
*                                        SWITCH              @Y30LPPE*/
         OI    LOCKSW,B'00010100'                               03S0130
         NI    DUPEFAIL,B'10111111'                             03S0130
*SAVEUCBP = UCBPTR;                                             03S0131
         MVC   SAVEUCBP(4),UCBPTR                               03S0131
*DO LUTNO = ONE BY ONE                 /*LOOP THRU ALL LUT ENTRIES OR*/
*  WHILE(IOSLUT(LUTNO)^= ENDINDIC & VOLSEROK = ON);/*UNTIL DUP FOUND */
         LA    LUTNO,1                                          03S0132
         B     @DE00132                                         03S0132
@DL00132 DS    0H                                               03S0133
*  IF IOSLUT(LUTNO) ^= NOUCBADR THEN   /*VALID UCB ADDR ?            */
         LR    @12,LUTNO                                        03S0133
         ALR   @12,@12                                          03S0133
         L     @11,CVTPTR                                       03S0133
         L     @11,CVTILK2(,@11)                                03S0133
         ST    @12,@TF00001                                     03S0133
         ALR   @12,@11                                          03S0133
         BCTR  @12,0                                            03S0133
         BCTR  @12,0                                            03S0133
         CLC   IOSLUT(2,@12),NOUCBADR                           03S0133
         BE    @RF00133                                         03S0133
*    DO;                                                        03S0134
*      UCBPTR = IOSLUT(LUTNO);         /*SET MAPPING MACRO BASE PTR  */
         AL    @11,@TF00001                                     03S0135
         BCTR  @11,0                                            03S0135
         BCTR  @11,0                                            03S0135
         SLR   @12,@12                                          03S0135
         ICM   @12,3,IOSLUT(@11)                                03S0135
         ST    @12,UCBPTR                                       03S0135
*      IF UCB3TAPE = ON ×              /*UCB FOR TAPE...             */
*         UCB3DACC = ON THEN           /*OR DA ?                     */
         TM    UCB3TAPE(@12),B'10100000'                        03S0136
         BZ    @RF00136                                         03S0136
*        IF (UCBVOLI = VOLSER &        /*VOLSER SAME AS VOL JUST MTD?*/
*           SAVEUCBP ^= UCBPTR) THEN   /*NOT UCB WHERE VOL MOUNTED   */
         CLC   UCBVOLI(6,@12),VOLSER                            03S0137
         BNE   @RF00137                                         03S0137
         C     @12,SAVEUCBP                                     03S0137
         BE    @RF00137                                         03S0137
*             DO;                      /*                    @Y30LPPE*/
*              VOLSEROK = OFF;         /*YES, INDICATE VOLSER IS DUP */
*              DUPEFAIL = ON;          /*AND SET DUPEFAIL SWITCH
*                                                            @Y30LPPE*/
         OI    DUPEFAIL,B'01000000'                             03S0140
         NI    VOLSEROK,B'11111011'                             03S0140
*             END;                     /*                    @Y30LPPE*/
*        ELSE;                         /*NO MATCH,VOLSER STILL OK    */
@RF00137 DS    0H                                               03S0143
*      ELSE;                           /*NOT TAPE OR DA UCB          */
@RF00136 DS    0H                                               03S0144
*    END;                                                       03S0144
*  ELSE;                               /*ZERO UCB ADDRESS            */
@RF00133 DS    0H                                               03S0146
*END;                                                           03S0146
         AH    LUTNO,@CH00100                                   03S0146
@DE00132 LR    @12,LUTNO                                        03S0146
         ALR   @12,@12                                          03S0146
         L     @11,CVTPTR                                       03S0146
         L     @11,CVTILK2(,@11)                                03S0146
         ALR   @11,@12                                          03S0146
         BCTR  @11,0                                            03S0146
         BCTR  @11,0                                            03S0146
         CLC   IOSLUT(2,@11),ENDINDIC                           03S0146
         BE    @DC00132                                         03S0146
         TM    VOLSEROK,B'00000100'                             03S0146
         BO    @DL00132                                         03S0146
@DC00132 DS    0H                                               03S0147
*UCBPTR = SAVEUCBP;                    /*RESTORE SAVED UCBPTR        */
         MVC   UCBPTR(4),SAVEUCBP                               03S0147
*          END;                                                 02S0148
*      END;                                                     02S0149
*    ELSE                              /*IO ERROR READING LABEL      */
*      DO;                                                      02S0150
         B     @RC00084                                         02S0150
@RF00084 DS    0H                                               02S0151
*        ERRTYPE = ON;                 /*INDICATE I/O ERROR  @ZM34521*/
*        VOLSEROK = OFF;               /*INDICATE BAD VOLSER         */
         OI    ERRTYPE,B'00100000'                              02S0152
         NI    VOLSEROK,B'11111011'                             02S0152
*        VOLSER = ZEROVLSR;            /*CLEAR VOLSER READ           */
         MVC   VOLSER(6),ZEROVLSR                               02S0153
*      END;                                                     02S0154
*    IF VOLSEROK = ON THEN             /*VOLUME SERIAL OK ?          */
@RC00084 TM    VOLSEROK,B'00000100'                             02S0155
         BNO   @RF00155                                         02S0155
*      DO;                                                      02S0156
*        IF LOCKSW = OFF THEN          /*LOCK NOT HELD YET   @YM07608*/
         TM    LOCKSW,B'00010000'                               02S0157
         BNZ   @RF00157                                         02S0157
*          DO;                         /*GET ON LOCKS        @YM07608*/
*             DO; /*MODESET  KEYADDR(ZERO)*/                    02S0159
*               SPKA (ZERO); /*SET PSW KEY*/                    02S0160
         SLR   @12,@12                                          02S0160
         SPKA  0(@12)                                           02S0160
*             END; /*MODESET  KEYADDR(ZERO)*/   /*ISSUE MODESET TO GET
*EY 0  */                                                       02S0161
*             DO; /* SETLOCK (OBTAIN) MODE(UNCOND) TYPE(LOCAL) REGS(USE
* RELATED(LOCAL LOCK,IEFAB49B(VERLABEL)) */                     02S0162
*               RFY (GPR11P,GPR12P,GPR13P,GPR14P) RSTD;         02S0163
*               RFY (GPR15P,GPR00P,GPR01P) RSTD;                02S0164
*               GPR15P = GPR11P; /* SAVE REG. 11 IN REG. 15. */ 02S0165
