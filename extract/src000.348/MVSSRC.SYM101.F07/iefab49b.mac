*/*                                                                  */
*/*  SEGMENT NAME - VMVENQDQ                                         */
*/*                                                                  */
*/* DESCRIPTIVE NAME - VM&V ENQ/DEQ ROUTINE                          */
*/*                                                                  */
*/* FUNCTION - THIS ROUTINE ENQ'S ON OR DEQ'S FROM THE VM&V RESOURCE.*/
*/*   THE VM&V RESOURCE IS THE ADDRESS OF THE UCB TO BE      @ZA07519*/
*/*   VERIFIED. THIS PREVENTS TWO OR MORE VERIFY'S FROM              */
*/*   VERIFING THE SAME VOLUME SERIAL.                               */
*/*   THE ADDRESS OF THE UCB IS SET UP IN A FIELD,           @ZA07519*/
*/*   ENQRNAME, AND USED AS THE MINOR NAME FOR BOTH ENQ AND  @ZA07519*/
*/*   DEQ.  THE UNIT NAME CAN NOT BE USED DIRECTLY FROM THE  @ZA07519*/
*/*   UCB AND SET UP AT BOTH ENQ AND DEQ BECAUSE DDR COULD   @ZA07519*/
*/*   OCCUR AFTER ENQ AND BEFORE DEQ. DDR SWAPS THE EBCDIC   @ZA07519*/
*/*   UNIT NAME IN THE UCB'S AND SO THE MINOR NAME AT DEQ    @ZA07519*/
*/*   WOULD NOT MATCH THE MINOR NAME AT ENQ CAUSING A 138    @ZA07519*/
*/*   ABEND.  THE ADDRESS OF THE UCB REMAINS UNCHANGED BY    @ZA07519*/
*/*   DDR AND REPRESENTS THE SAME RESOURCE ACROSS JOBS.      @ZA07519*/
*/*                                                                  */
*/* ENTRY - INLINE PROCEDURE.                                        */
*/*                                                                  */
*/*   PURPOSE - SEE FUNCTION.                                        */
*/*   INPUT -                                                        */
*/*     ENQDQSW - ONE BYTE OF INDICATORS                             */
*/*               BIT 0 = 1, ENQ ON VM&V RESOURCE                    */
*/*               BIT 1 = 1, DEQ FROM VM&V RESOURCE                  */
*/*     UCBNAME - PRINTABLE UCB ADDRESS,IN UCB                       */
*/* EXIT -                                                           */
*/*                                                                  */
*/*   CONDITION - VM&V RESOURCE SUCCESSFULLY OBTAINED OR RELEASED    */
*/*     OUTPUT - VM&V RESOURCE OBTAINED OR RELEASED.                 */
*/*                                                                  */
*/*   RETCODE -                                                      */
*/*       NONE                                                       */
*/*                                                                  */
*/*                                                                  */
*/********************************************************************/
*VMVENQDQ: PROC;                                                02S0410
VMVENQDQ STM   @14,@12,@SA00002                                 02S0410
*IF DOENQ = ON THEN                    /*ENQ REQUESTED ?             */
         TM    DOENQ,B'10000000'                                02S0411
         BNO   @RF00411                                         02S0411
*  DO;                                                          02S0412
*    ENQCORE(1:ENQLTH) = ENQLST(1:ENQLTH); /*MOVE LIST TO DSECT      */
         L     @12,ENQLTH                                       02S0413
         BCTR  @12,0                                            02S0413
         EX    @12,@SM01627                                     02S0413
*    RFY (REG1,REG2) RSTD;                                      02S0414
*    ENQRNAME = UCBPTR;                /*SET UP MINOR NAME   @ZA07519*/
         MVC   ENQRNAME(4),UCBPTR                               02S0415
*    REG2 = ADDR(ENQRNAME);            /*ADDR OF MINOR       @ZA07519*/
         LA    REG2,ENQRNAME                                    02S0416
*    GEN REFS(ENQCORE);                                         02S0417
    ENQ (RESNAME,(2),E,4,SYSTEM),MF=(E,ENQCORE)               ZA07519
*    RFY(REG1,REG2) UNRSTD;                                     02S0418
*  END;                                                         02S0419
*ELSE;                                 /*ENQ NOT REQUESTED           */
@RF00411 DS    0H                                               02S0421
*IF DODEQ = ON THEN                    /*DEQ REQUESTED ?             */
         TM    DODEQ,B'01000000'                                02S0421
         BNO   @RF00421                                         02S0421
*  DO;                                                          02S0422
*    DEQCORE(1:DEQLTH) = DEQLST(1:DEQLTH);/*MOVE LIST TO DSECT       */
         L     @12,DEQLTH                                       02S0423
         BCTR  @12,0                                            02S0423
         EX    @12,@SM01629                                     02S0423
*    RFY (REG1,REG2) RSTD;                                      02S0424
*    REG2 = ADDR(ENQRNAME);            /*ADDR OF MINOR       @ZA07519*/
         LA    REG2,ENQRNAME                                    02S0425
*    GEN REFS(DEQCORE);                                         02S0426
    DEQ (RESNAME,(2),4,SYSTEM),MF=(E,DEQCORE)                 ZA07519
*    RFY (REG1,REG2) UNRSTD;                                    02S0427
*  END;                                                         02S0428
*ELSE;                                                          02S0429
@RF00421 DS    0H                                               02S0430
*END;                                                           02S0430
@EL00002 DS    0H                                               02S0430
@EF00002 DS    0H                                               02S0430
@ER00002 LM    @14,@12,@SA00002                                 02S0430
         BR    @14                                              02S0430
*END IEFAB49B                                                   01S0431
