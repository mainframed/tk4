         TITLE 'IEFAB492 - HANDLE VM&&V REQUESTS                       *
                         '
IEFAB492 CSECT ,                                                01S0001
@MAINENT DS    0H                                               01S0001
         USING *,@15                                            01S0001
         B     @PROLOG                                          01S0001
         DC    AL1(24)                                          01S0001
         DC    C'IEFAB492 78089  UZ17548 '                      01S0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                  01S0001
         BALR  @12,0                                            01S0001
@PSTART  DS    0H                                               01S0001
         USING @PSTART,@12                                      01S0001
         L     @00,@SIZDATD                                     01S0001
         MODESET EXTKEY=SCHED,SAVEKEY=(2)
         GSPACE   R,LV=(0)
         LR    @11,@01                                          01S0001
         USING @DATD,@11                                        01S0001
         ST    @13,@SA00001+4                                   01S0001
         STC   @02,@OLDKEY                                      01S0001
         MODESET KEYADDR=(2)
         LM    @00,@02,20(@13)                                  01S0001
         ST    @11,8(,@13)                                      01S0001
         LR    @13,@11                                          01S0001
         MODESET EXTKEY=ZERO
         MVC   @PC00001(40),0(@01)                              01S0001
         MODESET EXTKEY=SCHED
JSCBDISP EQU 188
JSCBAOS1 EQU  72
JSCBAOS2 EQU 192
*ERRWORD=NOERROR;                      /*INITIALIZE RETURN CODE      */
         SLR   ERRWORD,ERRWORD                                  01S0076
*/********************************************************************/
*/*                                                                  */
*/* SEGMENT NAME - VMVCHAIN                                          */
*/*                                                                  */
*/* DESCRIPTIVE NAME - VM&V REQUEST BLOCK CHAINING ROUTINE           */
*/*                                                                  */
*/* FUNCTION -                                                       */
*/*   THIS ROUTINE CHAINS TOGETHER VM&V REQUEST BLOCKS FOR INPUT     */
*/*   TO VOLUME MOUNT AND VERIFY CONTROL ROUTINE.                    */
*/*                                                                  */
*/* OPERATION                                                        */
*/*   THIS ROUTINE SEARCHES DOWN THE SIOT CHAIN LOOKING FOR VM&V     */
*/*   REQUEST BLOCKS.                                                */
*/*   THE POINTER TO THE FIRST VM&V REQUEST BLOCK IS PLACED IN       */
*/*   FRSTVMVP. ALL SUBSEQUENT REQUEST BLOCKS ARE CHAINED TOGETHER.  */
*/*   ALL SIOT POINTERS TO VM&V REQUEST BLOCKS ARE ZEROED.           */
*/*                                                                  */
*/* ENTRY - INLINE SEGMENT OF IEFAB492.                              */
*/*                                                                  */
*/*   PURPOSE - SEE FUNCTION                                         */
*/*                                                                  */
*/*   INPUT -                                                        */
*/*                                                                  */
*/*     FSIOTPTR - POINTER TO FIRST SIOT IN CHAIN.                   */
*/*                                                                  */
*/*                                                                  */
*/* EXIT - NORMAL -                                                  */
*/*                                                                  */
*/*   CONDITION - ALL REQUEST BLOCKS CHAINED FOR INPUT TO VM&V       */
*/*                                                                  */
*/*   OUTPUT -                                                       */
*/*                                                                  */
*/*     FRSTVMVP - POINTER TO FIRST VM&V REQUEST BLOCK.              */
*/*                                                                  */
*/*     VMVNREQP - IN VMVRQBLK,SET TO POINT TO NEXT REQUEST BLOCK ON */
*/*                CHAIN. ZERO IF LAST.                              */
*/*                                                                  */
*/*     SIOTVMVP - SET TO 0 (FOR ANY SIOTS WHICH HAD POINTED TO VM&V */
*/*                REQUEST BLOCKS.                                   */
*/*                                                                  */
*/*     RETURN CODE - NONE                                           */
*/*                                                                  */
*/*                                                                  */
*/********************************************************************/
*FRSTVMVP = ZERO;                      /*INIT PTR TO FIRST VMV RB    */
         SLR   @10,@10                                          02S0077
         ST    @10,FRSTVMVP                                     02S0077
*FREQBLSW= OFF;                        /*INIT FIRST VMV REQ BLK SW   */
         NI    FREQBLSW,B'01111111'                             02S0078
*SIOTPTR=FSIOTPTR;                     /*POINT TO FIRST SIOT         */
         L     @10,@PC00001                                     02S0079
         L     SIOTPTR,FSIOTPTR(,@10)                           02S0079
*DO WHILE SIOTPTR ^= ZERO;             /*LOOP THRU ALL SIOTS         */
         B     @DE00080                                         02S0080
@DL00080 DS    0H                                               02S0081
*  IF SIOTVMVP ^= ZERO THEN            /*ANY VMV REQS FOR THIS SIOT? */
         L     @10,SIOTVMVP(,SIOTPTR)                           02S0081
         LTR   @10,@10                                          02S0081
         BZ    @RF00081                                         02S0081
*    DO;                                                        02S0082
*      VMVREQBP = SIOTVMVP;            /*SET PTR TO FIRST VMV REQ    */
         ST    @10,VMVREQBP                                     02S0083
*      DO WHILE VMVREQBP ^= ZERO;      /*LOOP THRU ALL VMV REQ BLKS  */
         B     @DE00084                                         02S0084
@DL00084 DS    0H                                               02S0085
*        SAVEREQP = VMVNREQP;          /*SAVE PTR TO NEXT RB         */
         L     @10,VMVREQBP                                     02S0085
         L     SAVEREQP,VMVNREQP(,@10)                          02S0085
*        VMVNREQP = ZERO;              /*CLEAR NEXT PTR IN VMVRB     */
         SLR   @09,@09                                          02S0086
         ST    @09,VMVNREQP(,@10)                               02S0086
*        IF FREQBLSW = OFF THEN        /*IS THIS FIRST REQUEST?      */
         TM    FREQBLSW,B'10000000'                             02S0087
         BNZ   @RF00087                                         02S0087
*          DO;                                                  02S0088
*            FRSTVMVP = VMVREQBP;      /*SAVE PTR TO FIRST BLOCK     */
         ST    @10,FRSTVMVP                                     02S0089
*            PREVVMVP = VMVREQBP;      /*SET BACKWARD POINTER        */
         LR    PREVVMVP,@10                                     02S0090
*            FREQBLSW = ON;            /*INDICATE FIRST ENTRY MADE   */
         OI    FREQBLSW,B'10000000'                             02S0091
*          END;                                                 02S0092
*        ELSE                          /*THIS NOT FIRST BLK ON CHAIN */
*          DO;                                                  02S0093
         B     @RC00087                                         02S0093
@RF00087 DS    0H                                               02S0094
*            PREVVMVP->VMVNREQP=VMVREQBP; /*CHAIN TO PREVIOUS REQ    */
         L     @10,VMVREQBP                                     02S0094
         ST    @10,VMVNREQP(,PREVVMVP)                          02S0094
*            PREVVMVP = VMVREQBP;      /*SET BACKWARD POINTER        */
         LR    PREVVMVP,@10                                     02S0095
*          END;                                                 02S0096
*        VMVREQBP = SAVEREQP;          /*POINT TO NEXT VMV REQ BLK   */
@RC00087 ST    SAVEREQP,VMVREQBP                                02S0097
*      END;                                                     02S0098
@DE00084 SLR   @10,@10                                          02S0098
         C     @10,VMVREQBP                                     02S0098
         BNE   @DL00084                                         02S0098
*      SIOTVMVP = ZERO;                /*ZERO POINTER TO VMV REQ BLK */
         ST    @10,SIOTVMVP(,SIOTPTR)                           02S0099
*    END;                                                       02S0100
*  ELSE;                               /*NO VMV REQS FOR THIS SIOT   */
@RF00081 DS    0H                                               02S0102
*  SIOTPTR = SIOTNPTR;                 /*POINT TO NEXT SIOT          */
         L     SIOTPTR,SIOTNPTR(,SIOTPTR)                       02S0102
*END;                                                           02S0103
@DE00080 LTR   SIOTPTR,SIOTPTR                                  02S0103
         BNZ   @DL00080                                         02S0103
*/********************************************************************/
*/*                                                                  */
*/* SEGMENT NAME - VMVDUPCK                                          */
*/*                                                                  */
*/* DESCRIPTIVE NAME - DUPLICATE VM&V RB CHECKER                     */
*/*                                                                  */
*/* FUNCTION - THIS ROUTINE SCANS THE VM&V REQUEST BLOCK CHAIN       */
*/*            AND MARKS MOUNT ENTRIES FOR THE SAME VOLUME AS        */
*/*            DUPLICATES.                                           */
*/*                                                                  */
*/* ENTRY - INLINE SEGMENT OF IEFAB492.                              */
*/*                                                                  */
*/*   PURPOSE - SEE FUNCTION.                                        */
*/*                                                                  */
*/*   INPUT -                                                        */
*/*     FRSTVMVP - POINTER TO FIRST VM&V RB.                         */
*/*                                                                  */
*/* EXIT -                                                           */
*/*                                                                  */
*/*   CONDITION - ALL DUPLICATE VM&V ENTRIES LOCATED AND MARKED AS   */
*/*               SUCH.                                              */
*/*                                                                  */
*/*   OUTPUT -                                                       */
*/*     VMVDUPVL - IN VMVRQBLK, SET ON IN ALL BUT THE FIRST VM&V RB  */
*/*                FOR A PARTICULAR VOLUME.                          */
*/*                                                                  */
*/*   RETURN CODE -                                                  */
*/*     NONE                                                         */
*/*                                                                  */
*/********************************************************************/
*VMVREQBP = FRSTVMVP;                  /*POINT TO FIRST VM&V RB      */
         MVC   VMVREQBP(4),FRSTVMVP                             02S0104
*DO WHILE VMVREQBP ^= ZERO;            /*LOOP THRU VM&V REQUEST BLOCK*/
         B     @DE00105                                         02S0105
@DL00105 DS    0H                                               02S0106
*  AVMVRBP = VMVNREQP;                 /*INIT ALTERNATE VM&V RB PTR  */
         L     @10,VMVREQBP                                     02S0106
         L     AVMVRBP,VMVNREQP(,@10)                           02S0106
*  DO WHILE AVMVRBP ^= ZERO;           /*LOOP THRU REMAINING VM&V RBS*/
         B     @DE00107                                         02S0107
@DL00107 DS    0H                                               02S0108
*    IF AVMVRBP-> VMVDUPVL = OFF THEN  /*IF THIS NOT DUP             */
         TM    VMVDUPVL(AVMVRBP),B'10000000'                    02S0108
         BNZ   @RF00108                                         02S0108
*      DO;                                                      02S0109
*        IF VMVUCBP=AVMVRBP->VMVUCBP THEN /*SAME VOL?                */
         L     @10,VMVREQBP                                     02S0110
         CLC   VMVUCBP(3,@10),VMVUCBP(AVMVRBP)                  02S0110
         BNE   @RF00110                                         02S0110
*          DO;                                                  02S0111
*            AVMVRBP->VMVDUPVL = ON;   /*SET DUP SWITCH              */
         OI    VMVDUPVL(AVMVRBP),B'10000000'                    02S0112
*            IF (VMVDSNMP= ZERO & AVMVRBP->VMVDSNMP^=ZERO) THEN /*1ST
*                                        NON TEMPORARY DSNAME MUST
*                                        APPEAR IN MOUNT MESSAGE     */
         SLR   @09,@09                                          02S0113
         C     @09,VMVDSNMP(,@10)                               02S0113
         BNE   @RF00113                                         02S0113
         L     @07,VMVDSNMP(,AVMVRBP)                           02S0113
         CR    @07,@09                                          02S0113
         BE    @RF00113                                         02S0113
*              VMVDSNMP = AVMVRBP -> VMVDSNMP;                  02S0114
         ST    @07,VMVDSNMP(,@10)                               02S0114
*            ELSE;                                              02S0115
@RF00113 DS    0H                                               02S0116
*          END;                                                 02S0116
*        ELSE;                                                  02S0117
@RF00110 DS    0H                                               02S0118
*      END;                                                     02S0118
*    ELSE;                                                      02S0119
@RF00108 DS    0H                                               02S0120
*    AVMVRBP = AVMVRBP-> VMVNREQP;     /*UPDATE POINTER TO NEXT VMVRB*/
         L     AVMVRBP,VMVNREQP(,AVMVRBP)                       02S0120
*  END;                                                         02S0121
@DE00107 LTR   AVMVRBP,AVMVRBP                                  02S0121
         BNZ   @DL00107                                         02S0121
*  DO WHILE(VMVNREQP ^= ZERO &         /*LOOP TO FIND NEXT NON-DUP   */
*    VMVNREQP -> VMVDUPVL = ON);                                02S0122
         B     @DE00122                                         02S0122
@DL00122 DS    0H                                               02S0123
*    VMVREQBP = VMVNREQP;              /*POINT TO NEXT VM&V RB       */
         L     @10,VMVREQBP                                     02S0123
         L     @10,VMVNREQP(,@10)                               02S0123
         ST    @10,VMVREQBP                                     02S0123
*  END;                                                         02S0124
@DE00122 L     @10,VMVREQBP                                     02S0124
         L     @10,VMVNREQP(,@10)                               02S0124
         LTR   @10,@10                                          02S0124
         BZ    @DC00122                                         02S0124
         TM    VMVDUPVL(@10),B'10000000'                        02S0124
         BO    @DL00122                                         02S0124
@DC00122 DS    0H                                               02S0125
*  VMVREQBP = VMVNREQP;                /*POINT TO NEXT VM&V RB       */
         L     @10,VMVREQBP                                     02S0125
         L     @10,VMVNREQP(,@10)                               02S0125
         ST    @10,VMVREQBP                                     02S0125
*END;                                                           02S0126
@DE00105 SLR   @10,@10                                          02S0126
         C     @10,VMVREQBP                                     02S0126
         BNE   @DL00105                                         02S0126
*IF FRSTVMVP ^= ZERO THEN              /*ANY VMV REQUESTS ?          */
         C     @10,FRSTVMVP                                     01S0127
         BE    @RF00127                                         01S0127
*  DO;                                                          01S0128
*    VMVCALL = ON;                     /*INDICATE 1ST CALL IN ESTAE
*                                                            @YM07266*/
         L     @10,@PC00001+36                                  01S0129
         L     @10,EXITPRMP(,@10)                               01S0129
         OI    VMVCALL(@10),B'00000010'                         01S0129
*    CALL IEFAB493(FRSTVMVP,ASID,CNECB); /*INVOKE VOLUME MOUNT  01S0130
*                                            AND VERIFY              */
         LA    @10,FRSTVMVP                                     01S0130
         ST    @10,@AL00001                                     01S0130
         L     @10,@PC00001+16                                  01S0130
         ST    @10,@AL00001+4                                   01S0130
         L     @10,@PC00001+4                                   01S0130
         ST    @10,@AL00001+8                                   01S0130
         L     @15,@CV01144                                     01S0130
         LA    @01,@AL00001                                     01S0130
         BALR  @14,@15                                          01S0130
*    ERRWORD=REG15;                    /*SAVE RETURN CODE            */
         LR    ERRWORD,REG15                                    01S0131
*    IF ERRWORD=OPRCANCL ×             /*OPERATOR CANCEL             */
*      ERRWORD = OPRSAYNO THEN         /*OR REPLY NO?                */
         CH    ERRWORD,@CH00098                                 01S0132
         BE    @RT00132                                         01S0132
         CH    ERRWORD,@CH00120                                 01S0132
         BNE   @RF00132                                         01S0132
@RT00132 DS    0H                                               01S0133
*      FAILRSCP->STRSNCDE = STCANCEL;  /*SHOW OPERATOR CANCELED STEP */
         L     @10,@PC00001+20                                  01S0133
         L     @10,FAILRSCP(,@10)                               01S0133
         MVC   STRSNCDE(2,@10),@CH00122                         01S0133
*    ELSE;                                                      01S0134
@RF00132 DS    0H                                               01S0135
*    IF ERRWORD = GETFAIL THEN         /*WAS THERE A GETMAIN FAILURE?*/
         CH    ERRWORD,@CH00096                                 01S0135
         BNE   @RF00135                                         01S0135
*      FAILRSCP -> STRSNCDE = GETMFAIL; /*GETMAIN FAIL REASON CODE   */
         L     @10,@PC00001+20                                  01S0136
         L     @10,FAILRSCP(,@10)                               01S0136
         MVC   STRSNCDE(2,@10),@CH00082                         01S0136
*    ELSE;                                                      01S0137
@RF00135 DS    0H                                               01S0138
*    IF ERRWORD = NOESTAE THEN         /*ESTAE ERROR                 */
         CH    ERRWORD,@CH00102                                 01S0138
         BNE   @RF00138                                         01S0138
*      FAILRSCP -> STRSNCDE = ESTFAIL;                          01S0139
         L     @10,@PC00001+20                                  01S0139
         L     @10,FAILRSCP(,@10)                               01S0139
         MVC   STRSNCDE(2,@10),@CH00098                         01S0139
*    ELSE;                                                      01S0140
@RF00138 DS    0H                                               01S0141
*    IF ERRWORD=NOTDEFND THEN          /*IF MSS VOLUME IS NOT DEFINED
*                                                            @Y30LPPE*/
         CH    ERRWORD,@CH00107                                 01S0141
         BNE   @RF00141                                         01S0141
*      FAILRSCP->STRSNCDE=NTDEFNDR;    /*SET REASON CODE TO 8@Y30LPPE*/
         L     @10,@PC00001+20                                  01S0142
         L     @10,FAILRSCP(,@10)                               01S0142
         MVC   STRSNCDE(2,@10),@CH00094                         01S0142
*    ELSE                              /*ELSE                @Y30LPPE*/
*      IF ERRWORD=NOTAVAIL THEN        /*IF MSS VOLUME IS NOT AVAILABLE
*                                                            @Y30LPPD*/
         B     @RC00141                                         01S0143
@RF00141 CH    ERRWORD,@CH00094                                 01S0143
         BNE   @RF00143                                         01S0143
*        FAILRSCP->STRSNCDE=NTAVAILR;  /*SET REASON CODE TO 3@Y30LPPE*/
         L     @10,@PC00001+20                                  01S0144
         L     @10,FAILRSCP(,@10)                               01S0144
         MVC   STRSNCDE(2,@10),@CH00111                         01S0144
*      ELSE                            /*ELSE                @Y30LPPE*/
*        IF ERRWORD=UNABTOMT THEN      /*IF MSS VOLUME CANNOT BE
*                                        MOUNTED             @Y30LPPD*/
         B     @RC00143                                         01S0145
@RF00143 CH    ERRWORD,@CH00127                                 01S0145
         BNE   @RF00145                                         01S0145
*          FAILRSCP->STRSNCDE=UNABTMTR;/*SET REASON CDE TO 10@Y30LPPE*/
         L     @10,@PC00001+20                                  01S0146
         L     @10,FAILRSCP(,@10)                               01S0146
         MVC   STRSNCDE(2,@10),@CH00129                         01S0146
*        ELSE                          /*ELSE                @Y30LPPE*/
*          IF ERRWORD=NOTACESS THEN    /*IF MSS VOLUME IS NOT   01S0147
*                                        ACCESSIBLE          @Y30LPPE*/
         B     @RC00145                                         01S0147
@RF00145 CH    ERRWORD,@CH00104                                 01S0147
         BNE   @RF00147                                         01S0147
*            FAILRSCP->STRSNCDE=NTACESSR;/*SET REASON CODE TO 23
*                                                            @Y30LPPE*/
         L     @10,@PC00001+20                                  01S0148
         L     @10,FAILRSCP(,@10)                               01S0148
         MVC   STRSNCDE(2,@10),@CH00109                         01S0148
*          ELSE                                                 01S0149
*            IF ERRWORD = MSGPINV THEN /*IF MSVGP NAME IS INVALID
*                                                            @Y30LPPE*/
         B     @RC00147                                         01S0149
@RF00147 CH    ERRWORD,@CH00071                                 01S0149
         BNE   @RF00149                                         01S0149
*              FAILRSCP->STRSNCDE=MSGPINVR; /*SET REASON CODE TO 24
*                                                            @Y30LPPE*/
         L     @10,@PC00001+20                                  01S0150
         L     @10,FAILRSCP(,@10)                               01S0150
         MVC   STRSNCDE(2,@10),@CH00127                         01S0150
*            ELSE                                               01S0151
*              IF ERRWORD = MSGPSPCE THEN /*IF SPACE OR MSVGP KEYWORDS
*                                         ARE MISSING        @Y30LPPE*/
         B     @RC00149                                         01S0151
@RF00149 CH    ERRWORD,@CH00133                                 01S0151
         BNE   @RF00151                                         01S0151
*                FAILRSCP->STRSNCDE = MSGPSPCR; /*SET REASON CODE TO 25
*                                                            @Y30LPPE*/
         L     @10,@PC00001+20                                  01S0152
         L     @10,FAILRSCP(,@10)                               01S0152
         MVC   STRSNCDE(2,@10),@CH00135                         01S0152
*    IF ERRWORD = NOERROR THEN         /*ANY ERRORS                  */
@RF00151 DS    0H                                               01S0153
@RC00149 DS    0H                                               01S0153
@RC00147 DS    0H                                               01S0153
@RC00145 DS    0H                                               01S0153
@RC00143 DS    0H                                               01S0153
@RC00141 LTR   ERRWORD,ERRWORD                                  01S0153
         BNZ   @RF00153                                         01S0153
*      DO;                                                      01S0154
*/********************************************************************/
*/*                                                                  */
*/*  SEGMENT NAME - NSPDACTL                                         */
*/*                                                                  */
*/*  DESCRIPTIVE NAME -  NON-SPECIFIC DIRECT ACCESS VOLUME CONTROLLER*/
*/*                                                                  */
*/*  FUNCTION - THIS ROUTINE SEARCHES THE VM&V RB CHAIN FOR REQUEST  */
*/*    BLOCKS OF NON-SPECIFIC DIRECT ACCESS VOLUMES. WHEN ONE IS     */
*/*    FOUND THE VOLUME SERIAL IS ENQ'D AND SPACE IS GOTTEN ON THE   */
*/*    VOLUME IF REQUESTED. WHEN ALL SUCH VM&V REQUEST BLOCKS HAVE   */
*/*    BEEN PROCESSED THEY ARE REMOVED FROM THE CHAIN. ALL DEVICE    */
*/*    GROUPS STILL SERIALIZED ARE RELEASED.                         */
*/*                                                                  */
*/*  ENTRY - INLINE SEGMENT OF IEFAB492.                             */
*/*                                                                  */
*/*    PURPOSE - SEE FUNCTION.                                       */
*/*                                                                  */
*/*    INPUT -                                                       */
*/*      FRSTVMVP - POINTER TO FIRST VM&V REQUEST BLOCK.             */
*/*      QMPARMP  - POINTER TO ALLOC QMGR PARM BLOCK OR ZERO @ZA10458*/
*/*                                                                  */
*/*  EXIT -                                                          */
*/*                                                                  */
*/*    CONDITION - PROCESSING SUCCESSFULLY COMPLETED.                */
*/*                                                                  */
*/*      OUTPUT -                                                    */
*/*        SIODADSM - IN SIOT, ON IF DADSM SUCCESSFUL                */
*/*        SPACE OBTAINED ON VOLUMES FOR NEW DATA SETS               */
*/*        NEW VM&V REQUEST LIST CONTAINING ONLY SPECIFIC DA AND     */
*/*        TAPE MOUNT REQUESTS.                                      */
*/*        ALL DEVICE GROUPS RELEASED.                               */
*/*                                                                  */
*/*      RETURN CODE -                                               */
*/*        ERRWORD - ZERO                                            */
*/*                                                                  */
*/*    CONDITION - VM&V FAILS TO MOUNT ANOTHER VOLUME AFTER A        */
*/*                   RECOVERABLE DADSM FAILURE DETECTED BY SEGMENT  */
*/*                   NSPDAVOL                                       */
*/*                                                                  */
*/*      OUTPUT -                                                    */
*/*        SAME AS ABOVE FOR ANY VM&V REQUEST BLOCK ALREADY          */
*/*        PROCESSED.                                                */
*/*                                                                  */
*/*      RETURN CODE -                                               */
*/*        ERRWORD - 20,TO INDICATE VM&V FAILURE.                    */
*/*                                                                  */
*/*    CONDITION - NON-RECOVERABLE DADSM FAILURE                     */
*/*                                                                  */
*/*      OUTPUT -                                                    */
*/*        SIOTRSNC - IN SIOT, WILL CONTAIN A REASON CODE GIVING     */
*/*                   THE SPECIFIC CAUSE OF FAILURE.                 */
*/*                   (SEE SEGMENT DADSEDAT FOR A LIST OF THESE      */
*/*                    REASON CODES)                                 */
*/*                                                                  */
*/*      RETURN CODE -                                               */
*/*        ERRWORD - 16, TO INDICATE NON-RECOVERABLE DADSM ERROR     */
*/*                                                                  */
*/********************************************************************/
*FNMAP = FNMAP && FNMAP;               /*CLEAR ENQ/DEQ FUNCTION MAP  */
         XC    FNMAP(1),FNMAP                                   02S0155
*FREQBLSW = OFF;                       /*SET CHAIN BEGUN SWITCH OFF  */
         NI    FREQBLSW,B'01111111'                             02S0156
*VMVREQBP = FRSTVMVP;                  /*POINT TO FIRST VM&V RB      */
         MVC   VMVREQBP(4),FRSTVMVP                             02S0157
*DO WHILE (ERRWORD = NOERROR &         /*LOOP THRU ALL VM&V RB'S     */
*  VMVREQBP ^= ZERO);                                           02S0158
         B     @DE00158                                         02S0158
@DL00158 DS    0H                                               02S0159
*  SAVEREQP = VMVNREQP;                /*SAVE PTR TO NEXT VM&V RB    */
         L     @10,VMVREQBP                                     02S0159
         L     SAVEREQP,VMVNREQP(,@10)                          02S0159
*  IF VMVVRLBL = ON &                  /*THIS REQUEST FOR NONSPEC DA?*/
*     VMVPROCD = OFF THEN              /*AND NOT ALREADY PROCESSED   */
         TM    VMVVRLBL(@10),B'00000010'                        02S0160
         BNO   @RF00160                                         02S0160
         TM    VMVPROCD(@10),B'10000000'                        02S0160
         BNZ   @RF00160                                         02S0160
*    DO;                                                        02S0161
*/********************************************************************/
*/*                                                                  */
*/*  SEGMENT NAME - NSPDAVOL                                         */
*/*                                                                  */
*/*  DESCRIPTIVE NAME - PROCESS NON-SPECIFIC DA VOLUMES MOUNTED      */
*/*                                                                  */
*/*  FUNCTION - FOR EACH NON-SPECIFIC DIRECT ACCESS VOLUME MOUNTED   */
*/*             BY VM&V THIS ROUTINE WILL ATTEMPT TO ENQUEUE ON THE  */
*/*             VOLUME SERIAL. IF SUCCESSFUL IT WILL PLACE THE VOLSER*/
*/*             IN THE JFCB OF EACH REQUEST WHICH REQUIRES           */
*/*             THIS VOLUME. SPACE FOR NEW DATA SETS IS THEN OBTAINED*/
*/*             IF INSUFFICIENT SPACE IS AVAILABLE ON THE VOLUME TO  */
*/*             CONTAIN ALL OF THE DATA SETS, ANY DATA SETS ALREADY  */
*/*             GOTTEN ARE SCRATCHED, AND THE VOLUME SERIAL IS DEQ'D */
*/*             IF THE VOLUME ENQ FAILED OR DADSM FAILED A NEW       */
*/*             VM&V REQUEST BLOCK IS CREATED. A CALL TO VOLUME      */
*/*             MOUNT AND VERIFY IS MADE TO UNLOAD THIS VOLUME AND   */
*/*             TO MOUNT AND VERIFY A NEW ONE. THIS VOLUME IS THEN   */
*/*             PROCESSED. THE REQUEST BLOCKS FOR THE NON-SPECIFIC   */
*/*             VOLUMES PROCESSED ARE FREED.                         */
*/*                                                                  */
*/*  ENTRY - INLINE SEGMENT OF IEFAB492                              */
*/*                                                                  */
*/*    PURPOSE - SEE FUNCTION.                                       */
*/*                                                                  */
*/*    INPUT -                                                       */
*/*                                                                  */
*/*       FRSTVMVP - POINTER TO FIRST VM&V REQUEST BLOCK             */
*/*       QMPARMP - POINTER TO ALLOC QMGR PARM BLOCK                 */
*/*                                                                  */
*/* EXIT -                                                           */
*/*                                                                  */
*/*    CONDITION - ALL NON-SPECIFIC DIRECT ACCESS VOLUMES PROCESSED  */
*/*                                                                  */
*/*      OUTPUT -                                                    */
*/*                                                                  */
*/*         SIODADSM - IN SIOT, ON IF DADSM SUCCESSFUL               */
*/*         SPACE OBTAINED ON VOLUMES FOR NEW DATA SETS              */
*/*         NEW VM&V REQUEST LIST WITH ALL REQUEST BLOCKS FOR NON-   */
*/*         SPECIFIC DIRECT ACCESS VOLUMES ELIMINATED.               */
*/*                                                                  */
*/*      RETURN CODE -                                               */
*/*                                                                  */
*/*          ERRWORD - ZERO,INDICATING NO ERRORS                     */
*/*                                                                  */
*/*    CONDITION - VM&V FAILS TO MOUNT ANOTHER VOLUME.               */
*/*                                                                  */
*/*      OUTPUT -                                                    */
*/*        SAME AS ABOVE FOR ANY VM&V REQUEST BLOCKS ALLREADY        */
*/*        PROCESSED.                                                */
*/*                                                                  */
*/*      RETURN CODE -                                               */
*/*                                                                  */
*/*        ERRWORD -20,TO INDICATE VM&V FAILURE                      */
*/*                                                                  */
*/*    CONDITION - MOUNT FAILED FOR VIRTUAL VOLUME           @Y30LPPE*/
*/*                                                          @Y30LPPE*/
*/*    OUTPUT -                                              @Y30LPPE*/
*/*      STRSNCDE- CONTAINS A REASONCODE INDICATING NATURE OF@Y30LPPE*/
*/*                FAILURE. THIS REASONCODE IS DETERMINED BY @Y30LPPE*/
*/*                THE REASONCODE RETURNED FROM VM&V.        @Y30LPPE*/
*/*      *************************************************** @Y30LPPE*/
*/*      *VM&V       *  FAILURE                 * STRSNCDE * @Y30LPPE*/
*/*      *REASONCODE *                          *          * @Y30LPPE*/
*/*      *************************************************** @Y30LPPE*/
*/*      *    8      *VIRT VOL NOT AVAILABLE    *     3    * @Y30LPPE*/
*/*      *    24     *UNABLE TO MOUNT VIRT VOL  *     10   * @Y30LPPE*/
*/*      *    28     *VIRT VOL NOT ACCESSIBLE   *     23   * @Y30LPPE*/
*/*      *************************************************** @Y30LPPE*/
*/*                                                          @Y30LPPE*/
*/*    RETURN CODE - 4 IN GPR 15                             @Y30LPPE*/
*/*                                                                  */
*/********************************************************************/
*ENQERROR = OFF;                       /*INIT ERROR SWITCH           */
         NI    ENQERROR,B'01111111'                             03S0162
*IF VMVDUPVL = OFF THEN                /*IF NOT DUPLICATE DO VOL ENQ */
         TM    VMVDUPVL(@10),B'10000000'                        03S0163
         BNZ   @RF00163                                         03S0163
*  DO;                                                          03S0164
*    VOLUMENQ = ON;                    /*SIGNIFY ENQ REQUESTED       */
         OI    VOLUMENQ,B'10000000'                             03S0165
*    IF VMVVUNTP -> VOLNSHR = ON THEN  /*EXCLUSIVE ENQ               */
         L     @10,VMVVUNTP(,@10)                               03S0166
         TM    VOLNSHR(@10),B'00001000'                         03S0166
         BNO   @RF00166                                         03S0166
*      VOLEXCLU = ON;                  /*REQUEST EXCLUSIVE ENQ       */
         OI    VOLEXCLU,B'00100000'                             03S0167
*    ELSE;                                                      03S0168
@RF00166 DS    0H                                               03S0169
*    UCBPTR = VMVUCBP;                 /*SET PTR TO UCB              */
         L     @10,VMVREQBP                                     03S0169
         SLR   UCBPTR,UCBPTR                                    03S0169
         ICM   UCBPTR,7,VMVUCBP(@10)                            03S0169
*    VOLSERP = ADDR(UCBVOLI);          /*POINT TO VOLUME SERIAL      */
         LA    @10,UCBVOLI(,UCBPTR)                             03S0170
         ST    @10,VOLSERP                                      03S0170
*    TCBP = JSCBPTR -> JSCBTCBP;       /*POINT TO TCB                */
         L     @10,@PC00001+24                                  03S0171
         L     @10,JSCBPTR(,@10)                                03S0171
         MVC   TCBP(4),JSCBTCBP(@10)                            03S0171
*    ERRWORD = NOERROR;                /*SET ZERO RETURN CODE@YM05583*/
         SLR   ERRWORD,ERRWORD                                  03S0172
*    IF UCBPRES = OFF THEN             /*FIXED DEVICE?       @YM05583*/
         TM    UCBPRES(UCBPTR),B'00000100'                      03S0173
         BNZ   @RF00173                                         03S0173
*      DO;                             /*NO, DO ENQ          @YM05583*/
*        CALL IEFAB4F0(FNMAP,VOLSERP,TCBP,VUTABPTR,VUTABLTH); /*ENQ
*                                        THE VOLUME SERIAL           */
         LA    @10,FNMAP                                        03S0175
         ST    @10,@AL00001                                     03S0175
         LA    @10,VOLSERP                                      03S0175
         ST    @10,@AL00001+4                                   03S0175
         LA    @10,TCBP                                         03S0175
         ST    @10,@AL00001+8                                   03S0175
         L     @10,@PC00001+8                                   03S0175
         ST    @10,@AL00001+12                                  03S0175
         L     @10,@PC00001+12                                  03S0175
         ST    @10,@AL00001+16                                  03S0175
         L     @15,@CV01149                                     03S0175
         LA    @01,@AL00001                                     03S0175
         BALR  @14,@15                                          03S0175
*        ERRWORD = REG15;              /*SAVE RETURN CODE            */
         LR    ERRWORD,REG15                                    03S0176
*      END;                            /*                    @YM05583*/
*    ELSE;                             /*FIXED DEV, DONT ENQ @YM05583*/
@RF00173 DS    0H                                               03S0179
*    IF ERRWORD ^=NOERROR THEN         /*WAS THERE AN ENQ ERROR ?    */
         LTR   ERRWORD,ERRWORD                                  03S0179
         BZ    @RF00179                                         03S0179
*      ENQERROR = ON;                  /*SET BIT TO INDICATE         */
         OI    ENQERROR,B'10000000'                             03S0180
*    ELSE;                                                      03S0181
@RF00179 DS    0H                                               03S0182
*    VOLUMENQ = OFF;                   /*CLEAR ENQ INDICATOR         */
*    VOLEXCLU = OFF;                   /*CLEAR EXCLUSIVE REQ INDIC.  */
         NI    VOLUMENQ,B'01011111'                             03S0183
*  END;                                                         03S0184
*ELSE;                                                          03S0185
@RF00163 DS    0H                                               03S0186
*IF ERRWORD = NOERROR THEN             /*ANY ERRORS ?                */
         LTR   ERRWORD,ERRWORD                                  03S0186
         BNZ   @RF00186                                         03S0186
*  DO;                                                          03S0187
*    SIOTPTR= VMVVUNTP->VOLSIOTP;      /*GET SIOT PTR                */
         L     @10,VMVREQBP                                     03S0188
         L     @09,VMVVUNTP(,@10)                               03S0188
         L     SIOTPTR,VOLSIOTP(,@09)                           03S0188
*    UCBPTR = VMVUCBP;                 /*POINT TO UCB        @YM02425*/
         SLR   UCBPTR,UCBPTR                                    03S0189
         ICM   UCBPTR,7,VMVUCBP(@10)                            03S0189
*    IF VMVVUNTP = SVOLUNAD            /*1ST VU FOR SIOT     @YM02425*/
*      & VMVVUNTP->VUDADSM = OFF THEN  /*DADSM REQ'D?        @YM02425*/
         C     @09,SVOLUNAD(,SIOTPTR)                           03S0190
         BNE   @RF00190                                         03S0190
         TM    VUDADSM(@09),B'00000010'                         03S0190
         BNZ   @RF00190                                         03S0190
*      DO;                             /*NO,FILL IN VOLSER   @YM02425*/
*        SJFCBPTR->JFCBVOLS(1:6)=UCBVOLI; /*PUT VOLSER N JFCB@YM05014*/
         L     @10,SJFCBPTR(,SIOTPTR)                           03S0192
         MVC   JFCBVOLS(6,@10),UCBVOLI(UCBPTR)                  03S0192
*        IF SJFCBPTR->JFCBNVOL = ZERO THEN /*IF NON SPECIFIC @YM02425*/
         CLI   JFCBNVOL(@10),0                                  03S0193
         BNE   @RF00193                                         03S0193
*          SJFCBPTR->JFCBNVOL=ONE;     /*SET # OF VOLS       @YM05014*/
         MVI   JFCBNVOL(@10),X'01'                              03S0194
*    END;                              /*                    @YM02425*/
@RF00193 DS    0H                                               03S0196
*    IF VMVVUNTP -> VUDADSM = ON &     /*DADSM REQUIRED?             */
*       SJFCBPTR -> JFCORGIS ^= ON THEN /*AND NOT ISAM?              */
@RF00190 L     @10,VMVREQBP                                     03S0196
         L     @10,VMVVUNTP(,@10)                               03S0196
         TM    VUDADSM(@10),B'00000010'                         03S0196
         BNO   @RF00196                                         03S0196
         L     @10,SJFCBPTR(,SIOTPTR)                           03S0196
         TM    JFCORGIS(@10),B'10000000'                        03S0196
         BO    @RF00196                                         03S0196
*      DO;                                                      03S0197
*/********************************************************************/
*/*                                                                  */
*/* SEGMENT NAME - DADSMINT                                          */
*/*                                                                  */
*/* DESCRIPTIVE NAME - DADSM INTERFACE ROUTINE.                      */
*/*                                                                  */
*/* FUNCTION -                                                       */
*/*   THIS ROUTINE INTERFACES WITH DIRECT ACCESS DEVICE SPACE        */
*/*   MANAGEMENT (DADSM) TO ACQUIRE SPACE ON A DIRECT ACCESS VOLUME  */
*/*   FOR NEW DATA SETS. DADSM REQUIRES THE FOLLOWING INPUT:         */
*/*       .REG0 - POINTER TO JFCB FOR THE DATA SET.                  */
*/*       .REG1 - POINTER TO A FULL WORD CONTAINING THE ADDRESS OF   */
*/*               THE UCB ON WHICH THE VOLUME IS MOUNTED.            */
*/*       .THE SERIAL NUMBER OF THE VOLUME MUST BE IN THE JFCB.      */
*/*                                                                  */
*/* ENTRY - INLINE SEGMENT OF IEFAB492.                              */
*/*   PURPOSE  - SEE FUNCTION.                                       */
*/*                                                                  */
*/*   INPUT -                                                        */
*/*     SIOTPTR - POINTER TO SIOT OF REQUEST, SIOT CONTAINS THE      */
*/*               POINTER TO THE JFCB.                               */
*/*     UCBPTR - POINTER TO UCB ALLOCATED TO REQUEST.                */
*/*                                                                  */
*/* EXITS -                                                          */
*/*   CONDITION - SPACE OBTAINED ON VOLUME.                          */
*/*                                                                  */
*/*     OUTPUT - NONE.                                               */
*/*       UPDATED JFCB                                               */
*/*                                                                  */
*/*     RETURN CODE -                                                */
*/*       REG15  - 0, TO INDICATE SPACE GOTTEN.                      */
*/*                                                                  */
*/*   CONDITION - UNABLE TO OBTAIN SPACE ON VOLUME SPECIFIED.        */
*/*                                                                  */
*/*     OUTPUT - NONE.                                               */
*/*                                                                  */
*/*     RETURN CODE -                                                */
*/*       ERRWORD  - RETURN CODE FROM DADSM TO INDICATE SPACE        */
*/*                  FAILURE.                                        */
*/*                                                                  */
*/*                                                                  */
*/********************************************************************/
*JFCBPTR = SJFCBPTR;                   /* GET JFCB POINTER FROM SIOT */
         LR    JFCBPTR,@10                                      04S0198
*JFCBVOLS(1:6) = UCBVOLI;              /* PUT VOLSER INTO JFCB       */
         MVC   JFCBVOLS(6,JFCBPTR),UCBVOLI(UCBPTR)              04S0199
*IF JFCBNVOL = ZERO THEN               /* IF NON-SPECIFIC REQUEST    */
         CLI   JFCBNVOL(JFCBPTR),0                              04S0200
         BNE   @RF00200                                         04S0200
*  JFCBNVOL = ONE;                     /* SET NUMBER OF VOLUMES      */
         MVI   JFCBNVOL(JFCBPTR),X'01'                          04S0201
*DADSPARM = UCBPTR;                    /* STORE POINTER TO UCB FOR
*                                         DADSM.                     */
@RF00200 ST    UCBPTR,DADSPARM                                  04S0202
*REG0 = JFCBPTR;                                                04S0203
         LR    REG0,JFCBPTR                                     04S0203
*REG1 = ADDR(DADSPARM);                                         04S0204
         LA    REG1,DADSPARM                                    04S0204
*GEN(SVC 32);                          /* INVOKE DADSM TO GET SPACE  */
         SVC 32
*ERRWORD = REG15;                      /*SAVE RETURN CODE            */
         LR    ERRWORD,REG15                                    04S0206
*        IF ERRWORD ^= NOERROR THEN    /*ANY DADSM ERRORS ?          */
         LTR   ERRWORD,ERRWORD                                  03S0207
         BZ    @RF00207                                         03S0207
*          DO;                                                  03S0208
*/********************************************************************/
*/*                                                                  */
*/*  SEGMENT NAME - DADSERR3                                         */
*/*                                                                  */
*/*  DESCRIPTIVE NAME - NON-SPECIFIC DA VOLUME DADSM ERROR RTN       */
*/*                                                                  */
*/*  FUNCTION - CLEANUP AFTER DADSM ERRORS ON A VOLUME MOUNTED TO    */
*/*    SATISFY A NON-SPECIFIC VOLUME REQUEST. SCRATCH ALL DATA       */
*/*    SETS JUST ALLOCATED TO THIS VOLUME.                           */
*/*                                                                  */
*/*                                                                  */
*/*  ENTRY - INLINE SEGMENT OF IEFAB492.                             */
*/*                                                                  */
*/*    PURPOSE - SEE FUNCTION                                        */
*/*                                                                  */
*/*    INPUT -                                                       */
*/*      FRSTVMVP -  POINTER TO FIRST VM&V REQUEST BLOCK.            */
*/*      VMVREQBP -  POINTER TO VM&V RB WHICH REQUESTED THE MOUNT    */
*/*                    OF THE VOLUME ON WHICH THE FAILURE OCCURED.   */
*/*                                                                  */
*/*  EXIT -                                                          */
*/*                                                                  */
*/*    CONDITION - ALL DATA JUST GOTTEN ON THIS VOLUME SCRATCHED     */
*/*                                                                  */
*/*      OUTPUT -                                                    */
*/*        DATA SETS JUST GOTTEN REMOVED FROM VOLUME.                */
*/*                                                                  */
*/*      RETURN CODE                                                 */
*/*        ERRWORD - 12, TO INDICATE A RECOVERABLE DADSM ERROR       */
*/*                                                                  */
*/*    CONDITION - NON-RECOVERABLE DADSM ERROR                       */
*/*                                                                  */
*/*      OUTPUT -                                                    */
*/*        DATA SETS JUST GOTTEN REMOVED FROM VOLUME.                */
*/*        SIOTRSNC - IN SIOT,REASON CODE GIVING CAUSE OF FAILURE    */
*/*                                                                  */
*/*      RETURN CODE -                                               */
*/*        ERRWORD - 16, TO INDICATE ERROR RECOVERY NOT POSSIBLE     */
*/*                                                                  */
*/*    CONDITION - ERROR DURING SCRATCH PROCESSING                   */
*/*                                                                  */
*/*      OUTPUT -                                                    */
*/*        DATA SETS JUST GOTTEN REMOVED FROM VOLUME IF POSSIBLE     */
*/*        SIOTRSNC - 55, TO INDICATE SCRATCH ERROR                  */
*/*                                                                  */
*/*      RETURN CODE -                                               */
*/*        ERRWORD - 16, TO INDICATE ERROR RECOVERY NOT POSSIBLE     */
*/*                                                                  */
*/********************************************************************/
*AVMVRBP = FRSTVMVP;                   /*POINT TO FIRST VM&V REQ BLK */
         L     AVMVRBP,FRSTVMVP                                 04S0209
*SCRERRSW = OFF;                       /*SET ERROR SWITCH OFF        */
         NI    SCRERRSW,B'01111111'                             04S0210
*DO WHILE AVMVRBP^= ZERO;              /*LOOP THRU ALL REQUEST BLOCKS*/
         B     @DE00211                                         04S0211
@DL00211 DS    0H                                               04S0212
*  IF VMVVUNTP -> VOLID = AVMVRBP -> VMVVUNTP-> VOLID & /*VOLID MATCH*/
*    AVMVRBP-> VMVVUNTP-> VOLSIOTP-> SIODADSM =ON THEN /*AND DADSM
*                                                        SUCCESSFUL ?*/
         L     @10,VMVREQBP                                     04S0212
         L     @10,VMVVUNTP(,@10)                               04S0212
         L     @09,VMVVUNTP(,AVMVRBP)                           04S0212
         CLC   VOLID(6,@10),VOLID(@09)                          04S0212
         BNE   @RF00212                                         04S0212
         L     @10,VOLSIOTP(,@09)                               04S0212
         TM    SIODADSM(@10),B'00000100'                        04S0212
         BNO   @RF00212                                         04S0212
*      DO;                                                      04S0213
*        DISPRB = DISPRB && DISPRB;    /*CLEAR REQUEST BLOCK FOR
*                                        DELETE PROCESSING           */
         XC    DISPRB(16),DISPRB                                04S0214
*        FMAP = FMAP && FMAP;          /* CLEAR UNALOC FNC MAP       */
         XC    FMAP(4),FMAP                                     04S0215
*        CFMAPTR = ADDR(FMAP);         /* POINT TO UNALOC FMAP       */
         LA    CFMAPTR,FMAP                                     04S0216
*        CMDISPRC = ON;                /* DO DISP PROCESSING         */
         OI    CMDISPRC(CFMAPTR),B'00100000'                    04S0217
*        CMNOPOST = ON;                /* BYPASS QMNGR POST          */
*        CMNOVUT = ON;                 /* NO VUT PROCESSING          */
*        CMPURGE = ON;                 /*REQUEST PURGE OPTION        */
         OI    CMNOPOST(CFMAPTR),B'11000010'                    04S0220
*        IF NOTIOTNQ = ON THEN         /*HAS TIOT BEEN ENQ'ED ON?    */
         L     @10,@PC00001+32                                  04S0221
         L     @10,FMAPPTR(,@10)                                04S0221
         TM    NOTIOTNQ(@10),B'00010000'                        04S0221
         BNO   @RF00221                                         04S0221
*          CMTIOTNQ = ON;              /*YES, INDICATE               */
         OI    CMTIOTNQ(CFMAPTR),B'00000001'                    04S0222
*        CMNOSMF = ON;                 /*NO SMF RECORD               */
@RF00221 OI    CMNOSMF(CFMAPTR),B'01000000'                     04S0223
*        CMRBPTR=ADDR(DISPRB);         /*POINT TO UNALLOC RB         */
         LA    @10,DISPRB                                       04S0224
         ST    @10,CMRBPTR                                      04S0224
*        CMSIOPTR = AVMVRBP-> VMVVUNTP-> VOLSIOTP; /*SET PTR TO SIOT */
         L     @09,VMVVUNTP(,AVMVRBP)                           04S0225
         MVC   CMSIOPTR(4,@10),VOLSIOTP(@09)                    04S0225
*        CMORDISP = ON;                /*SET SWITCH TO OVERRIDE DISP */
         OI    CMORDISP(@10),B'00010000'                        04S0226
*        CMODEL = ON;                  /*REQUEST DELETE PROCESSING   */
         OI    CMODEL(@10),B'00000100'                          04S0227
*        CALL IEFAB4A0(FMAP,CMRBPTR,JSCBPTR,ZERO,ZERO,ZERO,ZERO,
*            URCODES,0);               /*DELETE DATA SET     @ZD03010*/
         MVC   @AL00001+12(24),@AL00228                         04S0228
         LA    @10,FMAP                                         04S0228
         ST    @10,@AL00001                                     04S0228
         LA    @10,CMRBPTR                                      04S0228
         ST    @10,@AL00001+4                                   04S0228
         L     @10,@PC00001+24                                  04S0228
         ST    @10,@AL00001+8                                   04S0228
         LA    @10,URCODES                                      04S0228
         ST    @10,@AL00001+28                                  04S0228
         L     @15,@CV01146                                     04S0228
         LA    @01,@AL00001                                     04S0228
         BALR  @14,@15                                          04S0228
*        IF CMRETCDE^= ZERO × REG15 ^= ZERO THEN                04S0229
         SLR   @14,@14                                          04S0229
         L     @10,CMRBPTR                                      04S0229
         CH    @14,CMRETCDE(,@10)                               04S0229
         BNE   @RT00229                                         04S0229
         CR    REG15,@14                                        04S0229
         BE    @RF00229                                         04S0229
@RT00229 DS    0H                                               04S0230
*          DO;                         /*PUT CODE IN SIOT AND        */
*            AVMVRBP -> VMVVUNTP -> VOLSIOTP -> SIOTRSNC = SCRTCHER;
         L     @10,VMVVUNTP(,AVMVRBP)                           04S0231
         L     @10,VOLSIOTP(,@10)                               04S0231
         MVC   SIOTRSNC(2,@10),@CH00318                         04S0231
*            AVMVRBP -> VMVVUNTP -> VOLSIOTP -> SIOTALCD=OFF;   04S0232
         NI    SIOTALCD(@10),B'11111101'                        04S0232
*            SCRERRSW = ON;            /*INDICATE SCRATCH ERROR      */
         OI    SCRERRSW,B'10000000'                             04S0233
*          END;                                                 04S0234
*        ELSE                                                   04S0235
*          DO;                                                  04S0235
         B     @RC00229                                         04S0235
@RF00229 DS    0H                                               04S0236
*            AVMVRBP->VMVVUNTP->VOLSIOTP->SIODADSM=OFF; /*CLEAR DADSM
*                                        SUCCESSFUL INDICATOR        */
         L     @10,VMVVUNTP(,AVMVRBP)                           04S0236
         L     @10,VOLSIOTP(,@10)                               04S0236
         NI    SIODADSM(@10),B'11111011'                        04S0236
*            AVMVRBP->VMVPROCD=OFF;    /*SHOW VMVRB NOT PROCESSED    */
         NI    VMVPROCD(AVMVRBP),B'01111111'                    04S0237
*          END;                                                 04S0238
*          AVMVRBP -> VMVVUNTP -> VOLSIOTP -> SIOTDSPD = OFF;   04S0239
@RC00229 L     @10,VMVVUNTP(,AVMVRBP)                           04S0239
         L     @10,VOLSIOTP(,@10)                               04S0239
         NI    SIOTDSPD(@10),B'10111111'                        04S0239
*      END;                                                     04S0240
*  ELSE;                               /*SCRATCH NOT REQUIRED        */
@RF00212 DS    0H                                               04S0242
*  AVMVRBP = AVMVRBP -> VMVNREQP;      /*POINT TO NEXT VM&V RB       */
         L     AVMVRBP,VMVNREQP(,AVMVRBP)                       04S0242
*END;                                                           04S0243
@DE00211 LTR   AVMVRBP,AVMVRBP                                  04S0243
         BNZ   @DL00211                                         04S0243
*INDEX = ERRWORD/FOUR;                 /*CALC INDEX IN ERROR ARRAY   */
         LR    @00,ERRWORD                                      04S0244
         SRDA  @00,32                                           04S0244
         D     @00,@CF00096                                     04S0244
         LR    INDEX,@01                                        04S0244
*VMVVUNTP -> VOLSIOTP -> SIOTRSNC = ERORCODE(INDEX); /*YES,SHOW CAUSE
*                                        OF FAILURE                  */
         L     @10,VMVREQBP                                     04S0245
         L     @10,VMVVUNTP(,@10)                               04S0245
         L     @10,VOLSIOTP(,@10)                               04S0245
         LR    @09,INDEX                                        04S0245
         ALR   @09,@09                                          04S0245
         LH    @09,ERORCODE-2(@09)                              04S0245
         STH   @09,SIOTRSNC(,@10)                               04S0245
*VMVVUNTP -> VOLSIOTP -> SIOTALCD = OFF; /*SHOW ALLOC NOT SUCCESSFUL */
         NI    SIOTALCD(@10),B'11111101'                        04S0246
*IF ERORCODE(INDEX) = NORECOV1  ×      /*INVALID JFCB ERROR          */
*   ERORCODE(INDEX) = NORECOV2  ×      /*INVALID USER LABEL          */
*   ERORCODE(INDEX) = NORECOV3  ×      /*NO SPACE PARM               */
*   ERORCODE(INDEX) = NORECOV4 ×       /*TOO LARGE DIRECTORY SPACE   */
*   ERORCODE(INDEX) = NORECOV5 ×       /*NOT AUTHORIZED      @ZA16820*/
*   (ERORCODE(INDEX) = NORECOV6 &      /*DATA SET ALREADY DEF@ZA16820*/
*    RCVTVERS = ZERO) ×                /*AND THIS IS RACF 1  @ZA16820*/
*   (ERORCODE(INDEX) = NORECOV6 &      /*DS ALREADY DEFINED  @ZA16820*/
*    RCVTVERS ^= ZERO &                /*IS RACF 2 SYSTEM    @ZA16820*/
*    RCVTNDUP = ON) ×                  /*BUT INSTALLATION DOES NOT
*                                        ALLOW DUP NAMES DEFINED
*                                        TO RACF             @ZA16820*/
*   UCBPRES = ON THEN                  /*OR FIXED DEVICE             */
         CH    @09,@CH00301                                     04S0247
         BE    @RT00247                                         04S0247
         CH    @09,@CH00133                                     04S0247
         BE    @RT00247                                         04S0247
         CH    @09,@CH00304                                     04S0247
         BE    @RT00247                                         04S0247
         CH    @09,@CH00306                                     04S0247
         BE    @RT00247                                         04S0247
         CH    @09,@CH00308                                     04S0247
         BE    @RT00247                                         04S0247
         CH    @09,@CH00098                                     04S0247
         BNE   @GL00010                                         04S0247
         L     @10,CVTPTR                                       04S0247
         L     @10,CVTRAC(,@10)                                 04S0247
         CLI   RCVTVERS(@10),0                                  04S0247
         BE    @RT00247                                         04S0247
@GL00010 LR    @10,INDEX                                        04S0247
         ALR   @10,@10                                          04S0247
         LH    @10,ERORCODE-2(@10)                              04S0247
         CH    @10,@CH00098                                     04S0247
         BNE   @GL00009                                         04S0247
         L     @10,CVTPTR                                       04S0247
         L     @10,CVTRAC(,@10)                                 04S0247
         CLI   RCVTVERS(@10),0                                  04S0247
         BE    @GL00009                                         04S0247
         TM    RCVTNDUP(@10),B'00010000'                        04S0247
