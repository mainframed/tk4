         TITLE 'IEFAB478 - ALLOC FROM GROUPS ALG PICKED                *
                        '
IEFAB478 CSECT ,                                                01S0001
@MAINENT DS    0H                                               01S0001
         USING *,@15                                            01S0001
         B     @PROLOG                                          01S0001
         DC    AL1(24)                                          01S0001
         DC    C'IEFAB478 78054  UZ16857 '                      01S0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                  01S0001
         BALR  @12,0                                            01S0001
@PSTART  DS    0H                                               01S0001
         USING @PSTART,@12                                      01S0001
         L     @00,@SIZDATD                                     01S0001
         GSPACE   R,LV=(0)
         LR    @11,@01                                          01S0001
         USING @DATD,@11                                        01S0001
         ST    @13,@SA00001+4                                   01S0001
         LM    @00,@01,20(@13)                                  01S0001
         ST    @11,8(,@13)                                      01S0001
         LR    @13,@11                                          01S0001
         MVC   @PC00001(8),0(@01)                               01S0001
*RTNCODE = SUCCESS;                    /*ASSUME NO ERRORS            */
         SLR   RTNCODE,RTNCODE                                  01S0082
*SIOTPTR = SIOT1P;                     /*POINT TO FIRST SIOT IN CHAIN*/
         L     @02,@PC00001                                     01S0083
         L     @02,ALCWAPTR(,@02)                               01S0083
         MVC   SIOTPTR(4),SIOT1P(@02)                           01S0083
*DO WHILE (SIOTPTR^=ZERO                                        01S0084
*    & RTNCODE=SUCCESS & INDRETRY=NO); /*SCAN ALL SIOTS              */
         B     @DE00084                                         01S0084
@DL00084 DS    0H                                               01S0085
* IF SIOTALCD = NO & SIOTAFID=ZERO THEN /*IS SIOT FULLY ALLOCATED ?
*                                        OR PART OF A MULTI-UNIT/
*                                        GEN GROUP ?                 */
         L     @02,SIOTPTR                                      01S0085
         TM    SIOTALCD(@02),B'00000010'                        01S0085
         BNZ   @RF00085                                         01S0085
         SLR   @15,@15                                          01S0085
         CH    @15,SIOTAFID(,@02)                               01S0085
         BNE   @RF00085                                         01S0085
*  DO;                                 /*NO, PROCESS                 */
*    DONESIOT = YES;                   /*ASSUME SIOT WILL BE ALLOC'D */
         OI    DONESIOT,B'10000000'                             01S0087
*    VUPTR = SVOLUNAD;                 /*POINT TO SIOTS FIRST VOLUNIT*/
         MVC   VUPTR(4),SVOLUNAD(@02)                           01S0088
*    RELDEVNO = ZERO;                  /*INIT COUNT OF UNITS REQUIRED*/
         STH   @15,RELDEVNO                                     01S0089
*    DO VUCTR = ONE TO SVOLUNNO        /*SCAN ALL VU ENTRIES FOR SIOT*/
*        WHILE (RTNCODE=SUCCESS & INDRETRY=NO);                 01S0090
         LA    VUCTR,1                                          01S0090
         B     @DE00090                                         01S0090
@DL00090 SLR   @06,@06                                          01S0090
         CR    RTNCODE,@06                                      01S0090
         BNE   @DC00090                                         01S0090
         L     @02,@PC00001                                     01S0090
         L     @02,ALCWAPTR(,@02)                               01S0090
         TM    INDRETRY(@02),B'01000000'                        01S0090
         BNZ   @DC00090                                         01S0090
*      IF VDEVREQD = ON THEN           /*IS A DEVICE REQUIRED ?      */
         L     @02,VUPTR                                        01S0091
         TM    VDEVREQD(@02),B'00100000'                        01S0091
         BNO   @RF00091                                         01S0091
*        DO;                           /*YES, PROCESS                */
*          RELDEVNO = RELDEVNO + ONE;  /*INCREMENT THE COUNT         */
         LA    @15,1                                            01S0093
         LH    @14,RELDEVNO                                     01S0093
         ALR   @14,@15                                          01S0093
         STH   @14,RELDEVNO                                     01S0093
*          IF VUDNALOC = OFF           /*IF VU CAN BE PROCESSED..    */
*              & VOLALOC = OFF         /*AND IS STILL UNALLOCATED..  */
*              & CVRSKFLG = OFF THEN   /*AND WAS CONSIDER BY ALG,..  */
         TM    VUDNALOC(@02),B'10001000'                        01S0094
         BNZ   @RF00094                                         01S0094
         L     @02,VOLALGTP(,@02)                               01S0094
         TM    CVRSKFLG(@02),B'10000000'                        01S0094
         BNZ   @RF00094                                         01S0094
*            DO;                       /*..THEN PROCESS              */
*              GRPENTP = CVRGRPPT;     /*POINT TO FIRST GROUP ENTRY  */
         L     @01,VUPTR                                        01S0096
         L     @14,VOLALGTP(,@01)                               01S0096
         L     GRPENTP,CVRGRPPT(,@14)                           01S0096
*              MSSCNTL = MSSCNTL && MSSCNTL; /*ZERO MSS MOUNT EQUAL.
*                                        CONTROLS            @ZA13405*/
         XC    MSSCNTL(12),MSSCNTL                              01S0097
*              MSSPLIST = ON;          /*INIT. PLIST FOR MOUNT EQUAL
*                                        IF NEEDED           @ZA13405*/
*              MSSRECUR = ON;          /*PROCESS OTHER GROUPS FOR
*                                        MOUNT EQUAL.        @ZA13405*/
         OI    MSSPLIST,B'01010000'                             01S0099
*              GETCORE = ON;           /*GET CORE FOR CANDIDATE 01S0100
*                                        LIST                @ZA13405*/
         OI    GETCORE,B'10000000'                              01S0100
*              SSCUCBLP = ZERO;        /*INIT UCB PTRS       @ZA13405*/
         ST    @06,SSCUCBLP                                     01S0101
*              DO GRPCTR=ONE TO CVRNGRP /*SCAN ALL REQS GROUP ENTRIES*/
*                  WHILE (RTNCODE=SUCCESS /*UNTIL AN ERROR OCCURS..  */
*                  & INDRETRY=NO                                01S0102
*                  & VOLALOC=OFF);     /*..OR THE VU ENTRY IS ALLOCD */
         LR    GRPCTR,@15                                       01S0102
         B     @DE00102                                         01S0102
@DL00102 SLR   @15,@15                                          01S0102
         CR    RTNCODE,@15                                      01S0102
         BNE   @DC00102                                         01S0102
         L     @14,@PC00001                                     01S0102
         L     @01,ALCWAPTR(,@14)                               01S0102
         TM    INDRETRY(@01),B'01000000'                        01S0102
         BNZ   @DC00102                                         01S0102
         L     @01,VUPTR                                        01S0102
         TM    VOLALOC(@01),B'10000000'                         01S0102
         BNZ   @DC00102                                         01S0102
*                IF CVRGSKP=OFF & CVRGALL> ZERO THEN /*HAS ALG PICK
*                                        THIS GROUP ?                */
         TM    CVRGSKP(GRPENTP),B'10000000'                     01S0103
         BNZ   @RF00103                                         01S0103
         CH    @15,CVRGALL(,GRPENTP)                            01S0103
         BNL   @RF00103                                         01S0103
*                  DO;                 /*YES, ALLOC FROM THIS GROUP  */
*                    ARUGRPID=CVRGRPID; /*COPY THE GROUP ID FOR ARU  */
         LH    @15,CVRGRPID(,GRPENTP)                           01S0105
         ST    @15,ARUGRPID                                     01S0105
*                    CALL GENAPREP;    /*ALLOC FROM GROUP    @ZA13405*/
         BAL   @14,GENAPREP                                     01S0106
*                    IF MSSDEV = ON &  /*MSS UNIT AND        @ZA13405*/
*                       MSSRECUR = ON &/*RECURSION OK AND    @ZA13405*/
*                       MSSMATCH = OFF THEN /*MATCH NOT FOUND@ZA13405*/
         TM    MSSDEV,B'00011000'                               01S0107
         BNO   @RF00107                                         01S0107
         TM    MSSMATCH,B'00100000'                             01S0107
         BNZ   @RF00107                                         01S0107
*                      DO;             /*PROCESS OTHER GROUPS@ZA13405*/
*/********************************************************** @ZA13405*/
*/*                                                                  */
*/* SEGMENT NAME - MSSPROC                                   @ZA13405*/
*/*                                                                  */
*/* DESCRIPTIVE NAME - MSS GROUP PROCESSOR                   @ZA13405*/
*/*                                                                  */
*/* FUNCTION - THIS ROUTINE IS CALLED IF THE ALGORITHM DID   @ZA13405*/
*/*            NOT CHOOSE A UNIT THAT WAS IN THE FIRST LIST  @ZA13405*/
*/*            RETURNED BY ICBME. THE FIRST LIST RETURNED    @ZA13405*/
*/*            BY ICBME MAY HAVE BEEN RETURNED FOR DATA      @ZA13405*/
*/*            REUSE OR MOUNT EQUALIZATION. THIS ROUTINE     @ZA13405*/
*/*            WILL COMPARE ALL ELIGIBLE GROUPS FROM THE     @ZA13405*/
*/*            ALGORITHM TABLE AGAINST THE FIRST LIST  FROM  @ZA13405*/
*/*            ICBME. IF NO MATCH IS FOUND THE NEXT LIST IS  @ZA13405*/
*/*            REQUESTED FROM ICBME, IT IS COMPARED WITH     @ZA13405*/
*/*            THE UNITS FROM THE GROUP CHOSEN BY THE        @ZA13405*/
*/*            ALGORITHM. IF NO MATCH IS FOUND SUBSEQUENT    @ZA13405*/
*/*            GROUPS ARE PROCESSED AS DESCRIBED ABOVE.      @ZA13405*/
*/*            NOTE:  A GROUP IS ELIGIBLE IF IT HAS EXCESS   @ZA13405*/
*/*            UNITS (GRPEXCS>0), AND THE ALGORITHM DETER-   @ZA13405*/
*/*            MINED THAT IT IS ALRIGHT TO ALLOCATE FROM THE @ZA13405*/
*/*            GROUP (CVRGSKP^=1 AND CVRGALL=0). WHEN AN     @ZA13405*/
*/*            ELIGIBLE GROUP IS FOUND THE ALGORITHM TABLES  @ZA13405*/
*/*            ARE UPDATED TO REFLECT THE NEW CHOICE. IF     @ZA13405*/
*/*            AFTER ALL LISTS FROM ICBME (SSCUCBLP=0) ARE   @ZA13405*/
*/*            PROCESSED AND A MATCH IS STILL NOT FOUND THEN @ZA13405*/
*/*            THE GROUP THE ALGORITHM CHOSE IS PROCESSED    @ZA13405*/
*/*            WITH MSSRECUR TURNED ON WHICH WILL CAUSE      @ZA13405*/
*/*            THE OPTIMIZER TO GET INVOKED TO PICK ONE      @ZA13405*/
*/*            OF THE UNITS FROM THE GROUP THE ALGORITHM     @ZA13405*/
*/*            CHOSE.                                        @ZA13405*/
*/*                                                                  */
*/*   OPERATION - THE OPERATION IS DOCUMENTED BY THE CODE    @ZA13405*/
*/*                                                                  */
*/* ENTRY - (IN-LINE SUBROUTINE)                             @ZA13405*/
*/*                                                                  */
*/*   PURPOSE - (SEE FUNCTION)                               @ZA13405*/
*/*                                                                  */
*/*   INPUT - GRPENTP - PTR TO GROUP ENTRY                   @ZA13405*/
*/*           SSCUCBLP - PTR TO LIST OF UCB'S FROM ICBME     @ZA13405*/
*/*           CURGRPTR - PTR TO FIRST GROUP ENTRY            @ZA13405*/
*/*           CURGSKP - SKIP GROUP INDICATOR                 @ZA13405*/
*/*           GRPEXCS - NUMBER OF UNITS IN GROUP THAT HAS    @ZA13405*/
*/*                     NOT BEEN CHOSEN FOR ALLOCATION BY    @ZA13405*/
*/*                     THE ALGORITHM.                       @ZA13405*/
*/*           CURGALL - NUMBER ALLOCATED BY COVER IF:        @ZA13405*/
*/*                     1 - ALLOCATE FROM GROUP              @ZA13405*/
*/*                     0 - GROUP ELIGIBLE FOR ALLOCATION    @ZA13405*/
*/*                     FF - DO NOT ALLOCATE FROM GROUP      @ZA13405*/
*/*                                                                  */
*/* EXIT - NORMAL - (IN-LINE SUBROUTINE)                     @ZA13405*/
*/*                                                                  */
*/*   CONDITIONS - ALGORITHM TABLES UPDATED TO REFLECT GROUP @ZA13405*/
*/*                UNIT ALLOCATION FROM                      @ZA13405*/
*/*                                                                  */
*/*   OUTPUT - GRPEXCS - NUMBER OF EXCESS UNITS (SEE INPUT)  @ZA13405*/
*/*            CVRGALL - NUMBER ALLOCATED BY COVER(SEE INPUT)@ZA13405*/
*/*                                                                  */
*/*   RETURN CODES - NONE                                    @ZA13405*/
*/*                                                                  */
*/********************************************************** @ZA13405*/
*MSSALGP = GRPENTP;                    /*SAVE GRP ALG. CHOSE @ZA13405*/
         ST    GRPENTP,MSSALGP                                  02S0109
*DO WHILE MSSMATCH = OFF &             /*DO WHILE NO MATCH   @ZA13405*/
*         SSCUCBLP ^= ZERO;            /*AND OTHER LISTS     @ZA13405*/
         B     @DE00110                                         02S0110
@DL00110 DS    0H                                               02S0111
*  GRPENTP = CVRGRPPT;                 /*FIRST GROUP ENTRY   @ZA13405*/
         L     @06,VUPTR                                        02S0111
         L     @06,VOLALGTP(,@06)                               02S0111
         L     GRPENTP,CVRGRPPT(,@06)                           02S0111
*  MSSCURGP = MSSALGP;                 /*SET CUR GRPENT PTR  @ZA13405*/
         MVC   MSSCURGP(4),MSSALGP                              02S0112
*  DO GRPCTR = 1 TO CVRNGRP WHILE      /*SCAN GROUP WHILE    @ZA13405*/
*     MSSMATCH = OFF;                  /*NO MATCH            @ZA13405*/
         LA    GRPCTR,1                                         02S0113
         B     @DE00113                                         02S0113
@DL00113 TM    MSSMATCH,B'00100000'                             02S0113
         BNZ   @DC00113                                         02S0113
*    IF CVRGSKP = OFF &                /*DO NOT SKIP GROUP & @ZA13405*/
*       CVRGALL = ZERO &               /*GROUP ELIGIBLE      @ZA13405*/
*       GRPEXCS > ZERO &               /*GROUP HAS UNITS     @ZA13405*/
*       GRPENTP ^= MSSALGP THEN        /*AND NOT GROUP ALG PICKED
*                                        THEN                @ZA13405*/
         TM    CVRGSKP(GRPENTP),B'10000000'                     02S0114
         BNZ   @RF00114                                         02S0114
         SLR   @15,@15                                          02S0114
         CH    @15,CVRGALL(,GRPENTP)                            02S0114
         BNE   @RF00114                                         02S0114
         L     @14,CVRGCPTR(,GRPENTP)                           02S0114
         LH    @09,GRPEXCS(,@14)                                02S0114
         CR    @09,@15                                          02S0114
         BNH   @RF00114                                         02S0114
         C     GRPENTP,MSSALGP                                  02S0114
         BE    @RF00114                                         02S0114
*      DO;                             /*UPDATE ALG TABLES   @ZA13405*/
*        GRPEXCS = GRPEXCS - ONE;      /*DECR EXCESS UNITS   @ZA13405*/
         BCTR  @09,0                                            02S0116
         STH   @09,GRPEXCS(,@14)                                02S0116
*        CVRGALL = ONE;                /*ALLOCATE FROM GROUP @ZA13405*/
         LA    @14,1                                            02S0117
         STH   @14,CVRGALL(,GRPENTP)                            02S0117
*        MSSCURGP->CVRGCPTR->GRPEXCS = /*INCREMENT NUMBER OF @ZA13405*/
*          MSSCURGP->CVRGCPTR->GRPEXCS+ONE; /*EXCESS UNITS   @ZA13405*/
         L     @09,MSSCURGP                                     02S0118
         L     @08,CVRGCPTR(,@09)                               02S0118
         AH    @14,GRPEXCS(,@08)                                02S0118
         STH   @14,GRPEXCS(,@08)                                02S0118
*        MSSCURGP->CVRGALL = ZERO;     /*MAKE GRP ELIGIBLE   @ZA13405*/
         STH   @15,CVRGALL(,@09)                                02S0119
*        MSSCURGP = GRPENTP;           /*SET CUR GRPENT PTR  @ZA13405*/
         ST    GRPENTP,MSSCURGP                                 02S0120
*        MSSTBLUP = ON;                /*ALG. TBLS UPDATED   @ZA13405*/
         OI    MSSTBLUP,B'00000100'                             02S0121
*        ARUGRPID = CVRGRPID;          /*SET GROUP ID        @ZA13405*/
         LH    @15,CVRGRPID(,GRPENTP)                           02S0122
         ST    @15,ARUGRPID                                     02S0122
*        CALL GENAPREP;                /*ALLOC FROM GROUP    @ZA13405*/
         BAL   @14,GENAPREP                                     02S0123
*      END;                            /*                    @ZA13405*/
*    GRPENTP = GRPENTP+LENGTH(CVRGPLST); /*UPDATE POINTER    @ZA13405*/
@RF00114 AH    GRPENTP,@CH00048                                 02S0125
*  END;                                /*END SCAN OF GROUPS  @ZA13405*/
         AH    GRPCTR,@CH00040                                  02S0126
@DE00113 L     @01,VUPTR                                        02S0126
         L     @01,VOLALGTP(,@01)                               02S0126
         CH    GRPCTR,CVRNGRP(,@01)                             02S0126
         BNH   @DL00113                                         02S0126
@DC00113 DS    0H                                               02S0127
*  IF MSSMATCH = OFF &                 /*NO MATCH            @ZA13405*/
*     MSSTBLUP = ON THEN               /*ALG. TBLS UPDATED   @ZA13405*/
         TM    MSSTBLUP,B'00000100'                             02S0127
         BNO   @RF00127                                         02S0127
         TM    MSSMATCH,B'00100000'                             02S0127
         BNZ   @RF00127                                         02S0127
*    DO;                               /*RESET TO GROUP ALGORITHM
*                                        CHOSE               @ZA13405*/
*      MSSCURGP->CVRGCPTR->GRPEXCS =    /*INCREMENT EXCESS   @ZA13405*/
*        MSSCURGP->CVRGCPTR->GRPEXCS+ONE; /*UNITS            @ZA13405*/
         L     @15,MSSCURGP                                     02S0129
         L     @14,CVRGCPTR(,@15)                               02S0129
         LA    @09,1                                            02S0129
         LH    @08,GRPEXCS(,@14)                                02S0129
         ALR   @08,@09                                          02S0129
         STH   @08,GRPEXCS(,@14)                                02S0129
*      MSSCURGP->CVRGALL = ZERO;       /*GROUP IS ELIGIBLE   @ZA13405*/
         SLR   @14,@14                                          02S0130
         STH   @14,CVRGALL(,@15)                                02S0130
*      MSSALGP->CVRGCPTR->GRPEXCS =    /*DECREMENT EXCESS    @ZA13405*/
*        MSSALGP->CVRGCPTR->GRPEXCS-ONE; /*UNITS             @ZA13405*/
         L     @08,MSSALGP                                      02S0131
         L     @15,CVRGCPTR(,@08)                               02S0131
         LH    @14,GRPEXCS(,@15)                                02S0131
         BCTR  @14,0                                            02S0131
         STH   @14,GRPEXCS(,@15)                                02S0131
*      MSSALGP->CVRGALL = ONE;         /*ALLOCATE FROM GROUP @ZA13405*/
         STH   @09,CVRGALL(,@08)                                02S0132
*      ARUGRPID = MSSALGP->CVRGRPID;   /*SET GROUP ID        @ZA13405*/
         LH    @15,CVRGRPID(,@08)                               02S0133
         ST    @15,ARUGRPID                                     02S0133
*       MSSTBLUP = OFF;                /*SET TBL UPDATE OFF  @ZA13405*/
         NI    MSSTBLUP,B'11111011'                             02S0134
*    END;                              /*END RESETING GRP.   @ZA13405*/
*    IF SSCUCBLP ^= ZERO &             /*ICBME MAY HAVE      @ZA13405*/
*       MSSMATCH = OFF THEN            /*ANOTHER LIST        @ZA13405*/
@RF00127 L     @15,SSCUCBLP                                     02S0136
         LTR   @15,@15                                          02S0136
         BZ    @RF00136                                         02S0136
         TM    MSSMATCH,B'00100000'                             02S0136
         BNZ   @RF00136                                         02S0136
*      DO;                             /*                    @ZA13405*/
*        MSSNXLST = ON;                /*GET NEXT LIST       @ZA13405*/
         OI    MSSNXLST,B'10000000'                             02S0138
*         DO; /*MODESET  EXTKEY(SCHED)*/                        02S0139
*           SPKA (16); /*SET PSW KEY*/                          02S0140
         LA    @01,16                                           02S0140
         SPKA  0(@01)                                           02S0140
*         END; /*MODESET  EXTKEY(SCHED)*/       /*KEY 1 FOR FREE      @
*A13405*/                                                       02S0141
*        RFY (REG0,REG1) RSTD;         /*                    @ZA13405*/
*        REG0 = SSCLSPLN;              /*LENGTH AND SUBPOOL  @ZA13405*/
         L     REG0,SSCLSPLN(,@15)                              02S0143
*        REG1 = SSCUCBLP;              /*LIST FROM ICBME     @ZA13405*/
         LR    REG1,@15                                         02S0144
*        GEN (FREEMAIN R,LV=(0),A=(1));   /*FREE THE LIST    @ZA13405*/
         FREEMAIN R,LV=(0),A=(1)
*        RFY (REG0,REG1) UNRSTD;       /*                    @ZA13405*/
*        SSCUCBLP = ZERO;              /*ZERO LIST ADDRESS   @ZA13405*/
         SLR   @15,@15                                          02S0147
         ST    @15,SSCUCBLP                                     02S0147
*         DO; /*MODESET  KEYADDR(ZERO)*/                        02S0148
*           SPKA (ZERO); /*SET PSW KEY*/                        02S0149
         SPKA  0(@15)                                           02S0149
*         END; /*MODESET  KEYADDR(ZERO)*/       /*RESUME KEY 0        @
*A13405*/                                                       02S0150
*        CALL GENAPREP;                /*ALLOC FROM GROUP    @ZA13405*/
         BAL   @14,GENAPREP                                     02S0151
*        MSSNXLST = OFF;               /*TURN NEXT LIST OFF  @ZA13405*/
         NI    MSSNXLST,B'01111111'                             02S0152
*      END;                            /*                    @ZA13405*/
*END;                                  /*END DO WHILE NO MATCH AND
*                                        OTHER LISTS TO PROCESS 02S0154
*                                                            @ZA13405*/
@RF00136 DS    0H                                               02S0154
@DE00110 TM    MSSMATCH,B'00100000'                             02S0154
         BNZ   @DC00110                                         02S0154
         ICM   @15,15,SSCUCBLP                                  02S0154
         BNZ   @DL00110                                         02S0154
@DC00110 DS    0H                                               02S0155
*IF SSCUCBLP = ZERO &                  /*NO LIST FROM ICBME  @ZA13405*/
*   MSSMATCH = OFF THEN                /*AND NO MATCH        @ZA13405*/
         ICM   @15,15,SSCUCBLP                                  02S0155
         BNZ   @RF00155                                         02S0155
         TM    MSSMATCH,B'00100000'                             02S0155
         BNZ   @RF00155                                         02S0155
*  DO;                                 /*ALLOCATE FROM GROUP ALGORITHM
*                                        PICKED              @ZA13405*/
*    MSSRECUR = OFF;                   /*MUST ALLOCATE       @ZA13405*/
         NI    MSSRECUR,B'11101111'                             02S0157
*    CALL GENAPREP;                    /*ALLOC FROM GROUP    @ZA13405*/
         BAL   @14,GENAPREP                                     02S0158
*  END;                                /*END MUST ALLOCATE   @ZA13405*/
*                      END;            /*END PROCESSING GRPS @ZA13405*/
*                  END;                                         01S0161
*                ELSE;                 /*ALG DIDN'T JUST PICK GROUP  */
@RF00103 DS    0H                                               01S0163
*                GRPENTP=GRPENTP+LENGTH(CVRGPLST); /*PT TO NEXT GROUP*/
@RC00103 AH    GRPENTP,@CH00048                                 01S0163
*              END;                                             01S0164
         AH    GRPCTR,@CH00040                                  01S0164
@DE00102 L     @01,VUPTR                                        01S0164
         L     @01,VOLALGTP(,@01)                               01S0164
         CH    GRPCTR,CVRNGRP(,@01)                             01S0164
         BNH   @DL00102                                         01S0164
@DC00102 DS    0H                                               01S0165
*            END;                                               01S0165
*       ELSE;                          /*DONT PROCESS VOLUNIT ENTRY  */
@RF00094 DS    0H                                               01S0167
*     END;                                                      01S0167
*      ELSE;                           /*DEVICE NOT REQUIRED FOR VU  */
@RF00091 DS    0H                                               01S0169
*      IF (VOLALOC=OFF & VDEVREQD = ON) ×                       01S0169
*          (VUDNALOC=ON & VUDADSME=ON) × /*NON-SPEC AFF DADSM@ZA03616*/
*          (VUDNALOC=ON & VUVINELG=ON) THEN /*TAPE AFF ERROR @ZA03616*/
         L     @06,VUPTR                                        01S0169
         TM    VDEVREQD(@06),B'00100000'                        01S0169
         BNO   @GL00014                                         01S0169
         TM    VOLALOC(@06),B'10000000'                         01S0169
         BZ    @RT00169                                         01S0169
@GL00014 L     @06,VUPTR                                        01S0169
         TM    VUDNALOC(@06),B'00001100'                        01S0169
         BO    @RT00169                                         01S0169
         TM    VUDNALOC(@06),B'00001010'                        01S0169
         BNO   @RF00169                                         01S0169
@RT00169 DS    0H                                               01S0170
*        DO;                           /*NO, ENTRY IS NOT SATISFIED  */
*          DONESIOT = NO;              /*INDICATE SIOT NOT FULLY ALOC*/
         NI    DONESIOT,B'01111111'                             01S0171
*        END;                                                   01S0172
*      ELSE;                           /*VU ENTRY IS SATISFIED       */
@RF00169 DS    0H                                               01S0174
*      VUPTR = VUPTR + LENGTH(VOLUNTAB); /* POINT TO NEXT VOLUNIT ENT*/
         LA    @06,28                                           01S0174
         AL    @06,VUPTR                                        01S0174
         ST    @06,VUPTR                                        01S0174
*    END;                                                       01S0175
         AH    VUCTR,@CH00040                                   01S0175
@DE00090 L     @06,SIOTPTR                                      01S0175
         CH    VUCTR,SVOLUNNO(,@06)                             01S0175
         BNH   @DL00090                                         01S0175
@DC00090 DS    0H                                               01S0176
*    IF DONESIOT = YES & SIOTALCD = NO THEN /*IS SIOT FULLY ALLOC'D? */
         TM    DONESIOT,B'10000000'                             01S0176
         BNO   @RF00176                                         01S0176
         L     @02,SIOTPTR                                      01S0176
         TM    SIOTALCD(@02),B'00000010'                        01S0176
         BNZ   @RF00176                                         01S0176
