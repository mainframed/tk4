@RF00207 L     @12,UCBPTR                                       01S0209
         MVC   UCBVOLI(6,@12),LABELSER                          01S0209
*                       IF SLTAPESW = ON THEN /*IF SL INDICATOR ON   */
         TM    SLTAPESW,B'10000000'                             01S0210
         BNO   @RF00210                                         01S0210
*                         UCBDADI = ON; /*SET SL TAPE IN UCB         */
         OI    UCBDADI(@12),B'00000001'                         01S0211
*                       ELSE            /*NOT SL TAPE                */
*                         DO;                                   01S0212
         B     @RC00210                                         01S0212
@RF00210 DS    0H                                               01S0213
*                           IF ALTAPESW = ON THEN /*IF AL INDIC ON   */
         TM    ALTAPESW,B'10000000'                             01S0213
         BNO   @RF00213                                         01S0213
*                             UCBBSTR = ON;   /*SET AL TAPE IN UCB   */
         L     @12,UCBPTR                                       01S0214
         OI    UCBBSTR(@12),B'00000100'                         01S0214
*                           ELSE        /*NOT AL TAP                 */
*                             IF NSLTAPSW = ON THEN /*IF NSL    01S0215
*                                                     INDICAT ON     */
         B     @RC00213                                         01S0215
@RF00213 TM    NSLTAPSW,B'10000000'                             01S0215
         BNO   @RF00215                                         01S0215
*                               UCBNSLTP = ON; /*SET NSL TAPE IN UCB */
         L     @12,UCBPTR                                       01S0216
         OI    UCBNSLTP(@12),B'01000000'                        01S0216
*                         END;                                  01S0217
*                     END;              /*                   @YM07608*/
*                   ELSE;                                       01S0219
@RF00205 DS    0H                                               01S0220
*                   /*UNLOCK AND RESTORE KEY                         */
*                    DO; /* SETLOCK (RELEASE) TYPE(CMS) REGS(USE) RELAT
*D(UCB,IEFAB473(IEFAB473)) */                                   01S0220
@RC00205 DS    0H                                               01S0221
*                      RFY (GPR11P,GPR12P,GPR13P,GPR14P) RSTD;  01S0221
*                      RFY (GPR15P,GPR00P,GPR01P) RSTD;         01S0222
*                      GPR15P = GPR11P; /* SAVE REG. 11 IN REG. 15. */
         LR    GPR15P,GPR11P                                    01S0223
*                      GPR00P = GPR12P; /* SAVE REG. 12 IN REG. 0. */
         LR    GPR00P,GPR12P                                    01S0224
*                      GPR01P = GPR13P; /* SAVE REG. 13 IN REG. 1. */
         LR    GPR01P,GPR13P                                    01S0225
*                      GPR13P = PSALITA; /* ADDRESS OF LOCK INTERFACE T
*BLE. */               GEN(LM GPR11P,GPR13P,552(GPR13P)); /* LOAD LOCKW
*RD, CLHT AND ENTRY ADRS. */                                    01S0226
         L     GPR13P,PSALITA                                   01S0226
         LM GPR11P,GPR13P,552(GPR13P)
*                      GEN(BALR GPR14P,GPR13P); /* BALR TO SETLOCK RTN.
**/                    GPR11P = GPR15P; /* RESTORE REG. 11. */  01S0228
         BALR GPR14P,GPR13P
         LR    GPR11P,GPR15P                                    01S0229
*                      GPR15P = GPR13P; /* SAVE RETURN CODE. */ 01S0230
         LR    GPR15P,GPR13P                                    01S0230
*                      GPR12P = GPR00P; /* RESTORE REG. 12 */   01S0231
         LR    GPR12P,GPR00P                                    01S0231
*                      GPR13P = GPR01P; /* RESTORE REG. 13. */  01S0232
         LR    GPR13P,GPR01P                                    01S0232
*                      RFY(GPR15P,GPR00P,GPR01P) UNRSTD;        01S0233
*                      RFY (GPR11P,GPR12P,GPR13P,GPR14P) UNRSTD;
*                    END; /* SETLOCK (RELEASE) TYPE(CMS) REGS(USE) RELA
*ED(UCB,IEFAB473(IEFAB473)) */ /*RLSE CMS LOK */                01S0235
*                    DO; /* SETLOCK (RELEASE) TYPE(LOCAL) REGS(USE) REL
*TED(UCB,IEFAB473(IEFAB473)) */                                 01S0236
*                      RFY (GPR11P,GPR12P,GPR13P,GPR14P) RSTD;  01S0237
*                      RFY (GPR15P,GPR00P,GPR01P) RSTD;         01S0238
*                      GPR15P = GPR11P; /* SAVE REG. 11 IN REG. 15. */
         LR    GPR15P,GPR11P                                    01S0239
