@RF00190 DS    0H                                               02S0203
*END;                                                           02S0203
         AH    LUTNO,@CH00091                                   02S0203
@DE00189 LR    @12,LUTNO                                        02S0203
         ALR   @12,@12                                          02S0203
         L     @11,@PC00001+4                                   02S0203
         L     @11,LOOKUPTP(,@11)                               02S0203
         ALR   @11,@12                                          02S0203
         BCTR  @11,0                                            02S0203
         BCTR  @11,0                                            02S0203
         CLC   IOSLUT(2,@11),ENDINDIC                           02S0203
