         TITLE 'IEFAB473 - AVR CONTROL                                 *
                        '
IEFAB473 CSECT ,                                                01S0001
@MAINENT DS    0H                                               01S0001
         USING *,@15                                            01S0001
         B     @PROLOG                                          01S0001
         DC    AL1(24)                                          01S0001
         DC    C'IEFAB473 77278  UZ14747 '                      01S0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                  01S0001
         BALR  @10,0                                            01S0001
@PSTART  DS    0H                                               01S0001
         USING @PSTART,@10                                      01S0001
         L     @00,@SIZDATD                                     01S0001
         GSPACE   R,LV=(0)
         LR    @09,@01                                          01S0001
         USING @DATD,@09                                        01S0001
         ST    @13,@SA00001+4                                   01S0001
         LM    @00,@01,20(@13)                                  01S0001
         ST    @09,8(,@13)                                      01S0001
         LR    @13,@09                                          01S0001
         MVC   @PC00001(8),0(@01)                               01S0001
*RETCODE = SUCCESS;                    /*INITIALIZE RETURN CODE TO 0 */
         SLR   RETCODE,RETCODE                                  01S0069
*UCBPTR = UCBLSTEN(ONE);               /*PTR TO UCB                  */
         L     @12,@PC00001                                     01S0070
         L     @12,UCBLISTP(,@12)                               01S0070
         SLR   @11,@11                                          01S0070
         ICM   @11,3,UCBLSTEN(@12)                              01S0070
         ST    @11,UCBPTR                                       01S0070
*DO I=ONE BY ONE WHILE(UCBPTR ^= ENDINDIC); /*DO UNTIL END OF LIST   */
         LA    I,1                                              01S0071
         B     @DE00071                                         01S0071
@DL00071 DS    0H                                               01S0072
*  IF UCBONLI = ON                     /*IS UCB MARKED ONLINE?       */
*    & UCBALOC =OFF                    /*UNALLOCATED?                */
*    & UCBNOTRD = OFF                  /*READY?                      */
*    & UCBNALOC = OFF                  /*NOT IN-USE BY OLTEP         */
*    & UCBVOLI = NOVOLSER THEN         /*ZERO VOLSER?                */
         L     @12,UCBPTR                                       01S0072
         TM    UCBONLI(@12),B'10000000'                         01S0072
         BNO   @RF00072                                         01S0072
         TM    UCBALOC(@12),B'00001000'                         01S0072
         BNZ   @RF00072                                         01S0072
         TM    UCBNOTRD(@12),B'01000000'                        01S0072
         BNZ   @RF00072                                         01S0072
         TM    UCBNALOC(@12),B'00000100'                        01S0072
         BNZ   @RF00072                                         01S0072
         CLC   UCBVOLI(6,@12),@CB00088                          01S0072
         BNE   @RF00072                                         01S0072
*    DO;                               /*YES,THEN PROCESS            */
*     CALL IEFAB4E0(UCBPTR);           /*MAKE UCB NOT READY IF DEVICE
*                                        NOT READY OR NONEXISTENT    */
         LA    @12,UCBPTR                                       01S0074
         ST    @12,@AL00001                                     01S0074
         L     @15,@CV01565                                     01S0074
         LA    @01,@AL00001                                     01S0074
         BALR  @14,@15                                          01S0074
*     IF UCBNOTRD = OFF THEN           /*IF UCB STILL MARKED READY   */
         L     @12,UCBPTR                                       01S0075
         TM    UCBNOTRD(@12),B'01000000'                        01S0075
         BNZ   @RF00075                                         01S0075
*       DO;                                                     01S0076
*/********************************************************************/
*/*                                                                  */
*/* SEGMENT NAME - B473RLBC                                          */
*/*                                                                  */
*/* DESCRIPTIVE NAME - READ LABEL CONTROL                            */
*/*                                                                  */
*/* FUNCTION -                                                       */
*/*   THIS ROUTINE IS INVOKED WHEN AVR CONTROL IS PROCESSING A UCB   */
*/*   AND IS RESPONSIBLE FOR SETTING UP THE INPUT TO AND CALLING THE */
*/*   TAPE LABEL(IEFAB4F9) OR DIRECT ACCESS LABEL(IEFAB4F8) READ     */
*/*   ROUTINE                                                        */
*/*                                                                  */
*/*   OPERATION -                                                    */
*/*     THIS SEGMENT WILL DETERMINE IF THE LABEL TO BE READ IS ON A  */
*/*     TAPE OR DIRECT ACCESS DEVICE.                                */
*/*        -IF TAPE                                                  */
*/*             -7 TRK -WILL CHECK THE SYSGEN OPTION INDICATORS      */
*/*                     (IEFSGOPT) TO DETERMINE IN WHICH DENSITY     */
*/*                     A LABEL READ SHOULD FIRST BE TRIED.THE TAPE  */
*/*                     LABEL READ RTN(IEFAB4F9) WILL BE CALLED TO   */
*/*                     ATTEMPT A READ IN THIS DENSITY.IF THE READ   */
*/*                     IS UNSUCCESSFUL THIS SEGMENT WILL AGAIN CALL */
*/*                     THE TAPE LABEL READ RTN SPECIFYING A         */
*/*                     DIFFERENT DENSITY.ALL DENSITIES (800,556,200)*/
*/*                     WILL BE TRIED (IN THAT ORDER) BEFORE         */
*/*                     REJECTING THE REQUEST.                       */
*/*                                                                  */
*/*             - 9 TRK -WILL CALL THE TAPE LABEL READ RTN(IEFAB4F9) */
*/*                      TO READ THE TAPE LABEL.                     */
*/*        -IF DIRECT ACCESS -                                       */
*/*                       WILL CALL THE DIRECT ACCESS LABEL READ RTN */
*/*                       (IEFAB4F8) TO READ THE VOLUME LABEL.       */
*/*                                                                  */
*/* ENTRY POINT - (INLINE SUBROUTINE - ENTERED AT THE TOP).          */
*/*   PURPOSE - CONTROL READING OF VOLUME LABELS                     */
*/*                                                                  */
*/*   INPUT -                                                        */
*/*     UCBPTR - POINTER TO UCB                                      */
*/*     IEFSGOPT - SYSGEN OPTION INDICATORS                          */
*/*                                                                  */
*/* EXIT - NORMAL - (INLINE SUBROUTINE - EXITS AT THE BOTTOM).       */
*/*                                                                  */
*/*   CONDITION - LABEL READ SUCCESSFUL                              */
*/*                                                                  */
*/*   OUTPUT - 80 BYTE FIELD(RECORD)-WHICH CONTAINS LABEL            */
*/*            WHICH HAS BEEN READ.                                  */
*/*                                                                  */
*/*   RETURN CODE -                                                  */
*/*     RETCODE = 0                                                  */
*/*                                                                  */
*/* EXIT - ERROR - (INLINE SUBROUTINE - EXITS AT THE BOTTOM).        */
*/*                                                                  */
*/*   CONDITION - UNABLE TO READ LABEL                               */
*/*                                                                  */
*/*   OUTPUT = NONE                                                  */
*/*                                                                  */
*/*   RETURN CODE -                                                  */
*/*     RETCODE SET BY IEFAB4F8 OR IEFAB4F9                          */
*/*                                                                  */
*/********************************************************************/
*RETCODE = NSUCCESS;                   /*INDICATE NOT YET READ       */
         LA    RETCODE,12                                       02S0077
*RECORDP = ADDR(RECORD);               /*PTR TO LABEL READ-IN AREA   */
         LA    @11,RECORD                                       02S0078
         ST    @11,RECORDP                                      02S0078
*IF UCB3TAPE = ON THEN                 /*IS THIS TAPE ?              */
         TM    UCB3TAPE(@12),B'10000000'                        02S0079
         BNO   @RF00079                                         02S0079
*  DO;                                 /*YES THEN DO FOLLOWING       */
*    TRIEDENS = NONE;                  /*INDICATE NO DENSITIES TRIED */
         MVI   TRIEDENS,B'00000000'                             02S0081
*    IF UCB2OPT0 = ON THEN             /*IS THIS 7 TRK TAPE?         */
         TM    UCB2OPT0(@12),B'10000000'                        02S0082
         BNO   @RF00082                                         02S0082
*      DO;                             /*YES THEN DO FOLLOWING       */
*        TAPEBITS = SEVENTRK;          /*INDICATE SEVEN TRACK TAPE   */
         MVI   TAPEBITS,B'01000000'                             02S0084
*        DO WHILE(RETCODE ^= SUCCESS   /*DO UNTIL SUCCESSFUL READ OR */
*          & TRIEDENS ^= ALL);         /*ALL DENSITIES TRIED         */
         B     @DE00085                                         02S0085
@DL00085 DS    0H                                               02S0086
*          IF TRIEDENS = NONE THEN     /*IS THIS FIRST READ TRY      */
         CLI   TRIEDENS,B'00000000'                             02S0086
         BNE   @RF00086                                         02S0086
*            DO;                       /*YES THEN DO FOLLOWING       */
*              IF STAVR8 = YES THEN    /*IS 800 BPI INDICATED?       */
         L     @12,@CA00098                                     02S0088
         CLI   STAVR8-IEFSGOPT(@12),B'00000001'                 02S0088
         BNE   @RF00088                                         02S0088
*                TESTBITS = DEN1;      /*YES SET PARM TO 800         */
         MVI   TESTBITS,B'10000000'                             02S0089
*              ELSE                    /*IF NOT 800 THEN DO FOLLOWING*/
*                DO;                                            02S0090
         B     @RC00088                                         02S0090
@RF00088 DS    0H                                               02S0091
*                  IF STAVR5 = YES THEN /*IS 556 BPI INDICATED?      */
         L     @12,@CA00098                                     02S0091
         CLI   STAVR5-IEFSGOPT(@12),B'00000001'                 02S0091
         BNE   @RF00091                                         02S0091
*                    TESTBITS = DEN2;  /*YES SET PARM TO 556         */
         MVI   TESTBITS,B'01000000'                             02S0092
*                  ELSE                /*IF NOT THEN DO FOLLOWING    */
*                    TESTBITS = DEN3;  /*SET PARM TO 200             */
         B     @RC00091                                         02S0093
@RF00091 MVI   TESTBITS,B'00100000'                             02S0093
*                END;                                           02S0094
*            END;                                               02S0095
*          ELSE                        /*IF NOT FIRST READ TRY THEN  */
*            DO;                                                02S0096
         B     @RC00086                                         02S0096
@RF00086 DS    0H                                               02S0097
*              IF TRIEDEN1 ^= ON THEN  /*HAS 800 BPI BEEN TRIED ?    */
         TM    TRIEDEN1,B'10000000'                             02S0097
         BO    @RF00097                                         02S0097
*                TESTBITS = DEN1;      /*IF NOT THEN TRY 800         */
         MVI   TESTBITS,B'10000000'                             02S0098
*              ELSE                    /*IF 800 HAS BEEN TRIED THEN  */
*                DO;                                            02S0099
         B     @RC00097                                         02S0099
@RF00097 DS    0H                                               02S0100
*                  IF TRIEDEN2 ^= ON THEN /*HAS 556 BPI BEEN TRIED   */
         TM    TRIEDEN2,B'01000000'                             02S0100
         BO    @RF00100                                         02S0100
*                    TESTBITS = DEN2;  /*IF NOT THEN TRY 556         */
         MVI   TESTBITS,B'01000000'                             02S0101
*                  ELSE                /*IF BOTH 556 & 800 WERE TRIED*/
*                    TESTBITS = DEN3;  /*THEN TRY 200                */
         B     @RC00100                                         02S0102
@RF00100 MVI   TESTBITS,B'00100000'                             02S0102
*                END;                                           02S0103
@RC00100 DS    0H                                               02S0104
*            END;                                               02S0104
@RC00097 DS    0H                                               02S0105
*          TRIEDENS = TRIEDENS × TESTBITS; /*UPDATE DENSITIES TRIED  */
@RC00086 OC    TRIEDENS(1),TESTBITS                             02S0105
*          CALL IEFAB4F9(UCBPTR,TAPESTAT,RECORDP); /*CALL TAPE LABEL
*                                        READ ROUTINE                */
         LA    @12,UCBPTR                                       02S0106
         ST    @12,@AL00001                                     02S0106
         LA    @12,TAPESTAT                                     02S0106
         ST    @12,@AL00001+4                                   02S0106
         LA    @12,RECORDP                                      02S0106
         ST    @12,@AL00001+8                                   02S0106
         L     @15,@CV01567                                     02S0106
         LA    @01,@AL00001                                     02S0106
         BALR  @14,@15                                          02S0106
*          RETCODE = REG15;            /*PICK UP RETURN CODE         */
         LR    RETCODE,REG15                                    02S0107
*        END;                                                   02S0108
@DE00085 LTR   RETCODE,RETCODE                                  02S0108
         BZ    @DC00085                                         02S0108
         CLI   TRIEDENS,B'11100000'                             02S0108
         BNE   @DL00085                                         02S0108
@DC00085 DS    0H                                               02S0109
*      END;                                                     02S0109
*    ELSE                              /*MUST BE 9 TRK               */
*      DO;                                                      02S0110
         B     @RC00082                                         02S0110
@RF00082 DS    0H                                               02S0111
*        TAPEBITS = NINETRK;           /*INDICATE 9 TRK              */
         MVI   TAPEBITS,B'10000000'                             02S0111
*        CALL IEFAB4F9(UCBPTR,TAPESTAT,RECORDP); /*CALL TAPE LABEL
*                                        READ ROUTINE                */
         LA    @12,UCBPTR                                       02S0112
         ST    @12,@AL00001                                     02S0112
         LA    @12,TAPESTAT                                     02S0112
         ST    @12,@AL00001+4                                   02S0112
         LA    @12,RECORDP                                      02S0112
         ST    @12,@AL00001+8                                   02S0112
         L     @15,@CV01567                                     02S0112
         LA    @01,@AL00001                                     02S0112
         BALR  @14,@15                                          02S0112
*        RETCODE = REG15;              /*PICK UP RETURN CODE         */
         LR    RETCODE,REG15                                    02S0113
*      END;                                                     02S0114
*  END;                                                         02S0115
*ELSE                                  /*IF NOT TAPE ASSUME DA       */
*  DO;                                                          02S0116
         B     @RC00079                                         02S0116
@RF00079 DS    0H                                               02S0117
*    CALL IEFAB4F8(UCBPTR,RECORDP);    /*CALL DA LABEL READ RTN      */
         LA    @12,UCBPTR                                       02S0117
         ST    @12,@AL00001                                     02S0117
         LA    @12,RECORDP                                      02S0117
         ST    @12,@AL00001+4                                   02S0117
         L     @15,@CV01566                                     02S0117
         LA    @01,@AL00001                                     02S0117
         BALR  @14,@15                                          02S0117
*    RETCODE = REG15;                  /*PICK UP RETURN CODE         */
         LR    RETCODE,REG15                                    02S0118
*  END;                                                         02S0119
*         IF RETCODE = SUCCESS THEN    /*IF LABEL READ SUCCESSFUL    */
@RC00079 LTR   RETCODE,RETCODE                                  01S0120
         BNZ   @RF00120                                         01S0120
*           DO;                                                 01S0121
*             ALTAPESW = OFF;          /*INITIALIZE SWITCH           */
         NI    ALTAPESW,B'01111111'                             01S0122
*             NSLTAPSW = OFF;          /*INITIALIZE SWITCH           */
         NI    NSLTAPSW,B'01111111'                             01S0123
*             SLTAPESW = OFF;          /*INITIALIZE SWITCH           */
         NI    SLTAPESW,B'01111111'                             01S0124
*             IF UCB3TAPE = ON THEN    /*IF TAPE                     */
         L     @12,UCBPTR                                       01S0125
         TM    UCB3TAPE(@12),B'10000000'                        01S0125
         BNO   @RF00125                                         01S0125
*               DO;                                             01S0126
*/********************************************************************/
*/*                                                                  */
*/* SEGMENT NAME - B473LBCK                                          */
*/*                                                                  */
*/* DESCRIPTIVE NAME - CHECK VOLUME LABEL                            */
*/*                                                                  */
*/* FUNCTION -                                                       */
*/*   THIS SEGMENT IS RESPONSIBLE FOR CHECKING THE VALIDITY OF       */
*/*   THE VOLUME LABEL BEING PROCESSED AND OBTAINING THE VOL.SERIAL. */
*/*   IT IS INVOKED WHEN AVR CONTROL IS PROCESSING A TAPE UCB.       */
*/*                                                                  */
*/*   OPERATION -                                                    */
*/*     THIS SEGMENT WILL PROCESS THE VOLUME LABEL JUST READ AS      */
*/*     FOLLOWS:                                                     */
*/*           IF STANDARD LABEL THE VOL.SER. WILL BE OBTAINED FROM   */
*/*            THE VOLUME LABEL.                                     */
*/*           IF ANSII LABEL A CHECK WILL BE MADE TO SEE IF ANSII    */
*/*            WAS SYSGENED AND IF SO SVC 103 WILL BE INVOKED TO     */
*/*            TRANSLATE THE VOL.SER.                                */
*/*           IF NON-STANDARD LABEL THEN THE NSL LABEL RTN(IEFXVNSL) */
*/*            WILL BE INVOKED TO PROCESS THE VOLSER.                */
*/*                                                                  */
*/* ENTRY POINT - (INLINE SUBROUTINE - ENTERED AT THE TOP).          */
*/*   PURPOSE - CHECK VALIDITY OF A VOLUME LABEL AND OBTAIN VOLSER   */
*/*                                                                  */
*/*   INPUT -                                                        */
*/*     80 BYTE FIELD(RECORD) WHICH CONTAINS LABEL JUST READ         */
*/*     ALTAPESW - OFF INDICATING ANSII LABEL NOT YET FOUND          */
*/*     NSLTAPSW - OFF INDICATING NSL LABEL NOT YET FOUND            */
*/*     SLTAPESW - OFF INDICATING SL LABEL NOT YET FOUND             */
*/*                                                                  */
*/* EXIT - NORMAL -(INLINE SUBROUTINE - EXITS AT BOTTOM).            */
*/*                                                                  */
*/*   CONDITION- LABEL IS VALID IN THIS SYSTEM AND VOLSER OBTAINED.  */
*/*                                                                  */
*/*   OUTPUT - 6 BYTE FIELD(LABELSER) -WHICH CONTAINS VOLSER         */
*/*                                                                  */
*/*   RETURN CODE-                                                   */
*/*     RETCODE = 0                                                  */
*/*                                                                  */
*/* EXIT - ERROR - (INLINE SUBROUTINE - EXITS AT BOTTOM).            */
*/*                                                                  */
*/*   CONDITION - NSL TAPE ENCOUNTERED AND NSL LABEL RTN(IEFXVNSL)   */
*/*               RETURNED A NON-ZERO VALUE IN REG15                 */
*/*                                                                  */
*/*   OUTPUT - NONE                                                  */
*/*                                                                  */
*/*   RETURN CODE -                                                  */
*/*     RETCODE = 8                                                  */
*/*                                                                  */
*/* EXIT - ERROR - (INLINE SUBROUTINE - EXITS AT THE BOTTOM).        */
*/*                                                                  */
*/*   CONDITION - ANSI LABEL AND ANSI NOT SYSGENED                   */
*/*                                                                  */
*/*   OUTPUT - NONE                                                  */
*/*                                                                  */
*/*   RETURN CODE -                                                  */
*/*     RETCODE = 12                                                 */
*/********************************************************************/
*IF LABELTYP = VOL1 THEN               /*IS THIS A STANDARD LABEL ?  */
         CLC   LABELTYP(4),@CC00136                             02S0127
         BNE   @RF00127                                         02S0127
*  SLTAPESW = ON;                      /*INDICATE SL                 */
         OI    SLTAPESW,B'10000000'                             02S0128
*ELSE                                  /*NOT STANDARD LABEL          */
*  IF LABELTYP = VOL1USAS THEN         /*IS THIS ANSI LABEL ?        */
         B     @RC00127                                         02S0129
@RF00127 CLC   LABELTYP(4),@CB00139                             02S0129
         BNE   @RF00129                                         02S0129
*    DO;                                                        02S0130
*      IF CVTASCII = ON THEN           /*IS ANSI A SYSGEN OPTION     */
         L     @12,CVTPTR                                       02S0131
         TM    CVTASCII(@12),B'00000010'                        02S0131
         BNO   @RF00131                                         02S0131
*        DO;                                                    02S0132
*          ALTAPESW = ON;              /*SET USASCII LABEL INDICATOR */
         OI    ALTAPESW,B'10000000'                             02S0133
*          REG0 = USALNGTH;            /*SET TRANSLATE LENGTH PARM   */
         LA    REG0,6                                           02S0134
*          REG1 = ADDR(LABELSER);      /*SET TRANSLATE ADDR.PARM     */
         LA    REG1,LABELSER                                    02S0135
*          GENERATE(XLATE (1),(0));    /*CALL TRANSLATE  ROUTINE     */
         XLATE (1),(0)
*        END;                                                   02S0137
*      ELSE                            /*ANSI NOT SYSGEN OPTION      */
*        RETCODE = ANSIERRO;           /*ANSI TAPE IN NON-ANSI SYSTEM*/
         B     @RC00131                                         02S0138
@RF00131 LA    RETCODE,12                                       02S0138
*    END;                                                       02S0139
*  ELSE                                /*MUST BE NSL TAPE            */
*    DO;                                                        02S0140
         B     @RC00129                                         02S0140
@RF00129 DS    0H                                               02S0141
*      PARAMPTR = PARAMPTR && PARAMPTR; /*CLEAR VOL SER PTR          */
         L     @12,PARAMPTR                                     02S0141
         LR    @11,@12                                          02S0141
         XR    @11,@12                                          02S0141
         ST    @11,PARAMPTR                                     02S0141
*      CALL IEFXVNSL(RECORD,PARAMPTR); /*BRANCH TO USER LABEL RTN.OR
*                                        IBM CSECT IF THE USER HAS
*                                        NOT REPLACED THIS WITH HIS
*                                        OWN RTN.THE IBM CSECT IF NOT
*                                        REPLACED WILL INDICATE AN
*                                        ERROR CONDITION BY RETURNING
*                                        WITH REGISTER 15 NON-ZERO.  */
         LA    @12,RECORD                                       02S0142
         ST    @12,@AL00001                                     02S0142
         LA    @12,PARAMPTR                                     02S0142
         ST    @12,@AL00001+4                                   02S0142
         L     @15,@CV01568                                     02S0142
         LA    @01,@AL00001                                     02S0142
         BALR  @14,@15                                          02S0142
*      IF REG15 ^= SUCCESS THEN        /*IF RETURN REG.NOT ZERO THEN */
         LTR   REG15,REG15                                      02S0143
         BZ    @RF00143                                         02S0143
*        RETCODE = NSLERROR;           /*INDICATE ERROR              */
         LA    RETCODE,8                                        02S0144
*      ELSE                            /*NO ERROR                    */
*        DO;                                                    02S0145
         B     @RC00143                                         02S0145
@RF00143 DS    0H                                               02S0146
*          LABELSER = RETSER;          /*PICK UP VOL.SER             */
         L     @12,PARAMPTR                                     02S0146
         MVC   LABELSER(6),RETSER(@12)                          02S0146
*          NSLTAPSW = ON;              /*INDICATE NSL                */
         OI    NSLTAPSW,B'10000000'                             02S0147
*        END;                                                   02S0148
*    END;                                                       02S0149
*               END;                                            01S0150
*             ELSE;                     /*NOT TAPE                   */
*                                                               01S0151
@RF00125 DS    0H                                               01S0152
*             IF RETCODE = SUCCESS THEN /*IF VOLSER OBTAINED @YM07608*/
@RC00125 SLR   @12,@12                                          01S0152
         CR    RETCODE,@12                                      01S0152
         BNE   @RF00152                                         01S0152
*               DO;                                             01S0153
*             /*SET KEY 0 AND LOCK                                   */
*                   DO; /*MODESET  KEYADDR(ZERO)*/              01S0154
*                     SPKA (ZERO); /*SET PSW KEY*/              01S0155
         SPKA  0(@12)                                           01S0155
*                   END; /*MODESET  KEYADDR(ZERO)*/ /*GET KEY ZERO
*      */                                                       01S0156
*                   DO; /* SETLOCK (OBTAIN) MODE(UNCOND) TYPE(LOCAL) RE
*S(USE) RELATED(UCB,IEFAB473(IEFAB473)) */                      01S0157
*                     RFY (GPR11P,GPR12P,GPR13P,GPR14P) RSTD;   01S0158
*                     RFY (GPR15P,GPR00P,GPR01P) RSTD;          01S0159
*                     GPR15P = GPR11P; /* SAVE REG. 11 IN REG. 15. */
         LR    GPR15P,GPR11P                                    01S0160
*                     GPR00P = GPR12P; /* SAVE REG. 12 IN REG. 0. */
         LR    GPR00P,GPR12P                                    01S0161
*                     GPR01P = GPR13P; /* SAVE REG. 13 IN REG. 1. */
         LR    GPR01P,GPR13P                                    01S0162
*                     GPR13P = PSALITA; /* ADDRESS OF LOCK INTERFACE TA
*LE. */               GEN(LM GPR11P,GPR13P,576(GPR13P)); /* LOAD LOCKWO
*D, CLHT AND ENTRY ADRS. */                                     01S0163
         L     GPR13P,PSALITA                                   01S0163
         LM GPR11P,GPR13P,576(GPR13P)
*                     GEN(BALR GPR14P,GPR13P); /* BALR TO SETLOCK RTN.
*/                    GPR11P = GPR15P; /* RESTORE REG. 11. */   01S0165
         BALR GPR14P,GPR13P
         LR    GPR11P,GPR15P                                    01S0166
*                     GPR15P = GPR13P; /* SAVE RETURN CODE. */  01S0167
         LR    GPR15P,GPR13P                                    01S0167
*                     GPR12P = GPR00P; /* RESTORE REG. 12 */    01S0168
         LR    GPR12P,GPR00P                                    01S0168
*                     GPR13P = GPR01P; /* RESTORE REG. 13. */   01S0169
         LR    GPR13P,GPR01P                                    01S0169
*                     RFY(GPR15P,GPR00P,GPR01P) UNRSTD;         01S0170
*                     RFY (GPR11P,GPR12P,GPR13P,GPR14P) UNRSTD; 01S0171
*                   END; /* SETLOCK (OBTAIN) MODE(UNCOND) TYPE(LOCAL) R
*GS(USE) RELATED(UCB,IEFAB473(IEFAB473)) */ /*GET LOC LOCK  */  01S0172
*                   DO; /* SETLOCK (OBTAIN) MODE(UNCOND) TYPE(CMS) REGS
*USE) RELATED(UCB,IEFAB473(IEFAB473)) */                        01S0173
*                     RFY (GPR11P,GPR12P,GPR13P,GPR14P) RSTD;   01S0174
*                     RFY (GPR15P,GPR00P,GPR01P) RSTD;          01S0175
*                     GPR15P = GPR11P; /* SAVE REG. 11 IN REG. 15. */
         LR    GPR15P,GPR11P                                    01S0176
*                     GPR00P = GPR12P; /* SAVE REG. 12 IN REG. 0. */
         LR    GPR00P,GPR12P                                    01S0177
*                     GPR01P = GPR13P; /* SAVE REG. 13 IN REG. 1. */
         LR    GPR01P,GPR13P                                    01S0178
*                     GPR13P = PSALITA; /* ADDRESS OF LOCK INTERFACE TA
*LE. */               GEN(LM GPR11P,GPR13P,540(GPR13P)); /* LOAD LOCKWO
*D, CLHT AND ENTRY ADRS. */                                     01S0179
