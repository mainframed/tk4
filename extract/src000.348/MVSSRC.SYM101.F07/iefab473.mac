         LR    GPR00P,GPR12P                                    01S0240
*                      GPR01P = GPR13P; /* SAVE REG. 13 IN REG. 1. */
         LR    GPR01P,GPR13P                                    01S0241
*                      GPR13P = PSALITA; /* ADDRESS OF LOCK INTERFACE T
*BLE. */               GEN(LM GPR11P,GPR13P,588(GPR13P)); /* LOAD LOCKW
*RD, CLHT AND ENTRY ADRS. */                                    01S0242
         L     GPR13P,PSALITA                                   01S0242
         LM GPR11P,GPR13P,588(GPR13P)
*                      GEN(BALR GPR14P,GPR13P); /* BALR TO SETLOCK RTN.
**/                    GPR11P = GPR15P; /* RESTORE REG. 11. */  01S0244
         BALR GPR14P,GPR13P
         LR    GPR11P,GPR15P                                    01S0245
*                      GPR15P = GPR13P; /* SAVE RETURN CODE. */ 01S0246
         LR    GPR15P,GPR13P                                    01S0246
*                      GPR12P = GPR00P; /* RESTORE REG. 12 */   01S0247
         LR    GPR12P,GPR00P                                    01S0247
