         BZ    @DL00189                                         02S0203
@DC00189 DS    0H                                               02S0204
*RFY UCBOB BASED(UCBPTR);              /*RESPECIFY UCB DSECT         */
*                   IF RETCODE=SUCCESS THEN /*LABEL NOT DUPE @YM07608*/
         LTR   RETCODE,RETCODE                                  01S0205
         BNZ   @RF00205                                         01S0205
*                     DO;               /*                   @YM07608*/
*                       IF UCB3DACC = ON & /*DIRECT ACCESS   @YM05583*/
*                         UCBPRES = ON THEN  /*PERMRES       @YM05583*/
         L     @12,UCBPTR                                       01S0207
         TM    UCB3DACC(@12),B'00100000'                        01S0207
         BNO   @RF00207                                         01S0207
         TM    UCBPRES(@12),B'00000100'                         01S0207
         BNO   @RF00207                                         01S0207
*                          UCBBPUB = ON; /*SET PUB. ATRIBUTE @YM05583*/
