         LA    LUTNO,1                                          02S0189
         B     @DE00189                                         02S0189
@DL00189 DS    0H                                               02S0190
*  IF IOSLUT(LUTNO)^=NOUCBADR THEN     /*IS THERE A UCB ADDRESS      */
         LR    @12,LUTNO                                        02S0190
         ALR   @12,@12                                          02S0190
         L     @11,@PC00001+4                                   02S0190
         L     @11,LOOKUPTP(,@11)                               02S0190
         ALR   @11,@12                                          02S0190
         BCTR  @11,0                                            02S0190
         BCTR  @11,0                                            02S0190
