*/********************************************************************/
*/*                                                                  */
*/* SEGMENT NAME - ULNDINFC                                          */
*/*                                                                  */
*/* DESCRIPTIVE NAME - UNLOAD INTERFACE RTN                          */
*/*                                                                  */
*/* FUNCTION -                                                       */
*/*   THIS SEGMENT SETS UP THE INTERFACE FOR AND INVOKES VOLUME      */
*/*   MOUNT AND VERIFY CONTROL(IEFAB493) WHICH WILL PERFORM          */
*/*   UNLOAD PROCESSING FOR THE UCB BEING PROCESSED.                 */
*/*                                                                  */
*/* OPERATION -                                                      */
*/*   THE VM&V REQUEST BLOCK IS FILLED IN WITH AN UNLOAD INDICATOR,  */
*/*   A POINTER TO THE UCB BEING PROCESSED,AND THE VOLSER OF THE     */
*/*   VOLUME BEING UNLOADED                                          */
*/*                                                                  */
*/* ENTRY POINT - (INLINE SUBROUTINE - ENTERED AT THE TOP).          */
*/*                                                                  */
*/*   PURPOSE - INTERFACE TO VOL MT.& VERIFY FOR UNLOAD PROCESSING.  */
*/*                                                                  */
*/*   INPUT -                                                        */
*/*     LABELSER - VOLSER OF VOLUME BEING UNLOADED.                  */
*/*     UCBPTR   - UCB FOR UNIT BEING UNLOADED.                      */
*/*                                                                  */
*/* EXIT -  NORMAL  - (INLINE SUBROUTINE - EXITS AT BOTTOM).         */
*/*                                                                  */
*/*   CONDITION - INTERFACE TO VM&V SET-UP AND VM&V INVOKED.         */
*/*                                                                  */
*/*   OUTPUT -                                                       */
*/*   VM&V REQUEST BLOCK SET-UP                                      */
*/*                                                                  */
*/*   RETURN CODE - NONE                                             */
*/********************************************************************/
*VMVREQBP = ADDR(REQBLK);              /*PTR TO VM&V REQUEST BLK     */
         LA    @12,REQBLK                                       03S0312
         ST    @12,VMVREQBP                                     03S0312
*REQBLK = REQBLK && REQBLK;            /*CLEAR REQUEST BLOCK         */
         XC    REQBLK(52),REQBLK                                03S0313
*VMVUNLOD = ON;                        /*INDICATE UNLOAD             */
         OI    VMVUNLOD(@12),B'10000000'                        03S0314
*VMVUCBP = UCBPTR;                     /*POINTER TO UCB              */
         MVC   VMVUCBP(3,@12),UCBPTR+1                          03S0315
*IF RETCODE = DUPERROR THEN            /*DUP VOLSER ERROR    @YM07657*/
         CH    RETCODE,@CH00044                                 03S0316
         BNE   @RF00316                                         03S0316
*  VMVUVLSR = LABELSER;                /*VOLSER INTO REQ BLK @YM07657*/
         MVC   VMVUVLSR(6,@12),LABELSER                         03S0317
*ELSE                                  /*NOT DUPLICATE VOLSER@YM07657*/
*  VMVUCB = ON;                        /*UNLOAD VOLSER IN UCB        */
         B     @RC00316                                         03S0318
@RF00316 L     @12,VMVREQBP                                     03S0318
         OI    VMVUCB(@12),B'01000000'                          03S0318
*CALL IEFAB493(VMVREQBP,WORDZERO,WORDZERO); /*CALL VM&V FOR UNLOAD   */
@RC00316 MVC   @AL00001+4(8),@AL00319                           03S0319
         LA    @12,VMVREQBP                                     03S0319
         ST    @12,@AL00001                                     03S0319
         L     @15,@CV01564                                     03S0319
         LA    @01,@AL00001                                     03S0319
         BALR  @14,@15                                          03S0319
*    END;                                                       02S0320
*  ELSE;                               /*NO UNLOADS TO BE DONE       */
@RF00310 DS    0H                                               02S0322
*          END;                                                 01S0322
*        ELSE;                         /*NO ERROR                    */
@RF00259 DS    0H                                               01S0324
*       END;                                                    01S0324
*     ELSE;                            /*UCB NOW MARKED NOT     01S0325
*                                        READY               @YM07608*/
@RF00075 DS    0H                                               01S0326
*    END;                              /*                    @YM07608*/
*                                                               01S0326
*  ELSE;                               /*DON'T PROCESS UCB           */
@RF00072 DS    0H                                               01S0328
*  UCBPTR = UCBLSTEN(I+ONE);           /*PTR TO NEXT UCB             */
         LR    @12,I                                            01S0328
         ALR   @12,@12                                          01S0328
         L     @11,@PC00001                                     01S0328
         L     @11,UCBLISTP(,@11)                               01S0328
         ALR   @11,@12                                          01S0328
         SLR   @12,@12                                          01S0328
         ICM   @12,3,UCBLSTEN(@11)                              01S0328
         ST    @12,UCBPTR                                       01S0328
*END;                                                           01S0329
         AH    I,@CH00091                                       01S0329
@DE00071 SLR   @12,@12                                          01S0329
         ICM   @12,3,ENDINDIC                                   01S0329
         C     @12,UCBPTR                                       01S0329
         BNE   @DL00071                                         01S0329
*RETURN CODE(ZERO);                    /*RETURN CODE                 */
         L     @13,4(,@13)                                      01S0330
         L     @00,@SIZDATD                                     01S0330
         LR    @01,@09                                          01S0330
         FSPACE   R,LV=(0),A=(1)
         SLR   @15,@15                                          01S0330
         L     @14,12(,@13)                                     01S0330
         LM    @00,@12,20(@13)                                  01S0330
         BR    @14                                              01S0330
*END IEFAB473                                                   01S0331
