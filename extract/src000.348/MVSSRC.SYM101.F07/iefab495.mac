*     GTSTR4   FIXED(31) REG(4),/* CLEARING REG*/               02S0103
*     GTSTR5   FIXED(31) REG(5),/* CLEARING REG*/               02S0103
*     GTSTR6   FIXED(31) REG(6),/* CLEARING REG*/               02S0103
*     GTSTR7   FIXED(31) REG(7),/* CLEARING REG*/               02S0103
*     GTSTR8   FIXED(31) REG(8),/* CLEARING REG*/               02S0103
*     GTSTZERO FIXED(31) INIT(0),                               02S0103
*     GTST253  FIXED(31) CONSTANT(253);                         02S0103
*   RFY (GTSTR0,GTSTR1,GTSTR2) RSTD; /*SAVE RGS*/               02S0104
*   GTSTR0 = GETLENTH; /* SET AMOUNT*/                          02S0105
         LR    GTSTR0,GETLENTH                                  02S0105
