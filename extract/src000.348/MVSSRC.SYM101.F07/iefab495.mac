         LTR   RETCODE,RETCODE                                  02S0127
         BNZ   @RF00127                                         02S0127
*  DO;                                                          02S0128
*    IF RETURNC = NOMATCH THEN         /*IF  NO MVCA FOR THIS ASID   */
         CLC   RETURNC(2),@CH01558                              02S0129
         BNE   @RF00129                                         02S0129
*      DO;                                                      02S0130
*        GETLENTH = LENGTH(MVCA);      /*SIZE OF MVCA                */
         LA    GETLENTH,16                                      02S0131
*         /* ?GETSTORG (SUBPSQA,GETLENTH,MVCAPTR);*/            02S0132
*           RFY (GTSTR0,GTSTR1,GTSTR2) RSTD; /*SAVE RGS*/       02S0132
*           GTSTR0 = GETLENTH; /* SET AMOUNT*/                  02S0133
         LR    GTSTR0,GETLENTH                                  02S0133
*           GTSTR2 = SUBPSQA; /* SET SUBPOOL*/                  02S0134
         LA    GTSTR2,241                                       02S0134
*           GENERATE CODE;                                      02S0135
             GETMAIN RC,LV=(0),SP=(2)  DO GETMAIN
*           GTSTRTCD = GTSTR15; /* SAVE GETMAIN RET COD*/       02S0136
         ST    GTSTR15,GTSTRTCD                                 02S0136
*           MVCAPTR = GTSTR1; /* ADDR OF GOTN STORG*/           02S0137
         ST    GTSTR1,MVCAPTR                                   02S0137
*           RFY (GTSTR0,GTSTR1,GTSTR2) UNRSTD; /*RLSE */        02S0138
