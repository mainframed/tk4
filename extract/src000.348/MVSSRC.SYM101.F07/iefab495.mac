         LR    GTSTR5,GETLENTH                                  02S0143
*               GTSTR7 = '00000000'X; /*PAD CHAR & LNTH*/       02S0144
         SLR   GTSTR7,GTSTR7                                    02S0144
*               IF SUBPSQA = GTST253 THEN                       02S0145
         B     @RF00145                                         02S0145
*              GEN                                              02S0146
*           (MODESET KEYADDR=GTSTZERO,SAVEKEY=(2),WORKREG=8);   02S0146
         MODESET KEYADDR=GTSTZERO,SAVEKEY=(2),WORKREG=8
*               GEN REFS(GTSTR4,GTSTR6);                        02S0147
@RF00145 DS    0H                                               02S0147
                  MVCL  GTSTR4,GTSTR6 CLEAR GOTTEN STORG
*               IF SUBPSQA = GTST253 THEN                       02S0148
         B     @RF00148                                         02S0148
*                 GEN (MODESET KEYADDR=(2));                    02S0149
         MODESET KEYADDR=(2)
*               RFY (GTSTR2,GTSTR4,GTSTR5,GTSTR6,               02S0150
