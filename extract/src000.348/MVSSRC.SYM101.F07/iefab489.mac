         L     @09,ALGTABP(,@09)                                02S0178
         L     @02,ALGPARMP(,@09)                               02S0178
         NI    CVRFCALL(@02),B'11101111'                        02S0178
*                  CALL IEFAB480(ALGPARMP); /*INVOKE ALG TO REDUCE
*                                             SOLUTIONS              */
         ST    @09,@AL00001                                     02S0179
         L     @15,@CV00152                                     02S0179
         LA    @01,@AL00001                                     02S0179
         BALR  @14,@15                                          02S0179
*                  IF REG15 ^= ZERO THEN  /*ERROR RETURN CODE        */
         LTR   REG15,REG15                                      02S0180
         BZ    @RF00180                                         02S0180
*                    DO;                                        02S0181
*                      RETCODE = FAILURE; /*SET RETURN CODE TO 4     */
         MVC   RETCODE(4),@CF00138                              02S0182
*                      RSNCODE = GETMNERR; /*SET REASON CODE TO 2    */
         L     @09,@PC00001                                     02S0183
         L     @09,ALCWAPTR(,@09)                               02S0183
         L     @09,FAILRSCP(,@09)                               02S0183
         MVC   RSNCODE(2,@09),@CH00073                          02S0183
*                    END;                                       02S0184
*                END;                                           02S0185
*              ELSE;                   /*ENTRY WASN'T ALLOCATED      */
@RF00161 DS    0H                                               02S0187
*            END;                                               02S0187
*          ELSE;                       /*GROUP NOT ALLOCATABLE TO REQ*/
@RF00140 DS    0H                                               02S0189
*        END;                                                   02S0189
*      ELSE;                           /*NOT GROUP BEING PROCESSED   */
@RF00137 DS    0H                                               02S0191
*      GPLSTPTR = GPLSTPTR +           /*GET NEXT GROUP LIST ENTRY   */
*          LENGTH(CVRGPLST);                                    02S0191
@RC00137 LA    @09,16                                           02S0191
         AL    @09,GPLSTPTR                                     02S0191
         ST    @09,GPLSTPTR                                     02S0191
*    END;                                                       02S0192
         AH    GRPLCTR,@CH00066                                 02S0192
@DE00136 L     @09,RQLSTPTR                                     02S0192
         CH    GRPLCTR,CVRNGRP(,@09)                            02S0192
         BNH   @DL00136                                         02S0192
@DC00136 DS    0H                                               02S0193
*  ELSE;                               /*NO PROC'NG NEEDED FOR VU ENT*/
@RF00135 DS    0H                                               02S0194
*  VUPTR = VUPTR + LENGTH(VOLUNTAB);   /*POINT TO NEXT VU ENTRY      */
         LA    @09,28                                           02S0194
         AL    @09,VUPTR                                        02S0194
         ST    @09,VUPTR                                        02S0194
*END;                                                           02S0195
         LA    @09,1                                            02S0195
         AL    @09,VUCTR                                        02S0195
@DE00131 ST    @09,VUCTR                                        02S0195
         CR    @09,NOVUENTS                                     02S0195
         BNH   @DL00131                                         02S0195
@DC00131 DS    0H                                               02S0196
