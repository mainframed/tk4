         TITLE 'IEFAB489 - ALLOCATE ONLINE DEVICES                     *
                        '
IEFAB489 CSECT ,                                                01S0001
@MAINENT DS    0H                                               01S0001
         USING *,@15                                            01S0001
         B     @PROLOG                                          01S0001
         DC    AL1(24)                                          01S0001
         DC    C'IEFAB489 78054  UZ16857 '                      01S0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                  01S0001
         BALR  @12,0                                            01S0001
@PSTART  LA    @11,4095(,@12)                                   01S0001
         USING @PSTART,@12                                      01S0001
         USING @PSTART+4095,@11                                 01S0001
         L     @00,@SIZDATD                                     01S0001
         GSPACE   R,LV=(0)
         LR    @10,@01                                          01S0001
         USING @DATD,@10                                        01S0001
         ST    @13,@SA00001+4                                   01S0001
         LM    @00,@01,20(@13)                                  01S0001
         ST    @10,8(,@13)                                      01S0001
         LR    @13,@10                                          01S0001
         MVC   @PC00001(8),0(@01)                               01S0001
*RETCODE = ZERO;                       /*INIT RETURN CODE FIELD      */
         SLR   @15,@15                                          01S0076
         ST    @15,RETCODE                                      01S0076
*/********************************************************************/
*/*                                                                  */
*/* SEGMENT NAME - CHKDMNDS                                          */
*/*                                                                  */
*/* DESCRIPTIVE NAME - CHECK FOR DEMAND REQUESTS TO ALLOCATE         */
*/*                                                                  */
*/* FUNCTION -                                                       */
*/*   THIS ROUTINE SCANS THE SIOT CHAIN FOR DEMAND REQUESTS WHICH    */
*/*   REQUIRE AN OFFLINE UNIT FROM THE GROUP CURRENTLY               */
*/*   BEING PROCESSED. FOR EACH SUCH REQUEST FOUND, THE              */
*/*   'ALOCREQ' SUBROUTINE IS INVOKED TO ATTEMPT TO ALLOCATE.        */
*/*                                                                  */
*/* ENTRY - IN-LINE SEGMENT                                          */
*/*   PURPOSE - ALLOCATE DEMAND REQUESTS TO AVAILABLE UNITS          */
*/*   INPUT -                                                        */
*/*     SIOT1P - PTR TO FIRST SIOT IN STEP                           */
*/*       SIOTALCD - IF ON, SIOT HAS BEEN ALLOCATED                  */
*/*       SIOTDMND - ON FOR ALL DEMAND REQUESTS                      */
*/*       SIOTEDLP - PTR TO EDL FOR SIOT                             */
*/*       SVOLUNAD - PTR TO VOLUNIT ENTRY                            */
*/*     GRPIDENT - GROUP ID OF DEVICE GROUP BEING PROCESSED          */
*/*                                                                  */
*/* EXITS -                                                          */
*/*   CONDITION - NO REQUESTS TO ALLOCATE                            */
*/*     OUTPUT - NONE                                                */
*/*     RETURN CODES -                                               */
*/*       RETCODE = ZERO                                             */
*/*   CONDITION - AT LEAST ONE REQUEST ALLOCATED SUCCESSFULLY        */
*/*     OUTPUT                                                       */
*/*       SIOT/VOLUNIT ENTRY UPDATED                                 */
*/*       UCB/JFCB/PSAB/TIOT UPDATED                                 */
*/*       TOTREQS DECREMENTED                                        */
*/*     RETURN CODES -                                               */
*/*       RETCODE = ZERO                                             */
*/********************************************************************/
*SIOTPTR = SIOT1P;                     /*POINT TO FIRST SIOT         */
         L     @15,@PC00001                                     02S0077
         L     @01,ALCWAPTR(,@15)                               02S0077
         MVC   SIOTPTR(4),SIOT1P(@01)                           02S0077
*DO WHILE(SIOTPTR ^= ZERO &            /*SCAN ALL SIOTS ...          */
*    RETCODE = ZERO &                  /*... UNTIL ERROR OCCURS ...  */
*    TOTREQS > ZERO);                  /*... OR ALL REQS ARE ALLOC'D */
         B     @DE00078                                         02S0078
@DL00078 DS    0H                                               02S0079
*  VUPTR = SVOLUNAD;                   /*SET PTR TO VOLUNIT ENTRY    */
         L     @15,SIOTPTR                                      02S0079
         L     @14,SVOLUNAD(,@15)                               02S0079
         ST    @14,VUPTR                                        02S0079
*  IF SIOTDMND = YES &                 /*UNALLOCATED DEMAND REQUEST  */
*      SIOTALCD = NO &                                          02S0080
*      VUDMNDOF = YES THEN             /*NEEDED OFFLINE UNIT         */
         TM    SIOTDMND(@15),B'10000000'                        02S0080
         BNO   @RF00080                                         02S0080
         TM    SIOTALCD(@15),B'00000010'                        02S0080
         BNZ   @RF00080                                         02S0080
         TM    VUDMNDOF(@14),B'00100000'                        02S0080
         BNO   @RF00080                                         02S0080
*    DO;                                                        02S0081
*      EDLPTR = EDLFGENP;              /*POINT TO EDL GENERIC ENTRY  */
         L     @01,SIOTEDLP(,@15)                               02S0082
         L     EDLPTR,EDLFGENP(,@01)                            02S0082
*      EDLGNTP = EDLGRPTR;             /*GET PTR TO GROUP ENTRY      */
         L     @08,EDLGRPTR(,EDLPTR)                            02S0083
         ST    @08,EDLGNTP                                      02S0083
*      ARUGRPID = EDLGRID;             /*SET ID OF GROUP BEING PROC'D*/
         MVC   ARUGRPID(4),EDLGRID(@08)                         02S0084
*      RQLSTPTR = VOLALGTP;            /*SET PTR TO REQ LIST ENTRY   */
         MVC   RQLSTPTR(4),VOLALGTP(@14)                        02S0085
*      CALL ALOCREQ;                   /*TRY TO ALLOCATE THIS REQUEST*/
         BAL   @14,ALOCREQ                                      02S0086
*      IF VOLALOC = YES &              /*ENTRY WAS ALLOCATED         */
*          SIOTALCD = NO THEN                                   02S0087
         L     @01,VUPTR                                        02S0087
         TM    VOLALOC(@01),B'10000000'                         02S0087
         BNO   @RF00087                                         02S0087
         L     @15,SIOTPTR                                      02S0087
         TM    SIOTALCD(@15),B'00000010'                        02S0087
         BNZ   @RF00087                                         02S0087
*        DO;                           /*INDIC REQUEST WAS ALLOCATED */
*          SIOTALCD = ON;              /*SET INDICATOR IN SIOT       */
         OI    SIOTALCD(@15),B'00000010'                        02S0089
*          TOTREQS = TOTREQS - ONE;    /*DECR. REQS LEFT TO ALLOC    */
         L     @15,@PC00001                                     02S0090
         L     @15,ALCWAPTR(,@15)                               02S0090
         L     @14,TOTREQS(,@15)                                02S0090
         BCTR  @14,0                                            02S0090
         ST    @14,TOTREQS(,@15)                                02S0090
*        END;                                                   02S0091
*      ELSE;                           /*ENTRY WASN'T ALLOCATED      */
@RF00087 DS    0H                                               02S0093
*    END;                                                       02S0093
*  ELSE;                               /*NOT UNALLOC'D DEMAND REQUEST*/
@RF00080 DS    0H                                               02S0095
*  SIOTPTR = SIOTNPTR;                 /*POINT TO NEXT SIOT          */
         L     @01,SIOTPTR                                      02S0095
         L     @15,SIOTNPTR(,@01)                               02S0095
         ST    @15,SIOTPTR                                      02S0095
*END;                                                           02S0096
@DE00078 SLR   @15,@15                                          02S0096
         C     @15,SIOTPTR                                      02S0096
         BE    @DC00078                                         02S0096
         C     @15,RETCODE                                      02S0096
         BNE   @DC00078                                         02S0096
         L     @14,@PC00001                                     02S0096
         L     @01,ALCWAPTR(,@14)                               02S0096
         C     @15,TOTREQS(,@01)                                02S0096
         BL    @DL00078                                         02S0096
@DC00078 DS    0H                                               02S0097
*NOVUENTS = (VOLUNSZE -                /*NO. VU ENTRIES TO PROCESS   */
*   (FVUPTR - VOLUNPTR))/LENGTH(VOLUNTAB);                      01S0097
         L     @06,@PC00001                                     01S0097
         L     @06,ALCWAPTR(,@06)                               01S0097
         L     @02,@PC00001+4                                   01S0097
         L     NOVUENTS,FVUPTR(,@02)                            01S0097
         SL    NOVUENTS,VOLUNPTR(,@06)                          01S0097
         LCR   NOVUENTS,NOVUENTS                                01S0097
         AL    NOVUENTS,VOLUNSZE(,@06)                          01S0097
         SRDA  NOVUENTS,32                                      01S0097
         D     NOVUENTS,@CF00085                                01S0097
         LR    NOVUENTS,@09                                     01S0097
*GPCNTPTR = ALGGCNTP;                  /*POINT TO FIRST GROUP COUNT
*                                        ENTRY IN ALGORITHM TABLES   */
         L     @09,ALGTABP(,@06)                                01S0098
         L     GPCNTPTR,ALGGCNTP(,@09)                          01S0098
*NUMGRPS = GRPENTNO;                   /*NUMBER OF GROUPS TO SCAN    */
         L     @09,EDTABPTR(,@06)                               01S0099
         L     @09,EDTGRPSP(,@09)                               01S0099
         L     NUMGRPS,GRPENTNO(,@09)                           01S0099
*DO GRPCTR = 1 TO NUMGRPS              /*CHECK ALL GROUPS IN SYSTEM  */
*    WHILE(RETCODE = ZERO &                                     01S0100
*    TOTREQS > ZERO);                                           01S0100
         LA    @09,1                                            01S0100
         B     @DE00100                                         01S0100
@DL00100 SLR   @06,@06                                          01S0100
         C     @06,RETCODE                                      01S0100
         BNE   @DC00100                                         01S0100
         L     @02,@PC00001                                     01S0100
         L     @02,ALCWAPTR(,@02)                               01S0100
         C     @06,TOTREQS(,@02)                                01S0100
         BNL   @DC00100                                         01S0100
*  GRPPTR = EDTGRPSP + LENGTH(GRPHDR)  /*SET PTR TO EDT GROUP ENTRY  */
*      + (GRPCTR - 1) * GRPENTLN;                               01S0101
         L     @05,EDTABPTR(,@02)                               01S0101
         L     @05,EDTGRPSP(,@05)                               01S0101
         LA    GRPPTR,8                                         01S0101
         ALR   GRPPTR,@05                                       01S0101
         LR    @01,@09                                          01S0101
         BCTR  @01,0                                            01S0101
         M     @00,GRPENTLN(,@05)                               01S0101
         ALR   GRPPTR,@01                                       01S0101
*  OFFLNCT = ZERO;                     /*INIT COUNT OF OFFLINE UNITS */
         SLR   OFFLNCT,OFFLNCT                                  01S0102
*  DO DEVCTR = 1 TO GRPUCBNO;          /*CHECK ALL UCB'S IN THIS GRP */
         LA    DEVCTR,1                                         01S0103
         B     @DE00103                                         01S0103
@DL00103 DS    0H                                               01S0104
*    UCBPTR = IOSLUT(EUCBTPOS(DEVCTR) + 1); /*POINT TO UCB           */
         LR    @09,DEVCTR                                       01S0104
         ALR   @09,@09                                          01S0104
         L     @02,GRPUCBP(,GRPPTR)                             01S0104
         BCTR  @02,0                                            01S0104
         BCTR  @02,0                                            01S0104
         LH    @09,EUCBTPOS(@09,@02)                            01S0104
         ALR   @09,@09                                          01S0104
         L     @02,@PC00001                                     01S0104
         L     @02,ALCWAPTR(,@02)                               01S0104
         L     @02,IOSLUTP(,@02)                                01S0104
         ALR   @02,@09                                          01S0104
         SLR   @09,@09                                          01S0104
         ICM   @09,3,IOSLUT(@02)                                01S0104
         ST    @09,UCBPTR                                       01S0104
*    IF (UCBNALOC = NO) &              /*UNIT IS ALLOCATABLE...      */
*       ((UCB3DACC = YES &             /*NOT FIXED DASD              */
*       (UCBPRES = NO &                                         01S0105
*       UCBRESV = NO)) ×                                        01S0105
*       (UCB3DACC=YES &                /*PERMRES DA          @YM05583*/
*       UCBPRES=YES &                  /*                    @YM05583*/
*       UCBALOC = NO &                 /*WITH NO VOL NOUNTED @YM05583*/
*       (UCBONLI = NO ×                /*                    @YM05583*/
*       UCBVOLI = NOVOLSER ×           /*                    @YM05583*/
*       UCBMOUNT = ON)) ×              /*                    @YM05583*/
*       (UCB3TAPE = YES &              /*NOT RESERVED TAPE           */
*       UCBRESV = NO) ×                                         01S0105
*       (UCB3DACC = NO &               /*NOT D.A., TAPE, ...         */
*       UCB3TAPE = NO &                                         01S0105
*       UCBSYSR = NO)) THEN            /*... NOR PRIMARY CONSOLE     */
         TM    UCBNALOC(@09),B'00000100'                        01S0105
         BNZ   @RF00105                                         01S0105
         TM    UCB3DACC(@09),B'00100000'                        01S0105
         BNO   @GL00010                                         01S0105
         TM    UCBPRES(@09),B'00100100'                         01S0105
         BZ    @RT00105                                         01S0105
@GL00010 L     @09,UCBPTR                                       01S0105
         TM    UCB3DACC(@09),B'00100000'                        01S0105
         BNO   @GL00009                                         01S0105
         TM    UCBPRES(@09),B'00000100'                         01S0105
         BNO   @GL00009                                         01S0105
         TM    UCBALOC(@09),B'00001000'                         01S0105
         BNZ   @GL00009                                         01S0105
         TM    UCBONLI(@09),B'10000000'                         01S0105
         BZ    @RT00105                                         01S0105
         CLC   UCBVOLI(6,@09),NOVOLSER                          01S0105
         BE    @RT00105                                         01S0105
         TM    UCBMOUNT(@09),B'10000000'                        01S0105
         BO    @RT00105                                         01S0105
@GL00009 L     @09,UCBPTR                                       01S0105
         TM    UCB3TAPE(@09),B'10000000'                        01S0105
         BNO   @GL00008                                         01S0105
         TM    UCBRESV(@09),B'00100000'                         01S0105
         BZ    @RT00105                                         01S0105
@GL00008 L     @09,UCBPTR                                       01S0105
         TM    UCB3DACC(@09),B'10100000'                        01S0105
         BNZ   @RF00105                                         01S0105
         TM    UCBSYSR(@09),B'00000010'                         01S0105
         BNZ   @RF00105                                         01S0105
@RT00105 DS    0H                                               01S0106
*      IF UCBONLI = NO THEN            /*UNIT IS OFFLINE             */
         L     @09,UCBPTR                                       01S0106
         TM    UCBONLI(@09),B'10000000'                         01S0106
         BNZ   @RF00106                                         01S0106
*        DO;                                                    01S0107
*/********************************************************************/
*/*                                                                  */
*/* SEGMENT NAME - DMNDNEED                                          */
*/*                                                                  */
*/* DESCRIPTIVE NAME - TEST IF UCB IS DEMANDED                       */
*/*                                                                  */
*/* FUNCTION - THIS ROUTINE DETERMINES IF THE INPUT UCB IS           */
*/*            NEEDED BY A DEMAND REQUEST (E.G. UNIT=190).           */
*/*                                                                  */
*/*   OPERATION - THE SIOT CHAIN IS SCANNED TO LOCATE EACH           */
*/*               DEMAND REQUEST.                                    */
*/*                                                                  */
*/* ENTRY - (INLINE SUBROUTINE)                                      */
*/*                                                                  */
*/*   INPUT -                                                        */
*/*      SIOT1P - PTR TO THE FIRST SIOT IN THE CHAIN                 */
*/*        SIOTDMND - 1 FOR DEMAND REQUESTS                          */
*/*        SIOUCBAD - FOR DEMAND REQUEST, ADDRESS OF THE             */
*/*                   DEMANDED UCB                                   */
*/*        SIOTNPTR - PTR TO THE NEXT SIOT                           */
*/*      UCBPTR - PTR TO UCB                                         */
*/*                                                                  */
*/* EXIT - NORMAL - INLINE SUBROUTINE                                */
*/*                                                                  */
*/*   CONDITIONS - FUNCTION COMPLETED                                */
*/*                                                                  */
*/*   OUTPUT -                                                       */
*/*       USEUCB - 1 = NO DEMAND REQ NEEDS THIS UCB                  */
*/*              - 0 = A DEMAND REQ NEEDS THIS UCB                   */
*/*                                                                  */
*/********************************************************************/
*USEUCB = ON;                          /*ASSUME UCB NOT DEMANDED     */
         OI    USEUCB,B'00000100'                               02S0108
*RFY INDMSIOT BASED(SPTR);                                      02S0109
*SPTR = SIOT1P;                        /*POINT TO THE FIRST SIOT     */
         L     @09,@PC00001                                     02S0110
         L     @09,ALCWAPTR(,@09)                               02S0110
         L     SPTR,SIOT1P(,@09)                                02S0110
*DO WHILE (SPTR ^= ZERO                /*SCAN ALL SIOTS ..           */
*    & USEUCB = ON);                   /*..UNTIL UCB FOUND DEMANDED  */
         B     @DE00111                                         02S0111
@DL00111 DS    0H                                               02S0112
*  IF SIOTDMND = ON                    /*DEMAND REQ FOR THIS UCB ?   */
*      & SIOUCBAD = UCBPTR THEN                                 02S0112
         TM    SIOTDMND(SPTR),B'10000000'                       02S0112
         BNO   @RF00112                                         02S0112
         SLR   @09,@09                                          02S0112
         ICM   @09,7,SIOUCBAD(SPTR)                             02S0112
         C     @09,UCBPTR                                       02S0112
         BNE   @RF00112                                         02S0112
*    DO;                               /*YES,INDICATE UCB IS DEMANDED*/
*      USEUCB = OFF;                                            02S0114
         NI    USEUCB,B'11111011'                               02S0114
*    END;                                                       02S0115
*  ELSE;                               /*NOT DEMAND REQ FOR THIS UCB */
@RF00112 DS    0H                                               02S0117
*  SPTR = SIOTNPTR;                    /*POINT TO THE NEXT SIOT      */
         L     SPTR,SIOTNPTR(,SPTR)                             02S0117
*END;                                                           02S0118
@DE00111 LTR   SPTR,SPTR                                        02S0118
         BZ    @DC00111                                         02S0118
         TM    USEUCB,B'00000100'                               02S0118
         BO    @DL00111                                         02S0118
@DC00111 DS    0H                                               02S0119
*RFY INDMSIOT BASED(SIOTPTR);                                   02S0119
*          IF USEUCB = YES THEN        /*UNIT USEABLE BY NON-DMNDS   */
         TM    USEUCB,B'00000100'                               01S0120
         BNO   @RF00120                                         01S0120
*            OFFLNCT = OFFLNCT + ONE;  /*INCREMENT OFFLINES COUNT    */
         AH    OFFLNCT,@CH00066                                 01S0121
*        END;                                                   01S0122
*      ELSE;                           /*UNIT IS ONLINE              */
@RF00106 DS    0H                                               01S0124
*  END;                                                         01S0124
@RC00106 DS    0H                                               01S0124
@RF00105 AH    DEVCTR,@CH00066                                  01S0124
@DE00103 C     DEVCTR,GRPUCBNO(,GRPPTR)                         01S0124
         BNH   @DL00103                                         01S0124
*  IF OFFLNCT < GRPOFFLN THEN          /*FEWER OFFLINE UNITS IN GROUP*/
         LH    @09,GRPOFFLN(,GPCNTPTR)                          01S0125
         CR    OFFLNCT,@09                                      01S0125
         BNL   @RF00125                                         01S0125
*    DO;                               /*UPDATE COUNT TABLE ENTRY &
*                                        SEE IF CAN ALLOCATE WITH ANY
*                                        OF THE UNITS NOW ONLINE     */
*      NEWONS = GRPOFFLN - OFFLNCT;    /*NUMBER OF NEW OFFLINE  01S0127
*                                        DEVICES             @ZA10805*/
         SLR   @09,OFFLNCT                                      01S0127
         ST    @09,NEWONS                                       01S0127
*      GRPAVAIL = GRPAVAIL+(GRPOFFLN - OFFLNCT);/*UPDATE NO. UNITS
*                                        AVAILABLE                   */
         AH    @09,GRPAVAIL(,GPCNTPTR)                          01S0128
         STH   @09,GRPAVAIL(,GPCNTPTR)                          01S0128
*      GRPOFFLN = OFFLNCT;             /*PUT NEW COUNT IN ALG TABLE  */
         STH   OFFLNCT,GRPOFFLN(,GPCNTPTR)                      01S0129
*/********************************************************************/
*/*                                                                  */
*/* SEGMENT NAME - CHKNDMND                                          */
*/*                                                                  */
*/* DESCRIPTIVE NAME - CHECK FOR NON-DEMAND REQUESTS TO ALLOCATE     */
*/*                                                                  */
*/* FUNCTION -                                                       */
*/*   THIS ROUTINE DETERMINES IF ANY OF THE UNALLOCATED VOLUNIT      */
*/*   ENTRIES CAN BE ALLOCATED FROM THE DEVICE GROUP CURRENTLY       */
*/*   BEING PROCESSED. FOR ANY ENTRIES FOUND THE 'ALOCREQ' SUB-      */
*/*   ROUTINE IS INVOKED TO ATTEMPT TO ALLOCATE A UNIT FOR THE       */
*/*   VU ENTRY.                                                      */
*/*                                                                  */
*/*   OPERATION -                                                    */
*/*     THE ROUTINE WILL SCAN THE VOLUNIT TABLE, STARTING WITH       */
*/*     THE ENTRY POINTED TO BY THE INPUT PARAMETER 'FVUPTR'.        */
*/*     FOR EACH UNALLOCATED ENTRY WHICH REQUIRES A DEVICE THE       */
*/*     ROUTINE WILL DETERMINE IF THE GROUP BEING PROCESSED IS       */
*/*     ELIGIBLE TO THE REQUEST. IF IT IS, THE POINTER TO THE        */
*/*     CORRESPONDING EDL GROUP ENTRY IS OBTAINED AND THE 'ALOCREQ'  */
*/*     SUBROUTINE IS INVOKED.                                       */
*/*     IF THE ENTRY IS SUCCESSFULLY ALLOCATED, SEGMENT 'CHKSIOT'    */
*/*     WILL DETERMINE IF THIS REQUEST IS NOW COMPLETELY ALLOCATED.  */
*/*                                                                  */
*/* ENTRY -                                                          */
*/*   PURPOSE - SEE FUNCTION                                         */
*/*   INPUT -                                                        */
*/*     FVUPTR - PTR TO FIRST VOLUNIT ENTRY TO SCAN                  */
*/*     GRPIDENT - ID OF GROUP BEING PROCESSED                       */
*/*                                                                  */
*/* EXITS -                                                          */
*/*   CONDITION - NO ENTRIES TO ALLOCATE                             */
*/*     OUTPUT - NONE                                                */
*/*     RETURN CODES -                                               */
*/*       RETCODE = ZERO                                             */
*/*   CONDITION - AT LEAST ONE VOLUNIT ENTRY ALLOCATED SUCCESSFULLY  */
*/*     OUTPUT -                                                     */
*/*       VOLALOC - ON FOR EACH ENTRY ALLOCATED                      */
*/*       SIOTALCD - ON IF ENTIRE REQUEST ALLOCATED                  */
*/*       TOTREQS - DECREMENTED FOR EACH COMPLETELY ALLOCATED REQUEST*/
*/*       UCB/JFCB/DSAB/TIOT UPDATED                                 */
*/*       VM&V REQUEST BLOCKS BUILT IF REQUIRED                      */
*/*     RETURN CODES -                                               */
*/*       RETCODE = ZERO                                             */
*/*   CONDITION - TERMINATING ERROR ALLOCATING AN ENTRY              */
*/*     OUTPUT -                                                     */
*/*       RSNCODE = 2, IF GETMAIN ERROR                              */
*/*     RETURN CODES -                                               */
*/*       RETCODE = 4                                                */
*/********************************************************************/
*VUPTR = FVUPTR;                       /*POINT TO FIRST VU ENTRY TO
*                                        PROCESS                     */
         L     @09,@PC00001+4                                   02S0130
         MVC   VUPTR(4),FVUPTR(@09)                             02S0130
*DO VUCTR = 1 TO NOVUENTS              /*SCAN VOLUNIT TABLE          */
*    WHILE(RETCODE = ZERO &            /*STOP IF ERROR ...           */
*    TOTREQS > ZERO);                  /*... OR ALL REQS ALLOCATED   */
         LA    @09,1                                            02S0131
         B     @DE00131                                         02S0131
@DL00131 SLR   @09,@09                                          02S0131
         C     @09,RETCODE                                      02S0131
         BNE   @DC00131                                         02S0131
         L     @06,@PC00001                                     02S0131
         L     @06,ALCWAPTR(,@06)                               02S0131
         C     @09,TOTREQS(,@06)                                02S0131
         BNL   @DC00131                                         02S0131
*  RQLSTPTR = VOLALGTP;                /*POINT TO REQUEST LIST ENTRY */
         L     @06,VUPTR                                        02S0132
         L     @02,VOLALGTP(,@06)                               02S0132
         ST    @02,RQLSTPTR                                     02S0132
*  GPLSTPTR = CVRGRPPT;                /*POINT TO FIRST GROUP LIST   */
         MVC   GPLSTPTR(4),CVRGRPPT(@02)                        02S0133
*  GRPFOUND = OFF;                     /*ASSUME GROUP NOT IN LIST    */
         NI    GRPFOUND,B'11101111'                             02S0134
*  IF VOLALOC = NO &                   /*ENTRY NOT ALLOCATED ...     */
*      VDEVREQD = YES &                /*...REQUIRES A UNIT....      */
*      CVRALLCT = ZERO &               /*...NOT REDUCED YET....      */
*      CVRSKFLG = NO THEN              /*...IS CONSIDERED BY ALG.    */
         TM    VDEVREQD(@06),B'00100000'                        02S0135
         BNO   @RF00135                                         02S0135
         TM    VOLALOC(@06),B'10000000'                         02S0135
         BNZ   @RF00135                                         02S0135
         CH    @09,CVRALLCT(,@02)                               02S0135
         BNE   @RF00135                                         02S0135
         TM    CVRSKFLG(@02),B'10000000'                        02S0135
         BNZ   @RF00135                                         02S0135
*    DO GRPLCTR = 1 TO CVRNGRP         /*SCAN REQS ELIGIBLE GROUPS ..*/
*        WHILE (GRPFOUND = OFF);       /*... FOR GROUP JUST UPDATED  */
         LA    GRPLCTR,1                                        02S0136
         B     @DE00136                                         02S0136
@DL00136 TM    GRPFOUND,B'00010000'                             02S0136
         BNZ   @DC00136                                         02S0136
*      IF CVRGRPID = GRPIDENT THEN     /*LIST ENT IS FOR CURRENT GRP */
         L     @09,GPLSTPTR                                     02S0137
         LH    @02,CVRGRPID(,@09)                               02S0137
         CH    @02,GRPIDENT(,GPCNTPTR)                          02S0137
         BNE   @RF00137                                         02S0137
*        DO;                                                    02S0138
*          GRPFOUND = ON;              /*INDICATE GROUP FOUND        */
         OI    GRPFOUND,B'00010000'                             02S0139
*          IF CVRGSKP = NO &           /*GROUP IS ELIGIBLE TO REQUEST*/
*              CVRGALL => ZERO THEN    /*GROUP HAS POSSIBLE SOLUTION */
         TM    CVRGSKP(@09),B'10000000'                         02S0140
         BNZ   @RF00140                                         02S0140
         ICM   @15,3,CVRGALL(@09)                               02S0140
         BM    @RF00140                                         02S0140
