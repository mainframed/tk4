*/********************************************************************/
*/*                                                                  */
*/* SEGMENT - ALOCREQ                                                */
*/*                                                                  */
*/* DESCRIPTIVE NAME - ALLOCATE REQUEST                              */
*/*                                                                  */
*/* FUNCTION -                                                       */
*/*   THIS ROUTINE ATTEMPTS TO ALLOCATE THE INPUT REQUEST(I.E.,      */
*/*   VOLUNIT ENTRY) TO AN AVAILABLE DEVICE WITHIN THE INPUT         */
*/*   DEVICE GROUP.                                                  */
*/*                                                                  */
*/*   OPERATION -                                                    */
*/*     THE ROUTINE OPERATES AS FOLLOWS:                             */
*/*       .BUILDS A LIST OF ALL THE ONLINE,UNALLOCATED UCBS IN       */
*/*        THE INPUT DEVICE GROUP USING THE EDL UCB LIST FOR THIS    */
*/*        GROUP                                                     */
*/*       .IF A MSS REQUEST, DETERMINE WHETHER TO            @G18MP21*/
*/*        PERFORM MOUNT EQUALIZATION. IT IS NOT PERFORMED   @G18MP21*/
*/*        IF SU18 IS IN THE SYSTEM AND JES3 CHOSE UNITS     @G18MP21*/
*/*        FOR THE REQUEST.                                  @G18MP21*/
*/*       .IF THERE ARE UNITS AVAILABLE WHICH CAN BE ALLOCATED,      */
*/*        SEGMENT 'ALOCENT' WILL PICK THE UNIT TO ALLOCATE (VIA     */
*/*        THE OPTIMIZER) AND INVOKE IEFAB434 TO ALLOCATE THE        */
*/*        UNIT TO THIS VOLUNIT ENTRY.                               */
*/*       .IF NO UNITS ARE AVAILABLE NO FURTHER PROCESSING IS        */
*/*        DONE.                                                     */
*/*                                                                  */
*/* ENTRY - ALOCREQ (INTERNAL PROC)                                  */
*/*   PURPOSE - SEE FUNCTION                                         */
*/*   INPUT -                                                        */
*/*     VUPTR - PTR TO VOLUNIT ENTRY TO ALLOCATE                     */
*/*     EDLGNTP - PTR TO EDL GROUP ENTRY TO USE                      */
*/*       EDLUCBNO - NUMBER OF UCBS IN THIS GROUP                    */
*/*       EDLLISTP - PTR TO UCB LIST                                 */
*/*         EDLUCBAD - ARRAY OF UCB ADDRESSES                        */
*/*     SIOTPTR - PTR TO SIOT FOR VOLUNIT ENTRY                      */
*/*     ARUGRPID - GROUP ID OF DEVICE GROUP BEING PROCESSED          */
*/*     TOTVOLUN - NUMBER OF UNALLOCATED VOLUNIT ENTRIES             */
*/*                                                                  */
*/* EXIT - RETURN TO CALLER                                          */
*/*   CONDITION - ALLOCATION SUCCESSFUL                              */
*/*     OUTPUT -                                                     */
*/*       VOLALOC - ON TO INDICATE ENTRY ALLOCATED                   */
*/*       TOTVOLUN - DECREMENTED BY ONE                              */
*/*       UCB/JFCB/DSAB/TIOT UPDATED                                 */
*/*       VM&V REQUEST BLOCK CREATED IF NECESSARY                    */
*/*     RETURN CODES -                                               */
*/*       RETCODE = ZERO                                             */
*/*   CONDITION - TERMINATING ERROR DURING PROCESSING                */
*/*     OUTPUT -                                                     */
*/*       RSNCODE = 2, IF GETMAIN ERROR                              */
*/*     RETURN CODES -                                               */
*/*       RETCODE = 4                                                */
*/*                                                                  */
*/********************************************************************/
*ALOCREQ:                              /*ATTEMPT TO ALLOCATE REQUEST */
*  PROC;                                                        02S0201
ALOCREQ  STM   @14,@12,@SA00002                                 02S0201
* /* ?GETSTORG (SPLNO,LENGTH(OPTPARMS),OPTPARMP);*/             02S0202
*   /* DEFINITIONS OF GETMAIN PARMS */                          02S0202
*   DCL                                                         02S0202
*     GTSTR0   FIXED(31) REG(0),/*GTMN PARM REG*/               02S0202
*     GTSTR1   FIXED(31) REG(1),/*GTMN PARM REG*/               02S0202
*     GTSTR2   PTR(31)   REG(2),/*GTMN PARM REG*/               02S0202
*     GTSTR15  FIXED(31) REG(15),/*GTMN RET COD*/               02S0202
*     GTSTRTCD FIXED(31); /* GETMAIN RTN CODE  */               02S0202
*   DCL                                                         02S0203
*     GTSTR3   FIXED(31) REG(3),/* CLEARING REG*/               02S0203
*     GTSTR4   FIXED(31) REG(4),/* CLEARING REG*/               02S0203
*     GTSTR5   FIXED(31) REG(5),/* CLEARING REG*/               02S0203
*     GTSTR6   FIXED(31) REG(6),/* CLEARING REG*/               02S0203
*     GTSTR7   FIXED(31) REG(7),/* CLEARING REG*/               02S0203
*     GTSTR8   FIXED(31) REG(8),/* CLEARING REG*/               02S0203
*     GTSTZERO FIXED(31) INIT(0),                               02S0203
*     GTST253  FIXED(31) CONSTANT(253);                         02S0203
*   RFY (GTSTR0,GTSTR1,GTSTR2) RSTD; /*SAVE RGS*/               02S0204
*   GTSTR0 = LENGTH(OPTPARMS); /* SET AMOUNT*/                  02S0205
         LA    GTSTR0,20                                        02S0205
*   GTSTR2 = SPLNO; /* SET SUBPOOL*/                            02S0206
         LA    GTSTR2,253                                       02S0206
*   GENERATE CODE;                                              02S0207
     GETMAIN RC,LV=(0),SP=(2)  DO GETMAIN
*   GTSTRTCD = GTSTR15; /* SAVE GETMAIN RET COD*/               02S0208
         ST    GTSTR15,GTSTRTCD                                 02S0208
*   OPTPARMP = GTSTR1; /* ADDR OF GOTN STORG*/                  02S0209
         ST    GTSTR1,OPTPARMP                                  02S0209
*   RFY (GTSTR0,GTSTR1,GTSTR2) UNRSTD; /*RLSE */                02S0210
*   IF GTSTRTCD = 0 THEN /*GETMAIN SUCCESSFUL? */               02S0211
         L     @08,GTSTRTCD                                     02S0211
         LTR   @08,@08                                          02S0211
         BNZ   @RF00211                                         02S0211
*     DO;               /* YES, CLEAR STORAGE.*/                02S0212
*       RFY (GTSTR2,GTSTR4,GTSTR5,GTSTR6,                       02S0213
*                          GTSTR7,GTSTR8) RSTD;                 02S0213
*       GTSTR4 = OPTPARMP; /*STORG PTR*/                        02S0214
         L     @03,OPTPARMP                                     02S0214
         LR    GTSTR4,@03                                       02S0214
*       GTSTR5 = LENGTH(OPTPARMS); /*STORG AMNT*/               02S0215
         LA    GTSTR5,20                                        02S0215
*       GTSTR7 = '00000000'X; /*PAD CHAR & LNTH*/               02S0216
         SLR   GTSTR7,GTSTR7                                    02S0216
*       IF SPLNO = GTST253 THEN                                 02S0217
*      GEN                                                      02S0218
*   (MODESET KEYADDR=GTSTZERO,SAVEKEY=(2),WORKREG=8);           02S0218
         MODESET KEYADDR=GTSTZERO,SAVEKEY=(2),WORKREG=8
*       GEN REFS(GTSTR4,GTSTR6);                                02S0219
          MVCL  GTSTR4,GTSTR6 CLEAR GOTTEN STORG
*       IF SPLNO = GTST253 THEN                                 02S0220
*         GEN (MODESET KEYADDR=(2));                            02S0221
         MODESET KEYADDR=(2)
*       RFY (GTSTR2,GTSTR4,GTSTR5,GTSTR6,                       02S0222
*                          GTSTR7,GTSTR8) UNRSTD;               02S0222
*     END;       /* STORAGE CLEARING COMPLETED */               02S0223
*   ELSE                                                        02S0224
*   OPTPARMP= 0;   /* CLEAR RETURN ADDR. */                     02S0224
         B     @RC00211                                         02S0224
@RF00211 SLR   @08,@08                                          02S0224
         ST    @08,OPTPARMP                                     02S0224
*   GTSTR15 = GTSTRTCD;    /* GTMN RETURN CODE */               02S0225
@RC00211 L     GTSTR15,GTSTRTCD                                 02S0225
*                                      /* RESTORE LIST PRINT STATUS  */
*RETCODE = REG15;                      /*SAVE RETURN CODE            */
         ST    REG15,RETCODE                                    02S0226
*IF RETCODE = ZERO THEN                /*CORE GOTTEN O.K. FOR PARMS  */
         L     @08,RETCODE                                      02S0227
         LTR   @08,@08                                          02S0227
         BNZ   @RF00227                                         02S0227
*  DO;                                 /*GET CANDIDATE LIST CORE     */
*    CNDLSTLN = (EDLUCBNO * FOUR) + FOUR; /*SET SIZE OF CANDIDATE LST*/
         L     @08,EDLGNTP                                      02S0229
         L     @08,EDLUCBNO(,@08)                               02S0229
         SLA   @08,2                                            02S0229
         AL    @08,@CF00138                                     02S0229
         ST    @08,CNDLSTLN                                     02S0229
*     /* ?GETSTORG (SPLNO,CNDLSTLN,CANDLSTP);*/                 02S0230
*       RFY (GTSTR0,GTSTR1,GTSTR2) RSTD; /*SAVE RGS*/           02S0230
*       GTSTR0 = CNDLSTLN; /* SET AMOUNT*/                      02S0231
         LR    GTSTR0,@08                                       02S0231
*       GTSTR2 = SPLNO; /* SET SUBPOOL*/                        02S0232
         LA    GTSTR2,253                                       02S0232
*       GENERATE CODE;                                          02S0233
         GETMAIN RC,LV=(0),SP=(2)  DO GETMAIN
*       GTSTRTCD = GTSTR15; /* SAVE GETMAIN RET COD*/           02S0234
         ST    GTSTR15,GTSTRTCD                                 02S0234
*       CANDLSTP = GTSTR1; /* ADDR OF GOTN STORG*/              02S0235
         ST    GTSTR1,CANDLSTP                                  02S0235
*       RFY (GTSTR0,GTSTR1,GTSTR2) UNRSTD; /*RLSE */            02S0236
*       IF GTSTRTCD = 0 THEN /*GETMAIN SUCCESSFUL? */           02S0237
         L     @08,GTSTRTCD                                     02S0237
         LTR   @08,@08                                          02S0237
         BNZ   @RF00237                                         02S0237
*         DO;               /* YES, CLEAR STORAGE.*/            02S0238
*           RFY (GTSTR2,GTSTR4,GTSTR5,GTSTR6,                   02S0239
*                              GTSTR7,GTSTR8) RSTD;             02S0239
*           GTSTR4 = CANDLSTP; /*STORG PTR*/                    02S0240
         L     @03,CANDLSTP                                     02S0240
         LR    GTSTR4,@03                                       02S0240
*           GTSTR5 = CNDLSTLN; /*STORG AMNT*/                   02S0241
         L     GTSTR5,CNDLSTLN                                  02S0241
*           GTSTR7 = '00000000'X; /*PAD CHAR & LNTH*/           02S0242
         SLR   GTSTR7,GTSTR7                                    02S0242
*           IF SPLNO = GTST253 THEN                             02S0243
*          GEN                                                  02S0244
*       (MODESET KEYADDR=GTSTZERO,SAVEKEY=(2),WORKREG=8);       02S0244
         MODESET KEYADDR=GTSTZERO,SAVEKEY=(2),WORKREG=8
*           GEN REFS(GTSTR4,GTSTR6);                            02S0245
              MVCL  GTSTR4,GTSTR6 CLEAR GOTTEN STORG
*           IF SPLNO = GTST253 THEN                             02S0246
*             GEN (MODESET KEYADDR=(2));                        02S0247
         MODESET KEYADDR=(2)
*           RFY (GTSTR2,GTSTR4,GTSTR5,GTSTR6,                   02S0248
*                              GTSTR7,GTSTR8) UNRSTD;           02S0248
*         END;       /* STORAGE CLEARING COMPLETED */           02S0249
*       ELSE                                                    02S0250
