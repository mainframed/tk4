         LA    SSCLCTR,1                                        03S0341
         B     @DE00341                                         03S0341
@DL00341 TM    FOUNDSW,B'00100000'                              03S0341
         BNZ   @DC00341                                         03S0341
*                  IF CANDUCB(OLDLCTR) = /*OLD LIST ENTRY    @Y30LPPC*/
*                     SSCLUCBP(SSCLCTR) THEN /*NEW LIST ENT  @Y30LPPC*/
         LR    @08,OLDLCTR                                      03S0342
         SLA   @08,2                                            03S0342
         L     @05,CANDLSTP                                     03S0342
         L     @08,CANDUCB-4(@08,@05)                           03S0342
         LR    @05,SSCLCTR                                      03S0342
         SLA   @05,2                                            03S0342
         L     @01,SSCUCBLP                                     03S0342
         C     @08,SSCLUCBP-4(@05,@01)                          03S0342
         BNE   @RF00342                                         03S0342
*                    DO;               /*ADD UCB TO NEW LIST @Y30LPPC*/
*                      NEWLUCBP(NEWLCTR)= CANDUCB(OLDLCTR);/*@Y30LPPC*/
         LR    @05,NEWLCTR                                      03S0344
         SLA   @05,2                                            03S0344
         ST    @08,NEWLUCBP-4(@05,NEWLPTR)                      03S0344
*                      NEWLCANO = NEWLCTR; /*SET NUM ENTRIES @Y30LPPC*/
         ST    NEWLCTR,NEWLCANO(,NEWLPTR)                       03S0345
*                      NEWLCTR = NEWLCTR + ONE; /*NEXT ENT   @Y30LPPC*/
         AH    NEWLCTR,@CH00066                                 03S0346
*                      DONESW = ON;    /*OK TO QUIT          @Y30LPPC*/
*                      FOUNDSW = ON;   /*A MATCH WAS FOUND   @Y30LPPC*/
         OI    DONESW,B'10100000'                               03S0348
*                    END;              /*END ADD UCB         @Y30LPPC*/
*                END;                  /*END SCAN SSC LIST   @Y30LPPC*/
@RF00342 AH    SSCLCTR,@CH00066                                 03S0350
@DE00341 L     @08,SSCUCBLP                                     03S0350
         C     SSCLCTR,SSCLNUCB(,@08)                           03S0350
         BNH   @DL00341                                         03S0350
@DC00341 DS    0H                                               03S0351
*              END;                    /*END SCAN OLD LIST   @Y30LPPC*/
         AH    OLDLCTR,@CH00066                                 03S0351
@DE00339 C     OLDLCTR,CANDCTR                                  03S0351
         BNH   @DL00339                                         03S0351
*               DO; /*MODESET  EXTKEY(SCHED)*/                  03S0352
*                 SPKA (16); /*SET PSW KEY*/                    03S0353
         LA    @08,16                                           03S0353
         SPKA  0(@08)                                           03S0353
*               END; /*MODESET  EXTKEY(SCHED)*/ /*RESUME SCHED KEY    @
*30LPPC*/                                                       03S0354
*              RFY (REG0,REG1) RSTD;   /*                    @Y30LPPC*/
*              REG0 = SSCLSPLN;        /*SUBPOOL AND LENGTH  @Y30LPPC*/
         L     @08,SSCUCBLP                                     03S0356
         L     REG0,SSCLSPLN(,@08)                              03S0356
*              REG1 = SSCUCBLP;        /*ADDRESS OF LIST     @Y30LPPC*/
         LR    REG1,@08                                         03S0357
*              GEN (FREEMAIN R,LV=(0),A=(1)); /*FREE LIST    @Y30LPPC*/
         FREEMAIN R,LV=(0),A=(1)
*              RFY (REG0,REG1) UNRSTD; /*                    @Y30LPPC*/
*              SSCUCBLP = ZERO;        /*ZERO LIST ADDR      @ZM00427*/
         SLR   @08,@08                                          03S0360
         ST    @08,SSCUCBLP                                     03S0360
*              IF DONESW = ON THEN     /*NEW LIST BUILT      @Y30LPPC*/
         TM    DONESW,B'10000000'                               03S0361
         BNO   @RF00361                                         03S0361
*                DO;                   /*FREE OLD LIST       @Y30LPPC*/
*                  RFY (REG0,REG1) RSTD;/*                   @Y30LPPC*/
*                  REG0 = CNDLSTLN;    /*LENGTH OF OLD LIST  @Y30LPPC*/
         L     REG0,CNDLSTLN                                    03S0364
*                  REG0 = REG0 × FREESPNO; /* OR IN SUBPOOL  @Y30LPPC*/
         O     REG0,@CF00158                                    03S0365
*                  REG1 = CANDLSTP;    /*ADDR OF OLD LIST    @Y30LPPC*/
         L     REG1,CANDLSTP                                    03S0366
*                  GEN (FREEMAIN R,LV=(0),A=(1)); /*FREE LST @Y30LPPC*/
         FREEMAIN R,LV=(0),A=(1)
*                  RFY (REG0,REG1) UNRSTD;/*                 @Y30LPPC*/
*                  CANDLSTP = NEWLPTR; /*POINT TO NEW LIST   @Y30LPPC*/
         ST    NEWLPTR,CANDLSTP                                 03S0369
*                   DO; /*MODESET  KEYADDR(ZERO)*/              03S0370
*                     SPKA (ZERO); /*SET PSW KEY*/              03S0371
         SLR   @08,@08                                          03S0371
         SPKA  0(@08)                                           03S0371
*                   END; /*MODESET  KEYADDR(ZERO)*//*KEY ZERO FOR OPT @
*30LPPC*/                                                       03S0372
*                  CANDLPTR = NEWLPTR; /*POINT OPTPARMS TO      03S0373
*                                        NEW LIST            @Y30LPPC*/
         L     @08,OPTPARMP                                     03S0373
         ST    NEWLPTR,CANDLPTR(,@08)                           03S0373
*                  CANDCTR = NEWLCANO; /*SET TO NEW LIST     @Y30LPPC*/
         MVC   CANDCTR(4),NEWLCANO(NEWLPTR)                     03S0374
*                  NEWLPTR = ZERO;     /*CLEAR NEW LIST ADDR @Y30LPPC*/
         SLR   NEWLPTR,NEWLPTR                                  03S0375
*                   DO; /*MODESET  EXTKEY(SCHED)*/              03S0376
*                     SPKA (16); /*SET PSW KEY*/                03S0377
         LA    @08,16                                           03S0377
         SPKA  0(@08)                                           03S0377
*                   END; /*MODESET  EXTKEY(SCHED)*/ /*RESUME SCHED KEY@
*30LPPC*/                                                       03S0378
*                END;                  /*END FREE OLD LIST   @Y30LPPC*/
*             END;                     /*END MERGE LISTS     @Y30LPPC*/
*           ELSE                       /*NO LIST BUILT BY SSC@Y30LPPC*/
*             DONESW = ON;             /*GIVE UP             @Y30LPPC*/
         B     @RC00333                                         03S0381
@RF00333 OI    DONESW,B'10000000'                               03S0381
*         END;                         /*END LOOP UNTIL OK   @Y30LPPC*/
@RC00333 DS    0H                                               03S0382
@DE00331 TM    DONESW,B'10000000'                               03S0382
         BZ    @DL00331                                         03S0382
*       IF NEWLPTR ^= ZERO THEN        /*NEW LIST MUST BE FREED 03S0383
*                                                            @Y30LPPC*/
         LTR   NEWLPTR,NEWLPTR                                  03S0383
         BZ    @RF00383                                         03S0383
*         DO;                          /*FREE NEW LIST       @Y30LPPC*/
*           RFY (REG0,REG1) RSTD;      /*                    @Y30LPPC*/
*           REG0 = CNDLSTLN;           /*LENGTH TO FREE      @Y30LPPC*/
         L     REG0,CNDLSTLN                                    03S0386
*           REG0 = REG0 × FREESPNO;    /*SUBPOOL TO FREE     @Y30LPPC*/
         O     REG0,@CF00158                                    03S0387
*           REG1 = NEWLPTR;            /*ADDR TO FREE        @Y30LPPC*/
         LR    REG1,NEWLPTR                                     03S0388
*           GEN (FREEMAIN R,LV=(0),A=(1)); /*FREE THE LIST   @Y30LPPC*/
         FREEMAIN R,LV=(0),A=(1)
*           RFY (REG0,REG1) UNRSTD;    /*                    @Y30LPPC*/
*         END;                                                  03S0391
*       END;                           /*END CONTINUE        @Y30LPPC*/
@RF00383 DS    0H                                               03S0393
*  END;                                /*END DO MOUNT EQUAL  @Y30LPPC*/
@RF00328 DS    0H                                               03S0394
*            END;                      /*END MNT. EQUAL.     @G18MP21*/
@RF00298 DS    0H                                               02S0395
*        IF CANDCTR > ZERO THEN        /*LIST IS NOT EMPTY           */
@RF00296 DS    0H                                               02S0395
@RF00295 ICM   @15,15,CANDCTR                                   02S0395
