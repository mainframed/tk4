         AH    GPCNTPTR,@CH00047                                01S0198
*END;                                                           01S0199
         LA    @09,1                                            01S0199
         AL    @09,GRPCTR                                       01S0199
@DE00100 ST    @09,GRPCTR                                       01S0199
         CR    @09,NUMGRPS                                      01S0199
         BNH   @DL00100                                         01S0199
@DC00100 DS    0H                                               01S0200
*RETURN CODE(RETCODE);                 /*RETURN TO CALLER            */
         L     @09,RETCODE                                      01S0200
         L     @13,4(,@13)                                      01S0200
         L     @00,@SIZDATD                                     01S0200
         LR    @01,@10                                          01S0200
