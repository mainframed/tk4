         OI    SIOTALCD(@09),B'00000010'                        03S0174
*    TOTREQS = TOTREQS - ONE;          /*DECREMENT # REQS TO PROCESS */
         L     @09,@PC00001                                     03S0175
         L     @09,ALCWAPTR(,@09)                               03S0175
         L     @02,TOTREQS(,@09)                                03S0175
         BCTR  @02,0                                            03S0175
         ST    @02,TOTREQS(,@09)                                03S0175
