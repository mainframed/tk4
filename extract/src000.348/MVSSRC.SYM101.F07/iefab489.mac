*/*       EDLGRPTR - PTR TO FIRST GROUP ENTRY FOR GENERIC            */
*/*         EDLGRPNO - NUMBER OF GROUP ENTRIES                       */
*/*         EDLGRID - GROUP ID                                       */
*/*                                                                  */
*/* EXIT -                                                           */
*/*   CONDITION - FUNCTION COMPLETED                                 */
*/*     OUTPUT -                                                     */
*/*       EDLENTP - POINTER TO REQUIRED GROUP ENTRY                  */
*/*     RETURN CODES - NONE                                          */
*/********************************************************************/
*EDLPTR = EDLFGENP;                    /*GET PTR TO FIRST GENERIC ENT*/
         L     @02,SIOTEDLP(,@15)                               03S0144
         L     EDLPTR,EDLFGENP(,@02)                            03S0144
*FOUNDEDL = OFF;                       /*INIT SCAN CONTROL SWITCH    */
         NI    FOUNDEDL,B'10111111'                             03S0145
*DO GENCTR = 1 TO EDLGENNO             /*SCAN GENRIC ENTS FOR GROUP  */
*    WHILE(FOUNDEDL = OFF);                                     03S0146
         LA    GENCTR,1                                         03S0146
         B     @DE00146                                         03S0146
@DL00146 TM    FOUNDEDL,B'01000000'                             03S0146
         BNZ   @DC00146                                         03S0146
*  IF EINDEVTP = NO THEN               /*ELIGIBLE GENERIC            */
         TM    EINDEVTP(EDLPTR),B'10000000'                     03S0147
         BNZ   @RF00147                                         03S0147
*    DO;                               /*SCAN GROUP ENTRIES          */
*      EDLGNTP = EDLGRPTR;             /*POINT TO FIRST GROUP ENTRY  */
         MVC   EDLGNTP(4),EDLGRPTR(EDLPTR)                      03S0149
*      DO EGRPCTR = 1 TO EDLGRPNO      /*SCAN FOR GROUP ID MATCH     */
*          WHILE(FOUNDEDL = OFF);                               03S0150
         LA    EGRPCTR,1                                        03S0150
         B     @DE00150                                         03S0150
@DL00150 TM    FOUNDEDL,B'01000000'                             03S0150
         BNZ   @DC00150                                         03S0150
*        IF ARUGRPID = EDLGRID THEN    /*GROUP ID'S MATCH            */
         L     @05,EDLGNTP                                      03S0151
         CLC   ARUGRPID(4),EDLGRID(@05)                         03S0151
         BNE   @RF00151                                         03S0151
*          FOUNDEDL = ON;              /*SET EXIT INDICATOR          */
         OI    FOUNDEDL,B'01000000'                             03S0152
*        ELSE                          /*NOT INPUT GROUP             */
*          EDLGNTP = EDLNXGRP;         /*POINT TO NEXT GROUP ENTRY   */
         B     @RC00151                                         03S0153
@RF00151 L     @05,EDLGNTP                                      03S0153
         L     @05,EDLNXGRP(,@05)                               03S0153
         ST    @05,EDLGNTP                                      03S0153
*      END;                                                     03S0154
@RC00151 AH    EGRPCTR,@CH00066                                 03S0154
@DE00150 C     EGRPCTR,EDLGRPNO(,EDLPTR)                        03S0154
         BNH   @DL00150                                         03S0154
@DC00150 DS    0H                                               03S0155
*    END;                                                       03S0155
* ELSE;                                /*GENERIC NOT ELIGIBLE TO REQ */
@RF00147 DS    0H                                               03S0157
* IF FOUNDEDL = NO THEN                /*GROUP NOT FOUND YET         */
         TM    FOUNDEDL,B'01000000'                             03S0157
         BNZ   @RF00157                                         03S0157
*   EDLPTR = EDLNXTNT;                 /*POINT TO NEXT GENERIC ENTRY */
         L     EDLPTR,EDLNXTNT(,EDLPTR)                         03S0158
*END;                                                           03S0159
@RF00157 AH    GENCTR,@CH00066                                  03S0159
@DE00146 L     @01,SIOTPTR                                      03S0159
         L     @01,SIOTEDLP(,@01)                               03S0159
         C     GENCTR,EDLGENNO(,@01)                            03S0159
         BNH   @DL00146                                         03S0159
@DC00146 DS    0H                                               03S0160
*              CALL ALOCREQ;           /*TRY TO ALLOCATE THE VU ENTRY*/
         BAL   @14,ALOCREQ                                      02S0160
