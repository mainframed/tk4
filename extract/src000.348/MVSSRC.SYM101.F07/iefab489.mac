*DO AVUCTR = 1 TO SVOLUNNO             /*SCAN ALL ENTRIES ...        */
*    WHILE(REQALCD = ON);              /*... STOP IF ANY NOT ALLOC'D */
         LA    AVUCTR,1                                         03S0166
         B     @DE00166                                         03S0166
@DL00166 TM    REQALCD,B'00001000'                              03S0166
         BNO   @DC00166                                         03S0166
*  IF VOLALOC = NO &                   /*ENTRY NOT ALLOC'D AND ...   */
*      VDEVREQD = YES THEN             /*... NEEDS A DEVICE          */
         TM    VDEVREQD(AVUPTR),B'00100000'                     03S0167
         BNO   @RF00167                                         03S0167
         TM    VOLALOC(AVUPTR),B'10000000'                      03S0167
         BNZ   @RF00167                                         03S0167
*    REQALCD = OFF;                    /*INDICATE REQ NOT ALLOCATED  */
         NI    REQALCD,B'11110111'                              03S0168
*  ELSE;                                                        03S0169
@RF00167 DS    0H                                               03S0170
*  AVUPTR = AVUPTR + LENGTH(VOLUNTAB); /*POINT TO NEXT VU ENTRY      */
         AL    AVUPTR,@CF00085                                  03S0170
*END;                                                           03S0171
         AH    AVUCTR,@CH00066                                  03S0171
@DE00166 L     @09,SIOTPTR                                      03S0171
         CH    AVUCTR,SVOLUNNO(,@09)                            03S0171
         BNH   @DL00166                                         03S0171
@DC00166 DS    0H                                               03S0172
*IF REQALCD = YES &                    /*ALLOC'N COMPLETE FOR SIOT   */
*    SIOTALCD = NO THEN                                         03S0172
         TM    REQALCD,B'00001000'                              03S0172
         BNO   @RF00172                                         03S0172
         L     @09,SIOTPTR                                      03S0172
         TM    SIOTALCD(@09),B'00000010'                        03S0172
         BNZ   @RF00172                                         03S0172
*  DO;                                 /*UPDATE SIOT & COUNT TABLE   */
