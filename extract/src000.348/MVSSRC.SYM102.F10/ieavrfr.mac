         TITLE 'IEAVRFR:REAL FRAME REPLACEMENT--SELECT && STEAL        *
                         '
*                                              /* @Z40WPYC*/
IEAVRFR  CSECT                    DEFINE RFR CSECT  @Z40WPYC
         USING *,R15              SET TEMP BASE     @Z40WPYC
         MODID                                      @Z40WPYC
         ENTRY IEAVRFR2                             @Z40WPYC
         DROP  R15                                  @Z40WPYC
IEAVRFR2 DS    0H                                   @Z40WPYC
         STM   R14,R12,12(R13)            SAVE CALLERS REGS @Z40WPYC
         BALR  R9,0                       ESTABLISH BASE    @Z40WPYC
@PSTART  DS    0H                         @Z40WPYC
         USING *,R9                       @Z40WPYC
*   PARMPTR=R1;                     /* POINTER TO PARAMETER LIST     */
         LR    PARMPTR,R1                                          0093
*   PVTPTR=CVTPVTP;                 /* POINTER TO PVT                */
         L     @12,CVTPTR                                          0094
         L     PVTPTR,CVTPVTP(,@12)                                0094
*   RFY                                                            0095
*    (R10,                                                         0095
*     R11,                                                         0095
*     R12,                                                         0095
*     R7) RSTD;                                                    0095
*   R10=R13;                        /* SAVE R13 VALUE OVER SETLOCK 0096
*                                      AND OTHER EXTERNAL CALLS      */
*                                                                  0096
         LR    R10,R13                                             0096
*   /*****************************************************************/
*   /*                                                               */
*   /* OBTAIN THE SALLOC LOCK UNCONDITIONALLY                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0097
*GETLOCK:                                                          0097
*   GEN CODE NODEFS REFS(PSALITA,FLC) SETS(R11,R12,R13,R14) NOFLOWS;
*                                                                  0097
GETLOCK  DS    0H                                                  0097
         SETLOCK OBTAIN,TYPE=SALLOC,MODE=UNCOND,                      **
               RELATED=(SALLOC,IEAVRFR(FREELOCK))
*   /*****************************************************************/
*   /*                                                               */
*   /* ESTABLISH THE RSM RECOVERY ROUTINE.                   @Z30WPYC*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0098
*   GEN CODE REFS(R3,PSACSTK,FRRS,FRRSCURR,FRRSLAST,FRRSELEN,FRRSENTR,
*       PVTPRCV,FRRSFRRA,PVTPRCA,FRRSPARM,PSALSFCC) SETS(R11,R12,  0098
*       FRRSCURR,PVTPRCA);                                         0098
   SETFRR A,FRRAD=PVTPRCV(R3),PARMAD=(R7),WRKREGS=(R11,R12)
*   RFY                                                            0099
*    (R11,                                                         0099
*     R12) UNRSTD;                                                 0099
*   R7->RCACSECT=RCACREPL;          /* SET MODULE ID         @Z30WPYC*/
         MVI   RCACSECT(R7),X'0F'                                  0100
*   R7->RCARFR='1'B;                /* SET CSECT ENTERED     @Z30WPYC*/
         OI    RCARFR(R7),B'00000001'                              0101
*   R7->RCASAL='1'B;                /* SET SALLOC HELD BIT   @Z30WPYC*/
         OI    RCASAL(R7),B'10000000'                              0102
*   PVTPRCA=R7;                     /* STORE RCA ADDR IN PVT         */
         ST    R7,PVTPRCA(,PVTPTR)                                 0103
*   RFY                                                            0104
*    (R7) UNRESTRICTED;                                            0104
*   PGOUTERR='0'B;                  /* INITIALIZE PGOUT ERROR FLAG   */
         NI    PGOUTERR(PVTPTR),B'01111111'                        0105
*   AIAQUEUE=0;                     /* ZERO INTERNAL AIA QUEUE       */
         SLR   @12,@12                                             0106
         ST    @12,AIAQUEUE(,PVTPTR)                               0106
*   CIOQUEUE=0;                     /* ZERO INTERNAL CIOQ ANCHOR     */
         ST    @12,CIOQUEUE(,PVTPTR)                               0107
*   LIOQUEUE=0;                     /* ZERO INTERNAL LIOQ ANCHOR     */
         ST    @12,LIOQUEUE(,PVTPTR)                               0108
*   R10SAVE=R10;                    /* SAVE SAVEAREA PTR     @Z30WPYC*/
         ST    R10,R10SAVE(,PVTPTR)                                0109
*                                                                  0110
*   /*****************************************************************/
*   /*                                                               */
*   /* IF THE INPUT ASCB ADDRESS FIELD IS ZERO, SCAN THE COMMON FRAME*/
*   /* QUEUE. OTHERWISE, SCAN THE LOCAL FRAME QUEUE OF THE ADDRESS   */
*   /* SPACE IDENTIFIED IN THE PARAMETER LIST ENTRY.                 */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0110
*PARM:                                                             0110
*   RFY                                                            0110
*    (INDEX,                                                       0110
*     ENDQ,                                                        0110
*     STARTQ,                                                      0110
*     QHDRPTR,                                                     0110
*     STEALCNT,                                                    0110
*     LRBNSAV) RESTRICTED;                                         0110
PARM     DS    0H                                                  0111
*   STEALCNT=0;                     /* INDICATE 0 PAGES STOLEN     0111
*                                                            @Z30WPYC*/
         SLR   STEALCNT,STEALCNT                                   0111
*   ENDQ=0;                         /* CLEAR END-OF-REFERENCE QUEUE
*                                      PTR.                  @Z30WPYC*/
         SLR   ENDQ,ENDQ                                           0112
*   HIGHUIC='0'B;                   /* INITIALIZE INTERNAL FLAG WHICH
*                                      INDICATES WHEN HIGHEST UIC HAS
*                                      BEEN SAVED            @Z40FPYL*/
*   STEALIND='0'B;                  /* INITIALIZE INTERNAL FLAG WHICH
*                                      INDICATES WHEN STEAL MUST BE
*                                      SCHEDULED.                    */
         NI    HIGHUIC(PVTPTR),B'11001111'                         0114
*   IF ASCBADR=0 THEN               /* TEST IF STEAL IS FOR COMMON OR
*                                      LOCAL AREA                    */
         L     @11,ASCBADR-1(,PARMPTR)                             0115
         LA    @11,0(,@11)                                         0115
         LTR   @11,@11                                             0115
         BNZ   @RF00115                                            0115
*     DO;                           /* COMMON                        */
*       QHDRPTR=ADDR(PVTCFQF);      /* GET PTR TO COMMON FRAME QUEUE
*                                      PTRS.                 @Z30WPYC*/
         LA    QHDRPTR,PVTCFQF(,PVTPTR)                            0117
*       RSMHDPTR=PSAAOLD->ASCBRSM;  /* GET CURRENT RSMHDR ADDRESS    */
         L     @11,PSAAOLD                                         0118
         L     RSMHDPTR,ASCBRSM(,@11)                              0118
*     END;                                                         0119
*   ELSE                            /* ELSE, IF NOT FOR COMMON AREA  */
*     DO;                           /* SCAN THE LOCAL FRAME QUEUE    */
         B     @RC00115                                            0120
@RF00115 DS    0H                                                  0121
*       RSMHDPTR=ASCBRSM;           /* GET RSM HEADER ADDRESS        */
         L     @11,ASCBPTR(,PARMPTR)                               0121
         L     RSMHDPTR,ASCBRSM(,@11)                              0121
*       QHDRPTR=ADDR(RSMLFQF);      /* GET PTR TO LOCAL FRAME QUEUE
*                                      PTRS.                 @Z30WPYC*/
         LA    QHDRPTR,RSMLFQF(,RSMHDPTR)                          0122
*       IF RSMHDPTR=0 THEN          /* TEST IF RSMHDR EXISTS         */
         LTR   RSMHDPTR,RSMHDPTR                                   0123
         BZ    @RT00123                                            0123
*         GOTO GETNEXT;             /* IF NO RSM HEADER, GO ON TO  0124
*                                      NEXT ENTRY IN LIST            */
*     END;                                                         0125
*   INDEX=QHDRPTR->FIRSTRBN;        /* GET RBN INDEX TO FIRST PFTE.
*                                                            @Z30WPYC*/
@RC00115 LH    INDEX,FIRSTRBN(,QHDRPTR)                            0126
         N     INDEX,@CF01963                                      0126
*   LRBNSAV=QHDRPTR->LASTRBN;       /* SAVE PTR TO LAST RBN AS THIS
*                                      FIELD IS OVERLAID DURING    0127
*                                      REQUEUING PFTE        @Z30WPYC*/
         LH    LRBNSAV,LASTRBN(,QHDRPTR)                           0127
         N     LRBNSAV,@CF01963                                    0127
*                                                                  0128
*   /*****************************************************************/
*   /*                                                               */
*   /* SCAN THE ENTIRE FRAME QUEUE LOOKING FOR FRAMES THAT ARE       */
*   /* ELIGIBLE TO BE STOLEN. BEFORE BEGINNING THE SCAN, ENSURE THAT */
*   /* FQPTR HAS NOT CHANGED ACROSS A PREVIOUS CALL TO FREEPAGE.     */
*   /* FQPTR COULD HAVE CHANGED IF THE FREEPAGE CALL CAUSED AN       */
*   /* EXPANSION OF THE PCB POOL WHICH IN TURN CAUSED AN ENTRY INTPO */
*   /* IEAVSQA. IEAVSQA COULD THEN HAVE TAKEN THE FRAME REFERRED TO  */
*   /* BY FQPTR AND PLACED IT ON THE SQA FRAME QUEUE. PFTEPTR, THE   */
*   /* VARIABLE THAT HOLDS THE ADDRESS OF THE CURRENT PFTE, IS       */
*   /* INITIALLY SET TO POINT TO THE ORIGIN OF THE PAGE FRAME TABLE  */
*   /* SO THAT WHEN THE SCAN OF ONE QUEUE IS COMPLETED, IT CAN BE    */
*   /* DETERMINED IF THE QUEUE WAS EMPTY OR SCANNED COMPLETELY, OR   */
*   /* SCANNING WAS TERMINATED BEFORE THE END OF THE QUEUE.          */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0128
*   PFTEPTR=PVTPFTP;                /* INIT PFTEPTR TO TABLE ORIGIN
*                                                            @ZA17349*/
         L     PFTEPTR,PVTPFTP(,PVTPTR)                            0128
*   DO WHILE INDEX^=0;              /* LOOP UNTIL QUEUE IS EMPTY     */
         B     @DE00129                                            0129
@DL00129 DS    0H                                                  0130
*     PFTEPTR=PVTPFTP+INDEX;        /* CALCULATE THE PFTE ADDRESS    */
         LR    PFTEPTR,INDEX                                       0130
         AL    PFTEPTR,PVTPFTP(,PVTPTR)                            0130
*     IF PFTQNDX=PFTSQAN THEN       /* PFTE ON SQA FRAME Q   @ZA15747*/
         CLI   PFTQNDX(PFTEPTR),X'0C'                              0131
         BE    @RT00131                                            0131
*       GOTO CHKSRB;                /* YES, END FRAME Q SCAN @ZA15747*/
*     FQPTR=PFTFQPTR;               /* SAVE FORWARD QUEUE POINTER    */
         LH    FQPTR,PFTFQPTR(,PFTEPTR)                            0133
         N     FQPTR,@CF01963                                      0133
*     IF(PFTSTEAL='1'B×PFTFXCT^=0×PFTPCBSI='1'B×PFTBADPG='1'B×PFTVRALC=
*         '1'B) THEN                /* DETERMINE THAT IT IS NOT      */
         TM    PFTSTEAL(PFTEPTR),B'00001000'                       0134
         BO    @RT00134                                            0134
         LH    @11,PFTFXCT(,PFTEPTR)                               0134
         LTR   @11,@11                                             0134
         BNZ   @RT00134                                            0134
         TM    PFTPCBSI(PFTEPTR),B'00001110'                       0134
         BZ    @RF00134                                            0134
@RT00134 DS    0H                                                  0135
*       DO;                         /*                       @Z40FPYL*/
*         PFTUIC=0;                 /*                       @Z40FPYL*/
         MVI   PFTUIC(PFTEPTR),X'00'                               0136
*         GOTO ADDTOQ;                                             0137
         B     ADDTOQ                                              0137
*       END;                        /* ALREADY MARKED AS A STEAL   0138
*                                      CANDIDATE, HAS NO FIX COUNT,
*                                      AND NO PCB DEFINED, BAD PAGE,
*                                      OR V=R ALLOCATION FLAGS SET   */
*     ELSE                                                         0139
*       DO;                         /* STEALABLE SO CHECK CRITERIA   */
@RF00134 DS    0H                                                  0140
*         RESPECIFY                                                0140
*          (RSA) RESTRICTED;        /*                       @Z30WPYC*/
*         RSA=INDEX*256;            /* SETUP TO CHECK REFERENCE BITS.
*                                      REFERENCED PAGES CANNOT BE  0141
*                                      STOLEN.                       */
         LR    RSA,INDEX                                           0141
         SLA   RSA,8                                               0141
*         IF PFTOFINT='0'B&PFTVRINT='0'B THEN/* IS PFTE INTERCEPTED.
*                                                            @Z30WPYC*/
         TM    PFTOFINT(PFTEPTR),B'01000001'                       0142
         BNZ   @RF00142                                            0142
*           DO;                     /* NO - NORMAL PROCESSING      0143
*                                                            @Z30WPYC*/
*             CHNGFLAG='0'B;        /* RESET CHANGE FLAG             */
         NI    CHNGFLAG(PVTPTR),B'10111111'                        0144
*             GEN CODE REFS(RSA,PFTEPTR) SETS(PFTUIC);             0145
        RRB    0(RSA)            RESET REFERENCE BIT FOR 1ST 2K
        BC     3,RESETA          REFERENCE BIT ON           @Z30WPYC
        BC     8,NXT2K           PAGE IS UNCHANGED          @Z30WPYC
        OI     CHNGFLAG(PVTPTR),X'40' INDICATE PAGE CHNGED  @Z30WPYC
NXT2K   RRB    2048(RSA)         RESET REF-CHNG FOR 2ND 2K  @Z30WPYC
        BC     3,RESETB          REFERENCE BIT ON           @Z30WPYC
        BC     8,CHKENDQ         PAGE IS UNCHANGED          @Z30WPYC
        OI     CHNGFLAG(PVTPTR),X'40' INDICATE PGE CHNG     @Z30WPYC
        B      CHKENDQ           DETERMINE IF PFTE'S ARE TO BE
*                                RECHAINED                  @Z30WPYC
RESETA    RRB    2048(RSA)       RESET REF-CHNG FOR 2ND 2K  @Z30WPYC
RESETB    MVI    PFTUIC(PFTEPTR),X'00' ZERO THE UIC         @Z30WPYC
*ADDTOQ:                                                           0146
*             IF ENDQ=0 THEN        /* WAS REFERENCED Q STARTD     0146
*                                                            @Z30WPYC*/
ADDTOQ   LTR   ENDQ,ENDQ                                           0146
         BNZ   @RF00146                                            0146
*               STARTQ=INDEX;       /* NO- INIT PTR TO 1ST ENTRY   0147
*                                                            @Z30WPYC*/
         LR    STARTQ,INDEX                                        0147
*             ELSE                                                 0148
*               ;                   /* YES - CHAIN IS ALREADY STARTED
*                                                            @Z30WPYC*/
@RF00146 DS    0H                                                  0149
*             ENDQ=INDEX;           /* INIT PTR TO LAST ENTRY      0149
*                                                            @Z30WPYC*/
         LR    ENDQ,INDEX                                          0149
*             GOTO CHKLAST;         /* DETERMINE IF THIS IS THE LAST
*                                      ENTRY FOR THIS FRAME QUEUE. 0150
*                                                            @Z30WPYC*/
         B     CHKLAST                                             0150
*           END;                    /* END NOT INTERCEPT TEST      0151
*                                                            @Z30WPYC*/
*CHKENDQ:                                                          0152
*                                                                  0152
*         /***********************************************************/
*         /*                                                         */
*         /* DETERMINE IF A REFERENCED CHAIN WAS CONSTRUCTED FOR THIS*/
*         /* FRAME QUEUE. THE REFERENCED CHAIN CONSISTS OF THOSE     */
*         /* PFTE'S DESCRIBING STORAGE THAT HAS BEEN REFERENCED SINCE*/
*         /* THE LAST CALL TO THIS ROUTINE.                  @Z30WPYC*/
*         /*                                                         */
*         /***********************************************************/
*                                                                  0152
*         IF ENDQ^=0 THEN           /* WAS A REFERENCED PFTE CHAIN 0152
*                                      CONSTRUCTED           @Z30WPYC*/
@RF00142 DS    0H                                                  0152
CHKENDQ  LTR   ENDQ,ENDQ                                           0152
         BZ    @RF00152                                            0152
*           DO;                     /* YES - CHAIN TO END OF FRAME Q
*                                                            @Z30WPYC*/
*             RFY                                                  0154
*              (R15) RESTRICTED;                                   0154
*             GEN REFS(PVTPFTP,PFTBQPTR,PFTFQPTR,STARTQ,ENDQ,INDEX,
*                 BACKMASK,FIRSTRBN,LASTRBN,QHDRPTR) SETS(PTRPFT,BACKQ,
*                 WORKREG);                                        0155
        L     PTRPFT,PVTPFTP(,PVTPTR)     GET ORIGIN OF PFT @Z30WPYC
        LH    BACKQ,PFTBQPTR(STARTQ,PTRPFT) GET ADDR OF PFTE BEFORE
*                                THE FIRST TO BE REQUED;
*                                MAY BE ZERO.               @Z30WPYC
        STH   BACKQ,PFTBQPTR(,PFTEPTR)     CURRENT PFTE POINTS BACK
*                                TO PREVIOUS UNREF.         @Z30WPYC
        SR    WORKREG,WORKREG    CLEAR WORK REG             @Z30WPYC
        STH   WORKREG,PFTFQPTR(ENDQ,PTRPFT) LAST ON REQUE HAS
*                                A ZERO FORWARD PTR         @Z30WPYC
        CR    WORKREG,BACKQ      IF BACKQ IS 0, THEN THE REQUE
*                                CHAIN IS THE FIRST ON THE
*                                FRAME QUEUE.               @Z30WPYC
        BE    QFIRST             REQUEUING 1ST ON FRAME Q; GO DO
*                                SPECIAL PROCESSING         @Z30WPYC
        N     BACKQ,BACKMASK     CLEAR HIGH ORDER 2 BYTES   @ZM34507
        STH   INDEX,PFTFQPTR(BACKQ,PTRPFT) NOT REQUEUING 1ST ON Q;
*                                HAVE PREV. UNREF POINT TO
*                                CURRENT.                   @Z30WPYC
        B     QLAST              GO SETUP PTR TO NEW LAST PFTE ON
*                                THE QUEUE.                 @Z30WPYC
QFIRST  EQU   *
        STH   INDEX,FIRSTRBN(QHDRPTR) SINCE ARE REQUEUING 1ST ON Q,
*                                CURRENT PFTE IS NEW 1ST    @Z30WPYC
QLAST   EQU   *
        ICM   WORKREG,3,LASTRBN(QHDRPTR) ARE REQUEUING TO END SO GET
*                                PTR TO CURRENT END; REGISTER IS
*                                ALREADY ZERO.              @Z30WPYC
        STH   WORKREG,PFTBQPTR(STARTQ,PTRPFT) START OF REQ POINTS
*                                BACK TO CURRENT END OF Q   @Z30WPYC
        STH   STARTQ,PFTFQPTR(WORKREG,PTRPFT) CURRENT END POINTS
*                                FOREWARD TO START OF REQ   @Z30WPYC
        STH   ENDQ,LASTRBN(QHDRPTR) END OF REQUEUE IS NEW
*                                END OF FRAME QUEUE.        @Z30WPYC
        L     WORKREG,CVTPTR
        L     PVTPTR,CVTPVTP(WORKREG)
*             RFY                                                  0156
*              (R15) UNRESTRICTED;                                 0156
*           END;                    /* END OF REQUEUING      @Z30WPYC*/
*         ELSE                                                     0158
*           ;                       /* NO - REQUEUING NOT NECESSARY
*                                                            @Z30WPYC*/
@RF00152 DS    0H                                                  0159
*         IF PFTOFINT='0'B&PFTVRINT='0'B THEN/* PFTE INTERCEPTED   0159
*                                                            @Z30WPYC*/
         TM    PFTOFINT(PFTEPTR),B'01000001'                       0159
         BNZ   @RF00159                                            0159
*           DO;                     /* NO - CONT. PROCESSING @Z30WPYC*/
*             IF UPDATUIC='1'B THEN /* SHOULD UIC BE UPDATED @Z40FPYL*/
         TM    UPDATUIC(PARMPTR),B'01000000'                       0161
         BNO   @RF00161                                            0161
*               PFTUIC=PFTUIC+1;    /* YES - INCREMENT BY 1  @Z30WPYC*/
         LA    @15,1                                               0162
         SLR   @14,@14                                             0162
         IC    @14,PFTUIC(,PFTEPTR)                                0162
         ALR   @15,@14                                             0162
         STC   @15,PFTUIC(,PFTEPTR)                                0162
*             ELSE                                                 0163
*               ;                   /* NO- DONT INCREMENT UIC      0163
*                                                            @Z30WPYC*/
@RF00161 DS    0H                                                  0164
*/********************************************************************/
*/*                                                                  */
*/*      THE FOLLOWING CODE SAVES THE HIGHEST UIC EITHER IN THE      */
*/*      ADDRESS SPACE OR IN THE COMMON AREA AFTER THE STEALABLE     */
*/*      FRAMES HAVE BEEN SELECTED. THE ASSUMPTION IS MADE THAT      */
*/*      THE PFTE Q'S ARE QUEUED IN DESCENDING ORDER OF UICS.  SO,   */
*/*      WHEN THE UIC IN THE PFTE IS NOT GREATER THAN OR EQUAL TO    */
*/*      THE CRITERIA NUMBER PASSED BY SRM, THE SCAN OF THE PFTES    */
*/*      CAN BE STOPPED.                                    @Z40FPYL */
*/*                                                                  */
*/*      FRAMES CAN BE STOLEN WHEN THE UIC IN THE PFTE IS GREATER    */
*/*      THAN OR EQUAL TO THE CRITERIA NUMBER.              @Z40FPYL */
*/*                                                                  */
*/********************************************************************/
*                                                                  0164
*             IF PFTUIC<CRITNUM THEN/* UIC< CRITNUM CAN STOP @Z40FPYL*/
         CLC   PFTUIC(1,PFTEPTR),CRITNUM(PARMPTR)                  0164
         BNL   @RF00164                                            0164
*               DO;                 /* SCANNING PFTES.       @Z40FPYL*/
*                 IF HIGHUIC='0'B THEN/* HAS HIGHEST UIC BEEN      0166
*                                                            @Z40FPYL*/
         TM    HIGHUIC(PVTPTR),B'00010000'                         0166
         BNZ   @RF00166                                            0166
*                   DO;             /* SAVED? IF NO THEN     @Z40FPYL*/
*                     IF ASCBADR=0 THEN/* SAVE HIGHEST UIC FOR     0168
*                                                            @Z40FPYL*/
         L     @15,ASCBADR-1(,PARMPTR)                             0168
         LA    @15,0(,@15)                                         0168
         LTR   @15,@15                                             0168
         BNZ   @RF00168                                            0168
*                       PVTCHUIC=PFTUIC;/* C.A. IN PVT, OR FOR     0169
*                                                            @Z40FPYL*/
         SLR   @15,@15                                             0169
         IC    @15,PFTUIC(,PFTEPTR)                                0169
         STH   @15,PVTCHUIC(,PVTPTR)                               0169
*                     ELSE          /* PRIVATE AREA IN OUXB. @Z40FPYL*/
*                       OUXBUIC=PFTUIC;/*                    @Z40FPYL*/
         B     @RC00168                                            0170
@RF00168 L     @15,ASCBPTR(,PARMPTR)                               0170
         L     @15,OUXBPTR(,@15)                                   0170
         SLR   @14,@14                                             0170
         IC    @14,PFTUIC(,PFTEPTR)                                0170
         STH   @14,OUXBUIC(,@15)                                   0170
*                     HIGHUIC='1'B; /* SET UIC INDICATOR ON  @Z40FPYL*/
@RC00168 OI    HIGHUIC(PVTPTR),B'00010000'                         0171
*                   END;            /*                       @Z40FPYL*/
*                 ELSE                                             0173
*                   ;               /* HIGH UIC WAS SAVED    @Z40FPYL*/
@RF00166 DS    0H                                                  0174
*                 IF UPDATUIC='0'B THEN/* WAS SRM ENTRY TO UPDATE  0174
*                                      UICS?                         */
         TM    UPDATUIC(PARMPTR),B'01000000'                       0174
         BNZ   @RF00174                                            0174
*                   DO;             /* NO, CAN STOP STEALING AS    0175
*                                      REMAINDER OF PFTUICS SHOULD BE
*                                      LESS THAN CRITERIA NUMBER.  0175
*                                                            @Z40FPYL*/
*                     ENDQ=0;       /* RESET REF Q END PTR TO ZERO   */
         SLR   ENDQ,ENDQ                                           0176
*                     GOTO CHKSRB;  /* TERMINATE THE SCAN    @Z40FPYL*/
         B     CHKSRB                                              0177
*                   END;            /*                       @Z40FPYL*/
*                 ELSE              /*                       @Z40FPYL*/
*                   GOTO RSETENDQ;  /*                       @Z40FPYL*/
*               END;                /*                       @Z40FPYL*/
*             ELSE                                                 0181
*               ;                   /* UIC >= CRITERIA NUMBER      0181
*                                                            @Z40FPYL*/
@RF00164 DS    0H                                                  0182
*             IF UNCHNGD='1'B THEN  /* STEAL UNCHANGED ONLY  @Z40FPYL*/
         TM    UNCHNGD(PARMPTR),B'10000000'                        0182
         BNO   @RF00182                                            0182
*               DO;                 /*                       @Z40FPYL*/
*                 IF CHNGFLAG='1'B THEN/* YES- FRAME CHANGED @Z30WPYC*/
         TM    CHNGFLAG(PVTPTR),B'01000000'                        0184
         BNO   @RF00184                                            0184
*                   DO;             /* CHECK HIGH UIC        @Z40FPYL*/
*                     IF HIGHUIC='0'B THEN/* HAS HIGHEST UIC @Z40FPYL*/
         TM    HIGHUIC(PVTPTR),B'00010000'                         0186
         BNZ   @RF00186                                            0186
*                       DO;         /* BEEN STORED? NO, SO   @Z40FPYL*/
*                         IF ASCBADR=0 THEN/* SAVE HIGHEST UIC FOR 0188
*                                                            @Z40FPYL*/
         L     @15,ASCBADR-1(,PARMPTR)                             0188
         LA    @15,0(,@15)                                         0188
         LTR   @15,@15                                             0188
         BNZ   @RF00188                                            0188
*                           PVTCHUIC=PFTUIC;/* C.A. IN PVT, OR     0189
*                                                            @Z40FPYL*/
         SLR   @15,@15                                             0189
         IC    @15,PFTUIC(,PFTEPTR)                                0189
         STH   @15,PVTCHUIC(,PVTPTR)                               0189
*                         ELSE      /* FOR PRIVATE AREA      @Z40FPYL*/
*                           OUXBUIC=PFTUIC;/* IN OUXB        @Z40FPYL*/
         B     @RC00188                                            0190
@RF00188 L     @15,ASCBPTR(,PARMPTR)                               0190
         L     @15,OUXBPTR(,@15)                                   0190
         SLR   @14,@14                                             0190
         IC    @14,PFTUIC(,PFTEPTR)                                0190
         STH   @14,OUXBUIC(,@15)                                   0190
*                         HIGHUIC='1'B;/* SET INDICATOR ON   @Z40FPYL*/
@RC00188 OI    HIGHUIC(PVTPTR),B'00010000'                         0191
*                       END;        /*                       @Z40FPYL*/
*                     ELSE                                         0193
*                       ;           /*                       @Z40FPYL*/
@RF00186 DS    0H                                                  0194
