         AL    @12,PVTCAOUT(,PVTPTR)                               0445
         ST    @12,PVTCAOUT(,PVTPTR)                               0445
*         END;                      /* END COMMON AREA CHAINING      */
*       ELSE                        /* PRIVATE AREA CHAIN            */
*         DO;                                                      0447
