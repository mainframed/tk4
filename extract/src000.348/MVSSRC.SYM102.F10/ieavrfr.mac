         B     @RC00436                                            0447
@RF00436 DS    0H                                                  0448
*           /*********************************************************/
*           /*                                                       */
*           /* CHAIN THE PCB TO THE INTERNAL LIOQ AND UPDATE THE PAGE*/
*           /* OUT COUNTS FOR PRIVATE AREA PAGES                     */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0448
*           IF LIOQUEUE^=0 THEN     /* IS ANCHOR NOW ZERO            */
         L     @12,LIOQUEUE(,PVTPTR)                               0448
         LTR   @12,@12                                             0448
         BZ    @RF00448                                            0448
*             DO;                   /* NO - ADJUST POINTERS          */
*               PCBFQPA=LIOQUEUE;   /* FORWARD CHAIN CURRENT TO LAST */
         STCM  @12,7,PCBFQPA(PCBPTR)                               0450
*               LIOQUEUE->PCBBQPA=PCBPTR;/* BACKWARD CHAIN LAST PCB
*                                      TO THE ONE JUST CREATED       */
         ST    PCBPTR,PCBBQPA(,@12)                                0451
*             END;                                                 0452
*           LIOQUEUE=PCBPTR;        /* MAKE CURRENT PCB THE NEW    0453
*                                      ANCHOR                        */
@RF00448 ST    PCBPTR,LIOQUEUE(,PVTPTR)                            0453
*           IF XPTVIO='1'B THEN     /* IF FRAME IS IN VIO WINDOW   0454
*                                                            @Z40WPYC*/
         TM    XPTVIO(XPTEPTR),B'00001000'                         0454
         BNO   @RF00454                                            0454
*             DO;                   /* UPDATE VIO COUNTS             */
*               PVTVAMO=PVTVAMO+1;  /* NUMBER OF VIO PAGE OUTS       */
         LA    @12,1                                               0456
         L     @11,PVTVAMO(,PVTPTR)                                0456
         ALR   @11,@12                                             0456
         ST    @11,PVTVAMO(,PVTPTR)                                0456
*               OUXBVAMO=OUXBVAMO+1;/* NUMBER OF P.A. VIO PAGE OUTS  */
         L     @11,PSAAOLD                                         0457
         L     @11,OUXBPTR(,@11)                                   0457
         AL    @12,OUXBVAMO(,@11)                                  0457
         ST    @12,OUXBVAMO(,@11)                                  0457
*             END;                                                 0458
*           ELSE                    /* NOT A VIO FRAME               */
*             DO;                   /* UPDATE LOCAL PAGE OUT COUNTS  */
         B     @RC00454                                            0459
@RF00454 DS    0H                                                  0460
*               PVTNPOUT=PVTNPOUT+1;/* NUMBER PAGES PAGED OUT        */
         LA    @12,1                                               0460
         L     @11,PVTNPOUT(,PVTPTR)                               0460
         ALR   @11,@12                                             0460
         ST    @11,PVTNPOUT(,PVTPTR)                               0460
*               OUXBPOUT=OUXBPOUT+1;/* NUMBER P.A. PAGES PAGED OUT   */
         L     @11,PSAAOLD                                         0461
         L     @11,OUXBPTR(,@11)                                   0461
         AL    @12,OUXBPOUT(,@11)                                  0461
         ST    @12,OUXBPOUT(,@11)                                  0461
*             END;                                                 0462
*         END;                      /* END PRIVATE AREA CHAINING     */
@RC00454 DS    0H                                                  0464
*       RFY                                                        0464
*         XPTEPTR UNRSTD;                                          0464
@RC00436 DS    0H                                                  0465
*       IF PFTVRINT='1'B×PFTOFINT='1'B THEN/* IF FRAME IS MARKED   0465
*                                      INTERCEPTED,                  */
         TM    PFTVRINT(PFTEPTR),B'01000001'                       0465
         BZ    @RF00465                                            0465
*         DO;                       /*                       @ZA03848*/
*           PCBNOREC='1'B;          /* TELL GFA TO DEFER ANY PAGEINS
*                                      FOR THIS VBN.         @Z40WPYC*/
         OI    PCBNOREC(PCBPTR),B'00000100'                        0467
*         END;                      /*                       @ZA03848*/
*     END;                          /* END OF PCB INITIALIZATION     */
*   ELSE                            /* NO PCBS AVAILABLE FOR PAGE  0470
*                                      OUTS                          */
*     DO;                                                          0470
*                                                                  0470
         B     @RC00396                                            0470
@RF00396 DS    0H                                                  0471
*       /*************************************************************/
*       /*                                                           */
*       /* BACK OUT PROCESSING - REVALIDATE THE PAGE AND TURN OFF ALL*/
*       /* STEAL INDICATORS                                          */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0471
*REVAL:                             /* REVALIDATE THE PGTE           */
*       PGTPVM='0'B;                /* ZERO THE INVALID BIT          */
REVAL    NI    PGTPVM(PTEPTR),B'11110111'                          0471
*NOSTEAL:                           /* TO MAKE THE FRAME APPEAR TO BE
*                                      A NORMAL LFQ FRAME, ZERO THE
*                                      UNREFERENCED INTERVAL COUNT 0472
*                                      AND TURN OFF THE PENDING STEAL
*                                      BIT                           */
*       PFTSTEAL='0'B;              /* TURN OFF STEAL BIT            */
NOSTEAL  NI    PFTSTEAL(PFTEPTR),B'11110111'                       0472
*       PFTUIC=0;                   /* SET UIC COUNT TO ZERO         */
         MVI   PFTUIC(PFTEPTR),X'00'                               0473
*       R0=0;                       /* CLEAR REG             @Z30WPYC*/
         SLR   R0,R0                                               0474
*       R0=PFTQNDX;                 /* INDICATE THE QUEUE THIS FRAME
*                                      IS ON.                @Z30WPYC*/
         SLR   R0,R0                                               0475
         IC    R0,PFTQNDX(,PFTEPTR)                                0475
*       SLL(R0,16);                 /* SET TO CORRECT POSITION     0476
*                                                            @Z30WPYC*/
         SLL   R0,16                                               0476
*       R0=(R0×INDEX);              /* SET THE RBN           @Z30WPYC*/
         OR    R0,INDEX                                            0477
*       R1=PSAAOLD;                 /* SET ASCB ADDR         @Z30WPYC*/
         L     R1,PSAAOLD                                          0478
*       CALL IEAVPFTE;              /* ENQUEUE THE FRAME TO THE END
*                                      OF THE FRAME ITS ON NOW     0479
*                                                            @Z30WPYC*/
         L     @15,PVTPFTE(,PVTPTR)                                0479
         BALR  @14,@15                                             0479
*     END;                          /* END BACKOUT PROCESSING        */
*   RESPECIFY                                                      0481
*    (INDEX,                                                       0481
*     PTEPTR) UNRESTRICTED;         /*                       @Z30WPYC*/
*                                                                  0481
@RC00396 DS    0H                                                  0482
*   /*****************************************************************/
*   /*                                                               */
*   /* RESTORE REGISTERS 1 TO 14 AND EXIT FROM FREEPAGE              */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0482
*EXIT:                                                             0482
*   GEN CODE EXIT NOSEQFLOW REFS(PVTPTR,FREESAVE);                 0482
EXIT     DS    0H                                                  0482
         LM    R1,R14,FREESAVE(PVTPTR)      RESTORE INPUT REGISTERS
         BR    R14                          RETURN TO CALLER
*                                                                  0483
*   /*****************************************************************/
*   /*                                                               */
*   /* RFRASM - CALL ASM SUBROUTINE                                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0483
*/*                                                                  */
*                                                                  0483
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS INTERNAL SUBROUTINE IS CALLED BY EITHER SELECT OR STEAL. */
*   /* ITS FUNCTION IS TO ENQUEUE THE PCBS TO THE SYSTEM IO QUEUES   */
*   /* AND CALL ASM TO START THE PAGE OUT IO.                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0483
*RFRASM:                                                           0483
*                                                                  0483
*/********************************************************************/
*/*    INPUT:                                                        */
*/*          R9= BASE REGISTER                                       */
*/*         R10= SAVEAREA ADDR IF FROM SELECT-IF STEAL UNDETERMINED  */
*/*         R14= RETURN ADDRESS                                      */
*/*    OUTPUT:                                                       */
*/*          R9, R10 AND R14 WILL BE THE SAME AS INPUT               */
*/********************************************************************/
*                                                                  0483
*   RFY                                                            0483
*    (R14,                                                         0483
*     R9,                                                          0483
*     R10) RESTRICTED;                                             0483
RFRASM   DS    0H                                                  0484
*   RETURNAD=R14;                   /* SAVE THE CALLERS RETURN ADDR
*                                      IN PVTWSA15                   */
         ST    R14,RETURNAD(,PVTPTR)                               0484
*   RFY                                                            0485
*    (R14) UNRESTRICTED;                                           0485
*                                                                  0485
*   /*****************************************************************/
*   /*                                                               */
*   /* ENQUEUE THE PCBS                                              */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0486
*   RSMHDPTR=PSAAOLD->ASCBRSM;      /* SET RSMHD PTR FOR ASM @Z40WPYC*/
         L     @12,PSAAOLD                                         0486
         L     RSMHDPTR,ASCBRSM(,@12)                              0486
*   IF CIOQUEUE^=0 THEN             /* CHECK INTERNAL CIOQ FOR PCBS  */
         L     @12,CIOQUEUE(,PVTPTR)                               0487
         LTR   @12,@12                                             0487
         BZ    @RF00487                                            0487
*     DO;                           /* AND ENQ ANY THAT EXIST TO THE
*                                      SYSTEM CIOQ                   */
*       R0=PCBCIOQN;                /* SET THE CIOQ QUEUE NUMBER     */
         LA    R0,32                                               0489
*       R1=CIOQUEUE;                /* POINT TO ANCHOR OF INTERNAL 0490
*                                      CHAIN                         */
         LR    R1,@12                                              0490
*       CALL IEAVPCB;               /* CALL PCB MANAGER TO ENQ CHAIN */
         L     @15,PVTPCB(,PVTPTR)                                 0491
         BALR  @14,@15                                             0491
*     END;                                                         0492
*   IF LIOQUEUE^=0 THEN             /* CHECK INTERNAL LIOQ ALSO      */
@RF00487 L     @12,LIOQUEUE(,PVTPTR)                               0493
         LTR   @12,@12                                             0493
         BZ    @RF00493                                            0493
*     DO;                           /* AND ENQ ANY THE EXIST THERE   */
*       R0=PCBLIOQN;                /* SET QUEUE NUMBER FOR LIOQ     */
         LA    R0,136                                              0495
*       R1=LIOQUEUE;                /* POINT TO LIOQ ANCHOR          */
         LR    R1,@12                                              0496
*       CALL IEAVPCB;               /* CALL PCB MANAGER TO ENQ THEM  */
         L     @15,PVTPCB(,PVTPTR)                                 0497
         BALR  @14,@15                                             0497
*     END;                                                         0498
*                                                                  0499
*   /*****************************************************************/
*   /*                                                               */
*   /* ISSUE THE ILRCALL MACRO TO START THE PAGE OUT IO              */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0499
*ASMCALL:                           /* CALL ASM              @Z40WPYC*/
*   R1=AIAQUEUE;                    /* FIRST AIA FOR ASM     @Z40WPYC*/
@RF00493 DS    0H                                                  0499
ASMCALL  L     R1,AIAQUEUE(,PVTPTR)                                0499
*   CALL ILRPAGIO;                  /* CALL ILRPAGIO TO INITIATE THE
*                                      START OF THE OUTPUT I/O.    0500
*                                                            @Z40WPYC*/
*                                                                  0500
         L     @15,PVTPAGIO(,PVTPTR)                               0500
         BALR  @14,@15                                             0500
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK THE RETURN CODE FROM ASM AND EXIT IF SUCCESSFUL         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0501
*   RFY                                                            0501
*    (R1,                                                          0501
*     R15) RSTD;                                                   0501
*   IF R15=0 THEN                   /* IF CALL WAS SUCCESSFUL        */
         LTR   R15,R15                                             0502
         BNZ   @RF00502                                            0502
*ASMEXIT:                           /* EXIT                          */
*     DO;                                                          0503
ASMEXIT  DS    0H                                                  0504
*       RFY                                                        0504
*        (R14) RESTRICTED;                                         0504
*       R14=RETURNAD;               /* RESTORE REGISTER 14           */
         L     R14,RETURNAD(,PVTPTR)                               0505
*       GOTO R14;                   /* RETURN TO CALLER              */
         BR    R14                                                 0506
*       RFY                                                        0507
*        (R14) UNRESTRICTED;                                       0507
*     END;                                                         0508
*                                                                  0509
*   /*****************************************************************/
*   /*                                                               */
*   /* IF THE ILRCALL WAS NOT SUCCESSFUL, RECORD THE ERROR VIA THE   */
*   /* FRR ROUTINE, RESET ALL STEAL AND IO INDICATORS IN THE         */
*   /* ASSOCIATED PFTE, REVALIDATE THE PAGE, FREE THE FAILING PCB,   */
*   /* AND RESTART THE IO WITH THE NEXT AIA. ASM RETURNS THE ADDRESS */
*   /* OF THE FAILING AIA IN REGISTER 1.                             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0509
*   PCBPTR=R1-LENGTH(PCB)+LENGTH(PCBAIA);/* GET THE ADDRESS OF THE 0509
*                                      PCB WHICH THE IO FAILED FOR BY
*                                      ADJUSTING ITS AIA ADDRESS     */
@RF00502 LR    PCBPTR,R1                                           0509
         AL    PCBPTR,@CF02016                                     0509
*   IF CVTNIP='0'B THEN             /* IF NIP IS IN CONTROL          */
         L     @14,CVTPTR                                          0510
         TM    CVTNIP(@14),B'00010000'                             0510
         BNZ   @RF00510                                            0510
*     DO;                           /* DO NOT ISSUE ABEND            */
*       PVTPRCA->RCARCRD='04'X;     /* INDICATE ILRCALL ERROR        */
         L     @14,PVTPRCA(,PVTPTR)                                0512
         MVI   RCARCRD(@14),X'04'                                  0512
*       GEN CODE SETS(R1);                                         0513
        ABEND X'C0D',,,SYSTEM         INVOKE THE FRR ROUTINE
*     END;                                                         0514
*   RFY                                                            0515
*     R15 UNRSTD;                                                  0515
*                                                                  0515
@RF00510 DS    0H                                                  0516
*   /*****************************************************************/
*   /*                                                               */
*   /* RESET STEAL AND PCB DEFINED INDICATORS IN THE PFTE            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0516
*   PFTEPTR=PVTPFTP+PCBRBN;         /* GET ADDRESS OF THE FRAMES PFTE*/
         LH    PFTEPTR,PCBRBN(,PCBPTR)                             0516
         N     PFTEPTR,@CF01963                                    0516
         AL    PFTEPTR,PVTPFTP(,PVTPTR)                            0516
*   PFTPCBSI='0'B;                  /* TURN OFF THE PCB DEFINED BIT  */
         NI    PFTPCBSI(PFTEPTR),B'11110111'                       0517
*   PFTSTEAL='0'B;                  /* TURN OFF THE PENDING STEAL BIT*/
         NI    PFTSTEAL(PFTEPTR),B'11110111'                       0518
*                                                                  0519
*   /*****************************************************************/
*   /*                                                               */
*   /* RESET VALIDITY FLAGS IN THE PAGE TABLES OF THE FAILING PAGE   */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0519
*   RFY                                                            0519
*    (PTEPTR,                                                      0519
*     XPTEPTR) RSTD;                                               0519
*   PTEPTR=PCBPGTA;                 /* GET THE ADDRESS OF THE PGTE   */
         L     PTEPTR,PCBPGTA-1(,PCBPTR)                           0520
         LA    PTEPTR,0(,PTEPTR)                                   0520
*   XPTEPTR=PCBXPTA;                /* GET THE ADDRESS OF THE XPTE   */
         L     XPTEPTR,PCBXPTA-1(,PCBPTR)                          0521
         LA    XPTEPTR,0(,XPTEPTR)                                 0521
*   PGTPVM='0'B;                    /* REVALIDATE THE PGTE           */
         NI    PGTPVM(PTEPTR),B'11110111'                          0522
*   XPTXAV='0'B;                    /* ZERO THE AUX STORAGE VALID  0523
*                                      FLAG                          */
*                                                                  0523
         NI    XPTXAV(XPTEPTR),B'10111111'                         0523
*   /*****************************************************************/
*   /*                                                               */
*   /* FREE THE FAILING PCB AND RESTART THE IO IF NEEDED             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0524
*   AIAQUEUE=AIANXAIA;              /* ADJUST THE AIA ANCHOR TO    0524
*                                      RESTART THE IO                */
         L     @12,AIANXAIA(,PCBPTR)                               0524
         ST    @12,AIAQUEUE(,PVTPTR)                               0524
*   R0=PCBFREQN;                    /* SET UP TO FREE THE FAILING PCB*/
         LA    R0,16                                               0525
*   R1=PCBPTR;                      /* PASS ITS ADDRESS TO THE PCB 0526
*                                      MANAGER                       */
         LR    R1,PCBPTR                                           0526
*   CALL IEAVPCB;                   /* FREE THE PCB                  */
         L     @15,PVTPCB(,PVTPTR)                                 0527
         BALR  @14,@15                                             0527
*   IF AIAQUEUE^=0 THEN             /* DETERMINE IF THE IO MUST BE 0528
*                                      RESTARTED                     */
         L     @12,AIAQUEUE(,PVTPTR)                               0528
         LTR   @12,@12                                             0528
         BNZ   @RT00528                                            0528
*     GOTO ASMCALL;                 /* AND TRY AGAIN IF REQUIRED     */
*   ELSE                            /* EXIT IF FAILURE OCCURRED      */
*     GOTO ASMEXIT;                 /* ON THE LAST PCB               */
         B     ASMEXIT                                             0530
*   END IEAVRFR                                                    0531
