*                                      RB/SRB                        */
         L     @15,PVTPRSET(,PVTPTR)                               0377
         BALR  @14,@15                                             0377
*       RMWAPTR=RMPLRMWA;           /* RESTORE THE RMWA ADDRESS      */
         L     RMWAPTR,RMPLRMWA(,RMPLPTR)                          0378
*       RESPECIFY                                                  0379
*        (R4,                                                      0379
*         R6,                                                      0379
*         R13) UNRESTRICTED;                                       0379
*       PCBRESET='0'B;              /* TURN OFF THE RESET FLAG       */
         NI    PCBRESET(PCBPTR),B'11101111'                        0380
*     END;                                                         0381
*   RESPECIFY                                                      0382
*    (R14) RESTRICTED;                                             0382
@RF00363 DS    0H                                                  0383
*   R14=RMWALVL2;                   /* GET THE RETURN ADDRESS        */
         L     R14,RMWALVL2(,RMWAPTR)                              0383
*   RESPECIFY                                                      0384
*    (RELPCB,                                                      0384
*     PCBPTR,                                                      0384
*     PRIPCB,                                                      0384
*     R14) UNRESTRICTED;                                           0384
*   END RSETSUBR;                                                  0385
@EL00004 DS    0H                                                  0385
@EF00004 DS    0H                                                  0385
@ER00004 BR    @14                                                 0385
*                                                                  0386
*/********************************************************************/
*/*                                                                  */
*/* THE ROOT PURGE INTERNAL SUBROUTINE IS CALLED TO PURGE ROOT PCBS  */
*/* ASSOCIATED WITH PCBS BEING PURGED. IF THE ROOT IS FOR A SWAP-IN, */
*/* THE SWAP FLAGS IN THE PCB ARE TURNED OFF; OTHERWISE, THE         */
*/* INTERCPET FLAG IN THE ROOT PCB IS TURNED ON. IF THE ROOT         */
*/* IS FOR A FIX, THE FOLLOWING IS DONE: IF THE PURGE TYPE           */
*/* IS TCB, THE FOE PURGE SUBROUTINE IS CALLED TO GET RID OF         */
*/* THE FOE. FOR ALL PURGE TYPES WHICH ARE NOT DEFERRED (A           */
*/* FRAME EXISTS,) THE PFTE FIX COUNT IS DECREMENTED BY THE FIX      */
*/* COUNT IN THE PCB. IF THE PFTE FIX COUNT GOES TO ZERO, THE        */
*/* DEFERRED RELEASE ROUTINE IS CALLED IF NECESSARY. FOR ALL FIX     */
*/* PCBS, THE FIX COUNT IN THE PCB IS ZEROED. FOR ALL ROOT PCBS,     */
*/* THE ROOT COMPLETION COUNT IS DECREMENTED BY ONE.                 */
*/* IF THE ROOT COMPLETION COUNT IS NOW ZERO, THEN ALL PCBS FOR THE  */
*/* ROOT HAVE BEEN PROCESSED OR PURGED. IF THE PURGE TYPE IS ASCB AND*/
*/* THE ECB IS IN THE PRIVATE AREA, THE ROOT PCB MUST BE FREED (IS   */
*/* BECAUSE NO ROUTINE IN THE ADDRESS SPACE WILL EVER RUN AGAIN.) THE*/
*/* ROOT PCB IS SENT TO THE FREE QUEUE BY THE PCB MANAGER. ON RETURN,*/
*/* CONTROL RETURNS TO THE CALLER. IF THE ABOVE CRITERIA ARE NOT MET,*/
*/* THEN THE ROOT EXIT ROUTINE IS CALLED TO PROCESS THE ROOT PCB.    */
*/* ON RETURN FROM THE ROOT EXIT ROUTINE, CONTROL RETURNS TO THE     */
*/* CALLER.                                                          */
*/* IF THE ROOT COMPLETION COUNT IS NOT ZERO, CONTROL RETURNS TO THE */
*/* CALLER.                                                          */
*/*                                                                  */
*/********************************************************************/
*                                                                  0386
*ROOTPURG:                                                         0386
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0386
ROOTPURG DS    0H                                                  0387
*   RESPECIFY                                                      0387
*    (PCBRPTR,                                                     0387
*     RELPCB,                                                      0387
*     PCBPTR,                                                      0387
*     PRIPCB,                                                      0387
*     R14) RESTRICTED;                                             0387
*   RMWALVL3=R14;                   /* SAVE THE RETURN ADDRESS       */
         ST    R14,RMWALVL3(,RMWAPTR)                              0388
*   RESPECIFY                                                      0389
*    (R14) UNRESTRICTED;                                           0389
*   PCBRPTR=PCBRTPA;                /* GET THE ROOT PCB ADDRESS      */
         L     PCBRPTR,PCBRTPA-1(,PCBPTR)                          0390
         LA    PCBRPTR,0(,PCBRPTR)                                 0390
*   PCBROOT='0'B;                   /* TURN OFF THE PCB ROOT FLAG    */
         NI    PCBROOT(PCBPTR),B'11111011'                         0391
*   PCBRTPA=PCBRTCBA;               /* MOVE THE TCB ADDRESS FROM THE
*                                      ROOT PCB TO THE PCB           */
         MVC   PCBRTPA(3,PCBPTR),PCBRTCBA(PCBRPTR)                 0392
*   IF SPCTSWIN='1'B&SPCTSWRT=PCBRPTR THEN/* TEST IF THE ROOT IS FOR
*                                      A SWAP IN.                    */
         L     @15,RSMSPCT(,RSMHDPTR)                              0393
         TM    SPCTSWIN(@15),B'10000000'                           0393
         BNO   @RF00393                                            0393
         C     PCBRPTR,SPCTSWRT(,@15)                              0393
         BNE   @RF00393                                            0393
*     DO;                           /* IS A SWAP IN PCB, SO TURN OFF
*                                      THE PCB SWAP FLAGS.           */
*       PCBSWPIN='0'B;              /* TURN OFF THE SWAP IN FLAG.    */
*       PCBSWPS1='0'B;              /* TURN OFF THE STAGE ONE FLAG.  */
*       PCBSWPLS='0'B;              /* TURN OFF THE LSQA FLAG.       */
*       PCBDFRLS='0'B;              /* TURN OFF THE DEFERRED RELEASE
*                                      FLAG.                         */
         NI    PCBSWPIN(PCBPTR),B'10000111'                        0398
*     END;                                                         0399
*   ELSE                            /* ROOT IS NOT A SWAP IN ROOT.   */
*     PCBRINT='1'B;                 /* TURN ON THE ROOT INTERCEPT  0400
*                                      FLAG                          */
         B     @RC00393                                            0400
@RF00393 OI    PCBRINT(PCBRPTR),B'10000000'                        0400
*   IF PCBFXC^=0 THEN               /* TEST FOR ANY FIXES TO BE    0401
*                                      PURGED                        */
@RC00393 CLI   PCBFXC(PCBPTR),0                                    0401
         BE    @RF00401                                            0401
*     DO;                           /* ARE FIXES, SO PROCESS THEM.   */
*       IF RMPLTERM='0'B THEN       /* TEST FOR TCB PURGE            */
         TM    RMPLTERM(RMPLPTR),B'01000000'                       0403
         BNZ   @RF00403                                            0403
*         CALL FOEPURG;             /* IS TCB PURGE, SO PURGE FOES 0404
*                                      FOR THIS TCB. ON ASCB PURGE,
*                                      FOES ARE FREED AUTOMATICALLY.
*                                      VTERM IS ALSO IN THE WRONG  0404
*                                      ADDRESS SPACE TO PURGE FOES ON
*                                      ASCB PURGE                    */
         BAL   @14,FOEPURG                                         0404
*       IF RMWAGFAD='0'B THEN       /* TEST FOR NOT PROCESSING THE 0405
*                                      GFA DEFER QUEUE. THE PCBS ON
*                                      THE GFA QUEUE HAVE NO PFTES 0405
*                                      ASSIGNED AND DO NOT NEED PFTE
*                                      PROCESSING                    */
@RF00403 TM    RMWAGFAD(RMWAPTR),B'10000000'                       0405
         BNZ   @RF00405                                            0405
*         DO;                       /* PROCESS THE PFTE.             */
*           RESPECIFY                                              0407
*            (PFTEPTR) RESTRICTED;                                 0407
*           PFTEPTR=PCBRBN+PVTPFTP; /* GET THE ADDRESS OF THE PFTE 0408
*                                      ALLOCATED TO THIS PCB         */
         LH    PFTEPTR,PCBRBN(,PCBPTR)                             0408
         N     PFTEPTR,@CF02318                                    0408
         AL    PFTEPTR,PVTPFTP(,PVTPTR)                            0408
*           PFTFXCT=PFTFXCT-PCBFXC; /* DECREMENT NUMBER OF FIXES ON
*                                      THE FRAME BY THE NUMBER OF  0409
*                                      JUST-PURGED FIXES             */
         LH    @07,PFTFXCT(,PFTEPTR)                               0409
         SLR   @15,@15                                             0409
         IC    @15,PCBFXC(,PCBPTR)                                 0409
         SLR   @07,@15                                             0409
         STH   @07,PFTFXCT(,PFTEPTR)                               0409
*           IF PFTFXCT=0 THEN       /* TEST IF ALL FIXES ON THIS   0410
*                                      FRAME ARE GONE                */
         LTR   @07,@07                                             0410
         BNZ   @RF00410                                            0410
*             DO;                   /*                       @YA01572*/
*               IF PFTQNDX<PFTLFQN THEN/* TEST TO SEE IF     @YA01572*/
         CLI   PFTQNDX(PFTEPTR),X'80'                              0412
         BNL   @RF00412                                            0412
*                 DO;               /* PFTE IS ON LOCAL      @YA01572*/
*                   PVTCNTFX=PVTCNTFX-1;/* OR GLOBAL QUEUE   @YA01572*/
         LH    @07,PVTCNTFX(,PVTPTR)                               0414
         BCTR  @07,0                                               0414
         STH   @07,PVTCNTFX(,PVTPTR)                               0414
*                   PVTCOMFX=PVTCOMFX-1;/* MUST BE A GLOBAL  @YA01572*/
         LH    @07,PVTCOMFX(,PVTPTR)                               0415
         BCTR  @07,0                                               0415
         STH   @07,PVTCOMFX(,PVTPTR)                               0415
*                 END;              /* QUEUE                 @YA01572*/
*               ELSE                /* MUST BE A LOCAL       @YA01572*/
*                 DO;               /* QUEUE                 @YA01572*/
         B     @RC00412                                            0417
@RF00412 DS    0H                                                  0418
*                   RSMCNTFX=RSMCNTFX-1;/*                   @YA01572*/
         LH    @07,RSMCNTFX(,RSMHDPTR)                             0418
         BCTR  @07,0                                               0418
         STH   @07,RSMCNTFX(,RSMHDPTR)                             0418
*                   PVTCNTFX=PVTCNTFX-1;/*                   @YA01572*/
         LH    @07,PVTCNTFX(,PVTPTR)                               0419
         BCTR  @07,0                                               0419
         STH   @07,PVTCNTFX(,PVTPTR)                               0419
*                 END;              /*                       @YA01572
*                                      ALL FIXES GONE DO DEFERRED  0420
*                                      RELEASE PROC IF NECESSARY     */
*               PFTLNGFX='0'B;      /* TURN OFF THE LONG FIX FLAG    */
@RC00412 NI    PFTLNGFX(PFTEPTR),B'11101111'                       0421
*               IF PFTDFRLS='1'B THEN/* TEST IF DEFERRED RELEASE   0422
*                                      PROCESSING MUST BE DONE       */
         TM    PFTDFRLS(PFTEPTR),B'10000000'                       0422
         BNO   @RF00422                                            0422
*                 DO;               /* DEF. RELS. MUST BE DONE FOR 0423
*                                      THIS FRAME. NEED NOT BE DONE
*                                      FOR ASCB PURGE WITH PRIVATE 0423
*                                      AREA VBN                      */
*                                                                  0423
*                   /*************************************************/
*                   /*                                               */
*                   /* TEST FOR ASCB PURGE AND PRIVATE AREA VBN. IF  */
*                   /* NOT, THEN MUST DO DEFERRED RELEASE.           */
*                   /*                                               */
*                   /*************************************************/
*                                                                  0424
*                   IF^(RMPLTERM='1'B&PCBVBN<PVTLCSA) THEN         0424
*                                                                  0424
         TM    RMPLTERM(RMPLPTR),B'01000000'                       0424
         BNO   @RT00424                                            0424
         CLC   PCBVBN(2,PCBPTR),PVTLCSA(PVTPTR)                    0424
         BL    @RF00424                                            0424
@RT00424 DS    0H                                                  0425
*                     /***********************************************/
*                     /*                                             */
*                     /* TESTS TRUE                                  */
*                     /*                                             */
*                     /***********************************************/
*                                                                  0425
*                     DO;           /* IS NOT PRIV. VBN. MUST DO   0425
*                                      DEFERRED RELEASE PROCESSING   */
*                       RESPECIFY                                  0426
*                        (R0,                                      0426
*                         R1,                                      0426
*                         R15) RESTRICTED;                         0426
*                       R1=PCBVBN*256;/* GET THE VSA TO BE RELEASED  */
         LH    R1,PCBVBN(,PCBPTR)                                  0427
         N     R1,@CF02318                                         0427
         SLA   R1,8                                                0427
*/********************************************************************/
*/*                                                                  */
*/*                   DEFERRED RELEASE                               */
*/*                 INPUT:                                           */
*/*            R0: UNUSED                                            */
*/*            R1: VIRTUAL ADDRESS TO BE RELEASED                    */
*/*            R2: ADDRESS OF RSMHD                                  */
*/*            R3: ADDRESS OF PVT                                    */
*/*            R4 - R13: UNUSED                                      */
*/*            R14: RETURN ADDRESS LOADED BY BALR                    */
*/*            R15: ENTRY POINT ADDRESS LOADED BY BALR               */
*/*                 OUTPUT:                                          */
*/*            R0: ZERO                                              */
*/*            R1 - R14: RESTORED                                    */
*/*            R15: RETURN CODE                                      */
*/*                   0 - SUCCESSFUL                                 */
*/*                   NON-0 - UNSUCCESSFUL                           */
*/*                                                                  */
*/********************************************************************/
*                                                                  0428
*                       CALL IEAVRELF;/* CALL DEFERRED RELEASE       */
         L     @15,PVTPRELF(,PVTPTR)                               0428
         BALR  @14,@15                                             0428
*                       RCACSECT=RCACPTRM;/* RESET MODULE ID         */
         L     @14,PVTPRCA(,PVTPTR)                                0429
         MVI   RCACSECT(@14),X'11'                                 0429
*                       IF R15^=0 THEN/* TEST FOR A BAD RETURN CODE  */
         LTR   R15,R15                                             0430
         BZ    @RF00430                                            0430
*                         DO;       /* RECORD THE ERROR              */
*                           RCARCRD='0F'X;/* SET ERROR REASON CODE   */
         MVI   RCARCRD(@14),X'0F'                                  0432
*                           GENERATE CODE SETS(R1);                0433
         ABEND X'C0D',,,SYSTEM      RECORD THE ERROR
*                         END;                                     0434
*                       RESPECIFY                                  0435
*                        (R0,                                      0435
*                         R1,                                      0435
*                         R15,                                     0435
*                         PFTEPTR) UNRESTRICTED;                   0435
@RF00430 DS    0H                                                  0436
*                     END;                                         0436
*                 END;                                             0437
@RF00424 DS    0H                                                  0438
*             END;                                                 0438
@RF00422 DS    0H                                                  0439
*         END;                                                     0439
@RF00410 DS    0H                                                  0440
*       PCBFXC=0;                   /* ZERO THE FIX COUNT IN CASE THE
*                                      PCB REMAINS ON A QUEUE        */
@RF00405 MVI   PCBFXC(PCBPTR),X'00'                                0440
*     END;                                                         0441
*   PCBRCNT=PCBRCNT-1;              /* DECREMENT ROOT COMPLETION   0442
*                                      COUNT                         */
@RF00401 LH    @15,PCBRCNT(,PCBRPTR)                               0442
         BCTR  @15,0                                               0442
         STH   @15,PCBRCNT(,PCBRPTR)                               0442
*   IF PCBRCNT=0 THEN               /* TEST FOR ZERO COMPLETION COUNT
*                                      MEANS ALL PCBS FOR THE ROOT 0443
*                                      HAVE BEEN PROCESSED           */
         LTR   @15,@15                                             0443
         BNZ   @RF00443                                            0443
*     DO;                           /* COMPLETION COUNT IS ZERO. GET
*                                      RID OF THE ROOT PCB           */
*       IF RMPLTERM='1'B&SPCTSWIN='0'B THEN/* TEST FOR ASCB PURGE AND
*                                      SWAP IN NOT IN PROGRESS. IF 0445
*                                      ASCB IS BEING PURGED AND THE
*                                      ECB IS IN THE PRIVATE AREA, 0445
*                                      THE ROOT PCB MUST BE FREED, 0445
*                                      OTHERWISE IT WOULD NEVER BE 0445
*                                      PURGED. FOR COMMON AREA ECB,
*                                      SWAP IN, OR TCB PURGE, THE  0445
*                                      ROOT EXIT ROUTINE TAKES CARE
*                                      OF THE ROOT PCB.              */
         TM    RMPLTERM(RMPLPTR),B'01000000'                       0445
         BNO   @RF00445                                            0445
         L     @01,RSMSPCT(,RSMHDPTR)                              0445
         TM    SPCTSWIN(@01),B'10000000'                           0445
         BNZ   @RF00445                                            0445
*         DO;                       /* IS ASCB PURGE. CHECK FOR    0446
*                                      COMMON AREA ECB               */
*           RESPECIFY                                              0447
*            (VSAPTR) RESTRICTED;                                  0447
*           VSAPTR=PCBRECB;         /* GET THE VSA OF THE ECB        */
         L     VSAPTR,PCBRECB(,PCBRPTR)                            0448
*           VSAPTR=VSAPTR/4096;     /* SHIFT RIGHT 12                */
         SRL   VSAPTR,12                                           0449
*           VSAPTR=VSAPTR*16;       /* SHIFT LEFT 4 TO MAKE THE VBN  */
         SLL   VSAPTR,4                                            0450
*           IF VSAPTR<PVTLCSA THEN  /* TEST FOR PRIVATE AREA VBN ( 0451
*                                      ECB IN P.A.)                  */
         LH    @06,PVTLCSA(,PVTPTR)                                0451
         N     @06,@CF02318                                        0451
         CR    VSAPTR,@06                                          0451
         BNL   @RF00451                                            0451
*             DO;                   /* IS PRIVATE AREA ECB AND ASCB
*                                      PURGE, SO FREE THE ROOT PCB   */
*               RESPECIFY                                          0453
*                (VSAPTR) UNRESTRICTED;                            0453
*               PCBRPTR->PCBFQP='FF000000'X;/* SET ROOT PCB TO     0454
*                                      UNQUEUED STATE WITH ZERO    0454
*                                      FOREWARD POINTER              */
         MVC   PCBFQP(4,PCBRPTR),@CF02280                          0454
*               RESPECIFY                                          0455
*                (R0,                                              0455
*                 R1) RESTRICTED;                                  0455
*               R0=PCBFREQN;        /* SET TQN TO THE FREE QUEUE TQN */
         LA    R0,16                                               0456
*               R1=PCBRPTR;         /* SET POINTER TO THE ROOT PCB   */
         LR    R1,PCBRPTR                                          0457
*/********************************************************************/
*/*                                                                  */
*/*                   PCB MANAGER                                    */
*/*                 INPUT:                                           */
*/*            R0: TQN (FOR THE FREE QUEUE)                          */
*/*            R1: ADDRESS OF THE PCB                                */
*/*            R2: ADDRESS OF RSMHD                                  */
*/*            R3: ADDRESS OF PVT                                    */
*/*            R4 - R13: UNUSED                                      */
*/*            R14: RETURN ADDRESS LOADED BY BALR                    */
*/*            R15: ENTRY POINT ADDRESS LOADED BY BALR               */
*/*                 OUTPUT:                                          */
*/*            R0 - R15: RESTORED                                    */
*/*                                                                  */
*/********************************************************************/
*                                                                  0458
*               CALL IEAVPCB;       /* CALL PCB MANAGER TO FREE THE
*                                      ROOT PCB                      */
         L     @15,PVTPCB(,PVTPTR)                                 0458
         BALR  @14,@15                                             0458
*               GOTO EXITROOT;      /* GO TO THE EXIT ROUTINE        */
         B     EXITROOT                                            0459
*             END;                                                 0460
*         END;                                                     0461
*                                                                  0461
@RF00451 DS    0H                                                  0462
*       /*************************************************************/
*       /*                                                           */
*       /* MUST BE TCB PURGE, ASCB PURGE WITH COMMON AREA ECB, OR    */
*       /* ASCB PURGE WITH SWAP IN ONGOING TO BE HERE                */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0462
*       R1=PCBRPTR;                 /* GET THE ROOT PCB ADDRESS FOR
*                                      THE ROOT EXIT ROUTINE         */
@RF00445 LR    R1,PCBRPTR                                          0462
*/********************************************************************/
*/*                                                                  */
*/*                     ROOT EXIT ROUTINE                            */
*/*                   INPUT:                                         */
*/*              R0: UNUSED                                          */
*/*              R1: ADDRESS OF THE ROOT PCB                         */
*/*              R2: ADDRESS OF RSMHD                                */
*/*              R3: ADDRESS OF PVT                                  */
*/*              R4 - R13: UNUSED                                    */
*/*              R14: RETURN ADDRESS LOADED BY BALR                  */
*/*              R15: ENTRY POINT ADDRESS LOADED BY BALR             */
*/*                   OUTPUT:                                        */
*/*              R0 - R15: RESTORED                                  */
*/*                                                                  */
*/********************************************************************/
*                                                                  0463
*       CALL ROOTEXIT;              /* CALL THE ROOT EXIT ROUTINE    */
         L     @15,PCBRGOTO(,PCBRPTR)                              0463
         BALR  @14,@15                                             0463
*       RCACSECT=RCACPTRM;          /* RESET THE RCA CSECT ID        */
         L     @15,PVTPRCA(,PVTPTR)                                0464
         MVI   RCACSECT(@15),X'11'                                 0464
*     END;                                                         0465
*                                                                  0465
*   /*****************************************************************/
*   /*                                                               */
*   /* EXIT POINT FOR ROOTPURG SUBROUTINE                            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0466
*EXITROOT:                                                         0466
*   RESPECIFY                                                      0466
*    (R14) RESTRICTED;                                             0466
@RF00443 DS    0H                                                  0466
EXITROOT DS    0H                                                  0467
*   R14=RMWALVL3;                   /* GET THE RETURN ADDRESS        */
         L     R14,RMWALVL3(,RMWAPTR)                              0467
*   RESPECIFY                                                      0468
*    (R0,                                                          0468
*     R1,                                                          0468
*     PCBRPTR,                                                     0468
*     RELPCB,                                                      0468
*     PCBPTR,                                                      0468
*     PRIPCB,                                                      0468
*     R14) UNRESTRICTED;                                           0468
