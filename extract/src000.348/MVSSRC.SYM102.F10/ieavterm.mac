*         CALL FREEPCB;             /* CALL SUBROUTINE TO FREE THE 0293
*                                      PRESENT PCB                   */
         BAL   @14,FREEPCB                                         0293
*       END;                                                       0294
*     ELSE                          /* IS THE LAST RELATED PCB ON THE
*                                      CHAIN. SET UP FREE OF PCB     */
*       DO;                         /* SET UP POINTERS TO GET THE  0295
*                                      NEXT PCB TO BE PROCESSED      */
         B     @RC00290                                            0295
@RF00290 DS    0H                                                  0296
