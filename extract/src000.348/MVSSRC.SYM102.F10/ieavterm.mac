*/********************************************************************/
*/*                                                                  */
*/* THE FOE PURGE INTERNAL SUBROUTINE IS CALLED TO PURGE THE FOE     */
*/* ASSOCIATED WITH A FIX PCB WHICH IS BEING PURGED. THIS            */
*/* SUBROUTINE IS CALLED ONLY DURING TCB PURGE FOR PCBS WITH FIX     */
*/* ROOTS. THE FOE CHAIN OUT OF THE TCB ADDRESSED BY THE PCB IS      */
*/* SCANNED FOR THE FOE FOR THE PURGED FIX. IF THE FOE IS NOT        */
*/* FOUND, AN ERROR IS RECORDED AND CONTROL IS PASSED TO THE EXIT    */
*/* POINT. IF THE FOE IS FOUND, THE PCB FIX COUNT IS SUBTRACTED      */
*/* FROM THE FOE FIX COUNT. IF THE FOE FIX COUNT IS NOW ZERO, THE    */
*/* FOE IS DEQUEUED FROM TCBFOE AND INSERTED ON THE AVAILABLE QUEUE  */
*/* (RSMFOEQ). CONTROL RETURNS TO THE ROOT PURGE ROUTINE.            */
*/*                                                                  */
*/********************************************************************/
*                                                                  0470
*FOEPURG:                                                          0470
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0470
FOEPURG  DS    0H                                                  0471
*   RESPECIFY                                                      0471
*    (PCBRPTR,                                                     0471
*     FOEPTR,                                                      0471
*     RELPCB,                                                      0471
*     PCBPTR,                                                      0471
*     PRIPCB,                                                      0471
*     R7,                                                          0471
*     R14) RESTRICTED;                                             0471
*   RMWALVL4=R14;                   /* SAVE THE RETURN ADDRESS       */
         ST    R14,RMWALVL4(,RMWAPTR)                              0472
*   RESPECIFY                                                      0473
*    (R14) UNRESTRICTED;                                           0473
*   FOEPTR=TCBFOEA;                 /* GET THE FIRST FOE FOR THIS TCB*/
         L     @06,RMPLTCBA(,RMPLPTR)                              0474
         L     FOEPTR,TCBFOEA-1(,@06)                              0474
         LA    FOEPTR,0(,FOEPTR)                                   0474
*   R7=ADDR(TCBFOE);                /* SAVE THE BACK FOE POINTER     */
         LA    R7,TCBFOE(,@06)                                     0475
*TESTFOE:                                                          0476
*   IF FOEPTR=0 THEN                /* TEST FOR FOE NOT FOUND        */
TESTFOE  LTR   FOEPTR,FOEPTR                                       0476
         BNZ   @RF00476                                            0476
*     DO;                           /* NO FOE FOUND FOR THE INPUT  0477
*                                      VBN. IS AN ERROR. RECORD    0477
*                                      ERROR, THEN EXIT              */
*       RCARCRD='10'X;              /* SET ERROR REASON CODE         */
         L     @06,PVTPRCA(,PVTPTR)                                0478
         MVI   RCARCRD(@06),X'10'                                  0478
*       GENERATE CODE SETS(R1);                                    0479
         ABEND X'C0D',,,SYSTEM      RECORD THE ERROR
*       GOTO EXITFOE;               /* SKIP FOE PROCESSING           */
         B     EXITFOE                                             0480
*     END;                                                         0481
*   IF FOEVINDX^=PCBVBN THEN        /* TEST FOR CORRECT FOE BY VBN   */
@RF00476 CLC   FOEVINDX(2,FOEPTR),PCBVBN(PCBPTR)                   0482
         BE    @RF00482                                            0482
*     DO;                           /* IS NOT THE CORRECT FOE. GET 0483
*                                      NEXT FOE AND TEST FOR A MATCH */
*       R7=FOEPTR;                  /* SAVE THE POINTER TO THIS FOE  */
         LR    R7,FOEPTR                                           0484
*       FOEPTR=FOEFLINK;            /* GET THE ADDRESS OF THE NEXT 0485
*                                      FOE                           */
         L     @06,FOEFLINK-1(,FOEPTR)                             0485
         LA    @06,0(,@06)                                         0485
         LR    FOEPTR,@06                                          0485
*       GOTO TESTFOE;               /* GO TEST FOR CORRECT FOE       */
         B     TESTFOE                                             0486
*     END;                                                         0487
*                                                                  0487
*   /*****************************************************************/
*   /*                                                               */
*   /* IS CORRECT FOE IF PROCESSING COMES HERE                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0488
*   FOEFXCT=FOEFXCT-PCBFXC;         /* DECREMENT FOE FIX COUNT BY FIX
*                                      COUNT IN THE PCB. THE FIX HAS
*                                      BEEN PURGED AND THE FOE MUST
*                                      BE ADJUSTED                   */
@RF00482 LH    @06,FOEFXCT(,FOEPTR)                                0488
         SLR   @15,@15                                             0488
         IC    @15,PCBFXC(,PCBPTR)                                 0488
         SLR   @06,@15                                             0488
         STH   @06,FOEFXCT(,FOEPTR)                                0488
*   IF FOEFXCT=0 THEN               /* TEST IF ALL FIXES BY THIS TASK
*                                      HAVE GONE                     */
         LTR   @06,@06                                             0489
         BNZ   @RF00489                                            0489
*     DO;                           /* ALL FIXES GONE, SO RECHAIN FOE
*                                      TO AVAILABLE QUEUE    @Z30CNYC*/
*       R7->FOEFLINK=FOEFLINK;      /* SET PREVIOUS FOE TO POINT TO
*                                      THE FOE AFTER THE FOE TO BE 0491
*                                      DEQUEUED.                     */
         MVC   FOEFLINK(3,R7),FOEFLINK(FOEPTR)                     0491
*       FOEFLINK=RSMFOEQ;           /* AVAIL QUE TO THIS FOE @Z30CNYC*/
         MVC   FOEFLINK(3,FOEPTR),RSMFOEQ+1(RSMHDPTR)              0492
*       RSMFOEQ=FOEPTR;             /* CHAIN TO HEAD OF      @Z30CNYC
*                                      AVAILABLE QUEUE.      @Z30CNYC*/
         ST    FOEPTR,RSMFOEQ(,RSMHDPTR)                           0493
*     END;                                                         0494
*   RESPECIFY                                                      0495
*    (R14) RESTRICTED;                                             0495
@RF00489 DS    0H                                                  0496
*EXITFOE:                                                          0496
*   R14=RMWALVL4;                   /* GET THE RETURN ADDRESS        */
EXITFOE  L     R14,RMWALVL4(,RMWAPTR)                              0496
*   RESPECIFY                                                      0497
*    (PCBRPTR,                                                     0497
*     FOEPTR,                                                      0497
*     RELPCB,                                                      0497
*     PCBPTR,                                                      0497
*     PRIPCB,                                                      0497
*     R14,                                                         0497
*     R7) UNRESTRICTED;                                            0497
*   END FOEPURG;                                                   0498
@EL00006 DS    0H                                                  0498
@EF00006 DS    0H                                                  0498
@ER00006 BR    @14                                                 0498
*                                                                  0499
*/********************************************************************/
*/*                                                                  */
*/* THE PCB FREE INTERNAL SUBROUTINE FREES THE CURRENT PCB AND RESETS*/
*/* THE PCB QUEUES TO CONTINUE PROCESSING. IF THE PCB IS A RELATED   */
*/* PCB, THE RELATED POINTERS ARE ADJUSTED TO REMOVE THE PCB FROM    */
*/* THE RELATED CHAIN. THE PCB IS THEN FREED TO THE SYSTEM AND       */
*/* CONTROL RETURNS TO THE QUEUE SEARCH ROUTINE TO PROCESS THE NEXT  */
*/* PCB. IF THE PCB IS NOT A RELATED PCB, THEN IT IS TESTED FOR A    */
*/* RELATED PCB. IF THE PCB HAS NO RELATED PCBS, IT IS FREED AND     */
*/* CONTROL RETURNS TO THE QUEUE SEARCH ROUTINE. IF THERE ARE        */
*/* RELATED PCBS, THE WHOLE RELATED CHAIN IS ENQUEUED TO THE QUEUE   */
*/* WITH THE FIRST RELATED PCB AS THE NEW PRIMARY PCB. IF THE I/O IS */
*/* COMPLETE FOR THE PCB AND THE PCB IS ON THE COMMON I/O ACTIVE     */
*/* QUEUE, IEAVIOCP IS SCHEDULED FOR THE PCB. AFTER THE RELATED PCBS */
*/* ARE ENQUEUED, THE PRIMARY PCB IS FREED. CONTROL RETURNS TO THE   */
*/* QUEUE SEARCH MAINLINE.                                           */
*/*                                                                  */
*/********************************************************************/
*                                                                  0499
*FREEPCB:                                                          0499
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0499
FREEPCB  DS    0H                                                  0500
*   RESPECIFY                                                      0500
*    (R0,                                                          0500
*     R1,                                                          0500
*     BACKPCB,                                                     0500
*     RELPCB,                                                      0500
*     PCBPTR,                                                      0500
*     PRIPCB,                                                      0500
*     R14) RESTRICTED;                                             0500
*   RMWALVL2=R14;                   /* SAVE THE RETURN ADDRESS       */
         ST    R14,RMWALVL2(,RMWAPTR)                              0501
*   RESPECIFY                                                      0502
*    (R14) UNRESTRICTED;                                           0502
*   IF PCBCQN=PCBDEQN THEN          /* TEST IF THE PCB TO BE FREED IS
*                                      A RELATED PCB. (IS RELATED IF
*                                      CQN IS X'FF')                 */
         CLI   PCBCQN(PCBPTR),255                                  0503
         BNE   @RF00503                                            0503
*     RELPCB->PCBRLPA=PCBRLPA;      /* IS A RELATED PCB. SAVE      0504
*                                      POINTERS SO THAT THE REST OF
*                                      THE RELATED CHAIN CAN BE    0504
*                                      PROCESSED. SET THE PREVIOUS 0504
*                                      PCB TO POINT TO THE RELATED 0504
*                                      PCB AFTER THE INPUT PCB IS  0504
*                                      FREED                         */
         MVC   PCBRLPA(3,RELPCB),PCBRLPA(PCBPTR)                   0504
*   ELSE                            /* NOT A RELATED PCB. IF THE   0505
*                                      PRIMARY PCB HAS A RELATED PCB,
*                                      THE RELATED MUST BE ENQUEUED
*                                      AS A PRIMARY BEFORE THE FREE
*                                      OF THE INPUT PCB              */
*     IF PCBRLPA^=0 THEN            /* TEST FOR ANY RELATED PCBS     */
         B     @RC00503                                            0505
@RF00503 L     @15,PCBRLPA-1(,PCBPTR)                              0505
         LA    @15,0(,@15)                                         0505
         LTR   @15,@15                                             0505
         BZ    @RF00505                                            0505
*       DO;                         /* THERE IS A RELATED PCB. IT  0506
*                                      MUST BE ENQUEUED AS A PRIMARY
*                                      BEFORE THE INPUT PCB CAN BE 0506
*                                      FREED                         */
*         R1=PCBRLPA;               /* GET THE ADDRESS OF THE RELATED*/
         LR    R1,@15                                              0507
*         R1->PCBFQP='FF000000'X;   /* SET UP THE RELATED PCB AS AN
*                                      UNQUEUED PCB                  */
         MVC   PCBFQP(4,R1),@CF02280                               0508
*         IF PCBGFAD='1'B THEN      /* TEST FOR THE GFAD BIT ON IN 0509
*                                      THE PRIMARY PCB               */
         TM    PCBGFAD(PCBPTR),B'01000000'                         0509
         BNO   @RF00509                                            0509
*           R1->PCBGFAD='1'B;       /* GFAD IS ON IN THE PRIMARY, SO
*                                      TURN ON IN THE RELATED        */
         OI    PCBGFAD(R1),B'01000000'                             0510
*         R0=PCBCQN;                /* SET TQN SO THAT THE RELATED 0511
*                                      WILL BE ENQUEUED TO THE SAME
*                                      QUEUE AS THE PRIMARY          */
@RF00509 SLR   R0,R0                                               0511
         IC    R0,PCBCQN(,PCBPTR)                                  0511
*         PCBVBN=0;                 /* ZERO THE VBN IN THE PRIMARY SO
*                                      THAT TWO PCBS DO NOT HAVE THE
*                                      SAME VBN WHILE ON THE SAME  0512
*                                      QUEUE                         */
         SLR   @15,@15                                             0512
         STH   @15,PCBVBN(,PCBPTR)                                 0512
*/********************************************************************/
*/*                                                                  */
*/*                       PCB MANAGER                                */
*/*                     INPUT:                                       */
*/*                R0: TQN                                           */
*/*                R1: ADDRESS OF THE PCB                            */
*/*                R2: ADDRESS OF RSMHD                              */
*/*                R3: ADDRESS OF PVT                                */
*/*                R4 - R13: UNUSED                                  */
*/*                R14: RETURN ADDRESS LOADED BY BALR                */
*/*                R15: ENTRY POINT ADDRESS LOADED BY BALR           */
*/*                     OUTPUT:                                      */
*/*                R0 - R15: RESTORED                                */
*/*                                                                  */
*/********************************************************************/
*                                                                  0513
*         CALL IEAVPCB;             /* CALL PCB MANAGER TO ENQUEUE 0513
*                                      THE RELATED AS A PRIMARY      */
*                                                                  0513
         L     @15,PVTPCB(,PVTPTR)                                 0513
         BALR  @14,@15                                             0513
*         /***********************************************************/
*         /*                                                         */
*         /* TEST FOR I/O COMPLETE AND PROCESSING COMMON I/O Q AND   */
*         /* THE JUST-QUEUED PCB NOT FOR THE ADDRESS SPACE BEING     */
*         /* PURGED                                                  */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0514
*         IF(PCBIOCMP='1'B&RMWACIOQ='1'B&RMPLTERM='1'B&RMPLASCB^=R1->
*             PCBASCB) THEN                                        0514
*                                                                  0514
         TM    PCBIOCMP(PCBPTR),B'00000001'                        0514
         BNO   @RF00514                                            0514
         TM    RMWACIOQ(RMWAPTR),B'01000000'                       0514
         BNO   @RF00514                                            0514
         TM    RMPLTERM(RMPLPTR),B'01000000'                       0514
         BNO   @RF00514                                            0514
         CLC   RMPLASCB(4,RMPLPTR),PCBASCB(R1)                     0514
         BE    @RF00514                                            0514
*           /*********************************************************/
*           /*                                                       */
*           /* ABOVE TESTS TRUE                                      */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0515
*           CALL IEAVOPBR;          /* CALL ROUTINE TO SCHEDULE IOCP
*                                      FOR THE JUST-ENQUEUED PCB     */
         L     @15,PVTPOPBR(,PVTPTR)                               0515
         BALR  @14,@15                                             0515
*/********************************************************************/
*/*                                                                  */
*/*                     IOCP SCHEDULE BRANCH ENTRY (IN IEAVPIOP)     */
*/*                   INPUT:                                         */
*/*              R0: UNUSED                                          */
*/*              R1: ADDRESS OF THE PCB TO HAVE IOCP SCHEDULED       */
*/*              R2: UNUSED                                          */
*/*              R3: ADDRESS OF PVT                                  */
*/*              R4 - R13: UNUSED                                    */
*/*              R14: RETURN ADDRESS LOADED BY BALR                  */
*/*              R15: ENTRY POINT ADDRESS LOADED BY BALR             */
*/*                   OUTPUT:                                        */
*/*              R0 - R15: RESTORED                                  */
*/*                                                                  */
*/********************************************************************/
*                                                                  0516
*       END;                                                       0516
@RF00514 DS    0H                                                  0517
*   R1=PCBPTR;                      /* GET THE ADDRESS OF THE PRIMARY
*                                      PCB TO BE FREED               */
@RF00505 DS    0H                                                  0517
@RC00503 LR    R1,PCBPTR                                           0517
*   R0=PCBFREQN;                    /* SET TQN TO THE FREE QUEUE     */
         LA    R0,16                                               0518
*/********************************************************************/
*/*                                                                  */
*/*                 PCB MANAGER                                      */
*/*               INPUT:                                             */
*/*          R0: TQN (FOR THE FREE QUEUE)                            */
*/*          R1: ADDRESS OF THE PCB                                  */
*/*          R2: ADDRESS OF RSMHD                                    */
*/*          R3: ADDRESS OF PVT                                      */
*/*          R4 - R13: UNUSED                                        */
*/*          R14: RETURN ADDRESS LOADED BY BALR                      */
*/*          R15: ENTRY POINT ADDRESS LOADED BY BALR                 */
*/*               OUTPUT:                                            */
*/*          R0 - R15: RESTORED                                      */
*/*                                                                  */
*/********************************************************************/
*                                                                  0519
*   CALL IEAVPCB;                   /* CALL PCB MANAGER TO FREE THE
*                                      INPUT PCB                     */
         L     @15,PVTPCB(,PVTPTR)                                 0519
         BALR  @14,@15                                             0519
*   RESPECIFY                                                      0520
*    (R14) RESTRICTED;                                             0520
*   R14=RMWALVL2;                   /* GET THE RETURN ADDRESS        */
         L     R14,RMWALVL2(,RMWAPTR)                              0521
*   RESPECIFY                                                      0522
*    (R0,                                                          0522
*     R1,                                                          0522
*     BACKPCB,                                                     0522
*     RELPCB,                                                      0522
*     PCBPTR,                                                      0522
*     PRIPCB,                                                      0522
*     R14) UNRESTRICTED;                                           0522
*   END FREEPCB;                                                   0523
@EL00007 DS    0H                                                  0523
@EF00007 DS    0H                                                  0523
@ER00007 BR    @14                                                 0523
*                                                                  0524
*   /*****************************************************************/
*   /*                                                               */
*   /* THE FIX PURGE SUBROUTINE IS CALLED TO PURGE FIXES FOR A TCB   */
*   /* BEING TERMINATED. IT IS CALLED AFTER ALL PAGING I/O FOR THE   */
*   /* TCB HAS BEEN PURGED. IF FIX PURGES ARE NOT TO BE DONE, THIS   */
*   /* SUBROUTINE WILL NOT BE CALLED. THE FOE CHAIN IS OBTAINED FROM */
*   /* THE TCB BEING PURGED. FOR EACH FOE ON THE CHAIN, A TEST IS    */
*   /* MADE TO ENSURE THAT THE PAGE IS IN CORE. IF NOT IN CORE, THE  */
*   /* FIX COUNT IN THE FOE MUST BE ZERO. IF NOT ZERO, AN ERROR HAS  */
*   /* OCCURRED AND IS RECORDED BY THE FRR. IF THE PAGE IS NOT IN    */
*   /* CORE (ERROR OR NOT), THE FOE IS DEQUEUED FROM TCBFOE AND      */
*   /* INSERTED ON THE AVAILABLE QUEUE (RSMFOEQ), AND THE NEXT FOE   */
*   /* OBTAINED AND PROCESSED. IF THE PAGE IS IN CORE, THE PFTE IS   */
*   /* LOCATED AND A TEST IS MADE TO SEE IF DECREMENTING THE PFTE FIX*/
*   /* COUNT BY THE FOE FIX COUNT WILL CAUSE THE PFTE FIX COUNT TO GO*/
*   /* NEGATIVE. IF IT WILL, THE PFTE FIX COUNT IS NOT CHANGED, AN   */
*   /* ERROR IS RECORDED VIA THE FRR, AND THE FOE IS IS PUT ON THE   */
*   /* AVAILABLE QUEUE. OTHERWISE, THE PFTE FIX COUNT IS DECREMENTED */
*   /* BY THE FIX COUNT IN THE FOE. IF THE FRAME FIX COUNT IS NOW    */
*   /* ZERO, THE LONG FIX FLAG IS TURNED OFF. IN ADDITION, IF        */
*   /* DEFERRED RELEASE PROCESSING MUST BE DONE, THE DEFERRED RELEASE*/
*   /* ROUTINE IS CALLED TO PROCESS THE UNFIXED PAGE. ON RETURN FROM */
*   /* DEFERRED RELEASE, THE RETURN CODE IS CHECKED FOR SUCCESSFUL   */
*   /* COMPLETION. IF THE RETURN CODE IS NOT ZERO, AN ERROR IS       */
*   /* RECORDED BY THE FRR. WHETHER OR NOT THE FRAME COUNT IS REDUCED*/
*   /* TO ZERO, THE FOE IS DEQUEUED FROM THE TCB AND INSERTED ON THE */
*   /* AVAILABLE QUEUE (RSMFOEQ). THE NEXT FOE IS OBTAINED AND       */
*   /* PROCESSED. WHEN ALL FOES HAVE BEEN PURGED, CONTROL IS RETURNED*/
*   /* TO THE MAINLINE.                                              */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0524
*TERMPFIX:                                                         0524
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0524
TERMPFIX DS    0H                                                  0525
*   RESPECIFY                                                      0525
*    (VSAPTR,                                                      0525
*     FOEPTR,                                                      0525
*     PFTEPTR,                                                     0525
*     R14) RESTRICTED;                                             0525
*   RMWALVL1=R14;                   /* SAVE THE RETURN ADDRESS       */
         ST    R14,RMWALVL1(,RMWAPTR)                              0526
*   RESPECIFY                                                      0527
*    (R14) UNRESTRICTED;                                           0527
*   FOEPTR=TCBFOEA;                 /* GET THE ADDRESS OF THE FIRST
*                                      FOE FOR THE TCB BEING PURGED  */
         L     @11,RMPLTCBA(,RMPLPTR)                              0528
         L     FOEPTR,TCBFOEA-1(,@11)                              0528
         LA    FOEPTR,0(,FOEPTR)                                   0528
*   TCBFOEA=0;                      /* ZERO THE FOE POINTER.         */
         SLR   @12,@12                                             0529
         STCM  @12,7,TCBFOEA(@11)                                  0529
*FOETEST:                                                          0530
*   IF FOEPTR=0 THEN                /* TEST FOR END OF FOES TO BE  0530
*                                      DONE                          */
FOETEST  LTR   FOEPTR,FOEPTR                                       0530
         BZ    @RT00530                                            0530
*     GOTO EXITPFIX;                /* NO MORE FOES, SO EXIT         */
*   VSAPTR=FOEVINDX*256;            /* GET THE VSA OF THE FIXED AREA */
         LH    VSAPTR,FOEVINDX(,FOEPTR)                            0532
         N     VSAPTR,@CF02318                                     0532
         SLA   VSAPTR,8                                            0532
*   RESPECIFY                                                      0533
*    (RSAPTR) RESTRICTED;                                          0533
*   GENERATE CODE                   /* GET THE RBN TO REFERENCE PFTE */
*       REFS(VSAPTR) SETS(RSAPTR) FLOWS(MAKERBN);                  0534
         LRA   RSAPTR,0(0,VSAPTR)  GET THE RSA OF THE VSA
         BC    8,MAKERBN           IF GOOD RBN, GO PROCESS
*   IF FOEFXCT^=0 THEN              /* TEST FOR AN ERROR IN FOE. THE
*                                      FIX COUNT IN THE FOE MUST BE
*                                      ZERO OR AN ERROR HAS OCCURRED.*/
         LH    @12,FOEFXCT(,FOEPTR)                                0535
         LTR   @12,@12                                             0535
         BZ    @RF00535                                            0535
*     DO;                           /* RECORD THE ERROR              */
*       RCARCRD='01'X;              /* SET ERROR REASON CODE         */
         L     @12,PVTPRCA(,PVTPTR)                                0537
         MVI   RCARCRD(@12),X'01'                                  0537
*       GENERATE CODE SETS(R1);                                    0538
         ABEND X'C0D',,,SYSTEM      RECORD THE ERROR
*     END;                                                         0539
*   GOTO FREEFOE;                   /* GO FREE THE FOE               */
         B     FREEFOE                                             0540
*MAKERBN:                                                          0541
*                                                                  0541
*   /*****************************************************************/
*   /*                                                               */
*   /* THE PAGE IS IN STORAGE. IF THE FOE FIX COUNT IS GREATER THAN  */
*   /* THE PFTE FIX COUNT, THERE IS AN ERROR SO RECORD IT. IF NOT,   */
*   /* DECREMENT THE PFTE FIX COUNT BY THE FOE FIX COUNT. IN EITHER  */
*   /* CASE, FREE THE FOE.                                   @ZA26549*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0541
*   RSAPTR=RSAPTR/4096;             /* SHIFT RIGHT 12                */
MAKERBN  SRL   RSAPTR,12                                           0541
*   RSAPTR=RSAPTR*16;               /* SHIFT LEFT 4 TO MAKE A RBN    */
         SLL   RSAPTR,4                                            0542
*   PFTEPTR=PVTPFTP+RSAPTR;         /* GET THE ADDRESS OF THE PFTE 0543
*                                      FOR THE FIXED PAGE            */
         LR    PFTEPTR,RSAPTR                                      0543
         AL    PFTEPTR,PVTPFTP(,PVTPTR)                            0543
*   RESPECIFY                                                      0544
*    (RSAPTR) UNRESTRICTED;                                        0544
*   RFY                                                            0545
*    (R9) RSTD;                     /*                       @ZA26549*/
*   R9=PFTFXCT-FOEFXCT;             /* DIFFERENCE IN FIX CTS @ZA26549*/
         LH    R9,PFTFXCT(,PFTEPTR)                                0546
         SH    R9,FOEFXCT(,FOEPTR)                                 0546
*   IF R9<0 THEN                    /* DIFFERENCE NEGATIVE?  @ZA26549*/
         LTR   R9,R9                                               0547
         BNM   @RF00547                                            0547
*     DO;                           /* YES, RECORD AN ERROR  @ZA26549*/
*       RCARCRD='22'X;              /* SET ERROR REASON CODE @ZA26549*/
         L     @12,PVTPRCA(,PVTPTR)                                0549
         MVI   RCARCRD(@12),X'22'                                  0549
*       GEN SETS(R1)(ABEND X'C0D',,,SYSTEM);/*               @ZA26549*/
         ABEND X'C0D',,,SYSTEM
*       GOTO FREEFOE;               /* GO FREE THE FOE       @ZA26549*/
         B     FREEFOE                                             0551
*     END;                          /*                       @ZA26549*/
*   PFTFXCT=R9;                     /* REMOVE THE FIXES OF THIS FOE
*                                      FROM THE FRAME        @ZA26549*/
@RF00547 STH   R9,PFTFXCT(,PFTEPTR)                                0553
*   IF R9=0 THEN                    /* FRAME NOW FREED?      @ZA26549*/
         LTR   R9,R9                                               0554
         BNZ   @RF00554                                            0554
*     DO;                           /* YES                           */
*       RFY                                                        0556
*        (R9) UNRSTD;               /*                       @ZA26549*/
*       IF PFTQNDX<PFTLFQN THEN     /* TEST TO SEE IF        @YA01572*/
         CLI   PFTQNDX(PFTEPTR),X'80'                              0557
         BNL   @RF00557                                            0557
*         DO;                       /* PFTE IS ON LOCAL      @YA01572*/
*           PVTCNTFX=PVTCNTFX-1;    /* OR GLOBAL QUEUE       @YA01572*/
         LH    @12,PVTCNTFX(,PVTPTR)                               0559
         BCTR  @12,0                                               0559
         STH   @12,PVTCNTFX(,PVTPTR)                               0559
*           PVTCOMFX=PVTCOMFX-1;    /* MUST BE A GLOBAL      @YA01572*/
         LH    @12,PVTCOMFX(,PVTPTR)                               0560
         BCTR  @12,0                                               0560
         STH   @12,PVTCOMFX(,PVTPTR)                               0560
*         END;                      /* QUEUE                 @YA01572*/
*       ELSE                        /* MUST BE A LOCAL       @YA01572*/
*         DO;                       /* QUEUE                 @YA01572*/
         B     @RC00557                                            0562
@RF00557 DS    0H                                                  0563
*           RSMCNTFX=RSMCNTFX-1;    /*                       @YA01572*/
         LH    @12,RSMCNTFX(,RSMHDPTR)                             0563
         BCTR  @12,0                                               0563
         STH   @12,RSMCNTFX(,RSMHDPTR)                             0563
*           PVTCNTFX=PVTCNTFX-1;    /*                       @YA01572*/
         LH    @12,PVTCNTFX(,PVTPTR)                               0564
         BCTR  @12,0                                               0564
         STH   @12,PVTCNTFX(,PVTPTR)                               0564
*         END;                      /*                       @YA01572
*                                      FRAME IS FREED, SO CHECK FOR
*                                      ANY DEFERRED RELEASE        0565
*                                      PROCESSING                    */
*       PFTLNGFX='0'B;              /* TURN OFF LONG FIX IN UNFIXED
*                                      FRAME                         */
@RC00557 NI    PFTLNGFX(PFTEPTR),B'11101111'                       0566
*       IF PFTDFRLS='1'B THEN       /* TEST IF DEF RELS MUST BE DONE */
         TM    PFTDFRLS(PFTEPTR),B'10000000'                       0567
         BNO   @RF00567                                            0567
*         DO;                       /* DEF RELS MUST BE DONE         */
*           RESPECIFY                                              0569
*            (R0,                                                  0569
*             R1,                                                  0569
*             R15) RESTRICTED;                                     0569
*           R1=VSAPTR;              /* GET THE VSA TO BE DEF RELS    */
         LR    R1,VSAPTR                                           0570
*/********************************************************************/
*/*                                                                  */
*/*                         DEFERRED RELEASE                         */
*/*                       INPUT:                                     */
*/*                  R0: UNUSED                                      */
*/*                  R1: VIRTUAL ADDRESS TO BE RELEASED              */
*/*                  R2: ADDRESS OF RSMHD                            */
*/*                  R3: ADDRESS OF PVT                              */
*/*                  R4 - R13: UNUSED                                */
*/*                  R14: RETURN ADDRESS LOADED BY BALR              */
*/*                  R15: ENTRY POINT ADDRESS LOADED BY BALR         */
*/*                       OUTPUT:                                    */
*/*                  R0: ZERO                                        */
*/*                  R1 - R14: RESTORED                              */
*/*                  R15: RETURN CODE                                */
*/*                         0 - SUCCESSFUL                           */
*/*                         NON-0 - UNSUCCESSFUL                     */
*/*                                                                  */
*/********************************************************************/
*                                                                  0571
*           CALL IEAVRELF;          /* CALL DEFERRED RELEASE         */
         L     @15,PVTPRELF(,PVTPTR)                               0571
         BALR  @14,@15                                             0571
*           RCACSECT=RCACPTRM;      /* RESET THE RCA CSECT ID        */
         L     @14,PVTPRCA(,PVTPTR)                                0572
         MVI   RCACSECT(@14),X'11'                                 0572
*           IF R15^=0 THEN          /* TEST FOR A BAD RETURN CODE    */
         LTR   R15,R15                                             0573
         BZ    @RF00573                                            0573
*             DO;                   /* PROCESS THE ERROR             */
*               RCARCRD='0F'X;      /* SET ERROR REASON CODE         */
         MVI   RCARCRD(@14),X'0F'                                  0575
*               GENERATE CODE SETS(R1);                            0576
         ABEND X'C0D',,,SYSTEM      RECORD THE ERROR
*             END;                                                 0577
*         END;                                                     0578
@RF00573 DS    0H                                                  0579
*     END;                                                         0579
@RF00567 DS    0H                                                  0580
*FREEFOE:                                                          0580
*   RESPECIFY                                                      0580
*    (R14) RESTRICTED;              /*                       @Z30CNYC*/
@RF00554 DS    0H                                                  0580
FREEFOE  DS    0H                                                  0581
*   R14=FOEFLINK;                   /* SAVE PNTER TO NXT FOE @Z30CNYC*/
         L     R14,FOEFLINK-1(,FOEPTR)                             0581
         LA    R14,0(,R14)                                         0581
*   FOEFLINK=RSMFOEQ;               /* CHAIN ANY AVAILABLE   @Z30CNYC
*                                      FOE'S TO THIS FOE             */
         MVC   FOEFLINK(3,FOEPTR),RSMFOEQ+1(RSMHDPTR)              0582
*   RSMFOEQ=FOEPTR;                 /* CHAIN TO TOP OF       @Z30CNYC
*                                      AVAILABLE QUEUE               */
         ST    FOEPTR,RSMFOEQ(,RSMHDPTR)                           0583
*   FOEPTR=R14;                     /* GET NXT FOE POINTER   @Z30CNYC*/
         LR    FOEPTR,R14                                          0584
*   RESPECIFY                                                      0585
*    (R14) UNRESTRICTED;            /*                       @Z30CNYC*/
*   GOTO FOETEST;                   /* GO PROCESS THE NEXT FOE       */
         B     FOETEST                                             0586
*   RESPECIFY                                                      0587
*    (R0,                                                          0587
*     R1,                                                          0587
*     R4,                                                          0587
*     R15) UNRESTRICTED;                                           0587
*                                                                  0587
