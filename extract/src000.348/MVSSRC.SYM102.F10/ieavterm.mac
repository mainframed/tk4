@EL00002 DS    0H                                                  0312
@EF00002 DS    0H                                                  0312
@ER00002 BR    @14                                                 0312
*                                                                  0313
*/********************************************************************/
*/*                                                                  */
*/* THIS SUBROUTINE IS CALLED BY THE PAGING PURGE ROUTINE TO PROCESS */
*/* PCBS TO BE PURGED FOUND ON AN I/O ACTIVE QUEUE. EACH PCB FOUND IS*/
*/* PROCESSED, THEN CONTROL RETURNS TO THE PAGING PURGE MAINLINE TO  */
*/* CONTINUE THE QUEUE SCAN. IF THE PCB HAS A ROOT PCB, IT IS FIRST  */
*/* PROCESSED. IF THE ROOT PCB IS FOR OUTPUT, IT MUST BE A SWAP OUT  */
*/* ROOT, AND DOES NOT NEED TO BE PROCESSED. RETURN IS MADE TO THE   */
*/* MAINLINE. IF THE ROOT IS FOR INPUT, THE INTERNAL ROOT PURGE      */
*/* SUBROUTINE IS CALLED TO PROCESS AND GET RID OF THE ROOT PCB.     */
*/* IF THE PCB IS A RELATED PCB, IT IS MARKED TO BE FREED WITHOUT    */
*/* FURTHER PROCESSING AND RETURN IS MADE TO THE CALLING PROCESSOR.  */
*/* IF NOT A RELATED AND THE I/O IS COMPLETE, THE PFTE IS FREED IF   */
*/* POSSIBLE. RETURN IS MADE TO THE CALLER. IF THE I/O IS NOT        */
*/* COMPLETE, THE PCB IS MARKED TO FREE THE PFTE WHEN THE I/O DOES   */
*/* COMPLETE. ALSO, IF THE PCB IS ON THE LOCAL I/O QUEUE AND THE     */
*/* ADDRESS SPACE IS BEING PURGED, THE PCB IS ZEROED. THIS IS TO     */
*/* PREVENT RELATED PROCESSING WHILE THE PCB IS ON THE LOCAL I/O     */
*/* QUEUE OF ANOTHER ADDRESS SPACE.                                  */
*/*                                                                  */
*/********************************************************************/
*                                                                  0313
*TERMIO:                                                           0313
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0313
TERMIO   DS    0H                                                  0314
*   RESPECIFY                                                      0314
*    (RELPCB,                                                      0314
*     PCBPTR,                                                      0314
*     PRIPCB,                                                      0314
*     R14) RESTRICTED;                                             0314
*   RMWALVL2=R14;                   /* SAVE THE RETURN ADDRESS       */
         ST    R14,RMWALVL2(,RMWAPTR)                              0315
*   RESPECIFY                                                      0316
*    (R14) UNRESTRICTED;                                           0316
*   IF PCBROOT='1'B THEN            /* TEST IF A ROOT PCB EXISTS.    */
         TM    PCBROOT(PCBPTR),B'00000100'                         0317
         BNO   @RF00317                                            0317
*     IF PCBIOI='0'B THEN           /* A ROOT EXISTS. PROCESS      0318
*                                      ACCORDING TO INPUT OR OUTPUT. */
         TM    PCBIOI(PCBPTR),B'00000010'                          0318
         BNZ   @RF00318                                            0318
*       CALL ROOTPURG;              /* FOR INPUT ROOT, CALL ROOT   0319
*                                      HANDLER SUBROUTINE.           */
         BAL   @14,ROOTPURG                                        0319
*     ELSE                          /* OUTPUT ROOT CAN ONLY BE FOR A
*                                      SWAP OUT.                     */
*       GOTO LEAVEPCB;              /* ONLY WAY TO FIND A SWAP OUT 0320
*                                      PCB IS IF I/O IS NOT COMPLETE.
*                                      MUST BE LEFT ON THE I/O ACTIVE
*                                      QUEUE                         */
*   IF PCBCQN=PCBDEQN THEN          /* TEST FOR A QUEUED PCB.        */
@RF00317 CLI   PCBCQN(PCBPTR),255                                  0321
         BE    @RT00321                                            0321
*     GOTO FREEBIT;                 /* IS NOT A QUEUED PCB. ALL    0322
*                                      RELATED PCBS CAN BE FREED. GO
*                                      TURN ON FREE FLAG BIT.        */
*   IF PCBIOCMP='1'B THEN           /* IS A QUEUED PCB. TEST FOR I/O
*                                      COMPLETE.                     */
         TM    PCBIOCMP(PCBPTR),B'00000001'                        0323
         BNO   @RF00323                                            0323
*     DO;                           /* I/O IS DONE. CAN FREE THE   0324
*                                      FRAME UNDER CERTAIN         0324
*                                      CONDITIONS.                   */
*       IF PCBRLPA=0 THEN           /* TEST FOR NO RELATED REQUESTS. */
         L     @04,PCBRLPA-1(,PCBPTR)                              0325
         LA    @04,0(,@04)                                         0325
         LTR   @04,@04                                             0325
         BNZ   @RF00325                                            0325
*         DO;                       /* ARE NO RELATED REQUESTS. CAN
*                                      FREE THE FRAME.               */
*           IF PCBIOERR='0'B THEN   /* TEST FOR NO I/O ERROR.        */
         TM    PCBIOERR(PCBPTR),B'00100000'                        0327
         BNZ   @RF00327                                            0327
*             IF PCBCQN=PCBCIOQN×PCBVBN=0 THEN/* IF COMMON AREA FRAME
*                                      OR ZERO VBN IN THE PCB, THE 0328
*                                      FRAME HAS BEEN DISPOSED OF.   */
         CLI   PCBCQN(PCBPTR),32                                   0328
         BE    @RT00328                                            0328
         LH    @04,PCBVBN(,PCBPTR)                                 0328
         N     @04,@CF02318                                        0328
         LTR   @04,@04                                             0328
         BZ    @RT00328                                            0328
*               GOTO FREEBIT;       /* NO FRAME TO DO, SO GO FREE THE
*                                      PCB.                          */
*           RESPECIFY                                              0330
*            (PFTEPTR) RESTRICTED;                                 0330
@RF00327 DS    0H                                                  0331
*           PFTEPTR=PVTPFTP+PCBRBN; /* GET THE PFTE ADDR             */
         LH    PFTEPTR,PCBRBN(,PCBPTR)                             0331
         N     PFTEPTR,@CF02318                                    0331
         AL    PFTEPTR,PVTPFTP(,PVTPTR)                            0331
*           PFTPCBSI='0'B;          /* NO PAGING CURRENTLY ACTIVE.   */
         NI    PFTPCBSI(PFTEPTR),B'11110111'                       0332
*           IF PCBIOERR='1'B THEN   /* TEST FOR I/O ERROR.           */
         TM    PCBIOERR(PCBPTR),B'00100000'                        0333
         BNO   @RF00333                                            0333
*             PFTPGID=0;            /* I/O ERROR OCCURRED. ZERO THE
*                                      PGID TO PREVENT RECLAIM.      */
         SLR   @07,@07                                             0334
         ST    @07,PFTPGID(,PFTEPTR)                               0334
*           RESPECIFY                                              0335
*            (R0) RESTRICTED;                                      0335
@RF00333 DS    0H                                                  0336
*           R0=PCBRBN;              /* GET THE RBN.                  */
         LH    R0,PCBRBN(,PCBPTR)                                  0336
         N     R0,@CF02318                                         0336
*           R0=PFTECON×R0;          /* SET UP THE IEAVPFTE INTERFACE.*/
         O     R0,@CF02179                                         0337
*/********************************************************************/
*/*                                                                  */
*/*                  PFTE ENQ/DEQ                                    */
*/*                INPUT:                                            */
*/*           R0: TQN (FOR THE AFQ) AND RBN                          */
*/*           R1: UNUSED                                             */
*/*           R2: ADDRESS OF RSMHD                                   */
*/*           R3: ADDRESS OF PVT                                     */
*/*           R4 - R13: UNUSED                                       */
*/*           R14: RETURN ADDRESS LOADED BY BALR                     */
*/*           R15: ENTRY POINT ADDRESS LOADED BY BALR                */
*/*                OUTPUT:                                           */
*/*           R0 - R15: RESTORED                                     */
*/*                                                                  */
*/********************************************************************/
*                                                                  0338
*           CALL IEAVPFTE;          /* CALL PFTE ENQ/DEQ TO PUT PFTE
*                                      AT HEAD OF AFQ                */
         L     @15,PVTPFTE(,PVTPTR)                                0338
         BALR  @14,@15                                             0338
*           RESPECIFY                                              0339
*            (R0,                                                  0339
*             PFTEPTR) UNRESTRICTED;                               0339
*         END;                      /* END FREEING OF PFTE.          */
*FREEBIT:                                                          0341
*       RMWAFREE='1'B;              /* SET FLAG TO FREE THE PCB IN 0341
*                                      THE MAINLINE.                 */
@RF00325 DS    0H                                                  0341
FREEBIT  OI    RMWAFREE(RMWAPTR),B'00010000'                       0341
*     END;                          /* END I/O COMPLETE PROCESSING.  */
*   ELSE                            /* I/O IS NOT COMPLETE.          */
*     DO;                           /* PCB MUST REMAIN FOR THE I/O TO
*                                      COMPLETE.                     */
         B     @RC00323                                            0343
@RF00323 DS    0H                                                  0344
*       PCBFREAL='1'B;              /* SET FLAG TO FREE THE REAL   0344
*                                      FRAME                         */
*       PCBRESET='0'B;              /* TURN OFF RESET FLAG (ON TCB OR
*                                      ASCB PURGE, ROUTINE WILL NEVER
*                                      RUN AGAIN, SO A RESET DOES NOT
*                                      NEED TO BE DONE)              */
         OI    PCBFREAL(PCBPTR),B'10000000'                        0345
         NI    PCBRESET(PCBPTR),B'11101111'                        0345
*       IF RMWALIOQ='1'B&RMPLTERM='1'B THEN/* TEST FOR THE LOCAL I/O
*                                      QUEUE BEING PROCESSED AND ASCB
*                                      PURGE.                        */
         TM    RMWALIOQ(RMWAPTR),B'00100000'                       0346
         BNO   @RF00346                                            0346
         TM    RMPLTERM(RMPLPTR),B'01000000'                       0346
         BNO   @RF00346                                            0346
*         DO;                       /* ZERO THE VBN SO THAT WHEN THE
*                                      PCBS ARE MOVED TO M.S. LIOQ,
*                                      THERE WILL NOT BE TWO OF THE
*                                      SAME VBN.                     */
*LEAVEPCB:                                                         0348
*           PCBVBN=0;               /* ZERO THE VBN IN THE PCB. IF 0348
*                                      THE PCB IS MOVED TO MASTERS 0348
*                                      LIOQ, NO TWO QUEUED PCBS WILL
*                                      HAVE THE SAME VBN.            */
LEAVEPCB SLR   @04,@04                                             0348
         STH   @04,PCBVBN(,PCBPTR)                                 0348
*         END;                                                     0349
*       ELSE                                                       0350
*         DO;                       /* PCB IS FOR COMMON AREA      0350
*                                                            @Z40WPYD*/
         B     @RC00346                                            0350
@RF00346 DS    0H                                                  0351
*           IF RMPLTERM='1'B THEN                                  0351
*                                                                  0351
         TM    RMPLTERM(RMPLPTR),B'01000000'                       0351
         BNO   @RF00351                                            0351
*/* IF THIS IS A COMMON AREA PCB WITH I/O INCOMPLETE AND THE       0352
*   PURGE TYPE IS ASCB, THEN MASTER'S ASCB ADDRESS MUST BE PUT IN  0352
*   PCBASCB.                                  @Z40WPYD               */
*                                                                  0352
*             PCBASCB=PSAAOLD;      /* PUT MASTER'S ASCB ADDRESS IN
*                                      PCB                   @Z40WPYD*/
         L     @04,PSAAOLD                                         0352
         ST    @04,PCBASCB(,PCBPTR)                                0352
*         END;                                                     0353
@RF00351 DS    0H                                                  0354
*     END;                                                         0354
*                                                                  0354
@RC00346 DS    0H                                                  0355
*   /*****************************************************************/
*   /*                                                               */
*   /* EXIT POINT FOR TERMIO SUBROUTINE                              */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0355
*EXITIO:                                                           0355
*   RESPECIFY                                                      0355
*    (R14) RESTRICTED;                                             0355
@RC00323 DS    0H                                                  0355
EXITIO   DS    0H                                                  0356
*   R14=RMWALVL2;                   /* GET THE RETURN ADDRESS        */
         L     R14,RMWALVL2(,RMWAPTR)                              0356
*   RESPECIFY                                                      0357
*    (RELPCB,                                                      0357
*     PCBPTR,                                                      0357
*     PRIPCB,                                                      0357
*     R14) UNRESTRICTED;                                           0357
*   END TERMIO;                                                    0358
@EL00003 DS    0H                                                  0358
@EF00003 DS    0H                                                  0358
@ER00003 BR    @14                                                 0358
