*/********************************************************************/
*/*                                                                  */
*/* THE RESET INTERNAL SUBROUTINE IS CALLED TO RESET SUSPENDED RBS   */
*/* FOUND DURING RB PURGE OR SRBS FOUND DURING ASCB PURGE. IF THE PCB*/
*/* INDICATES THAT RESET SHOULD BE DONE, THE RESET MODULE IS CALLED. */
*/* ON RETURN, CONTROL RETURNS TO THE CALLER IN THE QUEUE SEARCH     */
*/* MAINLINE.                                                        */
*/*                                                                  */
*/********************************************************************/
*                                                                  0359
*RSETSUBR:                                                         0359
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0359
RSETSUBR DS    0H                                                  0360
*   RESPECIFY                                                      0360
*    (RELPCB,                                                      0360
*     PCBPTR,                                                      0360
*     PRIPCB,                                                      0360
*     R14) RESTRICTED;                                             0360
*   RMWALVL2=R14;                   /* SAVE THE RETURN ADDRESS       */
         ST    R14,RMWALVL2(,RMWAPTR)                              0361
*   RESPECIFY                                                      0362
*    (R14) UNRESTRICTED;                                           0362
*   IF PCBRESET='1'B THEN           /* TEST IF RESET MUST BE DONE    */
         TM    PCBRESET(PCBPTR),B'00010000'                        0363
         BNO   @RF00363                                            0363
*     DO;                           /* RESET IS TO BE DONE           */
*       RESPECIFY                                                  0365
*        (R4,                                                      0365
*         R6,                                                      0365
*         R13) RESTRICTED;                                         0365
*       IF PCBSRBMD='1'B THEN       /* TEST IF RESET IS FOR A SRB    */
         TM    PCBSRBMD(PCBPTR),B'00100000'                        0366
         BNO   @RF00366                                            0366
*         DO;                       /* RESET IS FOR A SRB            */
*           R4=PCBSRB;              /* GET THE SRB ADDRESS           */
         L     R4,PCBSRB(,PCBPTR)                                  0368
*           R5=0;                   /* INDICATES NO RB               */
         SLR   R5,R5                                               0369
*         END;                                                     0370
*       ELSE                        /* NOT SRB PCB. MUST BE RB PCB   */
*         DO;                       /* INIT FOE RB                   */
         B     @RC00366                                            0371
@RF00366 DS    0H                                                  0372
*           R4=PCBRTPA;             /* GET THE TCB ADDRESS           */
         L     R4,PCBRTPA-1(,PCBPTR)                               0372
         LA    R4,0(,R4)                                           0372
*           R5=PCBSRB;              /* GET THE RB ADDRESS            */
         L     R5,PCBSRB(,PCBPTR)                                  0373
*         END;                                                     0374
*       R6=0;                       /* NON I/O ERROR                 */
@RC00366 SLR   R6,R6                                               0375
*       R13=ADDR(PVTSAVE);          /* GET THE SAVE AREA ADDRESS     */
         LA    R13,PVTSAVE(,PVTPTR)                                0376
*/********************************************************************/
*/*                                                                  */
*/*               RESET A RB/SRB                                     */
*/*             INPUT:                                               */
*/*        R0 - R3: UNUSED                                           */
*/*        R4: ADDRESS OF THE SRB IF SRB RESET                       */
