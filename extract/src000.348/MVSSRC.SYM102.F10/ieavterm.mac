         TITLE 'IEAVTERM: PAGING TERMINATOR                            *
                        '
IEAVTERM CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'IEAVTERM  78.131'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @10,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@10                                         0001
*   RMPLPTR=R1->FOREPTRS;           /* GET THE ADDRESS OF THE INPUT
*                                      RMPL. USE STANDARD LINKAGE  0084
*                                      CONV                          */
         L     RMPLPTR,FOREPTRS(,R1)                               0084
*   RMWAPTR=RMPLRMWA;               /* GET THE ADDRESS OF THE RMWA   */
         L     RMWAPTR,RMPLRMWA(,RMPLPTR)                          0085
*   RMWA=RMWA&&RMWA;                /* CLEAR THE RMWA                */
         XC    RMWA(64,RMWAPTR),RMWA(RMWAPTR)                      0086
*   RMWASA13=R13;                   /* SAVE THE ADDR OF THE SAVE AREA*/
         ST    R13,RMWASA13(,RMWAPTR)                              0087
*   PVTPTR=CVTPVTP;                 /* GET THE PVT ADDRESS   @ZA19285*/
         L     @12,CVTPTR                                          0088
         L     PVTPTR,CVTPVTP(,@12)                                0088
*   RESPECIFY                                                      0089
*    (R11,                                                         0089
*     R12) RESTRICTED;                                             0089
*   IF RMPLTERM='1'B THEN           /* ADDR SPACE TERMINATING?     0090
*                                                            @ZA19285*/
         TM    RMPLTERM(RMPLPTR),B'01000000'                       0090
         BNO   @RF00090                                            0090
*     DO;                           /* YES, PREVENT AND PURGE SWAP-IN
*                                                            @ZA19285*/
*       RFY                                                        0092
*        (R0) RSTD;                 /*                       @ZA19285*/
*       GEN SETS(R0) REFS(RMPLASID,RMPLPTR)                        0093
*           (SYSEVENT MEMDEL,ASIDL=RMPLASID(,RMPLPTR),TYPE=NOSWIN);/*
*                                                            @ZA19285*/
         SYSEVENT MEMDEL,ASIDL=RMPLASID(,RMPLPTR),TYPE=NOSWIN
*       RFY                                                        0094
*        (R0) UNRSTD;               /*                       @ZA19285*/
*       RMWPASID=RMPLASID;          /* PURGE ASID TO ASIDTCB @ZA19285*/
         MVC   RMWPASID(2,RMWAPTR),RMPLASID(RMPLPTR)               0095
*       R11=ADDR(RMWASIDP);         /* ASIDTCB POINTER       @ZA19285*/
         LA    R11,RMWASIDP(,RMWAPTR)                              0096
*       R12=PVTPSRBP;               /* RMTR ADDRESS          @ZA19285*/
         L     R12,PVTPSRBP(,PVTPTR)                               0097
*       R1=ADDR(RMWAPDQL);          /* PURGEDQ PLIST ADDRESS @ZA19285*/
         LA    R1,RMWAPDQL(,RMWAPTR)                               0098
*       GEN REFS(R11,R12) SETS(RMWAPDQL)                           0099
*           (PURGEDQ MF=(E,(1)),ASIDTCB=(R11),RMTR=(R12));/* @ZA19285*/
         PURGEDQ MF=(E,(1)),ASIDTCB=(R11),RMTR=(R12)
*     END;                          /*                       @ZA19285*/
*   RESPECIFY                                                      0101
*    (R1) UNRESTRICTED;                                            0101
@RF00090 DS    0H                                                  0102
*   GENERATE CODE                   /* GET THE SALLOC AND LOCAL    0102
*                                      LOCKS.                        */
*       REFS(PSALITA,FLC) SETS(R11,R12,R13,R14);                   0102
GETLOCL  SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND,                       **
               RELATED=(RSM,IEAVTERM(FREELOCL),IEAVTERM(FREALOCL))
GETSALL  SETLOCK OBTAIN,TYPE=SALLOC,MODE=UNCOND,                      **
               RELATED=(RSM,IEAVTERM(FREESALL))
*   RESPECIFY                                                      0103
*    (R12,                                                         0103
*     R13) UNRESTRICTED;                                           0103
*   RESPECIFY                                                      0104
*    (R1,                                                          0104
*     R4) RESTRICTED;                                              0104
*   R1=PVTPRCV;                     /* GET THE ENTRY POINT TO THE RSM
*                                      RECOVERY ROUTINE              */
         L     R1,PVTPRCV(,PVTPTR)                                 0105
*   GENERATE CODE                   /* ESTABLISH THE RSM FRR         */
*       REFS(PSA,PSACSTK,FRRSCURR,FRRS,FRRSELEN,FRRSCURR,FRRSFRRA, 0106
*       FRRSENTR,FRRSPARM) SETS(R1,R4,R11);                        0106
GETFRR   SETFRR A,FRRAD=(1),PARMAD=(1),WRKREGS=(4,11),                **
               RELATED=(RSM,IEAVTERM(FREEFRR))
*   PVTPRCA=R1;                     /* SET POINTER TO RCA IN THE PVT */
         ST    R1,PVTPRCA(,PVTPTR)                                 0107
*   RMWARCA=R1;                     /* SAVE THE RCA ADDRESS FOR    0108
*                                      RESTORES ACROSS FREEMAINS     */
         ST    R1,RMWARCA(,RMWAPTR)                                0108
*   RMWAPVT=PVTPTR;                 /* SAVE PVT PTR FOR RESTORES   0109
*                                      ACROSS INTERFACES             */
         ST    PVTPTR,RMWAPVT(,RMWAPTR)                            0109
*   RESPECIFY                                                      0110
*    (R1,                                                          0110
*     R4,                                                          0110
*     R11) UNRESTRICTED;                                           0110
*   RCACSECT=RCACPTRM;              /* SET IEAVTERM CSECT ID         */
         L     @12,PVTPRCA(,PVTPTR)                                0111
         MVI   RCACSECT(@12),X'11'                                 0111
*   RCATERM='1'B;                   /* SET MODULE ENTERED FLAG       */
         OI    RCATERM(@12),B'00010000'                            0112
*   RCASAL='1'B;                    /* SET THE SALLOC-HELD FLAG      */
*   RCALL='1'B;                     /* LOCAL LOCK HELD, SO SET FLAG  */
         OI    RCASAL(@12),B'11000000'                             0114
*   LLOCK='1'B;                     /* LOCAL LOCK HELD, SO SET FLAG
*                                                            @VS05446*/
         OI    LLOCK(RMWAPTR),B'10000000'                          0115
*   RSMHDPTR=ASCBRSM;               /* GET THE ADDRESS OF THE RSMHD
*                                      IN A REGISTER                 */
         L     @12,RMPLASCB(,RMPLPTR)                              0116
         L     RSMHDPTR,ASCBRSM(,@12)                              0116
*   IF RSMHDPTR=0 THEN              /* TEST FOR NO RSMHD. THIS OCCURS
*                                      WHEN THE ADDRESS SPACE WAS NOT
*                                      COMPLETELY CREATED.   @VS05446*/
         SLR   @12,@12                                             0117
         CR    RSMHDPTR,@12                                        0117
         BE    @RT00117                                            0117
*     GOTO GETOUT;                  /* EXIT. NO WORK TO DO.  @VS05446*/
*   IF RMPLTERM='1'B THEN           /* TEST FOR ASCB PURGE TYPE.     */
         TM    RMPLTERM(RMPLPTR),B'01000000'                       0119
         BNO   @RF00119                                            0119
*     DO;                           /* IS ASCB PURGE. THE ADDRESS  0120
*                                      SPACE MAY HAVE A SWAP OUT OR
*                                      SWAP IN ONGOING. IF SO, CANCEL
*                                      THE SWAP                      */
*       IF SPCTOUT='1'B THEN        /* TEST FOR A SWAP OUT.          */
         L     @11,RSMSPCT(,RSMHDPTR)                              0121
         TM    SPCTOUT(@11),B'01000000'                            0121
         BNO   @RF00121                                            0121
*         DO;                       /*                       @Z40WPYD*/
*           IF SPCTSWRT^=0 THEN     /* IS THERE A PARAMETER LIST   0123
*                                      (PCB) TO FREE         @Z40WPYD*/
         L     @11,SPCTSWRT(,@11)                                  0123
         CR    @11,@12                                             0123
         BE    @RF00123                                            0123
*             DO;                                                  0124
*               RESPECIFY                                          0125
*                 SOUTPARM BASED(SPCTSWRT);/*                @Z40WPYC*/
*               SOUTINT='1'B;       /* INDICATE THE SWAPOUT HAS BEEN
*                                      INTERCEPTED. IEAVDLAS (RMTR)
*                                      WILL FREE THE SWAPOUT       0126
*                                      PARAMETER LIST        @Z40WPYC*/
         OI    SOUTINT(@11),B'10000000'                            0126
*             END;                                                 0127
*           ELSE                                                   0128
*             ;                     /*                       @Z40WPYD*/
@RF00123 DS    0H                                                  0129
*         END;                                                     0129
*       ELSE                        /* NOT SWAP OUT, SO MAY BE SWAP
*                                      IN                            */
*         IF SPCTSWIN='1'B THEN     /* TEST FOR SWAP IN.             */
         B     @RC00121                                            0130
@RF00121 L     @12,RSMSPCT(,RSMHDPTR)                              0130
         TM    SPCTSWIN(@12),B'10000000'                           0130
         BNO   @RF00130                                            0130
*           DO;                     /* IS A SWAP IN. CANCEL THE SWAP.*/
*             RESPECIFY                                            0132
*               PCBR BASED(SPCTSWRT);                              0132
*             PCBRFAIL='0'B;        /* IGNORE ANY I/O FAILURES.      */
*             PCBRPB='1'B;          /* HAVE THE SWAP IN ROOT FREE ANY
*                                      I/O COMPLETE PCBS AND UNDO  0134
*                                      INCORE COMMON AREA FIXES.     */
         L     @12,SPCTSWRT(,@12)                                  0134
         OI    PCBRPB(@12),B'00100000'                             0134
         NI    PCBRFAIL(@12),B'11110111'                           0134
*             RSMFAIL='1'B;         /* SET FAIL TO FORCE SWAPIN ROOT
*                                      EXIT THROUGH ITS ERROR PATH 0135
*                                                            @Z40WPYD*/
         OI    RSMFAIL(RSMHDPTR),B'00000010'                       0135
*             RESPECIFY                                            0136
*               PCBR BASED(PCBRPTR);                               0136
*           END;                                                   0137
*                                                                  0137
*       /*************************************************************/
*       /*                                                           */
*       /* SINCE PURGE TYPE IS ASCB, THE LOCAL LOCK IS NOT NEEDED    */
*       /* (VTERM DOES NOT RUN IN THE ADDRESS SPACE IT IS TERMINATING*/
*       /* ON ASCB PURGE.) FREE THE LOCAL LOCK.                      */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0138
*       RESPECIFY                                                  0138
*        (R11,                                                     0138
*         R12,                                                     0138
*         R13) RESTRICTED;                                         0138
@RF00130 DS    0H                                                  0138
@RC00121 DS    0H                                                  0139
*       GENERATE CODE               /* FREE THE LOCAL LOCK           */
*           REFS(PSALITA,FLC) SETS(R11,R12,R13,R14);               0139
FREALOCL SETLOCK RELEASE,TYPE=LOCAL,                                  **
               RELATED=(RSM,IEAVTERM(GETLOCL))
*       RESPECIFY                                                  0140
*        (R11,                                                     0140
*         R12,                                                     0140
*         R13) UNRESTRICTED;                                       0140
*       RCALL='0'B;                 /* LOCAL LOCK NOT HELD,SO ZERO 0141
*                                      FLAG                          */
         L     @12,PVTPRCA(,PVTPTR)                                0141
         NI    RCALL(@12),B'10111111'                              0141
*       LLOCK='0'B;                 /* LOCAL LOCK NOT HELD,SO ZERO 0142
*                                      INTERNAL FLAG         @VS05446*/
         NI    LLOCK(RMWAPTR),B'01111111'                          0142
*     END;                          /* END SWAP ROOT INTERCEPTION.   */
*   CALL TERMPGQ;                   /* CALL THE PAGING QUIESCE     0144
*                                      SUBROUTINE                    */
@RF00119 BAL   @14,TERMPGQ                                         0144
*   IF RMPLTERM='1'B THEN           /* TEST FOR ASCB PURGE           */
         TM    RMPLTERM(RMPLPTR),B'01000000'                       0145
         BNO   @RF00145                                            0145
*     DO;                           /* IS ASCB PURGE, SO CALL DELETE
*                                      ADDRESS SPACE TO GET RID OF 0146
*                                      THE ADDRESS SPACE AND FREE ITS
*                                      RESOURCES                     */
*       RESPECIFY                                                  0147
*        (R1) RESTRICTED;                                          0147
*       R1=RMPLASCB;                /* GET THE ASCB TO BE DELETED AND
*                                      PASS IT TO DELETE A.S.        */
         L     R1,RMPLASCB(,RMPLPTR)                               0148
*/********************************************************************/
*/*                                                                  */
*/*               DELETE ADDRESS SPACE                               */
*/*             INPUT:                                               */
*/*        R0: UNUSED                                                */
*/*        R1: ADDRESS OF ASCB TO BE DELETED                         */
*/*        R2: ADDRESS OF RSMHD TO BE DELETED                        */
*/*        R3: ADDRESS OF PVT                                        */
*/*        R4 - R13: UNUSED                                          */
*/*        R14: RETURN ADDRESS LOADED BY BALR                        */
*/*        R15: ENTRY POINT ADDRESS LOADED BY BALR                   */
*/*             OUTPUT:                                              */
*/*        R0 - R15: RESTORED                                        */
*/*                                                                  */
*/********************************************************************/
*                                                                  0149
*       CALL IEAVDLAS;              /* CALL THE DELETE ADDRESS SPACE
*                                      ROUTINE                       */
         L     @15,@CV02177                                        0149
         BALR  @14,@15                                             0149
*       RCACSECT=RCACPTRM;          /* RESET THE MODULE ID           */
         L     @12,PVTPRCA(,PVTPTR)                                0150
         MVI   RCACSECT(@12),X'11'                                 0150
*       RESPECIFY                                                  0151
*        (R1) UNRESTRICTED;                                        0151
*     END;                                                         0152
*                                                                  0152
*   /*****************************************************************/
*   /*                                                               */
*   /* MUST BE RB PURGE OR TCB PURGE TO BE HERE                      */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0153
*   ELSE                            /* NOT ASCB PURGE                */
*     IF(RMPLRBPP='0'B&RMPLIPUF='0'B) THEN/* TEST FOR A TCB PURGE AND
*                                      IF FIX PURGES ARE TO BE DONE. */
         B     @RC00145                                            0153
@RF00145 TM    RMPLRBPP(RMPLPTR),B'00110000'                       0153
         BNZ   @RF00153                                            0153
*       CALL TERMPFIX;              /* FIXES ARE TO BE PURGED, SO  0154
*                                      CALL THE FIX PURGE SUBROUTINE */
         BAL   @14,TERMPFIX                                        0154
*GETOUT:                                                           0155
*   RESPECIFY                                                      0155
*    (R4,                                                          0155
*     R11,                                                         0155
*     R12,                                                         0155
*     R13) RESTRICTED;                                             0155
@RF00153 DS    0H                                                  0155
@RC00145 DS    0H                                                  0155
GETOUT   DS    0H                                                  0156
*   GENERATE CODE                   /* REMOVE THE RSM FRR            */
*       REFS(PSA,PSACSTK,FRRSCURR,FRRS,FRRSELEN,FRRSCURR,FRRSFRRA, 0156
*       FRRSENTR,FRRSPARM) SETS(R4,R11);                           0156
FREEFRR  SETFRR D,WRKREGS=(4,11),                                     **
               RELATED=(RSM,IEAVTERM(GETFRR))
*   GENERATE CODE                   /* FREE THE SALLOC               */
*       REFS(PSALITA,FLC) SETS(R11,R12,R13,R14);                   0157
FREESALL SETLOCK RELEASE,TYPE=SALLOC,                                 **
               RELATED=(RSM,IEAVTERM(GETSALL))
*   IF LLOCK='1'B THEN              /* TEST IF LOCAL LOCK HELD.      */
         TM    LLOCK(RMWAPTR),B'10000000'                          0158
         BNO   @RF00158                                            0158
*     GENERATE CODE                 /* FREE THE LOCAL LOCK           */
*         REFS(PSALITA,FLC) SETS(R11,R12,R13,R14);                 0159
FREELOCL SETLOCK RELEASE,TYPE=LOCAL,                                  **
               RELATED=(RSM,IEAVTERM(GETLOCL))
*     RESPECIFY                                                    0160
*      (R4,                                                        0160
*       R11,                                                       0160
*       R12) UNRESTRICTED;                                         0160
@RF00158 DS    0H                                                  0161
*   R13=RMWASA13;                   /* RESTORE THE SAVE AREA POINTER */
         L     R13,RMWASA13(,RMWAPTR)                              0161
*   RETURN CODE(0);                 /* RETURN TO RTM. RETURN CODE IS
*                                      ALWAYS ZERO UNLESS DESIGN IS
*                                      CHANGED.                      */
         SLR   @15,@15                                             0162
         L     @14,12(,@13)                                        0162
         LM    @00,@12,20(@13)                                     0162
         BR    @14                                                 0162
*   RESPECIFY                                                      0163
*    (R13) UNRESTRICTED;                                           0163
*                                                                  0164
*/********************************************************************/
*/*                                                                  */
*/* THE PAGING PURGE SUBROUTINE IS CALLED BY THE MAINLINE TO PURGE   */
*/* ALL PCBS FOR THE FUNCTION UNDERGOING TERMINATION. THIS ROUTINE   */
*/* SCANS THE GFA DEFER, THE COMMON I/O ACTIVE, AND THEN THE LOCAL   */
*/* I/O ACTIVE PCB QUEUES SEARCHING FOR PCBS (QUEUED AND RELATED)    */
*/* IN OPERATION FOR THE FUNCTION BEING PURGED. ONCE A PCB FOR THE   */
*/* FUNCTION BEING PURGED IS LOCATED, IT IS PROCESSED AS DESCRIBED   */
*/* BELOW. IF THE PCB IS TO BE FREED, IT IS RETURNED TO THE FREE PCB */
*/* QUEUE. ONCE PROCESSING IS COMPLETE, IF THE PURGE TYPE IS RB,     */
*/* CONTROL IS RETURNED TO THE MAINLINE PROCESSOR BECAUSE THE ONLY   */
*/* PCB FOR THE RB HAS BEEN PURGED. OTHERWISE, THE NEXT PCB IS       */
*/* OBTAINED AND PROCESSED. ONCE ALL PCBS ON A QUEUE HAVE BEEN       */
*/* PROCESSED, OR IF NO PCBS WERE FOUND FOR A RB PURGE, THE NEXT     */
*/* QUEUE IS SELECTED AND SEARCHED. AT THE END OF THE THREE PCB      */
*/* QUEUES, CONTROL IS RETURNED TO THE MAINLINE FOR FURTHER          */
*/* PROCESSING.                                                      */
*/* EACH PCB ON THE THREE QUEUES IS TESTED TO DETERMINE IF THE PCB   */
*/* IS FOR THE ADDRESS SPACE IN WHICH THE TERMINATING FUNCTION WAS   */
*/* RUNNING. IF NOT, THE PCB IS TESTED FOR A COMMON AREA FIX SWAP    */
*/* IN FOR THE ASCB BEING TERMINATED. IF SO, THE SWAP FLAGS ARE      */
*/* TURNED OFF IN THE PCB. IF THE I/O IS COMPLETE AND THIS PCB IS THE*/
*/* ONLY PCB FOR THE PAGE, THE FRAME IS FREED. IF THE PCB IS A       */
*/* RELATED PCB OR THE I/O IS COMPLETE, THE PCB IS MARKED TO BE FREED*/
*/* LATER. THE SWAP IN ROOT COUNT IS DECREMENTED BY ONE, AND THE ROOT*/
*/* EXIT IS CALLED IF THE COUNT IS NOW ZERO. THE PCB IS FREED IF     */
*/* NECESSARY AND THE NEXT PCB IS OBTAINED AND PROCESSED.            */
*/* IF THE PCB IS FOR THE ADDRESS SPACE BEING PROCESSED, THE PURGE   */
*/* TYPE IS TESTED. IF ASCB PURGE, THEN THE PCB MUST BE PURGED.      */
*/* THE PCB IS TESTED TO SEE IF IT IS FOR A SRB ROUTINE WHICH MUST BE*/
*/* RESET. IF SO, THE ROUTINE IS RESET. FOR ALL ASCB PURGES, THE PCB */
*/* IS PASSED TO THE PCB PURGE ROUTINE. IF THE PURGE TYPE IS TCB, THE*/
*/* TCB ADDRESS IN THE PCB IS COMPARED TO THE TCB ADDRESS BEING      */
*/* PURGED. IF EQUAL, THE TCB IS PASSED TO THE PCB PURGE ROUTINE;    */
*/* OTHERWISE, THE NEXT PCB IS OBTAINED AND PROCESSED. IF THE PURGE  */
*/* TYPE IS NOT ASCB OR TCB, IT MUST BE RB. IF THE PCB IS FOR THE RB */
*/* BEING PURGED, THE RESET SUBROUTINE IS CALLED AND THE PCB IS      */
*/* PASSED TO THE PCB PURGE ROUTINE. OTHERWISE, THE NEXT PCB IS      */
*/* OBTAINED AND PROCESSED.                                          */
*/* THE PCB PURGE ROUTINE CONTROLS THE ACTUAL PURGE OF THE PCB. IF   */
*/* THE PCB IS ON THE GFA DEFERRED QUEUE AND IS A QUEUED PCB WITH NO */
*/* RELATED PCBS, THE XPTDEFER FLAG IS TURNED OFF TO PREVENT FURTHER */
*/* RELATING TO THE PCB. ALSO, IF THE DEFERRED PCB HAS A ROOT        */
*/* PCB, THE ROOT PURGE SUBROUTINE IS CALLED TO PURGE THE ROOT PCB.  */
*/* DEFERRED PCBS ARE ALWAYS FREED AS THERE IS NO I/O IN PROGRESS    */
*/* FOR THEM. ONCE FREED, THE NEXT PCB IS OBTAINED AND PROCESSED.    */
*/* IF THE PCB IS ON AN I/O ACTIVE QUEUE, THE I/O PURGE              */
*/* SUBROUTINE IS CALLED TO PURGE THE PCB. ON RETURN, IF             */
*/* THE PCB IS TO BE FREED, IT IS FREED. THE NEXT PCB IS THEN        */
*/* OBTAINED AND PROCESSED. AFTER ALL PROCESSING IS DONE, THE NEXT   */
*/* QUEUE IS OBTAINED AND PROCESSED AS ABOVE. WHEN ALL QUEUES HAVE   */
*/* BEEN PROCESSED, CONTROL RETURNS TO THE CALLER.                   */
*/*                                                                  */
*/* THE FOLLOWING REGISTER VARIABLES ARE USED TO PROCESS THE PCB     */
*/* QUEUES:                                                          */
*/*        PCBPTR: THIS ALWAYS POINTS TO THE PCB BEING PROCESSED. IT */
*/*                CANNOT BE ASSUMED THAT THE FOREWARD POINTER IN THE*/
*/*                PCB POINTED TO BY PCBPTR IS THE NEXT PCB TO BE    */
*/*                PROCESSED BECAUSE AFTER THE PCB IS FREED, PCBPTR  */
*/*                WILL STILL POINT TO THE PCB NOW ON THE FREE QUEUE.*/
*/*                METHOD OF OBTAINING THE NEXT PCB TO BE PROCESSED  */
*/*                IS DESCRIBED BELOW.                               */
*/*        PRIPCB: THIS POINTS TO THE LAST PROCESSED PRIMARY PCB     */
*/*                WHEN SCANNING A QUEUE. THE ONLY EXCEPTION IS WHEN */
*/*                PCBPTR POINTS TO THE FIRST PCB ON A QUEUE: THEN,  */
*/*                PRIPCB POINTS TO THE APPROPRIATE QUEUE HEADER.    */
*/*                THE PURPOSE OF THIS POINTER IS TO HAVE THE        */
*/*                ADDRESS OF THE FIELD WHICH WILL HAVE THE ADDRESS  */
*/*                OF THE NEXT PRIMARY PCB TO BE PROCESSED. WHEN ALL */
*/*                RELATED PCBS (RELATED TO THE PCB POINTED TO BY    */
*/*                PRIPCB) ARE PROCESSED, PRIPCB WILL POINT TO THE   */
*/*                FIELD CONTAINING THE ADDRESS OF THE NEXT PRIMARY  */
*/*                PCB TO BE PROCESSED.                              */
*/*        RELPCB: THIS POINTS TO THE LAST PROCESSED RELATED PCB ON  */
*/*                THE RELATED CHAIN. THIS POINTER IS USED TO RECHAIN*/
*/*                RELATED PCBS WHEN A RELATED PCB HAS BEEN REMOVED  */
*/*                FROM THE RELATED CHAIN AND PURGED AND TO  OBTAIN  */
*/*                THE ADDRESS OF THE NEXT RELATED PCB.IF THE RELATED*/
*/*                POINTER IN THE PCB ADDRESSED BY  RELPCB IS ZERO,  */
*/*                THEN THERE ARE NO MORE RELATED  PCBS FOR THIS     */
*/*                PRIMARY PCB, AND THE NEXT PRIMARY PCB IS OBTAINED */
*/*                AND PROCESSED. IF THE RELATED  POINTER IS NOT     */
*/*                ZERO, THEN THE RELATED POINTER IS THE ADDRESS OF  */
*/*                THE NEXT PCB TO BE PROCESSED.                     */
*/*        BACKPCB: THIS POINTS TO THE FIELD WHICH WILL HAVE THE     */
*/*                ADDRESS OF THE NEXT PCB TO BE PROCESSED (PRIMARY  */
*/*                OR RELATED) WHEN THE PCB POINTED TO BY PCBPTR     */
*/*                IS BEING FREED. THIS FIELD IS USED ONLY IF        */
*/*                THE CURRENT PCB IS ACTUALLY FREED, AND IS SET     */
*/*                USING PRIPCB OR RELPCB                            */
*/*                                                                  */
*/********************************************************************/
*                                                                  0164
*TERMPGQ:                                                          0164
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0164
TERMPGQ  DS    0H                                                  0165
*   RESPECIFY                                                      0165
*    (R14) RESTRICTED;                                             0165
*   RMWALVL1=R14;                   /* SAVE THE RETURN ADDRESS       */
         ST    R14,RMWALVL1(,RMWAPTR)                              0166
*   RESPECIFY                                                      0167
*    (R14) UNRESTRICTED;                                           0167
*   RMWAGFAD='1'B;                  /* SET THE GFA DEFER QUEUE FLAG  */
         OI    RMWAGFAD(RMWAPTR),B'10000000'                       0168
*   RESPECIFY                                                      0169
*    (PCBPTR,                                                      0169
*     PRIPCB,                                                      0169
*     RELPCB) RESTRICTED;                                          0169
*   PCBPTR=PVTGFADF;                /* GET THE FIRST GFA DEFER PCB   */
         L     PCBPTR,PVTGFADF(,PVTPTR)                            0170
*   PRIPCB=ADDR(PVTGFADF);          /* GET THE ADDRESS OF THE      0171
*                                      LOCATION OF THE NEXT PCB IF 0171
*                                      THE FIRST IS TO BE FREED      */
         LA    PRIPCB,PVTGFADF(,PVTPTR)                            0171
*   RELPCB=0;                       /* ZERO THE POINTER TO THE     0172
*                                      RELATED PCB                   */
         SLR   RELPCB,RELPCB                                       0172
*   GOTO TESTPCB;                   /* GO TEST IF THE PCB IS TO BE 0173
*                                      PROCESSED                     */
         B     TESTPCB                                             0173
*                                                                  0174
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS SECTION GETS THE NEXT PCB TO BE PROCESSED AND UPDATES THE*/
*   /* APPROPRIATE REGISTER QUEUE POINTERS.                          */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0174
*NEXTPCB:                                                          0174
*   IF RELPCB=0 THEN                /* TEST IF THIS IS THE FIRST   0174
*                                      RELATED PCB FOR THE PRIMARY   */
NEXTPCB  LTR   RELPCB,RELPCB                                       0174
         BNZ   @RF00174                                            0174
*     PRIPCB=PCBPTR;                /* IS THE FIRST RELATED. SET PRIM
*                                      PTR TO POINT TO THE JUST-DONE
*                                      PRIMARY                       */
         LR    PRIPCB,PCBPTR                                       0175
*   IF PCBRLPA=0 THEN               /* TEST IF A RELATED PCB EXISTS  */
@RF00174 L     @04,PCBRLPA-1(,PCBPTR)                              0176
         LA    @04,0(,@04)                                         0176
         LTR   @04,@04                                             0176
         BNZ   @RF00176                                            0176
