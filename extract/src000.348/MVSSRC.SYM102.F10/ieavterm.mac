*                                      NEXT PCB POINTS TO THE NEXT 0296
*                                      PRIMARY PCB                   */
         LR    BACKPCB,PRIPCB                                      0296
*         CALL FREEPCB;             /* FREE THE PRESENT PCB          */
         BAL   @14,FREEPCB                                         0297
*         RELPCB=0;                 /* SET THE POINTER TO THE LAST 0298
*                                      RELATED PCB TO NO RELATED     */
         SLR   RELPCB,RELPCB                                       0298
*       END;                                                       0299
