*     DO;                           /* DOALLOC DO/END GROUP          */
*/********************************************************************/
*/*     DYNAMICALLY ALLOCATE THE OUTPUT DATA SET (INTERNAL READER)   */
*/*                        - SYSOUT PROGRAM (WRITER) IS INTRDR       */
*/*                              //  DD  SYSOUT=(,INTRDR)            */
*/*                        - UNALLOCATE AT CLOSE                     */
*/*                        - RETURN THE DDNAME                       */
*/********************************************************************/
*/*                                                                  */
*/*     PARAMETERS FOR DYNAMIC ALLOCATION:                           */
*/*                                                                  */
*/*      R1 -> S99RBPTR -> S99RB      -> TXTPLIST -> TEXT UNITS      */
*/*                        (DYN ALLOC               (DESCRIBE THE    */
*/*                         PARAMETER                ALLOCATION      */
*/*                         LIST)                    REQUEST)        */
*/*                                                                  */
*/********************************************************************/
*                                                                  0096
*       S99RBPTR=ADDR(S99RB);       /* *** PTR TO DYNALLOC PARM LIST */
         LA    @10,S99RB                                           0096
         ST    @10,S99RBPTR                                        0096
*       S99RBPND=ON;                /* H-O BIT ON = STANDARD LINKAGE */
         OI    S99RBPND,B'10000000'                                0097
*       S99RB=S99RB&&S99RB;         /* ZERO THE PARM LIST            */
         XC    S99RB(20),S99RB                                     0098
*       S99RBLN=LENGTH(S99RB);      /* *** INIT THE DYNALLOC PARM  0099
*                                      LIST                          */
         MVI   S99RBLN,X'14'                                       0099
*       S99VERB=S99VRBAL;           /* THIS IS AN ALLOCATION REQUEST */
         MVI   S99VERB,X'01'                                       0100
*       S99TXTPP=ADDR(TXTPLIST);    /* POINTER TO LIST OF TEXT UNITS
*                                      POINTERS                      */
         LA    @10,TXTPLIST                                        0101
         ST    @10,S99TXTPP                                        0101
*       ALLOCTXT=NRTEXT;            /* COPY TEXT UNITS INTO REENTRANT
*                                      CORE                          */
         MVC   ALLOCTXT(34),NRTEXT                                 0102
*       TPSPGNM=ADDR(RENSPGNM);     /* PTR TO WTR = INTRDR TEXT UNIT */
         LA    @10,RENSPGNM                                        0103
         ST    @10,TPSPGNM                                         0103
*       TPSYSOU=ADDR(RENSYSOU);     /* PTR TO SYSOUT REQUEST T.U.    */
         LA    @10,RENSYSOU                                        0104
         ST    @10,TPSYSOU                                         0104
*       TPCLOSE=ADDR(RENCLOSE);     /* PTR TO UNALLOC AT CLOSE T.U.  */
         LA    @10,RENCLOSE                                        0105
         ST    @10,TPCLOSE                                         0105
*       TPRTDDN=ADDR(RENRTDDN);     /* PTR TO RETURN-THE-DDNAME TEXT
*                                      UNIT (NEED THE DDNAME TO OPEN
*                                      THE INTRDR)                   */
         LA    @10,RENRTDDN                                        0106
         ST    @10,TPRTDDN                                         0106
*       TPEND=ON;                   /* END OF TEXT UNIT POINTER LIST */
         OI    TPEND,B'10000000'                                   0107
*       RFY                                                        0108
*         R1 RSTD;                                                 0108
*       R1=ADDR(S99RBPTR);          /* PTR TO PTR TO DYNALLOC PARMLST*/
*                                                                  0109
         LA    R1,S99RBPTR                                         0109
*       /*************************************************************/
*       /*                                                           */
*       /* MACDATE Y-2 73082                                         */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0110
*       SVC(99);                    /* ISSUE DYNAMIC ALLOCATION SVC  */
         SVC   99                                                  0110
*       RFY                                                        0111
*         R1 UNRSTD;                                               0111
*       RFY                                                        0112
*         R15 RSTD;                                                0112
*       RFY                                                        0113
*         S99TUNIT BASED(TPRTDDN);  /* POINT TO TEXT UNIT FOR      0113
*                                      RETURNED DDNAME (NEED DDNAME
*                                      IN ACB TO OPEN THE INTRDR DATA
*                                      SET)                          */
*                                                                  0113
*       /*************************************************************/
*       /*                                                           */
*       /* ERROR IF DYNAMIC ALLOCATION FAILED - HAVE DAIRFAIL ISSUE  */
*       /* ERROR MESSAGE (CHANGE FROM KEY 1 TO 0 IN CASE SECOND LEVEL*/
*       /* MESSAGE ISSUED - STORED IN TMP'S KEY 8 SUBPOOL 78)        */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0114
*       IF R15^=ALLOCROK THEN                                      0114
         LTR   R15,R15                                             0114
         BZ    @RF00114                                            0114
*         DO;                                                      0115
*           RETCODE=R15;            /* SAVE DYNALLOC RETURN CODE     */
         ST    R15,RETCODE                                         0116
*           RFY                                                    0117
*             R15 UNRSTD;                                          0117
*           RFY                                                    0118
*             R0 RSTD;                                             0118
*           GEN(LOAD EP=IKJEFF18);  /* DAIRFAIL                      */
         LOAD EP=IKJEFF18
*           GEN;                                                   0120
*                                      /* CHANGE TO KEY 0 */
SETKEY0   MODESET EXTKEY=SUPR,RELATED=(KEY0,IKJEFF15(SETKEY1))
*           CALL IKJEFF18           /* DAIRFAIL ROUTINE              */
*               (S99RB,             /* NEEDS DYNALLOC PARAMETER LIST */
*               RETCODE,            /* AND DYNALLOC RETURN CODE      */
*               MSGRTN,             /* AND ADDR OF IKJEFF02 (OR 0)   */
*               CALLERID,           /* AND ID (WANT DYNALLOC INTFC)  */
*               CPPL);              /* AND CPPL (FOR PUTLINE)        */
         LA    @10,S99RB                                           0121
         ST    @10,@AL00001                                        0121
         LA    @10,RETCODE                                         0121
         ST    @10,@AL00001+4                                      0121
         LA    @10,MSGRTN(,MSGPTR)                                 0121
         ST    @10,@AL00001+8                                      0121
         LA    @10,CALLERID                                        0121
         ST    @10,@AL00001+12                                     0121
         L     @10,MTCPPL(,MSGPTR)                                 0121
         ST    @10,@AL00001+16                                     0121
         LR    @15,R0                                              0121
         LA    @01,@AL00001                                        0121
         BALR  @14,@15                                             0121
*           GEN;                                                   0122
*                                   /* CHANGE BACK TO SCHEDULER KEY 1*/
SETKEY1   MODESET EXTKEY=SCHED,RELATED=(KEY1,IKJEFF15(SETKEY0))
*           GEN(DELETE EP=IKJEFF18);                               0123
         DELETE EP=IKJEFF18
*           RFY                                                    0124
*             R0 UNRSTD;                                           0124
*           QUIT=ON;                                               0125
         OI    QUIT(HTPTR),B'10000000'                             0125
*         END;                                                     0126
*       ELSE                                                       0127
*         DO;                       /* ALLOCOK DO/END GROUP          */
*                                                                  0127
         B     @RC00114                                            0127
@RF00114 DS    0H                                                  0128
*           /*********************************************************/
*           /*                                                       */
*           /* OPEN THE OUTPUT DATA SET (INTRDR D.S.)                */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0128
*           REENTACB(1:LENACB)=IRDRACB(1:LENACB);/* COPY ACB TO    0128
*                                      REENTRANT CORE                */
         L     @10,LENACB                                          0128
         BCTR  @10,0                                               0128
         EX    @10,@SM00857                                        0128
*           RFY                                                    0129
*             R2 RESTRICTED;                                       0129
*           R2=ADDR(REENTACB);      /* GET ACB ADDRESS FOR OPEN      */
         LA    R2,REENTACB                                         0130
*           RFY                                                    0131
*             IFGACB BASED(R2);     /* USE MAP MACRO FOR REENT ACB   */
*           ACBDDNM=S99TUPAR(1:S99TULNG);/* PUT THE INTRDR'S DDNAME
*                                      INTO THE ACB                  */
         MVI   ACBDDNM+1(R2),C' '                                  0132
         MVC   ACBDDNM+2(6,R2),ACBDDNM+1(R2)                       0132
         L     @10,TPRTDDN                                         0132
         LH    @07,S99TULNG(,@10)                                  0132
         BCTR  @07,0                                               0132
         EX    @07,@SM00859                                        0132
*           RENOPENL(1:LENOPEN)=OPENLIST(1:LENOPEN);/* COPY LIST FORM
*                                      OF OPEN MACRO TO REENTRANT  0133
*                                      CORE                          */
         L     @10,LENOPEN                                         0133
         BCTR  @10,0                                               0133
         EX    @10,@SM00861                                        0133
*           RFY                                                    0134
*             R1 RSTD;                                             0134
*           R1=ADDR(RENOPENL);                                     0135
         LA    R1,RENOPENL                                         0135
*           GEN(OPEN  ((R2)),MF=(E,(R1)));/* OPEN THE ACB FOR INTRDR */
         OPEN  ((R2)),MF=(E,(R1))
*           RFY                                                    0137
*             R1 UNRSTD;                                           0137
*           RFY                                                    0138
*             R15 RSTD;                                            0138
*                                                                  0138
*           /*********************************************************/
*           /*                                                       */
*           /* ERROR IF OPEN FAILED                                  */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0139
*           IF R15^=OPENRCOK THEN                                  0139
         LTR   R15,R15                                             0139
         BZ    @RF00139                                            0139
*             DO;                   /* ISSUE OPEN ERROR MESSAGE    0140
*                                      WHETHER OR NOT ACBOPEN BIT IS
*                                      ON                            */
*               GFPARMS=GFPARMS&&GFPARMS;/* ZERO OUT VSAMFAIL      0141
*                                      PARAMETER LIST (GENERAL FAIL
*                                      LIST)                         */
         XC    GFPARMS(44),GFPARMS                                 0141
*               GFCALLID=GFOPEN;    /* INDICATE OPEN MACRO FAILED    */
         MVC   GFCALLID(2),@CH00549                                0142
*               CALL ACBERMSG;      /* ISSUE THE ERROR MESSAGE       */
         BAL   @14,ACBERMSG                                        0143
*               RFY                                                0144
*                 R15 UNRSTD;                                      0144
*             END;                                                 0145
*           ELSE                                                   0146
*             DO;                   /* OPENOK DO/END GROUP           */
*                                                                  0146
         B     @RC00139                                            0146
@RF00139 DS    0H                                                  0147
*               /*****************************************************/
*               /*                                                   */
*               /* NOW READ AND PROCESS INPUT DATA SETS (CALL        */
*               /* IKJEFF05)                                         */
*               /*                                                   */
*               /*****************************************************/
*                                                                  0147
*               CTACBPT=R2;         /* SAVE ADDRESS OF INTRDR'S ACB
*                                      (STAI RTN IKJEFF20 CHECKS IT) */
         ST    R2,CTACBPT(,CONLIST)                                0147
*               REENTRPL(1:LENRPL)=IRDRRPL(1:LENRPL);/* COPY RPL TO
*                                      REENTRANT CORE                */
         L     @10,LENRPL                                          0148
         BCTR  @10,0                                               0148
         EX    @10,@SM00863                                        0148
*               RFY                                                0149
*                 IFGRPL BASED(ADDR(REENTRPL));/* USE MAP MACRO FOR
*                                      REENT RPL                     */
*               RPLDACB=R2;         /* PUT ADDRESS OF OPENED ACB INTO
*                                      RPL                           */
         LA    @10,REENTRPL                                        0150
         ST    R2,RPLDACB(,@10)                                    0150
*               RPLID='C0'X;        /* SET ID SINCE RPL MACRO DOESN'T*/
         MVI   RPLID(@10),X'C0'                                    0151
*               CTRPLPT=ADDR(REENTRPL);/* SAVE ADDRESS OF INTRDR'S 0152
*                                      RPL (IKJEFF12 USES FOR PUTS 0152
*                                      AND ENDREQS)                  */
         ST    @10,CTRPLPT(,CONLIST)                               0152
*               RFY                                                0153
*                 R2 UNRESTRICTED;                                 0153
*               INOINIT=OFF;        /* EXITING FROM IKJEFF15         */
         NI    INOINIT(HTPTR),B'11111110'                          0154
*               CALL IKJEFF05       /* GO TO SUBMIT READER MODULE    */
*                   (CONTAB);       /* PASS SUBMIT CONTROL TABLE     */
         ST    CONLIST,@AL00001                                    0155
         L     @15,@CV00069                                        0155
         LA    @01,@AL00001                                        0155
         BALR  @14,@15                                             0155
*               INOINIT=ON;         /* INDICATE IN IKJEFF15 NOW      */
         OI    INOINIT(HTPTR),B'00000001'                          0156
*               HTCODE=CLOSCODE;    /* NOW WILL CLOSE THE INTRDR D.S.*/
         MVI   HTCODE(HTPTR),X'04'                                 0157
*             END;                  /* OPENOK DO/END GROUP           */
*         END;                      /* ALLOCOK DO/END GROUP          */
@RC00139 DS    0H                                                  0160
*     END;                          /* DOALLOC DO/END GROUP        0160
*                                      (OPENCODE)                  0160
*                                                                    */
@RC00114 DS    0H                                                  0161
*                                                                  0161
*   /*****************************************************************/
*   /*                                                               */
*   /* CLOSE THE INTRDR DATA SET                                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0161
*   IF HTCODE=CLOSCODE THEN         /* THE STAI ROUTINE (IKJEFF20) 0161
*                                      AND THIS MODULE SET CLOSCODE  */
@RF00094 CLI   HTCODE(HTPTR),4                                     0161
         BNE   @RF00161                                            0161
*     DO;                           /* DOCLOSE DO/END GROUP          */
*       RFY                                                        0163
*        (R1,                                                      0163
*         R2) RESTRICTED;                                          0163
*       R2=CTACBPT;                 /* MUST GET ACB ADDRESS FROM   0164
*                                      CONTROL TABLE, NOT          0164
*                                      ADDR(REENTACB), SINCE STAI  0164
*                                      ROUTINE IKJEFF20 COULD CALL 0164
*                                      IKJEFF15 TO CLOSE ORIGINAL ACB*/
         L     R2,CTACBPT(,CONLIST)                                0164
*       CTACBPT=ZEROADDR;           /* INDICATE CLOSE IS DONE (STAI
*                                      ROUTINE IKJEFF20 CHECKS THIS
*                                      FIELD AND DON'T WANT 20 TO  0165
*                                      RETRY CLOSE IF CLOSE ABENDS)  */
         SLR   @10,@10                                             0165
         ST    @10,CTACBPT(,CONLIST)                               0165
*       RENOPENL(1:LENOPEN)=OPENLIST(1:LENOPEN);/* COPY LIST FORM OF
*                                      OPEN MACRO TO REENTRANT CORE  */
         L     @10,LENOPEN                                         0166
         BCTR  @10,0                                               0166
         EX    @10,@SM00861                                        0166
*       R1=ADDR(RENOPENL);          /* CAN USE OPEN LIST FOR CLOSE   */
         LA    R1,RENOPENL                                         0167
*       GEN(CLOSE ((R2)),MF=(E,(R1)));/* CLOSE THE ACB FOR INTRDR    */
         CLOSE ((R2)),MF=(E,(R1))
*       RFY                                                        0169
*         R1 UNRESTRICTED;                                         0169
*       RFY                                                        0170
*         R15 RSTD;                                                0170
*                                                                  0170
*       /*************************************************************/
*       /*                                                           */
*       /* ERROR IF CLOSE FAILED                                     */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0171
*       IF R15^=CLOSRCOK THEN                                      0171
         LTR   R15,R15                                             0171
         BZ    @RF00171                                            0171
*         DO;                                                      0172
*           GFPARMS=GFPARMS&&GFPARMS;/* ZERO OUT VSAMFAIL PARAMETER
*                                      LIST (GENERAL FAIL LIST)      */
         XC    GFPARMS(44),GFPARMS                                 0173
*           GFCALLID=GFCLOSE;       /* INDICATE A CLOSE MACRO FAILED */
         MVC   GFCALLID(2),@CH00083                                0174
*           CALL ACBERMSG;          /* ISSUE THE ERROR MESSAGE       */
         BAL   @14,ACBERMSG                                        0175
*           RFY                                                    0176
*            (R2,                                                  0176
*             R15) UNRSTD;                                         0176
*         END;                                                     0177
*     END;                          /* DOCLOSE DO/END GROUP        0178
*                                                                    */
@RF00171 DS    0H                                                  0179
*                                                                  0179
*   /*****************************************************************/
*   /*                                                               */
*   /* ERROR IF INVALID ENTRY CODE                                   */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0179
*   IF^(HTCODE=OPENCODE×HTCODE=CLOSCODE) THEN                      0179
@RF00161 CLI   HTCODE(HTPTR),1                                     0179
         BE    @RF00179                                            0179
         CLI   HTCODE(HTPTR),4                                     0179
         BE    @RF00179                                            0179
*     DO;                                                          0180
*       MSGID='12C';                /* MESSAGE FOR INVALID ENTRY CODE*/
         MVI   MSGID+3(MSGPTR),C' '                                0181
         MVC   MSGID(3,MSGPTR),@CC00850                            0181
*       L1=ZEROLEN;                 /* DON'T INSERT A JOB NAME       */
         MVI   L1(MSGPTR),X'00'                                    0182
*       L2=LENGTH(MODNAME);         /* INSERT THE MODULE NAME        */
         MVI   L2(MSGPTR),X'08'                                    0183
*       VAR2=ADDR(MODNAME);                                        0184
         LA    @10,MODNAME                                         0184
         STCM  @10,7,VAR2(MSGPTR)                                  0184
*       RFY                                                        0185
*         R1 RSTD;                                                 0185
*       R1=ADDR(MSGTABLE);          /* PTR TO IKJEFF02'S PARAM LIST  */
         LR    R1,MSGPTR                                           0186
*       CALL IKJEFF02;              /* ISSUE THE ERROR MESSAGE       */
         L     @15,MSGRTN(,MSGPTR)                                 0187
         BALR  @14,@15                                             0187
*       RFY                                                        0188
*         R1 UNRSTD;                                               0188
*       QUIT=ON;                    /* INDICATE SUBMIT FAILED        */
         OI    QUIT(HTPTR),B'10000000'                             0189
*     END;                                                         0190
*   INOINIT=OFF;                    /* INDICATE NOT IN IKJEFF15    0191
*                                                                    */
@RF00179 NI    INOINIT(HTPTR),B'11111110'                          0191
