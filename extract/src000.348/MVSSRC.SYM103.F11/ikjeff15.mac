*   /*****************************************************************/
*   /*                                                               */
*   /* ACBERMSG INTERNAL PROCEDURE - CALLS IKJEFF19 TO ISSUE VSAMFAIL*/
*   /* MESSAGE FOR OPEN OR CLOSE MACRO (CODE CALLING ACBERMSG MUST   */
*   /* ZERO GFPARMS AND SET GFCALLID)                                */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0192
*ACBERMSG:                                                         0192
*   PROC;                                                          0192
@EL00001 L     @13,4(,@13)                                         0192
@EF00001 L     @00,@SIZDATD                                        0192
         LR    @01,@11                                             0192
         FREEMAIN R,LV=(0),A=(1)
@ER00001 LM    @14,@12,12(@13)                                     0192
         BR    @14                                                 0192
@PB00001 DS    0H                                                  0192
ACBERMSG STM   @14,@12,@SA00002                                    0192
*   RFY                                                            0193
*    (R2,                                                          0193
*     R15) RSTD;                                                   0193
*   GFRCODE=R15;                    /* SAVE MACRO RETURN CODE        */
         ST    R15,GFRCODE                                         0194
*   GFCBPTR=R2;                     /* ACB IS FAILING VSAM C.B.      */
         ST    R2,GFCBPTR                                          0195
*   RFY                                                            0196
*    (R2,                                                          0196
*     R15) UNRSTD;                                                 0196
*   IF MTKEY1SW=ON THEN             /* IF RUNNING IN SCHEDULER KEY   */
         TM    MTKEY1SW(MSGPTR),B'00001000'                        0197
         BNO   @RF00197                                            0197
*     GFKEYN08=ON;                  /* TELL IKJEFF19 TO DO MODESET   */
         OI    GFKEYN08,B'10000000'                                0198
*   GF02PTR=ADDR(IKJEFF02);         /* HAVE ADDRESS OF MSG ISSUER    */
@RF00197 L     @10,MSGRTN(,MSGPTR)                                 0199
         ST    @10,GF02PTR                                         0199
*   GFSUBSYS=ON;                    /* USING VSAM SUBSYSTEM INTERFACE*/
         OI    GFSUBSYS,B'01000000'                                0200
*   GFCPPLP=MTCPPL;                 /* PASS PTR TO TMP'S CPPL C.B. 0201
*                                      (NEED FOR PUTLINE)            */
         L     @10,MTCPPL(,MSGPTR)                                 0201
         ST    @10,GFCPPLP                                         0201
*   GFECBP=ECBPTR;                  /* USE SUBMIT-SUPPLIED ECB FOR 0202
*                                      PUTLINE                       */
         L     @10,ECBPTR(,MSGPTR)                                 0202
         ST    @10,GFECBP                                          0202
*   GFDSNLEN=LENGTH(INTRDRDS);      /* PASS 'INTRDR' NAME TO IKJEFF19
*                                      FOR DATA SET NAME MSG INSERT  */
         MVC   GFDSNLEN(2),@CH00549                                0203
*   GFDSNP=ADDR(INTRDRDS);                                         0204
         LA    @10,INTRDRDS                                        0204
         ST    @10,GFDSNP                                          0204
*   GFPGMNL=LENGTH(CTCMDNM);        /* WILL INSERT COMMAND NAME      */
         MVC   GFPGMNL(2),@CH00042                                 0205
*   GFPGMNP=ADDR(CTCMDNM);                                         0206
*                                                                  0206
         LA    @10,CTCMDNM(,CONLIST)                               0206
         ST    @10,GFPGMNP                                         0206
*   /*****************************************************************/
*   /*                                                               */
*   /* CALL VSAMFAIL                                                 */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0207
*   RFY                                                            0207
*     R0 RSTD;                                                     0207
*   GEN(LOAD  EP=IKJEFF19);         /* LOAD THE VSAMFAIL ROUTINE     */
         LOAD  EP=IKJEFF19
*   CALL IKJEFF19(GFPARMS);         /* ISSUE THE ERROR MESSAGE       */
         LA    @10,GFPARMS                                         0209
         ST    @10,@AL00001                                        0209
         LR    @15,R0                                              0209
         LA    @01,@AL00001                                        0209
         BALR  @14,@15                                             0209
*   GEN(DELETE EP=IKJEFF19);        /* DELETE THE VSAMFAIL MODULE    */
         DELETE EP=IKJEFF19
*   RFY                                                            0211
*     R0 UNRSTD;                                                   0211
*   QUIT=ON;                        /* INDICATE SUBMIT FAILED        */
         OI    QUIT(HTPTR),B'10000000'                             0212
*   END ACBERMSG;                                                  0213
@EL00002 DS    0H                                                  0213
@EF00002 DS    0H                                                  0213
@ER00002 LM    @14,@12,@SA00002                                    0213
         BR    @14                                                 0213
