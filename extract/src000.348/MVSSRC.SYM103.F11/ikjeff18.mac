@DL00550 LA    @07,VOLNAME-1(J)                                    0550
         CLI   0(@07),C' '                                         0550
         BNE   @DC00550                                            0550
*   END;                                                           0551
         BCTR  J,0                                                 0551
         LTR   J,J                                                 0551
