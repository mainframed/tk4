*/********************************************************************/
*/*     FINDTU - THIS PROCEDURE LOCATES THE TEXT UNITS NEEDED FOR    */
*/*              USE AS MESSAGE VARIABLES OR THOSE TEXT UNITS        */
*/*              NECESSARY TO CHECK THE STATUS OF A GIVEN REQUEST.   */
*/*              THE TUKEY TABLE CONTAINS ALL THE TEXT UNIT KEYS     */
*/*              NEEDED BY IKJEFF18, HOWEVER ONLY CERTAIN KEYS MAY   */
*/*              BE NEEDED DEPENDING ON THE DYNAMIC ALLOCATION VERB  */
*/*              OPERATION CODE.  THE TUSELECT TABLE IS USED TO      */
*/*              CONTROL THE SEARCH MECHANISM TO ONLY LOCATE         */
*/*              PARTICULAR TEXT UNITS ACCORDING TO THE DYNAMIC      */
*/*              ALLOCATION VERB OPERATION CODE (ALSO THIS ALLOWS    */
*/*              EASE IN UPDATING WHEN MORE KEYS ARE REQUIRED BY     */
*/*              IKJEFF18).  INDEX VARIABLE K IS SET TO THE PROPER   */
*/*              SELECTION SEQUENCE IN THE TUSELECT TABLE UPON ENTRY */
*/*              ACCORDING TO THE DYNAMIC ALLOCATION VERB OPERATION  */
*/*              CODE.  EACH SELECTION SEQUENCE ENDS IN ZERO WHICH   */
*/*              TERMINATES FINDTU PROC.  IF A TEXT UNIT IS          */
*/*              IDENTIFIED AS ONE REQUIRED BY IKJEFF18, THE TEXT    */
*/*              UNIT POINTER IS SAVED IN THE CORRESPONDING SLOT IN  */
*/*              THE TUTAB TABLE THEN J IS SET TO ZERO.              */
*/*       OUTPUT - THE TUTAB CONTAINS THE ADDRESSES OF THOSE TEXT    */
*/*                UNITS NEEDED FOR USE AS MESSAGE VARIABLES OR      */
*/*                THOSE TEXT UNITS NECESSARY TO DETERMINE THE       */
*/*                STATUS OF A DYNAMIC ALLOCATION REQUEST.           */
*/********************************************************************/
*                                                                  0639
*FINDTU:                                                           0639
*   PROC OPTIONS(SAVE(14));                                        0639
FINDTU   ST    @14,12(,@13)                                        0639
*   CYCLE=ONE;                      /* VARIABLE USED FOR LOOP CONTROL*/
         LA    @03,1                                               0640
         LR    CYCLE,@03                                           0640
*   I=ONE;                          /* INITIALIZE I TO ALWAYS GET  0641
*                                      FIRST TEXT UNIT POINTER       */
         LR    I,@03                                               0641
*   SAVEINDX=K;                     /* SAVE PRESET SELECTION SEQUENCE
*                                      NUMBER                        */
         LR    SAVEINDX,K                                          0642
*   RFY                                                            0643
*     S99TUNIT BASED(S99TUPTR(I));  /* SET BASE FOR THE TEXT UNITS   */
*   DO WHILE(CYCLE=1);                                             0644
         B     @DE00644                                            0644
@DL00644 DS    0H                                                  0645
*     IF(S99TUPTR(I)&AND3)^=ZERO THEN/* MAKE SURE THE TEXT           */
         LR    @03,I                                               0645
         SLA   @03,2                                               0645
         L     @15,S99RBPTR                                        0645
         L     @01,S99TXTPP(,@15)                                  0645
         AL    @01,@CF01933                                        0645
         L     @03,S99TUPTR(@03,@01)                               0645
         LR    @14,@03                                             0645
         N     @14,@CF00179                                        0645
         LTR   @14,@14                                             0645
         BZ    @RF00645                                            0645
*       DO;                         /* UNIT POINTER IS VALID         */
*         IF BADTUFLG=YES THEN      /* IF THE ERROR IS AN INVALID    */
         TM    BADTUFLG,B'01000000'                                0647
         BNO   @RF00647                                            0647
*           IF S99TUKEY=S99INFO THEN/* PARAMETER LIST CHECK          */
         CLC   S99TUKEY(2,@03),S99INFO(@15)                        0648
         BNE   @RF00648                                            0648
*             DO;                   /* CURRENT TEXT UNIT KEY AGAINST
*                                      THE TEXT UNIT KEY NUMBER      */
*               VAR(20)=ADDR(S99TUKEY);/* FOUND BY DYNAMIC ALLO-     */
         ST    @03,VAR+76                                          0650
*               VARLEN(20)=LENGTH(S99TUKEY);/* CATION TO BE IN       */
         MVI   VARLEN+76,X'02'                                     0651
*               VARHEX(20)=YES;     /* ERROR-WHEN THEY ARE THE SAME  */
         OI    VARHEX+76,B'10000000'                               0652
*               RETURN;             /* RETURN BECAUSE ANY REMAINING
*                                      TEXT UNITS WERE NOT SYNTAX  0653
*                                      CHECKED BY DYNAMIC ALLOCATION */
@EL00004 DS    0H                                                  0653
@EF00004 DS    0H                                                  0653
@ER00004 L     @14,12(,@13)                                        0653
         BR    @14                                                 0653
*             END;                                                 0654
*         J=TUSELECT(K);            /* J IS USED TO INDEX INTO THE   */
@RF00648 DS    0H                                                  0655
@RF00647 SLR   J,J                                                 0655
         IC    J,TUSELECT-1(K)                                     0655
*         DO WHILE(J^=0);           /* TABLE OF KEY NUMBERS (THOSE 0656
*                                      KEYS THAT MAY BE USED AS    0656
*                                      MESSAGE VARIABLES OR THOSE  0656
*                                      NEEDED FOR CHECKING THE STATUS
*                                      OF A DYNAMIC ALLOC REQUEST) 0656
*                                      WHEN J GOES TO ZERO NO OTHER
*                                      KEYS ARE REQUIRED             */
         B     @DE00656                                            0656
@DL00656 DS    0H                                                  0657
*           IF S99TUKEY=TUKEY(J) THEN/* CHECK FOR A REQUIRED         */
         LR    @15,I                                               0657
         SLA   @15,2                                               0657
         L     @01,S99RBPTR                                        0657
         L     @01,S99TXTPP(,@01)                                  0657
         AL    @01,@CF01933                                        0657
         L     @15,S99TUPTR(@15,@01)                               0657
         LR    @14,J                                               0657
         ALR   @14,@14                                             0657
         LA    @01,TUKEY-2(@14)                                    0657
         CLC   S99TUKEY(2,@15),0(@01)                              0657
         BNE   @RF00657                                            0657
*             DO;                   /* KEY NUMBER                    */
*               TUTAB(J)=S99TUPTR(I);                              0659
         LR    @14,J                                               0659
         SLA   @14,2                                               0659
         ST    @15,TUTAB-4(@14)                                    0659
*               J=ZERO;                                            0660
         SLR   J,J                                                 0660
*             END;                                                 0661
*           ELSE                    /* IF NOT A REQUIRED KEY THEN    */
*             DO;                   /* CHECK NEXT TEXT UNIT          */
         B     @RC00657                                            0662
@RF00657 DS    0H                                                  0663
*               K=ONE+K;                                           0663
         AL    K,@CF00033                                          0663
*               J=TUSELECT(K);                                     0664
         SLR   J,J                                                 0664
         IC    J,TUSELECT-1(K)                                     0664
*             END;                                                 0665
*         END;                                                     0666
@RC00657 DS    0H                                                  0666
@DE00656 LTR   J,J                                                 0666
         BNZ   @DL00656                                            0666
*       END;                                                       0667
*     IF S99TUPLN(I)=YES THEN       /* IF THIS IS THE LAST TEXT UNIT */
@RF00645 LR    @05,I                                               0668
         SLA   @05,2                                               0668
         L     @03,S99RBPTR                                        0668
         L     @03,S99TXTPP(,@03)                                  0668
         ALR   @03,@05                                             0668
         AL    @03,@CF01933                                        0668
         TM    S99TUPLN(@03),B'10000000'                           0668
         BNO   @RF00668                                            0668
*       CYCLE=ZERO;                 /* THEN SET LOOP CONTROL TO      */
         SLR   CYCLE,CYCLE                                         0669
*     I=ONE+I;                      /* EXIT FROM FINDTU              */
@RF00668 AL    I,@CF00033                                          0670
*     K=SAVEINDX;                                                  0671
         LR    K,SAVEINDX                                          0671
*   END;                                                           0672
@DE00644 C     CYCLE,@CF00033                                      0672
         BE    @DL00644                                            0672
*   END;                            /* END OF FINDTU PROC            */
         B     @EL00004                                            0673
