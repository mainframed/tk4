IKJEFF18 CSECT
*   INTFPTR=R1;                     /* SET UP BASE FOR INPUT PARMS   */
         LR    INTFPTR,R1                                          0108
*   RFY                                                            0109
*     R1 UNRSTD;                                                   0109
*   RCODE=DFRC;                     /* MAKE LOCAL, FULL WORD COPY OF
*                                      THE BASIC RETURN CODE --    0110
*                                      DYNAMIC ALLOCATION OR DAIR, 0110
*                                      WITH HIGH ORDER ZEROES        */
         L     @05,DFRCP(,INTFPTR)                                 0110
         L     @05,DFRC(,@05)                                      0110
         ST    @05,RCODE                                           0110
*   MSGTABLE=MSGTABLE&&MSGTABLE;    /* INITIALIZE MSG WTR INTERFACE  */
         XC    MSGTABLE(84),MSGTABLE                               0111
*   DARET=FCODE;                                                   0112
         MVC   DARET(4),FCODE                                      0112
*   IF DFWTP=YES THEN               /* IF A WTP HAS BEEN REQUESTED   */
         L     @05,DFIDP(,INTFPTR)                                 0113
         TM    DFWTP(@05),B'10000000'                              0113
         BNO   @RF00113                                            0113
*     MTWTPSW=YES;                  /* INDICATE WTP TO IKJEFF02      */
         OI    MTWTPSW,B'00000010'                                 0114
*   ELSE                            /* OTHERWISE USE THE DEFAULT OF  */
*     MTPUTLSW=YES;                 /* A PUTLINE REQUEST             */
         B     @RC00113                                            0115
@RF00113 OI    MTPUTLSW,B'01000000'                                0115
*   MTHEXSW=YES;                    /* INDICATE TO IKJEFF02 TO PRINT
*                                      RETURN CODES IN HEX           */
@RC00113 OI    MTHEXSW,B'00010000'                                 0116
*   FLGS=CODE0000;                                                 0117
         MVC   FLGS(1),CODE0000                                    0117
*   DO I=1 TO DIM(VAR);                                            0118
         LA    I,1                                                 0118
@DL00118 DS    0H                                                  0119
*     VAR(I)=ZERO;                  /* ZERO OUT MESSAGE VARIABLE   0119
*                                      ARRAY                         */
         LR    @05,I                                               0119
         SLA   @05,2                                               0119
         SLR   @03,@03                                             0119
         ST    @03,VAR-4(@05)                                      0119
*   END;                                                           0120
*                                                                  0120
         AL    I,@CF00033                                          0120
         C     I,@CF00089                                          0120
         BNH   @DL00118                                            0120
*/********************************************************************/
*/*         INITIALIZE DEFAULT VALUES FOR MESSAGE INSERTS            */
*/********************************************************************/
*                                                                  0121
*   VAR(5)=ADDR(ERRCODE);           /* INSERT IF UNDEFINED   @ZA02550*/
         LA    @05,ERRCODE                                         0121
         ST    @05,VAR+16                                          0121
*   VARLEN(5)=LENGTH(ERRCODE);      /*                       @ZA02550*/
         MVI   VARLEN+16,X'04'                                     0122
*   VARHEX(5)=YES;                  /* WILL TELL IKJEFF02 TO CONVERT
*                                      R.C. TO PRINTABLE HEX @ZA02550*/
         OI    VARHEX+16,B'10000000'                               0123
*   VAR(9)=ADDR(DADSM);                                            0124
         LA    @05,@CC00290                                        0124
         ST    @05,VAR+32                                          0124
*   VARLEN(9)=LENGTH(DADSM);                                       0125
         MVI   VARLEN+32,X'05'                                     0125
*   VAR(10)=ADDR(IKJ562);                                          0126
         LA    @05,@CC00275                                        0126
         ST    @05,VAR+36                                          0126
*   VARLEN(10)=LENGTH(IKJ562);                                     0127
         MVI   VARLEN+36,X'06'                                     0127
*   VAR(11)=ADDR(DATASET);                                         0128
         LA    @05,@CC00263                                        0128
         ST    @05,VAR+40                                          0128
*   VARLEN(11)=LENGTH(DATASET);                                    0129
         MVI   VARLEN+40,X'08'                                     0129
*   VAR(15)=ADDR(CATALOG);                                         0130
         LA    @05,@CC00256                                        0130
         ST    @05,VAR+56                                          0130
*   VARLEN(15)=LENGTH(CATALOG);                                    0131
         MVI   VARLEN+56,X'07'                                     0131
*   VAR(17)=ADDR(INVALID);                                         0132
         LA    @05,@CC00320                                        0132
         ST    @05,VAR+64                                          0132
*   VARLEN(17)=LENGTH(INVALID);                                    0133
         MVI   VARLEN+64,X'07'                                     0133
*   VAR(2)=ADDR(REQVAR);                                           0134
         LA    @05,@CC00292                                        0134
         ST    @05,VAR+4                                           0134
*   VARLEN(2)=LENGTH(REQVAR);                                      0135
         MVI   VARLEN+4,X'07'                                      0135
*   VAR(3)=ADDR(PERFORM);                                          0136
         LA    @05,@CC00294                                        0136
         ST    @05,VAR+8                                           0136
*   VARLEN(3)=LENGTH(PERFORM);                                     0137
         MVI   VARLEN+8,X'09'                                      0137
*   VAR(12)=ADDR(ATTRLIST);                                        0138
         LA    @05,@CC00286                                        0138
         ST    @05,VAR+44                                          0138
*   VARLEN(12)=LENGTH(ATTRLIST);                                   0139
         MVI   VARLEN+44,X'0E'                                     0139
*                                                                  0140
*/********************************************************************/
*/*         IDENTIFY THE INTERFACE OF CALLER                         */
*/********************************************************************/
*                                                                  0140
*   IF IDNUM=ONE THEN               /* IF CALLER NUMBER IS ONE THE   */
         L     @05,DFIDP(,INTFPTR)                                 0140
         CLI   IDNUM(@05),1                                        0140
         BE    @RT00140                                            0140
*     GO TO DAIRINIT;               /* USER HAS A DAIR ERROR         */
*   IF IDNUM^=FIFTY&                /* IF CALLER NUMBER IS NOT A     */
*       IDNUM^=CON51 THEN           /* VALID ID NUMBER FOR AN SVC 99 */
         CLI   IDNUM(@05),50                                       0142
         BE    @RF00142                                            0142
         CLI   IDNUM(@05),51                                       0142
         BE    @RF00142                                            0142
*     DO;                                                          0143
*       DFRETCDE=FOUR;              /* ERROR, THEN RETURN WITH ERROR */
         LA    DFRETCDE,4                                          0144
*       GO TO EXIT;                 /* CODE INDICATING INVALID CALLER
*                                      NUMBER SPECIFIED              */
         B     EXIT                                                0145
*     END;                                                         0146
*                                                                  0146
*/********************************************************************/
*/*         DYNAMIC ALLOCATION ERROR PROCESSING                      */
*/********************************************************************/
*/*           INITIALIZATION                                         */
*/********************************************************************/
*                                                                  0147
*                                   /* INITIALIZE DEFAULT MESSAGE    */
*   VAR(1)=ADDR(DYNAMAL);           /* VARIABLES FOR SVC 99 ERRORS   */
@RF00142 LA    @05,@CC00258                                        0147
         ST    @05,VAR                                             0147
*   VARLEN(1)=LENGTH(DYNAMAL);                                     0148
         MVI   VARLEN,X'12'                                        0148
*   VAR(4)=VAR(1);                                                 0149
         L     @05,VAR                                             0149
         ST    @05,VAR+12                                          0149
*   VAR(18)=ADDR(CONTAIN);                                         0150
         LA    @05,@CC00322                                        0150
         ST    @05,VAR+68                                          0150
*   VARLEN(18)=LENGTH(CONTAIN);                                    0151
         MVI   VARLEN+68,X'08'                                     0151
*   VAR(19)=ADDR(TEXTUNIT);                                        0152
         LA    @05,@CC00324                                        0152
         ST    @05,VAR+72                                          0152
*   VARLEN(19)=LENGTH(TEXTUNIT);                                   0153
         MVI   VARLEN+72,X'09'                                     0153
*   S99RBPTR=DFS99RBP;              /* SAVE ADDRESS OF SVC 99 REQUEST
*                                      BLOCK POINTER                 */
         L     @05,DFS99RBP(,INTFPTR)                              0154
         ST    @05,S99RBPTR                                        0154
*   DUMECB=ZERO;                    /* INITIALIZE THE ECB FOR THE    */
         SLR   @05,@05                                             0155
         ST    @05,DUMECB                                          0155
*   ECBPTR=ADDR(DUMECB);            /* MESSAGE WRITER - IKJEFF02     */
         LA    @05,DUMECB                                          0156
         ST    @05,ECBPTR                                          0156
*   IF DFWTP=NO THEN                /* IF A PUTLINE IS TO BE ISSUED  */
         L     @05,DFIDP(,INTFPTR)                                 0157
         TM    DFWTP(@05),B'10000000'                              0157
         BNZ   @RF00157                                            0157
*     MTCPPL=DFCPPLP;               /* SAVE THE CPPL ADDRESS PASSED  */
         L     @05,DFCPPLP(,INTFPTR)                               0158
         ST    @05,MTCPPL                                          0158
*   DO I=1 TO DIM(TUTAB);           /* INITIALIZE THE TEXT UNIT      */
@RF00157 LA    I,1                                                 0159
@DL00159 DS    0H                                                  0160
*     TUTAB(I)=ZERO;                /* POINTER TABLE                 */
         LR    @05,I                                               0160
         SLA   @05,2                                               0160
         SLR   @03,@03                                             0160
         ST    @03,TUTAB-4(@05)                                    0160
*   END;                                                           0161
         AL    I,@CF00033                                          0161
         C     I,@CF00053                                          0161
         BNH   @DL00159                                            0161
*                                                                  0162
*/********************************************************************/
*/*           IDENTIFY THE DYNAMIC ALLOCATION RETURN CODE            */
*/********************************************************************/
*                                                                  0162
*   I=DYALSTRT;                     /* SET I TO START OF DYNAMIC   0162
*                                      ALLOCATION RETURN CODES       */
         LA    I,13                                                0162
*   ERRCODE=RETCODE;                /* PREPARE SEARCH ARGUMENT FOR   */
         MVC   ERRCODE(4),RETCODE                                  0163
*   CALL SEARCH;                    /* THE RETURN CODE IDENTIFY RTN  */
         BAL   @14,SEARCH                                          0164
*   IF CODES(I)=CODEFFFF THEN       /* IF NO MATCH WAS FOUND BYPASS  */
         LR    @05,I                                               0165
         MH    @05,@CH00359                                        0165
         LA    @03,CODES-3(@05)                                    0165
         CLC   0(2,@03),CODEFFFF                                   0165
         BE    @RT00165                                            0165
*     GO TO SELECT;                 /* THE SPECIALIZED MESSAGE     0166
*                                      VARIABLE INITIALIZATION AND 0166
*                                      TAKE THE DEFAULT MESSAGE      */
*   IF RCODE^=ZERO THEN             /* DETERMINE IF THE RETURN       */
         L     @05,RCODE                                           0167
         SLR   @03,@03                                             0167
         CR    @05,@03                                             0167
         BE    @RF00167                                            0167
*     IF RCODE^=FOUR THEN           /* CODE INDICATES ANY SPECIAL    */
         C     @05,@CF00064                                        0168
         BE    @RF00168                                            0168
*       DO;                         /* CONDITIONS                    */
*         IF RCODE=EIGHT THEN       /* RETURN CODE OF 8 INDICATES    */
         C     @05,@CF00053                                        0170
         BE    @RT00170                                            0170
*           GO TO SELECT;           /* THE REQUEST WAS DENIED BY THE
*                                      INSTALLATION EXIT - BYPASS ANY
*                                      FURTHER ERROR ANALYSIS        */
*         ELSE                      /* RETURN CODE                   */
*           IF RCODE=CON12&         /* 12 INDICATES AN               */
*               S99INFO^=ZERO THEN  /* INVALID PARAMETER LIST - WHEN */
         C     @05,@CF00195                                        0172
         BNE   @RF00172                                            0172
         L     @05,S99RBPTR                                        0172
         LH    @05,S99INFO(,@05)                                   0172
