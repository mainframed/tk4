*   IF SYSOUTTU^=ZERO THEN          /* WHEN A SYSOUT DATA SET WAS    */
@RF00606 L     @05,SYSOUTTU                                        0611
         LTR   @05,@05                                             0611
         BZ    @RF00611                                            0611
*     DO;                           /* REQUESTED USE SYSOUT AND      */
*       VAR(1)=ADDR(SYSOUT);        /* DATA SET IN THE MESSAGE       */
         LA    @05,@CC00307                                        0613
         ST    @05,VAR                                             0613
*       VARLEN(1)=LENGTH(SYSOUT);                                  0614
         MVI   VARLEN,X'06'                                        0614
