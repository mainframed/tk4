*/********************************************************************/
*/*                                                                  */
*/*  STATMSG INTERNAL PROCEDURE                                      */
*/*                                                                  */
*/*  - SETS UP JOBNAME OR JOBNAME(JOBID) INSERT FOR THE STATUS       */
*/*   MESSAGE, CALLS THE MESSAGE ISSUER ROUTINE, AND CHECKS ITS      */
*/*   RETURN CODE.                                                   */
*/*  - NOTE JOBNINST, JOBIINST, LFTPAREN, RTPAREN, AND VAR1 ARE      */
*/*   PREVIOUSLY SET.                                                */
*/*                                                                  */
*/********************************************************************/
*                                                                  0323
*STATMSG:                                                          0323
*   PROC OPTIONS(SAVE(14));         /* JUST SAVE REGISTER 14         */
STATMSG  ST    @14,@SA00004                                        0323
*   VAR1=ADDR(JOBINSRT);            /* INITIALIZE POINTER TO JOBNAME
*                                      OR JOBNAME(JOBID) INSERT FOR
*                                      STATUS MESSAGE                */
         LA    @05,JOBINSRT                                        0324
         STCM  @05,7,VAR1(MTPTR)                                   0324
*   IF JOBIINST=ALLBLNKS THEN       /* NO JOBID SO                   */
         CLC   JOBIINST(8),ALLBLNKS                                0325
         BNE   @RF00325                                            0325
*     L1=LENGTH(JOBNINST);          /* JUST PUT JOBNAME IN MESSAGE   */
         MVI   L1(MTPTR),X'08'                                     0326
*   ELSE                            /* JOBNAME(JOBID) INSERT LENGTH  */
*     DO;                                                          0327
         B     @RC00325                                            0327
@RF00325 DS    0H                                                  0328
*       L1=LENGTH(JOBINSRT);                                       0328
         MVI   L1(MTPTR),X'12'                                     0328
*       MTJOBISW=ON;                /* TELL IKJEFF02 TO COMPRESS   0329
*                                      BLANKS OUT OF INSERT          */
         OI    MTJOBISW(MTPTR),B'00000100'                         0329
*     END;                                                         0330
*   RFY                                                            0331
*    (R1,                                                          0331
*     R15) RSTD;                    /* DON'T USE CALL02 - THIS CODE
*                                      IS MORE EFFICIENT             */
@RC00325 DS    0H                                                  0332
*   R1=ADDR(MSGTABLE);              /* PICK UP ADDR OF MSG PARAMETERS*/
         LR    R1,MTPTR                                            0332
*   CALL IKJEFF02;                  /* CALL MESSAGE ISSUER MODULE    */
         L     @15,MSGRTNPT(,CSPLPTR)                              0333
         BALR  @14,@15                                             0333
*   IF R15^=MSGRTOK THEN            /* CHECK FOR BAD PUTLINE R.C.    */
         LTR   R15,R15                                             0334
         BZ    @RF00334                                            0334
*     DO;                           /* - IF BAD R.C. -               */
*       ENDIDSW='1'B;               /* INDICATE GET OUT OF INNER   0336
*                                      (JOBID) LOOP                  */
         OI    ENDIDSW,B'00001000'                                 0336
*       QUIT='1'B;                  /* INDICATE ERROR EXIT           */
         L     @14,SWITPTR(,CSPLPTR)                               0337
         OI    QUIT(@14),B'00001000'                               0337
*     END;                                                         0338
*   ELSE                            /* O.K. RETURN                   */
*     MTJOBISW=OFF;                 /* MAY NOT NEED TO COMPRESS OUT
*                                      BLANKS NEXT TIME              */
         B     @RC00334                                            0339
@RF00334 NI    MTJOBISW(MTPTR),B'11111011'                         0339
*   RFY                                                            0340
*    (R1,                                                          0340
*     R15) UNRSTD;                                                 0340
*   END STATMSG;                                                   0341
@EL00004 DS    0H                                                  0341
@EF00004 DS    0H                                                  0341
@ER00004 L     @14,@SA00004                                        0341
         BR    @14                                                 0341
*   END IKJEFF52                                                   0342
