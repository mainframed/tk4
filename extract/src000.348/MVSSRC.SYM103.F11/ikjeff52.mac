         TITLE 'IKJEFF52 - STATUS COMMAND INTERFACE TO SUBSYSTEM       *
                        '
IKJEFF52 CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'IKJEFF52  74.093'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @12,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@12                                         0001
         L     @00,@SIZDATD                                        0001
         GETMAIN R,LV=(0)
         LR    @11,@01                                             0001
         USING @DATD,@11                                           0001
         ST    @13,@SA00001+4                                      0001
         LM    @00,@01,20(@13)                                     0001
         ST    @11,8(,@13)                                         0001
         LR    @13,@11                                             0001
         MVC   @PC00001(8),0(@01)                                  0001
DUMMY    DS    0H                                                  0003
*                                                                    */
*                                                                  0093
*   /*****************************************************************/
*   /*                                                               */
*   /* IKJEFF52 INITIALIZATION                                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0093
*   FIBCMPTR=ADDR(FIBPARML);        /* LOCAL POINTER TO INPUT      0093
*                                      FIBCPARM                      */
         L     FIBCMPTR,@PC00001                                   0093
*   CSPLPTR=ADDR(CSPARML);          /* LOCAL POINTER TO CANCEL/STATUS
*                                      PARMLIST - INSTEAD OF INPUT 0094
*                                      PTR                           */
         L     CSPLPTR,@PC00001+4                                  0094
*   MTPTR=MSGPTR;                   /* USE LOCAL POINTER TO MSGTABLE
*                                      FOR MORE EFFICIENT CODE       */
         L     MTPTR,MSGPTR(,CSPLPTR)                              0095
*   RESPECIFY                                                      0096
*    (MSGTABLE) BASED(MTPTR);                                      0096
*   INTSWS=INTSWS&&INTSWS;          /* CLEAR INTERNAL SWITCHES       */
         SLR   @15,@15                                             0097
         IC    @15,INTSWS                                          0097
         LR    @14,@15                                             0097
         XR    @14,@15                                             0097
         STC   @14,INTSWS                                          0097
*   LFTPAREN='(';                   /* INITIALIZE JOBNAME(JOBID)   0098
*                                      INSERT AREA FOR STATUS      0098
*                                      MESSAGES                      */
         MVI   LFTPAREN,C'('                                       0098
*   RTPAREN=')';                                                   0099
*                                                                  0099
         MVI   RTPAREN,C')'                                        0099
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE SSOB AND SSCS C.B. FOR JOB ENTRY SUBSYSTEM         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0100
*   SSOB=SSOB&&SSOB;                /* ZERO OUT THE SSOB             */
         XC    SSOB(20),SSOB                                       0100
*   SSOBINDV=ADDR(SSCSCORE);        /* PUT ADDR OF GOTTEN CORE FOR 0101
*                                      SSCS INTO SSOB                */
         LA    @15,SSCSCORE                                        0101
         ST    @15,SSOBINDV                                        0101
*   HEADERLN=LENGTH(SSCS)-32*(LENGTH(SSCSARAY));/* LENGTH OF SSCS  0102
*                                      HEADER                        */
         LA    HEADERLN,24                                         0102
*   SSCS(1:HEADERLN)=SSCS(1:HEADERLN)&&SSCS(1:HEADERLN);/* ZERO SSCS
*                                      HEADER AREA                   */
         LR    @14,HEADERLN                                        0103
         BCTR  @14,0                                               0103
         EX    @14,@SX00972                                        0103
*   SSOBLEN=LENGTH(SSOB);           /* SAVE LENGTH OF SSOB           */
         MVC   SSOBLEN(2),@CH00214                                 0104
*   SSOBFUNC=SSOBSTAT;              /* INDICATE A STATUS FUNCTION IS
*                                      BEING REQUESTED               */
         MVC   SSOBFUNC(2),@CH00108                                0105
*   SSCSLEN=LENGTH(SSCS);           /* SAVE SSCS LENGTH              */
         MVC   SSCSLEN(2,@15),@CH00969                             0106
*   CORELN=ZEROFLD;                 /* INITIALIZE THE AMOUNT OF    0107
*                                      EXPLICITLY GOTTEN CORE (FOR 0107
*                                      MORECORE SUBROUTINE)          */
         SLR   @14,@14                                             0107
         STCM  @14,7,CORELN                                        0107
*   HOLDARSZ=32*LENGTH(SSCSARAY);   /* SAVE SIZE OF JOBID ARRAY FOR
*                                      MORECORE RTN                  */
         LA    @14,512                                             0108
         ST    @14,HOLDARSZ                                        0108
*   SSCSDIMP=HOLDARSZ;              /* INDICATE SIZE OF ARRAY (FOR 0109
*                                      JOBID ANSWERS) PASSED TO    0109
*                                      SUBSYSTEM                     */
         STH   @14,SSCSDIMP(,@15)                                  0109
*   SSOBID='SSOB';                  /* SSOB CONTAINS ITS OWN NAME FOR
*                                      DEBUGGING                     */
         MVC   SSOBID(4),@CC00928                                  0110
*   SSOBPTR=ADDR(SSOB);                                            0111
         LA    @15,SSOB                                            0111
         ST    @15,SSOBPTR                                         0111
*   PTRHIGH='1'B;                   /* TURN ON HIGH-ORDER BIT OF PTR
*                                      FOR STANDARD LINKAGE        0112
*                                      CONVENTION                  0112
*                                                                    */
         OI    PTRHIGH,B'10000000'                                 0112
*                                                                  0113
*   /*****************************************************************/
*   /*                                                               */
*   /* LOOP TO PROCESS EACH JOBNAME IN INPUT LIST                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0113
*   DO T=1 TO TBLCOUNT WHILE(QUIT='0'B);                           0113
         LA    @15,1                                               0113
         B     @DE00113                                            0113
@DL00113 L     @15,SWITPTR(,CSPLPTR)                               0113
         TM    QUIT(@15),B'00001000'                               0113
         BNZ   @DC00113                                            0113
*     SSCSJOBN=JOBLISTP->JOBNMID(1);/* PICK UP JOBNAME               */
         L     @14,SSOBINDV                                        0114
         L     @07,JOBLISTP(,CSPLPTR)                              0114
         MVC   SSCSJOBN(8,@14),JOBNMID(@07)                        0114
*     SSCSJOBI=ALLBLNKS;            /* BLANK OUT JOBID FIRST         */
         MVC   SSCSJOBI(8,@14),ALLBLNKS                            0115
*     IF STATAUTO=ON THEN           /* IF STATUS COMMAND WITH NO   0116
*                                      OPERANDS                      */
         TM    STATAUTO(@15),B'00100000'                           0116
         BNO   @RF00116                                            0116
*       DO;                                                        0117
*         SSCSUSID=ON;              /* TELL THE SUBSYSTEM THAT IT  0118
*                                      MUST GET QUEUE STATUS OF ALL
*                                      JOBNAMES = TSO USERID PLUS ONE
*                                      CHARACTER (THE USERID IS IN 0118
*                                      THE SSCSJOBN FIELD ALREADY)   */
         OI    SSCSUSID(@14),B'10000000'                           0118
*         SSCSULEN=JOBLISTP->LEN1(1);/* GET THE USERID LENGTH FROM 0119
*                                      THE CANCEL/STATUS PARMLIST    */
         MVC   SSCSULEN(1,@14),LEN1(@07)                           0119
*       END;                                                       0120
*     ELSE                          /* STATUS COMMAND WITH OPERNAD(S)*/
*STATOPND:                          /* LABEL FOR DO/END GROUP        */
*       DO;                                                        0121
         B     @RC00116                                            0121
@RF00116 DS    0H                                                  0121
STATOPND DS    0H                                                  0122
*         IF JOBLISTP->LEN1(1)='44'X&IDUPDATE='0'B THEN/* PICK UP  0122
*                                      JOBID OF JOBNAME IF ONE IS  0122
*                                      AVAILABLE                     */
         L     @15,JOBLISTP(,CSPLPTR)                              0122
         CLI   LEN1(@15),X'44'                                     0122
         BNE   @RF00122                                            0122
         TM    IDUPDATE,B'00100000'                                0122
         BNZ   @RF00122                                            0122
*           DO;                                                    0123
*             JOBLISTP=JOBLISTP+LENGTH(JOBLIST);/* UPDATE TO POINT TO
*                                      JOBID IN TABLE                */
         AH    @15,@CH00131                                        0124
         ST    @15,JOBLISTP(,CSPLPTR)                              0124
*             SSCSJOBI=JOBLISTP->JOBNMID(1);/* PICK UP JOBID         */
         L     @01,SSOBINDV                                        0125
         MVC   SSCSJOBI(8,@01),JOBNMID(@15)                        0125
*             T=T+1;                /* PROCESSED ONE MORE ELEMENT IN
*                                      JOBNAME AND JOBID LIST        */
         LA    @15,1                                               0126
         AL    @15,T                                               0126
         ST    @15,T                                               0126
*           END;                                                   0127
*       END STATOPND;                                              0128
@RF00122 DS    0H                                                  0129
*                                                                  0129
*     /***************************************************************/
*     /*                                                             */
*     /* CALL SUBSYSTEM INTERFACE TO GET STATUS OF JOBNAME OR        */
*     /* JOBNAME(JOBID) - REENTER SUBSYSTEM FOR SAME JOBNAME IF NOT  */
*     /* ENOUGH SPACE IN SSCS ARRAY FOR ALL IDS OR IF INCOMPATIBLE   */
*     /* JOBNAME/JOBID (WILL GET STATUS FOR JOBNAME ONLY)            */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0129
*     CALLSW='1'B;                  /* IF ON, WILL CALL SUBSYSTEM  0129
*                                      INTERFACE                     */
@RC00116 OI    CALLSW,B'00010000'                                  0129
*     DO WHILE(CALLSW='1'B);        /* CODE TO CALL SUBSYSTEM        */
         B     @DE00130                                            0130
@DL00130 DS    0H                                                  0131
*       SSOBRETN=ZEROFLD;           /* ENSURE ZEROED R.C. FIELD      */
*                                                                  0131
         SLR   @15,@15                                             0131
         ST    @15,SSOBRETN                                        0131
*       /*************************************************************/
*       /*                                                           */
*       /* ZERO OUT THE AREA FOR SUBSYSTEM STATUS REPLIES            */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0132
*       RFY                                                        0132
*        (R2,                                                      0132
*         R3,                                                      0132
*         R4,                                                      0132
*         R5) RSTD;                 /* REGISTERS USED IN MVCL INSTR  */
*       R2=ADDR(SSCSARAY);          /* GET ADDRESS OF ARRAY (TO    0133
*                                      ADDRESS)                      */
         L     R2,SSOBINDV                                         0133
         LA    R2,SSCSARAY(,R2)                                    0133
*       IF CORELN^=ZEROFLD THEN     /* IF GOT CORE FOR LARGER ARRAY  */
         SLR   @15,@15                                             0134
         ICM   @15,7,CORELN                                        0134
         LTR   @15,@15                                             0134
         BZ    @RF00134                                            0134
*         R3=CORELN-HEADERLN;       /* SPECIFY ITS LENGTH (TO LENGTH)*/
         SLR   @15,HEADERLN                                        0135
         LR    R3,@15                                              0135
*       ELSE                        /* OTHERWISE ZERO ORIGINAL ARRAY */
*         R3=HOLDARSZ;                                             0136
         B     @RC00134                                            0136
@RF00134 L     R3,HOLDARSZ                                         0136
*       R4=ZEROFLD;                 /* FROM ADDRESS NOT NEEDED -   0137
*                                      PUTTING THE FROM LENGTH (R5)
*                                      TO ZERO TO CAUSE ZEROING OF 0137
*                                      ENTIRE TO AREA                */
@RC00134 SLR   R4,R4                                               0137
*       GEN SETS(R5)(SR    R5,R5);  /* NEED ALL ZEROS IN BITS 0-7 FOR
*                                      MASK AND ZERO FOR FROM LENGTH */
         SR    R5,R5
*       MVCL(R2,R4);                /* MOVE CHARACTER LONG         0139
*                                      INSTRUCTION TO ZERO OUT ENTIRE
*                                      SSCS ARRAY                    */
         MVCL  R2,R4                                               0139
*       RFY                                                        0140
*        (R2,                                                      0140
*         R3,                                                      0140
*         R4,                                                      0140
*         R5) UNRSTD;               /* END OF CODE TO ZERO OUT ARRAY
*                                                                    */
*                                                                  0141
*       /*************************************************************/
*       /*                                                           */
*       /* NOW CALL THE SUBSYSTEM INTERFACE ROUTINE                  */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0141
*       RFY                                                        0141
*        (R1,                                                      0141
*         R15) RSTD;                                               0141
*       R1=ADDR(SSOBPTR);           /* PLACE A PTR TO THE PTR OF THE
*                                      CONTROL BLOCK IN REGISTER ONE */
*                                                                  0142
         LA    R1,SSOBPTR                                          0142
*       /*************************************************************/
*       /*                                                           */
*       /* ? IEFSSREQ;                                               */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0143
*/*                                                                  */
*/*  PASS CONTROL TO JOB ENTRY SUBSYSTEM TO                          */
*/*  PROCESS REQUEST -                                               */
*/*                                                                  */
*/*    INPUT - REGISTER 1 POINTS TO A ONE-                           */
*/*            WORD PARAMETER LIST WHICH                             */
*/*            POINTS TO THE SSOB                                    */
*/*                                                                  */
*                                                                  0143
*       CALL SSREQ;                 /* CALL SUBSYSTEM INTFC        0143
*                                                                    */
         L     @14,CVTPTR                                          0143
         L     @14,CVTJESCT(,@14)                                  0143
         L     @15,JESSSREQ(,@14)                                  0143
         BALR  @14,@15                                             0143
*                                                                  0144
*       /*************************************************************/
*       /*                                                           */
*       /* DETERMINE THE RETURN CODES                                */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0144
*       SAVECODE=R15;               /* SAVE RETURN CODE FROM THE   0144
*                                      SUBSYSTEM INTERFACE ROUTINE   */
         ST    R15,SAVECODE                                        0144
*       RFY                                                        0145
*        (R1,                                                      0145
*         R15) UNRSTD;                                             0145
*                                                                  0145
*       /*************************************************************/
*       /*                                                           */
*       /* END LOOP OF GETTING MORE CORE IF SUBSYSTEM GAVE REPLY     */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0146
*       IF SSYSCODE^=SSCSMALL×SAVECODE^=SSRTOK THEN/* IF HAVE BIG  0146
*                                      ENOUGH SSCS C.B. OR INTERFACE
*                                      ERROR                         */
         CLC   SSYSCODE(4),@CF00212                                0146
         BNE   @RT00146                                            0146
         L     @15,SAVECODE                                        0146
         LTR   @15,@15                                             0146
         BZ    @RF00146                                            0146
@RT00146 DS    0H                                                  0147
*         CALLSW='0'B;              /* EXIT FROM CALL LOOP           */
*                                                                  0147
         NI    CALLSW,B'11101111'                                  0147
*       /*************************************************************/
*       /*                                                           */
*       /* CALL SUBSYSTEM INTERFACE AGAIN IF NOT ENOUGH SPACE IN SSCS*/
*       /* CONTROL BLOCK FOR SUBSYSTEM'S ANSWER                      */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0148
*       ELSE                        /* SSYSCODE=SSCSMALL & SAVECODE=0*/
*         CALL MORECORE;            /* GET BIGGER SSCS, THEN ISSUE 0148
*                                      CALL AGAIN                    */
         B     @RC00146                                            0148
@RF00146 BAL   @14,MORECORE                                        0148
*     END;                          /* END OF CALLSSYS             0149
*                                                                    */
@RC00146 DS    0H                                                  0149
@DE00130 TM    CALLSW,B'00010000'                                  0149
         BO    @DL00130                                            0149
*                                                                  0150
*     /***************************************************************/
*     /*                                                             */
*     /* PRINT STATUS MESSAGE(S) FOR THIS JOBNAME IF HAVE OK R.C.'S  */
*     /* FROM SUBSYSTEM AND SUBSYSTEM INTERFACE                      */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0150
*     IF SAVECODE=SSRTOK&(SSYSCODE=SSCSRTOK×SSYSCODE=SSCSBADI×SSYSCODE=
*         SSCSNOJB×SSYSCODE=SSCSYNTX) THEN                         0150
         SLR   @15,@15                                             0150
         C     @15,SAVECODE                                        0150
         BNE   @RF00150                                            0150
         L     @14,SSYSCODE                                        0150
         CR    @14,@15                                             0150
         BE    @RT00150                                            0150
         CH    @14,@CH00071                                        0150
         BE    @RT00150                                            0150
         CH    @14,@CH00110                                        0150
         BE    @RT00150                                            0150
         CH    @14,@CH00046                                        0150
         BNE   @RF00150                                            0150
@RT00150 DS    0H                                                  0151
*OKRC:                                                             0151
*       DO;                         /* IF NO SYSTEM ERROR HAS      0151
*                                      OCCURRED, CONTINUE PROCESSING */
OKRC     DS    0H                                                  0152
*         JOBNINST=SSCSJOBN;        /* PLACE JOBNAME IN INSERT AREA
*                                      FOR STATUS MESSAGES           */
*                                                                  0152
         L     @01,SSOBINDV                                        0152
         MVC   JOBNINST(8),SSCSJOBN(@01)                           0152
*         /***********************************************************/
*         /*                                                         */
*         /* ISSUE ANY WARNING MESSAGE                               */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0153
*         IF SSYSCODE^=SSCSRTOK THEN                               0153
         L     @15,SSYSCODE                                        0153
         LTR   @15,@15                                             0153
         BZ    @RF00153                                            0153
*NOTONQ:                                                           0154
*           DO;                     /* THIS CODE DOESN'T USE SSCSARAY*/
*                                                                  0154
NOTONQ   DS    0H                                                  0155
*             /*******************************************************/
*             /*                                                     */
*             /* 'NO JOBS FOUND' MESSAGE                             */
*             /*                                                     */
*             /*******************************************************/
*                                                                  0155
*             IF STATAUTO=ON&SSYSCODE=SSCSNOJB THEN/* IF STATUS    0155
*                                      COMMAND WITH NO OPERAND & 'JOB
*                                      NOT FOUND' RETURN CODE        */
         L     @01,SWITPTR(,CSPLPTR)                               0155
         TM    STATAUTO(@01),B'00100000'                           0155
         BNO   @RF00155                                            0155
         CLC   SSYSCODE(4),@CF00110                                0155
         BNE   @RF00155                                            0155
*               DO;                                                0156
*                 MSGID='216I';     /* 'NO JOBS FOUND' MESSAGE       */
         MVC   MSGID(4,MTPTR),@CC00937                             0157
*                 CALL CALL02;      /* ISSUE THE MESSAGE             */
         BAL   @14,CALL02                                          0158
*               END;                                               0159
*             ELSE                  /* ISSUE OTHER WARNING MESSAGES  */
*STATOPD2:                          /* LABEL FOR DO/END GROUP        */
*               DO;                                                0160
*                                                                  0160
         B     @RC00155                                            0160
@RF00155 DS    0H                                                  0160
STATOPD2 DS    0H                                                  0161
*                 /***************************************************/
*                 /*                                                 */
*                 /* 'JOBID XX HAS INVALID SYNTAX FOR XXX SUBSYSTEM' */
*                 /* MESSAGE                                         */
*                 /*                                                 */
*                 /***************************************************/
*                                                                  0161
*                 IF SSYSCODE=SSCSYNTX THEN                        0161
         CLC   SSYSCODE(4),@CF00046                                0161
         BNE   @RF00161                                            0161
*                   DO;                                            0162
*                     MSGID='194I';                                0163
         MVC   MSGID(4,MTPTR),@CC00941                             0163
*                     L1=LENGTH(SSCSJOBI);/* INSERT JOBID            */
         MVI   L1(MTPTR),X'08'                                     0164
*                     VAR1=ADDR(SSCSJOBI);                         0165
         L     @01,SSOBINDV                                        0165
         LA    @15,SSCSJOBI(,@01)                                  0165
         STCM  @15,7,VAR1(MTPTR)                                   0165
*                     IF SSOBSSIB^=ZEROFLD THEN/* INSERT SUBSYSTEM 0166
*                                      NAME                          */
         L     @15,SSOBSSIB                                        0166
         LTR   @15,@15                                             0166
         BZ    @RF00166                                            0166
*                       DO;                                        0167
*                         L2=LENGTH(SSIBSSNM);                     0168
         MVI   L2(MTPTR),X'04'                                     0168
*                         VAR2=ADDR(SSIBSSNM);                     0169
         L     @01,SSIBPTR                                         0169
         LA    @15,SSIBSSNM(,@01)                                  0169
         STCM  @15,7,VAR2(MTPTR)                                   0169
*                       END;                                       0170
*                     CALL CALL02;  /* ISSUE THE MESSAGE             */
@RF00166 BAL   @14,CALL02                                          0171
*                     IF SSCSJOBI^=ALLBLNKS THEN/* SHOULD HAVE JOBID
*                                      ON COMMAND UNLESS SUBSYSTEM 0172
*                                      HAD A LOGIC ERROR             */
         L     @01,SSOBINDV                                        0172
         CLC   SSCSJOBI(8,@01),ALLBLNKS                            0172
         BE    @RF00172                                            0172
*                       INVJOBID=ON;/* WILL ISSUE MESSAGE 194I AND 0173
*                                      LOOP AGAIN TO GET STATUS FOR
*                                      JOBNAME ONLY                  */
         OI    INVJOBID,B'10000000'                                0173
*                   END;                                           0174
*                 ELSE              /* IF NO MATCH YET               */
*                   DO;                                            0175
*                                                                  0175
         B     @RC00161                                            0175
@RF00161 DS    0H                                                  0176
*                     /***********************************************/
*                     /*                                             */
*                     /* 'JOB XX NOT FOUND' MESSAGE                  */
*                     /*                                             */
*                     /***********************************************/
*                                                                  0176
*                     IF SSYSCODE=SSCSNOJB THEN/* IF JOB NOT FOUND 0176
*                                      R.C.                          */
         CLC   SSYSCODE(4),@CF00110                                0176
         BNE   @RF00176                                            0176
*                       MSGID='202I';/* 'JOB <JOBNAME OR           0177
*                                      JOBNAME(JOBID)> NOT FOUND'  0177
*                                      MESSAGE                       */
*                                                                  0177
         MVC   MSGID(4,MTPTR),@CC00942                             0177
*                     /***********************************************/
*                     /*                                             */
*                     /* 'JOB XX COMBINATION INVALID' MESSAGE        */
*                     /*                                             */
*                     /***********************************************/
*                                                                  0178
*                     ELSE          /* MUST HAVE SSYSCODE = SSCSBADI
*                                      BY PRIOR TEST FOR OKRC        */
*                       DO;                                        0178
         B     @RC00176                                            0178
@RF00176 DS    0H                                                  0179
*                         MSGID='195I';/* 'JOB <JOBNAME(JOBID)>    0179
*                                      COMBINATION INVALID' MESSAGE  */
         MVC   MSGID(4,MTPTR),@CC00943                             0179
*                         IF SSCSJOBI^=ALLBLNKS THEN/* SHOULD HAVE 0180
*                                      JOBID ON COMMAND UNLESS     0180
*                                      SUBSYSTEM HAD A LOGIC ERROR   */
         L     @01,SSOBINDV                                        0180
         CLC   SSCSJOBI(8,@01),ALLBLNKS                            0180
         BE    @RF00180                                            0180
*                           INVJOBID=ON;/* WILL ISSUE MESSAGE 195I 0181
*                                      AND LOOP AGAIN TO GET STATUS
*                                      FOR JOBNAME ONLY              */
         OI    INVJOBID,B'10000000'                                0181
*                       END;                                       0182
@RF00180 DS    0H                                                  0183
*                     JOBIINST=SSCSJOBI;/* PUT JOBID INTO MESSAGE  0183
*                                      INSERT - SSCSJOBI MAY BE BLANK*/
@RC00176 L     @01,SSOBINDV                                        0183
         MVC   JOBIINST(8),SSCSJOBI(@01)                           0183
*                     CALL STATMSG; /* SETS UP JOBNAME OR          0184
*                                      JOBNAME(JOBID) INSERT & ISSUES
*                                      STATUS MESSAGE                */
         BAL   @14,STATMSG                                         0184
*                   END;                                           0185
*                 SSCSARAY(1)=SSCSARAY(1)&&SSCSARAY(1);/* INSURE   0186
*                                      ANSWER AREA IS ZEROED         */
@RC00161 L     @01,SSOBINDV                                        0186
         XC    SSCSARAY(16,@01),SSCSARAY(@01)                      0186
*                 MTINSRTS=''X;     /* ZERO OUT MESSAGE INSERT AREA  */
         XC    MTINSRTS(16,MTPTR),MTINSRTS(MTPTR)                  0187
*               END STATOPD2;                                      0188
*           END NOTONQ;                                            0189
*                                                                  0189
*         /***********************************************************/
*         /*                                                         */
*         /* IF SUBSYSTEM HAD GOOD RETURN CODE, ISSUE STATUS         */
*         /* MESSAGE(S) FOR JOB(S) ON SYSTEM QUEUE                   */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0190
*         ELSE                                                     0190
*ISONQ:                             /* LABEL FOR DO/END GROUP        */
*           DO;                                                    0190
         B     @RC00153                                            0190
@RF00153 DS    0H                                                  0190
ISONQ    DS    0H                                                  0191
*             ARRAYCTR=SSCSDIMR/LENGTH(SSCSARAY);/* DIVIDE LENGTH OF
*                                      ARRAY BY LENGTH OF ELEMENT IN
*                                      ARRAY TO GET LOOP COUNTER     */
         L     @15,SSOBINDV                                        0191
         LH    @00,SSCSDIMR(,@15)                                  0191
         SRDA  @00,32                                              0191
         D     @00,@CF00212                                        0191
         LR    ARRAYCTR,@01                                        0191
