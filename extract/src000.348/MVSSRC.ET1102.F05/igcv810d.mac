         TITLE 'IGCV810D, STOPLINE FOR 3705 LINES                      *
                        '
IGCV810D CSECT ,                                                01S0001
@PROLOG  DS    0H                                               01S0002
         USING *,RBASE                  ESTABLISH A BASE REGISTER
         LR    RBASE,REG15              SETUP BASE REG
IGCV810D IEDHJN AFTERHJN,HJN            MODULE IDENTIFICATION MACRO
*   ROPCAVT=RPARM;                  /* SET BASE POINTER FOR OP  03S0503
*                                      CONTROL AVT                   */
         LR    ROPCAVT,RPARM                                    03S0503
*   RAVT=OPCAVTPT;                  /* SET THE BASE POINTER FOR THE
*                                      TCAM ADDRESS VECTOR TABLE     */
         L     RAVT,OPCAVTPT(,ROPCAVT)                          03S0504
*   ROPCE=OPCCOPCE;                 /* SET THE BASE POINTER FOR THE
*                                      OPERATOR CONTROL ELEMENT      */
         L     ROPCE,OPCCOPCE(,ROPCAVT)                         03S0505
*   OPCFLAG1=ZERO;                  /* CLEAR CONTROL SWITCHES        */
         MVI   OPCFLAG1(ROPCAVT),X'00'                          03S0506
*   IF OCRESP=ON THEN                                           02S0507
         TM    OCRESP(ROPCE),B'00000001'                        02S0507
         BNO   @RF00507                                         02S0507
*     DO;                           /* IF THIS IS AN OPCE WITH A
*                                      NON-SNA RESPONSE, PROCESS THE
*                                      RESPONSE                      */
*                                                               03S0509
*       /*************************************************************/
*       /*                                                           */
*       /* PROCESS.RESPROC: THIS ROUTINE WILL PROCESS A NON-SNA      */
*       /* RESPONSE                                                  */
*       /*                                                           */
*       /*************************************************************/
*                                                               03S0509
*       RFY                                                     03S0509
*        (RWORK6,                                               03S0509
*         RWORK7) RSTD;                                         03S0509
*       CALL IPWLSRCH;              /* SEARCH WAITV CHAIN FOR   03S0510
*                                      MATCHING OPCE                 */
         BAL   @14,IPWLSRCH                                     03S0510
*       RZERO->OCLINK=RPARM->OCLINK;/* UNLINK OPCE FROM CHAIN        */
         LR    @09,RZERO                                        03S0511
         MVC   OCLINK(4,@09),OCLINK(RPARM)                      03S0511
*       RWORK6=RPARM;               /* SAVE ADDRESS OF REAL OPCE     */
         LR    RWORK6,RPARM                                     03S0512
*       CALL IPWLSRCH;              /* SEARCH FOR ANOTHER OPCE  03S0513
*                                      OY14092                       */
         BAL   @14,IPWLSRCH                                     03S0513
*       IF RPARM^=ZERO THEN         /* OPCE OY14092                  */
         LTR   RPARM,RPARM                                      03S0514
         BZ    @RF00514                                         03S0514
*         DO;                       /* YES OY14092                   */
*           RZERO->OCLINK=RPARM->OCLINK;/* DELINK OPCE OY14092       */
         L     @09,OCLINK(,RPARM)                               03S0516
         LR    @05,RZERO                                        03S0516
         ST    @09,OCLINK(,@05)                                 03S0516
*           RWORK6->OCATTACH=ON;    /* INDICATE OPCE ATTACH OY14092  */
         OI    OCATTACH(RWORK6),B'00000010'                     03S0517
*           RWORK6->OCELEM=RPARM;   /* ATTACH IT OY14092             */
         ST    RPARM,OCELEM(,RWORK6)                            03S0518
*           RPARM->OCLINK=RPARM->OCLINK&&RPARM->OCLINK;/* OY14092    */
         LR    @05,@09                                          03S0519
         XR    @05,@09                                          03S0519
         ST    @05,OCLINK(,RPARM)                               03S0519
*           IF RPARM->OCWTG=MODVE THEN/* PARTIALLY DONE OY14092      */
         CLC   OCWTG(2,RPARM),MODVE                             03S0520
         BNE   @RF00520                                         03S0520
*             RPARM->OCWTG=MODV8;   /* RESTART OY14092               */
         MVC   OCWTG(2,RPARM),MODV8                             03S0521
*           ELSE                    /* NOT STOPLINE OY14092          */
*             IF RPARM->OCWTG=MODVF THEN/* PARTIALLY DONE OY14092    */
         B     @RC00520                                         03S0522
@RF00520 CLC   OCWTG(2,RPARM),MODVF                             03S0522
         BNE   @RF00522                                         03S0522
*               RPARM->OCWTG=MODV9; /* RESTART OY14092               */
         MVC   OCWTG(2,RPARM),MODV9                             03S0523
*             ELSE                  /* OY14092                       */
*               IF RPARM->OCWTG^=MODVG&(RPARM->OCWTG^=MODV8&RPARM->
*                   OCWTG^=MODV9) THEN/* ALL STARTED OY14092         */
         B     @RC00522                                         03S0524
@RF00522 CLC   OCWTG(2,RPARM),MODVG                             03S0524
         BE    @RF00524                                         03S0524
         CLC   OCWTG(2,RPARM),MODV8                             03S0524
         BE    @RF00524                                         03S0524
         CLC   OCWTG(2,RPARM),MODV9                             03S0524
         BE    @RF00524                                         03S0524
*                 RPARM->OCWTG=MODV8;/* RESTART OY14092              */
         MVC   OCWTG(2,RPARM),MODV8                             03S0525
*         END;                      /* OY14092                       */
*       ELSE                                                    03S0527
*         ;                         /* NO OPCE WAITING OY14092       */
@RF00514 DS    0H                                               03S0528
*       RWORK7=OCPARM;              /* SAVE RETURN CODE              */
@RC00514 LH    @09,OCPARM(,ROPCE)                               03S0528
         LR    RWORK7,@09                                       03S0528
*       RWORK6->OCUNIT=INVALTIC;    /* INDICATE OPCE ONLY UNIT TO BE
*                                      FREED                         */
         MVC   OCUNIT(4,RWORK6),@CF02034                        03S0529
*       IF OCPARM^=ONE&RWORK6->OCALLFLG=OFF THEN                03S0530
         CH    @09,@CH00060                                     03S0530
         BE    @RF00530                                         03S0530
         TM    OCALLFLG(RWORK6),B'00000100'                     03S0530
         BNZ   @RF00530                                         03S0530
*         DO;                       /* IF NOT A GOOD RESPONSE AND NOT
*                                      AN 'ALL' REQUEST THEN @YM06145*/
*           IF OCPARM=TWO THEN                                  03S0532
         CH    @09,@CH00131                                     03S0532
         BNE   @RF00532                                         03S0532
*             DO;                   /* IF LINE ALREADY STOPPED THEN  */
*               IF RWORK6->OCTOTE=ON THEN                       03S0534
         TM    OCTOTE(RWORK6),B'01000000'                       03S0534
         BNO   @RF00534                                         03S0534
*                 TRMSTOTE=ON;      /* INDICATE TOTE OWN     @YM06145*/
         LR    @09,RTRM                                         03S0535
         AL    @09,@CF02691                                     03S0535
         OI    TRMSTOTE-7(@09),B'00000001'                      03S0535
*               OPCWRKSP(1)=MSG25;  /* SETUP MSG NUMBER              */
@RF00534 MVC   OPCWRKSP(4,ROPCAVT),@CF02601                     03S0536
*               OPCWRKSP(2)=EIGHT;  /* SETUP RETURN CODE             */
         MVC   OPCWRKSP+4(4,ROPCAVT),@CF00045                   03S0537
*             END;                                              03S0538
*           ELSE                                                03S0539
*             DO;                                               03S0539
         B     @RC00532                                         03S0539
@RF00532 DS    0H                                               03S0540
*               OPCWRKSP(1)=MSG199; /* BAD RESPONSE                  */
         MVC   OPCWRKSP(4,ROPCAVT),@CF02607                     03S0540
*               OPCWRKSP(2)=FOUR;   /* SETUP RETURN CODE             */
         MVC   OPCWRKSP+4(4,ROPCAVT),@CF00250                   03S0541
*               RWORK6->OCUNIT=OCUNIT;/* ATTACH UNIT FROM INPUT OPCE
*                                      TO REAL OPCE                  */
         MVC   OCUNIT(4,RWORK6),OCUNIT(ROPCE)                   03S0542
*             END;                                              03S0543
*         END;                                                  03S0544
@RC00532 DS    0H                                               03S0545
*       OCUNIT=INVALTIC;            /* INDICATE LAST UNIT    @ZM46746*/
@RF00530 MVC   OCUNIT(4,ROPCE),@CF02034                         03S0545
*       RPARM=ROPCE;                /* SETUP INPUT OPCE TO BE FREED  */
         LR    RPARM,ROPCE                                      03S0546
*       RFY                                                     03S0547
*         RSAVE RSTD;                                           03S0547
*       RSAVE=ADDR(OPCSAVE);        /* SETUP SAVEAREA                */
         LA    RSAVE,OPCSAVE(,ROPCAVT)                          03S0548
*       CALL FREEBUFF;              /* FREE INPUT OPCE AND ATTACHED
*                                      UNIT                          */
         L     @15,OPCFREBF(,ROPCAVT)                           03S0549
         BALR  @14,@15                                          03S0549
*       RFY                                                     03S0550
*         RSAVE UNRSTD;                                         03S0550
*       ROPCE=RWORK6;               /* SETUP ADDRESS OF REAL OPCE    */
         LR    ROPCE,RWORK6                                     03S0551
*       OPCCOPCE=RWORK6;            /* SAVE OPCE ADDR IN OP CONTROL
*                                      AVT                           */
         ST    RWORK6,OPCCOPCE(,ROPCAVT)                        03S0552
*       RPARM=OCTRMTBL;             /* GET TTCIN OF LINE TTE         */
         SLR   RPARM,RPARM                                      03S0553
         ICM   RPARM,3,OCTRMTBL(ROPCE)                          03S0553
*       CALL IEDQTNT;               /* GET LINE TTE                  */
         L     @15,AVTRNMPT(,RAVT)                              03S0554
         BALR  @14,@15                                          03S0554
*       RTRM=RPARM;                 /* SETUP TERM BASE               */
         LR    RTRM,RPARM                                       03S0555
*       OCWTG=OCWTGB;               /* SETUP WHERE-TO-GO FIELD       */
         MVC   OCWTG(2,ROPCE),OCWTGB(ROPCE)                     03S0556
*       IF RWORK7=ONE THEN                                      03S0557
         CH    RWORK7,@CH00060                                  03S0557
         BNE   @RF00557                                         03S0557
*         DO;                       /* WAS IT A GOOD RESPONSE?       */
*           OPCWRKSP(1)=MSG26;      /* SETUP GOOD RESPONSE           */
         MVC   OPCWRKSP(4,ROPCAVT),@CF02603                     03S0559
*           OPCWRKSP(2)=ZERO;       /* SETUP GOOD RETURN CODE        */
         SLR   @09,@09                                          03S0560
         ST    @09,OPCWRKSP+4(,ROPCAVT)                         03S0560
*           IF OCTOTE=ON THEN                                   03S0561
         TM    OCTOTE(ROPCE),B'01000000'                        03S0561
         BNO   @RF00561                                         03S0561
*             TRMSTOTE=ON;          /* INDICATE TOTE OWNERSHIP       */
         LR    @09,RTRM                                         03S0562
         AL    @09,@CF02691                                     03S0562
         OI    TRMSTOTE-7(@09),B'00000001'                      03S0562
*           CALL IPEXIT;            /* PERFORM NORMAL EXIT PROCESSING*/
@RF00561 BAL   @14,IPEXIT                                       03S0563
*         END;                                                  03S0564
*       ELSE                        /* EITHER A BAD RESPONSE OR LINE
*                                      ALREADY STOPPED               */
*         CALL IPERROR;             /* PERFORM ERROR PROCESSING      */
         B     @RC00557                                         03S0565
@RF00557 BAL   @14,IPERROR                                      03S0565
*       RFY                                                     03S0566
*        (RWORK6,                                               03S0566
*         RWORK7) UNRSTD;                                       03S0566
@RC00557 DS    0H                                               03S0567
*     END;                                                      02S0567
*   ELSE                                                        02S0568
*     IF OCPARM^=ZERO THEN                                      02S0568
         B     @RC00507                                         02S0568
@RF00507 LH    @09,OCPARM(,ROPCE)                               02S0568
         LTR   @09,@09                                          02S0568
         BZ    @RF00568                                         02S0568
*       DO;                         /* IF THIS IS AN OPCE WITH A SNA
*                                      RESPONSE, PROCESS IT          */
*                                                               03S0570
*         /***********************************************************/
*         /*                                                         */
*         /* PROCESS.SNAPROC: THIS SEGMENT PROCESSES SNA RESPONSES   */
*         /*                                                         */
*         /***********************************************************/
*                                                               03S0570
*         OCWTG=OCWTGB;             /* SETUP WHERE-TO-GO FIELD       */
         MVC   OCWTG(2,ROPCE),OCWTGB(ROPCE)                     03S0570
*         OPCWRKSP(1)=OCPARM;       /* SETUP MESSAGE NUMBER          */
         ST    @09,OPCWRKSP(,ROPCAVT)                           03S0571
*         RPARM=OCTRMTBL;           /* SETUP TTCIN OF SNA LINE       */
         SLR   RPARM,RPARM                                      03S0572
         ICM   RPARM,3,OCTRMTBL(ROPCE)                          03S0572
*         CALL IEDQTNT;             /* GET LINE TTE                  */
         L     @15,AVTRNMPT(,RAVT)                              03S0573
         BALR  @14,@15                                          03S0573
*         RTRM=RPARM;               /* SETUP TERM BASE               */
         LR    RTRM,RPARM                                       03S0574
*         IF OCPARM=MSG401 THEN                                 03S0575
         CLC   OCPARM(2,ROPCE),@CH02613                         03S0575
         BNE   @RF00575                                         03S0575
*           DO;                     /* IF SNA LINE DEACTIVATED, 03S0576
*                                      PROCESS NORMALLY              */
*             OPCWRKSP(2)=ZERO;     /* SETUP RETURN CODE             */
         SLR   @09,@09                                          03S0577
         ST    @09,OPCWRKSP+4(,ROPCAVT)                         03S0577
*             CALL IPEXIT;          /* DO NORMAL EXIT PROCESSING     */
         BAL   @14,IPEXIT                                       03S0578
*           END;                                                03S0579
*         ELSE                                                  03S0580
*           DO;                                                 03S0580
         B     @RC00575                                         03S0580
@RF00575 DS    0H                                               03S0581
*             IF OCPARM=MSG539 THEN                             03S0581
         CLC   OCPARM(2,ROPCE),@CH02615                         03S0581
         BNE   @RF00581                                         03S0581
*               OPCWRKSP(2)=EIGHT;  /* LINE ALREADY DEACTIVATED      */
         MVC   OPCWRKSP+4(4,ROPCAVT),@CF00045                   03S0582
*             ELSE                                              03S0583
*               OPCWRKSP(2)=FOUR;   /* ELSE SETUP ERROR RETURN CODE  */
         B     @RC00581                                         03S0583
@RF00581 MVC   OPCWRKSP+4(4,ROPCAVT),@CF00250                   03S0583
*             CALL IPERROR;         /* DO ERROR PROCESSING           */
@RC00581 BAL   @14,IPERROR                                      03S0584
*           END;                                                03S0585
*       END;                                                    02S0586
*     ELSE                                                      02S0587
*       FIRSTREQ=ON;                /* ELSE INDICATE ITS A FIRST
*                                      REQUEST                       */
         B     @RC00568                                         02S0587
@RF00568 OI    FIRSTREQ(ROPCAVT),B'10000000'                    02S0587
*   DO WHILE(FIRSTREQ=ON×MORLINES=ON);/* IF THIS IS A FIRST REQUEST
*                                      OR MORE LINES ARE LEFT TO
*                                      PROCESS, PROCESS STOPLINE
*                                      REQUEST                       */
@RC00568 DS    0H                                               02S0588
@RC00507 B     @DE00588                                         02S0588
@DL00588 DS    0H                                               02S0589
*                                                               03S0589
*     /***************************************************************/
*     /*                                                             */
*     /* PROCESS.STPLPROC: THIS SEGMENT PROCESSES STOPLINE REQUESTS  */
*     /*                                                             */
*     /***************************************************************/
*                                                               03S0589
*     MORLINES=OFF;                 /* RESET LINE INDICATOR          */
         NI    MORLINES(ROPCAVT),B'10111111'                    03S0589
*     IF FIRSTREQ=ON THEN                                       03S0590
         TM    FIRSTREQ(ROPCAVT),B'10000000'                    03S0590
         BNO   @RF00590                                         03S0590
*       DO;                         /* IF THIS REQUEST IS FIRST ONE,
*                                      SETUP TO ESTABLISH FIRST LINE */
*         FIRSTREQ=OFF;             /* TURN OFF FIRST REQUEST FLAG   */
         NI    FIRSTREQ(ROPCAVT),B'01111111'                    03S0592
*                                                               04S0593
*         /***********************************************************/
*         /*                                                         */
*         /* PROCESS.STPLPROC.FIRSTLN: THIS SEGMENT ESTABLISHES THE  */
*         /* FIRST LINE TO BE STOPPED                                */
*         /*                                                         */
*         /***********************************************************/
*                                                               04S0593
*         IF OCRENTRD=ON THEN                                   04S0593
         TM    OCRENTRD(ROPCE),B'10000000'                      04S0593
         BNO   @RF00593                                         04S0593
*           DO;                     /* IS THIS A RE-ENTRY?           */
*             OCRENTRD=OFF;         /* RE-SET 'RE-ENTERED' INDICATOR */
         NI    OCRENTRD(ROPCE),B'01111111'                      04S0595
*             OCWTG=OCWTGB;         /* SETUP WHERE-TO-GO FIELD       */
         MVC   OCWTG(2,ROPCE),OCWTGB(ROPCE)                     04S0596
*           END;                                                04S0597
*         RPARM=OCTRMTBL;           /* GET TTCIN OF RESOURCE         */
@RF00593 SLR   RPARM,RPARM                                      04S0598
         ICM   RPARM,3,OCTRMTBL(ROPCE)                          04S0598
*         CALL IEDQTNT;             /* FIND TTE OF RESOURCE          */
         L     @15,AVTRNMPT(,RAVT)                              04S0599
         BALR  @14,@15                                          04S0599
*         RTRM=RPARM;               /* SETUP TERM BASE               */
         LR    RTRM,RPARM                                       04S0600
*         IF TRMTYPE=TRMGRP THEN                                04S0601
         LR    @09,RTRM                                         04S0601
         SH    @09,@CH00035                                     04S0601
         CLI   TRMTYPE(@09),X'00'                               04S0601
         BNE   @RF00601                                         04S0601
*           DO;                     /* IS THIS A GROUP TTE           */
*             IF TRMSUBST^=ZERO THEN                            04S0603
         ICM   @05,3,TRMSUBST(@09)                              04S0603
         BZ    @RF00603                                         04S0603
*               DO;                                             04S0604
*                 OCTRMTBL=TRMSUBST;/* SAVE TTCIN OF LINE            */
         SLR   @05,@05                                          04S0605
         ICM   @05,3,TRMSUBST(@09)                              04S0605
         STH   @05,OCTRMTBL(,ROPCE)                             04S0605
*                 RPARM=OCTRMTBL;   /* GET TTCIN FROM OPCE           */
         LR    RPARM,@05                                        04S0606
*                 CALL IEDQTNT;     /* FIND LINE TTE                 */
         L     @15,AVTRNMPT(,RAVT)                              04S0607
         BALR  @14,@15                                          04S0607
*                 RTRM=RPARM;       /* SETUP LINE TTE ADDRESS        */
         LR    RTRM,RPARM                                       04S0608
*               END;                                            04S0609
*             ELSE                                              04S0610
*               DO;                                             04S0610
         B     @RC00603                                         04S0610
@RF00603 DS    0H                                               04S0611
*                 EXITFLG=ON;       /* PREPARE TO EXIT               */
         OI    EXITFLG(ROPCAVT),B'00010000'                     04S0611
*                 OPCMODID=MOD03;   /* SETUP MOD ID                  */
         MVC   OPCMODID(2,ROPCAVT),MOD03                        04S0612
*                 RZERO=MSG541;     /* GROUP HAS NO LINES ATTACHED   */
         LA    RZERO,541                                        04S0613
*                 RPARM=FOUR;       /* SETUP RETURN CODE             */
         LA    RPARM,4                                          04S0614
*               END;                                            04S0615
*           END;                                                04S0616
@RC00603 DS    0H                                               04S0617
*         IF EXITFLG=OFF THEN                                   04S0617
@RF00601 TM    EXITFLG(ROPCAVT),B'00010000'                     04S0617
         BNZ   @RF00617                                         04S0617
*           DO;                                                 04S0618
*             CALL IPWLSRCH;        /* SEARCH WAITV CHAIN            */
         BAL   @14,IPWLSRCH                                     04S0619
*             IF OCSWITCH^=OCSTPLNI THEN/* STOPLINE,C OY14092        */
         CLI   OCSWITCH(ROPCE),X'12'                            04S0620
         BE    @RF00620                                         04S0620
*               DO;                 /* YES OY14092                   */
*                 IF RPARM=ZERO THEN                            04S0622
         LTR   RPARM,RPARM                                      04S0622
         BNZ   @RF00622                                         04S0622
*                   DO;             /* IF A MATCH WAS NOT FOUND      */
*                     IF OCALLFLG=ON THEN                       04S0624
         TM    OCALLFLG(ROPCE),B'00000100'                      04S0624
         BNO   @RF00624                                         04S0624
*                       DO;         /* CHECK 'ALL' STATUS            */
*                         OCSOME=OFF;/* INDICATE AN UNSET STATUS     */
*                         OCNONE=OFF;                           04S0627
         NI    OCSOME(ROPCE),B'11111100'                        04S0627
*                       END;                                    04S0628
*                   END;                                        04S0629
*                 ELSE                                          04S0630
*                   DO;                                         04S0630
         B     @RC00622                                         04S0630
@RF00622 DS    0H                                               04S0631
*                     IF RPARM->OCSWITCH=OCSTPLNI THEN/* STOPLINE,I
*                                      OY14092                       */
         CLI   OCSWITCH(RPARM),X'12'                            04S0631
         BNE   @RF00631                                         04S0631
*                       DO;         /* YES OY14092                   */
*                         OCELEM=RPARM;/* ATTACH THE WAITING OPCE
*                                      OY14092                       */
         ST    RPARM,OCELEM(,ROPCE)                             04S0633
*                         RZERO->OCLINK=RPARM->OCLINK;/* DELINK OPCE
*                                      OY14092                       */
         L     @09,OCLINK(,RPARM)                               04S0634
         LR    @05,RZERO                                        04S0634
         ST    @09,OCLINK(,@05)                                 04S0634
*                         RPARM->OCLINK=RPARM->OCLINK&&RPARM->OCLINK;
         LR    @05,@09                                          04S0635
         XR    @05,@09                                          04S0635
         ST    @05,OCLINK(,RPARM)                               04S0635
*                                   /* CLEAROY14092                  */
*                         OCATTACH=ON;/* INDICATE OPCE IS ATTACHED
*                                      OY14092                       */
         OI    OCATTACH(ROPCE),B'00000010'                      04S0636
*                       END;        /* OY14092                       */
*                     ELSE          /* STOPLINE,C OY14092            */
*                       DO;         /* YES OY14092                   */
         B     @RC00631                                         04S0638
@RF00631 DS    0H                                               04S0639
*                         OCWTGB=OCWTG;/* SETUP BACKUP WHERE-TO-GO   */
         MVC   OCWTGB(2,ROPCE),OCWTG(ROPCE)                     04S0639
*                         OCRENTRD=ON;/* SETUP RE-ENTERED FLAG       */
         OI    OCRENTRD(ROPCE),B'10000000'                      04S0640
*                         OCWTG=MODV8;/* SETUP TO RETURN TO THIS
*                                      MODULE                        */
         MVC   OCWTG(2,ROPCE),MODV8                             04S0641
*                         RZERO=ZERO;/* SETUP INTERFACE TO NEXT MOD  */
         SLR   RZERO,RZERO                                      04S0642
*                         OPCMODID=MOD00;/* SETUP FOR XCTL TO NEXT
*                                      MODULE                        */
         MVC   OPCMODID(2,ROPCAVT),MOD00                        04S0643
*                         EXITFLG=ON;/* SETUP TO EXIT                */
         OI    EXITFLG(ROPCAVT),B'00010000'                     04S0644
*                                                               05S0645
*                         /*******************************************/
*                         /*                                         */
*                         /* PROCESS.STPLPROC.WLATTACH: THIS SEGMENT */
*                         /* ATTACHES THE INPUT OPCE TO AN OPCE ON   */
*                         /* THE WAITV CHAIN WITH THE SAME GROUPNAME */
*                         /*                                         */
*                         /*******************************************/
*                                                               05S0645
*                         DO WHILE(RPARM->OCATTACH=ON);/* SEARCH FOR
*                                      LAST OPCE ATTACHED            */
         B     @DE00645                                         05S0645
@DL00645 DS    0H                                               05S0646
*                           RPARM=RPARM->OCELMPTR;/* GOTO NEXT OPCE  */
         SLR   @09,@09                                          05S0646
         ICM   @09,7,OCELMPTR(RPARM)                            05S0646
         LR    RPARM,@09                                        05S0646
*                         END;                                  05S0647
@DE00645 TM    OCATTACH(RPARM),B'00000010'                      05S0647
         BO    @DL00645                                         05S0647
*                         RPARM->OCELMPTR=ROPCE;/* ATTACH INPUT OPCE
*                                      TO WAIT CHAIN                 */
         STCM  ROPCE,7,OCELMPTR(RPARM)                          05S0648
*                         RPARM->OCATTACH=ON;/* INDICATE OPCE IS
*                                      ATTACHED                      */
         OI    OCATTACH(RPARM),B'00000010'                      05S0649
*                       END;                                    04S0650
*                   END;            /* OY14092                       */
*               END;                /* OY14092                       */
*           END;                                                04S0653
*       END;                                                    03S0654
