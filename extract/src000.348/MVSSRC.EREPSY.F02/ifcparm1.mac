         B     @RC00304                                            0309
@RF00304 LA    @08,PARMFLD-1(INDEX1)                               0309
         CLC   0(2,@08),@CC00668                                   0309
         BNE   @RF00309                                            0309
*            INDEX1 = INDEX1 + 2;        /* STEP OVER = N    @Z427VLS*/
         AL    INDEX1,@CF00074                                     0310
*        ELSE                                                      0311
*          DO;                                             /*@Z427VLS*/
         B     @RC00309                                            0311
@RF00309 DS    0H                                                  0312
*            CALL OUTMSG(SYNTAX);                          /*@Z427VLS*/
         LA    @01,@AL00312                                        0312
         BAL   @14,OUTMSG                                          0312
*            CALL SCANC;                                   /*@Z427VLS*/
         BAL   @14,SCANC                                           0313
*            INDEX1 = INDEX1 + NEXTC;                      /*@Z427VLS*/
         AL    INDEX1,NEXTC                                        0314
