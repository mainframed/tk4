         B     @RC00302                                            0304
@RF00302 LA    @08,PARMFLD-1(INDEX1)                               0304
         CLC   0(2,@08),@CC00666                                   0304
         BNE   @RF00304                                            0304
*        DO;                                               /*@Z427VLS*/
