*/*                                                          @Z427VLS*/
*/*  VALID THE FIELD STARTING AT PARMFLD(INDEX1) FOR A LENGTH AS     */
*/*  GIVEN AS THE INPUT PARAMETER.  IF A CHARACTER IS NOT FOUND TO   */
*/*  BE A VALID HEX, THE FIELD IS INDICATED AS HAVING A SYNTAX       */
*/*  ERROR.  THE CONTENTS OF THE ORIGINAL FIELD ARE CHANGED TO THE   */
*/*  HEXADECIMAL EQUIVALENT FOR THE CHARACTER.  THOSE VALUES CAN THEN*/
*/*  BE EASILY CONVERTED FOR THE NECESSARY FIELD.            @Z427VLS*/
*/*                                                          @Z427VLS*/
*/********************************************************************/
* VALIDHEX: PROC (LENGTHH) ;                               /*@Z427VLS*/
VALIDHEX STM   @14,@12,@SA00005                                    0219
         MVC   @PC00005(4),0(@01)                                  0219
*    DCL LENGTHH FIXED(31);               /* LENGTH OF THE INPUT FLD */
*    I = INDEX1 + LENGTHH - 1;            /* LAST CHARACTER POSITION */
         L     @08,@PC00005                                        0221
         LR    I,INDEX1                                            0221
         AL    I,LENGTHH(,@08)                                     0221
         BCTR  I,0                                                 0221
*    DO INDEX3 = INDEX1 TO I BY 1;        /* STEP THROUGH FIELD      */
         LR    @08,INDEX1                                          0222
         ST    @08,INDEX3                                          0222
         B     @DE00222                                            0222
@DL00222 DS    0H                                                  0223
*      VALID = OFF;                                        /*@Z427VLS*/
         NI    VALID,B'01111111'                                   0223
*      /*                                                    @Z427VLS*/
*      /* CHECK TO SEE IF THE FIELD IS A VALID HEXADECIMAL NUMBER.   */
*      /* IF YES, TERMINATE THE SEARCH, AND PLACE THE HEX EQUIVALENT */
*      /* OF THE CHARACTER IN THE PARAMETER FIELD. IF NO, PRINT      */
*      /* AN ERROR MESSAGE FOR THAT OPERAND.                 @Z427VLS*/
*      /*                                                    @Z427VLS*/
*      DO J = 16 TO 1 BY -1 WHILE (VALID = OFF);           /*@Z427VLS*/
         LA    J,16                                                0224
@DL00224 TM    VALID,B'10000000'                                   0224
         BNZ   @DC00224                                            0224
*        IF PARMFLD(INDEX3) = NUMTAB(J) THEN                       0225
         L     @08,INDEX3                                          0225
         LA    @07,PARMFLD-1(@08)                                  0225
         LA    @03,NUMTAB-1(J)                                     0225
         CLC   0(1,@07),0(@03)                                     0225
         BNE   @RF00225                                            0225
*          DO;                                             /*@Z427VLS*/
*            VALID = ON;                                   /*@Z427VLS*/
         OI    VALID,B'10000000'                                   0227
*            PARMFLD(INDEX3) = HEXTAB(J);                  /*@Z427VLS*/
         LA    @07,PARMFLD-1(@08)                                  0228
         LA    @08,HEXTAB-1(J)                                     0228
         MVC   0(1,@07),0(@08)                                     0228
*          END;                                            /*@Z427VLS*/
