* VALIDNUM: PROC (LENGTHV) ;                               /*@Z427VLS*/
VALIDNUM STM   @14,@12,@SA00006                                    0237
         MVC   @PC00006(4),0(@01)                                  0237
*    DCL LENGTHV FIXED(31);               /* LENGTH OF THE INPUT FLD */
*    I = INDEX1 + LENGTHV - 1;            /* LAST CHARACTER POSITION */
         L     @08,@PC00006                                        0239
         LR    I,INDEX1                                            0239
         AL    I,LENGTHV(,@08)                                     0239
         BCTR  I,0                                                 0239
*    DO INDEX3 = INDEX1 TO I BY 1;                         /*@Z427VLS*/
         LR    @08,INDEX1                                          0240
         ST    @08,INDEX3                                          0240
         B     @DE00240                                            0240
@DL00240 DS    0H                                                  0241
*      VALID = OFF;                                        /*@Z427VLS*/
         NI    VALID,B'01111111'                                   0241
*      /*                                                    @Z427VLS*/
*      /* CHECK TO SEE IF THE FIELD IS A VALID DECIMAL NUMBER.  IF   */
*      /* YES, TERMINATE THE SEARCH AND REPLACE THE CHARACTER WITH   */
*      /* HEXADECIMAL EQUIVALENT.  IF NO, PRINT AN ERROR MESSAGE FOR */
*      /* THIS OPERAND.                                      @Z427VLS*/
*      /*                                                    @Z427VLS*/
*      DO J = 10 TO 1 BY -1 WHILE (VALID = OFF);           /*@Z427VLS*/
         LA    J,10                                                0242
@DL00242 TM    VALID,B'10000000'                                   0242
         BNZ   @DC00242                                            0242
*        IF PARMFLD(INDEX3) = NUMTAB(J) THEN                       0243
         L     @08,INDEX3                                          0243
         LA    @07,PARMFLD-1(@08)                                  0243
         LA    @03,NUMTAB-1(J)                                     0243
         CLC   0(1,@07),0(@03)                                     0243
         BNE   @RF00243                                            0243
*          DO;                                             /*@Z427VLS*/
*            VALID = ON;                                   /*@Z427VLS*/
         OI    VALID,B'10000000'                                   0245
*            PARMFLD(INDEX3) = HEXTAB(J);                  /*@Z427VLS*/
         LA    @07,PARMFLD-1(@08)                                  0246
         LA    @08,HEXTAB-1(J)                                     0246
         MVC   0(1,@07),0(@08)                                     0246
*          END;                                            /*@Z427VLS*/
*      END;                                                /*@Z427VLS*/
@RF00243 BCTR  J,0                                                 0248
         LTR   J,J                                                 0248
         BP    @DL00242                                            0248
@DC00242 DS    0H                                                  0249
*      IF VALID = OFF THEN                                         0249
         TM    VALID,B'10000000'                                   0249
         BNZ   @RF00249                                            0249
*        DO PLACE = INDEX1 - INDEX3;                       /*@Z427VLS*/
         LR    @08,INDEX1                                          0250
         SL    @08,INDEX3                                          0250
         ST    @08,PLACE                                           0250
*          CALL OUTMSG(SYNTAX);                            /*@Z427VLS*/
         LA    @01,@AL00251                                        0251
         BAL   @14,OUTMSG                                          0251
*        END;                                              /*@Z427VLS*/
*    END;                                                  /*@Z427VLS*/
@RF00249 LA    @08,1                                               0253
