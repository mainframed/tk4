*/*      SYNTAX CHECK FOR DEVICE SER NO (NNNNNN(,NNNNNN,...,NNNNNN)  */
*/*      PERTAINS ONLY TO 34XX DEVICES.                      @Z427VLS*/
*/*      MAXIMUM OF 8 ENTRIES                                @Z427VLS*/
*/*                                                          @Z427VLS*/
*/********************************************************************/
*PARMDEVS:  PROC OPTIONS(SAVE(1,14));                      /*@Z427VLS*/
PARMDEVS ST    @14,@SA00025                                        0508
         ST    @01,@SA00025+4                                      0508
*             NEXTRPC = OFF;               /* LOOK FOR R PAREN       */
         NI    NEXTRPC,B'11111011'                                 0509
*             IF SERFLG = ON THEN          /* DEVSER ALREADY SPECIFED*/
         TM    SERFLG,B'00000001'                                  0510
         BNO   @RF00510                                            0510
*               CALL DUPLICAT;                             /*@Z427VLS*/
         BAL   @14,DUPLICAT                                        0511
*             ELSE                                                 0512
*               DO INDEX1 = INDEX1 + 8;    /* NO, INCR OVER PARM     */
         B     @RC00510                                            0512
@RF00510 LA    @08,8                                               0512
         ALR   @08,INDEX1                                          0512
         LR    INDEX1,@08                                          0512
*                 SERFLG = ON;                             /*@Z427VLS*/
         OI    SERFLG,B'00000001'                                  0513
*                 /*                                         @Z427VLS*/
*                 /* SPACE IS GOTTEN FOR THE TABLE.  THE BASE POINTER*/
*                 /* IS ESTABLISHED AND THE TABLE IS CLEARED.        */
*                 /*                                         @Z427VLS*/
*                 RESPECIFY (R1) RESTRICTED;               /*@Z427VLS*/
*                 R1 = 32;                                 /*@Z427VLS*/
         LA    R1,32                                               0515
*                 GEN (GETMAIN R,LV=(1),SP=20);            /*@Z427VLS*/
         GETMAIN R,LV=(1),SP=20
*                 SERPTR = R1;                             /*@Z427VLS*/
         ST    R1,SERPTR(,R2)                                      0517
*                 RESPECIFY (R1) UNRESTRICTED;             /*@Z427VLS*/
*                 INDEX2 = ZERO;                           /*@Z427VLS*/
         SLR   @08,@08                                             0519
         ST    @08,INDEX2                                          0519
*                 DEVSERAL = DEVSERAL && DEVSERAL;         /*@Z427VLS*/
         L     @08,SERPTR(,R2)                                     0520
         XC    DEVSERAL(18,@08),DEVSERAL(@08)                      0520
*                 DO UNTIL (RPORC = OFF);                  /*@Z427VLS*/
@DL00521 DS    0H                                                  0522
*                   CALL SCANCRP;              /* FIND DELIMITER     */
         BAL   @14,SCANCRP                                         0522
*                   IF NEXTC = 6 THEN          /* VALID LENGTH       */
         CLC   NEXTC(4),@CF00081                                   0523
         BNE   @RF00523                                            0523
*                     DO;                                  /*@Z427VLS*/
*                       CALL VALIDNUM(6);      /* VALIDATE FOR NUMBER*/
         LA    @01,@AL00525                                        0525
         BAL   @14,VALIDNUM                                        0525
*                       /*                                   @Z427VLS*/
*                       /* CONVERT TO BINARY                 @Z427VLS*/
*                       /*                                   @Z427VLS*/
*                       INDEX3 = PARMFLD(INDEX1) ;         /*@Z427VLS*/
         SLR   @08,@08                                             0526
         IC    @08,PARMFLD-1(INDEX1)                               0526
         ST    @08,INDEX3                                          0526
*                       DO I=1 TO 5;                       /*@Z427VLS*/
         LA    I,1                                                 0527
@DL00527 DS    0H                                                  0528
*                         INDEX3 = INDEX3*10 + PARMFLD(INDEX1+I);  0528
         L     @08,INDEX3                                          0528
         MH    @08,@CH00034                                        0528
         LR    @07,INDEX1                                          0528
         ALR   @07,I                                               0528
         SLR   @04,@04                                             0528
         IC    @04,PARMFLD-1(@07)                                  0528
         ALR   @08,@04                                             0528
         ST    @08,INDEX3                                          0528
*                       END;                               /*@Z427VLS*/
         AL    I,@CF00055                                          0529
         C     I,@CF00079                                          0529
         BNH   @DL00527                                            0529
*                       IF INDEX2 < DIM(SERVAL) THEN               0530
         L     @08,INDEX2                                          0530
         C     @08,@CF00066                                        0530
         BNL   @RF00530                                            0530
*                         INDEX2 = INDEX2 + 1;  /* GET INDEX VALUE   */
         AL    @08,@CF00055                                        0531
         ST    @08,INDEX2                                          0531
*                       ELSE                                       0532
*                         CALL OUTMSG(SYNTAX);             /*@Z427VLS*/
         B     @RC00530                                            0532
@RF00530 LA    @01,@AL00532                                        0532
         BAL   @14,OUTMSG                                          0532
*                       IF INDEX3 > 65535 THEN                     0533
@RC00530 L     @08,INDEX3                                          0533
         C     @08,@CF00814                                        0533
         BNH   @RF00533                                            0533
*                         CALL OUTMSG(SYNTAX);             /*@Z427VLS*/
         LA    @01,@AL00534                                        0534
         BAL   @14,OUTMSG                                          0534
*                       SERVAL(INDEX2) = INDEX3; /* STORE VALUE      */
@RF00533 L     @08,INDEX2                                          0535
         ALR   @08,@08                                             0535
         L     @04,SERPTR(,R2)                                     0535
         L     @03,INDEX3                                          0535
         ALR   @04,@08                                             0535
         STCM  @03,3,SERVAL-2(@04)                                 0535
*                     END;                                 /*@Z427VLS*/
*                   ELSE                                           0537
*                       CALL OUTMSG(SYNTAX);               /*@Z427VLS*/
         B     @RC00523                                            0537
@RF00523 LA    @01,@AL00537                                        0537
         BAL   @14,OUTMSG                                          0537
*                   INDEX1 = INDEX1 + NEXTC + 1; /* NEXT OPERAND     */
@RC00523 LR    @08,INDEX1                                          0538
         AL    @08,NEXTC                                           0538
         AL    @08,@CF00055                                        0538
         LR    INDEX1,@08                                          0538
*                 END;                                     /*@Z427VLS*/
@DE00521 TM    RPORC,B'10000000'                                   0539
         BNZ   @DL00521                                            0539
*                 SERNUM = INDEX2;              /* STORE NUM OF ENTR */
         L     @08,SERPTR(,R2)                                     0540
         L     @04,INDEX2                                          0540
         STH   @04,SERNUM(,@08)                                    0540
*                 /*                                         @Z427VLS*/
*                 /* CHECK FOR DUPLICATE ENTRIES             @Z427VLS*/
*                 /*                                         @Z427VLS*/
*                 DO J = 1 TO SERNUM BY 1;                 /*@Z427VLS*/
         LA    J,1                                                 0541
         B     @DE00541                                            0541
@DL00541 DS    0H                                                  0542
*                   DO I = J+1 TO SERNUM BY 1;             /*@Z427VLS*/
         LA    I,1                                                 0542
         ALR   I,J                                                 0542
         B     @DE00542                                            0542
@DL00542 DS    0H                                                  0543
*                     IF SERVAL(J) = SERVAL(I) THEN                0543
         LR    @08,J                                               0543
         ALR   @08,@08                                             0543
         L     @07,SERPTR(,R2)                                     0543
         LR    @03,I                                               0543
         ALR   @03,@03                                             0543
         ALR   @08,@07                                             0543
         ALR   @03,@07                                             0543
         CLC   SERVAL-2(2,@08),SERVAL-2(@03)                       0543
         BNE   @RF00543                                            0543
*                       DO PLACE = (SERNUM - I) * 6 + 7;   /*@Z427VLS*/
         LH    @08,SERNUM(,@07)                                    0544
         SLR   @08,I                                               0544
         MH    @08,@CH00081                                        0544
         AL    @08,@CF00083                                        0544
         ST    @08,PLACE                                           0544
*                         CALL OUTMSG(DUPE);               /*@Z427VLS*/
         LA    @01,@AL00545                                        0545
         BAL   @14,OUTMSG                                          0545
*                       END;                               /*@Z427VLS*/
*                   END;                                   /*@Z427VLS*/
@RF00543 AL    I,@CF00055                                          0547
@DE00542 L     @08,SERPTR(,R2)                                     0547
         CH    I,SERNUM(,@08)                                      0547
         BNH   @DL00542                                            0547
*                 END;                                     /*@Z427VLS*/
         AL    J,@CF00055                                          0548
@DE00541 L     @08,SERPTR(,R2)                                     0548
         CH    J,SERNUM(,@08)                                      0548
         BNH   @DL00541                                            0548
*                 PARMMES = ON;           /* ONLY FOR MES/THRESHOLD  */
         OI    PARMMES(R2),B'01000000'                             0549
*               END;                                       /*@Z427VLS*/
*END;                                                      /*@Z427VLS*/
@EL00025 DS    0H                                                  0551
@EF00025 DS    0H                                                  0551
@ER00025 L     @14,@SA00025                                        0551
         L     @01,@SA00025+4                                      0551
         BR    @14                                                 0551
*/********************************************************************/
*/*                                                          @Z427VLS*/
*/*      SYNTAX CHECK FOR MOD   (NNN(,NNNN,...,NNN))         @Z427VLS*/
