*END;                                                      /*@Z427VLS*/
@EL00012 DS    0H                                                  0316
@EF00012 DS    0H                                                  0316
@ER00012 L     @14,@SA00012                                        0316
         L     @01,@SA00012+4                                      0316
         BR    @14                                                 0316
*/********************************************************************/
*/*                                                          @Z427VLS*/
*/*  GENERATE A VALID CPU/CUA ENTRY.  THE ENTRY HAS A CPU SERIAL     */
*/*  NUMBER FIRST, FOLLOWED BY A CUA.  THIS IS TRANSLATED INTO A     */
*/*  HEX VALUE AND PLACED IN THE NEXT CPUCUAV ENTRY, INDEXED BY THE  */
*/*  VARIABLE INDEX2.                                        @Z427VLS*/
*/*                                                          @Z427VLS*/
*/********************************************************************/
*VALCPUCU: PROC OPTIONS(SAVE(1,14));                       /*@Z427VLS*/
VALCPUCU ST    @14,@SA00013                                        0317
         ST    @01,@SA00013+4                                      0317
*  IF NEXTC = 10 & PARMFLD(INDEX1+6) = PERIOD THEN                 0318
         CLC   NEXTC(4),@CF00034                                   0318
         BNE   @RF00318                                            0318
         LA    @08,PARMFLD+5(INDEX1)                               0318
         CLI   0(@08),C'.'                                         0318
         BNE   @RF00318                                            0318
*    DO;                                                   /*@Z427VLS*/
*      /*                                                    @Z427VLS*/
*      /* CHECK THE CPU SERIAL NUMBER FOR VALID DECIMAL NUMBERS.     */
*      /* A ZERO IS PLACED OVER THE COMMA, AND THE CUA FIELD IS      */
*      /* VALIDATED FOR HEXADECIMAL CHARACTERS.  THE ENTIRE FIELD IS */
*      /* PACKED FOR THE CPUCUA TABLE.                       @Z427VLS*/
*      /*                                                    @Z427VLS*/
*      CUAHOLD = PARMFLD(INDEX1+7:INDEX1+9);     /* SAVE THE CUA     */
         LA    @08,PARMFLD+6(INDEX1)                               0320
         MVC   CUAHOLD(3),0(@08)                                   0320
*      IF CUAHOLD(3) = 'X' THEN                                    0321
         CLI   CUAHOLD+2,C'X'                                      0321
         BNE   @RF00321                                            0321
*        PARMFLD(INDEX1+9) = '0';         /* CHANGE X TO ZERO        */
         LA    @08,PARMFLD+8(INDEX1)                               0322
         MVI   0(@08),C'0'                                         0322
*      CALL VALIDNUM(6);                                   /*@Z427VLS*/
@RF00321 LA    @01,@AL00323                                        0323
         BAL   @14,VALIDNUM                                        0323
*      INDEX1 = INDEX1 + 6;                                /*@Z427VLS*/
         AL    INDEX1,@CF00081                                     0324
*      PARMFLD(INDEX1) = '0';                              /*@Z427VLS*/
         LA    @08,PARMFLD-1(INDEX1)                               0325
         MVI   0(@08),C'0'                                         0325
*      CALL VALIDHEX(4);                                   /*@Z427VLS*/
         LA    @01,@AL00326                                        0326
         BAL   @14,VALIDHEX                                        0326
*      INDEX1 = INDEX1 - 6;                                /*@Z427VLS*/
         SL    INDEX1,@CF00081                                     0327
*      CALL HEXPACK(10);                                   /*@Z427VLS*/
         LA    @01,@AL00328                                        0328
         BAL   @14,HEXPACK                                         0328
*      INDEX1 = INDEX1 + 1;                 /* INCR OVER DELIMITER   */
         AL    INDEX1,@CF00055                                     0329
*    END;                                                  /*@Z427VLS*/
*  ELSE                                                            0331
*    DO;                                    /* BAD INPUT LENGTH      */
         B     @RC00318                                            0331
@RF00318 DS    0H                                                  0332
*      CALL OUTMSG(SYNTAX);                                /*@Z427VLS*/
         LA    @01,@AL00332                                        0332
         BAL   @14,OUTMSG                                          0332
*      INDEX1 = INDEX1 + NEXTC + 1;                        /*@Z427VLS*/
         LR    @08,INDEX1                                          0333
         AL    @08,NEXTC                                           0333
         AL    @08,@CF00055                                        0333
         LR    INDEX1,@08                                          0333
*    END;                                                  /*@Z427VLS*/
