*YEARDAY: PROC OPTIONS(SAVE(1,14));                        /*@Z427VLS*/
YEARDAY  ST    @14,@SA00010                                        0275
         ST    @01,@SA00010+4                                      0275
*    /*                                                      @Z427VLS*/
*    /* THE FIELD IS FIRST CHECKED FOR VALID NUMERICS.  THE FIRST    */
*    /* FOUR HEX VALUES ARE THEN PACKED INTO THE FIELD HOLD, AND THEN*/
*    /* PLACED IN THE YEAR/DAY HOLD AREA (YYDDD).  THE REMAINING HEX */
*    /* VALUE IS SHIFTED AND ORED WITH A SIGN FLAG.  THE FIELD IS    */
*    /* THEN CHECKED AGAINST DAY 366.  IF GREATER, AN ERROR MESSAGE  */
*    /* IS OUTPUT.                                           @Z427VLS*/
*    /*                                                      @Z427VLS*/
*    CALL VALIDNUM(5);                                     /*@Z427VLS*/
         LA    @01,@AL00276                                        0276
         BAL   @14,VALIDNUM                                        0276
*    CALL HEXPACK(4);                                      /*@Z427VLS*/
         LA    @01,@AL00277                                        0277
         BAL   @14,HEXPACK                                         0277
*    YY = HOLD(1);                                         /*@Z427VLS*/
         MVC   YY(1),HOLD                                          0278
*    DD = HOLD(2);                                         /*@Z427VLS*/
         MVC   DD(1),HOLD+1                                        0279
*    SLL(PARMFLD(INDEX1),FOUR);                            /*@Z427VLS*/
         SLR   @08,@08                                             0280
         IC    @08,PARMFLD-1(INDEX1)                               0280
         SLL   @08,4                                               0280
         STC   @08,PARMFLD-1(INDEX1)                               0280
*    DS = PARMFLD(INDEX1)  SIGNFLAG;                      /*@Z427VLS*/
         LA    @08,PARMFLD-1(INDEX1)                               0281
         MVC   DS(1),0(@08)                                        0281
         OI    DS,X'0F'                                            0281
*    IF DDDS > '366F'X  DDDS = '000F'X THEN                       0282
         CLC   DDDS(2),@CB00749                                    0282
         BH    @RT00282                                            0282
         CLC   DDDS(2),@CB00750                                    0282
         BNE   @RF00282                                            0282
@RT00282 DS    0H                                                  0283
*      DO PLACE = 2;                                       /*@Z427VLS*/
         LA    @08,2                                               0283
         ST    @08,PLACE                                           0283
*        CALL OUTMSG(SYNTAX);                              /*@Z427VLS*/
         LA    @01,@AL00284                                        0284
         BAL   @14,OUTMSG                                          0284
*      END;                                                /*@Z427VLS*/
*    INDEX1 = INDEX1 + 1;                                  /*@Z427VLS*/
@RF00282 AL    INDEX1,@CF00055                                     0286
*END;                                                      /*@Z427VLS*/
@EL00010 DS    0H                                                  0287
@EF00010 DS    0H                                                  0287
@ER00010 L     @14,@SA00010                                        0287
         L     @01,@SA00010+4                                      0287
         BR    @14                                                 0287
*/********************************************************************/
*/*                                                          @Z427VLS*/
*/*  PACK A FIELD OF A GIVEN LENGTH INTO THE VARIABLE HOLD.  THE     */
*/*  INPUT IS ASSUMED TO BE HEX CHARACTERS, WITH A LEADING ZERO FOR  */
*/*  EACH TWO DIGITS.                                        @Z427VLS*/
*/********************************************************************/
*HEXPACK: PROC (HEXLEN) OPTIONS(SAVE(1,14));               /*@Z427VLS*/
HEXPACK  ST    @14,12(,@13)                                        0288
         ST    @01,24(,@13)                                        0288
         MVC   @PC00011(4),0(@01)                                  0288
*   DCL HEXLEN FIXED(31);                                  /*@Z427VLS*/
*   INDEX3 = HEXLEN / 2;                                   /*@Z427VLS*/
         L     @08,@PC00011                                        0290
         L     @00,HEXLEN(,@08)                                    0290
         SRDA  @00,32                                              0290
         D     @00,@CF00074                                        0290
         ST    @01,INDEX3                                          0290
*   DO I = 1 TO INDEX3 BY 1;                               /*@Z427VLS*/
         LA    I,1                                                 0291
         B     @DE00291                                            0291
@DL00291 DS    0H                                                  0292
*     /*                                                     @Z427VLS*/
*     /* THE DATA IN PARMFLD IS ACCESSED TWO CHARACTERS AT A TIME.   */
*     /* THE FIRST CHARACTER IS SHIFTED LEFT FOUR BITS AND THEN ORED */
*     /* WITH THE SECOND CHARACTER.  THE SECOND CHARACTER IS ASSUMED */
*     /* TO HAVE THE FIRST FOUR BITS AS ZEROES.              @Z427VLS*/
*     /*                                                     @Z427VLS*/
*     HOLD(I) = PARMFLD(INDEX1);                           /*@Z427VLS*/
         LA    @08,HOLD-1(I)                                       0292
         LA    @07,PARMFLD-1(INDEX1)                               0292
         MVC   0(1,@08),0(@07)                                     0292
*     SLL (HOLD(I),FOUR);                                  /*@Z427VLS*/
         SLR   @08,@08                                             0293
         IC    @08,HOLD-1(I)                                       0293
         SLL   @08,4                                               0293
         STC   @08,HOLD-1(I)                                       0293
*     HOLD(I) = HOLD(I)  PARMFLD(INDEX1 + 1);             /*@Z427VLS*/
         LA    @08,HOLD-1(I)                                       0294
         LA    @07,PARMFLD(INDEX1)                                 0294
         OC    0(1,@08),0(@07)                                     0294
*     INDEX1 = INDEX1 + 2;                                 /*@Z427VLS*/
         AL    INDEX1,@CF00074                                     0295
*   END;                                                   /*@Z427VLS*/
         AL    I,@CF00055                                          0296
@DE00291 C     I,INDEX3                                            0296
         BNH   @DL00291                                            0296
*END;                                                      /*@Z427VLS*/
@EL00011 DS    0H                                                  0297
@EF00011 DS    0H                                                  0297
@ER00011 L     @14,12(,@13)                                        0297
         L     @01,24(,@13)                                        0297
         BR    @14                                                 0297
