*/*                                                          @Z427VLS*/
*/*      SYNTAX CHECK FOR TERMN (1-8 ALPHANUMERIC CHARACTERS)        */
*/*                                                          @Z427VLS*/
*/********************************************************************/
*PARMTERM:  PROC OPTIONS(SAVE(1,14));                      /*@Z427VLS*/
PARMTERM ST    @14,@SA00027                                        0599
         ST    @01,@SA00027+4                                      0599
*             IF TERMFLG = ON THEN           /* TERMN ALREADY SPECIF */
         TM    TERMFLG,B'00001000'                                 0600
         BNO   @RF00600                                            0600
*               CALL DUPLICAT;                             /*@Z427VLS*/
         BAL   @14,DUPLICAT                                        0601
*             ELSE                                                 0602
*               DO INDEX1 = INDEX1 + 6;      /* INCR OVER PARM       */
         B     @RC00600                                            0602
@RF00600 LA    @08,6                                               0602
         ALR   @08,INDEX1                                          0602
         LR    INDEX1,@08                                          0602
*                 TERMFLG = ON;                            /*@Z427VLS*/
         OI    TERMFLG,B'00001000'                                 0603
*                 /*                                         @Z427VLS*/
*                 /* FIND THE TERMINATOR.  IF THE LENGTH IS BAD, O/P */
*                 /* AN ERROR  MESSAGE.  ELSE MOVE IN THE CORRECT    */
*                 /* FIELD.                                  @Z427VLS*/
*                 /*                                         @Z427VLS*/
*                 CALL SCANC;                              /*@Z427VLS*/
         BAL   @14,SCANC                                           0604
*                 IF NEXTC = 0  NEXTC > 8 THEN                    0605
         L     @08,NEXTC                                           0605
         LTR   @08,@08                                             0605
         BZ    @RT00605                                            0605
         C     @08,@CF00066                                        0605
         BNH   @RF00605                                            0605
@RT00605 DS    0H                                                  0606
*                   CALL OUTMSG(SYNTAX);                   /*@Z427VLS*/
         LA    @01,@AL00606                                        0606
         BAL   @14,OUTMSG                                          0606
*                 ELSE                                             0607
*                   DO;                                    /*@Z427VLS*/
         B     @RC00605                                            0607
@RF00605 DS    0H                                                  0608
*                     TERM (1:NEXTC) = PARMFLD(INDEX1:INDEX1+NEXTC-1);
         L     @08,NEXTC                                           0608
         BCTR  @08,0                                               0608
         LA    @04,PARMFLD-1(INDEX1)                               0608
         EX    @08,@SM01062                                        0608
*                     VALID = OFF;                         /*@Z427VLS*/
         NI    VALID,B'01111111'                                   0609
*                     DO J = 11 TO 38 WHILE (VALID = OFF); /*@Z427VLS*/
         LA    J,11                                                0610
@DL00610 TM    VALID,B'10000000'                                   0610
         BNZ   @DC00610                                            0610
*                       /*                                   @Z427VLS*/
*                       /* FIRST CHARACTER MUST BE ALPHABETIC        */
*                       /*                                   @Z427VLS*/
*                       IF PARMFLD(INDEX1) = TABLE(J) THEN         0611
         LA    @08,PARMFLD-1(INDEX1)                               0611
         LA    @03,TABLE-1(J)                                      0611
         CLC   0(1,@08),0(@03)                                     0611
         BNE   @RF00611                                            0611
*                         VALID = ON;                      /*@Z427VLS*/
         OI    VALID,B'10000000'                                   0612
*                     END;                                 /*@Z427VLS*/
@RF00611 AL    J,@CF00055                                          0613
