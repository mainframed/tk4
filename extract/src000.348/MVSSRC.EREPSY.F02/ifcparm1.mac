*/*                                                          @Z427VLS*/
*/********************************************************************/
*PARMMOD:  PROC OPTIONS(SAVE(1,14));                       /*@Z427VLS*/
PARMMOD  ST    @14,@SA00026                                        0552
         ST    @01,@SA00026+4                                      0552
*             NEXTRPC = OFF;                 /* LOOK FOR R PAREN     */
         NI    NEXTRPC,B'11111011'                                 0553
*             IF MODFLG = ON THEN            /* MODEL ALREADY SPEC   */
         TM    MODFLG,B'00000100'                                  0554
         BNO   @RF00554                                            0554
*               CALL DUPLICAT;                             /*@Z427VLS*/
         BAL   @14,DUPLICAT                                        0555
*             ELSE                                                 0556
*               DO INDEX1= INDEX1 + 5;       /* INCR OVER PARM       */
         B     @RC00554                                            0556
@RF00554 LA    @08,5                                               0556
         ALR   @08,INDEX1                                          0556
         LR    INDEX1,@08                                          0556
*                 MODFLG = ON;                             /*@Z427VLS*/
         OI    MODFLG,B'00000100'                                  0557
*                 /*                                         @Z427VLS*/
*                 /* GET SPACE FOR TABLE.  ESTABLISH THE BASE POINTER*/
*                 /* CLEAR THE TABLE.                        @Z427VLS*/
*                 /*                                         @Z427VLS*/
*                 RESPECIFY (R1) RESTRICTED;               /*@Z427VLS*/
*                 R1 = 40;                                 /*@Z427VLS*/
         LA    R1,40                                               0559
*                 GEN(GETMAIN R,LV=(1),SP=20);             /*@Z427VLS*/
         GETMAIN R,LV=(1),SP=20
*                 MODPTR = R1;                             /*@Z427VLS*/
         ST    R1,MODPTR(,R2)                                      0561
*                 RESPECIFY (R1) UNRESTRICTED;             /*@Z427VLS*/
*                 INDEX2 = ZERO;                           /*@Z427VLS*/
         SLR   @08,@08                                             0563
         ST    @08,INDEX2                                          0563
*                 MODELS = MODELS && MODELS;               /*@Z427VLS*/
         L     @08,MODPTR(,R2)                                     0564
         XC    MODELS(10,@08),MODELS(@08)                          0564
*                 DO UNTIL (RPORC = OFF);                  /*@Z427VLS*/
@DL00565 DS    0H                                                  0566
*                   CALL SCANCRP;              /* FIND TERMINATOR    */
         BAL   @14,SCANCRP                                         0566
*                   FOUND = OFF;                           /*@Z427VLS*/
         NI    FOUND,B'11101111'                                   0567
*                   /*                                       @Z427VLS*/
*                   /* IF THE FIELD IS 3 CHARS LONG, INSERT A ZERO   */
*                   /* IN FRONT OF THE FIELD AND CHANGE THE LENGTH   */
*                   /* OF THE FIELD TO FOUR.                 @Z427VLS*/
*                   /*                                       @Z427VLS*/
*                   IF NEXTC = 3 THEN                              0568
         CLC   NEXTC(4),@CF00076                                   0568
         BNE   @RF00568                                            0568
*                     DO;                                  /*@Z427VLS*/
*                       INDEX1 = INDEX1 - 1;               /*@Z427VLS*/
         BCTR  INDEX1,0                                            0570
*                       PARMFLD(INDEX1) = '0';             /*@Z427VLS*/
         LA    @08,PARMFLD-1(INDEX1)                               0571
         MVI   0(@08),C'0'                                         0571
*                       NEXTC = 4;                         /*@Z427VLS*/
         MVC   NEXTC(4),@CF00064                                   0572
*                       PLACE = -1;                        /*@Z427VLS*/
         MVC   PLACE(4),@CF00726                                   0573
*                     END;                                 /*@Z427VLS*/
*                   /*                                       @Z427VLS*/
*                   /* LOOK FOR THE MODEL IN THE CPUTABLE.  IF FOUND */
*                   /* PLACE THE HEX CONVERSION IN THE LOOK UP TABLE */
*                   /* FOR THE FILTER.                       @Z427VLS*/
*                   /*                                       @Z427VLS*/
*                   IF NEXTC = 4 THEN                              0575
@RF00568 CLC   NEXTC(4),@CF00064                                   0575
         BNE   @RF00575                                            0575
*                     DO I = DIM(CPUTABLE) TO 1 BY -1              0576
*                                    WHILE (FOUND = OFF);  /*@Z427VLS*/
         LA    I,16                                                0576
@DL00576 TM    FOUND,B'00010000'                                   0576
         BNZ   @DC00576                                            0576
*                       IF PARMFLD(INDEX1:INDEX1+3) = CPUEBDIC(I) THEN
         LR    @08,I                                               0577
         MH    @08,@CH00081                                        0577
         LA    @07,PARMFLD-1(INDEX1)                               0577
         LA    @04,CPUEBDIC-6(@08)                                 0577
         CLC   0(4,@07),0(@04)                                     0577
         BNE   @RF00577                                            0577
*                         DO;                              /*@Z427VLS*/
*                           FOUND = ON;                    /*@Z427VLS*/
         OI    FOUND,B'00010000'                                   0579
*                           /*                               @Z427VLS*/
*                           /* DETERMINE INDEX AND STORE VALUE.      */
*                           /*                               @Z427VLS*/
*                           IF INDEX2 < DIM(MODVAL) THEN           0580
         L     @08,INDEX2                                          0580
         C     @08,@CF00064                                        0580
         BNL   @RF00580                                            0580
*                             INDEX2 = INDEX2 + 1;         /*@Z427VLS*/
         AL    @08,@CF00055                                        0581
         ST    @08,INDEX2                                          0581
*                           ELSE                                   0582
*                             CALL OUTMSG(SYNTAX);         /*@Z427VLS*/
         B     @RC00580                                            0582
@RF00580 LA    @01,@AL00582                                        0582
         BAL   @14,OUTMSG                                          0582
*                           /*                               @Z427VLS*/
*                           /* STORE VALUE.                  @Z427VLS*/
*                           /*                               @Z427VLS*/
*                           IF CPUNO(I) = HEXZEROS THEN            0583
@RC00580 LR    @08,I                                               0583
         MH    @08,@CH00081                                        0583
         LA    @07,CPUNO-6(@08)                                    0583
         CLC   0(2,@07),@CB00208                                   0583
         BNE   @RF00583                                            0583
*                             CALL OUTMSG(DUPE);           /*@Z427VLS*/
         LA    @01,@AL00584                                        0584
         BAL   @14,OUTMSG                                          0584
*                           ELSE                                   0585
*                             DO;                          /*@Z427VLS*/
         B     @RC00583                                            0585
@RF00583 DS    0H                                                  0586
*                               MODVAL(INDEX2) = CPUNO(I); /*@Z427VLS*/
         L     @08,INDEX2                                          0586
         ALR   @08,@08                                             0586
         L     @07,MODPTR(,R2)                                     0586
         ALR   @07,@08                                             0586
         LR    @08,I                                               0586
         MH    @08,@CH00081                                        0586
         LA    @04,CPUNO-6(@08)                                    0586
         MVC   MODVAL-2(2,@07),0(@04)                              0586
*                               CPUNO(I) = HEXZEROS;       /*@Z427VLS*/
         LA    @07,CPUNO-6(@08)                                    0587
         MVC   0(2,@07),@CB00208                                   0587
*                             END;                         /*@Z427VLS*/
*                         END;                             /*@Z427VLS*/
@RC00583 DS    0H                                                  0590
*                     END;                                 /*@Z427VLS*/
@RF00577 BCTR  I,0                                                 0590
         LTR   I,I                                                 0590
         BP    @DL00576                                            0590
@DC00576 DS    0H                                                  0591
*                   IF FOUND = OFF THEN                            0591
@RF00575 TM    FOUND,B'00010000'                                   0591
         BNZ   @RF00591                                            0591
*                     CALL OUTMSG(SYNTAX);                 /*@Z427VLS*/
         LA    @01,@AL00592                                        0592
         BAL   @14,OUTMSG                                          0592
*                   INDEX1 = INDEX1 + NEXTC + 1;           /*@Z427VLS*/
@RF00591 LR    @08,INDEX1                                          0593
         AL    @08,NEXTC                                           0593
         AL    @08,@CF00055                                        0593
         LR    INDEX1,@08                                          0593
*                   PLACE = ZERO;                          /*@Z427VLS*/
         SLR   @08,@08                                             0594
         ST    @08,PLACE                                           0594
*                 END;                                     /*@Z427VLS*/
@DE00565 TM    RPORC,B'10000000'                                   0595
         BNZ   @DL00565                                            0595
*                 MODNUM = INDEX2;        /* STORE # OF ENTRIES     */
         L     @08,MODPTR(,R2)                                     0596
         L     @04,INDEX2                                          0596
         STH   @04,MODNUM(,@08)                                    0596
