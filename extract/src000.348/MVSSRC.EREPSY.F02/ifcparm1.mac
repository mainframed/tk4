         ST    @01,@SA00012+4                                      0298
         MVC   @PC00012(4),0(@01)                                  0298
*    DCL INCR FIXED(31);                 /* LENGTH OF THE PARAMETER  */
*    INDEX1 = INDEX1 + INCR;             /* STEP OVER PARM   @Z427VLS*/
         L     @08,@PC00012                                        0300
         AL    INDEX1,INCR(,@08)                                   0300
