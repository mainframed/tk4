*/*                                                          @Z427VLS*/
*/*  CONVERT THE HEXADECIMAL VALUES IN THE BYTES STRAIGHT TO @Z427VLS*/
*/*  CHARACTER REPRESENTATION.                               @Z427VLS*/
*/********************************************************************/
*UNPACK: PROC (OPLEN) OPTIONS(SAVE(1,14));                 /*@Z427VLS*/
UNPACK   ST    @14,@SA00004                                        0131
         ST    @01,@SA00004+4                                      0131
         MVC   @PC00004(4),0(@01)                                  0131
*      DCL OPLEN   FIXED(31);            /* NUMBER OF CHAR TO CONVERT*/
*      DO J = 1 TO OPLEN BY 1;                             /*@Z427VLS*/
         LA    J,1                                                 0133
         B     @DE00133                                            0133
@DL00133 DS    0H                                                  0134
*        HOLDAREA = HOLDAREA && HOLDAREA;  /* CHEAR HOLD AREA        */
         XC    HOLDAREA(5),HOLDAREA                                0134
*        HOLDAREA(2) = PARMFLD(J);       /* CHARACTERS ARE IN PARMFLD*/
         LA    @08,PARMFLD-1(J)                                    0135
         MVC   HOLDAREA+1(1),0(@08)                                0135
*        SLL (HOLDAREA(1:2),FOUR);       /* SHIFT FIRST TO FIRST BYTE*/
         SLR   @08,@08                                             0136
         ICM   @08,3,HOLDAREA                                      0136
         SLL   @08,4                                               0136
         STCM  @08,3,HOLDAREA                                      0136
*        SRL (HOLDAREA(2:2),FOUR);       /* SECOND TO SECOND BYTE    */
         SLR   @08,@08                                             0137
         IC    @08,HOLDAREA+1                                      0137
         SRL   @08,4                                               0137
         STC   @08,HOLDAREA+1                                      0137
*        INDEX3 = HOLDAREA(1) + 1;       /* GET VALUE AS INDEX       */
         LA    @08,1                                               0138
         SLR   INDEX3,INDEX3                                       0138
         IC    INDEX3,HOLDAREA                                     0138
         ALR   INDEX3,@08                                          0138
*        PARMPT(2*J+1) = NUMTAB(INDEX3);  /* STORE CHARACTER @Z427VLS*/
         LR    @07,J                                               0139
         ALR   @07,@07                                             0139
         LA    @03,PARMPT(@07)                                     0139
         LA    @01,NUMTAB-1(INDEX3)                                0139
         MVC   0(1,@03),0(@01)                                     0139
*        INDEX3 = HOLDAREA(2) + 1;       /* GET VALUE AS INDEX       */
         SLR   INDEX3,INDEX3                                       0140
         IC    INDEX3,HOLDAREA+1                                   0140
         ALR   @08,INDEX3                                          0140
         LR    INDEX3,@08                                          0140
*        PARMPT(2*J+2) = NUMTAB(INDEX3);  /* STORE CHARACTER @Z427VLS*/
         LA    @08,PARMPT+1(@07)                                   0141
         LA    @07,NUMTAB-1(INDEX3)                                0141
         MVC   0(1,@08),0(@07)                                     0141
*      END;                                                /*@Z427VLS*/
         AL    J,@CF00058                                          0142
@DE00133 L     @08,@PC00004                                        0142
         C     J,OPLEN(,@08)                                       0142
         BNH   @DL00133                                            0142
*      PRINTADR = ADDR(PARMPT);                            /*@Z427VLS*/
         LA    @08,PARMPT                                          0143
         ST    @08,PRINTADR(,R2)                                   0143
*      CALL IOHND(RITEPRTR);                               /*@Z427VLS*/
         L     @15,ADIOHND(,R2)                                    0144
         LA    @01,@AL00144                                        0144
         BALR  @14,@15                                             0144
*   END;                                                   /*@Z427VLS*/
@EL00004 DS    0H                                                  0145
@EF00004 DS    0H                                                  0145
@ER00004 L     @14,@SA00004                                        0145
