         ALR   @08,@07                                             0235
         ST    @08,INDEX2                                          0235
*  KEYWORD(2*J) = NUMTAB(INDEX2);                          /*@Z427VLS*/
         LA    @07,KEYWORD-1(@03)                                  0236
         LA    @03,NUMTAB-1(@08)                                   0236
         MVC   0(1,@07),0(@03)                                     0236
*END;                                                      /*@Z427VLS*/
@EL00009 DS    0H                                                  0237
@EF00009 DS    0H                                                  0237
