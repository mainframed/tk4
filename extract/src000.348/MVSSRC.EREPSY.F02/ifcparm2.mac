*/*                                                          @Z427VLS*/
*/*  PACK THE HEXADECIMAL CHARACTER INTO TWO PRINTABLE CHARACTERS.   */
*/*  THE HEX CHARACTER IS IN HOLDAREA(2), THE RESULT IS PLACED IN    */
*/*  KEYWORD, INDEXED BY THE VARIABLE J.                     @Z427VLS*/
*/*                                                          @Z427VLS*/
*/********************************************************************/
*CHARPACK: PROC OPTIONS(SAVE(1,14));                       /*@Z427VLS*/
CHARPACK ST    @14,12(,@13)                                        0230
         ST    @01,24(,@13)                                        0230
*  SLL (HOLDAREA(1:2),FOUR);                               /*@Z427VLS*/
         SLR   @08,@08                                             0231
         ICM   @08,3,HOLDAREA                                      0231
         SLL   @08,4                                               0231
         STCM  @08,3,HOLDAREA                                      0231
*  SRL (HOLDAREA(2:2),FOUR);                               /*@Z427VLS*/
         SLR   @08,@08                                             0232
         IC    @08,HOLDAREA+1                                      0232
         SRL   @08,4                                               0232
         STC   @08,HOLDAREA+1                                      0232
*  INDEX2 = HOLDAREA(1) + 1;                               /*@Z427VLS*/
         LA    @08,1                                               0233
         SLR   @07,@07                                             0233
         IC    @07,HOLDAREA                                        0233
         ALR   @07,@08                                             0233
         ST    @07,INDEX2                                          0233
*  KEYWORD(2*J-1) = NUMTAB(INDEX2);                        /*@Z427VLS*/
         LR    @03,J                                               0234
         ALR   @03,@03                                             0234
         LA    @01,KEYWORD-2(@03)                                  0234
         LA    @15,NUMTAB-1(@07)                                   0234
         MVC   0(1,@01),0(@15)                                     0234
*  INDEX2 = HOLDAREA(2) + 1;                               /*@Z427VLS*/
         SLR   @07,@07                                             0235
