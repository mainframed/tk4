*WTLREC:                                                           0293
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0293
*                                                                  0293
         B     @PB00002                                            0293
WTLREC   DS    0H                                                  0294
*   /*****************************************************************/
*   /*                                                               */
*   /* ENTRY IS TO SET UP THE WTL RECORD ELEMENTS IN CORRECT ORDER   */
*   /* (FIRST IN / FIRST OUT -(FIFO)) FROM THE WTL QUEUE POOL WHICH  */
*   /* WAS SET UP IN (LAST IN / FIRST OUT-(LIFO)) ORDER. EACH RECORD */
*   /* ELEMENT HAS A BACKWARD PTR TO THE PREVIOUS ELEMENT ON THE WTL */
*   /* POOL CHAIN, EXCEPT THE FIRST RECORD ELEMENT, WHICH HAS A ZERO */
*   /* PTR FIELD (CHAINPTR). THE PTR TO THE WTL QUEUE CHAIN AND COUNT*/
*   /* OF RECORD ELEMENTS ON THE CHAIN (LCAQHEAD) IS LOADED INTO AN  */
*   /* EVEN/ODD PAIR OF REGS, AND THE VALUES ARE PROPAGATED TO       */
*   /* ANOTHER EVEN/ODD PAIR OF REGS WITH THE COUNT VALUE DECREMENTED*/
*   /* BY ONE. THE NEW VALUES ARE STORED BACK TO LCAQHEAD USING      */
*   /* COMPARE DOUBLE AND SWAP (CDS) INSTRUCTION FOR SERIALIZATION OF*/
*   /* THE FIELD. THE WTL POOL CHAIN IS SCANNED FOR A ZERO PTR TO    */
*   /* NEXT ELEMENT. THIS IS THE RECORD ELEMENT THAT WILL BE PUT TO  */
*   /* THE SYSTEM LOG DATA SET. THE BACKWARD PTR OF THE PREVIOUS     */
*   /* RECORD ELEMENT IS SET TO ZERO AND THE PUT RTN PUTREC IS CALLED*/
*   /* TO WRITE THE RECORD OUT. RETURN CODES (REG15) ARE DETERMINED  */
*   /* BY THE VALUE OF THE LCAQHEAD COUNT FIELD. IF THE COUNT IS     */
*   /* ZERO, REG15 IS SET ZERO, IF THE COUNT IS NOT ZERO, REG15 IS   */
*   /* SET TO FOUR. RETURN CODES ARE PROCESSED BY CALLER.            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0294
*   DCL                                                            0294
*     AGAN LABEL;                   /* USED BY 'CDS'                 */
*   RFY                                                            0295
*    (PR1ELCNT,                                                    0295
*     PR1ELAD,                                                     0295
*     PR2ELCNT,                                                    0295
*     PR2ELAD) RESTRICTED;                                         0295
*   REGSAV=REG14;                   /* SAVE RETURN ADDRESS           */
         LR    REGSAV,REG14                                        0296
*   RETCODE=4;                      /* SET LOOP SWITCH       @ZA07784*/
         MVI   RETCODE,X'04'                                       0297
*   DO WHILE(RETCODE=4);            /* SET PUT LOOP          @ZA07784*/
         B     @DE00298                                            0298
@DL00298 DS    0H                                                  0299
*     GEN( LM    PR1ELCNT,PR1ELAD,LCAQHEAD(LCAPTR));/* SET UP      0299
*                                      'CDS'REGS                     */
          LM    PR1ELCNT,PR1ELAD,LCAQHEAD(LCAPTR)
*     IF PR1ELCNT^=0000 THEN        /* ANY QUEUE ELEMENTS TO PUT OUT */
         LTR   PR1ELCNT,PR1ELCNT                                   0300
         BZ    @RF00300                                            0300
*       DO;                                                        0301
*                                                                  0301
*         /***********************************************************/
*         /*                                                         */
*         /* RECORD ELEMENT CHAIN IS NOT EMPTY. SET UP TO DECREMENT  */
*         /* THE ELEMENT COUNT -LCAQHEAD(LCAELCNT) IN REG6 (PR1ELCNT)*/
*         /* USING 'CDS' FOR SERIALIZATION ON THE WTL QUEUE CHAIN PTR*/
*         /* (LCAQHEAD)                                              */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0302
*AGAN:                                                             0302
*         PR2ELCNT=PR1ELCNT-1;      /* DECREMENT ELEMENT COUNT       */
AGAN     LR    PR2ELCNT,PR1ELCNT                                   0302
         BCTR  PR2ELCNT,0                                          0302
*         PR2ELAD=PR1ELAD×REGMASK;  /* SET HI-BIT FLAG ON            */
         LR    PR2ELAD,PR1ELAD                                     0303
         O     PR2ELAD,REGMASK                                     0303
*         IF PR2ELCNT=0000 THEN     /* THIS LAST ELEMENT ON QUEUE    */
         LTR   PR2ELCNT,PR2ELCNT                                   0304
         BNZ   @RF00304                                            0304
*           PR2ELAD=0000;           /* ZERO CHAIN PTR                */
         SLR   PR2ELAD,PR2ELAD                                     0305
*         CDS(PR1ELCNT,PR2ELCNT,LCAQHEAD);/* SWAP IN NEW VALUES      */
@RF00304 CDS   PR1ELCNT,PR2ELCNT,LCAQHEAD(LCAPTR)                  0306
*         BC(7,AGAN);               /* RE-PROCESS IF COMPARE FAILED  */
         BC    7,AGAN                                              0307
*         IF PR2ELCNT^=0000 THEN    /* NOT LAST ELEMENT ON QUEUE     */
         LTR   PR2ELCNT,PR2ELCNT                                   0308
         BZ    @RF00308                                            0308
*           DO;                                                    0309
*                                                                  0309
*             /*******************************************************/
*             /*                                                     */
*             /* SCAN THE WTL QUEUE CHAIN FOR A ZERO PTR TO NEXT     */
*             /* ELEMENT- (CHAINPTR). THE RECORD ELEMENT WITH THIS   */
*             /* PTR IS THE LAST ELEMENT ON THE CHAIN AND WILL BE PUT*/
*             /* TO THE LOG DATA SET THE (CHAINPTR) FIELD OF THE     */
*             /* PREVIOUS ELEMENT WILL BE SET TO ZERO FOR THE NEXT   */
*             /* QUEUE SCAN                                          */
*             /*                                                     */
*             /*******************************************************/
*                                                                  0310
*             DO WHILE(CHAINPTR^=0000);/* SCAN FOR LAST ELEMENT      */
         B     @DE00310                                            0310
@DL00310 DS    0H                                                  0311
*               PREVREG=PR1ELAD;    /* SAVE PREVIOUS ELEMENT PTR     */
         ST    PR1ELAD,PREVREG                                     0311
*               PR1ELAD=CHAINPTR;   /* GET NEXT ELEMENT PTR          */
         L     PR1ELAD,CHAINPTR(,PR1ELAD)                          0312
*               IF PR1ELAD^=0000 THEN/* NOT LAST WTL ELEMENT @ZA07784*/
         LTR   PR1ELAD,PR1ELAD                                     0313
         BZ    @RF00313                                            0313
*                 DO;                                              0314
*                   IF PREVCNT^=SWITCHFD+1 THEN/* BAD CNT    @ZA07784*/
         L     @09,PREVREG                                         0315
         LA    @08,1                                               0315
         AL    @08,SWITCHFD(,PR1ELAD)                              0315
         CL    @08,PREVCNT(,@09)                                   0315
         BE    @RF00315                                            0315
*                     IF MSWTL='1'B THEN/* IN RECURSION      @ZA07784*/
         LA    @09,136                                             0316
         L     @08,CVTPTR                                          0316
         AL    @09,CVTMSER(,@08)                                   0316
         TM    MSWTL(@09),B'00100000'                              0316
         BNO   @RF00316                                            0316
*                       DO;                                        0317
*                         LCASTAE='1'B;/* SET LOG RECURS BIT @ZA07784*/
         MVI   LCASTAE(LCAPTR),X'01'                               0318
*                         MSWTL='0'B;/* ZERO BUFFER RECURS   @ZA07784*/
         NI    MSWTL(@09),B'11011111'                              0319
*                         CALL ABENDRTN;/* GO ISSUE ABEND. NO RETURN
*                                      FROM THIS CALLED RTN. @ZA07784*/
         BAL   @14,ABENDRTN                                        0320
*                       END;                                       0321
*                     ELSE                                         0322
*                       CALL CHAINRTN;/* GO REPAIR CHAIN     @ZA07784*/
         B     @RC00316                                            0322
@RF00316 BAL   @14,CHAINRTN                                        0322
*                   ELSE                                           0323
*                     ;                                            0323
@RF00315 DS    0H                                                  0324
*                 END;                                             0324
@RC00315 DS    0H                                                  0325
*             END;                                                 0325
@RF00313 DS    0H                                                  0325
@DE00310 SLR   @09,@09                                             0325
         C     @09,CHAINPTR(,PR1ELAD)                              0325
         BNE   @DL00310                                            0325
*             ZEROPTR=0000;         /* PREVIOUS ELEMENT PTR FIELD    */
         L     @08,PREVREG                                         0326
         ST    @09,ZEROPTR(,@08)                                   0326
*           END;                                                   0327
*         ELSE                                                     0328
*           RETCODE='00'X;          /* SET LAST WTL INDICATOR        */
         B     @RC00308                                            0328
@RF00308 MVI   RETCODE,X'00'                                       0328
*         CALL PUTREC;              /* CALL PUT RTN                  */
@RC00308 BAL   @14,PUTREC                                          0329
*       END;                                                       0330
*     ELSE                                                         0331
*       RETCODE=0000;               /* NO WTLS TO WRITE      @ZA07784*/
         B     @RC00300                                            0331
@RF00300 MVI   RETCODE,X'00'                                       0331
*   END;                            /*                       @ZA07784*/
@RC00300 DS    0H                                                  0332
@DE00298 CLI   RETCODE,4                                           0332
         BE    @DL00298                                            0332
*   IF RETCODE=4 THEN               /* NOT LAST WTL ON QUEUE         */
         CLI   RETCODE,4                                           0333
         BNE   @RF00333                                            0333
*     REG15=4;                      /* SET RETURN CODE FOR CALLER    */
         LA    REG15,4                                             0334
*   ELSE                            /* ALL WTLS PUT TO LOG           */
*     REG15=0000;                   /* SET RETURN CODE TO CALLER     */
         B     @RC00333                                            0335
@RF00333 SLR   REG15,REG15                                         0335
*   RFY                                                            0336
*    (PR1ELCNT,                                                    0336
*     PR1ELAD,                                                     0336
*     PR2ELCNT,                                                    0336
*     PR2ELAD) UNRESTRICTED;                                       0336
@RC00333 DS    0H                                                  0337
*   REG14=REGSAV;                   /* RESTORE RETURN ADDRESS        */
         LR    REG14,REGSAV                                        0337
*   END WTLREC;                                                    0338
@EL00002 DS    0H                                                  0338
@EF00002 DS    0H                                                  0338
@ER00002 BR    @14                                                 0338
*PUTREC:                                                           0339
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0339
*                                                                  0339
PUTREC   DS    0H                                                  0340
*   /*****************************************************************/
*   /*                                                               */
*   /* ENTRY IS TO INITIALIZE THE RPL CONTROL BLOCK WITH THE SIZE AND*/
*   /* ADDRESS OF THE MESSAGE TEXT TO BE PUT TO THE SYSTEM LOG DATA  */
*   /* SET, AND TO RELEASE THE WTL QUEIE POOL ELEMENT VIA A FREECELL */
*   /* MACRO . FOR THE PUT, REG1 IS SET UP WITH THE ADDRESS OF THE   */
*   /* RPL CONTROL BLOCK AND THE PUT MACRO IS ISSUED. IF THE PUT IS  */
*   /* NOT SUCCESSFUL ( REG15 = FOUR ), THE LOGCORE AND CONTROL AREAS*/
*   /* ARE DUMPED VIA CALL TO DUMPRTN, AND AN ABEND MACRO IS ISSUED. */
*   /* THE LOG ESTAE PROCESSOR, IEEMB806, WILL ATTEMPT LOG RECOVERY. */
*   /* EXIT IS VIA ABEND . OTHERWISE, THE WTL ELEMENT IS RELEASED FOR*/
*   /* RE-USE VIA THE FREECELL MACRO. REG15 IS TESTED FOR A VALUE OF */
*   /* TWENTY AND IF EQUAL, THE FREED ELEMENT WAS THE LAST ELEMENT IN*/
*   /* USE IN A WTL QUEUE POOL EXTENSION AREA. THE POOL EXTENSION IS */
*   /* FREED VIA A FREEMAIN MACRO, WITH REG0 AND REG1 POINTING TO THE*/
*   /* POOL SIZE AND THE POOL ADDRESS. THESE POINTERS ARE RETURNED BY*/
*   /* THE FREECELL PROCESSING WHEN THIS CONDITION EXISTS. RETURN TO */
*   /* CALLER .                                                      */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0340
*   RETREG1=REG14;                  /* SAVE RETURN ADDRESS           */
         LR    RETREG1,REG14                                       0340
*   RFY                                                            0341
*    (REG5) RESTRICTED;                                            0341
*   RPLAREA=ADDR(DATATXT);          /* PTR TO RECORD TO BE PUT       */
         L     @07,LCARPLP(,LCAPTR)                                0342
         LA    @04,DATATXT(,PR1ELAD)                               0342
         ST    @04,RPLAREA(,@07)                                   0342
*   RPLRLEN=DATASIZE-4;             /* RECORD SIZE-HEADER SIZE       */
         LH    @04,DATASIZE(,PR1ELAD)                              0343
         N     @04,@CF02454                                        0343
