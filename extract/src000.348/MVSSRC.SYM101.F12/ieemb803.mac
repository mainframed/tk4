SWITCHDS DS    0H                                                  0423
*   /*****************************************************************/
*   /*                                                               */
*   /* DATA SET SWITCH ECB POSTED- CALL ALLOCRTN TO GET A NEW LOG    */
*   /* DATA SET. IF NOT SUCCESSFUL,SET 'SWITCH FAILED' MSG           */
*   /* CODE,RETURN TO MULTIPLE WAIT IN MAIN-LINE. CALL NEWACB TO GET */
*   /* ACB CORE ,CALL OPENRTN TO OPEN NEW LOG DATA SET. IF OPEN      */
*   /* FAILS,FREE ACB CORE, CALL UNALLOC RTN TO FREE NEW DATA SET,   */
*   /* SET 'SW FAIL' MSG CODE,RETURN TO MULTI WAIT IN MAIN-LINE.     */
*   /* UPDATE ACB AND DATA SET PTRS, CALL CLOSRTN TO CLOSE 'OLD' LOG */
*   /* DATA SET, UNALLOCATE 'OLD' DATA SET. IF UNALLOCATE FAILS SET  */
*   /* 'DATA LOSS' MSG CODE, ELSE 'Q'ED MSG CODE,RETURN TO MULTI-WAIT*/
*   /* IN MAIN-LINE.                                                 */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0423
*   DCL                                                            0423
*     REDO LABEL;                   /* LABEL FOR 'CS' INSTRUCTION    */
*   REGSAV2=REG14;                  /* SAVE RETURN ADDRESS           */
         ST    REG14,REGSAV2                                       0424
*   MSLGWLOG=0000;                  /* CLEAR WRITELOG ECB            */
         L     @03,CVTPTR                                          0425
         L     @03,CVTMSER(,@03)                                   0425
         SLR   @09,@09                                             0425
         ST    @09,MSLGWLOG(,@03)                                  0425
*   REG15=4;                        /* SET CONTROL SW FOR PUT RTN    */
         LA    REG15,4                                             0426
*   DO WHILE(REG15^=0000);                                         0427
         B     @DE00427                                            0427
@DL00427 DS    0H                                                  0428
*     CALL WTLREC;                  /* PUT WTL QUEUE TO LOG DATA SET */
         BAL   @14,WTLREC                                          0428
*   END;                                                           0429
@DE00427 LTR   REG15,REG15                                         0429
         BNZ   @DL00427                                            0429
*   CALL ALLOCRTN;                  /* GET NEW LOG DATA SET          */
         BAL   @14,ALLOCRTN                                        0430
*   IF REG15=0000 THEN              /* ALLOCATION SUCCESSFUL         */
         LTR   REG15,REG15                                         0431
         BNZ   @RF00431                                            0431
*     DO;                                                          0432
*       CALL NEWACB;                /* GET, INITIALIZE ACB           */
         BAL   @14,NEWACB                                          0433
*       IF REG15=0000 THEN          /* CORE GOTTEN FOR ACB           */
         LTR   REG15,REG15                                         0434
         BNZ   @RF00434                                            0434
*         DO;                                                      0435
*           CALL OPENRTN;           /* OPEN NEW LOG DATA SET         */
