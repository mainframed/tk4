         AL    @08,@CF02465                                        0617
         L     @08,UCMMCSPT(,@08)                                  0617
         OI    UCMSYSO(@08),B'00000010'                            0617
*   MSLOGTHD='0'B;                  /* LOG ACTIVE BIT                */
         L     @03,CVTMSER(,@03)                                   0618
