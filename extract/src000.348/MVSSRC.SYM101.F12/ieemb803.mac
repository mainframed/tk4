         LR    REGSAV,REG14                                        0518
*   BALOG=0000;                     /* ZERO LCA PTR                  */
         L     @09,CVTPTR                                          0519
         L     @08,CVTMSER(,@09)                                   0519
         SLR   @07,@07                                             0519
         ST    @07,BALOG(,@08)                                     0519
*   MSLOGTHD='1'B;                  /* SET COMM-TASK INDICATOR ON    */
