*       SVCWAIT=1;                  /* SET LOOP VALUE                */
         LA    SVCWAIT,1                                           0532
*       DO WHILE(SVCWAIT=1);                                       0533
         B     @DE00533                                            0533
@DL00533 DS    0H                                                  0534
*         REG1=ADDR(LCAELST);       /* ADDR OF LOG ECB LIST          */
         LA    REG1,LCAELST(,LCAPTR)                               0534
*         GEN( WAIT  ECBLIST=(1));  /* WAIT ON ALL ECBS              */
          WAIT  ECBLIST=(1)
*         IF MSWTRP^='1'B THEN      /* NOT POSTED FOR PUT RTN        */
         L     @05,CVTPTR                                          0536
         L     @05,CVTMSER(,@05)                                   0536
         TM    MSWTRP(@05),B'01000000'                             0536
         BO    @RF00536                                            0536
*           SVCWAIT='00'X;          /* EXIT FROM LOOP                */
         SLR   SVCWAIT,SVCWAIT                                     0537
*         ELSE                                                     0538
*           MSWTRSW=MSWTRSW&'00111111'B;/* CLEAR POST BIT , RE-WAIT  */
         B     @RC00536                                            0538
@RF00536 L     @05,CVTPTR                                          0538
         L     @05,CVTMSER(,@05)                                   0538
         LA    @09,63                                              0538
         SLR   @08,@08                                             0538
         IC    @08,MSWTRSW(,@05)                                   0538
         NR    @09,@08                                             0538
         STC   @09,MSWTRSW(,@05)                                   0538
*       END;                                                       0539
@RC00536 DS    0H                                                  0539
@DE00533 C     SVCWAIT,@CF00078                                    0539
         BE    @DL00533                                            0539
*     END;                                                         0540
