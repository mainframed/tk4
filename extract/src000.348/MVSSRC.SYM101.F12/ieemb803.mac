*       POSTCORE=POSTLIST;          /* MOVE LIST TO GOTTEN CORE      */
         MVC   POSTCORE(12),POSTLIST                               0523
*       UCMSYSO='1'B;               /* SET LOG ATTENTION BIT         */
         OI    UCMSYSO(@08),B'00000010'                            0524
*       REG1=ADDR(POSTCORE);        /* ADDR OF POST PARM LIST        */
