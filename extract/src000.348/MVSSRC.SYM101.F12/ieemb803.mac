*   DCL                                                            0578
*     GETLEN PTR(8) INIT(ADDR(GETEND)-ADDR(GETLIST));/* LIST LENGTH  */
*   DCL                                                            0579
*     GETADPTR PTR(31);             /* GOTTEN CORE ADDRESS           */
*   REGSAV=REG14;                   /* SAVE RETURN ADDRESS           */
         LR    REGSAV,REG14                                        0580
*   RFY                                                            0581
