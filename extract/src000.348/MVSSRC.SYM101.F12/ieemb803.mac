         L     @08,OPENLEN2                                        0598
         BCTR  @08,0                                               0598
         EX    @08,@SM02473                                        0598
*   ACBDDNM=DDALNM;                 /* LOG DATA SET DDNAME TO ACB    */
         MVC   ACBDDNM(8,@09),DDALNM                               0599
*   GEN( OPEN ((2)),MF=(E,(1)));    /* OPEN THE ACB                  */
          OPEN ((2)),MF=(E,(1))
*   IF ACBOPEN^='1'B THEN           /* OPEN FAILED                   */
         L     @09,ACBPTR                                          0601
         TM    ACBOPEN(@09),B'00010000'                            0601
         BO    @RF00601                                            0601
*     REG15=4;                      /* SET FAILED RETURN CODE        */
         LA    REG15,4                                             0602
