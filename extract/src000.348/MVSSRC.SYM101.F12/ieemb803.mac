*   CALL IEEMB807;                  /* ISSUE WTO MSG                 */
         L     @15,@CV02192                                        0483
         BALR  @14,@15                                             0483
*   REG14=REGSAV2;                  /* RESTORE RETURN ADDRESS        */
         L     REG14,REGSAV2                                       0484
*   END SWITCHDS;                                                  0485
@EL00007 DS    0H                                                  0485
@EF00007 DS    0H                                                  0485
@ER00007 BR    @14                                                 0485
*LOGTERM:                                                          0486
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0486
*                                                                  0486
LOGTERM  DS    0H                                                  0487
*   /*****************************************************************/
*   /*                                                               */
*   /* IF AT LOG CLOSE, THE ECB (MSLGCLOZ) CONTAINS          @YM06135*/
*   /* A COMPLETION CODE OF FOUR, A MESSAGE (IEE775I)        @YM06135*/
*   /* IS ISSUED TO INFORM THE OPERATOR THE LOG              @YM06135*/
*   /* TERMINATION IS IN PROCESS BECAUSE BUFFER CORE         @YM06135*/
*   /* IS NOT AVAILABLE.                                     @YM06135*/
*   /* LOG TASK INACTIVE ECB POSTED - CALL SHUTDOWN TO SET LOG TASK  */
*   /* NOT ACTIVE. CALL WTLREC RTN TO WRITE THE WTL QUEUE TO THE LOG */
*   /* DATA SET. UPDATE ACB AND DDNAME PTRS AND CLOSE AND UNALLOCATE */
*   /* THE LOG DATA SET. SET MSG CODE TO REFLECT RETURN CODE FROM    */
*   /* UNALLOC RTN. CALL WTLCLOSE RTN TO FREE ALL CONTROL BLOCKS AND */
*   /* AREAS CREATED BY LOG TASK. RETURN TO TOP OF MAIN-LINE RTN TO  */
*   /* WAIT ON ECB MSLGSTRT FOR A WRITELOG START COMMAND TO BE       */
*   /* ISSUED.                                                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0487
*   DCL                                                            0487
*     1 ECBMAP DEF(MSLGCLOZ),       /* COMPLETION CODE MAPP  @YM06135*/
*      2 * CHAR(3),                                                0487
*      2 ECBCODE CHAR(1);                                          0487
*   REGSAV2=REG14;                  /* SAVE RETURN ADDRESS           */
         ST    REG14,REGSAV2                                       0488
*   IF ECBCODE=4 THEN               /* SVC BLDCPOOL FAIL     @YM06135*/
         L     @03,CVTPTR                                          0489
         L     @03,CVTMSER(,@03)                                   0489
         CLI   ECBCODE(@03),4                                      0489
         BNE   @RF00489                                            0489
*     DO;                                                          0490
*       MSGONE=MSG8;                /* CORE UNAVAILABLE MSG  @YM06135*/
         IC    @03,MSG8                                            0491
         STC   @03,MSGONE                                          0491
*       NUMMSGS=1;                  /* WTOS TO ISSUE         @YM06135*/
         MVI   NUMMSGS,X'01'                                       0492
*       REG1=ADDR(MSGPARM);         /* SET PARM REG          @YM06135*/
         LA    REG1,MSGPARM                                        0493
*       CALL IEEMB807;              /* ISSUE WTO MSG         @YM06135*/
         L     @15,@CV02192                                        0494
         BALR  @14,@15                                             0494
*     END;                                                         0495
*   MSLGCLOZ=0000;                  /* CLEAR CLOSE ECB               */
@RF00489 L     @03,CVTPTR                                          0496
