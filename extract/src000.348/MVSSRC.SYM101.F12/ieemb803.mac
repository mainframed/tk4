*               LCACBONE=ACBPTR;    /* CURRENT ACB PTR               */
         L     @09,ACBPTR                                          0440
         ST    @09,LCACBONE(,LCAPTR)                               0440
*               LCADDTWO=LCADDONE;  /* OLD DDNAME PTR FOR UNALLOC    */
         MVC   LCADDTWO(8,LCAPTR),LCADDONE(LCAPTR)                 0441
*               LCADDONE=DDALNM;    /* CURRENT DDNAME PTR            */
         MVC   LCADDONE(8,LCAPTR),DDALNM                           0442
*               ACBPTR=LCACBTWO;    /* SET OLD ACB PTR FOR FREE      */
         ST    @03,ACBPTR                                          0443
*               CALL CLOSRTN;       /* CLOSE OLD DATA SET            */
         BAL   @14,CLOSRTN                                         0444
*               CALL UNALLOC;       /* UNALLOCATE OLD DATA SET       */
         BAL   @14,UNALLOC                                         0445
*               IF REG15=0000 THEN  /* UNALLOCATION SUCCESSFUL       */
         LTR   REG15,REG15                                         0446
         BNZ   @RF00446                                            0446
*                 MSGONE=MSG1;      /* 'Q' TO SYSOUT MSG             */
         IC    @03,MSG1                                            0447
         STC   @03,MSGONE                                          0447
*               ELSE                                               0448
*                 MSGONE=MSG2;      /* UNALLOC FAILED MSG            */
         B     @RC00446                                            0448
@RF00446 IC    @03,MSG2                                            0448
         STC   @03,MSGONE                                          0448
*               MSGCLS=OUTCLS;      /* SET OUTPUT CLASS VALUE        */
@RC00446 IC    @03,OUTCLS                                          0449
         STC   @03,MSGCLS                                          0449
*               LOGLOOP='00'X;      /* END POST/PUT LOOP             */
         MVI   LOGLOOP,X'00'                                       0450
*             END;                                                 0451
*           ELSE                    /* OPEN FAILURE                  */
*             DO;                                                  0452
         B     @RC00437                                            0452
@RF00437 DS    0H                                                  0453
*               LCADDTWO=DDALNM;    /* UNALLOCATE DDNAME             */
         MVC   LCADDTWO(8,LCAPTR),DDALNM                           0453
*               CALL UNALLOC;       /* UNALLOCATE NEW LOG DATA SET   */
         BAL   @14,UNALLOC                                         0454
*               MSGONE=MSG6;        /* D/S SWITCH MSG                */
         IC    @03,MSG6                                            0455
         STC   @03,MSGONE                                          0455
*               LOGLOOP='00'X;      /* END POST/PUT LOOP             */
         MVI   LOGLOOP,X'00'                                       0456
*             END;                                                 0457
*           CALL FREEACB;           /* FREE ACB CORE                 */
@RC00437 BAL   @14,FREEACB                                         0458
*         END;                                                     0459
*       ELSE                        /* GET ACB CORE FAILED           */
*         DO;                                                      0460
         B     @RC00434                                            0460
@RF00434 DS    0H                                                  0461
*           LCADDTWO=DDALNM;        /* ALLOCATED DDNAME              */
         MVC   LCADDTWO(8,LCAPTR),DDALNM                           0461
*           CALL UNALLOC;           /* UNALLOCATE NEW DATA SET       */
         BAL   @14,UNALLOC                                         0462
*           MSGONE=MSG6;            /* D/S SWITCH FAILED MSG         */
         IC    @03,MSG6                                            0463
         STC   @03,MSGONE                                          0463
*           LOGLOOP='00'X;          /* END POST/PUR LOOP             */
         MVI   LOGLOOP,X'00'                                       0464
*         END;                                                     0465
*     END;                                                         0466
*   ELSE                            /* ALLOCATION FAILED             */
*     DO;                                                          0467
         B     @RC00431                                            0467
@RF00431 DS    0H                                                  0468
*       MSGONE=MSG6;                /* D/S SWITCH FAILED MSG         */
         IC    @03,MSG6                                            0468
         STC   @03,MSGONE                                          0468
*       LOGLOOP='00'X;              /* END POST/PUT LOOP             */
         MVI   LOGLOOP,X'00'                                       0469
*     END;                                                         0470
*   POSTWLOG='0'B;                  /* ZERO POSTED BIT IN LCA        */
@RC00431 L     @03,POSTWLOG(,LCAPTR)                               0471
@GL00001 LR    @09,@03                                             0471
         N     @09,@CF02461                                        0471
         CS    @03,@09,POSTWLOG(LCAPTR)                            0471
         BNE   @GL00001                                            0471
*   IF MSGONE^=MSG6 THEN            /* UNSUCCESSFUL D/S SWITCH       */
         CLC   MSGONE(1),MSG6                                      0472
         BE    @RF00472                                            0472
*     DO;                                                          0473
*       RFY                                                        0474
*        (WREG1,                                                   0474
*         WREG2) RESTRICTED;                                       0474
*       WREG1=LCALIMCT;             /* WTL COUNT FOR WRITELOG        */
