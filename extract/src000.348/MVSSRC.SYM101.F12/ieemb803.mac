CHCKPOST DS    0H                                                  0401
*   /*****************************************************************/
*   /*                                                               */
*   /* ENTRY HERE WHEN THE MULTIPLE ECB WAIT HAS BEEN POSTED TO - .  */
*   /* SET THE LOG TASK INACTIVE - MSLGCLOZ ECB . . SWITCH THE LOG   */
*   /* DATA SETS - MSLGWLOG ECB . . PUT WTL RECORDS TO LOG DATA SET -*/
*   /* MSLGWTR ECB THE POST BIT OF THE ECBS IS TESTED AND THE        */
*   /* REQUIRED PROCESSING IS PERFORMED TO SATISFY THE REQUEST. MSG  */
*   /* MODULE IEEMB807 IS CALLED TO PROCESS MSG CODES AND ISSUE WTOS.*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0401
*   NUMMSGS='00'X;                  /* PRE-SET NULL MSG CODE         */
         MVI   NUMMSGS,X'00'                                       0401
*   MAINSAV=REG14;                  /* SAVE RETURN ADDRESS           */
         ST    REG14,MAINSAV                                       0402
*   LOGLOOP=1;                      /* SET SW FOR POST/PUT LOOP      */
         MVI   LOGLOOP,X'01'                                       0403
*   DO WHILE(LOGLOOP=1);                                           0404
         B     @DE00404                                            0404
@DL00404 DS    0H                                                  0405
*     IF MSCLOSP^='1'B THEN         /* NOT WRITELOG CLOSE ECB        */
         L     @03,CVTPTR                                          0405
         L     @03,CVTMSER(,@03)                                   0405
         TM    MSCLOSP(@03),B'01000000'                            0405
         BO    @RF00405                                            0405
*       IF MSWLOGP^='1'B THEN       /* NOT WRITELOG ECB              */
         TM    MSWLOGP(@03),B'01000000'                            0406
         BO    @RF00406                                            0406
*         IF MSWTRP='1'B THEN       /* PUT RTN ECB                   */
         TM    MSWTRP(@03),B'01000000'                             0407
         BNO   @RF00407                                            0407
*           DO;                                                    0408
*                                                                  0408
*             /*******************************************************/
*             /*                                                     */
*             /* PUT ECB POSTED- RTN WTLREC ISSUES A PUT ON THE      */
*             /* BOTTOM RECORD ON THE WTL QUEUE. IF THIS IS THE LAST */
*             /* RECORD,THE MSLGWTR POST BIT IS TURNED OFF, AND EXIT */
*             /* IS MADE TO RE-ISSUE THE MULTIPLE ECB WAIT IN MAIN-  */
*             /* LINE RTN. ELSE RETURN IS TO TOP OF CHCKPOST RTN TO  */
*             /* TEST FOR A POST OR MSLGCLOZ OR MSLGWLOG ECBS. IF    */
*             /* THEY HAVE BEEN POSTED,THE POST IS PROCESSED, ELSE   */
*             /* THE PUT RTN CONTINUES TO WRITE OUT THE WTL QUEUE.   */
*             /*                                                     */
*             /*******************************************************/
*                                                                  0409
*             CALL WTLREC;          /* PUT RECORD TO LOG DATA SET    */
         BAL   @14,WTLREC                                          0409
*             IF REG15=0000 THEN    /* LAST RECORD PUT TO LOG DATASET*/
         SLR   @03,@03                                             0410
         CR    REG15,@03                                           0410
         BNE   @RF00410                                            0410
*               DO;                                                0411
*                 MSLGWTR=0000;     /* CLEAR PUT ECB                 */
         L     @09,CVTPTR                                          0412
         L     @09,CVTMSER(,@09)                                   0412
         ST    @03,MSLGWTR(,@09)                                   0412
*                 LOGLOOP='00'X;    /* END POST/PUT LOOP             */
         MVI   LOGLOOP,X'00'                                       0413
*               END;                                               0414
*           END;                                                   0415
*         ELSE                                                     0416
*           ;                                                      0416
@RF00407 DS    0H                                                  0417
*       ELSE                        /* LOG DATA SET SWITCH POSTED    */
*         CALL SWITCHDS;            /* PROCESS TO SWITCH DATA SETS   */
         B     @RC00406                                            0417
@RF00406 BAL   @14,SWITCHDS                                        0417
