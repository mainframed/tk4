REDO     SLR   WREG2,WREG2                                         0476
*       CS(WREG1,WREG2,LCALIMCT);   /* SWAP IN ZERO VALUE            */
         CS    WREG1,@07,LCALIMCT(LCAPTR)                          0477
*       BC(7,REDO);                 /* RE-PROCESS IF COMPARE FAILS   */
         BC    7,REDO                                              0478
*       RFY                                                        0479
*        (WREG1,                                                   0479
*         WREG2) UNRESTRICTED;                                     0479
*     END;                                                         0480
*   NUMMSGS=1;                      /* WTOS TO ISSUE                 */
@RF00472 MVI   NUMMSGS,X'01'                                       0481
*   REG1=ADDR(MSGPARM);             /* SET PARM REG                  */
