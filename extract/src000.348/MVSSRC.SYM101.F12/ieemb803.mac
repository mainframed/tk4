         LR    @08,@09                                             0521
         AL    @08,@CF02465                                        0521
         L     @08,UCMMCSPT(,@08)                                  0521
         TM    UCMSYSG(@08),B'00000010'                            0521
         BNO   @RF00521                                            0521
*     DO;                                                          0522
*                                                                  0522
*       /*************************************************************/
*       /*                                                           */
*       /* COMM-TASK MUST BE MADE AWARE THAT THE LOG IS CLOSING SET  */
*       /* UP AND POST COMM-TASK ATTENTION ECB.                      */
*       /*                                                           */
*       /*************************************************************/
