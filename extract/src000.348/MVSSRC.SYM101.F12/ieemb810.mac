                        '
IEEMB810 CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'IEEMB810  76.202'                                 0001
         DROP  @15
@PROLOG  BALR  @12,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@12                                         0001
         LR    @10,@01                                             0001
         L     @00,@SIZDATD                                        0001
         GETMAIN  R,LV=(0)
         LR    @11,@01                                             0001
         USING @DATD,@11                                           0001
         LR    @01,@10                                             0001
         ST    @13,@SA00001+4                                      0001
         LR    @13,@11                                             0001
         XC    @ZTEMPS(@ZLEN),@ZTEMPS
         MVC   @PC00001(4),0(@01)                                  0001
*                                                                  0047
*   /*****************************************************************/
*   /*                                                               */
*   /* WHEN ENTERED REG 1 POINTS TO RESET CSCB. A STAE ENVIRONMENT IS*/
*   /* CREATED AND A DUMMY XSA IS BUILT TO BE PASSED TO MESSAGE      */
*   /* MODULE IEE0503D. THE JOBNAME ENTERED WITH THE RESET VERB IS   */
*   /* SCANNED OFF AND MOVED INTO A BLANKED OUT AREA FOR LATER       */
*   /* COMPARISON.                                                   */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0047
*   REG14SAV=REG14;                 /* SAVE RETURN ADDRESS           */
         ST    REG14,REG14SAV                                      0047
*   CALL ISSUSTAE;                  /* CREATE STAE ENVIRONMENT       */
         BAL   @14,ISSUSTAE                                        0048
*   IF REG15=0 THEN                 /* IF STAE CREATED CONTINUE      */
         LTR   REG15,REG15                                         0049
         BNZ   @RF00049                                            0049
*     DO;                                                          0050
*       CALL BUILDXSA;              /* BUILD DUMMY XSA FOR ACCESS BY
*                                      MSG MOD                       */
         BAL   @14,BUILDXSA                                        0051
*       CALL SCANINIT;              /* INITIALIZE FOR THE SCAN OF THE
*                                      JOBNAME                       */
         BAL   @14,SCANINIT                                        0052
*       CALL SCANNAME;              /* SCAN THE JOBNAME              */
*                                                                  0053
         BAL   @14,SCANNAME                                        0053
*       /*************************************************************/
*       /*                                                           */
*       /* THE POINTER TO THE CSCB IS PLACED INTO REG3 AND A         */
*       /* COMPARISON MADE OF THE JOBNAME FOUND IN THE CSCB WITH THE */
*       /* JOBNAME FOUND IN THE INPUT BUFFER. IF NOT EQUAL THE NEXT  */
*       /* CSCB ON THE CHAIN IS ACCESSED AND THE JOBNAME IN IT       */
*       /* COMPARED ALL ERRORS ARE FLAGGED BY MOVING A VALUE INTO THE*/
*       /* XAE FIELD OF THE XSA. ANY ERRORS TERMINATE THE DO LOOP.   */
*       /* WHEN ALL CSCBS HAVE BEEN ACCESSED THE LOOP WILL ALSO      */
*       /* TERMINATE.                                                */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0054
*       GEN(ENQ  (QN,RN,S,3,SYSTEM));/* SERIALIZE CSCB USE           */
         ENQ  (QN,RN,S,3,SYSTEM)
*       REG3=BACHN;                 /* GET THE POINTER TO THE CSCB 0055
*                                      CHAIN.                        */
         L     @10,CVTPTR                                          0055
         L     @10,CVTMSER(,@10)                                   0055
         L     REG3,BACHN(,@10)                                    0055
*       DO WHILE(XAE=0&REG3^=0);                                   0056
         B     @DE00056                                            0056
@DL00056 DS    0H                                                  0057
*         IF CHKEY(1:8)=BUFFER1 THEN/* COMPARE THE JOBNAME FOUND IN
*                                      THE INPUT BUFFER WITH THE NAME
*                                      FOUND IN THE CSCB.            */
         CLC   CHKEY(8,CSCBPTR),BUFFER1                            0057
         BNE   @RF00057                                            0057
*           DO;                                                    0058
*                                                                  0058
*             /*******************************************************/
*             /*                                                     */
*             /* IF THE NAMES COMPARE EQUAL, THEN THE JOB IS ACTIVE  */
*             /* IEEBUFSC MACRO IS USED TO SCAN OFF THE 'PERFORM'    */
*             /* KEYWORD. THE RETURN CODES FROM IEEBUFSC ARE CHECKED.*/
*             /* IF ZERO THEN THE PERFORM VALUE IS CHECKED TO MAKE   */
*             /* SURE IT IS A NUMERIC VALUE OF THE CORRECT LENGTH.   */
*             /*                                                     */
*             /*******************************************************/
*                                                                  0059
*             CALL GETBUFSC;        /* THE IEEBUFSC ROUTINE          */
         BAL   @14,GETBUFSC                                        0059
*             IF REG15=0 THEN       /* IF ALL WENT WELL              */
         LTR   REG15,REG15                                         0060
         BNZ   @RF00060                                            0060
*               DO;                 /* THEN CONTINUE.                */
*                 CALL CKVALUE;     /* CK LENGTH AND WHETHER NUMERIC
*                                      OR NOT.                       */
         BAL   @14,CKVALUE                                         0062
*                 IF REG15=0 THEN                                  0063
         LTR   REG15,REG15                                         0063
         BNZ   @RF00063                                            0063
*                   DO;             /* IF LENGTH OK AND A NUMERIC  0064
*                                      VALUE THEN CONTINUE.          */
*                                                                  0064
*                     /***********************************************/
*                     /*                                             */
*                     /* THE PERFORM VALUE IS PASSED TO THE SYSTEM   */
*                     /* RESOURCE MANAGER THRU THE SYSEVENT MACRO.   */
*                     /* THE RETURN CODE IN REG 1 IS CHECKED TO      */
*                     /* DETERMINE IF THE MANAGER ACCEPTED THE NEW   */
*                     /* PERFORM VALUE.                              */
*                     /*                                             */
*                     /***********************************************/
*                                                                  0065
*                     CALL SYSEVENT;                               0065
         BAL   @14,SYSEVENT                                        0065
*                     CALL CHECKRC2;                               0066
         BAL   @14,CHECKRC2                                        0066
*                   END;                                           0067
*               END;                                               0068
*             ELSE                                                 0069
*               CALL CHECKRC1;      /* CHECK IEEBUFSC RC             */
         B     @RC00060                                            0069
@RF00060 BAL   @14,CHECKRC1                                        0069
*           END;                                                   0070
@RC00060 DS    0H                                                  0071
*         IF XAE=0 THEN             /* WAS THE JOBNAME FOUND         */
@RF00057 CLI   XAE(XSAPTR),0                                       0071
         BNE   @RF00071                                            0071
*           REG3=CHPTR;             /* IF NOT, GET NEXT CSCB         */
         L     REG3,CHPTR(,CSCBPTR)                                0072
*       END;                                                       0073
*                                                                  0073
@RF00071 DS    0H                                                  0073
@DE00056 CLI   XAE(XSAPTR),0                                       0073
         BNE   @DC00056                                            0073
         LTR   REG3,REG3                                           0073
         BNZ   @DL00056                                            0073
@DC00056 DS    0H                                                  0074
*       /*************************************************************/
*       /*                                                           */
*       /* IF NO CSCB COULD BE FOUND THAT CONTAINED THE JOBNAME      */
*       /* REQUESTED . THE XAE IS FLAGGED WITH THE JOBNAME NOT ACTIVE*/
*       /* INDICATOR. FOR ALL ERROR CONDITIONS CONTROL IS PASSED TO  */
*       /* THE MESSAGE MODULE IEE0503D TO ISSUE THE MESSAGE. A       */
*       /* SUCCESSFUL COMPLETION MESSAGE IS ALSO ISSUED.             */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0074
*       GEN(DEQ (QN,RN,3,SYSTEM));  /* RELEASE CSCB CHAIN            */
         DEQ (QN,RN,3,SYSTEM)
*       IF XAE=0 THEN               /* IF NO VALUE THEN THE          */
         CLI   XAE(XSAPTR),0                                       0075
         BNE   @RF00075                                            0075
*         DO;                                                      0076
*           XAE='24'X;              /* JOBNAME NOT FOUND             */
         MVI   XAE(XSAPTR),X'24'                                   0077
*           XAV=BUFFER1;            /* MOVE IN JOBNAME               */
         MVC   XAV(8,XSAPTR),BUFFER1                               0078
*         END;                      /* IN THE CSCB CHAIN             */
*       IF XAE^='FF'X THEN          /* BYPASS MSG MOD IF INVALID XAE */
@RF00075 CLI   XAE(XSAPTR),X'FF'                                   0080
         BE    @RF00080                                            0080
*         CALL GET0503D;            /* PUT OUT MESSAGE               */
         BAL   @14,GET0503D                                        0081
*     END;                                                         0082
@RF00080 DS    0H                                                  0083
*   CALL FREECORE;                  /* FREE CSCB AND XSA             */
@RF00049 BAL   @14,FREECORE                                        0083
*   REG14=REG14SAV;                 /* RESTORE RETURN ADDRESS        */
         L     REG14,REG14SAV                                      0084
*   RETURN;                                                        0085
@EL00001 L     @13,4(,@13)                                         0085
@EF00001 L     @00,@SIZDATD                                        0085
         LR    @01,@11                                             0085
         FREEMAIN R,LV=(0),A=(1)
@ER00001 BR    @14                                                 0085
*ISSUSTAE:                                                         0086
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0086
