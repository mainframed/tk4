ISSUSTAE DS    0H                                                  0087
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS ROUTINE CREATES A STAE ENVIRONMENT THRU MODULE IEECB860. */
*   /* A PARAMETER LIST IS BUILT AND PASSED TO MODULE IEECB860 VIA   */
*   /* REGISTER 1.                                                   */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0087
*   REG8=REG1;                      /* SAVE SETS CSCB PTR            */
         LR    REG8,REG1                                           0087
*   REG4=REG14;                     /* SAVE RETURN ADDR              */
         LR    REG4,REG14                                          0088
*   REG1=ADDR(WORKAREA);            /* GET CORE FOR PARM LIST        */
         LA    REG1,WORKAREA                                       0089
*   REG2=REG1;                      /* SAVE CORE ADDR                */
         LR    REG2,REG1                                           0090
*   CSCBADDR=REG8;                  /* STORE CSCB PTR IN LIST        */
         ST    REG8,CSCBADDR(,REG1)                                0091
*   REG5=ADDR(STAEBUF);             /* GET PTR TO CMD BUF            */
         LA    REG5,STAEBUF(,REG1)                                 0092
*   BUFPTR=REG5;                    /* STORE CMD BUF PTR IN PARAMETER
*                                      LIST                          */
         ST    REG5,BUFPTR(,REG1)                                  0093
*   CMDSIZE=5;                      /* STORE LENGTH OF CMD           */
         MVI   CMDSIZE(REG1),X'05'                                 0094
*   COMMAND=RESET;                  /* STORE VERB                    */
         MVI   COMMAND+5(REG1),C' '                                0095
         MVC   COMMAND+6(5,REG1),COMMAND+5(REG1)                   0095
         MVC   COMMAND(5,REG1),RESET                               0095
*   RESPECIFY                                                      0096
*    (REG11,                                                       0096
*     REG12,                                                       0096
*     REG13) RESTRICTED;                                           0096
*   REG4SAV=REG4;                   /* SAVE RETURN ADDRESS           */
         ST    REG4,REG4SAV(,REG1)                                 0097
*   REG8SAV=REG8;                   /* SAVE CSCB ADDRESS             */
         ST    REG8,REG8SAV(,REG1)                                 0098
*   REG12SAV=REG12;                 /* SAVE BASE REGISTER            */
         ST    REG12,REG12SAV(,REG1)                               0099
*   REG11SAV=REG11;                 /* SAVE DATA REG ADDRESS         */
         ST    REG11,REG11SAV(,REG1)                               0100
*   REG13SAV=REG13;                 /* SAVE SAVE AREA PTR            */
         ST    REG13,REG13SAV(,REG1)                               0101
*   GEN(LOAD EP=IEECB860);          /* LOAD STAE CREATING MOD        */
         LOAD EP=IEECB860
*   REG15=REG0;                     /* PUT ENTRY ADDR IN R15         */
         LR    REG15,REG0                                          0103
*   REG1=REG2;                      /* RESTORE PARM PTR              */
         LR    REG1,REG2                                           0104
*   GEN(BALR REG14,REG15);          /* BR TO STAE MODULE             */
         BALR REG14,REG15
*   REG4=REG4SAV;                   /* RESTORE RETURN ADDR           */
         L     REG4,REG4SAV(,REG1)                                 0106
*   REG8=REG8SAV;                   /* RESTORE CSCB ADDR             */
         L     REG8,REG8SAV(,REG1)                                 0107
*   REG11=REG11SAV;                 /* RESTORE DATA REG PTR          */
         L     REG11,REG11SAV(,REG1)                               0108
*   REG12=REG12SAV;                 /* RESTORE BASE REG              */
         L     REG12,REG12SAV(,REG1)                               0109
*   REG13=REG13SAV;                 /* RESTORE SAVE AREA PTR         */
         L     REG13,REG13SAV(,REG1)                               0110
*   RESPECIFY                                                      0111
*    (REG11,                                                       0111
*     REG12,                                                       0111
*     REG13) UNRESTRICTED;                                         0111
*   REG5=REG15;                     /* SAVE RETURN CODE              */
         LR    REG5,REG15                                          0112
*   GEN(DELETE EP=IEECB860);        /* DELETE STAE CREATE MOD        */
         DELETE EP=IEECB860
*   REG15=REG5;                     /* PUT RETURN CODE INTO R15      */
         LR    REG15,REG5                                          0114
*   REG14=REG4;                     /* RESTORE THE RETURN ADDR       */
         LR    REG14,REG4                                          0115
*   END;                                                           0116
@EL00002 DS    0H                                                  0116
@EF00002 DS    0H                                                  0116
@ER00002 BR    @14                                                 0116
*BUILDXSA:                                                         0117
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0117
*                                                                  0117
BUILDXSA DS    0H                                                  0118
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS ROUTINE BUILDS A DUMMY XSA TO PASS TO MESSAGE MODULE     */
*   /* IEE0503D. THE XSA WILL BE USED FOR ALL MESSAGES.              */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0118
*   REG2=ADDR(WORKAREA);            /* GET ADDR FOR XSA              */
         LA    REG2,WORKAREA                                       0118
*   REG0=LENGTH(XSAMAP);            /* GET SIZE OF CORE              */
         LA    REG0,136                                            0119
*   XSAMAP(1:REG0)=XSAMAP(1:REG0)&&XSAMAP(1:REG0);/* ZERO OUT THE  0120
*                                      CORE                          */
         LR    @10,REG0                                            0120
         BCTR  @10,0                                               0120
         EX    @10,@SX01250                                        0120
*   XAV=RESET;                      /* PLACE RESET VERB IN XSA FOR 0121
*                                      MSG MOD                       */
         MVI   XAV+5(XSAPTR),C' '                                  0121
         MVC   XAV+6(2,XSAPTR),XAV+5(XSAPTR)                       0121
         MVC   XAV(5,XSAPTR),RESET                                 0121
*   XAU=CHUCMP;                     /* STORE ISSUING CONSOLE ID      */
         MVC   XAU(1,XSAPTR),CHUCMP(CSCBPTR)                       0122
*   END;                                                           0123
@EL00003 DS    0H                                                  0123
@EF00003 DS    0H                                                  0123
@ER00003 BR    @14                                                 0123
*FREECORE:                                                         0124
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0124
*                                                                  0124
FREECORE DS    0H                                                  0125
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS ROUTINE ISSUES THE MGCR MACRO TO FREE THE CSCB           */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0125
*   REG8->CHFC='1'B;                /* TURN ON FREE CSCB SWITCH IN 0125
*                                      CSCB                          */
         OI    CHFC(REG8),B'00000010'                              0125
*   REG1=REG8;                      /* GET RESET CSCB PTR            */
         LR    REG1,REG8                                           0126
*   GEN(MGCR (1),CHAIN);            /* REMOVE CSCB FROM CHAIN AND  0127
*                                      FREE CORE                     */
         MGCR (1),CHAIN
*   END;                                                           0128
@EL00004 DS    0H                                                  0128
@EF00004 DS    0H                                                  0128
@ER00004 BR    @14                                                 0128
*SCANINIT:                                                         0129
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0129
*                                                                  0129
SCANINIT DS    0H                                                  0130
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS ROUTINE BLANKS OUT THE BUFFER THAT WILL HOLD THE JOBNAME */
*   /* AND INITIALIZES REG 1 TO POINT TO THE BEGINNING OF THE INPUT  */
*   /* BUFFER AND REG 0 TO POINT TO THE END OF THE BUFFER            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0130
*   REG1=ADDR(REG8->CHBUF);         /* GET POINTER TO KEYWORD        */
         LA    REG1,CHBUF(,REG8)                                   0130
*   BUFFER1(1:8)=' ';               /* PUT BLANKS IN HOLD AREA       */
         MVI   BUFFER1+1,C' '                                      0131
         MVC   BUFFER1+2(6),BUFFER1+1                              0131
         MVI   BUFFER1,C' '                                        0131
*   SIZE=0;                         /* LENGTH OF JOBNAME SAVED IN  0132
*                                      SIZE FIELD                    */
         MVI   SIZE,X'00'                                          0132
*   REG0=ADDR(REG8->CHTYPE);        /* GET END OF INPUT BUFFER + 1   */
         LA    REG0,CHTYPE(,REG8)                                  0133
*   END;                                                           0134
@EL00005 DS    0H                                                  0134
@EF00005 DS    0H                                                  0134
@ER00005 BR    @14                                                 0134
*SCANNAME:                                                         0135
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0135
*                                                                  0135
SCANNAME DS    0H                                                  0136
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS ROUTINE SCANS THE JOBNAME FROM THE COMMAND INPUT BUFFER. */
*   /* ONCE SCANNED IT IS MOVED TO THE HOLDAREA FOR LATER COMPARISON */
*   /* WITH THE NAMES FOUND IN THE CSCBS. IF NO NAME IS FOUND THEN WE*/
*   /* TERMINATE AND ISSUE ERROR MSG IEE308I 'RESET TERM LENGTH      */
*   /* ERROR'.                                                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0136
*   DO WHILE(SCANCOL^=','&REG1^=REG0);/* SCAN FOR COMMA OR END OF  0136
*                                      BUFFER.                       */
         B     @DE00136                                            0136
@DL00136 DS    0H                                                  0137
*     REG1=REG1+1;                  /* INCREMENT SCAN PTR            */
         LA    @10,1                                               0137
         ALR   REG1,@10                                            0137
*     SIZE=SIZE+1;                  /* INCREMENT LENGTH COUNTER.     */
         MVC   @ZT00003+3(1),SIZE                                  0138
         AL    @10,@ZT00003                                        0138
         STC   @10,SIZE                                            0138
*   END;                                                           0139
@DE00136 CLI   SCANCOL(REG1),C','                                  0139
         BE    @DC00136                                            0139
         CR    REG1,REG0                                           0139
         BNE   @DL00136                                            0139
@DC00136 DS    0H                                                  0140
*   IF REG1^=REG0 THEN              /* END OF BUFFER?                */
         CR    REG1,REG0                                           0140
         BE    @RF00140                                            0140
*     DO;                           /* IF NOT THEN PROCEED           */
*       IF SIZE=0×SIZE>8 THEN       /* IF THE LENGTH IS 0 OR >8 THEN
*                                      NO JOBNAME WAS FOUND          */
         CLI   SIZE,0                                              0142
         BE    @RT00142                                            0142
         CLI   SIZE,8                                              0142
         BNH   @RF00142                                            0142
@RT00142 DS    0H                                                  0143
*         XAE='08'X;                /* PLACE ERROR MSG ID INTO XAE 0143
*                                      FIELD                         */
         MVI   XAE(XSAPTR),X'08'                                   0143
*       ELSE                                                       0144
*         DO;                                                      0144
         B     @RC00142                                            0144
@RF00142 DS    0H                                                  0145
*           REG1=ADDR(REG8->CHBUF); /* POINT TO BEGINNING OF       0145
*                                      PARAMETER                     */
         LA    REG1,CHBUF(,REG8)                                   0145
*           BUFFER1(1:SIZE)=INBUFFER(1:SIZE);                      0146
         SLR   @10,@10                                             0146
         IC    @10,SIZE                                            0146
         BCTR  @10,0                                               0146
         EX    @10,@SM01253                                        0146
*         END;                                                     0147
*     END;                                                         0148
*   ELSE                                                           0149
*     XAE='08'X;                    /* EXCEEDED BUFFER               */
         B     @RC00140                                            0149
@RF00140 MVI   XAE(XSAPTR),X'08'                                   0149
*   RETURN;                         /* RETURN TO CALLER              */
@EL00006 DS    0H                                                  0150
@EF00006 DS    0H                                                  0150
@ER00006 BR    @14                                                 0150
*   END;                                                           0151
         B     @EL00006                                            0151
*GETBUFSC:                                                         0152
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0152
*                                                                  0152
GETBUFSC DS    0H                                                  0153
*/*                                                                0153
********************************************************************
**                                                                 0153
**     ROUTINE NAME        IEEBUFSC                                0153
**                                                                 0153
**     FUNCTION            TO SCAN A BUFFER FOR A GIVEN KEYWORD OF 0153
**                         A GIVEN LENGTH AND TO RETURN A POINTER  0153
**                         TO THE KEYWORD VALUE AND THE LENGTH OF  0153
**                         THE VALUE                               0153
**                                                                 0153
**     ENTRY POINT         IEEBUFSC                                0153
**                                                                 0153
**     ATTRIBUTES          REFRESHABLE                             0153
**                                                                 0153
**     OPERATION           1. SET UP PTR TO BUFFER -1 AND PTR      0153
**                            TO KEYWORD LENGTH                    0153
**                         2. IF BUFFER PTR = SPECIFIED KEYWORD    0153
**                            GO TO FOUND ROUTINE (3).             0153
**                            A. IF NOT, INCREMENT BUFFER PTR      0153
**                               BY ONE.                           0153
**                            B. IF PTR IS AT END OF BUFFER SET    0153
**                               UP NOT FOUND RETURN CODE (8) IN   0153
**                               R15 AND EXIT.                     0153
**                            C. IF PTR IS AT A COMMA, GO BACK TO  0153
**                               OPERATION 2.                      0153
**                            D. IF PTR IS AT A BLANK SET UP NOT   0153
**                               FOUND RETURN CODE (8) IN R15 AND  0153
**                               EXIT.                             0153
**                            E. OTHERWISE GO TO OPERATION 2.A.    0153
**                         3. AT FOUND ROUTINE SET UP R14 TO POINT 0153
**                            TO KEYWORD VALUE                     0153
**                         4. SCAN KEYWORD VALUE TO THE END OF     0153
**                            THE BUFFER, A COMMA OR A BLANK       0153
**                         5. CALCULATE LENGTH OF KEYWORD VALUE.   0153
**                         6. IF LENGTH IS < OR = ZERO, SET UP     0153
**                            AN INVALID KEYWORD VALUE LENGTH      0153
**                            RETURN CODE (4) IN R15 AND EXIT.     0153
**                         7. OTHERWISE, SET GOOD RETURN CODE      0153
**                            OF ZERO IN R15 AND EXIT.             0153
**                                                                 0153
**     EXTERNAL REFS       NONE                                    0153
**                                                                 0153
**     SVCS                NONE                                    0153
**                                                                 0153
**     INPUT               R1 - PTR TO BEGINNING OF BUFFER         0153
**                         R0 - PTR TO LAST BYTE OF BUFFER + 1     0153
**                         R15- PTR TO KEYWORD WITH THE LENGTH     0153
**                              OF THE KEYWORD IN THE HIGH ORDER   0153
**                              BYTE.                              0153
**                              EG. KEYWORD FORMAT 'DSNAME='       0153
**                                  R15 WOULD POINT TO THE 'D'     0153
**                                  WITH A 6 IN THE HIGH ORDER     0153
**                                  BYTE.                          0153
**                                                                 0153
**     OUTPUT              R14  PTR TO FIRST BYTE OF KEYWORD       0153
**                              VALUE IN BUFFER OR WORKAREA        0153
**                         R1   LENGTH OF KEYWORD VALUE            0153
**                         R15  RETURN CODE OF ZERO.               0153
**                                                                 0153
**     ERROR OUTPUT        R15  CONTAINS A FOUR IF KEYWORD         0153
**                              HAD AN INVALID LENGTH              0153
**                         R15  CONTAINS AN EIGHT IF THE KEYWORD   0153
**                              WAS NOT FOUND IN THE BUFFER        0153
**                                                                 0153
**     EXIT                TO NEXT SEQUENTIAL INSTRUCTION AFTER    0153
**                         MACRO CALL                              0153
**                                                                 0153
**     NOTES               LISTS OF OPERANDS IN PARENTHESES ARE    0153
**                         NOT PROCESSED. THIS MACRO IS INTENDED   0153
**                         TO PROCESS ONLY A SINGLE VALUE  FOR     0153
**                         A GIVEN KEYWORD                         0153
********************************************************************
*                                                                    */
*                                                                  0153
*   REG4=REG14;                     /* SAVE RETURN ADDRESS           */
         LR    REG4,REG14                                          0153
*   REG1=ADDR(REG8->CHBUF);         /* GET ADDR OF BUFFER            */
         LA    REG1,CHBUF(,REG8)                                   0154
*   REG0=ADDR(REG8->CHTYPE);        /* GET ADDR OF BUF END           */
         LA    REG0,CHTYPE(,REG8)                                  0155
*   REG15=ADDR(KEYWORD);            /* GET PTR TO KEYWORD            */
         LA    REG15,KEYWORD                                       0156
*   REG15=REG15×KEYWDLEN;           /* GET LENGTH OF KEYWD           */
         O     REG15,KEYWDLEN                                      0157
*   GEN(IEEBUFSC);                                                 0158
         IEEBUFSC
*   REG5=REG14;                     /* SAVE PTR TO VALUE             */
         LR    REG5,REG14                                          0159
*   REG14=REG4;                     /* RESTORE RETURN ADDR           */
         LR    REG14,REG4                                          0160
*   END;                                                           0161
