*                                                                  0162
CHECKRC1 DS    0H                                                  0163
*   /*****************************************************************/
*   /*                                                               */
*   /* THE RETURN CODES FROM IEEBUFSC ARE CHECKED. IF RC = 4 THEN THE*/
*   /* ERROR MSG IEE308I 'RESET TERM LENGTH ERROR IS ISSUED'. IF THE */
*   /* RC = 8 THEN THE KEYWORD WAS MISSING AND THE ERROR MSG IEE309I */
*   /* 'RESET UNIDENTIFIABLE KEYWORD' ISSUED.                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0163
*   IF REG15=4 THEN                 /* RC=4 VALUE MISSING            */
         CH    REG15,@CH00061                                      0163
         BNE   @RF00163                                            0163
*     XAE='08'X;                                                   0164
         MVI   XAE(XSAPTR),X'08'                                   0164
*   ELSE                                                           0165
*     DO;                                                          0165
         B     @RC00163                                            0165
@RF00163 DS    0H                                                  0166
*       IF REG15=8 THEN             /* RC=8 KEYWORD MISSING          */
         CH    REG15,@CH00040                                      0166
         BNE   @RF00166                                            0166
*         XAE='09'X;                                               0167
         MVI   XAE(XSAPTR),X'09'                                   0167
*       ELSE                                                       0168
*         XAE='FF'X;                /* CANT IDENTIFY RC              */
         B     @RC00166                                            0168
@RF00166 MVI   XAE(XSAPTR),X'FF'                                   0168
*     END;                                                         0169
*   END;                                                           0170
@EL00008 DS    0H                                                  0170
@EF00008 DS    0H                                                  0170
@ER00008 BR    @14                                                 0170
*CKVALUE:                                                          0171
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0171
*                                                                  0171
CKVALUE  DS    0H                                                  0172
*   /*****************************************************************/
*   /*                                                               */
*   /* IEEBUFSC HAS LOCATED THE KEYWORD AND REG 5 POINTS TO THE      */
*   /* VALUE. A CHECK IS NOW MADE THAT THE VALUE HAS THE CORRECT     */
*   /* LENGTH AND IS ALL NUMERIC.                                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0172
*   REG7=REG5;                      /* SAVE VALUE PTR                */
         LR    REG7,REG5                                           0172
*   REG4=REG1;                      /* SAVE LENTH OF VALUE           */
         LR    REG4,REG1                                           0173
*   IF REG1>3 THEN                  /* COMPARE SCANNED VALUE'S LENGTH
*                                      WITH MAX LENGTH               */
         CH    REG1,@CH00431                                       0174
         BNH   @RF00174                                            0174
*     XAE='08'X;                    /* FLAG FOR ERROR MSG IF TOO   0175
*                                      LARGE                         */
         MVI   XAE(XSAPTR),X'08'                                   0175
*   ELSE                            /* IF LENGTH CORRECT THEN CK IF
*                                      NUMERIC                       */
*     DO WHILE(REG1>0);             /* SETUP DO LOOP TO CK ALL     0176
*                                      NUMBERS                       */
         B     @RC00174                                            0176
@RF00174 B     @DE00176                                            0176
@DL00176 DS    0H                                                  0177
*       IF PERVALUE(1)>'9'×PERVALUE(1)<'0' THEN/* IS IT A NUMERIC  0177
*                                      VALUE?                        */
         CLI   PERVALUE(REG5),C'9'                                 0177
         BH    @RT00177                                            0177
         CLI   PERVALUE(REG5),C'0'                                 0177
         BNL   @RF00177                                            0177
@RT00177 DS    0H                                                  0178
*         XAE='06'X;                /* NO, SETUP ERROR MSG           */
         MVI   XAE(XSAPTR),X'06'                                   0178
*       REG5=REG5+1;                /* ADVANCE COL PTR REG           */
@RF00177 AH    REG5,@CH00047                                       0179
*       REG1=REG1-1;                /* DECREASE LENGTH               */
         BCTR  REG1,0                                              0180
*     END;                                                         0181
@DE00176 LTR   REG1,REG1                                           0181
         BP    @DL00176                                            0181
*   IF XAE=0 THEN                   /* ALL ARE NUMERIC               */
@RC00174 CLI   XAE(XSAPTR),0                                       0182
         BNE   @RF00182                                            0182
*     DO;                           /* THEN MOVE VALUE               */
*       REG5=REG7;                  /* RESTORE STARTING PTR          */
         LR    REG5,REG7                                           0184
*       REG7=5-REG4;                /* GET CORRECT BYTE LOCATION TO
*                                      MOVE VALUE INTO               */
         LA    REG7,5                                              0185
         SLR   REG7,REG4                                           0185
*       ZONEAREA(REG7:4)=PERVALUE(1:REG4);/* MOVE TO HOLDAREA FOR  0186
*                                      CONVERT TO BINARY             */
         LA    @10,ZONEAREA-1(REG7)                                0186
         LA    @09,4                                               0186
         SLR   @09,REG7                                            0186
         EX    @09,@SM01255                                        0186
*       IF ZONEAREA(2:4)^>'255' THEN/* VALUE GREATER THAN 255        */
         CLC   ZONEAREA+1(3),@CC01237                              0187
         BH    @RF00187                                            0187
*         REG15=0;                  /* YES,PUT OK RC IN REG15        */
         SLR   REG15,REG15                                         0188
*       ELSE                                                       0189
*         DO;                                                      0189
         B     @RC00187                                            0189
@RF00187 DS    0H                                                  0190
*           REG15=4;                /* NO, PUT NO GOOD RC IN 15      */
         LA    REG15,4                                             0190
*           XAE='3D'X;              /* XAE = UNACCEPTABLE VALUE      */
         MVI   XAE(XSAPTR),X'3D'                                   0191
*           XAV=PERFORM;            /* MOVE IN PERFORM KEYWD         */
         MVC   XAV(8,XSAPTR),PERFORM                               0192
*         END;                                                     0193
*     END;                                                         0194
*   ELSE                                                           0195
*     REG15=4;                      /* PUT BAD RC IN R15             */
         B     @RC00182                                            0195
@RF00182 LA    REG15,4                                             0195
*   END;                                                           0196
@EL00009 DS    0H                                                  0196
@EF00009 DS    0H                                                  0196
@ER00009 BR    @14                                                 0196
*SYSEVENT:                                                         0197
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0197
*                                                                  0197
SYSEVENT DS    0H                                                  0198
*   /*****************************************************************/
*   /*                                                               */
*   /* BEFORE ISSUING THE SYSEVENT MACRO THE PERFORM VALUE IS        */
*   /* CONVERTED TO BINARY AND THE CONVERTED VALUE MOVED INTO THE 3RD*/
*   /* BYTE OF REGISTER 1. THE ASID FROM THE CSCB IS PLACED INTO THE */
*   /* TWO LOW ORDER BYTES OF REGISTER 0.                            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0198
*   REG0=CHASID;                    /* GET ASID FROM CSCB            */
         LH    REG0,CHASID(,CSCBPTR)                               0198
*   GEN;                                                           0199
     PACK PACKAREA,ZONEAREA            PACK PERFORM VALUE
     CVB  REG1,PACKAREA                CONVERT TO BINARY
     SYSEVENT  RESETPG,ASID=(0)        OF REG1.
     SLL REG1,16                       MOVE OUT ANY EXTRA
     SRL REG1,24                       DATA BEFORE DOING COMPARE
*   END;                                                           0200
@EL00010 DS    0H                                                  0200
@EF00010 DS    0H                                                  0200
@ER00010 BR    @14                                                 0200
*GET0503D:                                                         0201
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0201
*                                                                  0201
GET0503D DS    0H                                                  0202
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS ROUTINE LOADS MSG MODULE IEE0503D. BRANCHES TO IT TO     */
*   /* ISSUE ALL MESSAGES THEN DELETES IT UPON RETURN.               */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0202
*   REG6=REG14;                     /* SAVE RETURN ADDRESS           */
         LR    REG6,REG14                                          0202
*   GEN(LOAD EP=IEE0503D);          /* LOAD IN IEE0503D              */
         LOAD EP=IEE0503D
*   REG15=REG0;                     /* GET ENTRY ADDRESS             */
         LR    REG15,REG0                                          0204
*   GEN(BALR REG14,REG15);          /* BRANCH TO MSG MODULE          */
         BALR REG14,REG15
*   GEN(DELETE EP=IEE0503D);        /* DELETE IEE0503D               */
         DELETE EP=IEE0503D
*   REG14=REG6;                     /* RESTORE RETURN ADDRESS        */
         LR    REG14,REG6                                          0207
*   END;                                                           0208
@EL00011 DS    0H                                                  0208
@EF00011 DS    0H                                                  0208
@ER00011 BR    @14                                                 0208
*CHECKRC2:                                                         0209
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0209
*                                                                  0209
CHECKRC2 DS    0H                                                  0210
*   /*****************************************************************/
*   /*                                                               */
*   /* THE RETURN FROM SYSEVENT IS CHECKED. IF THE 2ND BYTE OF REG1  */
*   /* CONTAINS 0 THE NEW PERFORM VALUE WAS ACCEPTED. IF IT WAS 4 THE*/
*   /* VALUE WAS REJECTED. IF IT WAS 8 THE JOB WAS NOT ACTIVE.       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0210
*   IF REG1=0 THEN                  /* CHECK RC FROM                 */
         LTR   REG1,REG1                                           0210
         BNZ   @RF00210                                            0210
*     DO;                                                          0211
*       XAE='04'X;                  /* SYSEVENT.IF ZERO              */
         MVI   XAE(XSAPTR),X'04'                                   0212
*       XAV=BUFFER1;                /* MOVE JOBNAME INTO XAE         */
         MVC   XAV(8,XSAPTR),BUFFER1                               0213
*     END;                          /* VALUE ACCEPTABLE OTHERWISE    */
*   ELSE                                                           0215
*     IF REG1=4 THEN                /* VALUE UNACCEPTABLE            */
         B     @RC00210                                            0215
@RF00210 CH    REG1,@CH00061                                       0215
         BNE   @RF00215                                            0215
*       DO;                                                        0216
*         XAE='3D'X;                /* SET ERROR CONDITION           */
         MVI   XAE(XSAPTR),X'3D'                                   0217
*         XAV=PERFORM;              /* MOVE IN PERFORM KEYWD         */
         MVC   XAV(8,XSAPTR),PERFORM                               0218
*       END;                                                       0219
*     ELSE                                                         0220
*       IF REG1=8 THEN                                             0220
         B     @RC00215                                            0220
@RF00215 CH    REG1,@CH00040                                       0220
         BNE   @RF00220                                            0220
*         DO;                                                      0221
*           XAE='24'X;              /* JOB NOT ACTIVE                */
         MVI   XAE(XSAPTR),X'24'                                   0222
*           XAV=BUFFER1;            /* MOVE JOBNAME INTO XAE         */
         MVC   XAV(8,XSAPTR),BUFFER1                               0223
*         END;                                                     0224
*       ELSE                                                       0225
*         XAE='FF'X;                /* CANT IDENTIFY RC              */
         B     @RC00220                                            0225
@RF00220 MVI   XAE(XSAPTR),X'FF'                                   0225
*   END;                                                           0226
@EL00012 DS    0H                                                  0226
@EF00012 DS    0H                                                  0226
@ER00012 BR    @14                                                 0226
