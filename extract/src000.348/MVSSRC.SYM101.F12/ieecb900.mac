         TITLE 'IEECB900 - VARY CN SYNTAX CHECKER                      *
                        '
IEECB900 CSECT ,                                                   0001
@MAINENT BALR  @15,0                                               0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'IEECB900  74.165'                                 0001
         DROP  @15
@PROLOG  BALR  @11,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@11                                         0001
         LR    @03,@01                                             0001
         L     @00,@SIZDATD                                        0001
         GETMAIN  R,LV=(0)
         LR    @12,@01                                             0001
         USING @DATD,@12                                           0001
         LR    @01,@03                                             0001
         ST    @13,@SA00001+4                                      0001
         LR    @13,@12                                             0001
         MVC   @PC00001(4),0(@01)                                  0001
*                                                                  0096
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS IS THE MAIN ENTRY POINT FOR COMMAND PROCESSING.          */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0096
*   R14SAVE=REG14;                  /* SAVE RETURN ADDRESS           */
         ST    REG14,R14SAVE                                       0096
*   REG0=LENGTH(PARMMAP);           /* PARM LIST LENGTH TO CLEAR     */
         LA    REG0,18                                             0097
*   PARMMAP(1:REG0)=PARMMAP(1:REG0)&&PARMMAP(1:REG0);/* CLEAR AREA   */
         LR    @10,REG0                                            0098
         BCTR  @10,0                                               0098
         EX    @10,@SX01371                                        0098
*   CSCBPTR=REG1;                   /* SAVE CSCB PTR                 */
         ST    REG1,CSCBPTR                                        0099
*   MSGCODE=ZERO;                   /* SET ERROR INDICATOR OFF       */
         MVI   MSGCODE,X'00'                                       0100
*   VERBNAME=VARYC;                 /* INIT MESSAGE VARIABLE         */
*                                                                  0101
         MVC   VERBNAME(8),VARYC                                   0101
*   /*****************************************************************/
*   /*                                                               */
*   /* SET UP FOR ESTAE ENVIRONMENT FOR THIS MODULE.                 */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0102
*   CALL STAERTN;                                                  0102
         BAL   @14,STAERTN                                         0102
*   IF REG15=ZERO THEN              /* ESTAE SUCCESSFUL ?            */
         LTR   REG15,REG15                                         0103
         BNZ   @RF00103                                            0103
*     DO;                                                          0104
*       GEN(LOAD EP=IEE0503D);      /* GET MSG MODULE ADDRESS        */
         LOAD EP=IEE0503D
*       MSGMPTR=REG0;               /* SAVE ADDRESS IN PARM LIST     */
         ST    REG0,MSGMPTR                                        0106
*       MSGAPTR=ADDR(MSGAREA);      /* INIT PARM LIST ADDR           */
         LA    @10,MSGAREA                                         0107
         ST    @10,MSGAPTR                                         0107
*       CALL EOBRTN;                /* CALCULATE INPUT LENGTH        */
*                                                                  0108
         BAL   @14,EOBRTN                                          0108
*       /*************************************************************/
*       /*                                                           */
*       /* THE FOLLOWING RTN, CNCHECK, WILL PROCESS THE VALUES OF THE*/
*       /* 'CN(' PARAMETER FOR VALIDITY. IT CALLS SUB-RTN PARENSCN   */
*       /* (TO VERIFY BALANCED PARENS AND THAT THERE IS A VALID DATA */
*       /* LENGTH WITHIN PARENS), AND SUB-RTN DELIMRTN (TO VERIFY    */
*       /* NUMERIC DATA AND VALID CONSOLE ID LENGTH - ONE/TWO        */
*       /* DIGITS.) INVALID VALUES RESULT IN FIELD MSGCODE BEING SET */
*       /* TO NON-ZERO.                                              */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0109
*       CALL CNCHECK;               /* VERIFY VALID PARAMETER        */
         BAL   @14,CNCHECK                                         0109
*       IF MSGCODE=ZERO THEN        /* PARAMETER, PARENS CHECK OK    */
         CLI   MSGCODE,0                                           0110
         BNE   @RF00110                                            0110
*         DO;                                                      0111
*                                                                  0111
*           /*********************************************************/
*           /*                                                       */
*           /* SET UP TO PROCESS COMMAND OPERANDS. THE INPUT LINE    */
*           /* WILL BE SCANNED FOR A VALID OPERAND. IF FOUND, THE    */
*           /* OPERAND VALUE WILL BE PROCESSED TO COMPLETION. REPEAT */
*           /* SCAN UNTIL ALL OPERANDS HAVE BEEN PROCESSED, OR AN    */
*           /* ERROR CONDITION IS SET - MSGCODE = NON-ZERO.          */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0112
*           SPINPTR=ONE;            /* SET LOOP CONDITION            */
         MVI   SPINPTR,X'01'                                       0112
*           DO WHILE(MSGCODE=ZERO&SPINPTR=ONE);/* SET UP LOOP        */
         B     @DE00113                                            0113
@DL00113 DS    0H                                                  0114
*             IF INPTR->COLSCAN=COMMA THEN/* OPERAND DELIMITER       */
         CLC   COLSCAN(1,INPTR),COMMA                              0114
         BNE   @RF00114                                            0114
*               DO;                                                0115
*                                                                  0115
*                 /***************************************************/
*                 /*                                                 */
*                 /* SET UP INPUT TO RTN VALIDSCN. INPTR=1ST CHAR    */
*                 /* PAST COMMA DELIMITER. REG15 PTR TO THE TABLE OF */
*                 /* VALID OPERAND ADDRESSES. RTN VALIDSCN WILL      */
*                 /* COMPARE THE INPUT(INPTR) TO THE ENTRIES IN THE  */
*                 /* OPERAND TABLE. FOR A VALID MATCH, REG15 WILL    */
*                 /* CONTAIN A PTR TO THE OPERAND, AND FIELD TABINDX */
*                 /* WILL CONTAIN AN OFFSET VALUE TO BE USED TO      */
*                 /* REFERENCE THE OPERAND PROCESSOR VIA A SUBRTN    */
*                 /* BRANCH TABLE.                                   */
*                 /*                                                 */
*                 /***************************************************/
*                                                                  0116
*                 INPTR=INPTR+ONE;  /* BUMP TO OPERAND               */
         AL    INPTR,@CF00064                                      0116
*                 REG15=ADDR(OPADRTAB);/* PTR TO OPERAND ADDRESSES   */
         LA    REG15,OPADRTAB                                      0117
*                 CALL VALIDSCN;    /* CHECK FOR VALID OPERAND       */
         BAL   @14,VALIDSCN                                        0118
*                 IF REG15^=ZERO THEN/* PTR TO VALID OPERAND         */
         LTR   REG15,REG15                                         0119
         BZ    @RF00119                                            0119
*                   DO;                                            0120
*                                                                  0120
*                     /***********************************************/
*                     /*                                             */
*                     /* VALID OPERAND FOUND. SET INDEX VALUE FOR    */
*                     /* BRANCH TABLE AND PROCESS THE OPERAND VALUES.*/
*                     /* ON RETURN FROM NORMAL PROCESSING THE OPERAND*/
*                     /* PTR - INPTR, IS EXPECTED TO POINT AT OPERAND*/
*                     /* DELIMITER - COMMA, OR END OF INPUT ADDRESS  */
*                     /*                                             */
*                     /***********************************************/
*                                                                  0121
*                     ARRINDX=(TABINDX/FOUR)+ONE;/* ARRAY INDEX PTR  */
         SLR   @10,@10                                             0121
         IC    @10,TABINDX                                         0121
         SRL   @10,2                                               0121
         AL    @10,@CF00064                                        0121
         STC   @10,ARRINDX                                         0121
*                     CALL RTNPTR;  /* IDENTIFY/PROCESS OPERAND      */
         SLR   @10,@10                                             0122
         IC    @10,ARRINDX                                         0122
         SLA   @10,2                                               0122
         L     @15,PTRARR-4(@10)                                   0122
         BALR  @14,@15                                             0122
*                   END;                                           0123
*                 ELSE                                             0124
*                   MSGCODE=MSG3E;  /* SET ERROR CODE - IEE535I      */
         B     @RC00119                                            0124
@RF00119 MVC   MSGCODE(1),MSG3E                                    0124
*               END;                                               0125
*             ELSE                  /* EOB OR INVALID DELIMITER      */
*               DO;                                                0126
         B     @RC00114                                            0126
@RF00114 DS    0H                                                  0127
*                 IF INPTR=BUFSIZE THEN/* END OF INPUT               */
         CR    INPTR,BUFSIZE                                       0127
         BNE   @RF00127                                            0127
*                   SPINPTR=ZERO;   /* SET END OF LOOP INDICATOR     */
         MVI   SPINPTR,X'00'                                       0128
*                 ELSE              /* INVALID DELIMITER             */
*                   MSGCODE=MSG7;   /* SET ERROR MSG CODE - IEE307I  */
         B     @RC00127                                            0129
@RF00127 MVC   MSGCODE(1),MSG7                                     0129
*               END;                                               0130
@RC00127 DS    0H                                                  0131
*           END;                                                   0131
@RC00114 DS    0H                                                  0131
@DE00113 CLI   MSGCODE,0                                           0131
         BNE   @DC00113                                            0131
         CLI   SPINPTR,1                                           0131
         BE    @DL00113                                            0131
@DC00113 DS    0H                                                  0132
*           IF MSGCODE=ZERO THEN    /* SUCCESSFUL PROCESSING         */
         CLI   MSGCODE,0                                           0132
         BNE   @RF00132                                            0132
*             DO;                                                  0133
*               REG1=ADDR(PARMMAP); /* SET UP PARM LIST PTR          */
         LA    REG1,PARMMAP                                        0134
*               CALL IEECB901;      /* CALL UCME PROCESSOR           */
         L     @15,@CV01165                                        0135
         BALR  @14,@15                                             0135
*               IF COMPCODE=ONE THEN/* AUTH FIELD UPDATED            */
         CLI   COMPCODE,1                                          0136
         BNE   @RF00136                                            0136
*                 DO;                                              0137
*                   VERBNAME=VARYC; /* INIT MSG VARIABLE FIELD       */
         MVC   VERBNAME(8),VARYC                                   0138
*                   MSGCODE=MSG3;   /* COMPLETION MSG - IEE712I      */
         MVC   MSGCODE(1),MSG3                                     0139
*                 END;                                             0140
*             END;                                                 0141
@RF00136 DS    0H                                                  0142
*         END;                                                     0142
@RF00132 DS    0H                                                  0143
*       CALL MSGRTN;                /* SET UP FOR MSG ISSUE          */
@RF00110 BAL   @14,MSGRTN                                          0143
*       GEN( DELETE EP=IEE0503D);   /* FREE MSG MODULE               */
          DELETE EP=IEE0503D
*     END;                                                         0145
*   CALL FREECSCB;                  /* FREE CSCB CORE                */
@RF00103 BAL   @14,FREECSCB                                        0146
*   REG14=R14SAVE;                  /* RESTORE RETURN ADDRESS        */
         L     REG14,R14SAVE                                       0147
*   RETURN;                                                        0148
@EL00001 L     @13,4(,@13)                                         0148
@EF00001 L     @00,@SIZDATD                                        0148
         LR    @01,@12                                             0148
         FREEMAIN R,LV=(0),A=(1)
@ER00001 BR    @14                                                 0148
*EOBRTN:                                                           0149
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0149
*                                                                  0149
EOBRTN   DS    0H                                                  0150
*   /*****************************************************************/
*   /*                                                               */
*   /* ENTRY IS TO DETERMINE THE SIZE OF THE INPUT, AND TO INITIALIZE*/
*   /* THE FIELD - BUFSIZE, WITH THE ADDRESS OF END-OF-INPUT         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0150
*   DCL                                                            0150
*     EOBSIZE PTR(31);              /* END OF BUFFER PRE             */
*   INPTR=ADDR(CHBUF);              /* INIT CSCB OPERAND ADDRESS     */
         L     INPTR,CSCBPTR                                       0151
         LA    INPTR,CHBUF(,INPTR)                                 0151
*   EOBSIZE=INPTR+LENGTH(CHBUF);    /* ADDR OF BUFFER LENGTH         */
         LA    EOBSIZE,124                                         0152
         ALR   EOBSIZE,INPTR                                       0152
*   DO WHILE(COLSCAN^=BLANK&INPTR<EOBSIZE);                        0153
         B     @DE00153                                            0153
@DL00153 DS    0H                                                  0154
*     INPTR=INPTR+ONE;              /* UPDATE SCAN POINTER           */
         AL    INPTR,@CF00064                                      0154
*   END;                                                           0155
@DE00153 CLC   COLSCAN(1,INPTR),BLANK                              0155
         BE    @DC00153                                            0155
         CR    INPTR,EOBSIZE                                       0155
         BL    @DL00153                                            0155
@DC00153 DS    0H                                                  0156
*   BUFSIZE=INPTR;                  /* INITIALIZE INPUT LENGTH       */
         LR    BUFSIZE,INPTR                                       0156
*   END EOBRTN;                                                    0157
@EL00002 DS    0H                                                  0157
@EF00002 DS    0H                                                  0157
@ER00002 BR    @14                                                 0157
*CNCHECK:                                                          0158
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0158
*                                                                  0158
CNCHECK  DS    0H                                                  0159
*   /*****************************************************************/
*   /*                                                               */
*   /* ENTRY HERE IS TO VALIDITY CHECK THE 'CN' PARAMETER. VALID     */
*   /* VALUES ARE 'CN('. IF VALUE IS 'CN(', SUBRTN PARENSCN IS CALLED*/
*   /* TO VERIFY BALANCED PARENS. VALID VALUES - PTR (INPTR) IS      */
*   /* BUMPED TO EXPECTED OPERAND . INDICATOR, PTR TO PARENTHEZIED   */
*   /* CONSOLE ID(S) IS STORED IN PARAMETER LIST FIELD - CONIDPTR.   */
*   /* INVALID VALUES - ERROR RETURN CODE, ERROR MSG CODE IS SET - . */
*   /* INVALID PARAMETER - MSG10 - IEE310I . UNBALANCED PAREN - MSG7 */
*   /* - IEE307I. INVALID DELIMITER - COMMA, - MSG3E - IEE535I       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0159
*   SAVE1=REG14;                    /* SAVE RETURN ADDRESS           */
         ST    REG14,SAVE1                                         0159
*   INPTR=ADDR(CHBUF);              /* PTR TO COMMAND OPERAND FIELD  */
         L     @10,CSCBPTR                                         0160
         LA    INPTR,CHBUF(,@10)                                   0160
*   IF CHBUF(1:3)=CNP THEN          /* VALID PARAMETER?              */
         CLC   CHBUF(3,@10),CNP                                    0161
         BNE   @RF00161                                            0161
*     DO;                                                          0162
*       INPTR=INPTR+THREE;          /* BUMP PAST 'CN(' FIELD         */
         AL    INPTR,@CF00282                                      0163
*       CONIDPTR=INPTR;             /* SAVE UCMID PTR IN PARM LIST   */
         ST    INPTR,CONIDPTR                                      0164
*       CALL PARENSCN;              /* CHECK FOR BALANCE PARENS      */
         BAL   @14,PARENSCN                                        0165
*       IF MSGCODE=ZERO THEN        /* PARENS O.K.                   */
         CLI   MSGCODE,0                                           0166
         BNE   @RF00166                                            0166
*         DO;                                                      0167
*           INPTR=INPTR+ONE;        /* PTR TO EXPECTED DELIM, COMMA  */
         AL    INPTR,@CF00064                                      0168
*           IF INPTR->COLSCAN=COMMA THEN/* DELIM PRESENT             */
         CLC   COLSCAN(1,INPTR),COMMA                              0169
         BNE   @RF00169                                            0169
*             CALL DELIMRTN;        /* VERIFY CN VALUES              */
         BAL   @14,DELIMRTN                                        0170
*           ELSE                                                   0171
*             MSGCODE=MSG3E;        /* SET MSG CODE - IEE535I        */
         B     @RC00169                                            0171
@RF00169 MVC   MSGCODE(1),MSG3E                                    0171
*         END;                                                     0172
*     END;                                                         0173
*   ELSE                            /* INVALID PARAMETER             */
*     MSGCODE=MSG10;                /* SET MSG CODE - IEE3103I       */
         B     @RC00161                                            0174
@RF00161 MVC   MSGCODE(1),MSG10                                    0174
*   REG14=SAVE1;                    /* RESTORE RETURN ADDRESS        */
@RC00161 L     REG14,SAVE1                                         0175
*   END CNCHECK;                                                   0176
@EL00003 DS    0H                                                  0176
@EF00003 DS    0H                                                  0176
@ER00003 BR    @14                                                 0176
*PARENSCN:                                                         0177
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0177
*                                                                  0177
PARENSCN DS    0H                                                  0178
*   /*****************************************************************/
*   /*                                                               */
*   /* ENTRY IS TO SCAN FOR A BALANCED PAREN-(RIGHT PAREN). ON ENTRY,*/
*   /* INPTR IS POINTING TO THE FIRST CHARACTER PAST A LEFT PAREN.   */
*   /* ERROR CONDITIONS ARE - SCANNING A LEFT PAREN, SCANNING TO END */
*   /* OF INPUT BEFORE SCANNING A RIGHT PAREN, NO DATA IN PARENS.IF  */
*   /* ERROR, SET MSGCODE FIELD. RETURN TO CALLER.                   */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0178
*   CNT=ONE;                        /* INIT CHARACTER COUNT FIELD    */
         MVI   CNT,X'01'                                           0178
*   DO WHILE(COLSCAN^=LPAREN&COLSCAN^=RPAREN&MSGCODE=ZERO);        0179
         B     @DE00179                                            0179
@DL00179 DS    0H                                                  0180
*     IF INPTR<BUFSIZE THEN         /* NOT END OF INPUT              */
         CR    INPTR,BUFSIZE                                       0180
         BNL   @RF00180                                            0180
*       DO;                                                        0181
*         CNT=CNT+ONE;              /* INCREMENT COUNT FIELD         */
         LA    @10,1                                               0182
         SLR   @08,@08                                             0182
         IC    @08,CNT                                             0182
         ALR   @10,@08                                             0182
         STC   @10,CNT                                             0182
*         INPTR=INPTR+ONE;          /* BUMP TO NEXT CHARACTER        */
         AL    INPTR,@CF00064                                      0183
*       END;                                                       0184
*     ELSE                          /* END OF INPUT, NO RPAREN       */
*       MSGCODE=MSG7;               /* SET ERROR MSG CODE            */
         B     @RC00180                                            0185
@RF00180 MVC   MSGCODE(1),MSG7                                     0185
*   END;                                                           0186
@RC00180 DS    0H                                                  0186
@DE00179 CLC   COLSCAN(1,INPTR),LPAREN                             0186
         BE    @DC00179                                            0186
         CLC   COLSCAN(1,INPTR),RPAREN                             0186
         BE    @DC00179                                            0186
         CLI   MSGCODE,0                                           0186
         BE    @DL00179                                            0186
@DC00179 DS    0H                                                  0187
*   IF COLSCAN=LPAREN THEN          /* INVLAID DELIMITER             */
         CLC   COLSCAN(1,INPTR),LPAREN                             0187
         BNE   @RF00187                                            0187
*     MSGCODE=MSG7;                 /* SET MSG CODE                  */
         MVC   MSGCODE(1),MSG7                                     0188
*   IF COLSCAN=RPAREN THEN          /* BALANCED PARENS FOUND         */
@RF00187 CLC   COLSCAN(1,INPTR),RPAREN                             0189
         BNE   @RF00189                                            0189
*     IF CNT<TWO THEN               /* NO DATA IN PARENS             */
         CLI   CNT,2                                               0190
         BNL   @RF00190                                            0190
*       MSGCODE=MSG11;              /* SET ERROR CODE - IEE311I      */
         MVC   MSGCODE(1),MSG11                                    0191
*   END PARENSCN;                                                  0192
@EL00004 DS    0H                                                  0192
@EF00004 DS    0H                                                  0192
@ER00004 BR    @14                                                 0192
*DELIMRTN:                                                         0193
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0193
*                                                                  0193
DELIMRTN DS    0H                                                  0194
*   /*****************************************************************/
*   /*                                                               */
*   /* SCAN INPUT FOR A DELIMITER - RPAREN, OR A VALID LENGTH, (1/2  */
*   /* DIGITS) OR NUMERIC VALUE - 0/9. ANY OTHER VALUES RESULT IN    */
*   /* ERROR MSG BEING ISSUED, AND END OF PROCESSING FOR VARY        */
*   /* COMMAND. RETURN TO CALLER .                                   */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0194
*   DCL                                                            0194
*     SCANPTR PTR(31);              /* PTR FOT INPUT SCAN            */
*   DCL                                                            0195
*     CHRSCAN CHAR(1) BASED(SCANPTR);/* SCAN PTR FOR CHAR VALUE      */
*   DCL                                                            0196
*     FOX0 CHAR(1) INIT('0');       /* LOW LIMIT VALUE               */
*   DCL                                                            0197
*     FOX9 CHAR(1) INIT('9');       /* HIGH LIMIT VALUE              */
*   SCANPTR=CONIDPTR;               /* WORK PTR FOR SCAN             */
         L     SCANPTR,CONIDPTR                                    0198
*   CNT=ONE;                        /* INIT COUNT FIELD              */
         MVI   CNT,X'01'                                           0199
*   DO WHILE(CHRSCAN^=RPAREN&       /* END OF SCAN INDICATOR         */
*         MSGCODE=ZERO);            /* NO ERROR INDICATOR            */
         B     @DE00200                                            0200
@DL00200 DS    0H                                                  0201
*     IF CHRSCAN>=FOX0&             /* NUMERIC ZERO OR GREATER       */
*         CHRSCAN<=FOX9×            /* NUMERIC NINE OR LESS          */
*         CHRSCAN=COMMA THEN        /* VALUE DELIMITER COMMA         */
         CLC   CHRSCAN(1,SCANPTR),FOX0                             0201
         BL    @GL00005                                            0201
         CLC   CHRSCAN(1,SCANPTR),FOX9                             0201
         BNH   @RT00201                                            0201
@GL00005 CLC   CHRSCAN(1,SCANPTR),COMMA                            0201
         BNE   @RF00201                                            0201
@RT00201 DS    0H                                                  0202
*       DO;                                                        0202
*         IF CHRSCAN=COMMA THEN     /* COMMA DELIMITER               */
         CLC   CHRSCAN(1,SCANPTR),COMMA                            0203
         BNE   @RF00203                                            0203
*           IF CNT<TWO×             /* VALUE LENGTH OF ZERO          */
*               CNT>THREE THEN      /* VALUE LENGTH > TWO CHARS      */
         CLI   CNT,2                                               0204
         BL    @RT00204                                            0204
         CLI   CNT,3                                               0204
         BNH   @RF00204                                            0204
@RT00204 DS    0H                                                  0205
*             MSGCODE=MSG6;         /* SET ERROR CODE - IEE306I      */
         MVC   MSGCODE(1),MSG6                                     0205
*           ELSE                    /* VALID LENGTH VALUE            */
*             CNT=ZERO;             /* RE-SET COUNT VALUE            */
         B     @RC00204                                            0206
@RF00204 MVI   CNT,X'00'                                           0206
*         SCANPTR=SCANPTR+ONE;      /* BUMP SCAN PTR                 */
@RC00204 DS    0H                                                  0207
@RF00203 LA    @10,1                                               0207
         ALR   SCANPTR,@10                                         0207
*         CNT=CNT+ONE;              /* BUMP COUNT FIELD              */
         SLR   @08,@08                                             0208
         IC    @08,CNT                                             0208
         ALR   @10,@08                                             0208
         STC   @10,CNT                                             0208
*       END;                                                       0209
*     ELSE                          /* INVALID NUMERIC               */
*       MSGCODE=MSG6;               /* SET ERROR MSG - IEE306I       */
         B     @RC00201                                            0210
@RF00201 MVC   MSGCODE(1),MSG6                                     0210
*   END;                                                           0211
@RC00201 DS    0H                                                  0211
@DE00200 CLC   CHRSCAN(1,SCANPTR),RPAREN                           0211
         BE    @DC00200                                            0211
         CLI   MSGCODE,0                                           0211
         BE    @DL00200                                            0211
@DC00200 DS    0H                                                  0212
*   IF CHRSCAN=RPAREN THEN          /* RIGHT PAREN DELIMITER         */
         CLC   CHRSCAN(1,SCANPTR),RPAREN                           0212
         BNE   @RF00212                                            0212
*     IF CNT<TWO×                   /* ONE DIGIT PLUS DELIMITER      */
*         CNT>THREE THEN            /* TWO DIGITS PLUS DELIMITER     */
         CLI   CNT,2                                               0213
         BL    @RT00213                                            0213
         CLI   CNT,3                                               0213
         BNH   @RF00213                                            0213
@RT00213 DS    0H                                                  0214
*       MSGCODE=MSG6;               /* SET ERROR CODE - IEE306I      */
         MVC   MSGCODE(1),MSG6                                     0214
*     ELSE                                                         0215
*       IF MSGCODE=ZERO THEN        /* NO ERROR INDICATOR            */
         B     @RC00213                                            0215
@RF00213 CLI   MSGCODE,0                                           0215
         BNE   @RF00215                                            0215
*         IF CONIDPTR=SCANPTR THEN  /* NO VALID INPUT                */
         C     SCANPTR,CONIDPTR                                    0216
         BNE   @RF00216                                            0216
*           MSGCODE=MSG11;          /* SET ERROR CODE - IEE311I      */
         MVC   MSGCODE(1),MSG11                                    0217
*         ELSE                                                     0218
*           ;                                                      0218
@RF00216 DS    0H                                                  0219
*   END DELIMRTN;                                                  0219
@EL00005 DS    0H                                                  0219
@EF00005 DS    0H                                                  0219
@ER00005 BR    @14                                                 0219
*VALIDSCN:                                                         0220
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0220
*                                                                  0220
VALIDSCN DS    0H                                                  0221
*   /*****************************************************************/
*   /*                                                               */
*   /* ENTRY HERE IS TO DETERMINE THAT THE OPERAND(S) IS VALID. INPUT*/
*   /* IS REG15 PTR TO OPERAND/VALUE TABLE, AS MAPPED BY STRUCTURE - */
*   /* TABMAP. INITIALIZE THE TABLE INDEX VALUE - TABINDX AND COMPARE*/
*   /* INPUT VALUE TO THE TABLE ENTRY. IF NO MATCH, INCREMENT THE    */
*   /* TABINDX VALUE AND COMPARE TO NEXT TABLE ENTRY. IF NO MATCH    */
*   /* FOUND IN TABLE, SET ERROR VALUES, RETURN TO CALLER IF A MATCH */
*   /* IS FOUND, REG15 CONTAINS THE ADDRESS OF THE MATCH- ING        */
*   /* OPERAND. RETURN TO CALLER.                                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0221
*   DCL                                                            0221
*     1 TABMAP BASED(APTR),                                        0221
*                                                                  0221
*     /***************************************************************/
*     /*                                                             */
*     /* THIS STRUCTURE IS USED TO MAP THE CONTENTS OF OPERAND AND   */
*     /* VALUE TABLES. OFFSETS ARE FOR LENGTH, AND DATA FIELDS.      */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0221
*      2 DATALEN PTR(8),            /* LENGTH FIELD                  */
*      2 DATAWORD CHAR(*);          /* CHARACTER DATA FIELD          */
*   DCL                                                            0222
*     CHAR8 CHAR(*) BASED;          /* CHAR REFERENCE FOR COMPARE    */
*   DCL                                                            0223
*     ADDRINDX PTR(8);              /* ADDRESSING INDEX PTR          */
*   ADDRINDX=ZERO;                  /* SET FIELD OFF                 */
         SLR   ADDRINDX,ADDRINDX                                   0224
*   TABINDX=ZERO;                   /* SET TABLE INDEX VALUE         */
         MVI   TABINDX,X'00'                                       0225
*   SPINSW=ONE;                     /* SET LOOP INDICATOR            */
         MVI   SPINSW,X'01'                                        0226
*   DO WHILE(SPINSW=ONE&            /* LOOP INDICATOR = ON           */
*         DATAWORD(1:2)^=FOXDATA);  /* END OF TABLE INDICATOR        */
         B     @DE00227                                            0227
@DL00227 DS    0H                                                  0228
*     REG15=REG15+ADDRINDX;         /* TABLE OFFSET OF OPERAND       */
         ALR   REG15,ADDRINDX                                      0228
*     MASKTAB=DATALEN;              /* OPERAND LENGTH                */
         L     @10,APTR(,REG15)                                    0229
         MVC   MASKTAB(1),DATALEN(@10)                             0229
*     IF INPTR->CHAR8(1:MASKTAB)^=DATAWORD(1:MASKTAB) THEN         0230
         L     @10,APTR(,REG15)                                    0230
         SLR   @03,@03                                             0230
         IC    @03,MASKTAB                                         0230
         BCTR  @03,0                                               0230
         EX    @03,@SC01373                                        0230
         BE    @RF00230                                            0230
*       DO;                         /* NO COMPARE MATCH              */
*         ADDRINDX=FOUR;            /* SET ADDRESS INCREMENT         */
         LA    @10,4                                               0232
         LR    ADDRINDX,@10                                        0232
*         TABINDX=TABINDX+FOUR;     /* NO, UPDATE TABLE INDEX.       */
         SLR   @03,@03                                             0233
         IC    @03,TABINDX                                         0233
         ALR   @10,@03                                             0233
         STC   @10,TABINDX                                         0233
*       END;                                                       0234
*     ELSE                                                         0235
*       SPINSW=ZERO;                /* YES, EXIT FROM LOOP           */
         B     @RC00230                                            0235
@RF00230 MVI   SPINSW,X'00'                                        0235
*   END;                                                           0236
@RC00230 DS    0H                                                  0236
@DE00227 CLI   SPINSW,1                                            0236
         BNE   @DC00227                                            0236
         L     @10,APTR(,REG15)                                    0236
         CLC   DATAWORD(2,@10),FOXDATA                             0236
         BNE   @DL00227                                            0236
@DC00227 DS    0H                                                  0237
*   IF DATAWORD(1:2)=FOXDATA THEN   /* INVALID OPERAND               */
         L     @10,APTR(,REG15)                                    0237
         CLC   DATAWORD(2,@10),FOXDATA                             0237
         BNE   @RF00237                                            0237
*     DO;                                                          0238
*       MSGCODE=MSG3D;              /* SET MSG CODE - IEE708I        */
         MVC   MSGCODE(1),MSG3D                                    0239
*       REG15=ZERO;                 /* ZERO ADDRESS REG              */
         SLR   REG15,REG15                                         0240
*     END;                                                         0241
*   END VALIDSCN;                                                  0242
@EL00006 DS    0H                                                  0242
@EF00006 DS    0H                                                  0242
@ER00006 BR    @14                                                 0242
*AUTHRTN:                                                          0243
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0243
*                                                                  0243
AUTHRTN  DS    0H                                                  0244
*   /*****************************************************************/
*   /*                                                               */
*   /* ENTRY IS TO VALIDATE AND PROCESS THE VALUES SPECIFIED ON THE  */
*   /* 'AUTH=' OPERAND. COMMAND ISSUER MUST BE AUTHORIZED TO USE THIS*/
*   /* OPERAND - SYSTEM (CHUCMP = ZERO) OR MASTER CONSOLE (UCMDISPA =*/
*   /* ON). IF PARENS ARE PRESENT, THEY ARE TESTED FOR CLOSING PAREN.*/
*   /* IF A SINGLE VALUE IS SPECIFIED, A TABLE OF VALID VALUES IS    */
*   /* SCANNED AND COMPARED TO INPUT. IF A MATCH IS FOUND THE        */
*   /* CORRESPONDING BIT PATTERN IS STORED IN THE PARAMETER LIST TO  */
*   /* BE PASSED TO MODULE IEECB901. ELSE, ERROR FIELDS ARE          */
*   /* INITIALIZED AND RETURN TO CALLER. PARENTHEZIED VALUES MAY     */
*   /* CONTAIN MORE THAN ONE VALUE, SO THE INPUT IS SCANNED FOR A    */
*   /* DELIMITER - (',' OR ')'), VALID VALUES ARE CHECKED, AND THE   */
*   /* BIT PATTERN IS 'OR'D' INTO THE PARM LIST UNTIL THE INPUT HAS  */
*   /* BEEN TOTALLY SCANNED. INVALID VALUES RESULT IN SETTING ERROR  */
*   /* FIELDS AND RETURN TO CALLER. ON NORMAL EXIT FROM THIS RTN, THE*/
*   /* VALUE - INPTR, SHOULE BE POINTING AT END OF OPERAND DELIMITER */
*   /* - COMMA, OR END-OF INPUT                                      */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0244
*   DCL                                                            0244
*     VALUEPTR PTR(31);             /* TEMP PTR TO INPUT, USED ONLY
*                                      BY THIS RTN                   */
*   SAVE2=REG14;                    /* SAVE RETURN ADDRESS           */
         ST    REG14,SAVE2                                         0245
*   IF CHUCMP=HEX80 THEN            /* COMMAND FROM INPUT STREAM?    */
         L     @10,CSCBPTR                                         0246
         CLC   CHUCMP(1,@10),HEX80                                 0246
         BNE   @RF00246                                            0246
*     MSGCODE=MSG29;                /* SET MSG CODE - IEE345I        */
         MVC   MSGCODE(1),MSG29                                    0247
*   ELSE                                                           0248
*     DO;                                                          0248
         B     @RC00246                                            0248
@RF00246 DS    0H                                                  0249
*       RFY                                                        0249
*         UCMLIST BASED(UCMPTR);                                   0249
*       UCMPTR=UCMVEA+(UCMVEZ*(CHUCMP-ONE));/* PTR TO ISSUER UCME    */
         L     @10,CVTPTR                                          0250
         L     @10,UCMBASPT(,@10)                                  0250
         L     @08,CSCBPTR                                         0250
         SLR   UCMPTR,UCMPTR                                       0250
         IC    UCMPTR,CHUCMP(,@08)                                 0250
         BCTR  UCMPTR,0                                            0250
         LR    @05,UCMPTR                                          0250
         M     @04,UCMVEZ(,@10)                                    0250
         AL    @05,UCMVEA(,@10)                                    0250
         LR    UCMPTR,@05                                          0250
*       IF CHUCMP=ZERO×             /* THIS A SYSTEM ISSUER?         */
*           UCMDISPA='1'B THEN      /* THIS MASTER CONSOLE?          */
         CLI   CHUCMP(@08),0                                       0251
         BE    @RT00251                                            0251
         TM    UCMDISPA(UCMPTR),B'10000000'                        0251
         BNO   @RF00251                                            0251
@RT00251 DS    0H                                                  0252
*         DO;                                                      0252
*           VALUEPTR=INPTR+MASKTAB; /* INIT PTR TO OPERAND START     */
         LR    VALUEPTR,INPTR                                      0253
         SLR   @10,@10                                             0253
         IC    @10,MASKTAB                                         0253
         ALR   VALUEPTR,@10                                        0253
*           IF VALUEPTR->CHAR1=LPAREN THEN/* VALUES IN PARENT?       */
         CLC   CHAR1(1,VALUEPTR),LPAREN                            0254
         BNE   @RF00254                                            0254
*             DO;                                                  0255
*               INPTR=VALUEPTR+ONE; /* BUMP PAST LEFT PAREN          */
         LA    INPTR,1                                             0256
         ALR   INPTR,VALUEPTR                                      0256
*               CALL PARENSCN;      /* CHECK PARENS BALANCED         */
         BAL   @14,PARENSCN                                        0257
*               PARENSW=ONE;        /* SET PAREN SWITCH              */
         MVI   PARENSW,X'01'                                       0258
*               INPTR=VALUEPTR+ONE; /* RE-SET VALUE PTR PAST LPAREN  */
         LA    INPTR,1                                             0259
         ALR   INPTR,VALUEPTR                                      0259
*             END;                                                 0260
*           ELSE                                                   0261
*             DO;                                                  0261
         B     @RC00254                                            0261
@RF00254 DS    0H                                                  0262
*               INPTR=VALUEPTR;     /* RESET PTR TO OPERAND VALUE    */
         LR    INPTR,VALUEPTR                                      0262
*               PARENSW=ZERO;       /* SET SWITCH OFF                */
         MVI   PARENSW,X'00'                                       0263
*             END;                                                 0264
*           IF MSGCODE=ZERO THEN    /* PARENS BALANCED               */
@RC00254 CLI   MSGCODE,0                                           0265
         BNE   @RF00265                                            0265
*             DO;                                                  0266
*               IF PARENSW=ZERO THEN/* SINGLE VALUE EXPECTED         */
         CLI   PARENSW,0                                           0267
         BNE   @RF00267                                            0267
*                 DO;                                              0268
*                   REG15=ADDR(AUTHTAB);/* PTR TO VALID VALUE TABLE  */
         LA    REG15,AUTHTAB                                       0269
*                   CALL VALIDSCN;  /* CHECK FOR VALID INPUT         */
         BAL   @14,VALIDSCN                                        0270
*                   IF REG15^=ZERO THEN/* VALID INPUT FOUND          */
         LTR   REG15,REG15                                         0271
         BZ    @RF00271                                            0271
*                     DO;                                          0272
*                       REG15=ADDR(BITSTAB)+TABINDX;/* PTR TO AUTH'TY
*                                      BIT MASK                      */
         LA    REG15,BITSTAB                                       0273
         SLR   @10,@10                                             0273
         IC    @10,TABINDX                                         0273
         ALR   REG15,@10                                           0273
*                       AUTHMASK=APTR->CHAR1;/* INIT PARM LIST AREA  */
         L     @10,APTR(,REG15)                                    0274
         MVC   AUTHMASK(1),CHAR1(@10)                              0274
*                       INPTR=VALUEPTR+MASKTAB;/* RE-SET OPER. PTR   */
         LR    INPTR,VALUEPTR                                      0275
         SLR   @10,@10                                             0275
         IC    @10,MASKTAB                                         0275
         ALR   INPTR,@10                                           0275
*                     END;                                         0276
*                 END;                                             0277
*               ELSE                /* PARENTHESIED VALUES EXPECTED  */
*                 DO;                                              0278
         B     @RC00267                                            0278
@RF00267 DS    0H                                                  0279
*                   CALL PARMLOOP;  /* PROCESS VALUES                */
         BAL   @14,PARMLOOP                                        0279
*                   PARENSW=ZERO;   /* RE-SET PAREN INDICATOR        */
         MVI   PARENSW,X'00'                                       0280
*                 END;                                             0281
*             END;                                                 0282
*         END;                                                     0283
*       ELSE                                                       0284
*         MSGCODE=MSG29;            /* SET MSG CODE - IEE345I        */
         B     @RC00251                                            0284
@RF00251 MVC   MSGCODE(1),MSG29                                    0284
*     END;                                                         0285
@RC00251 DS    0H                                                  0286
*   IF MSGCODE=MSG3D THEN           /* INVALID OPERAND VALUE         */
@RC00246 CLC   MSGCODE(1),MSG3D                                    0286
         BNE   @RF00286                                            0286
*     VERBNAME=AUTHDATA;            /* OPERAND NAME TO MSG VARIABLE  */
         MVI   VERBNAME+5,C' '                                     0287
         MVC   VERBNAME+6(2),VERBNAME+5                            0287
         MVC   VERBNAME(5),AUTHDATA                                0287
*   REG14=SAVE2;                    /* RESTORE RETURN ADDRESS        */
@RF00286 L     REG14,SAVE2                                         0288
*   END AUTHRTN;                                                   0289
@EL00007 DS    0H                                                  0289
@EF00007 DS    0H                                                  0289
@ER00007 BR    @14                                                 0289
*PARMLOOP:                                                         0290
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0290
*                                                                  0290
PARMLOOP DS    0H                                                  0291
*   /*****************************************************************/
*   /*                                                               */
*   /* PROCESS THE PARENTHEZIED VALUES FOR 'AUTH=' OPERAND. AT ENTRY,*/
*   /* SCANPTR IS INITIALIZED WITH THE ADDRESS OF VALUES FOLLOWING   */
*   /* THE LEFT PAREN. SCAN FOR VALID OPERAND VALUE VIA-CALL TO VALID*/
*   /* SCN. . IF FOUND - REG15 NON-ZERO - SET PTR TO TABLE OF BIT    */
*   /* PATTERNS . AND 'OR' APPROPRIATE VALUES IN PARM LIST FIELD     */
*   /* AUTHMASK. . COMPARE FOR VALID DELIMITER - COMMA/RIGHT PAREN. .*/
*   /* IF NO DELIMITER FOUND, SET ERROR CODES AND RETURN TO CALLER . */
*   /* ELSE, IF DELIMITER = COMMA, RE-SET SCAN PTR (INPTR) AND .     */
*   /* CONTINUE SCAN. IF DELIMITER = RIGHT PAREN, END OF INPUT HAS . */
*   /* BEEN REACHED. SET LOOP INDICATOR OFF, RETURN TO CALLER.       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0291
*   SAVE3=REG14;                    /* SAVE RETURN ADDRESS           */
         ST    REG14,SAVE3                                         0291
*   SPINNER=ONE;                    /* SET UP LOOP INDICATOR         */
         MVI   SPINNER,X'01'                                       0292
*   DO WHILE(MSGCODE=ZERO&          /* NO ERROR CONDITIONS           */
*         SPINNER=ONE);             /* NOT END OF INPUT              */
         B     @DE00293                                            0293
@DL00293 DS    0H                                                  0294
*     REG15=ADDR(PARENPTR);         /* PTR TO VALID PAREN VALUES     */
         LA    REG15,PARENPTR                                      0294
*     CALL VALIDSCN;                /* CHECK FOR VALID VALUE         */
         BAL   @14,VALIDSCN                                        0295
*     IF REG15=ZERO THEN            /* INVALID VALUE                 */
         LTR   REG15,REG15                                         0296
         BNZ   @RF00296                                            0296
*       SPINNER=ZERO;               /* SET EXIT INDICATOR            */
         MVI   SPINNER,X'00'                                       0297
*     ELSE                          /* SCAN SUCCESSFUL               */
*       DO;                                                        0298
         B     @RC00296                                            0298
@RF00296 DS    0H                                                  0299
*         REG15=ADDR(PRNBADR)+TABINDX;/* PTR TO BIT PATTERN TABLE    */
         LA    REG15,PRNBADR                                       0299
         SLR   @10,@10                                             0299
         IC    @10,TABINDX                                         0299
         ALR   REG15,@10                                           0299
*         AUTHMASK=AUTHMASK×APTR->CHAR1;/* 'OR' IN BIT VALUES        */
         L     @10,APTR(,REG15)                                    0300
         SLR   @08,@08                                             0300
         IC    @08,AUTHMASK                                        0300
         SLR   @07,@07                                             0300
         IC    @07,CHAR1(,@10)                                     0300
         OR    @08,@07                                             0300
         STC   @08,AUTHMASK                                        0300
*         REG15=ADDR(PARENPTR)+TABINDX;/* PTR TO VALUE LENGTH        */
         LA    REG15,PARENPTR                                      0301
         SLR   @10,@10                                             0301
         IC    @10,TABINDX                                         0301
         ALR   REG15,@10                                           0301
*         INPTR=INPTR+APTR->CHAR1;  /* BUMP PAST OPERAND VALUE       */
         L     @10,APTR(,REG15)                                    0302
         SLR   @08,@08                                             0302
         IC    @08,CHAR1(,@10)                                     0302
         ALR   INPTR,@08                                           0302
*         IF INPTR->CHAR1^=COMMA&   /* COMMA DELIMITER?              */
*             INPTR->CHAR1^=RPAREN THEN/* END OF INPUT DELIMITER     */
         CLC   CHAR1(1,INPTR),COMMA                                0303
         BE    @RF00303                                            0303
         CLC   CHAR1(1,INPTR),RPAREN                               0303
         BE    @RF00303                                            0303
*           MSGCODE=MSG3D;          /* SET ERROR MSG - IEE708I       */
         MVC   MSGCODE(1),MSG3D                                    0304
*         ELSE                                                     0305
*           DO;                                                    0305
         B     @RC00303                                            0305
@RF00303 DS    0H                                                  0306
*             IF INPTR->CHAR1=RPAREN THEN/* CONTINUE SCAN ?          */
         CLC   CHAR1(1,INPTR),RPAREN                               0306
         BNE   @RF00306                                            0306
*               SPINNER=ZERO;       /* SET EXIT INDICATOR            */
         MVI   SPINNER,X'00'                                       0307
*             INPTR=INPTR+ONE;      /* BUMP TO NEXT DELIMITER, COMMA/
*                                      BLANK                         */
@RF00306 AL    INPTR,@CF00064                                      0308
*           END;                                                   0309
*       END;                                                       0310
@RC00303 DS    0H                                                  0311
*   END;                                                           0311
@RC00296 DS    0H                                                  0311
@DE00293 CLI   MSGCODE,0                                           0311
         BNE   @DC00293                                            0311
         CLI   SPINNER,1                                           0311
         BE    @DL00293                                            0311
@DC00293 DS    0H                                                  0312
*   REG14=SAVE3;                    /* RESTORE RETURN ADDRESS        */
         L     REG14,SAVE3                                         0312
*   END PARMLOOP;                                                  0313
@EL00008 DS    0H                                                  0313
@EF00008 DS    0H                                                  0313
@ER00008 BR    @14                                                 0313
*MSGRTN:                                                           0314
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0314
*                                                                  0314
MSGRTN   DS    0H                                                  0315
*   /*****************************************************************/
*   /*                                                               */
*   /* SET UP TO ISSUE MESSAGES VIA MODULE IEE0503D, WHICH MAPS ITS  */
*   /* INPUT TO THE XSA MAPPING MACRO. SET WORKAREA BASED ON XSA,    */
*   /* CLEAR CORE, AND INITIALIZE THE PARM LIST AND CALL THE MESSAGE */
*   /* MODULE. NO RETURN CODE IS EXPECTED - RETURN TO CALLER.        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0315
*   DCL                                                            0315
*     MSGMOD ENTRY BASED(REG15);    /* ENTRY TO ESTABLISH CALL     0315
*                                      CAPABILITY TO MSG MODULE ADDR */
*   SAVE1=REG14;                    /* SAVE RETURN ADDRESS           */
         ST    REG14,SAVE1                                         0316
*   REG2=ADDR(MSGAREA);             /* PTR TO PARM LIST              */
         LA    REG2,MSGAREA                                        0317
*   REG0=LENGTH(XASVRB);            /* LENGTH OF CORE TO CLEAR       */
         LA    REG0,48                                             0318
*   XSAMAP(1:REG0)=XSAMAP(1:REG0)&&XSAMAP(1:REG0);/* ZERO PARM CORE  */
         LR    @10,REG0                                            0319
         BCTR  @10,0                                               0319
         EX    @10,@SX01375                                        0319
*   XAE=MSGCODE;                    /* PTR TO APPROPRIATE MESSAGE    */
         MVC   XAE(1,XSAPTR),MSGCODE                               0320
*   XAV=VERBNAME;                   /* FILLER FOR MESSAGE            */
         MVC   XAV(8,XSAPTR),VERBNAME                              0321
*   IF CHUCMP=HEX80 THEN            /* COMMAND FROM INPUT STREAM     */
         L     @10,CSCBPTR                                         0322
         CLC   CHUCMP(1,@10),HEX80                                 0322
         BNE   @RF00322                                            0322
*     XAU=ZERO;                     /* SET MASTER CONSOLE ISSUER     */
         MVI   XAU(XSAPTR),X'00'                                   0323
*   ELSE                                                           0324
*     XAU=CHUCMP;                   /* CONSOLE ID OF ISSUER          */
         B     @RC00322                                            0324
@RF00322 L     @10,CSCBPTR                                         0324
         MVC   XAU(1,XSAPTR),CHUCMP(@10)                           0324
*   REG15=MSGMPTR;                  /* ADDRESS OF IEE0503D           */
@RC00322 L     REG15,MSGMPTR                                       0325
*   CALL MSGMOD;                    /* ISSUE WTO VIA IEE0503D        */
         BALR  @14,@15                                             0326
*   REG14=SAVE1;                    /* RESTORE RETURN ADDRESS        */
         L     REG14,SAVE1                                         0327
*   END MSGRTN;                                                    0328
@EL00009 DS    0H                                                  0328
@EF00009 DS    0H                                                  0328
@ER00009 BR    @14                                                 0328
*FREECSCB:                                                         0329
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0329
*                                                                  0329
FREECSCB DS    0H                                                  0330
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS RTN FREE THE CSCB VIA THE MGCR MACRO                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0330
*   REG1=CSCBPTR;                   /* SET PTR TO CSCB               */
         L     @10,CSCBPTR                                         0330
         LR    REG1,@10                                            0330
*   CHFC='1'B;                      /* SET FREE CSCB BIT ON          */
         OI    CHFC(@10),B'00000010'                               0331
*   GEN(MGCR (1),CHAIN);            /* ISSUE MACRO                   */
         MGCR (1),CHAIN
*   END FREECSCB;                                                  0333
@EL00010 DS    0H                                                  0333
@EF00010 DS    0H                                                  0333
@ER00010 BR    @14                                                 0333
*STAERTN:                                                          0334
*   PROC OPTIONS(NOSAVEAREA,NOSAVE);                               0334
*                                                                  0334
STAERTN  DS    0H                                                  0335
*   /*****************************************************************/
*   /*                                                               */
*   /* ENTRY HERE IS TO ESTABLISH AN ESTAE ENVIRONMENT FOR THIS      */
*   /* MODULE. A PARAMETER LIST IS INITIALIZED AND THE ESTAE MODULE  */
*   /* IEECB860 IS LOADED, CONTROL IS PASSED TO IT WITH REG1 PTR TO  */
*   /* THE PARM LIST. ON RETURN, RETURN CODE - REG15- IS SAVED.      */
*   /* IEECB860 IS DELETED, AND RETURN TO CALLER WITH REG15 RETURN   */
*   /* CODE.                                                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0335
*   DCL                                                            0335
*     1 STAEPARM BASED(REG1),                                      0335
*                                                                  0335
*     /***************************************************************/
*     /*                                                             */
*     /* THIS IS A MAPPING OF THE PARAMETER AREA PASSED TO IEECB860  */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0335
*      2 CSCBADDR PTR(31),          /* CSCB PTR                      */
*      2 CMNDPTR PTR(31),           /* PTR TO COMMAND, LENGTH        */
*      2 CMNDBUF,                                                  0335
*       3 CMNDSIZE CHAR(1),         /* COMMAND LENGTH                */
*       3 CMNDWORD CHAR(7),         /* COMMAND WORD                  */
*      2 SAVEPTRS,                                                 0335
*       3 REG11SAV PTR(31),         /* REG11 SAVE PTR                */
*       3 REG12SAV PTR(31);         /* REG12 SAVE PTR                */
*   DCL                                                            0336
*     STAEMOD ENTRY BASED(REG15);   /* ENTRY TO ESTABLISH A CALL   0336
*                                      CAPABILITY TO ESTAE MODULE    */
*   SAVE1=REG14;                    /* SAVE RETURN ADDRESS           */
         ST    REG14,SAVE1                                         0337
*   REG1=ADDR(MSGAREA);             /* PTR TO PARM AREA CORE         */
         LA    REG1,MSGAREA                                        0338
*   CSCBADDR=CSCBPTR;               /* PTR TO CSCB                   */
         L     @10,CSCBPTR                                         0339
         ST    @10,CSCBADDR(,REG1)                                 0339
*   CMNDPTR=ADDR(CMNDBUF);          /* PTR TO COMMAND, LENGTH        */
         LA    @10,CMNDBUF(,REG1)                                  0340
         ST    @10,CMNDPTR(,REG1)                                  0340
*   CMNDSIZE=FOUR;                  /* SET COMMAND LENGTH            */
         MVI   CMNDSIZE(REG1),X'04'                                0341
*   CMNDWORD=VARYC;                 /* INIT COMMAND WORD             */
         MVC   CMNDWORD(7,REG1),VARYC                              0342
*   REG11SAV=REG11;                 /* SAVE BASE REG                 */
         ST    REG11,REG11SAV(,REG1)                               0343
*   REG12SAV=REG12;                 /* SAVE DATA REG                 */
         ST    REG12,REG12SAV(,REG1)                               0344
*   REG2=REG1;                      /* SAVE PARM PTR                 */
         LR    REG2,REG1                                           0345
*   GEN( LOAD EP=IEECB860);         /* BRING IN ESTAE RTN            */
          LOAD EP=IEECB860
*   REG15=REG0;                     /* PTR TO IEECB860               */
         LR    REG15,REG0                                          0347
*   REG1=REG2;                      /* RESTORE PARM PTR              */
         LR    REG1,REG2                                           0348
*   CALL STAEMOD;                   /* CALL ESTAE RTN                */
         BALR  @14,@15                                             0349
*   REG11=REG11SAV;                 /* RESTORE                       */
         L     REG11,REG11SAV(,REG1)                               0350
*   REG12=REG12SAV;                 /* REGISTERS                     */
         L     REG12,REG12SAV(,REG1)                               0351
