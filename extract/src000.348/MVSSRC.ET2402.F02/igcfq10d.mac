         TM    OCREDRVE(ROPCE),B'00010000'                      01S0838
         BNO   @RF00838                                         01S0838
*         DO;                                                   01S0839
*                                                               01S0839
*           /*********************************************************/
*           /*                                                       */
*           /* A REDRIVE OPCE HAS BEEN CREATED FOR COPY DEVICE       */
*           /* SESSION INFORMATION COMMANDS. OBTAIN AS MANY UNITS AS */
*           /* ALLOWABLE UP TO THE NUMBER OF UNITS NEEDED TO SEND OUT*/
*           /* ALL COMMANDS AND THEN ENTER REDRIVE PROCESSING WHICH  */
*           /* BUILDS COPY DEVICE SESSION INFORMATION OPCE- PACKED RU*/
*           /* PAIRS FOR EACH RESOURCE.                              */
*           /*                                                       */
*           /*********************************************************/
*                                                               01S0840
*                                                               02S0840
*           /*********************************************************/
*           /*                                                       */
*           /* OBTAIN AS MANY BUFFER UNITS AS THE OPCE NEEDS UP TO   */
*           /* 50% OF TCAM'S UNITS. CHAIN THEM OFF OCUNIT FIELD OF   */
*           /* THE OPCE.                                             */
*           /*                                                       */
*           /*********************************************************/
*                                                               02S0840
*           UNITPTR=ROPCE;          /* SET TO POINT TO R/D OPCE      */
         LR    UNITPTR,ROPCE                                    02S0840
*           IF OPCADGOT+OCBUFNED>HALFUNIT THEN/* IF TOTAL OF UNITS
*                                      A/D HAS AND UNITS NEEDED 02S0841
*                                      EXCEEDS THE 50% LEVEL         */
         LH    @14,HALFUNIT                                     02S0841
         SLR   @11,@11                                          02S0841
         ICM   @11,3,OPCADGOT(ROPCAVT)                          02S0841
         SLR   @09,@09                                          02S0841
         ICM   @09,3,OCBUFNED(ROPCE)                            02S0841
         ALR   @09,@11                                          02S0841
         CR    @09,@14                                          02S0841
         BNH   @RF00841                                         02S0841
*             UNITSGET=HALFUNIT-OPCADGOT;/* GET UNITS UP TO 50% LEVEL*/
         SLR   @14,@11                                          02S0842
         STH   @14,UNITSGET                                     02S0842
*           ELSE                                                02S0843
*             UNITSGET=OCBUFNED;    /* GET AS MANY UNITS AS NEEDED   */
         B     @RC00841                                         02S0843
@RF00841 MVC   UNITSGET(2),OCBUFNED(ROPCE)                      02S0843
*           DO WHILE(UNITSGET^=0);  /* GO TO ALLOCBUF AS MANY TIMES
*                                      AS NECESSAY TO OBTAIN UNITS   */
@RC00841 B     @DE00844                                         02S0844
@DL00844 DS    0H                                               02S0845
*             IF UNITSGET>MAXUNTGT THEN/* IF FK IS TO GET MORE THAN
*                                      THE # OF UNITS ALLOCBUF CAN
*                                      GIVE PER CALL                 */
         LA    @14,255                                          02S0845
         CH    @14,UNITSGET                                     02S0845
         BNL   @RF00845                                         02S0845
*               RPARM=MAXUNTGT;     /* GET THE MAXIMUM               */
         LR    RPARM,@14                                        02S0846
*             ELSE                                              02S0847
*               RPARM=UNITSGET;     /* GET # TO GET                  */
         B     @RC00845                                         02S0847
@RF00845 LH    RPARM,UNITSGET                                   02S0847
*             CALL ALLOCBUF;        /* GET BUFFER UNITS              */
*                                                               02S0848
@RC00845 L     @15,OPCGETBF(,ROPCAVT)                           02S0848
         BALR  @14,@15                                          02S0848
*             /*******************************************************/
*             /*                                                     */
*             /* LINK UNITS JUST GOTTEN OFF OF OCLINK AT END OF      */
*             /* CURRENT CHAIN OF BUFFERS                            */
*             /*                                                     */
*             /*******************************************************/
*                                                               02S0849
*             RFY                                               02S0849
*               IEDQOPCE BASED(UNITPTR);                        02S0849
*             RFY                                               02S0850
*               TICFLD BASED(ADDR(OCUNIT));                     02S0850
*             DO WHILE(TIC=VALTIC); /* LOOP TO FIND END OF CURRENT
*                                      CHAIN                         */
         B     @DE00851                                         02S0851
@DL00851 DS    0H                                               02S0852
*               UNITPTR=OCUNIT;     /* POINT TO NEXT UNIT            */
         L     UNITPTR,OCUNIT(,UNITPTR)                         02S0852
*             END;                                              02S0853
@DE00851 TM    TIC+8(UNITPTR),B'00000011'                       02S0853
         BZ    @DL00851                                         02S0853
*             OCUNIT=RPARM;         /* POINT TO UNITS JUST GOTTEN    */
         ST    RPARM,OCUNIT(,UNITPTR)                           02S0854
*             RFY                                               02S0855
*               TICFLD BASED;                                   02S0855
*             RFY                                               02S0856
*               IEDQOPCE BASED(ROPCE);                          02S0856
*             UNITSGET=UNITSGET-RZERO;/* UPDATE # OF UNITS TO GET    */
         LH    @14,UNITSGET                                     02S0857
         SLR   @14,RZERO                                        02S0857
         STH   @14,UNITSGET                                     02S0857
*             OCBUFNED=OCBUFNED-RZERO;/* UPDATE # OF UNITS OPCE NEEDS*/
         SLR   @14,@14                                          02S0858
         ICM   @14,3,OCBUFNED(ROPCE)                            02S0858
         SLR   @14,RZERO                                        02S0858
         STH   @14,OCBUFNED(,ROPCE)                             02S0858
*             OCBUFGOT=OCBUFGOT+RZERO;/* UPDATE # OF UNITS OPCE HAS  */
         SLR   @14,@14                                          02S0859
         ICM   @14,3,OCBUFGOT(ROPCE)                            02S0859
         ALR   @14,RZERO                                        02S0859
         STH   @14,OCBUFGOT(,ROPCE)                             02S0859
*             OPCADGOT=OPCADGOT+RZERO;/* UPDATE OPCADGOT             */
         SLR   @14,@14                                          02S0860
         ICM   @14,3,OPCADGOT(ROPCAVT)                          02S0860
         ALR   @14,RZERO                                        02S0860
         STH   @14,OPCADGOT(,ROPCAVT)                           02S0860
*           END;                                                02S0861
@DE00844 ICM   @14,3,UNITSGET                                   02S0861
         BNZ   @DL00844                                         02S0861
*           CALL REDRIVE;                                       01S0862
         BAL   @14,REDRIVE                                      01S0862
*         END;                                                  01S0863
*       ELSE                                                    01S0864
*         ;                         /* EITHER OPCE IS A ACTLINK 01S0864
*                                      REQUEST OPCE OR IS A REPLACE
*                                      DSI COMMAND OPCE. IN EITHER
*                                      CASE, EXIT.                   */
@RF00838 DS    0H                                               01S0865
*     END;                                                      01S0865
*                                                               01S0865
*   /*****************************************************************/
*   /*                                                               */
*   /* EXIT CODE                                                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0866
*   IF OPCMODID=CNTLMOD THEN        /* IF XCTL IS TO IGC0010D        */
@RC00634 CLC   OPCMODID(2,ROPCAVT),CNTLMOD                      01S0866
         BNE   @RF00866                                         01S0866
*     RZERO=''B;                    /* CLEAR                         */
         SLR   RZERO,RZERO                                      01S0867
*   RPARM=ROPCAVT;                                              01S0868
@RF00866 LR    RPARM,ROPCAVT                                    01S0868
*   DO;                             /* IEDXCTL                       */
*     GEN EXIT REFS(ROPCAVT,OPCTRACE,OPCXCTL) NOSEQFLOW;        01S0870
*                                   /*
         LA    15,OPCXCTL(,ROPCAVT)   POINT TO OPCXCTL LIST
         EX    0,OPCTRACE(,ROPCAVT)    INVOKE XCTL TRACE
         XCTL  SF=(E,(15))             XCTL TO NEXT MODULE
*   END;                            /* END OF IEDXCTL                */
*/* @OZ35627                                                         */
