         TITLE 'IGCFQ10D - COPY/REPLACE DEVICE SESSION INFO.'
IGCFQ10D CSECT ,                                                01S0001
@PROLOG  BALR  @12,0                                            01S0001
@PSTART  DS    0H                                               01S0001
         USING @PSTART,@12                                      01S0001
*                                   /*
IGCFQ10D IEDHJN AFTERHJN,HJN
*                                                               01S0624
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZATION                                                */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0624
*   ROPCAVT=RPARM;                  /* SET POINTER TO OP. CTL. AVT   */
         LR    ROPCAVT,RPARM                                    01S0624
*   ROPCE=OPCCOPCE;                 /* SET POINTER TO CURRENT OPCE   */
         L     ROPCE,OPCCOPCE(,ROPCAVT)                         01S0625
*   RAVT=OPCAVTPT;                  /* SET POINTER TO TCAM AVT       */
         L     RAVT,OPCAVTPT(,ROPCAVT)                          01S0626
*   RSAVE=ADDR(OPCSAVE);            /* SET POINTER TO SAVE AREA      */
         LA    RSAVE,OPCSAVE(,ROPCAVT)                          01S0627
*   RWRKAREA=OPCWORK;               /* OP. CTL. WORKAREA BASE - USED
*                                      FOR TEMPORARY STORAGE         */
*                                                               01S0628
         L     RWRKAREA,OPCWORK(,ROPCAVT)                       01S0628
*   /*****************************************************************/
*   /*                                                               */
*   /* BASE COMPILER GENERATED DSECT (CREATED BY REENTRANT OPTION)   */
*   /* FOR TEMPORARY STORAGE ON WORK AREA BASE.                      */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0629
*   GEN REFS(RWRKAREA);                                         01S0629
*                        /*
         USING @DATD,RWRKAREA    */
*   FLAGS=''B;                      /* INIT FLAGS FIELD              */
*                                                               01S0630
         MVI   FLAGS,X'00'                                      01S0630
*   /*****************************************************************/
*   /*                                                               */
*   /* CALCULATE 50% OF TCAM'S LINE UNITS                            */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0631
*   HALFUNIT=AVTNOLBF;              /* # OF TCAM'S LINE UNITS        */
         LH    @14,AVTNOLBF(,RAVT)                              01S0631
         STH   @14,HALFUNIT                                     01S0631
*   SRL(HALFUNIT,1);                /* DIVIDE BY TWO FOR 50%         */
*                                                               01S0632
         SRL   @14,1                                            01S0632
         STH   @14,HALFUNIT                                     01S0632
*   /*****************************************************************/
*   /*                                                               */
*   /* OBTAIN ADDR. OF NEG. PREFIX OF ORIG. OPCE                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0633
*   ORGNOPCE=OCPTRSAV-LENGTH(IEDQOPCN);/* OBTAIN THE ADDR. OF THE
*                                      START OF THE NEG. PREFIX IN
*                                      THE ORIG. OPCE                */
         L     ORGNOPCE,OCPTRSAV(,ROPCE)                        01S0633
         SH    ORGNOPCE,@CH00045                                01S0633
*                                                               01S0634
*   /*****************************************************************/
*   /*                                                               */
*   /* MAIN PROCDURE                                                 */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0634
*   IF OCREDRVE=ON THEN             /* IF THIS IS A REDRIVE OPCE     */
*                                                               01S0634
         TM    OCREDRVE(ROPCE),B'00010000'                      01S0634
         BNO   @RF00634                                         01S0634
*     /***************************************************************/
*     /*                                                             */
*     /* THIS IS A REDRIVE OPCE WITH ATTACHED UNITS.                 */
*     /*                                                             */
*     /***************************************************************/
*                                                               01S0635
*     CALL REDRIVE;                                             01S0635
         BAL   @14,REDRIVE                                      01S0635
*   ELSE                                                        01S0636
*     DO;                                                       01S0636
*                                                               01S0636
         B     @RC00634                                         01S0636
@RF00634 DS    0H                                               01S0637
*       /*************************************************************/
*       /*                                                           */
*       /* THIS IS A RESPONSE OPCE OR A REQUEST FROM IGCFL10D.       */
*       /*                                                           */
*       /*************************************************************/
*                                                               01S0637
*       ORGNOPCE->OCRESPCT=ORGNOPCE->OCRESPCT-1;/* DECREMENT RSP. CT.*/
         SLR   @14,@14                                          01S0637
         ICM   @14,3,OCRESPCT(ORGNOPCE)                         01S0637
         BCTR  @14,0                                            01S0637
         STH   @14,OCRESPCT(,ORGNOPCE)                          01S0637
*                                                               02S0638
*       /*************************************************************/
*       /*                                                           */
*       /* ---------- THIS SEGMENT PROCESSES RESPONSES --------      */
*       /*                                                           */
*       /*************************************************************/
*                                                               02S0638
*       IF OCSWITCH=OCRPLDSI THEN   /* IF REPLACE RESPONSE           */
         CLI   OCSWITCH(ROPCE),X'E4'                            02S0638
         BNE   @RF00638                                         02S0638
*         FREUTFLG=ON;              /* SET FLAG TO FREE UNITS        */
         OI    FREUTFLG,B'10000000'                             02S0639
*       ELSE                                                    02S0640
*         IF OCSWITCH=OCOPYDSI THEN /* IF COPY RESPONSE              */
         B     @RC00638                                         02S0640
@RF00638 CLI   OCSWITCH(ROPCE),X'E3'                            02S0640
         BNE   @RF00640                                         02S0640
*           DO;                                                 02S0641
*                                                               02S0641
*             /*******************************************************/
*             /*                                                     */
*             /* DETERMINE IF REPLACE SHOULD BE DONE.                */
*             /*                                                     */
*             /*******************************************************/
*                                                               02S0642
*                                                               03S0642
*             /*******************************************************/
*             /*                                                     */
*             /* --- THIS SEGMENT PROCESSES "COPY DEVICE SESSION     */
*             /* INFO." RESPONSES                                    */
*             /*                                                     */
*             /*******************************************************/
*                                                               03S0642
*             RFY                                               03S0642
*               IEDDSI1 BASED(ADDR(PRUDATA)+LENGTH(IEDBDU));    03S0642
*             RFY                                               03S0643
*               IEDQTRM BASED(RPARM);                           03S0643
*             IF BDUSYRSP=GOODRSP THEN/* IF GOOD RESPONSE ON COPY    */
         L     @14,OCUNIT(,ROPCE)                               03S0644
         CLI   BDUSYRSP+12(@14),X'60'                           03S0644
         BNE   @RF00644                                         03S0644
*               DO;                                             03S0645
*                 RPARM=OCTRMTBL;   /* OBTAIN TTE .....              */
         SLR   RPARM,RPARM                                      03S0646
         ICM   RPARM,3,OCTRMTBL(ROPCE)                          03S0646
*                 CALL IEDQTNT;     /* .... ADDR.                    */
         L     @15,AVTRNMPT(,RAVT)                              03S0647
         BALR  @14,@15                                          03S0647
*                 RZERO=TRMDDIGA;   /* OBTAIN ADDR. .....            */
         LH    RZERO,@CH02076                                   03S0648
*                 CALL IEDQTL;      /* .... OF DEVICE DEPENDENT FIELD*/
         L     @15,AVTDDFT(,RAVT)                               03S0649
         BALR  @14,@15                                          03S0649
*                 DEVDPADR=RTNCODE; /* ADDR. OF DIAL DIGITS FIELD    */
         LR    DEVDPADR,RTNCODE                                 03S0650
*                                                               04S0651
*                 /***************************************************/
*                 /*                                                 */
*                 /* ------ PLACE RU DIAL DATA IN CONTIGUOUS STORAGE */
*                 /* IN OP. CTL. WORK AREA. SAVE POINTERS TO THE UNIT*/
*                 /* ADDR. AND STARTING ADDR. OF DIAL DATA IN RU FOR */
*                 /* MOVE BACK.-------                               */
*                 /*                                                 */
*                 /***************************************************/
*                                                               04S0651
*                 RFY                                           04S0651
*                   IEDDSI1 BASED(PLMVSD);/* BASE ON PARM LIST SOURCE
*                                      DATA                          */
*                 RFY                                           04S0652
*                   IEDDSI2 BASED(PLMVSD);/* ADDR.                   */
*                 RFY                                           04S0653
*                   IEDDSI3 BASED(PLMVSD);                      04S0653
*                 RFY                                           04S0654
*                   IEDDSI4 BASED(PLMVSD);                      04S0654
*                                                               04S0654
*                 /***************************************************/
*                 /*                                                 */
*                 /* INIT. PARM. LIST FOR IEDMVBFR. THIS ROUTINE IS  */
*                 /* USED TO FIND ADDR. OF DIAL DATA BY MOVING BYTES */
*                 /* INTO THEMSELVES THUS MOVING POINTER.            */
*                 /*                                                 */
*                 /***************************************************/
*                                                               04S0655
*                 IEDPLMV=''B;      /* CLEAR PARM. LIST AREA         */
         LA    @14,PARMLSP                                      04S0655
         XC    IEDPLMV(24,@14),IEDPLMV(@14)                     04S0655
*                 PLMVSU=OCUNIT;    /* SOURCE UNIT ADDR.             */
         L     @05,OCUNIT(,ROPCE)                               04S0656
         ST    @05,PLMVSU(,@14)                                 04S0656
*                 PLMVTU=OCUNIT;    /* TARGET UNIT ADDR.             */
         ST    @05,PLMVTU(,@14)                                 04S0657
*                 PLMVSD=ADDR(PRUDATA)+LENGTH(IEDBDU);/* SOURCE DATA
*                                      ADDR.                         */
         LA    @05,PRUDATA(,@05)                                04S0658
         AH    @05,@CH00080                                     04S0658
         ST    @05,PLMVSD(,@14)                                 04S0658
*                 PLMVTD=ADDR(PRUDATA)+LENGTH(IEDBDU);/* TARGET DATA
*                                      ADDR.                         */
         ST    @05,PLMVTD(,@14)                                 04S0659
*                 PLMVLNTH=DSIPOLNM+LENGTH(IEDDSI1);/* NUMBER OF
*                                      POLLING CHARACTERS            */
*                                                               04S0660
         LA    @07,4                                            04S0660
         SLR   @06,@06                                          04S0660
         IC    @06,DSIPOLNM(,@05)                               04S0660
         ALR   @07,@06                                          04S0660
         STH   @07,PLMVLNTH(,@14)                               04S0660
*                 /***************************************************/
*                 /*                                                 */
*                 /* LOCATE START OF ADDR. CHARS. FIELD              */
*                 /*                                                 */
*                 /***************************************************/
*                                                               04S0661
*                 RPARM=ADDR(PARMLSP);/* ADDR. OF PARM LIST  @YM05982*/
         LR    RPARM,@14                                        04S0661
*                 CALL IEDMVBFR;                                04S0662
         L     @05,AVTSAVTP(,RAVT)                              04S0662
         L     @15,SAVTMVBF(,@05)                               04S0662
         BALR  @14,@15                                          04S0662
*                 PLMVLNTH=DSIADRNM+LENGTH(IEDDSI2);/* NUMBER OF
*                                      ADDRESSING CHARACTERS         */
*                                                               04S0663
         LA    @14,PARMLSP                                      04S0663
         L     @05,PLMVSD(,@14)                                 04S0663
         LA    @07,1                                            04S0663
         SLR   @06,@06                                          04S0663
         IC    @06,DSIADRNM(,@05)                               04S0663
         ALR   @07,@06                                          04S0663
         STH   @07,PLMVLNTH(,@14)                               04S0663
*                 /***************************************************/
*                 /*                                                 */
*                 /* LOCATE INPUT IDENTIFIER FIELD.                  */
*                 /*                                                 */
*                 /***************************************************/
*                                                               04S0664
*                 CALL IEDMVBFR;                                04S0664
*                                                               04S0664
         L     @05,AVTSAVTP(,RAVT)                              04S0664
         L     @15,SAVTMVBF(,@05)                               04S0664
         BALR  @14,@15                                          04S0664
*                 /***************************************************/
*                 /*                                                 */
*                 /* IF INPUT ID IS 0 REMAINING FIELDS IN IEDDSI3 ARE*/
*                 /* NOT PRESENT                                     */
*                 /*                                                 */
*                 /***************************************************/
*                                                               04S0665
*                 IF DSINPTID='00'X THEN/* IF 0                      */
         LA    @14,PARMLSP                                      04S0665
         L     @14,PLMVSD(,@14)                                 04S0665
         CLI   DSINPTID(@14),X'00'                              04S0665
         BNE   @RF00665                                         04S0665
*                   FREUTFLG=ON;    /* THEN SET FLAG         @YM07670*/
         OI    FREUTFLG,B'10000000'                             04S0666
*                 ELSE                                          04S0667
*                   DO;             /*                       @YM07430*/
         B     @RC00665                                         04S0667
@RF00665 DS    0H                                               04S0668
*                     PLMVLNTH=LENGTH(IEDDSI3);/* MOVE LENGTH OF
*                                      IEDDSI3                       */
*                                                               04S0668
         LA    @14,PARMLSP                                      04S0668
         MVC   PLMVLNTH(2,@14),@CH00256                         04S0668
*                     /***********************************************/
*                     /*                                             */
*                     /* LOCATE BEGINNING OF DIAL DATA FIELDS        */
*                     /*                                             */
*                     /***********************************************/
*                                                               04S0669
*                     CALL IEDMVBFR;                            04S0669
*                                                               04S0669
         L     @05,AVTSAVTP(,RAVT)                              04S0669
         L     @15,SAVTMVBF(,@05)                               04S0669
         BALR  @14,@15                                          04S0669
*                     /***********************************************/
*                     /*                                             */
*                     /* SAVE POINTERS FOR UNIT AND START OF DIAL    */
*                     /* DATA                                        */
*                     /*                                             */
*                     /***********************************************/
*                                                               04S0670
*                     UNITSAVE=PLMVTU;                          04S0670
         LA    @14,PARMLSP                                      04S0670
         MVC   UNITSAVE(4),PLMVTU(@14)                          04S0670
*                     DATASAVE=PLMVTD;                          04S0671
*                                                               04S0671
         MVC   DATASAVE(4),PLMVTD(@14)                          04S0671
*                     /***********************************************/
*                     /*                                             */
*                     /* MOVE DIAL DATA INTO CONTIGUOUS STORAGE IN   */
*                     /* WORK AREA.                                  */
*                     /*                                             */
*                     /***********************************************/
*                                                               04S0672
*                     PLMVTU=ADDR(DIALDATA);/* ADDR. IN WRK AREA     */
         LA    @05,DIALSP                                       04S0672
         ST    @05,PLMVTU(,@14)                                 04S0672
*                     PLMVTD=ADDR(DIALDATA);/* ADDR. IN WRK AREA     */
         ST    @05,PLMVTD(,@14)                                 04S0673
*                     PLMVTCTG=ON;  /* FLAG FOR CONTIGUOUS TARGET    */
         OI    PLMVTCTG(@14),B'00100000'                        04S0674
*                     PLMVLNTH=DSIDLCNT+LENCTFLD;/* LENGTH OF DIAL
*                                      DATA                          */
         L     @05,PLMVSD(,@14)                                 04S0675
         LA    @07,1                                            04S0675
         SLR   @06,@06                                          04S0675
         IC    @06,DSIDLCNT(,@05)                               04S0675
         ALR   @07,@06                                          04S0675
         STH   @07,PLMVLNTH(,@14)                               04S0675
*                     CALL IEDMVBFR;                            04S0676
         L     @05,AVTSAVTP(,RAVT)                              04S0676
         L     @15,SAVTMVBF(,@05)                               04S0676
         BALR  @14,@15                                          04S0676
*                     RFY                                       04S0677
*                       IEDDSI1 BASED(ADDR(PRUDATA)+LENGTH(IEDBDU));
*                                   /* RESET BASE FOR START OF DATA
*                                      RECORD                        */
*                   END;            /*                       @YM07430*/
*                                   /* ... WORK AREA FOR EASY   03S0679
*                                      HANDLING ADDR. OF DIAL FIELD
*                                      IN RU PLACED IN RUDIALAD      */
*                 IF FREUTFLG=OFF THEN/*                     @YM07670*/
@RC00665 TM    FREUTFLG,B'10000000'                             03S0679
         BNZ   @RF00679                                         03S0679
*                   DO;             /*                       @YM07430*/
*                     IF TTEDIALN>MAXIMUM THEN/* IF TTE DIAL DATA >
*                                      MAX. ALLOWED                  */
         LA    @14,DIALSP                                       03S0681
         CLC   TTEDIALN(1,DEVDPADR),MAXIMUM(@14)                03S0681
         BNH   @RF00681                                         03S0681
*                       FREUTFLG=ON;/* SET TO FREE OPCE AND RU       */
         OI    FREUTFLG,B'10000000'                             03S0682
*                     ELSE                                      03S0683
*                                                               03S0683
*                       /*********************************************/
*                       /*                                           */
*                       /* REPLACE CAN BE DONE IF DIAL DIGITS DO NOT */
*                       /* MATCH.                                    */
*                       /*                                           */
*                       /*********************************************/
*                                                               03S0683
*                       IF TTEDIALN=ACTUAL THEN/* IF NUMBER OF TTE
*                                      DIAL DIGITS IS SAME AS NO. OF
*                                      RU DIGITS                     */
         B     @RC00681                                         03S0683
@RF00681 LA    @14,DIALSP                                       03S0683
         CLC   TTEDIALN(1,DEVDPADR),ACTUAL(@14)                 03S0683
         BNE   @RF00683                                         03S0683
*                         DO;       /* CONVERT NCP'S DIGITS TO BINARY
*                                      AND COMPARE WITH TCAM'S  03S0684
*                                                            @YM06517*/
*                           DIGITS(1:1)=DIGITS(1:1)&CLEARHI;/*  03S0685
*                                      CONVERT FIRST BYTE    @YM06517*/
         NI    DIGITS(@14),X'0F'                                03S0685
*                           IF ACTUAL>1 THEN/* IF MORE THAN 1 DIGIT
*                                                            @YM06875*/
         CLI   ACTUAL(@14),1                                    03S0686
         BNH   @RF00686                                         03S0686
*                             MVZ(DIGITS(2:ACTUAL),DIGITS(1:ACTUAL-1));
         LA    @05,DIALSP                                       03S0687
         SLR   @07,@07                                          03S0687
         IC    @07,ACTUAL(,@05)                                 03S0687
         BCTR  @07,0                                            03S0687
         BCTR  @07,0                                            03S0687
         EX    @07,@SB02300                                     03S0687
*                                   /* CONVERT REST OF DIGITS   03S0688
*                                                            @YM06517*/
*                           ELSE                                03S0688
*                             ;     /*                       @YM06875*/
@RF00686 DS    0H                                               03S0689
*                           IF TTEDIALD(1:TTEDIALN)=DIGITS(1:TTEDIALN)
*                             THEN  /* IF MATCH                      */
         LA    @14,DIALSP                                       03S0689
         SLR   @05,@05                                          03S0689
         IC    @05,TTEDIALN(,DEVDPADR)                          03S0689
         BCTR  @05,0                                            03S0689
         EX    @05,@SC02302                                     03S0689
         BNE   @RF00689                                         03S0689
*                             FREUTFLG=ON;/* REPLACE DOES NOT NEED TO
*                                      BE DONE - FREE OPCE AND RU    */
         OI    FREUTFLG,B'10000000'                             03S0690
*                           ELSE                                03S0691
*                             BLRPLFLG=ON;/* SET FLAG TO BUILD  03S0691
*                                      REPLACE                       */
         B     @RC00689                                         03S0691
@RF00689 OI    BLRPLFLG,B'01000000'                             03S0691
*                         END;      /*                       @YM06517*/
*                       ELSE                                    03S0693
*                         DO;                                   03S0693
         B     @RC00683                                         03S0693
@RF00683 DS    0H                                               03S0694
*                           RFY                                 03S0694
*                             RTNCODE UNRSTD;                   03S0694
*                           RFY                                 03S0695
*                             IEDPRUQD BASED(UNITSAVE);         03S0695
*                           RFY                                 03S0696
*                             TICFLD BASED(ADDR(PRUTIC));       03S0696
*                                                               03S0696
*                           /*****************************************/
*                           /*                                       */
*                           /* IF THE NUMBER OF DIAL DIGITS IN THE   */
*                           /* TTE IS TOO LARGE FOR CURRENT RU, GET  */
*                           /* ANOTHER UNIT.                         */
*                           /*                                       */
*                           /*****************************************/
*                                                               03S0697
*                           IF TIC^=VALTIC THEN/* IF START OF DIAL
*                                      DATA IS IN THE LAST UNIT      */
         L     @05,UNITSAVE                                     03S0697
         TM    TIC+8(@05),B'00000011'                           03S0697
         BZ    @RF00697                                         03S0697
*                             IF(DATASAVE-UNITSAVE)+(COUNT+1)+(TTEDIALN
*                                 -ACTUAL)>(AVTKEYLE+12) THEN/* IS
*                                      ANOTHER UNIT NEEDED           */
         LCR   @05,@05                                          03S0698
         AL    @05,DATASAVE                                     03S0698
         LA    @15,1                                            03S0698
         LA    @14,DIALSP                                       03S0698
         SLR   @07,@07                                          03S0698
         IC    @07,COUNT(,@14)                                  03S0698
         ALR   @07,@15                                          03S0698
         ALR   @05,@07                                          03S0698
         SLR   @07,@07                                          03S0698
         IC    @07,TTEDIALN(,DEVDPADR)                          03S0698
         SLR   @06,@06                                          03S0698
         IC    @06,ACTUAL(,@14)                                 03S0698
         SLR   @07,@06                                          03S0698
         ALR   @05,@07                                          03S0698
         LA    @14,12                                           03S0698
         AH    @14,AVTKEYLE(,RAVT)                              03S0698
         CR    @05,@14                                          03S0698
         BNH   @RF00698                                         03S0698
*                               DO;                             03S0699
*                                 RPARM=1;/* GET ANOTHER ....        */
         LR    RPARM,@15                                        03S0700
*                                 CALL ALLOCBUF;/* .... UNIT         */
         L     @15,OPCGETBF(,ROPCAVT)                           03S0701
         BALR  @14,@15                                          03S0701
*                                 PRUNUNIT=RPARM;/* SET TIC FIELD OF
*                                      LAST UNIT TO POINT TO NEW UNIT*/
         L     @05,UNITSAVE                                     03S0702
         STCM  RPARM,7,PRUNUNIT(@05)                            03S0702
*                               END;                            03S0703
*                             ELSE                              03S0704
*                               ;                               03S0704
@RF00698 DS    0H                                               03S0705
*                           ELSE                                03S0705
*                             ;                                 03S0705
@RF00697 DS    0H                                               03S0706
*                           RFY                                 03S0706
*                             TICFLD BASED;                     03S0706
*                           RFY                                 03S0707
*                             IEDPRUQD BASED(OCUNIT);           03S0707
*                                                               03S0707
*                           /*****************************************/
*                           /*                                       */
*                           /* RESET COUNT FIELDS FOR PACKED RU AND  */
*                           /* GO BUILD REPLACE DSI                  */
*                           /*                                       */
*                           /*****************************************/
*                                                               03S0708
*                           PRUDATCT=PRUDATCT+(TTEDIALN-ACTUAL);/*
*                                      DATA COUNT IN PACKED RU       */
         L     @05,OCUNIT(,ROPCE)                               03S0708
         LA    @15,DIALSP                                       03S0708
         SLR   @14,@14                                          03S0708
         IC    @14,TTEDIALN(,DEVDPADR)                          03S0708
         SLR   @07,@07                                          03S0708
         IC    @07,ACTUAL(,@15)                                 03S0708
         SLR   @14,@07                                          03S0708
         LH    @07,PRUDATCT(,@05)                               03S0708
         ALR   @07,@14                                          03S0708
         STH   @07,PRUDATCT(,@05)                               03S0708
*                           DSITOTNM=DSITOTNM+(TTEDIALN-ACTUAL);/*
*                                      COUNT IN DATA RECORD          */
         LA    @05,PRUDATA(,@05)                                03S0709
         AH    @05,@CH00080                                     03S0709
         SLR   @07,@07                                          03S0709
         IC    @07,DSITOTNM(,@05)                               03S0709
         ALR   @07,@14                                          03S0709
         STC   @07,DSITOTNM(,@05)                               03S0709
*                           COUNT=COUNT+(TTEDIALN-ACTUAL);/* DIAL
*                                      DATA COUNT FIELD IN WRK AREA  */
         SLR   @05,@05                                          03S0710
         IC    @05,COUNT(,@15)                                  03S0710
         ALR   @14,@05                                          03S0710
         STC   @14,COUNT(,@15)                                  03S0710
*                           BLRPLFLG=ON;/* SET FLAG TO BUILD REPLACE */
         OI    BLRPLFLG,B'01000000'                             03S0711
*                           RFY                                 03S0712
*                             RTNCODE RSTD;                     03S0712
*                         END;                                  03S0713
*                   END;            /*                       @YM07430*/
*                 ELSE                                          03S0715
*                   ;               /*                       @YM07430*/
@RF00679 DS    0H                                               03S0716
*               END;                                            03S0716
*             ELSE                                              03S0717
*               FREUTFLG=ON;        /* FREE OPCE AND RU              */
