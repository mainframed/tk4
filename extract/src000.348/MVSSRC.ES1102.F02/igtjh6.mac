TJH6     TITLE 'IGTJH6 - 3838 SUBSYSTEM MONITOR ACTIVE CHECK.          *
                        '
IGTJH6   CSECT ,                                                01S0001
@MAINENT DS    0H                                               01S0001
         USING *,@15                                            01S0001
         B     @PROLOG                                          01S0001
         DC    AL1(16)                                          01S0001
         DC    C'IGTJH6    77.092'                              01S0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                  01S0001
         BALR  @07,0                                            01S0001
@PSTART  DS    0H                                               01S0001
         USING @PSTART,@07                                      01S0001
         L     @00,@SIZDATD                                     01S0001
         IGTYGSV  R,LV=(0)
         LR    @06,@01                                          01S0001
         USING @DATD,@06                                        01S0001
         ST    @13,@SA00001+4                                   01S0001
         LM    @00,@01,20(@13)                                  01S0001
         ST    @06,8(,@13)                                      01S0001
         LR    @13,@06                                          01S0001
*   ALLENQ=1;                       /* ALLENQ IS INCREMENTED FOLLOW- */
         LA    ALLENQ,1                                         01S0055
*   FIRSTENQ=0;                     /* ING EACH ENQ ATTEMPT. IT IS
*                                      USED WITH FIRSTENQ TO DETECT
*                                      UNUSUAL MONITOR INITIATION/
*                                      TERMINATION SEQUENCES.        */
         SLR   FIRSTENQ,FIRSTENQ                                01S0056
*JH6DO:                                                         01S0057
*   DO WHILE ALLFAIL=BOFF&ALLMONUP=BOFF;/* DO UNTIL SUCCESSFUL OR    */
JH6DO    B     @DE00057                                         01S0057
@DL00057 DS    0H                                               01S0058
*     RFY                                                       01S0058
*      (R3P,                                                    01S0058
*       R15P) RSTD;                 /* CANCELLED OR ERROR CONDITION  */
*     GEN REFS(APCTENQM,APCTPTR,ALLOCATE,INITTCB,ENQLISTT,NQLISTTL,
*         ALLCWA,ALLCWAPT) SETS(DYNQLIST);                      01S0059
*                                   /*
         MVC   DYNQLIST(L'NQLISTTL),ENQLISTT INIT ENQ DYN LIST AREA
         ENQ   (APCTENQM-APCT(,APCTPTR),ALLOCATE,S,8,SYSTEM),          *
               RET=USE,TCB=INITTCB-ALLCWA(,ALLCWAPT),MF=(E,DYNQLIST)
*     R3P=R15P;                     /* SAVE POINTER TO ENQ WORK AREA */
         LR    R3P,R15P                                         01S0060
*     RFY                                                       01S0061
*       R15P UNRSTD;                                            01S0061
*     IF ALLENQ=1 THEN              /* IF FIRST ATTEMPT THEN SAVE ENQ*/
         C     ALLENQ,@CF00038                                  01S0062
         BNE   @RF00062                                         01S0062
*       IF R3P=0 THEN               /* IF RETURN CODE IS ZERO THEN   */
         LTR   R3P,R3P                                          01S0063
         BNZ   @RF00063                                         01S0063
*         ALLENQRC=0;               /* SAVED RETURN CODE IS ZERO     */
         MVI   ALLENQRC(ALLCWAPT),X'00'                         01S0064
*       ELSE                        /* ENQ RETURN CODE ^= 0          */
*         ALLENQRC=ENQRC;           /* SAVE RC FROM DYNAMIC AREA     */
         B     @RC00063                                         01S0065
@RF00063 IC    @09,ENQRC(,R3P)                                  01S0065
         STC   @09,ALLENQRC(,ALLCWAPT)                          01S0065
*     ALLENQ=ALLENQ+1;              /* NO LONGER FIRST ATTEMPT       */
@RC00063 DS    0H                                               01S0066
@RF00062 AL    ALLENQ,@CF00038                                  01S0066
*     IF R3P=0 THEN                 /* TASK NOW HAS CONTROL OF RE-   */
         LTR   R3P,R3P                                          01S0067
         BNZ   @RF00067                                         01S0067
*       IF APCTACT=BON THEN         /* SOURCE. CHECK TO SEE THAT MONI*/
         TM    APCTACT(APCTPTR),B'10000000'                     01S0068
         BNO   @RF00068                                         01S0068
*         DO;                       /* TOR IS ACTIVE. IF SO, SHOW IT.*/
*           ALLMONUP=BON;           /* AND INDICATE                  */
         OI    ALLMONUP(ALLCWAPT),B'00100000'                   01S0070
*           ALLALL=BON;             /* CONTROL OF ENQ RESOURCE.      */
         OI    ALLALL(ALLCWAPT),B'01000000'                     01S0071
*         END;                                                  01S0072
*       ELSE                        /* MONITOR NOT ACTIVE. RELEASE   */
*         DO;                       /* RESOURCE SO IT'S AVAILABLE FOR
*                                      MONITOR INITIALIZATION.       */
         B     @RC00068                                         01S0073
@RF00068 DS    0H                                               01S0074
*           GEN REFS(APCTENQM,APCT,APCTPTR,ALLOCATE,INITTCB,DEQLISTT,
*               DQLISTTL,ALLCWA,ALLCWAPT) SETS(DYNDEQLA);       01S0074
*                                   /*
         MVC   DYNDEQLA(L'DQLISTTL),DEQLISTT INIT DEQ DYN LIST AREA
         DEQ   (APCTENQM-APCT(,APCTPTR),ALLOCATE,8,SYSTEM),            *
               TCB=INITTCB-ALLCWA(,ALLCWAPT),MF=(E,DYNDEQLA)
*           ALLALL=BOFF;            /* NO LONGER CONTROL ENQ RESOURCE*/
         NI    ALLALL(ALLCWAPT),B'10111111'                     01S0075
*           CALL IGTJH9;            /* INITIATE SUBSYSTEM MONITOR    */
         BAL   @14,IGTJH9                                       01S0076
*         END;                                                  01S0077
*     ELSE                          /* ENQRC ^= 0                    */
*       IF ENQRC=4 THEN             /* IF MONITOR CONTROLS RESOURCE &*/
         B     @RC00067                                         01S0078
@RF00067 CLI   ENQRC(R3P),4                                     01S0078
         BNE   @RF00078                                         01S0078
*         IF APCTSTRT=BON THEN      /* IF MONITOR IS STARTING THEN   */
         TM    APCTSTRT(APCTPTR),B'01000000'                    01S0079
         BNO   @RF00079                                         01S0079
*           CALL IGTJH9;            /* ATTEMPT OPERATOR DIALOGUE     */
         BAL   @14,IGTJH9                                       01S0080
*         ELSE                      /* MONITOR IS NOT STARTING       */
*           IF APCTTERM=BON THEN    /* IF MONITOR IS TERMINATING     */
         B     @RC00079                                         01S0081
@RF00079 TM    APCTTERM(APCTPTR),B'00001000'                    01S0081
         BNO   @RF00081                                         01S0081
*             CALL IGTJH9;          /* ATTEMPT OPERATOR DIALOGUE     */
         BAL   @14,IGTJH9                                       01S0082
*           ELSE                    /* MONITOR IS IN STATE OF TRANSI-*/
*             IF ALLENQ=FIRSTENQ+1 THEN/* TION BUT IS NEITHER START
*                                      ING NOR TERMINATING. TRY ONE  */
         B     @RC00081                                         01S0083
@RF00081 LA    @09,1                                            01S0083
         ALR   @09,FIRSTENQ                                     01S0083
         CR    ALLENQ,@09                                       01S0083
         BNE   @RF00083                                         01S0083
*               DO;                 /* MORE TIME.                    */
*                 ALLFAIL=BON;      /* ALLOCATION FAILS ON THE SECOND*/
*                                                               01S0085
         OI    ALLFAIL(ALLCWAPT),B'10000000'                    01S0085
*                 /***************************************************/
*                 /*                                                 */
*                 /* IGTYJHE ERR(482)                                */
*                 /*                                                 */
*                 /***************************************************/
*                                                               01S0086
*                 RFY                                           01S0086
*                   R1F RSTD;                                   01S0086
*                 R1F=5;            /* PROVIDE INDEX                 */
         LA    R1F,5                                            01S0087
*                 CALL APCVZJHE;    /* SPECIFY ERROR INFORMATION
*                                      PRIOR TO RETURN               */
         L     @15,APCTZJHE(,APCTPTR)                           01S0088
         BALR  @14,@15                                          01S0088
*                 RFY                                           01S0089
*                   R1F UNRSTD;     /* DONE WITH REG CONSEQUTIVE TIME
*                                      THRU                          */
*               END;                /* THIS PATH                     */
*             ELSE                  /* NOT YET THE SECOND CONSEQUTIVE*/
*               FIRSTENQ=ALLENQ;    /* TRAVERSE OF THIS PATH         */
         B     @RC00083                                         01S0091
@RF00083 LR    FIRSTENQ,ALLENQ                                  01S0091
*       ELSE                        /* ENQRC ^= 4                    */
*         IF ENQRC=8 THEN           /* INITIATOR ALREADY HAS SHARED  */
         B     @RC00078                                         01S0092
@RF00078 CLI   ENQRC(R3P),8                                     01S0092
         BNE   @RF00092                                         01S0092
*           DO;                     /* CONTROL OF RESOURCE. INDICATE */
*             ALLMONUP=BON;         /* MONITOR UP. THE MONITOR IS
*                                      ASSUMED TO BE AVAILABLE TO
*                                      HANDLE THIS DYNAMIC ALLOCA-
*                                      TION.                         */
         OI    ALLMONUP(ALLCWAPT),B'00100000'                   01S0094
*             ALLALL=BON;           /* INDICATE THE ENQ RES IS OWNED */
         OI    ALLALL(ALLCWAPT),B'01000000'                     01S0095
*           END;                    /* OF SEGMENT                    */
*         ELSE                      /* ENQRC ^= 0,4,OR 8. ERROR COND */
*           DO;                     /* ITION EXISTS.                 */
         B     @RC00092                                         01S0097
@RF00092 DS    0H                                               01S0098
*             ALLFAIL=BON;          /* ALLOCATION FAILS.             */
*                                                               01S0098
         OI    ALLFAIL(ALLCWAPT),B'10000000'                    01S0098
*             /*******************************************************/
*             /*                                                     */
*             /* IGTYJHE ERR(483)                                    */
*             /*                                                     */
*             /*******************************************************/
*                                                               01S0099
*             RFY                                               01S0099
*               R1F RSTD;                                       01S0099
*             R1F=6;                /* PROVIDE INDEX                 */
         LA    R1F,6                                            01S0100
*             CALL APCVZJHE;        /* SPECIFY ERROR INFORMATION
*                                      PRIOR TO RETURN               */
         L     @15,APCTZJHE(,APCTPTR)                           01S0101
         BALR  @14,@15                                          01S0101
*             RFY                                               01S0102
*               R1F UNRSTD;         /* DONE WITH REG ERROR INFO      */
*           END;                                                01S0103
*     RFY                                                       01S0104
*       R3P UNRSTD;                 /* NO LONGER NEED REGISTER       */
@RC00092 DS    0H                                               01S0104
@RC00078 DS    0H                                               01S0104
@RC00067 DS    0H                                               01S0105
*   END JH6DO;                                                  01S0105
@DE00057 TM    ALLFAIL(ALLCWAPT),B'10100000'                    01S0105
         BZ    @DL00057                                         01S0105
*   IF ALLSTART=BON THEN            /* IF STARTUP RESOURCE CONTROLLED*/
         TM    ALLSTART(ALLCWAPT),B'00010000'                   01S0106
         BNO   @RF00106                                         01S0106
*     DO;                           /* FREE IT.                      */
*       GEN REFS(APCTENQM,STARTUP,DEQLIST1,DQLIST1L) SETS(DYNDEQLA);
*                                   /*
         MVC   DYNDEQLA(L'DQLIST1L),DEQLIST1 INIT DEQ DYN LIST AREA
         DEQ   (APCTENQM-APCT(,APCTPTR),STARTUP,7,SYSTEM),             *
               MF=(E,DYNDEQLA)
*       ALLSTART=BOFF;              /* NO LONGER CONTROL STARTUP     */
         NI    ALLSTART(ALLCWAPT),B'11101111'                   01S0109
*     END;                                                      01S0110
*   RETURN;                                                     01S0111
@EL00001 L     @13,4(,@13)                                      01S0111
@EF00001 L     @00,@SIZDATD                                     01S0111
         LR    @01,@06                                          01S0111
         IGTYFSV  R,LV=(0),A=(1)
@ER00001 LM    @14,@12,12(@13)                                  01S0111
         BR    @14                                              01S0111
*IGTJH9:                                                        01S0112
*   PROC;                                                       01S0112
IGTJH9   STM   @14,@12,@SA00002                                 01S0112
*                                                               01S0113
*   /*****************************************************************/
*   /*                                                               */
*   /* DYNAMIC VARIABLES                                             */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0113
*   DCL                                                         01S0113
*     ENQECB PTR(31);               /* ECB POSTED UPON ENQ COMPLETION*/
*   DCL                                                         01S0114
*     CNCLECB PTR(31) BASED(CNCLECBP);/* POINTER TO THE CECB         */
*   DCL                                                         01S0115
*     WTORL GEND AUTO CHAR(*);      /* LIST FORM OF WTOR             */
*   GEN DATA DEFS(WTORL);                                       01S0116
*   GEN DATA DEFS(WTORL2,WTORL2L);                              01S0117
*   DCL                                                         01S0118
*    (WTORL2,                                                   01S0118
*     WTORL2L) STATIC LOCAL CHAR(*);                            01S0118
*   DCL                                                         01S0119
*     WTORECB PTR(31);              /* ECB POSTED UPON COMPLETION OF
*                                      WTOR                          */
*   DCL                                                         01S0120
*     WTOREPLY CHAR(6) AUTO;        /* DYN AREA FOR REPLY            */
*   DCL                                                         01S0121
*     1 ECBLIST BASED(ECBLISTP),    /* DCSCRIBES ECBLISTS            */
*      2 THEECBP PTR(31),           /* POINTER TO ENQ ECB/APCTEMNA   */
*      2 CNCLECBP PTR(31),          /* POINTER TO CANCEL ECB         */
*       3 LASTECBI BIT(01);         /* LAST ECB PTR INDICATOR        */
*   DCL                                                         01S0122
*     ECBL1 CHAR(LENGTH(ECBLIST))   /* AREA FOR AN ECB LIST          */
*         BDY(WORD) AUTO;                                       01S0122
*   DCL                                                         01S0123
*     ECBLISTP PTR(31);             /* BASE FOR ECB LISTS            */
*                                                               01S0124
*   /*****************************************************************/
*   /*                                                               */
*   /* STATIC VARIABLES                                              */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0124
         EJECT
*   IF SSAGWAIT=BON THEN            /* IF WAITING IS ALLOWED THEN    */
         L     @05,SSOBPTR(,ALLCWAPT)                           01S0124
         L     @05,SSOBINDV(,@05)                               01S0124
         TM    SSAGWAIT(@05),B'10000000'                        01S0124
         BNO   @RF00124                                         01S0124
*     DO;                           /* BEGIN BY                      */
*       ECBLISTP=ADDR(ECBL1);       /* POINT TO AN ECBLIST           */
         LA    ECBLISTP,ECBL1                                   01S0126
*       THEECBP=ADDR(ENQECB);       /* PRIMARY ECB IS ENQ ECB        */
         LA    @09,ENQECB                                       01S0127
         ST    @09,THEECBP(,ECBLISTP)                           01S0127
*       CNCLECBP=ACNCLECB;          /* ADDRESS OF CANCEL ECB OR DUMMY*/
         L     @09,ACNCLECB(,ALLCWAPT)                          01S0128
         ST    @09,CNCLECBP(,ECBLISTP)                          01S0128
*       LASTECBI=BON;               /* INDICATE LAST ADDRESS IN LIST.*/
         OI    LASTECBI(ECBLISTP),B'10000000'                   01S0129
*       IF ALLSTART=BOFF THEN       /* IF TASK DOES NOT HAVE ENQ RES */
         TM    ALLSTART(ALLCWAPT),B'00010000'                   01S0130
         BNZ   @RF00130                                         01S0130
*         DO;                       /* GET THE RESOURCE              */
*           ENQECB=0;               /* CLEARING ECB                  */
         SLR   @09,@09                                          01S0132
         ST    @09,ENQECB                                       01S0132
*           GEN REFS(APCTENQM,STARTUP,ENQECB,ENQLISTE,NQLISTEL) SETS(
*               DYNQLIST);                                      01S0133
*                                   /*
         MVC   DYNQLIST(L'NQLISTEL),ENQLISTE INIT DYNAMIC ENQ LIST AREA
         ENQ  (APCTENQM-APCT(,APCTPTR),STARTUP,E,7,SYSTEM),            *
               ECB=ENQECB,MF=(E,DYNQLIST)
*           RFY                                                 01S0134
*             R15P RSTD;            /* SAVE REGISTER CONTENTS TEMP   */
*           ALLSTART=BON;           /* INDICATE PENDING OWNERSHIP OF
*                                      ENQ RESOURCE                  */
         OI    ALLSTART(ALLCWAPT),B'00010000'                   01S0135
*           IF R15P^=0 THEN         /* IF TASK DOESN'T HAVE EXCLUSIVE*/
         LTR   R15P,R15P                                        01S0136
         BZ    @RF00136                                         01S0136
*             DO;                   /* CONTROL OF RESOURCE THEN WAIT.*/
*               RFY                                             01S0138
*                 R15P UNRSTD;      /* NO LONGER NEED IT             */
*               DO;                 /* IGTYWAIT                 01S0139
*                                      (1)ECBLIST(ECBL1)LONG(YES)    */
*                 RFY                                           01S0140
*                  (GPR00F,                                     01S0140
*                   GPR01P) RSTD;   /* REGISTERS USED                */
*                 GPR01P=-ADDR(ECBL1);/* POINT TO ECB LIST           */
         LA    GPR01P,ECBL1                                     01S0141
         LCR   GPR01P,GPR01P                                    01S0141
*                 GPR00F=1×'80000000'X;/* WAIT COUNT                 */
         L     GPR00F,@CF00868                                  01S0142
*                 SVC(1);           /* WAIT FOR ECB POSTING          */
         SVC   1                                                01S0143
*                 RFY                                           01S0144
*                  (GPR00F,                                     01S0144
*                   GPR01P) UNRSTD; /* FINISHED WITH REGISTERS       */
*               END;                /* ECB WAIT COMPLETE WAIT        */
*               IF CNCLECBP->ECBPOSTB=BON THEN/* IF CANCELLED DURING */
         L     @09,CNCLECBP(,ECBLISTP)                          01S0146
         TM    ECBPOSTB(@09),B'01000000'                        01S0146
         BNO   @RF00146                                         01S0146
*                 DO;               /* WAIT THEN ALLOCATION FAILS    */
*                   ALLFAIL=BON;    /* INDICATE IT                   */
*                                                               01S0148
         OI    ALLFAIL(ALLCWAPT),B'10000000'                    01S0148
*                   /*************************************************/
*                   /*                                               */
*                   /* IGTYJHE ERR(492)                              */
*                   /*                                               */
*                   /*************************************************/
*                                                               01S0149
*                   RFY                                         01S0149
*                     R1F RSTD;                                 01S0149
*                   R1F=11;         /* PROVIDE INDEX                 */
         LA    R1F,11                                           01S0150
*                   CALL APCVZJHE;  /* SPECIFY ERROR INFORMATION
*                                      PRIOR TO RETURN               */
         L     @15,APCTZJHE(,APCTPTR)                           01S0151
         BALR  @14,@15                                          01S0151
*                   RFY                                         01S0152
*                     R1F UNRSTD;   /* DONE WITH REG PROVIDE ERROR
*                                      RETURN INFO                   */
*                 END;                                          01S0153
*               RFY                                             01S0154
*                 R15P UNRSTD;      /* DON'T NEED IT ANY LONGER      */
@RF00146 DS    0H                                               01S0155
*             END;                  /* DO IF ENQRC ^= 0              */
*           ELSE                    /* R15P = 0 AND WE OWN STARTUP   */
*             APCTEMNA='';          /* CLEAR THE MONITOR ACTIVE ECB  */
         B     @RC00136                                         01S0156
@RF00136 SLR   @09,@09                                          01S0156
         ST    @09,APCTEMNA(,APCTPTR)                           01S0156
*         END;                      /* DO IF ALLSTART = BOFF         */
@RC00136 DS    0H                                               01S0158
*       IF ALLFAIL=BOFF THEN        /* IF STARTUP CONTROLLED THEN    */
@RF00130 TM    ALLFAIL(ALLCWAPT),B'10000000'                    01S0158
         BNZ   @RF00158                                         01S0158
*         DO;                       /* SEE IF MONITOR HAS BECOME ACTV*/
*           APCTEMNA=0;             /* CLEAR THE MONITOR ACTIVE ECB  */
         SLR   @09,@09                                          01S0160
         ST    @09,APCTEMNA(,APCTPTR)                           01S0160
*           RFY                                                 01S0161
*            (R3P,                                              01S0161
*             R15P) RSTD;                                       01S0161
*           GEN REFS(APCTENQM,ALLOCATE,INITTCB,ENQLISTT,NQLISTTL,ALLCWA
*               ,ALLCWAPT) SETS(DYNQLIST);                      01S0162
*                                   /*
         MVC   DYNQLIST(L'NQLISTTL),ENQLISTT INIT ENQ DYNAMIC LIST AREA
         ENQ   (APCTENQM-APCT(,APCTPTR),ALLOCATE,S,8,SYSTEM),          *
               RET=USE,TCB=INITTCB-ALLCWA(,ALLCWAPT),MF=(E,DYNQLIST)
*           R3P=R15P;               /* SAVE POINTER TO ENQ WORK AREA */
         LR    R3P,R15P                                         01S0163
*           RFY                                                 01S0164
*             R15P UNRSTD;          /* NO LONGER NEED TO RESERVE IT  */
*           IF R3P=0×ENQRC=8 THEN   /* IF TASK HAS SHARED CONTROL    */
         LTR   R3P,R3P                                          01S0165
         BZ    @RT00165                                         01S0165
         CLI   ENQRC(R3P),8                                     01S0165
         BNE   @RF00165                                         01S0165
@RT00165 DS    0H                                               01S0166
*             DO;                   /* OF RESOURCE THEN CHECK TO SEE */
*               RFY                                             01S0167
*                 R3P UNRSTD;       /* WHETHER MONITOR IS NOW ACTIVE */
*               ALLALL=BON;         /* INDICATE OWNERSHIP OF RESOURCE*/
         OI    ALLALL(ALLCWAPT),B'01000000'                     01S0168
*               IF APCTACT=BON THEN /* IF MONITOR IS ACTIVE, INDICATE*/
         TM    APCTACT(APCTPTR),B'10000000'                     01S0169
         BNO   @RF00169                                         01S0169
*                 ALLMONUP=BON;     /* IT LOCALLY.                   */
         OI    ALLMONUP(ALLCWAPT),B'00100000'                   01S0170
*               ELSE                /* OTHERWISE, RELEASE CONTROL OF */
*                 DO;               /* RESOURCE SO MONITOR CAN START */
         B     @RC00169                                         01S0171
@RF00169 DS    0H                                               01S0172
*                   GEN REFS(APCTENQM,ALLOCATE,DEQLISTT,DQLISTTL,
*                       INITTCB,ALLCWA,ALLCWAPT) SETS(DYNDEQLA);
*                                   /*
         MVC   DYNDEQLA(L'DQLISTTL),DEQLISTT INIT DEQ DYN LIST AREA
         DEQ   (APCTENQM-APCT(,APCTPTR),ALLOCATE,8,SYSTEM),            *
               MF=(E,DYNDEQLA),TCB=INITTCB-ALLCWA(,ALLCWAPT)
*                   ALLALL=BOFF;    /* NO LONGER HAVE CONTROL OF RES */
         NI    ALLALL(ALLCWAPT),B'10111111'                     01S0173
*                 END;              /*                               */
*             END;                  /* IF ENQRC = 0 OR 8             */
@RC00169 DS    0H                                               01S0176
*           IF ALLMONUP^=BON THEN   /* IF MONITOR IS ACTIVE THEN     */
@RF00165 TM    ALLMONUP(ALLCWAPT),B'00100000'                   01S0176
         BO    @RF00176                                         01S0176
*             DO;                   /* INCLUDE THE WHOLE SEGMENT     */
*               GEN REFS(WTORL2,WTORL2L) SETS(WTORL);           02S0178
*                                   /*
         MVC   WTORL(L'WTORL2L),WTORL2 MOVE THE MESSAGE TO OUTPUT AREA
*               WTORL(22:25)=APCTSSID;/* PUT THE SUBSYSTEM NAME IN
*                                      THE MESSAGE                   */
         MVC   WTORL+21(4),APCTSSID(APCTPTR)                    02S0179
*               DO UNTIL WTOREPLY='W'×WTOREPLY='C'×WTOREPLY='R';/*
*                                      CANCEL IS RECEIVED            */
@DL00180 DS    0H                                               02S0181
*                 WTOREPLY='';      /* BLANK INPUT AREA              */
         MVI   WTOREPLY,C' '                                    02S0181
         MVC   WTOREPLY+1(5),WTOREPLY                           02S0181
*                 WTORECB=''X;      /* CLEAR THE ECB                 */
         SLR   @09,@09                                          02S0182
         ST    @09,WTORECB                                      02S0182
*                 GEN;                                          02S0183
*                                   /*
     WTOR ,WTOREPLY,6,WTORECB,MF=(E,WTORL) ASK HIM
*                 DO;               /* IGTYWAIT ECB(WTORECB)LONG(YES)*/
*                   RFY                                         02S0185
*                    (GPR00F,                                   02S0185
*                     GPR01P) RSTD; /* REGISTERS USED                */
*                   DCL                                         02S0186
*                     BIT0034X BIT(8) BASED(ADDR(WTORECB));/* OVERLAY
*                                      BYTE-0 OF ECB                 */
*                   IF BIT0034X(2)='0'B THEN/* IF ECB NOT ALREADY
*                                      POSTED                        */
         LA    @09,WTORECB                                      02S0187
         TM    BIT0034X(@09),B'01000000'                        02S0187
         BNZ   @RF00187                                         02S0187
*                     DO;           /* THEN ISSUE WAIT SVC           */
*                       GPR01P=ADDR(WTORECB);/* POINT TO ECB         */
         LR    GPR01P,@09                                       02S0189
*                       GPR00F=1×'80000000'X;/* WAIT COUNT           */
         L     GPR00F,@CF00868                                  02S0190
*                       SVC(1);     /* WAIT FOR ECB POSTING          */
         SVC   1                                                02S0191
