@DL00184 DS    0H                                               01S0185
*     APDSPTR=SSAGSSCM;             /* POINT TO APDS ASSOC W/ REQUEST*/
         L     APDSPTR,SSAGSSCM(,SSDDRPTR)                      01S0185
*     IF APDSOWN=EXC×               /* IF APDS REPRESENTS A UNIQUE   */
*         APDSLSR=BON THEN          /* RESOURCE DEMAND               */
         TM    APDSOWN(APDSPTR),B'10000010'                     01S0186
         BZ    @RF00186                                         01S0186
*       DO;                         /* ATTEMPT TO SATISFY IT         */
*         SMLAPUB=0;                /* PTR TO SMALLEST SUITABLE APUB */
