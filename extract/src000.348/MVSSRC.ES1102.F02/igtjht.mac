TJHT     TITLE 'IGTJHT - SS ALLOCATION RECOVERY SUBROUTINE- TEST       *
                        '
IGTJHT   CSECT ,                                                01S0001
@MAINENT DS    0H                                               01S0001
         USING *,@15                                            01S0001
         B     @PROLOG                                          01S0001
         DC    AL1(16)                                          01S0001
         DC    C'IGTJHT    77.092'                              01S0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                  01S0001
         BALR  @07,0                                            01S0001
@PSTART  DS    0H                                               01S0001
         USING @PSTART,@07                                      01S0001
         L     @00,@SIZDATD                                     01S0001
         IGTYGSV  R,LV=(0)
         LR    @06,@01                                          01S0001
         USING @DATD,@06                                        01S0001
         ST    @13,@SA00001+4                                   01S0001
         LM    @00,@01,20(@13)                                  01S0001
         ST    @06,8(,@13)                                      01S0001
         LR    @13,@06                                          01S0001
*   DO;                             /* GET ENRTIRE SEGMENT           */
*/*   SEGMENT DETERMINES THEN NUMBER OF PORTS REQUIRED BY JOB AND THE
*     BULK STORAGE REQUIREMENT FOR THOSE PORTS.                      */
*                                                               02S0059
*     LRGSTSPR=0;                   /* LARGEST SHARED PORT REGION
*                                      REQUESTED                     */
         SLR   LRGSTSPR,LRGSTSPR                                02S0059
*     NPNTOT=0;                     /* TOTAL # PORTS REQUIRED BY THIS
*                                      JOB                           */
         SLR   @15,@15                                          02S0060
         STH   @15,NPNTOT(,ALLCWAPT)                            02S0060
*     BSRTOT=0;                     /* TOTAL BULK STORAGE REQUIRED BY
*                                      BY THIS JOB                   */
         ST    @15,BSRTOT(,ALLCWAPT)                            02S0061
*     NUMIER=0;                     /* NUMBER OF REQUESTS FOR WHICH
*                                      AN INSTALLATION EXIT     02S0062
*                                      PREVIOUSLY DISAPPROVED AN
*                                      AVAILABLE PORT                */
         STH   @15,NUMIER(,ALLCWAPT)                            02S0062
*     SSDDRPTR=FIRSTARB;            /* START WITH FIRST REQUEST BLK  */
         L     SSDDRPTR,FIRSTARB(,ALLCWAPT)                     02S0063
*     DO UNTIL SSDDRPTR=0;          /* AND CONTINUE THROUGH LAST     */
@DL00064 DS    0H                                               02S0065
*       APDSPTR=SSAGSSCM;           /* POINT TO APDS ASSOC W/ REQUEST*/
         L     APDSPTR,SSAGSSCM(,SSDDRPTR)                      02S0065
*       IF APDSOWN=SHR THEN         /* IF THIS IS A SHARED PORT REQST*/
         TM    APDSOWN(APDSPTR),B'10000000'                     02S0066
         BNZ   @RF00066                                         02S0066
*         IF APDSREG>=LRGSTSPR THEN /* AND THE LARGEST ONE YET       */
         L     @15,APDSREG(,APDSPTR)                            02S0067
         CR    @15,LRGSTSPR                                     02S0067
         BL    @RF00067                                         02S0067
*           DO;                     /* SAVE RELEVANT INFORMATION     */
*             LRGSTSPR=APDSREG;     /* SIZE OF REGION REQUESTED      */
         LR    LRGSTSPR,@15                                     02S0069
*             LRGSTAPD=APDSPTR;     /* ADDRESS OF APDS               */
         LR    LRGSTAPD,APDSPTR                                 02S0070
*           END;                    /* DO IF APDSREG >= LRGSTSPR     */
*         ELSE                                                  02S0072
*           ;                       /* NO ACTION REQUIRED            */
@RF00067 DS    0H                                               02S0073
*       ELSE                        /* EXCLUSIVE REQUEST             */
*         DO;                       /* COLLECT THE DATA WE CAME FOR  */
         B     @RC00066                                         02S0073
@RF00066 DS    0H                                               02S0074
*           NPNTOT=NPNTOT+1;        /* ONE PORT NEEDED FOR EACN EXC  */
         LA    @15,1                                            02S0074
         AH    @15,NPNTOT(,ALLCWAPT)                            02S0074
         STH   @15,NPNTOT(,ALLCWAPT)                            02S0074
*           BSRTOT=BSRTOT+APDSREG;  /* REQUEST AND MORE BULK STORAGE */
         L     @15,BSRTOT(,ALLCWAPT)                            02S0075
         AL    @15,APDSREG(,APDSPTR)                            02S0075
         ST    @15,BSRTOT(,ALLCWAPT)                            02S0075
*         END;                      /* ELSE EXCLUSIVE REQUEST        */
*       IF APDSIER=BON THEN         /* INSTALLATION EXIT REJECTED    */
@RC00066 TM    APDSIER(APDSPTR),B'00000100'                     02S0077
         BNO   @RF00077                                         02S0077
*         NUMIER=NUMIER+1;          /* ON MORE                       */
         LA    @15,1                                            02S0078
         AH    @15,NUMIER(,ALLCWAPT)                            02S0078
         STH   @15,NUMIER(,ALLCWAPT)                            02S0078
*       SSDDRPTR=SSAGNRBP;          /* ADVANCE TO NEXT REQUEST / DONE*/
@RF00077 L     SSDDRPTR,SSAGNRBP(,SSDDRPTR)                     02S0079
*     END;                          /* DO UNTIL SSDDRPTR = 0         */
@DE00064 SLR   @15,@15                                          02S0080
         CR    SSDDRPTR,@15                                     02S0080
         BNE   @DL00064                                         02S0080
*     IF LRGSTSPR^=0 THEN           /* AT LEAST ONE SHARED REQUEST   */
         CR    LRGSTSPR,@15                                     02S0081
         BE    @RF00081                                         02S0081
*       DO;                         /* INCLUDE DATA FOR IT           */
*         NPNTOT=NPNTOT+1;          /* ONE MORE PORT TO HANDLE SHR'D
*                                      RESOURCE REQUESTS             */
         LA    @05,1                                            02S0083
         AH    @05,NPNTOT(,ALLCWAPT)                            02S0083
         STH   @05,NPNTOT(,ALLCWAPT)                            02S0083
*         LRGSTAPD->APDSLSR=BON;    /* INDICATE LARGEST SHARED REQST */
         OI    APDSLSR(LRGSTAPD),B'00000010'                    02S0084
*         BSRTOT=BSRTOT+LRGSTAPD->APDSREG;/* ADD BULK STOR REQUIRED  */
         L     @05,BSRTOT(,ALLCWAPT)                            02S0085
         AL    @05,APDSREG(,LRGSTAPD)                           02S0085
         ST    @05,BSRTOT(,ALLCWAPT)                            02S0085
*       END;                        /* DO IF LRGSTSPR ^= 0           */
*     IF NUMIER=NUMALREQ THEN       /* IF INST EXIT REJECTED EACH 1  */
@RF00081 LH    @05,NUMIER(,ALLCWAPT)                            02S0087
         CL    @05,NUMALREQ(,ALLCWAPT)                          02S0087
         BNE   @RF00087                                         02S0087
*       EXITREJ=BON;                /* LET 1JHT KNOW ABOUT IT        */
         OI    EXITREJ(ALLCWAPT),B'10000000'                    02S0088
*   END;                            /* END OF SEGMENT                */
@RF00087 DS    0H                                               02S0090
*   DO;                             /* GET ENTIRE SEGMENT            */
*/*   SEGMENT DETERMINES THE AVAILABILITY, ACTUAL AND POTENTIAL, OF
*     ALL APUBS ANCHORED BY THE SUBSYSTEM APCT.  IN ADDITION, THE
*     SIZES OF THE AVAILABLE PARTITONS ARE SUMMED TO DETERMINE WHETHER
*     THE JOB'S REQUESTS COULD BE SATISFIES WITHOUT RECONFIGURATION. */
*                                                               02S0091
*     MSTRAPUB=APCTAPUB;            /* ADDRESS INITIAL APUB (MSTR)   */
         L     MSTRAPUB,APCTAPUB(,APCTPTR)                      02S0091
*     DO UNTIL MSTRAPUB=0;          /* DO ONCE FOR EACH MASTER APUB  */
@DL00092 DS    0H                                               02S0093
*       APUBPTR=MSTRAPUB->APUBAPUB; /* POINT TO FIRST USER SUBCHANNEL*/
         L     APUBPTR,APUBAPUB(,MSTRAPUB)                      02S0093
*       IF APUBUNLD=BON THEN        /* IF THE 3838 IS UNLOADED OR    */
         TM    APUBUNLD(APUBPTR),B'00100000'                    02S0094
         BNO   @RF00094                                         02S0094
*         DO;                       /* ABOUT TO BE                   */
*           USCUNLD=USCUNLD+USCP3838;/* ASSUME MAX # SUBS PER 3838   */
         LA    @05,7                                            02S0096
         AH    @05,USCUNLD(,ALLCWAPT)                           02S0096
         STH   @05,USCUNLD(,ALLCWAPT)                           02S0096
*           BSUNLD=BSUNLD+BSP3838;  /* ASSUME MAX BULK STORAGE SIZE  */
         LA    @05,1024                                         02S0097
         AH    @05,BSUNLD(,ALLCWAPT)                            02S0097
         STH   @05,BSUNLD(,ALLCWAPT)                            02S0097
*         END;                      /* DO IF APUBUNLD = BON          */
*       ELSE                        /* 3838 IS IPLED. EXAMINE EACH SC*/
*         DO UNTIL APUBPTR=0;       /* ONCE PER NON-CNTL SUBCHANNEL  */
         B     @RC00094                                         02S0099
@RF00094 DS    0H                                               02S0099
@DL00099 DS    0H                                               02S0100
*           APUBALF3=0;             /* CLEAR TEMPORARY INDICATORS    */
         MVI   APUBALF3(APUBPTR),X'00'                          02S0100
*           IF APUBZRGN=BOFF&       /* IF PORT HAS BULK STOR ASSIGND */
*               APUBBERR=BOFF THEN  /* AND NOT TOO MANY ERRORS       */
         TM    APUBZRGN(APUBPTR),B'10010000'                    02S0101
         BNZ   @RF00101                                         02S0101
*             DO;                   /* PORT IS AVAILABLE             */
*               USCAVAIL=USCAVAIL+1;/* ONE MORE PORT AVAILABLE       */
         LA    @05,1                                            02S0103
         AH    @05,USCAVAIL(,ALLCWAPT)                          02S0103
         STH   @05,USCAVAIL(,ALLCWAPT)                          02S0103
*               BSAVAIL=BSAVAIL+APUBRGN;/* INCLUDE PORT'S BULK STOR  */
         LH    @05,BSAVAIL(,ALLCWAPT)                           02S0104
         AH    @05,APUBRGN(,APUBPTR)                            02S0104
         STH   @05,BSAVAIL(,ALLCWAPT)                           02S0104
*               APUBSCAV=BON;       /* SUBCHANNEL AVAILABLE FOR ALLOC*/
         L     @05,APUBSCAV-3(,APUBPTR)                         02S0105
@GL00001 LR    @03,@05                                          02S0105
         O     @03,@CF00902                                     02S0105
         CS    @05,@03,APUBSCAV-3(APUBPTR)                      02S0105
         BNE   @GL00001                                         02S0105
*             END;                  /* DO IF PORT IS AVAILABLE       */
*           ELSE                    /* PORT IS NOT "AVAILABLE"       */
*             DO;                   /* INCLUDE INFO IN DATA FOR UNLD */
         B     @RC00101                                         02S0107
@RF00101 DS    0H                                               02S0108
*               USCUNLD=USCUNLD+1;  /* ONE MORE PORT                 */
         LA    @05,1                                            02S0108
         AH    @05,USCUNLD(,ALLCWAPT)                           02S0108
         STH   @05,USCUNLD(,ALLCWAPT)                           02S0108
*               BSUNLD=BSUNLD+APUBRGN;/* RGN IS REAL OR ZERO         */
         LH    @05,BSUNLD(,ALLCWAPT)                            02S0109
         AH    @05,APUBRGN(,APUBPTR)                            02S0109
         STH   @05,BSUNLD(,ALLCWAPT)                            02S0109
*             END;                  /* ELSE PORT IS NOT AVAILABLE    */
*           APUBPTR=APUBAPUB;       /* ON TO NEXT OR DONE            */
@RC00101 L     APUBPTR,APUBAPUB(,APUBPTR)                       02S0111
*         END;                      /* DO UNTIL APUBPTR = 0          */
@DE00099 LTR   APUBPTR,APUBPTR                                  02S0112
         BNZ   @DL00099                                         02S0112
*       MSTRAPUB=MSTRAPUB->APUBMSTR;/* ADVANCE TO NEXT MASTER / DON  */
@RC00094 L     MSTRAPUB,APUBMSTR(,MSTRAPUB)                     02S0113
*     END;                          /* DO UNTIL MSTRAPUB = 0         */
@DE00092 LTR   MSTRAPUB,MSTRAPUB                                02S0114
         BNZ   @DL00092                                         02S0114
*   END;                            /* END OF SEGMENT                */
*   IF NPNTOT>USCAVAIL+USCUNLD THEN /* IF TOTAL # OF PORTS NEEDED >  */
         LH    @05,USCAVAIL(,ALLCWAPT)                          01S0116
         AH    @05,USCUNLD(,ALLCWAPT)                           01S0116
         CH    @05,NPNTOT(,ALLCWAPT)                            01S0116
         BNL   @RF00116                                         01S0116
*     DO;                           /* TOTAL # OF NON-CONTROL SUBCHAN*/
*       EXITIMPP=BON;               /* NELS IT IS IMPOSSIBLE TO      */
         OI    EXITIMPP(ALLCWAPT),B'10000000'                   01S0118
*       ALLFAIL=BON;                /* SATISFY THE USER'S REQUESTS   */
*                                                               01S0119
         OI    ALLFAIL(ALLCWAPT),B'10000000'                    01S0119
*       /*************************************************************/
*       /*                                                           */
*       /* IGTYJHE ERR(478)                                          */
*       /*                                                           */
*       /*************************************************************/
*                                                               01S0120
*       RFY                                                     01S0120
*         R1F RSTD;                                             01S0120
*       R1F=19;                     /* PROVIDE INDEX                 */
         LA    R1F,19                                           01S0121
*       CALL APCVZJHE;              /* SPECIFY ERROR INFORMATION
*                                      PRIOR TO RETURN               */
         L     @15,APCTZJHE(,APCTPTR)                           01S0122
         BALR  @14,@15                                          01S0122
*       RFY                                                     01S0123
*         R1F UNRSTD;               /* DONE WITH REG INDICATE PROBLEM
*                                      TO USER                       */
*     END;                          /* DO IF INSUFFICIENT PORTS      */
*   ELSE                            /* SUFFICIENT # PORTS MAY EXIST  */
*     IF BSRTOT>BSAVAIL+BSUNLD THEN /* NOT ENOUGH BULK STORAGE TO    */
         B     @RC00116                                         01S0125
@RF00116 LH    @05,BSAVAIL(,ALLCWAPT)                           01S0125
         AH    @05,BSUNLD(,ALLCWAPT)                            01S0125
         C     @05,BSRTOT(,ALLCWAPT)                            01S0125
         BNL   @RF00125                                         01S0125
*       DO;                         /* SATISFY REQUESTS              */
*         ALLFAIL=BON;              /* INDICATE ALLOCATION FAILS     */
         OI    ALLFAIL(ALLCWAPT),B'10000000'                    01S0127
*         EXITIMPS=BON;             /* INSUFFICIENT STORAGE          */
*                                                               01S0128
         OI    EXITIMPS(ALLCWAPT),B'01000000'                   01S0128
*         /***********************************************************/
*         /*                                                         */
*         /* IGTYJHE ERR(479)                                        */
*         /*                                                         */
*         /***********************************************************/
*                                                               01S0129
*         RFY                                                   01S0129
*           R1F RSTD;                                           01S0129
*         R1F=20;                   /* PROVIDE INDEX                 */
         LA    R1F,20                                           01S0130
*         CALL APCVZJHE;            /* SPECIFY ERROR INFORMATION
*                                      PRIOR TO RETURN               */
         L     @15,APCTZJHE(,APCTPTR)                           01S0131
         BALR  @14,@15                                          01S0131
*         RFY                                                   01S0132
*           R1F UNRSTD;             /* DONE WITH REG INDICATE PROBLEM
*                                      TO USER                       */
*       END;                        /* DO IF BSRTOT > ...            */
*     ELSE                          /* SUFFICIENT BULK STOR MAY EXIST*/
*       DO;                         /* TO SATISFY THE REQUESTS       */
*                                                               01S0134
         B     @RC00125                                         01S0134
@RF00125 DS    0H                                               01S0135
*         /***********************************************************/
*         /*                                                         */
*         /* ATTEMPT TO SATISFY THE USER'S REQUEST ASSUMING THAT ALL */
*         /* LOADED PORTS NOT HAVING ZERO REGIONS OR EXCESSIVE ERRORS*/
*         /* ARE AVAILABLE.                                          */
*         /*                                                         */
*         /***********************************************************/
*                                                               01S0135
*         ALLMARK=BON;              /* TRYALLOC IS TO PROVIDE DD
*                                      LEVEL ERROR INFORMATION       */
         OI    ALLMARK(ALLCWAPT),B'00010000'                    01S0135
*         CALL TRYALLOC;            /* PERFORM HYPOTHETICAL ALLOCATN */
         BAL   @14,TRYALLOC                                     01S0136
*         IF NUMNTSAT=0 THEN        /* ALL REQUESTS WERE SATISFIED   */
         LH    @05,NUMNTSAT(,ALLCWAPT)                          01S0137
         LTR   @05,@05                                          01S0137
         BNZ   @RF00137                                         01S0137
*           DO;                     /* ALLOCATION COULD SUCCEED W/0  */
*             EXITALOK=BON;         /* RECONFIGURATION OF RESOURCES  */
         OI    EXITALOK(ALLCWAPT),B'00100000'                   01S0139
*             DO;                   /* GET ENTIRE SEGMENT            */
*/*   SEGMENT DETERMINES THE ACTUAL AVAILABILITY OF ALL APUBS.       */
*                                                               02S0141
*               MSTRAPUB=APCTAPUB;  /* ADDRESS INITIAL APUB (MSTR)   */
         L     MSTRAPUB,APCTAPUB(,APCTPTR)                      02S0141
*               DO UNTIL MSTRAPUB=0;/* DO ONCE FOR EACH MASTER APUB  */
@DL00142 DS    0H                                               02S0143
*                 APUBPTR=MSTRAPUB->APUBAPUB;/* POINT TO FIRST USER
*                                      SUBCHANNEL                    */
         L     APUBPTR,APUBAPUB(,MSTRAPUB)                      02S0143
*                 IF APUBUNLD=BOFF THEN/* IF THE 3838 IS UNLOADED DO */
         TM    APUBUNLD(APUBPTR),B'00100000'                    02S0144
         BNZ   @RF00144                                         02S0144
*                   DO UNTIL APUBPTR=0;/* ONCE PER NON-CNTL     02S0145
*                                      SUBCHANNEL                    */
@DL00145 DS    0H                                               02S0146
*                     APUBALF3=0;   /* CLEAR TEMPORARY INDICATORS    */
         MVI   APUBALF3(APUBPTR),X'00'                          02S0146
*                     IF APUBZRGN=BOFF&/* IF PORT HAS BULK STOR 02S0147
*                                      ASSIGND                       */
*                         APUBDSBL=BOFF&/* AND IS NOT DISABLED AND   */
*                         APUBBERR=BOFF THEN/* AND NOT TOO MANY 02S0147
*                                      ERRORS                        */
         TM    APUBZRGN(APUBPTR),B'11010000'                    02S0147
         BNZ   @RF00147                                         02S0147
*                       APUBSCAV=BON;/* SUBCHANNEL AVAILABLE FOR
*                                      ALLOC                         */
         L     @05,APUBSCAV-3(,APUBPTR)                         02S0148
@GL00002 LR    @03,@05                                          02S0148
         O     @03,@CF00902                                     02S0148
         CS    @05,@03,APUBSCAV-3(APUBPTR)                      02S0148
         BNE   @GL00002                                         02S0148
*                     APUBPTR=APUBAPUB;/* ON TO NEXT OR DONE         */
@RF00147 L     APUBPTR,APUBAPUB(,APUBPTR)                       02S0149
*                   END;            /* DO UNTIL APUBPTR = 0          */
@DE00145 LTR   APUBPTR,APUBPTR                                  02S0150
         BNZ   @DL00145                                         02S0150
*                 MSTRAPUB=MSTRAPUB->APUBMSTR;/* ADVANCE TO NEXT
*                                      MASTER / DON                  */
@RF00144 L     MSTRAPUB,APUBMSTR(,MSTRAPUB)                     02S0151
*               END;                /* DO UNTIL MSTRAPUB = 0         */
@DE00142 LTR   MSTRAPUB,MSTRAPUB                                02S0152
         BNZ   @DL00142                                         02S0152
*             END;                  /* END OF SEGMENT                */
*             ALLMARK=BOFF;         /* TRYALLOC IS NOT TO PROVIDE MSG*/
         NI    ALLMARK(ALLCWAPT),B'11101111'                    01S0154
*             CALL TRYALLOC;        /* PERFORM HYPOTHETICAL ALLOCATN */
         BAL   @14,TRYALLOC                                     01S0155
*             IF NUMNTSAT^=0 THEN   /* IF ENABLING OF 1 OR MORE PORTS*/
         LH    @05,NUMNTSAT(,ALLCWAPT)                          01S0156
         LTR   @05,@05                                          01S0156
         BZ    @RF00156                                         01S0156
*               EXITENRQ=BON;       /* IS REQUIRED THEN SO INDICATE  */
         OI    EXITENRQ(ALLCWAPT),B'00010000'                   01S0157
*           END;                    /* FOR INSTALLATION EXIT         */
*         ELSE                      /* NOT ALL REQUESTS WERE SATISFID*/
*           IF USCUNLD=0 THEN       /* IF NO UNLOADED 3838S EXIST    */
         B     @RC00137                                         01S0159
@RF00137 LH    @05,USCUNLD(,ALLCWAPT)                           01S0159
         LTR   @05,@05                                          01S0159
         BNZ   @RF00159                                         01S0159
*             EXITRCON=BON;         /* RECONFIGURATION IS REQUIRED   */
         OI    EXITRCON(ALLCWAPT),B'00001000'                   01S0160
*       END;                        /* DO IF SUFFICIENT BS MAY EXIST */
@RF00159 DS    0H                                               01S0161
@RC00137 DS    0H                                               01S0162
*   ALLINST=BON;                    /* INDICATE INST EXIT IN CONTROL */
@RC00125 DS    0H                                               01S0162
@RC00116 OI    ALLINST(ALLCWAPT),B'00100000'                    01S0162
*   CALL APCT1JHT(ALLCWA);          /* FINALLY CALL INST EXIT        */
         ST    ALLCWAPT,@AL00001                                01S0163
         L     @15,APCT1JHT(,APCTPTR)                           01S0163
         LA    @01,@AL00001                                     01S0163
         BALR  @14,@15                                          01S0163
*   ALLINST=BOFF;                   /* IND INST EXIT NOT IN CONTROL  */
         NI    ALLINST(ALLCWAPT),B'11011111'                    01S0164
*   IF EXITCOMM=BOFF THEN           /* IF OPERATOR DIALOGUE TO BE    */
         TM    EXITCOMM(ALLCWAPT),B'01000000'                   01S0165
         BNZ   @RF00165                                         01S0165
*     CALL APCVZJHR;                /* DONE, DO IT.                  */
         L     @15,APCTZJHR(,APCTPTR)                           01S0166
         BALR  @14,@15                                          01S0166
*   ELSE                            /* OTHERWISE, NO COMMUNICATION   */
*     IF EXITCANC=BON THEN          /* REQUEST DENIED BY INST EXIT   */
         B     @RC00165                                         01S0167
@RF00165 TM    EXITCANC(ALLCWAPT),B'00100000'                   01S0167
         BNO   @RF00167                                         01S0167
*       DO;                         /* CLEAN UP & LET USER KNOW WHO  */
*         ALLFAIL=BON;              /* INDICATE THAT WERE DONE       */
*                                                               01S0169
         OI    ALLFAIL(ALLCWAPT),B'10000000'                    01S0169
*         /***********************************************************/
*         /*                                                         */
*         /* IGTYJHE ERR(485)                                        */
*         /*                                                         */
*         /***********************************************************/
*                                                               01S0170
*         RFY                                                   01S0170
*           R1F RSTD;                                           01S0170
*         R1F=8;                    /* PROVIDE INDEX                 */
         LA    R1F,8                                            01S0171
*         CALL APCVZJHE;            /* SPECIFY ERROR INFORMATION
*                                      PRIOR TO RETURN               */
         L     @15,APCTZJHE(,APCTPTR)                           01S0172
         BALR  @14,@15                                          01S0172
*         RFY                                                   01S0173
*           R1F UNRSTD;             /* DONE WITH REG GOT HIM         */
*       END;                        /* DO IF EXITCANC = BON          */
*     ELSE                          /* EXIT TRN HAS SPECIFIED WAIT   */
*       DO;                         /* DO IF EXITCANC = BOFF         */
         B     @RC00167                                         01S0175
@RF00167 DS    0H                                               01S0176
*         ALLWAIT=BON;              /* INDICATE IN ALLSTAT           */
         OI    ALLWAIT(ALLCWAPT),B'01000000'                    01S0176
*         IF EXITHOLD=BON THEN      /* IF HOLD IS SPECIFIED THEN     */
         TM    EXITHOLD(ALLCWAPT),B'00010000'                   01S0177
         BNO   @RF00177                                         01S0177
*           ALLHOLD=BON;            /* INDICATE IN ALLSTAT           */
         OI    ALLHOLD(ALLCWAPT),B'01000000'                    01S0178
*       END;                        /* DO IF EXITCANC = BOFF         */
*   RETURN;                                                     01S0180
@EL00001 L     @13,4(,@13)                                      01S0180
@EF00001 L     @00,@SIZDATD                                     01S0180
         LR    @01,@06                                          01S0180
         IGTYFSV  R,LV=(0),A=(1)
@ER00001 LM    @14,@12,12(@13)                                  01S0180
         BR    @14                                              01S0180
         EJECT
*TRYALLOC:                                                      01S0181
*   PROC;                           /* THIS SUBROUTINE TRYS TO ALLOC8*/
TRYALLOC STM   @14,@12,@SA00002                                 01S0181
*/* THIS SUBROUTINE DEPENDS ON APUBS HAVING BEEN PROPERLY INITIALIZED
*   BY EITHER TSTAPUB1 OR TESTAPUB2.  ESPECIALLY IMPORTANT ARE THE BITS
*   IN APUBALF3 - APUBSCAV AND APUBSCAL.  THE SUBROUTINE IGNORES
*   EXISTING ALLOCATIONS AND IN SOME CASES THE FACT THAT PORTS ARE
*   DISABLED, AND ATTEMPTS TO SATISFY THE ENTIRE RESOURCE REQUIREMENTS
*   OF THE JOB IN ALLOCATION.  IF NUMNTSAT IS ZERO ON EXIT, ALL 01S0182
*   THE REQUESTS COULD BE SATISFIED GIVEN THE ENVIRONMENT REPRESENTED
*   BY THE APUBS ON ENTRY TO TRYALLOC.  BEFORE SUCCESSIVE EXECUTIONS
*   OF THIS ROUTINE APUBALF3 IN EACH LOADED APUB MUST BE INITIALIZED
*   TO ZERO AND THEN BIT APUBSCAV SET TO INDICATE THAT THE APUB IS
*   TO BE CONSIDERED AVAILABLE FOR ALLOCATION TO THIS JOB.           */
*                                                               01S0182
*   NUMNTSAT=0;                     /* NUMBER OF REQUESTS ^SATISFIED */
         SLR   @15,@15                                          01S0182
         STH   @15,NUMNTSAT(,ALLCWAPT)                          01S0182
*   SSDDRPTR=FIRSTARB;              /* START WITH FIRST REQUEST      */
