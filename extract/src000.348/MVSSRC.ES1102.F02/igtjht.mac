*                         SMLREG=APUBRGN;/* PARTITION SIZE           */
         LR    SMLREG,@09                                       01S0200
*                         SMLAPUB=APUBPTR;/* APUB ADDR               */
         LR    SMLAPUB,APUBPTR                                  01S0201
*                       END;        /* DO IF SMALLEST YET            */
*                 END;              /* DO IF APUB IS ALLOCATABLE     */
*               ELSE                                            01S0204
*                 ;                 /* SUB NOT AVAIL OR PREV ALLOC'D */
@RF00195 DS    0H                                               01S0205
*               APUBPTR=APUBAPUB;   /* ON TO NEXT OR DONE            */
@RC00195 L     APUBPTR,APUBAPUB(,APUBPTR)                       01S0205
*             END;                  /* DO UNTIL APUBPTR = 0          */
@DE00194 LTR   APUBPTR,APUBPTR                                  01S0206
         BNZ   @DL00194                                         01S0206
*           ELSE                                                01S0207
*             ;                     /* 3838 NOT IPLED                */
@RF00193 DS    0H                                               01S0208
*           MSTRAPUB=MSTRAPUB->APUBMSTR;/* ON TO NEXT MASTER/DONE    */
         L     MSTRAPUB,APUBMSTR(,MSTRAPUB)                     01S0208
*         END;                      /* DO UNTIL MSTRAPUB = 0         */
