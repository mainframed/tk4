@RF00186 DS    0H                                               01S0227
*     SSDDRPTR=SSAGNRBP;            /* ON TO NEXT REQUEST OR DONE    */
@RC00186 L     SSDDRPTR,SSAGNRBP(,SSDDRPTR)                     01S0227
