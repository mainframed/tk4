*                   R1F UNRSTD;     /* DONE WITH REG                 */
*                 RFY                                           01S0222
*                   R0P UNRSTD;     /* UNABLE TO ALLOCATE WITH ...   */
*               END;                /* DO IF ALLMARK = BON           */
