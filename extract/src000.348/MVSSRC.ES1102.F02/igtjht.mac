*           DO;                     /* INDICATE AT LEAST ONE UNSATSFD*/
         B     @RC00210                                         01S0212
@RF00210 DS    0H                                               01S0213
*             NUMNTSAT=NUMNTSAT+1;  /* COUNT THEM                    */
         LA    @15,1                                            01S0213
