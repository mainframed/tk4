TJBA     TITLE 'IGTJBA -- VPSS SUBSYSTEM CLOSE INITIALIZATION          *
                        '
IGTJBA   CSECT ,                                                01S0001
@MAINENT DS    0H                                               01S0001
         USING *,@15                                            01S0001
         B     @PROLOG                                          01S0001
         DC    AL1(16)                                          01S0001
         DC    C'IGTJBA    77.092'                              01S0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                  01S0001
         BALR  @07,0                                            01S0001
@PSTART  DS    0H                                               01S0001
         USING @PSTART,@07                                      01S0001
*   APCTPTR=SSCTPTR->SSCTSUSE;      /* POINT TO APCT                 */
         LR    @06,SSCTPTR                                      01S0094
         L     APCTPTR,SSCTSUSE(,@06)                           01S0094
*   RSSOBPTR=R1P;                   /* SAVE SSOB PTR                 */
         LR    RSSOBPTR,R1P                                     01S0095
*   RFY                                                         01S0096
*    (R1P,                                                      01S0096
*     SSCTPTR) UNRSTD;              /* FINISHED WITH INPUT REGS      */
*   RFY                                                         01S0097
*     SSOB BASED(RSSOBPTR);         /* BASIC CLOSE CONTROL BLOCK     */
*   RFY                                                         01S0098
*     INFMJFCB BASED(SSDAJFCB);     /* JFCB                          */
*   RFY                                                         01S0099
*     DEBBASIC BASED(SSDADEBP);     /* DEB                           */
*   RFY                                                         01S0100
*     DEB BASED(ADDR(DEBBASIC)-LENGTH(DEB));/* PREFIX CONTIG.        */
*   RFY                                                         01S0101
*     APDX BASED(DEBIRBAD);         /* VPSS DEB EXTENSION            */
*   RFY                                                         01S0102
*     FORCORE BASED(RCORE);         /* JFCB IS CONTAINED IN O/C/EOV
*                                      WORKAREA. RCORE IS DUMMY PTR  */
*   RFY                                                         01S0103
*     IFGACB BASED((ADDR(INFMJFCB)+ /* COPY OF ACB LOCATED           */
*         (ADDR(DXPDCBAD)-ADDR(DXJBF)))/* IN PROTECTED               */
*         ->I031P);                 /* STORAGE.                      */
*   RFY                                                         01S0104
*     APCX BASED(ADDR(IFGACB)+      /* VPSS EXTENSION IS COPY TOO.   */
*         (LENGTH(IFGACB)+7)/8*8);  /* FOLLOWS ACB PROPER            */
*   RFY                                                         01S0105
*     APDS BASED(SSDASSCM);         /* VPSS ALLOCATION CONTROL BLOCK */
         EJECT
*                                                               01S0106
*   /*****************************************************************/
*   /*                                                               */
*   /* DETERMINE IF AT MOST APOC MUST BE FREED. IF SO, RELEASE THE   */
*   /* STORAGE AND RETURN TO THE CALLER.                             */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0106
*   DO;                             /* START OF CODE                 */
*                                                               02S0106
*     /***************************************************************/
*     /*                                                             */
*     /* CHECK FOR DEB (DEBIRBAD) POINTING TO APOC INSTEAD OF APDX.  */
*     /* THIS MAY OCCUR IF (1) OPEN FAILED BEFORE APDX WAS BUILT, (2)*/
*     /* CLOSE FAILED AFTER APDX WAS FREED. IF THIS CONDITION EXISTS,*/
*     /* JUST FREE THE PREVIOUS WORKAREA AND RETURN TO THE CALLER.   */
*     /* ALSO PROCESS 0 POINTER SINCE THIS MEANS EVEN THE APOC DOES  */
*     /* NOT EXIST. NOTE: DEBIRBAD POINTS TO APOC IF BIT 31 OF       */
*     /* ADDRESS IS ONE.                                             */
*     /*                                                             */
*     /***************************************************************/
*                                                               02S0107
*     IF(DEBIRBAD&'00000001'X)^=0 THEN/* IF BIT 31 OF DEBIRBAD       */
         SLR   @06,@06                                          02S0107
         L     @03,SSOBINDV(,RSSOBPTR)                          02S0107
         L     @03,SSDADEBP(,@03)                               02S0107
         L     @15,DEBIRBAD(,@03)                               02S0107
         N     @15,@CF03107                                     02S0107
         CR    @15,@06                                          02S0107
         BE    @RF00107                                         02S0107
*       DO;                         /* IS ON, IS APOC POINTER.       */
*         RFY                                                   02S0109
*          (R0F,                                                02S0109
*           R1P,                                                02S0109
*           R14P) RSTD;             /* REQUIRED REGISTERS            */
*         R14P=DEBIRBB&'FFFFFFFE'X; /* POINT TO APOC                 */
         L     R14P,DEBIRBB-1(,@03)                             02S0110
         LA    R14P,0(,R14P)                                    02S0110
         N     R14P,@CF03110                                    02S0110
*         RFY                                                   02S0111
*           APOC BASED(R14P);       /* TELL COMPILER                 */
*         DEBIRBB=0;                /* CLEAR FOR RECURSION/TERMINATE */
         STCM  @06,7,DEBIRBB(@03)                               02S0112
*         R0F=APOCLNI;              /* SUBPOOL/LENGTH                */
         L     R0F,APOCLNI(,R14P)                               02S0113
*         R1P=APOCADI;              /* SIZE                          */
         L     R1P,APOCADI(,R14P)                               02S0114
*         GEN(FREEMAIN R,LV=(0),A=(1));/* FREE THE STORAGE           */
         FREEMAIN R,LV=(0),A=(1)
*         RFY                                                   02S0116
*          (R0F,                                                02S0116
*           R1P,                                                02S0116
*           R14P) UNRSTD;           /* FINISHED WITH APOC            */
*         RFY                                                   02S0117
*           APOC;                   /* & REGISTERS                   */
*       END;                                                    02S0118
*     IF DEBIRBB=0 THEN             /* IF NO APDX OR APOC,           */
@RF00107 L     @06,SSOBINDV(,RSSOBPTR)                          02S0119
         L     @06,SSDADEBP(,@06)                               02S0119
         L     @06,DEBIRBB-1(,@06)                              02S0119
         LA    @06,0(,@06)                                      02S0119
         LTR   @06,@06                                          02S0119
         BNZ   @RF00119                                         02S0119
*       DO;                         /* THEN RETURN TO CALLER         */
*         SSOBRETN=SSDMOK;          /* SHOW CLOSE WORKED             */
         SLR   @06,@06                                          02S0121
         ST    @06,SSOBRETN(,RSSOBPTR)                          02S0121
*         RETURN CODE(0);           /* SUBSYSTEM REQUEST PROCESSED OK*/
         LR    @15,@06                                          02S0122
         L     @14,12(,@13)                                     02S0122
         LM    @00,@12,20(@13)                                  02S0122
         BR    @14                                              02S0122
*       END;                        /* FALL THRU FOR NORMAL CLOSE    */
*   END;                            /* PROCESSING OR OPEN RECOVERY.  */
@RF00119 DS    0H                                               02S0125
         EJECT
*                                                               01S0125
*   /*****************************************************************/
*   /*                                                               */
*   /* ACQUIRE SAVEAREA/WORKAREA AND BUILD APOC. IF APOC ALREADY     */
*   /* EXISTS (OPEN RECOVERY OR FORCE-CLOSE) USE THE EXISTING        */
*   /* SAVEAREA WORKAREA. OTHERWISE ISSUE A GETMAIN FOR A NEW ONE.   */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0125
*   IF APDXAPOC^=0 THEN             /* IF APOC ALREADY EXISTS.       */
         L     @06,SSOBINDV(,RSSOBPTR)                          01S0125
         L     @06,SSDADEBP(,@06)                               01S0125
         L     @06,DEBIRBAD(,@06)                               01S0125
         L     @06,APDXAPOC(,@06)                               01S0125
         LTR   @06,@06                                          01S0125
         BZ    @RF00125                                         01S0125
*     DO;                           /* START OF CODE                 */
*                                                               02S0126
*       /*************************************************************/
*       /*                                                           */
*       /* APOC/SA-WA REMAINS FROM PREVIOUS OPEN/CLOSE REQUEST.      */
*       /* RE-USE THIS AREA TO BUILD A NEW SA-WA AND CREATE AN APOC  */
*       /* USING THE CURRENT CONTENTS.                               */
*       /*                                                           */
*       /*************************************************************/
*                                                               02S0127
*       RFY                                                     02S0127
*        (R0P,                                                  02S0127
*         R1F,                                                  02S0127
*         R14P,                                                 02S0127
*         R15F,                                                 02S0127
*         APOCPTR) RSTD;            /* REQUIRED REGISTERS            */
*       R14P=APDXAPOC;              /* POINT TO OLD APOC             */
         LR    R14P,@06                                         02S0128
*       RFY                                                     02S0129
*         APOC BASED(R14P);         /* TELL COMPILER                 */
*       APOCPTR=APOCADI+            /* MOVE APOC TO END OF           */
*           (APOCLNI&'00FFFFFF'X)-  /* DYNAMIC AREA. THIS ALLOWS     */
*           APOCLN;                 /* NEW SA-WA AND APOC TO BE      */
         LA    @06,72                                           02S0130
         L     APOCPTR,APOCLNI(,R14P)                           02S0130
         N     APOCPTR,@CF03117                                 02S0130
         AL    APOCPTR,APOCADI(,R14P)                           02S0130
         SLR   APOCPTR,@06                                      02S0130
*       R0P=APOCPTR;                /* CREATED.                      */
         LR    R0P,APOCPTR                                      02S0131
*       R1F=APOCLN;                 /* USE MVCL TO MOVE APOC         */
         LR    R1F,@06                                          02S0132
*       R15F=R1F;                   /* IN CASE LENGTH OF             */
         LR    R15F,R1F                                         02S0133
*       MVCL(R0P,R14P);             /* APOC EXCEEDS 256 BYTES.       */
         MVCL  R0P,R14P                                         02S0134
*       RFY                                                     02S0135
*        (R0P,                                                  02S0135
*         R1F,                                                  02S0135
*         R14P,                                                 02S0135
*         R15F) UNRSTD;             /* FINISHED WITH MVCL REGISTERS  */
*       RFY                                                     02S0136
*         APOC BASED(APOCPTR);      /* TELL COMPILER OF NEW POINTER  */
*       APDXAPOC=ADDR(APOC);        /* SAVE NEW PTR SINCE DESTROYING
*                                      OLD APOC AREA.                */
         L     @11,SSOBINDV(,RSSOBPTR)                          02S0137
         L     @11,SSDADEBP(,@11)                               02S0137
         L     @11,DEBIRBAD(,@11)                               02S0137
         ST    APOCPTR,APDXAPOC(,@11)                           02S0137
*       RFY                                                     02S0138
*        (R0F,                                                  02S0138
*         R1P) RSTD;                /* REQUIRED REGISTERS            */
*       R0F=APOCLNI;                /* BUILD SA-WA SKELETON          */
         L     R0F,APOCLNI(,APOCPTR)                            02S0139
*       R1P=APOCADI;                /* R0 = SUBPOOL & LENGTH         */
         L     R1P,APOCADI(,APOCPTR)                            02S0140
*       CALL APCVZUAF;              /* R1 = DYNAMIC AREA ADDRESS ON
*                                      RETURN: R1->APSC.             */
         L     @15,APCTZUAF(,APCTPTR)                           02S0141
         BALR  @14,@15                                          02S0141
*       R0F=(APSVLN+APSCLN+@SIZDATD+7)/* ALLOW FOR APSV, APSC, PL/S  */
*           &'0000FFF8'X;           /* WORKAREA, & ROUND TO DWORD.   */
         LA    R0F,95                                           02S0142
         AL    R0F,@SIZDATD                                     02S0142
         N     R0F,@CF03120                                     02S0142
*       R1P->APSCCRSZ=R0F;          /* SET USED SIZE IN APSC         */
         STH   R0F,APSCCRSZ(,R1P)                               02S0143
*       R1P=R1P+APSCLN;             /* POINT TO NEW OS SAVEAREA      */
         LA    R1P,8(,R1P)                                      02S0144
*       RFY                                                     02S0145
*         R0F UNRSTD;               /* FINISHED WITH REGISTER        */
*                                                               02S0145
*       /*************************************************************/
*       /*                                                           */
*       /* BUILD SAVEAREA. POINT TO DYNAMIC AREA.                    */
*       /*                                                           */
*       /*************************************************************/
*                                                               02S0146
*       APSVNEXT=R1P;               /* FORWARD CHAIN                 */
         ST    R1P,APSVNEXT(,APSVPTR)                           02S0146
*       R1P->APSVPREV=APSVPTR;      /* BACK CHAIN                    */
         ST    APSVPTR,APSVPREV(,R1P)                           02S0147
*       APSVPTR=R1P;                /* POINT TO NEW SAVEAREA         */
         LR    APSVPTR,R1P                                      02S0148
*       RFY                                                     02S0149
*         R1P UNRSTD;               /* FINISHED WITH REGISTER        */
*       RFY                                                     02S0150
*        (APDATA) RSTD;             /* POINT TO PL/S DYNAMIC AREA    */
*       APDATA=ADDR(APSV)+APSVLN;   /* AFTER SAVEAREA.               */
         LA    APDATA,80(,APSVPTR)                              02S0151
*       GEN(USING @DATD,APDATA)                                 02S0152
*       NOSETS REFS(APDATA);        /* TELL ASSEMBLER                */
         USING @DATD,APDATA
*       RFY                                                     02S0153
*        (R0P,                                                  02S0153
*         R1F,                                                  02S0153
*         R14P,                                                 02S0153
*         R15F) RSTD;               /* MVCL REGISTERS                */
*       R0P=ADDR(DAPOC);            /* MOVE APOC TO DAPOC            */
         LA    R0P,DAPOC                                        02S0154
*       R14P=ADDR(APOC);            /* CURRENT APOC ADDRESS          */
         LR    R14P,APOCPTR                                     02S0155
*       R1F=APOCLN;                 /* LENGTH OF APOC                */
         LA    R1F,72                                           02S0156
*       R15F=R1F;                   /* MOVE ENTIRE APOC              */
         LR    R15F,R1F                                         02S0157
*       MVCL(R0P,R14P);             /* COPY SAVED APOC               */
         MVCL  R0P,R14P                                         02S0158
*       RFY                                                     02S0159
*        (R0P,                                                  02S0159
*         R1F,                                                  02S0159
*         R14P,                                                 02S0159
*         R15F) UNRSTD;             /* TO NEW                        */
*       APOCPTR=ADDR(DAPOC);        /* APOC WORKAREA, AND UPDATE     */
         LA    APOCPTR,DAPOC                                    02S0160
*       APDXAPOC=ADDR(APOC);        /* REGISTER & APDX POINTERS.     */
         L     @11,SSOBINDV(,RSSOBPTR)                          02S0161
         L     @11,SSDADEBP(,@11)                               02S0161
         L     @03,DEBIRBAD(,@11)                               02S0161
         ST    APOCPTR,APDXAPOC(,@03)                           02S0161
*       APOCDACB=DEBDCBB;           /* ADDR OF ACB POINTED TO BY DEB */
         L     @03,DEBDCBB-1(,@11)                              02S0162
         LA    @03,0(,@03)                                      02S0162
         ST    @03,APOCDACB(,APOCPTR)                           02S0162
*       APOCUKEY=DEBPROTG/16;       /* USER'S PROTECT KEY (BITS 4-7) */
         SLR   @03,@03                                          02S0163
         IC    @03,DEBPROTG(,@11)                               02S0163
         SRL   @03,4                                            02S0163
         STC   @03,APOCUKEY(,APOCPTR)                           02S0163
*       MVO(APOCUKEY,APOCUKEY);     /* SET BITS 0-3 TO USER KEY ALSO */
         MVO   APOCUKEY(1,APOCPTR),APOCUKEY(1,APOCPTR)          02S0164
*       APOCSFC=BON;                /* SHOW FORCE CLOSE PROCESSING.  */
         OI    APOCSFC(APOCPTR),B'00010000'                     02S0165
*     END;                          /* OLD SA-WA USED. NEW SA-WA AND
*                                      APOC BUILT.                   */
*   ELSE                            /* OTHERWISE                     */
         EJECT
*     DO;                           /* START OF CODE SEGMENT         */
         B     @RC00125                                         02S0167
@RF00125 DS    0H                                               02S0168
*       GEN(DROP APDATA)                                        02S0168
*       NOSETS REFS(APDATA);        /* APDATA, APOCPTR NOT SET ON    */
         DROP APDATA
*       RFY                                                     02S0169
*        (APOC,                                                 02S0169
*         APOCPTR,                                              02S0169
*         APDATA);                  /* ENTRY TO THIS SEGMENT.        */
*                                                               02S0169
*       /*************************************************************/
*       /*                                                           */
*       /* BUILD AND INITIALIZE A SAVEAREA/WORKAREA AND AN APOC.     */
*       /* ISSUE A CONDITIONAL GETMAIN FOR THE REQUESTED SA/WA SIZE. */
*       /* IF THE STORAGE IS UNAVAILABLE, ISSUE AN UNCONDITIONAL     */
*       /* GETMAIN FROM REGION (CSA IF END-OF-TASK SUBSYSTEM EXIT IN */
*       /* PROGRESS). INITIALIZE THE SAVEAREA/WORKAREA (VIA IGTUAF)  */
*       /* AND CONSTRUCT AN APOC.                                    */
*       /*                                                           */
*       /*************************************************************/
*                                                               02S0170
*       RFY                         /* REQUIRED REGISTERS            */
*        (RSP,                      /* DYNAMIC AREA SUBPOOL          */
*         RLV,                      /* DYNAMIC AREA LENGTH           */
*         RADDR,                    /* DYNAMIC AREA ADDRESS          */
*         R1P,                      /* GETMAIN RETURNED AREA ADDRESS */
*         R15F) RSTD;               /* GETMAIN RETURN CODE           */
*       RSP=CNORMSP;                /* NORMAL SUBPOOL                */
         LA    RSP,230                                          02S0171
*       RLV=CNORMLV;                /* NORMAL LENGTH REQUIREMENT     */
         L     RLV,@CF03081                                     02S0172
*       GEN REFS(RSP,RLV);                                      02S0173
*                                   /*
         GETMAIN RC,               ISSUE CONDITIONAL GETMAIN           *
               LV=(RLV),           * REQUIRED LENGTH                   *
               SP=(RSP),           * REQUIRED SUBPOOL                  *
               BNDRY=PAGE          * START ON PAGE BOUNDARY            *
                                   * R1P->STORAGE. R15F HAS RETURN CODE
*       IF R15F^=0 THEN             /* IS STORAGE NOT ACQUIRED,      */
         LTR   R15F,R15F                                        02S0174
         BZ    @RF00174                                         02S0174
*         DO;                       /* TRY A DIFFERENT SUBPOOL.      */
*           IF APDXSEOT=BON THEN    /* IF VPSS SUBSYSTEM END-OF-TASK */
         L     @14,SSOBINDV(,RSSOBPTR)                          02S0176
         L     @14,SSDADEBP(,@14)                               02S0176
         L     @14,DEBIRBAD(,@14)                               02S0176
         TM    APDXSEOT(@14),B'01000000'                        02S0176
         BNO   @RF00176                                         02S0176
*             DO;                   /* CALL, USE GLOBAL STORAGE.     */
*               RSP=CEOTSP;         /* SUBPOOL                       */
         LA    RSP,241                                          02S0178
*               RLV=CEOTLV;         /* STORAGE SIZE                  */
         LA    RLV,2048                                         02S0179
*             END;                  /* EOT SET                       */
*           ELSE                    /* OTHERWISE                     */
*             DO;                   /* USE REGION FOR STORAGE.       */
         B     @RC00176                                         02S0181
@RF00176 DS    0H                                               02S0182
*               RSP=CEMERSP;        /* SUBPOOL                       */
         LA    RSP,230                                          02S0182
*               RLV=CEMERLV;        /* LENGTH                        */
         LA    RLV,2048                                         02S0183
*             END;                  /* REGION PARMS SET              */
*           GEN REFS(RSP,RLV);                                  02S0185
@RC00176 DS    0H                                               02S0185
*                                   /*
         GETMAIN RU,               ISSUE UNCONDITIONAL GETMAIN         *
               LV=(RLV),           * REQUIRED LENGTH                   *
               SP=(RSP),           * REQUIRED SUBPOOL                  *
               BNDRY=DBLWD         * NO SPECIAL ALIGNMENT              *
                                   * R1 -> ACQUIRED STORAGE
*         END;                                                  02S0186
*       RADDR=R1P;                  /* SAVE ADDR ACQUIRED STORAGE    */
@RF00174 LR    RADDR,R1P                                        02S0187
*       RFY                                                     02S0188
*        (R0F,                                                  02S0188
*         R1P) UNRSTD;              /* GETMAIN REGISTERS NOT NEEDED. */
*                                                               02S0188
*       /*************************************************************/
*       /*                                                           */
*       /* STORAGE NOW OBTAINED. 'RADDR' CONTAINS ITS ADDRESS, 'RSP' */
*       /* ITS SUBPOOL, AND 'RLV' ITS LENGTH. CONSTRUCT A            */
*       /* SAVEAREA/WORKAREA USING IGTUAF. ON INPUT TO THIS PROGRAM, */
*       /* REGISTER 1 MUST CONTAIN THE DYNAMIC STORAGE ADDRESS.      */
*       /* REGISTER 0 MUST HAVE THE SUBPOOL NUMBER IN BYTE 0, AND THE*/
*       /* LENGTH OF THE AREA IN BYTES 1-3. ON RETURN, REGISTER 1    */
*       /* CONTAINS THE ADDRESS OF THE APSC FOR THE NEW SAVEAREA.    */
*       /* THEN CHAIN THE SAVEAREAS TOGETHER.                        */
*       /*                                                           */
*       /*************************************************************/
*                                                               02S0189
*       RFY                                                     02S0189
*        (R0F,                                                  02S0189
*         R1P) RSTD;                /* NEEDED REGISTERS              */
*       R0F=RSP;                    /* SUBPOOL                       */
         LR    R0F,RSP                                          02S0190
*       SLL(R0F,24);                /* SUBPOOL IN BYTE 0             */
         SLL   R0F,24                                           02S0191
*       R0F=R0F×RLV;                /* LENGTH IN BYTES 1-3.          */
         OR    R0F,RLV                                          02S0192
*       R1P=RADDR;                  /* ADDRESS OF DYNAMIC STORAGE    */
         LR    R1P,RADDR                                        02S0193
*       CALL APCVZUAF;              /* INITIALIZE SA-WA. ON RETURN,
*                                      R1 -> APSC.                   */
         L     @15,APCTZUAF(,APCTPTR)                           02S0194
         BALR  @14,@15                                          02S0194
*       R0F=(APSVLN+APSCLN+@SIZDATD+7)/* ALLOW FOR APSV, APSC, AND   */
*           &'0000FFF8'X;           /* DYNAMIC AREA. ROUND TO DWORD. */
         LA    R0F,95                                           02S0195
         AL    R0F,@SIZDATD                                     02S0195
         N     R0F,@CF03120                                     02S0195
*       R1P->APSCCRSZ=R0F;          /* SET USED SIZE IN APSC         */
         STH   R0F,APSCCRSZ(,R1P)                               02S0196
*       R1P=R1P+APSCLN;             /* SKIP APSC. NOW AT APSV.       */
         LA    R1P,8(,R1P)                                      02S0197
*       RFY                                                     02S0198
*         R0F UNRSTD;               /* FINISHED WITH REGISTER 0      */
*                                                               02S0198
*       /*************************************************************/
*       /*                                                           */
*       /* BUILD SAVEAREA. POINT TO DYNAMIC AREA.                    */
*       /*                                                           */
*       /*************************************************************/
*                                                               02S0199
*       APSVNEXT=R1P;               /* SAVE CURRENT SAVEAREA ADDRESS */
         ST    R1P,APSVNEXT(,APSVPTR)                           02S0199
*       R1P->APSVPREV=APSVPTR;      /* KEEP PREVIOUS SA FOR EXIT     */
         ST    APSVPTR,APSVPREV(,R1P)                           02S0200
*       APSVPTR=R1P;                /* POINT TO NEW SAVEAREA         */
         LR    APSVPTR,R1P                                      02S0201
*       RFY                                                     02S0202
*         R1P UNRSTD;               /* FINISHED WITH REGISTER 1      */
*       RFY                                                     02S0203
*         APDATA RSTD;              /* SET APDATA TO ADDRESS         */
*       APDATA=ADDR(APSV)+APSVLN;   /* OF PL/S DYNAMIC AREA          */
         LA    APDATA,80(,APSVPTR)                              02S0204
*       GEN(USING @DATD,APDATA)                                 02S0205
*       NOSETS REFS(APDATA);        /* (AFTER SAVEAREA).             */
         USING @DATD,APDATA
*                                                               02S0206
*       /*************************************************************/
*       /*                                                           */
*       /* BUILD THE COMMON VPSS OPEN/CLOSE WORKAREA (APOC). THIS    */
*       /* AREA CONTAINS THE POINTERS AND DATA USED BY THE VPSS      */
*       /* PROCESSING ROUTINES. REGISTER 'APOCPTR' IS RESERVED TO    */
*       /* POINT TO THIS CONTROL BLOCK.                              */
*       /*                                                           */
*       /*************************************************************/
*                                                               02S0206
*       RFY                                                     02S0206
*         APOCPTR RSTD;             /* APOC PTR                      */
*       APOCPTR=ADDR(DAPOC);        /* APOC BUILD AREA               */
         LA    APOCPTR,DAPOC                                    02S0207
*       RFY                                                     02S0208
*        (R0P,                                                  02S0208
*         R1F,                                                  02S0208
*         R15F) RSTD;               /* NEEDED REGISTERS              */
*       R0P=APOCPTR;                /* SET APOC TO ZEROES.           */
         LR    R0P,APOCPTR                                      02S0209
*       R1F=APOCLN;                 /* USE MVCL IN CASE APOC         */
         LA    R1F,72                                           02S0210
*       R15F=0;                     /* EXPANDS BEYOND 256 BYTES.     */
         SLR   R15F,R15F                                        02S0211
*       MVCL(R0P,R14P);                                         02S0212
         MVCL  R0P,R14P                                         02S0212
*       RFY                                                     02S0213
*        (R0P,                                                  02S0213
*         R1F,                                                  02S0213
*         R15F) UNRSTD;             /* FINISHED WITH REGS            */
*       RFY                                                     02S0214
*         APOC BASED(APOCPTR);      /* TELL COMPILER OF BASE         */
*                                                               02S0214
*       /*************************************************************/
*       /*                                                           */
*       /* INITIALIZE POINTERS                                       */
*       /*                                                           */
*       /*************************************************************/
*                                                               02S0215
*       APOCID=APOCIDC;             /* IN-CORE ID                    */
         MVC   APOCID(4,APOCPTR),@CC03004                       02S0215
*       APOCLNI=RLV;                /* ACQUIRED DYNAMIC LENGTH       */
         ST    RLV,APOCLNI(,APOCPTR)                            02S0216
*       APOCLNIS=RSP;               /* SUBPOOL IN HI-BYTE OF LN.     */
         STC   RSP,APOCLNIS(,APOCPTR)                           02S0217
*       APOCADI=RADDR;              /* DYNAMIC AREA ADDRESS          */
         ST    RADDR,APOCADI(,APOCPTR)                          02S0218
*       RFY                                                     02S0219
*        (RLV,                                                  02S0219
*         RSP,                                                  02S0219
*         RADDR) UNRSTD;            /* FINISHED WITH REGISTERS       */
*       APOCSSOB=ADDR(SSOB);        /* SSOB ADDRESS                  */
         ST    RSSOBPTR,APOCSSOB(,APOCPTR)                      02S0220
*       APOCJFCB=ADDR(INFMJFCB);    /* JFCB ADDRESS                  */
         L     @11,SSOBINDV(,RSSOBPTR)                          02S0221
         L     @03,SSDAJFCB(,@11)                               02S0221
         ST    @03,APOCJFCB(,APOCPTR)                           02S0221
*       APOCDEB=ADDR(DEBBASIC);     /* DEB                           */
         L     @15,SSDADEBP(,@11)                               02S0222
         ST    @15,APOCDEB(,APOCPTR)                            02S0222
*       APOCAPDX=ADDR(APDX);        /* VPSS DEB EXTENSION            */
         L     @14,DEBIRBAD(,@15)                               02S0223
         ST    @14,APOCAPDX(,APOCPTR)                           02S0223
*       APOCOCEW=ADDR(INFMJFCB)-    /* JFCB IN SYSTEM O/C/EOV        */
*           (ADDR(DXJBF)-ADDR(FORCORE));/* WORKAREA.                 */
         LR    @01,@03                                          02S0224
         SL    @01,@CF03238                                     02S0224
         ST    @01,APOCOCEW(,APOCPTR)                           02S0224
*       APOCUACB=APOCOCEW->DXUDCBAD;/* USER ACB ADDRESS              */
         L     @09,DXUDCBAD(,@01)                               02S0225
         ST    @09,APOCUACB(,APOCPTR)                           02S0225
*       APOCACB=ADDR(IFGACB);       /* PROTECTED (FAKE) ACB          */
         L     @03,I031P+472(,@03)                              02S0226
         ST    @03,APOCACB(,APOCPTR)                            02S0226
*       APOCAPDS=ADDR(APDS);        /* VPSS ALLOCATION BLOCK         */
         L     @11,SSDASSCM(,@11)                               02S0227
         ST    @11,APOCAPDS(,APOCPTR)                           02S0227
*       APOCDACB=DEBDCBB;           /* ADDR ACB POINTED TO BY DEB    */
         L     @11,DEBDCBB-1(,@15)                              02S0228
         LA    @11,0(,@11)                                      02S0228
         ST    @11,APOCDACB(,APOCPTR)                           02S0228
*       APOCUKEY=DEBPROTG/16;       /* USER'S PROTECT KEY (BITS 4-7) */
         SLR   @11,@11                                          02S0229
         IC    @11,DEBPROTG(,@15)                               02S0229
         SRL   @11,4                                            02S0229
         STC   @11,APOCUKEY(,APOCPTR)                           02S0229
*       MVO(APOCUKEY,APOCUKEY);     /* SET BITS 0-3 TO USER'S KEY    */
         MVO   APOCUKEY(1,APOCPTR),APOCUKEY(1,APOCPTR)          02S0230
*       APOCSOPN=BOFF;              /* SHOW CLOSE                    */
         NI    APOCSOPN(APOCPTR),B'01111111'                    02S0231
*       APDXAPOC=ADDR(APOC);        /* SAVE APOC POINTER             */
         ST    APOCPTR,APDXAPOC(,@14)                           02S0232
*     END;                          /* APOC CREATED                  */
         EJECT
*                                                               01S0234
*   /*****************************************************************/
*   /*                                                               */
*   /* ESTABLISH CONTROL BLOCK POINTERS                              */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0234
*   RFY                                                         01S0234
*    (APOCPTR) RSTD;                /* APOC ADDR PASSED IN REGISTER  */
@RC00125 DS    0H                                               01S0235
*   RFY                                                         01S0235
*     APOC BASED(APOCPTR);          /* APOC POINTS TO ALL            */
*   RFY                                                         01S0236
*     SSOB BASED(APOCSSOB);         /* OTHER MAJOR VPSS              */
*   RFY                                                         01S0237
*     INFMJFCB BASED(APOCJFCB);     /* SUBSYSTEM CONTROL             */
*   RFY                                                         01S0238
*     DEBBASIC BASED(APOCDEB);      /* BLOCKS REQUIRED FOR           */
*   RFY                                                         01S0239
*     DEB BASED(ADDR(DEBBASIC)-LENGTH(DEB));/* OPEN PROCESSING.      */
*   RFY                                                         01S0240
*     APDS BASED(APOCAPDS);         /* ALLOCATION DATA               */
*   RFY                                                         01S0241
*     IFGACB BASED(APOCACB);        /* ACB & APCX ARE DUMMY COPIES   */
*   RFY                                                         01S0242
*     APCX BASED(ADDR(IFGACB)+      /* SO ACBAMAX IS INVALID         */
*         (LENGTH(IFGACB)+7)/8*8);  /* (APCX STILL AFTER IFGACB.     */
*   RFY                                                         01S0243
*     APDX BASED(DEBIRBAD);         /* VPSS DEB EXTENSION            */
*   RFY                                                         01S0244
*     IFGEXLST BASED(ACBEXLST);     /* EXIT LIST ROUTINES/ADDRESSES  */
*                                                               01S0244
*   /*****************************************************************/
*   /*                                                               */
*   /* INDICATE TYPE OF CLOSE AND PROCESSING REQUIRED. FOLLOWING     */
*   /* CONDITIONS ARE CHECKED: (1) VPSS END-OF-TASK EXIT, (2) JOBSTEP*/
*   /* ABEND ISSUEING CLOSE, (3) FORCE-CLOSE ENTRY. BITS ARE SET IN  */
*   /* THE APOC TO INDICATE ANY OF THE ABOVE CONDITIONS.             */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0245
*   IF TCBJSTCB=ADDR(TCB)&          /* IF JOBSTEP TASK               */
*       TCBFA=BON THEN              /* IS ABENDING,                  */
         L     @11,TCBPTR                                       01S0245
         C     @11,TCBJSTCB(,@11)                               01S0245
         BNE   @RF00245                                         01S0245
         TM    TCBFA(@11),B'10000000'                           01S0245
         BNO   @RF00245                                         01S0245
*     APOCSAB=BON;                  /* INDICATE IN APOC.             */
         OI    APOCSAB(APOCPTR),B'00001000'                     01S0246
*   IF APDXSEOT=BON THEN            /* IF EOT FLAG SET,              */
@RF00245 L     @11,APOCDEB(,APOCPTR)                            01S0247
         L     @11,DEBIRBAD(,@11)                               01S0247
         TM    APDXSEOT(@11),B'01000000'                        01S0247
         BNO   @RF00247                                         01S0247
*     APOCSEOT=BON;                 /* INDICATE IN APOC. NOTE: FORCE
*                                      CLOSE ALREADY SET IF     01S0248
*                                      APPROPRIATE.                  */
         OI    APOCSEOT(APOCPTR),B'00000100'                    01S0248
         EJECT
*                                                               01S0249
*   /*****************************************************************/
*   /*                                                               */
*   /* CALL THE APPROPRIATE CLOSE PROCESSING ROUTINE. IF OPEN FAILED,*/
*   /* CALL IGTJAP TO RESET THE ACB (TREAT LIKE OPEN-ERROR           */
*   /* PROCESSING.) IF THE ACB IS OPEN, CALL IGTJBD TO CLOSE THE ACB.*/
*   /* ON RETURN, DEBIRBAD CONTAINS APOC ADDRESS. INPUT TO EITHER    */
*   /* MODULE IS THE APOC ADDRESS, CONTAINED IN REGISTER 'APOCPTR'.  */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0249
*   IF APOCSOPN=BON THEN            /* IF DEB IN PROCESS OF BEING    */
@RF00247 TM    APOCSOPN(APOCPTR),B'10000000'                    01S0249
         BNO   @RF00249                                         01S0249
*     CALL APCVZJAP;                /* OPENED, CALL OPEN-RESET.      */
         L     @15,APCTZJAP(,APCTPTR)                           01S0250
         BALR  @14,@15                                          01S0250
*   ELSE                            /* IF DEB FULLY OPENED, THEN     */
*     CALL APCVZJBD;                /* CALL NORMAL CLOSE PROCESSOR.  */
*                                                               01S0251
         B     @RC00249                                         01S0251
@RF00249 L     @15,APCTZJBD(,APCTPTR)                           01S0251
         BALR  @14,@15                                          01S0251
*   /*****************************************************************/
*   /*                                                               */
*   /* DEBIRBAD NOW POINTS TO APOC. IF THIS IS NOT A JOBSTEP ABEND OR*/
*   /* AN END-OF-TASK CALL, AND THE DEB DOES NOT POINT TO THE        */
*   /* PROTECTED VERSION OF THE ACB (NORMAL CASE), COPY THE MODIFIED */
*   /* FIELDS OF THE PROTECTED ACB TO THE USER'S ACB. THIS IS        */
*   /* REQUIRED SINCE NORMAL CLOSE WILL REFRESH THE PROTECTED ACB    */
*   /* FROM THE USER'S ACB, THUS DESTROYING THE MODIFICATIONS MADE BY*/
*   /* CLOSE.                                                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0252
*   IF APOCSEOT=BOFF&               /* IF NOT END-OF-TASK, AND       */
*       APOCSAB=BOFF&               /* NOT A JOBSTEP ABEND, AND      */
*       APOCPACB^=APOCDACB THEN     /* DEB DOES NOT POINT TO THE     */
@RC00249 TM    APOCSEOT(APOCPTR),B'00001100'                    01S0252
         BNZ   @RF00252                                         01S0252
         L     @11,APOCPACB(,APOCPTR)                           01S0252
         C     @11,APOCDACB(,APOCPTR)                           01S0252
         BE    @RF00252                                         01S0252
*     DO;                           /* PROTECTED COPY, THEN DO--     */
*       SPKA(APOCUKEY);             /* SWITCH TO USER KEY            */
         SLR   @03,@03                                          01S0254
         IC    @03,APOCUKEY(,APOCPTR)                           01S0254
         SPKA  0(@03)                                           01S0254
*       APOCUACB->ACBAMWAP=APOCPACB->ACBAMWAP;/* MOVE CHANGED        */
         L     @03,APOCUACB(,APOCPTR)                           01S0255
         L     @15,ACBAMWAP(,@11)                               01S0255
         ST    @15,ACBAMWAP(,@03)                               01S0255
*       APOCUACB->ACBSTRNO=APOCPACB->ACBSTRNO;/* FIELDS FROM         */
         IC    @15,ACBSTRNO(,@11)                               01S0256
         STC   @15,ACBSTRNO(,@03)                               01S0256
*       IF APOCPACB->ACBLENG>=((LENGTH(IFGACB)+7)/8*8+/* PROTECTED   */
*           APCXLN) THEN            /* COPY OF ACB                   */
         LH    @15,ACBLENG(,@11)                                01S0257
         C     @15,@CF03241                                     01S0257
         BL    @RF00257                                         01S0257
*         EVAL(APOCUACB+(LENGTH(IFGACB)+7)/8*8)->APCX=/* TO THE      */
*             EVAL(APOCPACB+(LENGTH(IFGACB)+7)/8*8)->APCX;/* USER'S  */
         LA    @15,80                                           01S0258
         ALR   @03,@15                                          01S0258
         ALR   @11,@15                                          01S0258
         MVC   APCX(40,@03),APCX(@11)                           01S0258
*       GEN(MODESET EXTKEY=DATAMGT)                             01S0259
*       NOREFS NOSETS;              /* VERSION OF THE ACB.           */
@RF00257 DS    0H                                               01S0259
         MODESET EXTKEY=DATAMGT
*     END;                          /* USER'S ACB NOW UPDATED        */
*                                                               01S0260
*   /*****************************************************************/
*   /*                                                               */
*   /* USER'S ACB NOW UPDATED. FREE THE SAVEAREA-WORKAREA FOR THIS   */
*   /* ROUTINE. MUST KEEP THE SSOB ADDRESS IN A REGISTER SINCE THE   */
*   /* APOC IS BEING RELEASED (SSOB POINTS TO OTHER MAJOR CONTROL    */
*   /* BLOCKS). NOTE THAT THE APDS IS NO LONGER LOCKED AND MAY NO    */
*   /* LONGER BE UPDATED (REFERENCE OK).                             */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0261
*   DEBIRBB=0;                      /* FINISHED WITH APOC PTR        */
*                                                               01S0261
@RF00252 L     @11,APOCDEB(,APOCPTR)                            01S0261
         SLR   @03,@03                                          01S0261
         STCM  @03,7,DEBIRBB(@11)                               01S0261
*   /*****************************************************************/
*   /*                                                               */
*   /* FREE THE SAVEAREA-WORKAREA FOR THIS ROUTINE. MUST KEEP THE    */
*   /* SSOB ADDRESS IN A REGISTER SINCE THE APOC IS BEING RELEASED   */
*   /* (SSOB POINTS TO OTHER MAJOR CONTROL BLOCKS).                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0262
*   RFY                                                         01S0262
*     RSSOBPTR RSTD;                /* -> SSOB                       */
*   RSSOBPTR=ADDR(SSOB);            /* POINT TO THE SSOB             */
         L     RSSOBPTR,APOCSSOB(,APOCPTR)                      01S0263
*   RFY                                                         01S0264
*     SSOB BASED(RSSOBPTR);         /* TELL THE COMPILER             */
*   RFY                                                         01S0265
*     INFMJFCB BASED(SSDAJFCB);     /* JOB FILE CONTROL BLOCK        */
*   RFY                                                         01S0266
*     DEBBASIC BASED(SSDADEBP);     /* DATA EXTENT BLOCK             */
*   RFY                                                         01S0267
*     DEB BASED(ADDR(DEBBASIC)-LENGTH(DEB));/* DEB PREFIX            */
*   RFY                                                         01S0268
*     FORCORE BASED(RCORE);         /* JFCB IS CONTAINED IN O/C/EOV
*                                      WORKAREA. RCORE IS DUMMY PTR  */
*   RFY                                                         01S0269
*     IFGACB BASED((ADDR(INFMJFCB)+ /* COPY OF ACB LOCATED           */
*         (ADDR(DXPDCBAD)-ADDR(DXJBF)))/* IN PROTECTED               */
*         ->I031P);                 /* STORAGE.                      */
*   RFY                                                         01S0270
*     APCX BASED(ADDR(IFGACB)+      /* VPSS ACB CALL EXTENSION IS    */
*         (LENGTH(IFGACB)+7)/8*8);  /* COPIED BEHIND ACB.            */
*   RFY                                                         01S0271
*     APDS BASED(SSDASSCM);         /* APDS CREATED BY SS ALLOCATION */
*   APSVPTR=APSVPREV;               /* ADDRESS OF PREVIOUS SAVEAREA  */
         L     APSVPTR,APSVPREV(,APSVPTR)                       01S0272
*   RFY                                                         01S0273
*    (R0F,                                                      01S0273
*     R1P) RSTD;                    /* REQUIRED FREEMAIN REGISTERS   */
*   R0F=APOCLNI;                    /* SUBPOOL/LENGTH OF AREA        */
         L     R0F,APOCLNI(,APOCPTR)                            01S0274
*   R1P=APOCADI;                    /* ADDRESS OF AREA               */
         L     R1P,APOCADI(,APOCPTR)                            01S0275
*   GEN(FREEMAIN R,LV=(0),A=(1))    /* FREE THE STORAGE              */
*   REFS(R0F,R1P);                  /* APOC ELIMINATED. R13 POINTING
*                                      TO INPUT (CALLER) SAVEAREA    */
         FREEMAIN R,LV=(0),A=(1)
*   RFY                                                         01S0277
*     APOC;                         /* APOC NO LONGER ADDRESSABLE    */
*   GEN(DROP  APDATA)               /* NO LONGER ADDRESS DYNAMIC AREA*/
*   NOSETS REFS(APDATA);            /* SINCE NOW FREEMAINED          */
         DROP  APDATA
*   RFY                                                         01S0279
*    (R0F,                                                      01S0279
*     R1P) UNRSTD;                  /* FINISHED WITH FREEMAIN REGS   */
         EJECT
*                                                               01S0280
*   /*****************************************************************/
*   /*                                                               */
*   /* INDICATE IN THE SSOB THAT THE CLOSE WAS SUCCESSFUL. THEN      */
*   /* RETURN TO THE CALLER.                                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0280
*   SSOBRETN=SSDMOK;                /* SHOW CLOSE WORKED.            */
         SLR   @11,@11                                          01S0280
         ST    @11,SSOBRETN(,RSSOBPTR)                          01S0280
*   RETURN CODE(0);                 /* RETURN TO COMMON CLOSE.       */
         LR    @15,@11                                          01S0281
         L     @14,12(,@13)                                     01S0281
         LM    @00,@12,20(@13)                                  01S0281
         BR    @14                                              01S0281
         EJECT
