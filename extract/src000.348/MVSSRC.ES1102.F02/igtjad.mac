TJAD     TITLE 'IGTJAD -- PROCESS ACB-OPEN DEFAULTS                    *
                        '
IGTJAD   CSECT ,                                                01S0001
@MAINENT DS    0H                                               01S0001
         USING *,@15                                            01S0001
         B     @PROLOG                                          01S0001
         DC    AL1(16)                                          01S0001
         DC    C'IGTJAD    77.092'                              01S0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                  01S0001
         BALR  @07,0                                            01S0001
@PSTART  DS    0H                                               01S0001
         USING @PSTART,@07                                      01S0001
         L     @00,@SIZDATD                                     01S0001
         IGTYGSV  R,LV=(0)
         LR    @06,@01                                          01S0001
         USING @DATD,@06                                        01S0001
         ST    @13,@SA00001+4                                   01S0001
         LM    @00,@01,20(@13)                                  01S0001
         ST    @06,8(,@13)                                      01S0001
         LR    @13,@06                                          01S0001
*                                                               01S0075
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE MODIFICATION MASK                                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0075
*   APOCMSK=APOCMSK&&APOCMSK;       /* SET MASK TO ZEROS             */
*                                                               01S0075
         XC    APOCMSK(8,APOCPTR),APOCMSK(APOCPTR)              01S0075
*   /*****************************************************************/
*   /*                                                               */
*   /* PROCESS OWN= INDICATOR                                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0076
*   IF APCXOWN=BON&                 /* IF USER REQUESTS EXCLUSIVE    */
*       APDSOWN=BOFF THEN           /* ALLOCATION, BUT SHARED GIVEN  */
         L     @11,APOCACB(,APOCPTR)                            01S0076
         TM    APCXOWN+80(@11),B'10000000'                      01S0076
         BNO   @RF00076                                         01S0076
         L     @01,APOCAPDS(,APOCPTR)                           01S0076
         TM    APDSOWN(@01),B'10000000'                         01S0076
         BNZ   @RF00076                                         01S0076
*     DO;                           /* IS ERROR. FAIL THE REQUEST    */
*       ACBERFLG=APCCS001;          /* GIVE ERROR NUMBER             */
         MVI   ACBERFLG(@11),X'01'                              01S0078
*       GOTO EXIT;                  /* RETURN NON-ZERO CODE          */
         B     EXIT                                             01S0079
*     END;                          /* PROCESSED DOWNGRADE REQUEST   */
*   IF APCXOWN=BOFF&                /* IF SHARED ALLOCATION REQUESTED*/
*       APDSOWN=BON THEN            /* BUT EXCLUSIVE GIVEN, IS OK.   */
@RF00076 LA    @11,80                                           01S0081
         AL    @11,APOCACB(,APOCPTR)                            01S0081
         TM    APCXOWN(@11),B'10000000'                         01S0081
         BNZ   @RF00081                                         01S0081
         L     @01,APOCAPDS(,APOCPTR)                           01S0081
         TM    APDSOWN(@01),B'10000000'                         01S0081
         BNO   @RF00081                                         01S0081
*     DO;                           /* MODIFY ACB TO SHOW UPGRADE    */
*       APCXOWN=BON;                /* SHOW EXCLUSIVE ALLOCATION     */
         OI    APCXOWN(@11),B'10000000'                         01S0083
*       APOCMSKO=BON;               /* SHOW OWN= FIELD MODIFIED      */
         OI    APOCMSKO(APOCPTR),B'10000000'                    01S0084
*       APOCMOWN=BOFF;              /* SAVE ORIGINAL VALUE           */
         NI    APOCMOWN(APOCPTR),B'01111111'                    01S0085
*     END;                          /* UPGRADE PROCESSED             */
*                                                               01S0086
*   /*****************************************************************/
*   /*                                                               */
*   /* PROCESS REGION= VALUE                                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0087
*   IF APDSREG<APCXREG THEN         /* IF ALLOCATED LESS THAN        */
@RF00081 L     @11,APOCAPDS(,APOCPTR)                           01S0087
         L     @15,APOCACB(,APOCPTR)                            01S0087
         L     @11,APDSREG(,@11)                                01S0087
         C     @11,APCXREG+80(,@15)                             01S0087
         BNL   @RF00087                                         01S0087
*     DO;                           /* REQUIRED, IS ERROR            */
*       ACBERFLG=APCCS002;          /* FLAG CONDITION & EXIT         */
         MVI   ACBERFLG(@15),X'02'                              01S0089
*       GOTO EXIT;                  /* OPEN WILL BE TERMINATED       */
         B     EXIT                                             01S0090
*     END;                          /* DOWNGRADE PROCESSED           */
*   IF APDSREG>APCXREG THEN         /* IF MORE THAN REQUESTED        */
@RF00087 L     @11,APOCAPDS(,APOCPTR)                           01S0092
         L     @11,APDSREG(,@11)                                01S0092
         LA    @15,80                                           01S0092
         AL    @15,APOCACB(,APOCPTR)                            01S0092
         L     @14,APCXREG(,@15)                                01S0092
         CR    @11,@14                                          01S0092
         BNH   @RF00092                                         01S0092
*     DO;                           /* ALLOCATED, IS OK.             */
*       APOCMSKR=BON;               /* SHOW APCX MODIFIED            */
         OI    APOCMSKR(APOCPTR),B'01000000'                    01S0094
*       APOCMREG=APCXREG;           /* SAVE ORIGINAL APCXREG VALUE   */
         ST    @14,APOCMREG(,APOCPTR)                           01S0095
*       APCXREG=APDSREG;            /* SHOW ACTUAL REGION ALLOCATED  */
         ST    @11,APCXREG(,@15)                                01S0096
*     END;                          /* UPGRADE PROCESSED             */
*                                                               01S0097
*   /*****************************************************************/
*   /*                                                               */
*   /* PROCESS ACBSTRNO VALUE                                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0098
*   IF ACBSTRNO=0 THEN              /* IF NO STRINGS REQUESTED       */
@RF00092 L     @11,APOCACB(,APOCPTR)                            01S0098
         CLI   ACBSTRNO(@11),0                                  01S0098
         BNE   @RF00098                                         01S0098
*     DO;                           /* USE THE APCT DEFAULT          */
*       APOCMSKS=BON;               /* SHOW ACB FIELD MODIFIED       */
         OI    APOCMSKS(APOCPTR),B'00100000'                    01S0100
*       ACBSTRNO=APCTSTDF;          /* SET ACB TO DEFAULT VALUE      */
         L     @15,APCTSTDF(,APCTPTR)                           01S0101
         STC   @15,ACBSTRNO(,@11)                               01S0101
*     END;                          /* UPGRADE PROCESSED             */
*   ELSE                            /* IF STRINGS REQUESTED,         */
*     IF ACBSTRNO>APCTSTMX THEN     /* INSURE TOO MANY NOT REQUESTED */
         B     @RC00098                                         01S0103
@RF00098 L     @11,APOCACB(,APOCPTR)                            01S0103
         SLR   @15,@15                                          01S0103
         IC    @15,ACBSTRNO(,@11)                               01S0103
         C     @15,APCTSTMX(,APCTPTR)                           01S0103
         BNH   @RF00103                                         01S0103
*       DO;                         /* IF EXCESSIVE NUMBER,          */
*         ACBERFLG=APCCS003;        /* ERROR FLAG IN ACB             */
         MVI   ACBERFLG(@11),X'03'                              01S0105
*         GOTO EXIT;                /* RETURN ERROR CODE             */
         B     EXIT                                             01S0106
*       END;                        /* DOWN GRADE PROCESSED          */
         EJECT
*                                                               01S0108
*   /*****************************************************************/
*   /*                                                               */
*   /* LOAD REQUESTED LERR AND/OR PERR MODULES REQUESTED IN THE APDS */
*   /* (IF ANY).                                                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0108
*   DO;                                                         02S0108
*                                                               02S0108
@RF00103 DS    0H                                               02S0108
@RC00098 DS    0H                                               02S0109
*     /***************************************************************/
*     /*                                                             */
*     /* IF THE APDS SPECIFIES A LOGICAL (LERR) AND/OR PHYSICAL      */
*     /* (PERR) ERROR EXIT, LOAD THEM. THE LOAD PROCESS REQUIRES TWO */
*     /* STEPS: (1) DETERMINE IF A EXIT ROUTINE IS REQUIRED & UPDATE */
*     /* THE ACB TO INDICATE ANY ERROR CONDITIONS, (2) THE ACTUAL    */
*     /* LOADING OF THE MODULE AND THE UPDATING OF THE EXLST OCCURS  */
*     /* UNDER A PRB.                                                */
*     /*                                                             */
*     /***************************************************************/
*                                                               02S0109
*     IF(APDSVLER=BON×              /* PERFORM LOADING ONLY IF       */
*         APDSVPER=BON)&            /* EXIT RTN AND ACB              */
*         ACBEXLST>1 THEN           /* EXLST EXIST.                  */
         L     @11,APOCAPDS(,APOCPTR)                           02S0109
         TM    APDSVLER(@11),B'00110000'                        02S0109
         BZ    @RF00109                                         02S0109
         L     @01,APOCACB(,APOCPTR)                            02S0109
         CLC   ACBEXLST(4,@01),@CF00056                         02S0109
         BNH   @RF00109                                         02S0109
*       DO;                         /* IF NOT, IGNORE THE LOADING.   */
*                                                               02S0110
*         /***********************************************************/
*         /*                                                         */
*         /* SINCE EXECUTING IN DATA MANAGEMENT KEY, CANNOT REFERENCE*/
*         /* OR UPDATE THE EXLST. HOWEVER THE ACB COPY IS IN         */
*         /* PROTECTED STORAGE. PASS THE SYNCH ROUTINE THE NAME OF   */
*         /* THE MODULE TO LOAD IN REGISTER 1, AND THE OFFSET INTO   */
*         /* THE EXLST OF THE REQUIRED ENTRY IN REGISTER 0. ONLY     */
*         /* REGISTERS APDATA, APCODE, AND APSVPTR ARE PROTECTED     */
*         /* ACROSS THE SYNCH. ON EXIT REGISTER 15 CONTAINS A        */
*         /* INDICATOR WHETHER THE MODULE WAS LOADED: 0--MODULE      */
*         /* LOADED, 4--EXLST TOO SHORT, 8--UNABLE TO LOAD THE       */
*         /* MODULE. IF THE ACB WAS BUILD IN PROTECTED STORAGE (KEY  */
*         /* 0-7), CALL THE SYNCH ROUTINE SINCE THE CALLER MUST BE   */
*         /* AUTHORIZED.                                             */
*         /*                                                         */
*         /***********************************************************/
*                                                               02S0111
*         STM(R2F,R11F,SYNCHREG);   /* SAVE REGS ACROSS SYNCH        */
         STM   R2F,R11F,SYNCHREG                                02S0111
*         RFY                                                   02S0112
*          (R0P,                                                02S0112
*           R1P,                                                02S0112
*           R14F,                                               02S0112
*           R15F) RSTD;             /* USED REGISTERS                */
*         IF APDSVLER=BON THEN      /* IF LOGICAL ERROR EXIT         */
         TM    APDSVLER(@11),B'00100000'                        02S0113
         BNO   @RF00113                                         02S0113
*           DO;                     /* ROUTINE WANTED --             */
*             R1P=ADDR(EXLLERF)-ADDR(IFGEXLST);/* EXLST OFFSET       */
         LA    R1P,15                                           02S0115
*             R0P=ADDR(APDSLERR);   /* @ MODULE NAME TO LOAD         */
         LA    R0P,APDSLERR(,@11)                               02S0116
*             IF APOCUKEY>='80'X THEN/* IF USER PROTECT KEY (8-15),  */
         CLI   APOCUKEY(APOCPTR),X'80'                          02S0117
         BL    @RF00117                                         02S0117
*               GEN(SYNCH SYNCHRTN)                             02S0118
*               REFS(SYNCHRTN);     /* LOAD UNDER SYNCH PRB          */
         SYNCH SYNCHRTN
*             ELSE                  /* ELSE, IF AUTHORIZED KEY (0-7),*/
*               DO;                 /* THEN LOAD WHILE IN SUPV STATE */
         B     @RC00117                                         02S0119
@RF00117 DS    0H                                               02S0120
*                 SPKA(APOCUKEY);   /* SWITCH TO ACB/EXLST KEY       */
         SLR   @11,@11                                          02S0120
         IC    @11,APOCUKEY(,APOCPTR)                           02S0120
         SPKA  0(@11)                                           02S0120
*                 CALL SYNCHRTN;    /* LOAD THE MODULE               */
         BAL   @14,SYNCHRTN                                     02S0121
*                 GEN(MODESET EXTKEY=DATAMGT)                   02S0122
*                 NOREFS NOSETS;    /* RESUME DATA MANAGEMENT KEY    */
         MODESET EXTKEY=DATAMGT
*               END;                /* MODULE NOW LOADED             */
*             LM(R2F,R11F,SYNCHREG);/* RELOAD DESTROYED REGISTERS    */
@RC00117 LM    R2F,R11F,SYNCHREG                                02S0124
*             IF R15F=0 THEN        /* IF MODULE LOADED,             */
         LTR   R15F,R15F                                        02S0125
         BNZ   @RF00125                                         02S0125
*               APOCMLER=BON;       /* UPDATE CLOSE/RECOVERY MASK    */
         OI    APOCMLER(APOCPTR),B'00000001'                    02S0126
*             ELSE                  /* IF MODULE NOT LOADED          */
*               IF R15F=8 THEN      /* BECAUSE MODULE MISSING, THEN  */
         B     @RC00125                                         02S0127
@RF00125 C     R15F,@CF00040                                    02S0127
         BNE   @RF00127                                         02S0127
*                 DO;               /* FLAG ACB & TERMINATE OPEN.    */
*                   ACBERFLG=APCCS004;/* IF NO EXLST ENTRY, IS OK,   */
         L     @11,APOCACB(,APOCPTR)                            02S0129
         MVI   ACBERFLG(@11),X'05'                              02S0129
*                   GOTO EXIT;      /* SO IGNORE REQUEST             */
         B     EXIT                                             02S0130
*                 END;                                          02S0131
*           END;                    /* LERR ROUTINE PROCESSED        */
@RF00127 DS    0H                                               02S0132
@RC00125 DS    0H                                               02S0133
*         IF APDSVPER=BON THEN      /* IF PHYSICAL ERROR EXIT        */
@RF00113 L     @11,APOCAPDS(,APOCPTR)                           02S0133
         TM    APDSVPER(@11),B'00010000'                        02S0133
         BNO   @RF00133                                         02S0133
*           DO;                     /* ROUTINE WANTED --             */
*             R1P=ADDR(EXLSYNF)-ADDR(IFGEXLST);/* EXLST OFFSET       */
         LA    R1P,10                                           02S0135
*             R0P=ADDR(APDSPERR);   /* ADDR MODULE NAME TO LOAD      */
         LA    R0P,APDSPERR(,@11)                               02S0136
*             IF APOCUKEY>='80'X THEN/* IF USER PROTECT KEY (8-15),  */
         CLI   APOCUKEY(APOCPTR),X'80'                          02S0137
         BL    @RF00137                                         02S0137
*               GEN(SYNCH SYNCHRTN)                             02S0138
*               REFS(SYNCHRTN);     /* LOAD UNDER SYNCH PRB          */
         SYNCH SYNCHRTN
*             ELSE                  /* ELSE, IF AUTHORIZED KEY (0-7),*/
*               DO;                 /* THEN LOAD WHILE IN SUPV STATE */
         B     @RC00137                                         02S0139
@RF00137 DS    0H                                               02S0140
*                 SPKA(APOCUKEY);   /* SWITCH TO ACB/EXLST KEY       */
         SLR   @11,@11                                          02S0140
         IC    @11,APOCUKEY(,APOCPTR)                           02S0140
         SPKA  0(@11)                                           02S0140
*                 CALL SYNCHRTN;    /* LOAD THE MODULE               */
         BAL   @14,SYNCHRTN                                     02S0141
*                 GEN(MODESET EXTKEY=DATAMGT)                   02S0142
*                 NOREFS NOSETS;    /* RESUME DATA MANAGEMENT KEY    */
         MODESET EXTKEY=DATAMGT
*               END;                /* MODULE NOW LOADED             */
*             LM(R2F,R11F,SYNCHREG);/* RELOAD DESTROYED REGISTERS    */
@RC00137 LM    R2F,R11F,SYNCHREG                                02S0144
*             IF R15F=0 THEN        /* IF MODULE LOADED              */
         LTR   R15F,R15F                                        02S0145
         BNZ   @RF00145                                         02S0145
*               APOCMPER=BON;       /* UPDATE CLOSE/RECOVERY MASK    */
         OI    APOCMPER(APOCPTR),B'00000010'                    02S0146
*             ELSE                  /* IF MODULE NOT LOADED          */
*               IF R15F=8 THEN      /* BECAUSE MODULE MISSING, THEN  */
         B     @RC00145                                         02S0147
@RF00145 C     R15F,@CF00040                                    02S0147
         BNE   @RF00147                                         02S0147
*                 DO;               /* FLAG ACB & TERMINATE OPEN.    */
*                   ACBERFLG=APCCS005;/* IF NO EXLST ENTRY, IS OK,   */
         L     @11,APOCACB(,APOCPTR)                            02S0149
         MVI   ACBERFLG(@11),X'06'                              02S0149
*                   GOTO EXIT;      /* SO IGNORE REQUEST.            */
         B     EXIT                                             02S0150
*                 END;                                          02S0151
*           END;                    /* PERR ROUTINE PROCESSED        */
@RF00147 DS    0H                                               02S0152
@RC00145 DS    0H                                               02S0153
*         RFY                                                   02S0153
*          (R0P,                                                02S0153
*           R1P,                                                02S0153
*           R14F,                                               02S0153
*           R15F) UNRSTD;           /* FINISHED WITH REGS            */
@RF00133 DS    0H                                               02S0154
*       END;                        /* LOADING COMPLETE              */
*   END;                            /* END OF CODE                   */
         EJECT
*SYNCHRTN:                          /* NOW UNDER PRB                 */
*   PROC OPTIONS(NOSAVE,            /* R13->PROTECTED STORAGE        */
*       NOSAVEAREA);                /* PROTECTED REGS ARE 6,7,12,13  */
*                                                               02S0156
         B     @PB00002                                         02S0156
SYNCHRTN DS    0H                                               02S0157
*   /*****************************************************************/
*   /*                                                               */
*   /* DO THE MODULE LOADING UNDER A PRB. THIS SOLVES SEVERAL        */
*   /* INTEGRITY PROBLEMS: (1) REFERENCES/STORES OF EXLST (USER      */
*   /* PROTECT KEY) OCCUR WITH THE PSW HAVING THE USER PROTECT KEY.  */
*   /* (2) CONTENTS SUPERVISION PERFORMS AUTHORIZATION CHECKS ONLY IF*/
*   /* THE LOAD SVC WAS ISSUED IN PROBLEM PROGRAM STATE. SINCE       */
*   /* EXECUTING IN THE USER'S KEY, CANNOT UPDATE THE ACB (FAKE      */
*   /* COPY), THE APOC, OR THE APDS, AND CANNOT SAVE REGISTERS IN THE*/
*   /* R13 SAVEAREA. THEREFORE REGISTERS ARE SAVED PRIOR TO THE SYNCH*/
*   /* AND RESTORED ON RETURN. SINCE ACBERFLG CANNOT BE UPDATED,     */
*   /* REGISTER 15 IS SET TO INDICATE THE STATUS OF THE LOAD:        */
*   /* 0--LOADED SUCCESSFULLY, 4--EXLST TOO SHORT, 8--LOAD SVC       */
*   /* FAILED. ON ENTRY, REGISTER 1 CONTAINS THE OFFSET INTO THE     */
*   /* EXLST OF THE DESIRED EXIT, AND REGISTER 0 CONTAINS THE ADDRESS*/
*   /* OF THE MODULE NAME TO LOAD.                                   */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S0157
*                                                               02S0157
*   /*****************************************************************/
*   /*                                                               */
*   /* SYNCHRTN DECLARES AND REGISTER USAGE                          */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S0157
*   DCL                                                         02S0157
*     RXRETURN PTR(31) REG(03),     /* SAVE RETURN ADDRESS           */
*     RXEXLST PTR(31) REG(02);      /* EXLST POINTER                 */
*   RFY                             /* REQUIRED REGISTERS --         */
*    (R0P,                          /* ADDR MODULE NAME TO LOAD      */
*     R1P,                          /* OFFSET INTO EXLST OF ENTRY    */
*     R15F,                         /* RETURN CODE FROM SYNCH        */
*     R14P,                         /* INPUT RETURN ADDRESS          */
*     RXRETURN,                     /* HOLDS RETURN ADDRESS          */
*     RXEXLST) RSTD;                /* EXLST POINTER                 */
*   RXRETURN=R14P;                  /* SAVE RETURN ADDRESS           */
         LR    RXRETURN,R14P                                    02S0159
*   RFY                                                         02S0160
*     R14P UNRSTD;                  /* RETURN REGISTER               */
*   RXEXLST=ACBEXLST;               /* POINT TO EXLST                */
         L     @14,APOCACB(,APOCPTR)                            02S0161
         L     RXEXLST,ACBEXLST(,@14)                           02S0161
*   RFY                                                         02S0162
*     IFGEXLST BASED(RXEXLST);      /* TELL COMPILER                 */
*   IF R1P>=EXLLEN THEN             /* IF EXIT NOT INCLUDED IN       */
         CH    R1P,EXLLEN(,RXEXLST)                             02S0163
         BL    @RF00163                                         02S0163
*     R15F=4;                       /* EXLST, SET RETURN CODE        */
         LA    R15F,4                                           02S0164
*   ELSE                            /* IF EXLST ENTRY EXISTS,        */
*     DO;                           /* THEN LOAD THE MODULE          */
         B     @RC00163                                         02S0165
@RF00163 DS    0H                                               02S0166
*       RXEXLST=ADDR(IFGEXLST)+R1P; /* POINT TO EXLST ENTRY          */
         ALR   RXEXLST,R1P                                      02S0166
*       RFY                                                     02S0167
*         IFGEXLST;                 /* NOT POINTING AT EXLST HEADER  */
*       RFY                                                     02S0168
*         IFGEXLEF BASED(RXEXLST);  /* EXLST ENTRY OVERLAY           */
*       GEN FLOWS(SYNCHERR) SEQFLOW NOEXIT REFS(R0P);           02S0169
*                                   /*
         LOAD  EPLOC=(0),          LOAD SPECIFIED MODULE               *
               ERRET=SYNCHERR      MODULE NOT LOADED ERROR EXIT
*SYNCHERR:                          /* MODULE NOT LOADED -- R15^=0   */
*       IF R15F^=0 THEN             /* IF NOT LOADED (R15 NOT ZERO)  */
SYNCHERR LTR   R15F,R15F                                        02S0170
         BZ    @RF00170                                         02S0170
*         R15F=8;                   /* SET ERROR RETURN INDICATOR.   */
         LA    R15F,8                                           02S0171
*       ELSE                        /* OTHERWISE,                    */
*         DO;                       /* UPDATE ACB EXLST ENTRY.       */
         B     @RC00170                                         02S0172
@RF00170 DS    0H                                               02S0173
*           EXLFLAG=0;              /* RESET EXIT FLAGS              */
         MVI   EXLFLAG(RXEXLST),X'00'                           02S0173
*           EXLSPEC=BON;            /* EXIT SPECIFIED                */
*           EXLACTM=BON;            /* EXIT ACTIVE                   */
         OI    EXLSPEC(RXEXLST),B'11000000'                     02S0175
*           EXLEXITP=R0P;           /* ADDRESS OF MODULE LOADED      */
         STCM  R0P,15,EXLEXITP(RXEXLST)                         02S0176
*         END;                      /* R15F ALREADY 0.               */
*     END;                          /* MODULE LOAD ATTEMPTED         */
@RC00170 DS    0H                                               02S0179
*   RFY                                                         02S0179
*     R14P RSTD;                    /* RESET REGISTER 14 TO RETURN   */
@RC00163 DS    0H                                               02S0180
*   R14P=RXRETURN;                  /* ADDRESS (SVC-3 OR IGTJAD)     */
         LR    R14P,RXRETURN                                    02S0180
*   RETURN CODE(R15F);              /* RETURN TO CALLER              */
@EL00002 DS    0H                                               02S0181
@EF00002 DS    0H                                               02S0181
@ER00002 BR    @14                                              02S0181
*   RFY                                                         02S0182
*     IFGEXLEF;                     /* FINISHED WITH EXLST OVERLAY   */
*   RFY                                                         02S0183
*    (R0P,                                                      02S0183
*     R1P,                                                      02S0183
*     R14P,                                                     02S0183
*     R15F,                                                     02S0183
*     RXRETURN,                                                 02S0183
*     RXEXLST) UNRSTD;              /* RELEASE REGS                  */
*   END SYNCHRTN;                   /* END OF SYNCH ROUTINE          */
@PB00002 DS    0H                                               02S0185
         EJECT
*                                                               01S0185
*   /*****************************************************************/
*   /*                                                               */
*   /* DEFAULTS PROCESSED. SAVE THE ACB MODIFICATION MASK IN THE     */
*   /* APDS. THEN RETURN TO THE CALLER WITH REGISTER 15 SHOWING IF   */
*   /* THE PROCESSING WAS SUCCESSFUL. (ACBERFLG SET IF AN ERROR WAS  */
*   /* DETECTED).                                                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0185
*EXIT:                              /* ERROR EXIT POINT              */
*   GEN(MODESET EXTKEY=SCHED)                                   01S0185
*   NOREFS NOSETS;                  /* SWITCH TO APDS KEY.           */
EXIT     MODESET EXTKEY=SCHED
*   APDSMSK=APOCMSK;                /* SAVE CHANGE MASK IN APDS.     */
         L     @11,APOCAPDS(,APOCPTR)                           01S0186
         MVC   APDSMSK(8,@11),APOCMSK(APOCPTR)                  01S0186
*   GEN(MODESET EXTKEY=DATAMGT)                                 01S0187
