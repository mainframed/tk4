*FIXAPIOV:                          /* APIO VERIFICATION EXIT        */
*   PROC OPTIONS(NOSAVE,            /* R13 SAVEAREA NOT AVAILABLE    */
*       NOSAVEAREA);                /* NO DYNAMIC AREA POINTER       */
*                                                               01S0293
FIXAPIOV DS    0H                                               01S0294
*   /*****************************************************************/
*   /*                                                               */
*   /* FOLLOWING INTERNAL PROCEDURE GIVEN CONTROL BY IEAVEQV3 TO     */
*   /* VERIFY THAT THE CONTROL BLOCK POINTED TO BY REGISTER 0 IS AN  */
*   /* APIO. THIS ROUTINE MUST INSURE THAT A PROGRAM CHECK DOES NOT  */
*   /* OCCUR. ON EXIT REGISTER 15 INDICATE IF THE BLOCK IS VALID     */
*   /* (0=YES, 8=NO). REGISTERS 8-14 MUST BE RETURNED UNCHANGED.     */
*   /* OTHERS ARE VOLITILE.                                          */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0294
*   RFY                                                         01S0294
*    (R0P,                          /* APIO ADDRESS                  */
*     R8P,                                                      01S0294
*     R9P,                                                      01S0294
*     R10P,                                                     01S0294
*     R11P,                                                     01S0294
*     R12P,                                                     01S0294
*     R13P,                         /* MUST REMAIN UNCHANGED         */
*     R14P,                         /* RETURN ADDRESS                */
*     R15P) RSTD;                   /* INPUT ADDRESS (APIOFIXV)      */
*   GEN REFS(R15P,APCODE,FIXAPIOV) SETS(APCODE);                01S0295
*                                   /* USING ENVIRONMENT
         PUSH  USING               SAVE CURRENT USING STATUS
         DROP  ,                   DROP ALL BASE REGISTERS
         LR    APCODE,R15P         PUT ENTRY POINT IN BASE REGISTER
         USING FIXAPIOV,APCODE     TELL ASSEMBLER
*   RFY                                                         01S0296
*     R15P UNRSTD;                  /* DONE WITH TEMPORARY BASE      */
*   RFY                                                         01S0297
*    (R1P,                                                      01S0297
*     R2P,                                                      01S0297
*     R15F) RSTD;                   /* REQUIRED REGISTERS            */
*   R1P=R0P&'FFFFFFF8'X;            /* APIO ADDRESS IN USEABLE REG
*                                      (INSURE DOUBLEWORD ALIGNED)   */
         LR    R1P,R0P                                          01S0298
         N     R1P,@CF03695                                     01S0298
*   RFY                                                         01S0299
*     APIO BASED(R1P);              /* TELL PL/S                     */
*                                                               01S0299
*   /*****************************************************************/
*   /*                                                               */
*   /* FOLLOWING CHECKS USED TO VERIFY APIO -- (1) APIO IN REAL      */
*   /* STORAGE, (2) APIO IN KEY-5, NON-FETCH PROTECTED STORAGE, AND  */
*   /* (3) APIOID HAS 'APIO'. CARE IS TAKEN TO INSURE THAT A PROGRAM */
*   /* CHECK DOES NOT OCCUR VERIFYING THE APIO.                      */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0300
*   R15F=8;                         /* ERROR RETURN CODE             */
         LA    R15F,8                                           01S0300
*   IF R1P=R0P THEN                 /* INSURE APIO WAS DOUBLE WORD   */
         CR    R1P,R0P                                          01S0301
         BNE   @RF00301                                         01S0301
*     DO;                           /* ALIGNED. IF NOT IS ERROR.     */
*       GEN(LRA   R0P,0(,R1P))                                  01S0303
*       REFS(R1P) SETS(R0P);        /* SEE IF IN STORAGE             */
         LRA   R0P,0(,R1P)
*       BC(7,FIXAPIOZ);             /* NO--NOT APIO                  */
         BC    7,FIXAPIOZ                                       01S0304
*       DO;                         /* (TREAT ABOVE AS IF)           */
*         R0P=R0P&'FFFFF800'X;      /* HARDWARE PAGE BDY (2K)        */
         N     R0P,@CF03699                                     01S0306
*         GEN(ISK   R0P,R0P)                                    01S0307
*         REFS(R0P) SETS(R0P);      /* STORAGE KEY                   */
         ISK   R0P,R0P
*         R0P=R0P&'000000F8'X;      /* LEAVE ACCESS CODE, FETCH AUTH */
         N     R0P,@CF03700                                     01S0308
*         IF R0P=16*APIOKEY THEN    /* MUST BE KEY OF APIO AND       */
         C     R0P,@CF02411                                     01S0309
         BNE   @RF00309                                         01S0309
*           DO;                     /* NOT FETCH PROTECTED.          */
*             R2P=ADDR(APIOSIZE);   /* INSURE 'APIOSIZE' FIELD       */
         LA    R2P,APIOSIZE(,R1P)                               01S0311
*             GEN(LRA   R0P,0(,R2P))                            01S0312
*             REFS(R2P) SETS(R0P);  /* IS REFERENCABLE               */
         LRA   R0P,0(,R2P)
*             BC(7,FIXAPIOZ);       /* (KEY CHECK NOT REQUIRED)      */
         BC    7,FIXAPIOZ                                       01S0313
*             DO;                   /* TREAT ABOVE AS 'IF'           */
*               R2P=ADDR(APIO)+APIOSIZE/* SET R2P = ADDR             */
*                   -1;             /* OF LAST BYTE IN APIO          */
         LR    R2P,R1P                                          01S0315
         AH    R2P,APIOSIZE(,R1P)                               01S0315
         BCTR  R2P,0                                            01S0315
*               GEN(LRA   R0P,0(,R1P))                          01S0316
*               REFS(R1P) SETS(R0P);/* REPEAT ABOVE CHECKS           */
         LRA   R0P,0(,R1P)
*               BC(7,FIXAPIOZ);     /* TO INSURE THAT THE ENTIRE     */
         BC    7,FIXAPIOZ                                       01S0317
*               DO;                 /* APIO (LESS THAN 2K) EXISTS    */
*                 R0P=R0P&'FFFFF800'X;/* IN VALID STORAGE.           */
         N     R0P,@CF03699                                     01S0319
*                 GEN(ISK   R0P,R0P)                            01S0320
*                 REFS(R0P) SETS(R0P);/* IF NOT,                     */
         ISK   R0P,R0P
*                 R0P=R0P&'000000F8'X;/* IS ERROR.                   */
         N     R0P,@CF03700                                     01S0321
*                 IF R0P=16*APIOKEY THEN/* IF OK SO FAR,             */
         C     R0P,@CF02411                                     01S0322
         BNE   @RF00322                                         01S0322
*                   IF APIOID=APIOIDC THEN/* THEN CAN                */
         CLC   APIOID(4,R1P),@CC03378                           01S0323
         BNE   @RF00323                                         01S0323
*                     R15F=0;       /* REFERENCE THE APIO.           */
         SLR   R15F,R15F                                        01S0324
*               END;                /* INSURE THAT THE APIO          */
@RF00323 DS    0H                                               01S0325
@RF00322 DS    0H                                               01S0326
*             END;                  /* HEADER ID IS ALSO VALID       */
*           END;                    /* (THIS CHECKS FOR OVERLAID     */
*       END;                        /* APIO).                        */
@RF00309 DS    0H                                               01S0329
*     END;                          /* R15F INDICATES IF BAD APIO.   */
*   RFY                                                         01S0330
*     APIO;                         /* FINISHED WITH APIO            */
@RF00301 DS    0H                                               01S0331
*FIXAPIOZ:                                                      01S0331
*   ;                               /* APIO VERIFIED                 */
*                                                               01S0331
FIXAPIOZ DS    0H                                               01S0332
