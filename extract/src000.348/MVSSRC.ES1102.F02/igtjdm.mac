*     /* APESREGS. ISSUE DUMMY RESTRICT TO WIPE PL/S OPTIMIZATION    */
*     /* MEMORY OF CURRENT REGISTER VALUES                           */
*     /*                                                             */
*     /***************************************************************/
*                                                               01S0338
*     RFY                                                       01S0338
*      (R0RSTD,                                                 01S0338
*       R1RSTD,                                                 01S0338
*       R2RSTD,                                                 01S0338
*       R3RSTD,                                                 01S0338
*       R4RSTD,                                                 01S0338
*       R5RSTD,                     /* 6-7 BASES                     */
*       R8RSTD,                                                 01S0338
*       R9RSTD,                                                 01S0338
*       R10RSTD,                                                01S0338
*       R11RSTD,                    /* 12-13 APCT/APSV PTRS          */
*       R14RSTD,                                                01S0338
*       R15RSTD) RSTD;              /* KEEP PL/S FROM CARRYING DATA  */
*     GEN(SPACE ,);                 /* NULL CODE                     */
         SPACE ,
