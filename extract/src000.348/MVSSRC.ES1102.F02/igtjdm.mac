TJDM     TITLE 'IGTJDM -- EOT/EOAS APIO CLEANUP                        *
                        '
IGTJDM   CSECT ,                                                01S0001
@MAINENT DS    0H                                               01S0001
         USING *,@15                                            01S0001
         B     @PROLOG                                          01S0001
         DC    AL1(16)                                          01S0001
         DC    C'IGTJDM    77.093'                              01S0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                  01S0001
         BALR  @07,0                                            01S0001
@PSTART  DS    0H                                               01S0001
         USING @PSTART,@07                                      01S0001
*   APCTPTR=R0P->SSCTSUSE;          /* ESTABLISH APCT ADDRESS        */
         LR    @08,R0P                                          01S0094
         L     APCTPTR,SSCTSUSE(,@08)                           01S0094
*   RSSOBPTR=R1P;                   /* SSOB ADDRESS IN SAFE REGISTER */
         LR    RSSOBPTR,R1P                                     01S0095
*   RFY                                                         01S0096
*     SSOB BASED(RSSOBPTR);         /* TELL PL/S OF THE SSOB BASE    */
*   RFY                                                         01S0097
*    (R0P,                                                      01S0097
*     R1P) UNRSTD;                  /* FINISHED WITH INPUT REGISTERS */
         EJECT
*                                                               01S0098
*   /*****************************************************************/
*   /*                                                               */
*   /* GET THE LOCAL AND CMS LOCKS. REQUIRED TO USE APVUWORK (PL/S   */
*   /* DYNAMIC AREA) AND TO SCAN/UPDATE THE APIO QUEUE.              */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0098
*   DO;                             /* START OF CODE SEGMENT         */
*                                                               02S0098
*     /***************************************************************/
*     /*                                                             */
*     /* OBTAIN THE LOCAL & CMS LOCKS. THE CMS LOCK IS REQUIRED TO   */
*     /* SCAN/UPDATE THE APIO CHAIN AND TO USE APVUWORK.             */
*     /*                                                             */
*     /***************************************************************/
*                                                               02S0099
*     GEN REFS(PSALITA) SETS(R0P,R1P,R11P,R12P,R13P,R14P);      02S0099
*                                                        /*
         LR    R0P,R12P            SAVE INPUT REGS 12 & 13
         LR    R1P,R13P            * (APCT, SAVEAREA)
         SETLOCK OBTAIN,TYPE=LOCAL,MODE=UNCOND, GET THE LOCAL LOCK     *
               RELATED=(APVUWORK,APIO-CHAIN)
         SETLOCK OBTAIN,TYPE=CMS,MODE=UNCOND, GET THE CMS LOCK         *
               RELATED=(APVUWORK,APIO-CHAIN)
         LR    R12P,R0P            RESTORE DESTROYED REGISTERS
         LR    R13P,R1P            * R11 & R14 STILL BAD
*   END;                            /* LOCAL & CMS LOCKS OWNED       */
         EJECT
*                                                               01S0101
*   /*****************************************************************/
*   /*                                                               */
*   /* BUILD AND INITIALIZE THE PL/S SAVEAREA AND WORKAREA. THE CMS  */
*   /* LOCK MUST BE OWNED FOR THIS PROCESS.                          */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0101
*   DO;                             /* START OF CODE SEGMENT         */
*                                                               02S0101
*     /***************************************************************/
*     /*                                                             */
*     /* BUILD AND INITIALIZE A SAVEAREA/WORKAREA. USE 'APVUWORK' FOR*/
*     /* THE DYNAMIC AREA. NOTE THAT THE CMS LOCK MUST BE OWNED TO   */
*     /* USE THIS AREA (FOR SERIALIZATION). CARE MUST BE USED TO     */
*     /* INSURE MORE DYNAMIC STORAGE IS NOT USED THAN EXISTS IN      */
*     /* APMVWORK (2000 BYTES). THE ADDRESS OF 'APVU' IS CONTAINED IN*/
*     /* THE APCT (APCTAPVU).                                        */
*     /*                                                             */
*     /***************************************************************/
*                                                               02S0102
*     RFY                                                       02S0102
*       R1P RSTD;                   /* CHAIN REGISTER                */
*     R1P=ADDR(APVUWORK);           /* POINT TO WORKAREA             */
*                                                               02S0103
         L     R1P,APCTAPVU(,APCTPTR)                           02S0103
         LA    R1P,APVUWORK(,R1P)                               02S0103
*     /***************************************************************/
*     /*                                                             */
*     /* BUILD AND CHAIN OS SAVEAREAS TOGETHER. DO NOT USE THE VPSS  */
*     /* SAVEAREA FORMAT. THEREFORE STANDARD VPSS ROUTINES CANNOT BE */
*     /* CALLED (SINCE THE LOCAL AND CMS LOCKS ARE OWNED, GETMAINS   */
*     /* FOR ADDITIONAL AREAS CANNOT BE TOLERATED).                  */
*     /*                                                             */
*     /***************************************************************/
*                                                               02S0104
*     APSVPTR->APSVNEXT=R1P;        /* FORWARD POINTER               */
         ST    R1P,APSVNEXT(,APSVPTR)                           02S0104
*     R1P->APSVPREV=APSVPTR;        /* BACKWARD CHAIN POINTER        */
         ST    APSVPTR,APSVPREV(,R1P)                           02S0105
*     APSVPTR=R1P;                  /* POINT TO NEW SAVEAREA         */
         LR    APSVPTR,R1P                                      02S0106
*     RFY                                                       02S0107
*       R1P UNRSTD;                 /* FINISHED WITH REGISTER 1      */
*     RFY                                                       02S0108
*       APDATA RSTD;                /* SET APDATA TO ADDRESS         */
*     APDATA=ADDR(APSV)+APSVOSLN;   /* OF PL/S DYNAMIC AREA          */
         LA    APDATA,72(,APSVPTR)                              02S0109
*     GEN(USING @DATD,APDATA)                                   02S0110
*     NOSETS REFS(APDATA);          /* (AFTER SAVEAREA).             */
         USING @DATD,APDATA
*   END;                            /* END OF CODE SEGMENT           */
         EJECT
*                                                               01S0112
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE THE RECOVERY AREA AND ISSUE A SETFRR TO COVER THIS */
*   /* PROGRAM'S PROCESSING.                                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0112
*   DO;                             /* START OF CODE SEGMENT         */
*                                                               02S0112
*     /***************************************************************/
*     /*                                                             */
*     /* ESTABLISH RECOVERY ENVIRONMENT. THIS EXIT WILL GAIN CONTROL */
*     /* IF AN ABEND OCCURS. A DUMP IS TAKEN, THE APPROPRIATE ERROR  */
*     /* FLAGS SET, AND CONTROL PASSED TO THE CLEANUP ROUTINE TO     */
*     /* RELEASE RESOURCES BEFORE RETURNING TO THE CALLER.           */
*     /*                                                             */
*     /***************************************************************/
*                                                               02S0113
*                                                               02S0113
*     /***************************************************************/
*     /*                                                             */
*     /* BUILD ESTAE/FRR EXIT PARAMETER LIST (APES)                  */
*     /*                                                             */
*     /***************************************************************/
*                                                               02S0113
*     MVC(APES(1:LENGTH(CAPES)),CAPES);/* INITIALIZE APES CONSTANTS  */
         MVC   APES(136),CAPES                                  02S0113
*     APESADDR=ADDR(APES);          /* ESTAE/SETFRR PARM VALUE       */
         LA    @08,APES                                         02S0114
         ST    @08,APESADDR                                     02S0114
*     APESREGP=ADDR(APESREGZ);      /* DEFAULT REGISTER SAVEAREA     */
         LA    @08,APESREGZ                                     02S0115
         ST    @08,APESREGP                                     02S0115
*     RFY                                                       02S0116
*      (R15P) RSTD;                 /* SET REGISTER 15 TO            */
*     R15P=ADDR(EXIT);              /* BACKOUT ROUTINE               */
         LA    R15P,EXIT                                        02S0117
*     STM(R0F,R15F,APESREGZ);       /* SAVE REGISTERS FOR RETRY      */
         STM   R0F,R15F,APESREGZ                                02S0118
*     RFY                                                       02S0119
*       R15P UNRSTD;                /* FINISHED WITH 15              */
*                                                               02S0119
*     /***************************************************************/
*     /*                                                             */
*     /* ISSUE SETFRR TO COVER THE PROCESSING. PARAMETER LIST IS     */
*     /* 'APES' BLOCK. THE EXIT ROUTINE CAN PROCESS BOTH (E)STAE AND */
*     /* FRR ENTRIES.                                                */
*     /*                                                             */
*     /***************************************************************/
*                                                               02S0120
*     APESCODE=CERRN01;             /* SETFRR ERROR CODE             */
         MVI   APESCODE,X'01'                                   02S0120
*     RFY                                                       02S0121
*       R15P RSTD;                  /* SETFRR SETS TO PARM LIST      */
*     GEN REFS(PSA,PSACSTK,PSALSFCC,APCTZUKA,APCT,APCTPTR,FRRSFRRA,
*         FRRSENTR,FRRSPARM,FRRSCURR,FRRSLAST,FRRS) SETS(R14F,R15F,R15P
*         );                                                    02S0122
*                                   /*
         SETFRR A,FRRAD=APCTZUKA-APCT(,APCTPTR), IGTUKA IS EXIT RTN    *
               PARMAD=(R15P),      * SAVE @ 6-WORD PARM FIELD HERE     *
               CLEAR=NO,           * DO NOT CLEAR PARM FIELD           *
               WRKREGS=(R14F,R15F), * WORK REGISTERS                   *
               RELATED=(FREELLCK,APES) * APES IS INPUT
*     APESCODE=CERRN99;             /* RESET ERROR CODE              */
         MVI   APESCODE,X'03'                                   02S0123
*     R15P->I031P=ADDR(APES);       /* 1ST WORD OF PARM LIST IS 02S0124
*                                      ADDRESS OF 'APES'             */
         LA    @14,APES                                         02S0124
         ST    @14,I031P(,R15P)                                 02S0124
*     RFY                                                       02S0125
*       R15P UNRSTD;                /* SETFRR PARM FIELD SET         */
*     APESFFRR=BON;                 /* SHOW SETFRR ISSUED            */
         OI    APESFFRR,B'10000000'                             02S0126
*   END;                            /* RECOVERY ENVIRONMENT SET      */
         EJECT
*                                                               01S0128
*   /*****************************************************************/
*   /*                                                               */
*   /* SCAN THE APIO QUEUE AND RELEASE ANY APIO'S STILL OWNED BY THE */
*   /* TERMINATING TASK OR ADDRESS SPACE                             */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0128
*   DO;                                                         02S0128
*                                                               02S0128
*     /***************************************************************/
*     /*                                                             */
*     /* SCAN THE APIO'S TO FIND ENTRIES BELONGING TO THE TERMINATED */
*     /* TASK OR ADDRESS SPACE. IF THIS IS A JOBSTEP TERMINATION,    */
*     /* TREAT LIKE AN ADDRESS SPACE TERMINATION (THE INITIATOR DOES */
*     /* NOT USE VPSS).                                              */
*     /*                                                             */
*     /***************************************************************/
*                                                               02S0129
*     DRETRY(01)=APESREGF;          /* SAVE CURRENT APESREGS         */
         L     @08,APESREGP                                     02S0129
         L     @15,APESREGF(,@08)                               02S0129
         ST    @15,DRETRY                                       02S0129
*     DRETRY(02)=APESREGE;          /* FOR REGISTERS 14 & 15         */
         L     @15,APESREGE(,@08)                               02S0130
         ST    @15,DRETRY+4                                     02S0130
*     APESREGE=ADDR(SCANIO1);       /* RETRY APIO SCAN POINT         */
         LA    @15,SCANIO1                                      02S0131
         ST    @15,APESREGE(,@08)                               02S0131
*     APESREGF=ADDR(FIXAPIO);       /* FIX APIO CHAIN ROUTINE        */
         LA    @15,FIXAPIO                                      02S0132
         ST    @15,APESREGF(,@08)                               02S0132
*     GOTO SCANIO2;                 /* SKIP RETRY SCAN CODE          */
         B     SCANIO2                                          02S0133
*SCANIO1:                           /* RETRY APIO SCAN               */
*     DO;                           /* ENTRY FROM ESTAE/FRR RETRY    */
*                                                               02S0134
SCANIO1  DS    0H                                               02S0135
*       /*************************************************************/
*       /*                                                           */
*       /* ENTRY FROM ESTAE/FRR RETRY (IGTUKA). RETRY DOES NOT INSURE*/
*       /* REGISTERS 0-5, 8-11, 14-15 VALID SINCE NOT SAVED IN       */
*       /* APESREGS. ISSUE DUMMY RESTRICT TO WIPE PL/S OPTIMIZATION  */
*       /* MEMORY OF CURRENT REGISTER VALUES                         */
*       /*                                                           */
*       /*************************************************************/
*                                                               02S0135
*       RFY                                                     02S0135
*        (R0RSTD,                                               02S0135
*         R1RSTD,                                               02S0135
*         R2RSTD,                                               02S0135
*         R3RSTD,                                               02S0135
*         R4RSTD,                                               02S0135
*         R5RSTD,                   /* 6-7 BASES                     */
*         R8RSTD,                                               02S0135
*         R9RSTD,                                               02S0135
*         R10RSTD,                                              02S0135
*         R11RSTD,                  /* 12-13 APCT/APSV PTRS          */
*         R14RSTD,                                              02S0135
*         R15RSTD) RSTD;            /* KEEP PL/S FROM CARRYING DATA  */
*       GEN(SPACE ,);               /* NULL CODE                     */
         SPACE ,
*       RFY                                                     02S0137
*        (R0RSTD,                                               02S0137
*         R1RSTD,                                               02S0137
*         R2RSTD,                                               02S0137
*         R3RSTD,                                               02S0137
*         R4RSTD,                                               02S0137
*         R5RSTD,                   /* 6-7 BASES                     */
*         R8RSTD,                                               02S0137
*         R9RSTD,                                               02S0137
*         R10RSTD,                                              02S0137
*         R11RSTD,                  /* 12-13 APCT/APSV PTRS          */
*         R14RSTD,                                              02S0137
*         R15RSTD) UNRSTD;          /* ALLOW PL/S TO USE REGS AGAIN  */
*     END;                          /* RETRY ENVIRONMENT SET         */
*     APESREGF=DRETRY(01);          /* RESTORE ORIGINAL              */
         L     @08,APESREGP                                     02S0139
         L     @15,DRETRY                                       02S0139
         ST    @15,APESREGF(,@08)                               02S0139
*     APESREGE=DRETRY(02);          /* REGISTERS 14 & 15             */
         L     @15,DRETRY+4                                     02S0140
         ST    @15,APESREGE(,@08)                               02S0140
*     DO;                           /* ERROR RECOVERY COMPLETE       */
*       APESFPRV=BOFF;              /* RESET RECURSION FLAG.         */
         NI    APESFPRV,B'10111111'                             02S0142
*       APESCODE=CERRN99;           /* RESET ERROR CODE              */
         MVI   APESCODE,X'03'                                   02S0143
*       APESFREC=BON;               /* RESET DUMP/LOG FLAGS          */
*       APESFSDP=BON;               /* TO THEIR NORMAL VALUES        */
         OI    APESFREC,B'01100000'                             02S0145
*     END;                          /* ALL INDICATORS RESET 2ND ABEND
*                                      TERMINATES REQUEST            */
*     APESCOD1=0;                   /* CLEAR ERROR INDICATOR         */
         MVI   APESCOD1,X'00'                                   02S0147
*SCANIO2:                           /* FIRST PASS START              */
*                                                               02S0148
*     /***************************************************************/
*     /*                                                             */
*     /* SET RASCBPTR = ADDRESS OF ASCB BEING PROCESSED AND RTCCBPTR */
*     /* = ADDRESS OF THE TCB BEING PROCESSED (=0 IF ADDRESS-SPACE OR*/
*     /* JOBSTEP TERMINATION).                                       */
*     /*                                                             */
*     /***************************************************************/
*                                                               02S0148
*     RFY                                                       02S0148
*      (RZERO,                      /* WORK REGISTER (=0)            */
*       RASCBPTR,                   /* ADDRESS SPACE                 */
*       RTCBPTR) RSTD;              /* TCB (OR 0)                    */
SCANIO2  DS    0H                                               02S0149
*     RZERO=0;                      /* ZERO VALUE                    */
         SLR   RZERO,RZERO                                      02S0149
*     RASCBPTR=SSETASCB;            /* ASCB (MAY NOT BE CURRENT)     */
         L     @05,SSOBINDV(,RSSOBPTR)                          02S0150
         L     RASCBPTR,SSETASCB(,@05)                          02S0150
*     RTCBPTR=SSETCBA;              /* TCB IF EOT                    */
         L     RTCBPTR,SSETCBA(,@05)                            02S0151
*     IF((SSOBFUNC=SSOBEOM)×        /* IF JOBSTEP OR ADDRESS SPACE   */
*         (RTCBPTR->TCBJSTCA=RTCBPTR))/* TERMINATION, SET            */
*       THEN                        /* RTCBPTR=0 (TO SHOW PURGE OF   */
         CLC   SSOBFUNC(2,RSSOBPTR),@CH00037                    02S0152
         BE    @RT00152                                         02S0152
         L     @05,TCBJSTCA-1(,RTCBPTR)                         02S0152
         LA    @05,0(,@05)                                      02S0152
         CR    RTCBPTR,@05                                      02S0152
         BNE   @RF00152                                         02S0152
@RT00152 DS    0H                                               02S0153
*       RTCBPTR=RZERO;              /* ALL APIO'S FOR THIS ASID).    */
*                                                               02S0153
         LR    RTCBPTR,RZERO                                    02S0153
*     /***************************************************************/
*     /*                                                             */
*     /* SCAN ALL APIOS. NOTE -- LOCAL & CMS LOCKS MUST BE HELD.     */
*     /*                                                             */
*     /***************************************************************/
*                                                               02S0154
*     RFY                                                       02S0154
*       RAPIOPTR RSTD;              /* APIO SCANNER                  */
@RF00152 DS    0H                                               02S0155
*     RAPIOPTR=APCTAPIO;            /* 1ST APIO                      */
         L     RAPIOPTR,APCTAPIO(,APCTPTR)                      02S0155
*     RFY                                                       02S0156
*       APIO BASED(RAPIOPTR);       /* TELL PL/S                     */
*     APESCODE=CERRN02;             /* APIO CHAIN ERROR              */
         MVI   APESCODE,X'02'                                   02S0157
*     GEN(MODESET EXTKEY=DATAMGT)                               02S0158
*     NOREFS NOSETS;                /* APIO KEY. MAY NOT UPDATE PL/S
*                                      DYNAMIC AREA.                 */
         MODESET EXTKEY=DATAMGT
*     DO WHILE(RAPIOPTR^=RZERO);    /* SCAN ALL APIO ENTRIES         */
         B     @DE00159                                         02S0159
@DL00159 DS    0H                                               02S0160
*       GEN NOREFS NOSETS FLOWS(SCANIO1,FIXAPIO) SEQFLOW;       02S0160
*                                          /* SHOW RTNS USED (NO CODE)
*       IF APIOID^=APIOIDC THEN     /* IF BAD APIO, FIX-UP CHAIN     */
         CLC   APIOID(4,RAPIOPTR),@CC03378                      02S0161
         BE    @RF00161                                         02S0161
*         DO;                       /* FORCE ABEND TO LOG ERROR &
*                                      PASS CONTROL TO THE (E)STAE OR
*                                      FRR EXIT ROUTINE.             */
*           DO;                     /* ABEND(APCTABND)               */
*             RESPECIFY                                         02S0164
*              (GPR01F) RESTRICTED;                             02S0164
*             GPR01F=APCTABND;      /* COMP CODE IN REG 1            */
         L     GPR01F,APCTABND(,APCTPTR)                        02S0165
*             SVC(13);              /* ISSUE ABEND SVC               */
         SVC   13                                               02S0166
*             RESPECIFY                                         02S0167
*              (GPR01F) UNRESTRICTED;                           02S0167
*           END;                    /* ABEND(APCTABND) FORCE ABEND   */
*           GEN NOREFS NOSETS NOSEQFLOW FLOWS(APCVZUKA);        02S0169
*                                                  /* NO CODE
*         END;                      /* ABEND ISSUED                  */
*       IF(RASCBPTR=APIOASCB)&      /* IF CORRECT ADDRESS SPACE &    */
*           ((RTCBPTR=RZERO)×       /* EITHER MEMORY-TERM/JOBSTEP OR */
*           (APIODEB->DEBTCBB=RTCBPTR)) THEN/* CORRECT SUBTASK       */
@RF00161 C     RASCBPTR,APIOASCB(,RAPIOPTR)                     02S0171
         BNE   @RF00171                                         02S0171
         CR    RTCBPTR,RZERO                                    02S0171
         BE    @RT00171                                         02S0171
         L     @02,APIODEB(,RAPIOPTR)                           02S0171
         L     @02,DEBTCBB-1(,@02)                              02S0171
         LA    @02,0(,@02)                                      02S0171
         CR    RTCBPTR,@02                                      02S0171
         BNE   @RF00171                                         02S0171
@RT00171 DS    0H                                               02S0172
*         DO;                       /* THEN MUST RELEASE.            */
*                                                               02S0172
*           /*********************************************************/
*           /*                                                       */
*           /* APIO FOUND WHICH MUST BE RELEASED (RAPIOPTR->APIO)    */
*           /*                                                       */
*           /*********************************************************/
*                                                               02S0173
*           IF APIOACTV^=0 THEN     /* IF APIO HAS OUTSTANDING I/O   */
         CLI   APIOACTV(RAPIOPTR),0                             02S0173
         BE    @RF00173                                         02S0173
*             DO;                   /* THEN MUST CLEAN-UP THE APIO.  */
*               IF SSOBFUNC=SSOBEOT THEN/* IF END-OF-TASK            */
         CLC   SSOBFUNC(2,RSSOBPTR),@CH00045                    02S0175
         BNE   @RF00175                                         02S0175
*                 DO;               /* START OF SEGMENT              */
*                                                               03S0176
*                   /*************************************************/
*                   /*                                               */
*                   /* APIO SHOWS ACTIVE. FREE THE ASSOCIATED I/O    */
*                   /* RESOURCES BY CALLING IGTKCA (TERMINATION      */
*                   /* ROUTINE). NOTE THAT THE I/O MUST HAVE BEEN    */
*                   /* TERMINATED BY NOW SINCE ALL DATASETS SHOULD   */
*                   /* HAVE BEEN CLOSED AND I/O PURGED BY EOT OR EOM.*/
*                   /*                                               */
*                   /*************************************************/
*                                                               03S0177
*                   DRETRY(04)=APESRTRY;/* SAVE CURRENT RETRY POINT  */
         L     @02,APESREGP                                     03S0177
         L     @15,APESRTRY(,@02)                               03S0177
         ST    @15,DRETRY+12                                    03S0177
*                   APESRTRY=ADDR(IOACTV1);/* NEW RETRY POINT        */
         LA    @15,IOACTV1                                      03S0178
         ST    @15,APESRTRY(,@02)                               03S0178
*                   GEN(SPACE ,)                                03S0179
*                   NOREFS NOSETS FLOWS(IOACTV1) SEQFLOW;/* PL/S FLOW
*                                      ANALYSIS                      */
         SPACE ,
*                   DO;             /* FORCE ABEND TO LOG ERROR &
*                                      PASS CONTROL TO THE (E)STAE OR
*                                      FRR EXIT ROUTINE.             */
*                     DO;           /* ABEND(APCTABND)               */
*                       RESPECIFY                               04S0182
*                        (GPR01F) RESTRICTED;                   04S0182
*                       GPR01F=APCTABND;/* COMP CODE IN REG 1        */
         L     GPR01F,APCTABND(,APCTPTR)                        04S0183
*                       SVC(13);    /* ISSUE ABEND SVC               */
         SVC   13                                               04S0184
*                       RESPECIFY                               04S0185
*                        (GPR01F) UNRESTRICTED;                 04S0185
*                     END;          /* ABEND(APCTABND) FORCE ABEND   */
*                     GEN NOREFS NOSETS NOSEQFLOW FLOWS(APCVZUKA);
*                                                  /* NO CODE
*                   END;            /* ABEND ISSUED                  */
*IOACTV1:                           /* STAE RECOVERY POINT           */
*                   DO;             /* ENTRY FROM ESTAE/FRR RETRY    */
*                                                               03S0189
IOACTV1  DS    0H                                               03S0190
*                     /***********************************************/
*                     /*                                             */
*                     /* ENTRY FROM ESTAE/FRR RETRY (IGTUKA). RETRY  */
*                     /* DOES NOT INSURE REGISTERS 0-5, 8-11, 14-15  */
*                     /* VALID SINCE NOT SAVED IN APESREGS. ISSUE    */
*                     /* DUMMY RESTRICT TO WIPE PL/S OPTIMIZATION    */
*                     /* MEMORY OF CURRENT REGISTER VALUES           */
*                     /*                                             */
*                     /***********************************************/
*                                                               03S0190
*                     RFY                                       03S0190
*                      (R0RSTD,                                 03S0190
*                       R1RSTD,                                 03S0190
*                       R2RSTD,                                 03S0190
*                       R3RSTD,                                 03S0190
*                       R4RSTD,                                 03S0190
*                       R5RSTD,     /* 6-7 BASES                     */
*                       R8RSTD,                                 03S0190
*                       R9RSTD,                                 03S0190
*                       R10RSTD,                                03S0190
*                       R11RSTD,    /* 12-13 APCT/APSV PTRS          */
*                       R14RSTD,                                03S0190
*                       R15RSTD) RSTD;/* KEEP PL/S FROM CARRYING DATA*/
*                     GEN(SPACE ,); /* NULL CODE                     */
         SPACE ,
*                     RFY                                       03S0192
*                      (R0RSTD,                                 03S0192
*                       R1RSTD,                                 03S0192
*                       R2RSTD,                                 03S0192
*                       R3RSTD,                                 03S0192
*                       R4RSTD,                                 03S0192
*                       R5RSTD,     /* 6-7 BASES                     */
*                       R8RSTD,                                 03S0192
*                       R9RSTD,                                 03S0192
*                       R10RSTD,                                03S0192
*                       R11RSTD,    /* 12-13 APCT/APSV PTRS          */
*                       R14RSTD,                                03S0192
