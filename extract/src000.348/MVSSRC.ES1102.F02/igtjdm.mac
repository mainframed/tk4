*   END;                            /* RETRY ENVIRONMENT SET         */
*   APESRTRY=DRETRY(03);            /* RESTORE ORIGINAL RECOVERY ADDR*/
         L     @08,DRETRY+8                                     01S0342
         L     @01,APESREGP                                     01S0342
         ST    @08,APESRTRY(,@01)                               01S0342
*   GEN(MODESET EXTKEY=SCHED)                                   01S0343
*   NOREFS NOSETS;                  /* SWITCH TO APCT KEY            */
         MODESET EXTKEY=SCHED
*   APCTAPIO=0;                     /* CLEAR THE APIO CHAIN          */
