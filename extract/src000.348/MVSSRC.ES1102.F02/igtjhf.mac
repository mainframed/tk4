TJHF     TITLE 'IGTJHF - 3838 SUBSYSTEM ALLOCATION RECOVERY ROUTINE.   *
                        '
IGTJHF   CSECT ,                                                01S0001
@MAINENT DS    0H                                               01S0001
         USING *,@15                                            01S0001
         B     @PROLOG                                          01S0001
         DC    AL1(16)                                          01S0001
         DC    C'IGTJHF    77.092'                              01S0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                  01S0001
         BALR  @07,0                                            01S0001
@PSTART  DS    0H                                               01S0001
         USING @PSTART,@07                                      01S0001
         L     @00,@SIZDATD                                     01S0001
         IGTYGSV  R,LV=(0)
         LR    @06,@01                                          01S0001
         USING @DATD,@06                                        01S0001
         ST    @13,@SA00001+4                                   01S0001
         LM    @00,@01,20(@13)                                  01S0001
         ST    @06,8(,@13)                                      01S0001
         LR    @13,@06                                          01S0001
*   SSDDRPTR=FIRSTARB;              /* POINT TO FIRST/ONLY REQUEST IN
*                                      CASE WAITING NOT ALLOWED      */
         L     SSDDRPTR,FIRSTARB(,ALLCWAPT)                     01S0063
*                                                               02S0064
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS SEGMENT CREATES AN ECB LIST FOR USE FOLLOWING AN ENQ WITH*/
*   /* ECB OPTION.                                                   */
*   /*                                                               */
*   /*****************************************************************/
*                                                               02S0064
*   DO;                             /* INSURE ENTIRE SEGMENT IS INCLD*/
*     THEECBP=ADDR(ENQECB);         /* POINTER TO ENQ ECB            */
         LA    @10,ENQECB                                       02S0065
         ST    @10,THEECBP                                      02S0065
*     CNCLECBP=ACNCLECB;            /* POINTER TO CANCEL ECB OR DUMMY*/
         L     @10,ACNCLECB(,ALLCWAPT)                          02S0066
         ST    @10,CNCLECBP                                     02S0066
*     LASTECBI=BON;                 /* ADDRESSES SET, INDICATE LAST  */
         OI    LASTECBI,B'10000000'                             02S0067
*   END;                            /* END OF SEGMENT.               */
*   IF SSAGWAIT=BON THEN            /* IF WAITING IS ALLOWED         */
         L     @10,SSOBPTR(,ALLCWAPT)                           01S0069
         L     @10,SSOBINDV(,@10)                               01S0069
         TM    SSAGWAIT(@10),B'10000000'                        01S0069
         BNO   @RF00069                                         01S0069
*     DO;                           /* THEN WE COULD WAIT            */
*       IF ALLWAIT=BOFF THEN        /* AND WE HAVE NOT YET WAITED    */
         TM    ALLWAIT(ALLCWAPT),B'01000000'                    01S0071
         BNZ   @RF00071                                         01S0071
*         CALL APCVZJHT;            /* TEST REASONABLENESS OF REQUEST
*                                      & ADEQUACY OF RESOURCES       */
         L     @15,APCTZJHT(,APCTPTR)                           01S0072
         BALR  @14,@15                                          01S0072
*       IF ALLWAIT=BON&ALLFAIL=BOFF THEN/* WILL WAIT AGAIN OR FOR FOR
*                                      THE FIRST TIME                */
@RF00071 TM    ALLWAIT(ALLCWAPT),B'01000000'                    01S0073
         BNO   @RF00073                                         01S0073
         TM    ALLFAIL(ALLCWAPT),B'10000000'                    01S0073
         BNZ   @RF00073                                         01S0073
*         DO;                       /* THEN EITHER HOLD OR NO HOLD   */
*           IF ALLHOLD=BON THEN     /* IF WAIT HOLDING RESOURCES THEN*/
         TM    ALLHOLD(ALLCWAPT),B'01000000'                    01S0075
         BNO   @RF00075                                         01S0075
*             DO;                   /* DO IT AGAIN. RESOURCES NOT YET
*                                      AVAILABLE.                    */
*               DO;                 /* INSURE ENTIRE SEGMENT INCLUDED*/
*                 GEN REFS(DEQLIST1,DQLIST1L,APCTENQM,APUBENQR) SETS(
*                     DYNDEQLA);                                02S0078
*                                   /*
         MVC   DYNDEQLA(L'DQLIST1L),DEQLIST1 INIT DYN DEQ AREA
         DEQ   (APCTENQM-APCT(,APCTPTR),APUBENQR,4,SYSTEM),            *
               MF=(E,DYNDEQLA)
*                 ALLAPUB=BOFF;     /* NO LONGER CONTROL RESOURCE    */
         NI    ALLAPUB(ALLCWAPT),B'11011111'                    02S0079
*               END;                                            02S0080
*               CALL APCVZJHW;      /* LOGICAL WAIT ON ALLOCATION
*                                      PENDING ECB.                  */
         L     @15,APCTZJHW(,APCTPTR)                           01S0081
         BALR  @14,@15                                          01S0081
*             END;                                              01S0082
*           ELSE                    /* WAIT, NOT HOLDING RESOURCES   */
*             DO;                   /* WAS SPECIFIED EARILIER. DO IT */
         B     @RC00075                                         01S0083
@RF00075 DS    0H                                               01S0084
*               CALL APCVZJHO;      /* AGAIN. RELEASE RESOURCES.     */
         L     @15,APCTZJHO(,APCTPTR)                           01S0084
         BALR  @14,@15                                          01S0084
*               DO;                 /* INSURE ENTIRE SEGMENT INCLUDED*/
*                 GEN REFS(DEQLIST2,DQLIST2L,APCTENQM,APUBENQR,LOCKOUT)
*                     SETS(DYNDEQLA);                           02S0086
*                                   /*
         MVC   DYNDEQLA(L'DQLIST2L),DEQLIST2 INIT DYN DEQ AREA
         DEQ  (APCTENQM-APCT(,APCTPTR),APUBENQR,4,SYSTEM,              *
               APCTENQM-APCT(,APCTPTR),LOCKOUT,7,SYSTEM),              *
               MF=(E,DYNDEQLA)
*                 ALLAPUB=BOFF;     /* NO LONGER CONTROL RESOURCE    */
*                 ALLSER=BOFF;      /* NO LONGER CONTROL RESOURCE    */
         NI    ALLAPUB(ALLCWAPT),B'01011111'                    02S0088
*               END;                                            02S0089
*               CALL APCVZJHW;      /* LOGICAL WAIT ON ALLC PEND ECB */
         L     @15,APCTZJHW(,APCTPTR)                           01S0090
         BALR  @14,@15                                          01S0090
*               ENQECB=0;           /* CLEAR ENQ ECB                 */
         SLR   @10,@10                                          02S0091
         ST    @10,ENQECB                                       02S0091
*               RFY                                             02S0092
*                 R15P RSTD;        /* NEED REG 15 FOR ENQ RETCODE   */
*               GEN REFS(APCTENQM,LOCKOUT,ENQECB,ENQLISTE,NQLISTEL)
*                   SETS(DYNQLIST);                             02S0093
*                                   /*
         MVC   DYNQLIST(L'NQLISTEL),ENQLISTE INIT ENQ DYN LIST AREA
         ENQ   (APCTENQM-APCT(,APCTPTR),LOCKOUT,E,7,SYSTEM),           *
               ECB=ENQECB,MF=(E,DYNQLIST)
*               ALLSER=BON;         /* INDICATE ENQ RES (TO BE) OWNED*/
         OI    ALLSER(ALLCWAPT),B'10000000'                     02S0094
*               IF R15P^=0&ENQRC^=8 THEN/* IF TASK DOES NOT HAVE
*                                      SHR'D                         */
         LTR   R15P,R15P                                        02S0095
         BZ    @RF00095                                         02S0095
         CLI   ENQRC(R15P),8                                    02S0095
         BE    @RF00095                                         02S0095
*                 DO;               /* CONTROL OR RESOURCE THEN WAIT */
*                   RFY                                         02S0097
*                     R15P UNRSTD;  /* NO LONGER NEED IT             */
*                   DO;             /* IGTYWAIT                 02S0098
*                                      (1)ECBLIST(ECBLIST)LONG(YES)  */
*                     RFY                                       02S0099
*                      (GPR00F,                                 02S0099
*                       GPR01P) RSTD;/* REGISTERS USED               */
*                     GPR01P=-ADDR(ECBLIST);/* POINT TO ECB LIST     */
         LA    GPR01P,ECBLIST                                   02S0100
         LCR   GPR01P,GPR01P                                    02S0100
*                     GPR00F=1×'80000000'X;/* WAIT COUNT             */
         L     GPR00F,@CF00846                                  02S0101
*                     SVC(1);       /* WAIT FOR ECB POSTING          */
         SVC   1                                                02S0102
*                     RFY                                       02S0103
*                      (GPR00F,                                 02S0103
*                       GPR01P) UNRSTD;/* FINISHED WITH REGISTERS    */
*                   END;            /* ECB WAIT COMPLETE LONG WAIT
*                                      WAIT FOR ONE TO COMPLETE      */
*                   IF CNCLECBP->ECBPOSTB=BON THEN/* IF POSTED,      */
         L     @10,CNCLECBP                                     02S0105
         TM    ECBPOSTB(@10),B'01000000'                        02S0105
         BNO   @RF00105                                         02S0105
*                     DO;           /* CANCELED. SO INDICATE.        */
*                       ALLFAIL=BON;/* ALLOCATION & JOB FAIL         */
*                                                               02S0107
         OI    ALLFAIL(ALLCWAPT),B'10000000'                    02S0107
*                       /*********************************************/
*                       /*                                           */
*                       /* IGTYJHE ERR(491)                          */
*                       /*                                           */
*                       /*********************************************/
*                                                               02S0108
*                       RFY                                     02S0108
*                         R1F RSTD;                             02S0108
*                       R1F=10;     /* PROVIDE INDEX                 */
         LA    R1F,10                                           02S0109
*                       CALL APCVZJHE;/* SPECIFY ERROR INFORMATION
*                                      PRIOR TO RETURN               */
         L     @15,APCTZJHE(,APCTPTR)                           02S0110
         BALR  @14,@15                                          02S0110
*                       RFY                                     02S0111
*                         R1F UNRSTD;/* DONE WITH REG PROVIDE ERR
*                                      INFO                          */
*                     END;                                      02S0112
*                 END;                                          02S0113
@RF00105 DS    0H                                               02S0114
*             END;                                              01S0114
@RF00095 DS    0H                                               01S0115
*           ENQECB=0;               /* CLEAR ENQ ECB                 */
@RC00075 SLR   @10,@10                                          02S0115
         ST    @10,ENQECB                                       02S0115
*           RFY                                                 02S0116
*             R15P RSTD;            /* NEED REG 15 FOR ENQ RETCODE   */
*           GEN REFS(APCTENQM,APUBENQR,ENQECB,ENQLISTE,NQLISTEL) SETS(
*               DYNQLIST);                                      02S0117
*                                   /*
         MVC   DYNQLIST(L'NQLISTEL),ENQLISTE INIT ENQ DYN LIST AREA
         ENQ   (APCTENQM-APCT(,APCTPTR),APUBENQR,E,4,SYSTEM),          *
               ECB=ENQECB,MF=(E,DYNQLIST)
*           ALLAPUB=BON;            /* INDICATE ENQ RES (TO BE) OWNED*/
         OI    ALLAPUB(ALLCWAPT),B'00100000'                    02S0118
*           IF R15P^=0&ENQRC^=8 THEN/* IF TASK DOES NOT HAVE SHR'D   */
         LTR   R15P,R15P                                        02S0119
         BZ    @RF00119                                         02S0119
         CLI   ENQRC(R15P),8                                    02S0119
         BE    @RF00119                                         02S0119
*             DO;                   /* CONTROL OR RESOURCE THEN WAIT */
*               RFY                                             02S0121
*                 R15P UNRSTD;      /* NO LONGER NEED IT             */
*               DO;                 /* IGTYWAIT                 02S0122
*                                      (1)ECBLIST(ECBLIST)LONG(NO)   */
*                 RFY                                           02S0123
*                  (GPR00F,                                     02S0123
*                   GPR01P) RSTD;   /* REGISTERS USED                */
*                 GPR01P=-ADDR(ECBLIST);/* POINT TO ECB LIST         */
         LA    GPR01P,ECBLIST                                   02S0124
         LCR   GPR01P,GPR01P                                    02S0124
*                 GPR00F=1;         /* WAIT COUNT                    */
         LA    GPR00F,1                                         02S0125
*                 SVC(1);           /* WAIT FOR ECB POSTING          */
         SVC   1                                                02S0126
*                 RFY                                           02S0127
*                  (GPR00F,                                     02S0127
*                   GPR01P) UNRSTD; /* FINISHED WITH REGISTERS       */
*               END;                /* ECB WAIT COMPLETE NOT SO LONG
*                                      WAIT WAIT FOR ONE TO COMPLETE */
*               IF CNCLECBP->ECBPOSTB=BON THEN/* IF POSTED,          */
         L     @10,CNCLECBP                                     02S0129
         TM    ECBPOSTB(@10),B'01000000'                        02S0129
         BNO   @RF00129                                         02S0129
*                 DO;               /* CANCELED. SO INDICATE.        */
*                   ALLFAIL=BON;    /* ALLOCATION & JOB FAIL         */
*                                                               02S0131
         OI    ALLFAIL(ALLCWAPT),B'10000000'                    02S0131
*                   /*************************************************/
*                   /*                                               */
*                   /* IGTYJHE ERR(490)                              */
*                   /*                                               */
*                   /*************************************************/
*                                                               02S0132
*                   RFY                                         02S0132
*                     R1F RSTD;                                 02S0132
*                   R1F=9;          /* PROVIDE INDEX                 */
         LA    R1F,9                                            02S0133
*                   CALL APCVZJHE;  /* SPECIFY ERROR INFORMATION
*                                      PRIOR TO RETURN               */
         L     @15,APCTZJHE(,APCTPTR)                           02S0134
         BALR  @14,@15                                          02S0134
*                   RFY                                         02S0135
*                     R1F UNRSTD;   /* DONE WITH REG PROVIDE ERR INFO*/
*                 END;                                          02S0136
*             END;                                              02S0137
@RF00129 DS    0H                                               02S0138
*           ALLOCNTR=APCTACTR;      /* SAVE COUNTER FOR IGTJHW       */
@RF00119 L     @10,APCTACTR(,APCTPTR)                           01S0138
         ST    @10,ALLOCNTR(,ALLCWAPT)                          01S0138
*         END;                      /* DO IF ALLWAIT = BON           */
*       SSDDRPTR=FIRSTARB;          /* POINT TO FIRST REQUEST IN CASE*/
@RF00073 L     SSDDRPTR,FIRSTARB(,ALLCWAPT)                     01S0140
*       IF ALLFAIL=BON THEN         /* IF ALLOCATION IS TERMINATING  */
         TM    ALLFAIL(ALLCWAPT),B'10000000'                    01S0141
         BNO   @RF00141                                         01S0141
*         DO UNTIL SSDDRPTR=0;      /* THEN INSURE THAT EACH UNSATIS-*/
@DL00142 DS    0H                                               01S0143
*           APDSPTR=SSAGSSCM;       /* FIED REQUEST HAS MESSAGE TO   */
         L     APDSPTR,SSAGSSCM(,SSDDRPTR)                      01S0143
*           IF APDSSLPS=UNAL THEN   /* INDICATE THAT WE HAD TROUBLE  */
         TM    APDSSLPS(APDSPTR),B'00000001'                    01S0144
         BNO   @RF00144                                         01S0144
*             DO;                   /* INDICATE WE HAD TROUBLE IN ARB*/
*                                                               01S0145
*               /*****************************************************/
*               /*                                                   */
*               /* IGTYJHE ERR(499)                                  */
*               /*                                                   */
*               /*****************************************************/
*                                                               01S0146
*               RFY                                             01S0146
*                 R0P RSTD;                                     01S0146
*               R0P=SSDDRPTR;                                   01S0147
         LR    R0P,SSDDRPTR                                     01S0147
*               RFY                                             01S0148
*                 R1F RSTD;                                     01S0148
*               R1F=15;             /* PROVIDE INDEX                 */
         LA    R1F,15                                           01S0149
*               CALL APCVZJHE;      /* SPECIFY ERROR INFORMATION
*                                      PRIOR TO RETURN               */
         L     @15,APCTZJHE(,APCTPTR)                           01S0150
         BALR  @14,@15                                          01S0150
*               RFY                                             01S0151
*                 R1F UNRSTD;       /* DONE WITH REG                 */
*               RFY                                             01S0152
*                 R0P UNRSTD;       /* PROVIDE ERR RETURN INFORMATION*/
*             END;                  /* DO IF APDSSLPS = UNAL         */
*           SSDDRPTR=SSAGNRBP;      /* ON TO NEXT OR DONE            */
@RF00144 L     SSDDRPTR,SSAGNRBP(,SSDDRPTR)                     01S0154
*         END;                      /* DO UNTIL SSDDRPTR = 0         */
@DE00142 LTR   SSDDRPTR,SSDDRPTR                                01S0155
         BNZ   @DL00142                                         01S0155
*     END;                          /* DO IF SSAGWAIT = BON          */
*   ELSE                            /* WAITING NOT ALLOWED           */
*     DO;                           /* JOB FAILS ALLOCATION ATTEMPT  */
         B     @RC00069                                         01S0157
@RF00069 DS    0H                                               01S0158
*       ALLFAIL=BON;                /* INDICATE END OF ATTEMPT       */
*                                                               01S0158
         OI    ALLFAIL(ALLCWAPT),B'10000000'                    01S0158
*       /*************************************************************/
*       /*                                                           */
*       /* IGTYJHE ERR(499)                                          */
*       /*                                                           */
*       /*************************************************************/
*                                                               01S0159
*       RFY                                                     01S0159
*         R0P RSTD;                                             01S0159
*       R0P=SSDDRPTR;                                           01S0160
         LR    R0P,SSDDRPTR                                     01S0160
*       RFY                                                     01S0161
*         R1F RSTD;                                             01S0161
*       R1F=15;                     /* PROVIDE INDEX                 */
         LA    R1F,15                                           01S0162
*       CALL APCVZJHE;              /* SPECIFY ERROR INFORMATION
*                                      PRIOR TO RETURN               */
         L     @15,APCTZJHE(,APCTPTR)                           01S0163
         BALR  @14,@15                                          01S0163
*       RFY                                                     01S0164
*         R1F UNRSTD;               /* DONE WITH REG                 */
*       RFY                                                     01S0165
*         R0P UNRSTD;               /* PROVIDE ERR RETURN INFORMATION*/
*     END;                          /* IGTXEPLG                      */
*                                                               01S0167
*   /*****************************************************************/
*   /*                                                               */
*   /* SYSTEM DECLARES                                               */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0167
*   DECLARE                         /* GENERAL PURPOSE REGISTERS     */
*     GPR00F FIXED(31) REG(0),                                  01S0167
*     GPR01P PTR(31) REG(1);                                    01S0167
@RC00069 DS    0H                                               01S0168
*   DECLARE                         /* COMMON VARIABLES              */
*     I256C CHAR(256) BASED,                                    01S0168
*     I031F FIXED(31) BASED,                                    01S0168
*     I031P PTR(31) BASED,                                      01S0168
*     I015F FIXED(15) BASED,                                    01S0168
*     I015P PTR(15) BASED,                                      01S0168
*     I008P PTR(8) BASED,                                       01S0168
*     I001C CHAR(1) BASED;                                      01S0168
*                                                               01S0169
*/*   MODULE PATCH AREA                                              */
*                                                               01S0169
*   DCL                                                         01S0169
*     APPATCH CHAR(*) INT STATIC LOCAL GEND;                    01S0169
*   GENERATE DATA DEFS(APPATCH);                                01S0170
*                                                               01S0171
*   /*****************************************************************/
*   /*                                                               */
*   /* GENERAL REGISTER DECLARES                                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0171
*   DCL                                                         01S0171
*     R0F FIXED(31) REG(0),                                     01S0171
*     R0P PTR(24) REG(0),                                       01S0171
*     R0RSTD PTR(31) REG(0),                                    01S0171
*     R1F FIXED(31) REG(1),                                     01S0171
*     R1P PTR(24) REG(1),                                       01S0171
*     R1RSTD PTR(31) REG(1),                                    01S0171
*     R2F FIXED(31) REG(2),                                     01S0171
*     R2P PTR(24) REG(2),                                       01S0171
*     R2RSTD PTR(31) REG(2),                                    01S0171
*     R3F FIXED(31) REG(3),                                     01S0171
*     R3P PTR(24) REG(3),                                       01S0171
*     R3RSTD PTR(31) REG(3),                                    01S0171
*     R4F FIXED(31) REG(4),                                     01S0171
*     R4P PTR(24) REG(4),                                       01S0171
*     R4RSTD PTR(31) REG(4),                                    01S0171
*     R5F FIXED(31) REG(5),                                     01S0171
*     R5P PTR(24) REG(5),                                       01S0171
*     R5RSTD PTR(31) REG(5),                                    01S0171
*     R6F FIXED(31) REG(6),                                     01S0171
*     R6P PTR(24) REG(6),                                       01S0171
*     R6RSTD PTR(31) REG(6),                                    01S0171
*     R7F FIXED(31) REG(7),                                     01S0171
*     R7P PTR(24) REG(7),                                       01S0171
*     R7RSTD PTR(31) REG(7),                                    01S0171
*     R8F FIXED(31) REG(8),                                     01S0171
*     R8P PTR(24) REG(8),                                       01S0171
*     R8RSTD PTR(31) REG(8),                                    01S0171
*     R9F FIXED(31) REG(9),                                     01S0171
*     R9P PTR(24) REG(9),                                       01S0171
*     R9RSTD PTR(31) REG(9),                                    01S0171
*     R10F FIXED(31) REG(10),                                   01S0171
*     R10P PTR(24) REG(10),                                     01S0171
*     R10RSTD PTR(31) REG(10),                                  01S0171
*     R11F FIXED(31) REG(11),                                   01S0171
*     R11P PTR(24) REG(11),                                     01S0171
*     R11RSTD PTR(31) REG(11),                                  01S0171
*     R12F FIXED(31) REG(12),                                   01S0171
*     R12P PTR(24) REG(12),                                     01S0171
*     R12RSTD PTR(31) REG(12),                                  01S0171
*     R13F FIXED(31) REG(13),                                   01S0171
*     R13P PTR(24) REG(13),                                     01S0171
*     R13RSTD PTR(31) REG(13),                                  01S0171
*     R14F FIXED(31) REG(14),                                   01S0171
*     R14P PTR(24) REG(14),                                     01S0171
*     R14RSTD PTR(31) REG(14),                                  01S0171
*     R15F FIXED(31) REG(15),                                   01S0171
*     R15P PTR(24) REG(15),                                     01S0171
*     R15RSTD PTR(31) REG(15),                                  01S0171
*                                                               01S0171
*     /***************************************************************/
*     /*                                                             */
*     /* VPSS CONTROL BLOCK POINTER DECLARES                         */
*     /*                                                             */
*     /***************************************************************/
