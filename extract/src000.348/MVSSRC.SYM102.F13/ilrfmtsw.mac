*                                                                  0231
*   /*****************************************************************/
*   /*                                                               */
*   /* SAR1:ILRMLINE ((ID,ASIS),SIZE,USE,FXDNX,MOVNX,DSNL) OF(SART)  */
*   /* LABEL VARIABLES REFERENCED IN FORMAT PATTERNS                 */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0231
*   DCL                                                            0231
*     SAR1LB1 CHAR(LENGTH('ID')) INIT('ID');                       0231
*   DCL                                                            0232
*     SAR1LB2 CHAR(LENGTH('SIZE')) INIT('SIZE');                   0232
*   DCL                                                            0233
*     SAR1LB3 CHAR(LENGTH('USE')) INIT('USE');                     0233
*   DCL                                                            0234
*     SAR1LB4 CHAR(LENGTH('FXDNX')) INIT('FXDNX');                 0234
*   DCL                                                            0235
*     SAR1LB5 CHAR(LENGTH('MOVNX')) INIT('MOVNX');                 0235
*   DCL                                                            0236
*     SAR1LB6 CHAR(LENGTH('DSNL')) INIT('DSNL');                   0236
*                                                                  0236
*   /*****************************************************************/
*   /*                                                               */
*   /* FORMAT PATTERNS FOR ONE LINE OF OUTPUT                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0237
*   DCL                                                            0237
*     1 SAR1PTS BDY(WORD),                                         0237
*      2 * CHAR(12) BDY(WORD),      /* FORMAT PATTERN SEE PTTRNS     */
*       3 * BIT(8) BDY(BYTE) INIT('00101111'B),/* SEE PTTFLGS        */
*       3 * FIXED(8) BDY(BYTE) INIT(LENGTH(SARID)-1+16*LENGTH('ID')-16)
*         ,                         /* SEE PTTLBLGH AND PTTDTLGH     */
*       3 * FIXED(8) INIT(5),                                      0237
*       3 * FIXED(8) INIT(11),                                     0237
*       3 * PTR(31) INIT(ADDR(SAR1LB1)),/* SEE PTTLBPTR              */
*       3 SAR1DT1 PTR(31) INIT(0),  /* SEE PTTDTPTR                  */
*      2 * CHAR(12) BDY(WORD),      /* FORMAT PATTERN SEE PTTRNS     */
*       3 * BIT(8) BDY(BYTE) INIT('00001111'B),/* SEE PTTFLGS        */
*       3 * FIXED(8) BDY(BYTE) INIT(LENGTH(SARSIZE)-1+16*LENGTH('SIZE')
*             -16),                 /* SEE PTTLBLGH AND PTTDTLGH     */
*       3 * FIXED(8) INIT(15+8),                                   0237
*       3 * FIXED(8) INIT(21+8),                                   0237
*       3 * PTR(31) INIT(ADDR(SAR1LB2)),/* SEE PTTLBPTR              */
*       3 SAR1DT2 PTR(31) INIT(0),  /* SEE PTTDTPTR                  */
*      2 * CHAR(12) BDY(WORD),      /* FORMAT PATTERN SEE PTTRNS     */
*       3 * BIT(8) BDY(BYTE) INIT('00001111'B),/* SEE PTTFLGS        */
*       3 * FIXED(8) BDY(BYTE) INIT(LENGTH(SARUSE)-1+16*LENGTH('USE')-
