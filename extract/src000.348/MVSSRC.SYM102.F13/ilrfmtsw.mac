         ST    R0,SARTPTR                                          0120
*         CALL ACCESS;              /* ACCESS SART ITSELF            */
         L     @12,ABDPLPTR                                        0121
         L     @15,ADPLMEMA(,@12)                                  0121
         BALR  @14,@15                                             0121
*         IF R15^=GOOD THEN                                        0122
         SLR   @12,@12                                             0122
         CR    R15,@12                                             0122
         BE    @RF00122                                            0122
*           SARTPTR=NULLPTR;                                       0123
         ST    @12,SARTPTR                                         0123
*       END;                                                       0124
*     ELSE                                                         0125
*       SARTPTR=NULLPTR;                                           0125
         B     @RC00116                                            0125
@RF00116 SLR   @12,@12                                             0125
         ST    @12,SARTPTR                                         0125
*     RFY                                                          0126
