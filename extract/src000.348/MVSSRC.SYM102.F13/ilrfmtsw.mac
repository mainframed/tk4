@RC00157 DS    0H                                                  0161
*       END;                                                       0161
*       IF AIAPTR^=NULLPTR THEN     /* THE QUEUE IS NOT EMPTY        */
         LTR   AIAPTR,AIAPTR                                       0162
         BZ    @RF00162                                            0162
