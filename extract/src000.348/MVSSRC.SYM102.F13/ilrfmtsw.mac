         L     @12,ABDPLPTR                                        0113
         LR    R1,@12                                              0113
*     R0=ADDR(ASMSART);             /* ADDR OF CELL WITH PTR         */
         L     @08,ASMVTPTR                                        0114
         LA    R0,ASMSART(,@08)                                    0114
*     CALL ACCESS;                  /* ACCESS PTR TO SART            */
         L     @15,ADPLMEMA(,@12)                                  0115
         BALR  @14,@15                                             0115
*     IF R15=GOOD THEN                                             0116
         LTR   R15,R15                                             0116
