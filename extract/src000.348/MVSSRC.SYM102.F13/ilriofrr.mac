*/********************************************************************/
*/*                                                                  */
*/*  QTOPARTQ IS AN INTERNAL ROUTINE THAT QUEUES A CHAIN OF AIA'S TO */
*/*           THE PART AIA ERROR QUEUE.                              */
*/*                                                                  */
*/*  INPUT - AIAPTR CONTAINS THE PTR TO THE CHAIN OF AIA'S OR 0      */
*/*                                                                  */
*/*  PROCESS - IF AIAPTR IS NOT 0, INDICATING THAT THERE ARE AIA'S   */
*/*            ON THE CHAIN, THE WHOLE CHAIN IS ADDED TO THE PART    */
*/*            AIA ERROR QUEUE (VIA CS).                             */
*/*                                                                  */
*/********************************************************************/
*                                                                  0651
*QTOPARTQ:                                                         0651
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0651
QTOPARTQ DS    0H                                                  0652
*   RFY                                                            0652
*    (AIAPTR,                                                      0652
*     R14) RSTD;                    /* RESTRICT REGS CONTAINING INPUT
*                                      PARAMETER AND RETURN ADDRESS  */
*   RFY                                                            0653
*     R13 RSTD;                     /* RESTRICT REG RESTRICTED ACROSS
*                                      CALL TO THIS RTN              */
*   IF AIAPTR^=ZERO THEN            /* TEST IF THERE ARE ANY AIA'S ON
*                                      THE INPUT CHAIN               */
         LTR   AIAPTR,AIAPTR                                       0654
         BZ    @RF00654                                            0654
*     DO;                           /* IF THERE ARE, QUEUE THEM TO 0655
*                                      THE PART AIA ERROR QUEUE      */
*       LASTAIA=AIAPTR;             /* INIT PTR TO LAST AIA          */
         LR    LASTAIA,AIAPTR                                      0656
*       DO WHILE(LASTAIA->AIANXAIA^=ZERO);/* LOOP THROUGH INPUT CHAIN
*                                      TO FIND LAST AIA              */
         B     @DE00657                                            0657
@DL00657 DS    0H                                                  0658
*         LASTAIA=LASTAIA->AIANXAIA;/* UPDATE PTR TO LAST AIA        */
         L     LASTAIA,AIANXAIA(,LASTAIA)                          0658
*       END;                        /* UPON COMPLETION OF THIS LOOP,
*                                      LASTAIA WILL POINT TO THE LAST
*                                      AIA ON THE CHAIN              */
@DE00657 ICM   @15,15,AIANXAIA(LASTAIA)                            0659
         BNZ   @DL00657                                            0659
*       RFY                                                        0660
*         WORK1 RSTD;               /* RESTRICT REG USED FOR CS      */
*       WORK1=PARTAIAE;             /* GET PTR TO AIA ERROR QUEUE    */
         L     @15,PARTPTR(,ASMVTPTR)                              0661
         L     WORK1,PARTAIAE(,@15)                                0661
*REDOAIAE:                          /* RETRY POINT FOR CS            */
*       LASTAIA->AIANXAIA=WORK1;    /* PLACE INPUT CHAIN ON AIA ERROR
*                                      QUEUE                         */
REDOAIAE ST    WORK1,AIANXAIA(,LASTAIA)                            0662
*       CS(WORK1,AIAPTR,PARTAIAE);  /* TRY TO UPDATE AIA ERROR QUEUE
*                                      IN PART                       */
         L     @15,PARTPTR(,ASMVTPTR)                              0663
         CS    WORK1,@11,PARTAIAE(@15)                             0663
*       BC(NE,REDOAIAE);            /* IF CS FAILED, TRY IT AGAIN    */
         BC    7,REDOAIAE                                          0664
*       RFY                                                        0665
*        (AIAPTR,                                                  0665
*         WORK1,                                                   0665
*         R13,                                                     0665
*         R14) UNRSTD;              /* RELEASE REGS                  */
*     END;                          /* END OF SECTION TO PROCESS   0666
*                                      NON-ZERO INPUT CHAIN          */
*   END QTOPARTQ;                                                  0667
@EL00002 DS    0H                                                  0667
@EF00002 DS    0H                                                  0667
@ER00002 BR    @14                                                 0667
