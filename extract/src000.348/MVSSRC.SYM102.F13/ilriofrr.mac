*/*                                                                  */
*/********************************************************************/
*                                                                  0572
*ILRCQIOE:                                                         0572
*   ENTRY;                          /* QIOE RECOVERY SUBRTN          */
@EP00572 DS    0H                                                  0573
*   RFY                                                            0573
*    (SAVEPTR,                                                     0573
*     R14) RSTD;                    /* RESTRICT REG CONTAINING RETURN
*                                      ADDRESS AND REG USED TO POINT
*                                      TO REG SAVE AREA              */
*   RETURN1=R14;                    /* SAVE RETURN ADDRESS           */
         ST    R14,RETURN1                                         0574
*   RFY                                                            0575
*     R14 UNRSTD;                   /* RELEASE REG                   */
*   SAVEPTR=ADDR(ASMSAVE);          /* USE ASMSAVE AS REG SAVE AREA
*                                      SINCE SALLOC LOCK IS HELD     */
