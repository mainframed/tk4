*/********************************************************************/
*/*                                                                  */
*/*  PMSCHEDL IS AN INTERNAL ROUTINE THAT SCHEDULES THE SRB FOR PART */
*/*           MONITOR IF IT IS NOT ALREADY SCHEDULED.                */
*/*                                                                  */
*/*  INPUT - NONE                                                    */
*/*                                                                  */
*/*  PROCESS - IF ASMSRBCT INDICATES THAT THE SRB FOR PART MONITOR   */
*/*            IS NOT SCHEDULED, SET ASMSRBCT (VIA CS) AND SCHEDULE  */
*/*            THE SRB (NOTE: THE SCHEDULE DESTROYS REGS 0 & 1)      */
*/*                                                                  */
*/********************************************************************/
*                                                                  0668
*PMSCHEDL:                                                         0668
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0668
PMSCHEDL DS    0H                                                  0669
*   RFY                                                            0669
*     R14 RSTD;                     /* RESTRICT REG CONTAINING RETURN
*                                      ADDRESS                       */
*   RFY                                                            0670
*     R13 RSTD;                     /* RESTRICT REG RESTRICTED ACROSS
*                                      CALL TO THIS RTN              */
*   IF ASMSRBCT=ZERO THEN           /* IF SRB IS NOT SCHEDULED       */
         ICM   @15,15,ASMSRBCT(ASMVTPTR)                           0671
         BNZ   @RF00671                                            0671
*     DO;                           /* SCHEDULE SRB AND INCREMENT  0672
*                                      COUNT IN ASMVT                */
*       RFY                                                        0673
*        (WORK1,                                                   0673
*         WORK2) RSTD;              /* RESTRICT REGS USED FOR CS     */
*       WORK1=ZERO;                 /* SRB COUNT SHOULD BE 0         */
         SLR   WORK1,WORK1                                         0674
*       WORK2=ONE;                  /* TRY TO SET SRB COUNT TO 1     */
         LA    WORK2,1                                             0675
*       CS(WORK1,WORK2,ASMSRBCT);   /* TRY TO SET SRB COUNT IN ASMVT */
         CS    WORK1,@06,ASMSRBCT(ASMVTPTR)                        0676
*       BC(NE,NOSCHED);             /* IF CS FAILED, DO NOT RETRY IT */
         BC    7,NOSCHED                                           0677
*       RFY                                                        0678
*        (WORK1,                                                   0678
*         WORK2) UNRSTD;            /* RELEASE REGS                  */
*       RFY                                                        0679
*         R1 RSTD;                  /* RESTRICT REG TO POINT TO SRB  */
*       R1=ASMPSRB;                 /* GET ADDR OF PART MONITOR SRB  */
         L     R1,ASMPSRB(,ASMVTPTR)                               0680
*       GEN(SCHEDULE SRB=(1),SCOPE=GLOBAL)                         0681
*       REFS(CVTGSMQ,SRBFLNK,SRBSECT);/* SCHEDULE SRB FOR PART     0681
*                                      MONITOR                       */
         SCHEDULE SRB=(1),SCOPE=GLOBAL
*       RFY                                                        0682
*         R1 UNRSTD;                /* RELEASE REG                   */
*     END;                                                         0683
*NOSCHED:                           /* LABEL FOR BRANCH FOLLOWING  0684
*                                      UNSUCCESSFUL CS               */
*   RFY                                                            0684
*    (R13,                                                         0684
*     R14) UNRSTD;                  /* RELEASE REGS                  */
*   END PMSCHEDL;                                                  0685
@EL00003 DS    0H                                                  0685
@EF00003 DS    0H                                                  0685
@ER00003 BR    @14                                                 0685
