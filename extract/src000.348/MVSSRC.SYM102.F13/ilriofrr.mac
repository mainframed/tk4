         B     @DE00692                                            0692
@DL00692 DS    0H                                                  0693
*         LASTIOE=LASTIOE->IOENXT;  /* UPDATE PTR TO LAST IOE        */
         L     LASTIOE,IOENXT(,LASTIOE)                            0693
