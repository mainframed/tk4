*/********************************************************************/
*/*                                                                  */
*/*  FREEACE IS AN INTERNAL ROUTINE THAT FREES THE CELL USED FOR AN  */
*/*          ACE.                                                    */
*/*                                                                  */
*/*  INPUT - CELLPTR CONTAINS THE PTR TO THE ACE                     */
*/*                                                                  */
*/*  PROCESS - THE ILRGMA MACRO IS USED TO FREE THE ACE (NOTE:  THE  */
*/*            ILRGMA MACRO DESTROYS REG 1).                         */
*/*                                                                  */
*/********************************************************************/
*                                                                  0769
*FREEACE:                                                          0769
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0769
FREEACE  DS    0H                                                  0770
*   RFY                                                            0770
*    (CELLPTR,                                                     0770
*     R14) RSTD;                    /* RESTRICT REGS CONTAINING INPUT
*                                      PARM AND RETURN ADDRESS       */
*   RETURN1=R14;                    /* SAVE RETURN ADDRESS           */
         ST    R14,RETURN1                                         0771
*   RFY                                                            0772
*     R14 UNRSTD;                   /* RELEASE REG                   */
*                                                                  0772
*   /*****************************************************************/
*   /*                                                               */
*   /* BEGIN ILRGMA MACRO EXPANSION                                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0773
*   DO;                             /* ILRGMA FREE                 0773
*                                      CELL(ACE)WORKREGS(WORK1,WORK2)*/
*     DECLARE                       /* ASMVT VECTOR TO ILRPEX        */
*       ILRPEX ENTRY BASED(ASMPEX) VALRG(0);                       0774
*     DECLARE                       /* MAP FOR CELL CONTROL WORDS    */
*       1 GMACELL BASED BDY(WORD),                                 0775
*        2 GMANEXT PTR(31);         /* PTR NEXT FREE CELL            */
*     RESPECIFY                                                    0776
*      (R0,                                                        0776
*       R1,                                                        0776
*       WORK1,                                                     0776
*       WORK2,                                                     0776
*       R14,                                                       0776
*       R15) RESTRICTED;                                           0776
*     RESPECIFY                                                    0777
*       ASMPOOLS BASED(WORK1);                                     0777
*     RESPECIFY                                                    0778
*       GMACELL BASED(R1);                                         0778
*     WORK1=ADDR(ASMACEPC);                                        0779
         LA    WORK1,ASMACEPC(,ASMVTPTR)                           0779
*     LM(R14,R15,ASMCPRSQ);         /* GET CONTENTS OF Q             */
         LM    R14,R15,ASMCPRSQ(WORK1)                             0780
*GMA00031:                                                         0781
*     IF R14^=0 THEN                /* TEST RESERVE Q                */
GMA00031 LTR   R14,R14                                             0781
         BZ    @RF00781                                            0781
*       DO;                         /* REPLENISH ACE RESERVE Q       */
*         R0=R14-1;                 /* SET UP FOR COMPARE AND SWAP   */
         LR    R0,R14                                              0783
         BCTR  R0,0                                                0783
*         GMANEXT=R15;              /* CELL TO FREE IN R1            */
         ST    R15,GMANEXT(,R1)                                    0784
*         CDS(R14,R0,ASMCPRSQ);     /* ADD CELL TO RESERVE Q         */
         CDS   R14,R0,ASMCPRSQ(WORK1)                              0785
*         BC(7,GMA00031);                                          0786
         BC    7,GMA00031                                          0786
*       END;                                                       0787
*     ELSE                                                         0788
*       DO;                         /* RETURN ACE TO NORMAL Q        */
         B     @RC00781                                            0788
@RF00781 DS    0H                                                  0789
*         R14=ASMCPAVL;                                            0789
         L     R14,ASMCPAVL(,WORK1)                                0789
*GMA00032:                                                         0790
*         GMANEXT=R14;              /* PUT ADDR CURRENT FIRST IN   0790
*                                      INPUT CELL                    */
GMA00032 ST    R14,GMANEXT(,R1)                                    0790
*         CS(R14,R1,ASMCPAVL);      /* ADD INPUT CELL TO POOL        */
         CS    R14,@01,ASMCPAVL(WORK1)                             0791
*         BC(7,GMA00032);                                          0792
         BC    7,GMA00032                                          0792
*       END;                        /* ACE RETURNED TO NORMAL Q      */
*     RESPECIFY                                                    0794
*      (R0,                                                        0794
*       R1,                                                        0794
*       WORK1,                                                     0794
*       WORK2,                                                     0794
*       R14,                                                       0794
*       R15) UNRESTRICTED;                                         0794
@RC00781 DS    0H                                                  0795
*   END;                            /* ILRGMA FREE                 0795
*                                      CELL(ACE)WORKREGS(WORK1,WORK2)*/
*                                                                  0795
*   /*****************************************************************/
*   /*                                                               */
*   /* END ILRGMA MACRO EXPANSION                                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0796
*                                   /* FREE CELL USED FOR ACE        */
*   RFY                                                            0796
*     R14 RSTD;                     /* RESTRICT REG USED FOR RETURN
*                                      ADDRESS                       */
*   R14=RETURN1;                    /* RESTORE RETURN ADDRESS        */
         L     R14,RETURN1                                         0797
*   RFY                                                            0798
*    (CELLPTR,                                                     0798
*     R14) UNRSTD;                  /* RELEASE REGS                  */
*   END FREEACE;                                                   0799
@EL00007 DS    0H                                                  0799
@EF00007 DS    0H                                                  0799
@ER00007 BR    @14                                                 0799
*   END ILRIOFRR                                                   0800
