*       CALL QUEUEIOE;              /* QUEUE IOE'S TO PART WRITE Q   */
         BAL   @14,QUEUEIOE                                        0627
*       IOEPTR=ASM2DUPQ;            /* POINT TO DUPLEXED IOE'S       */
         L     IOEPTR,ASM2DUPQ(,ASMVTPTR)                          0628
*       ASM2DUPQ=ZERO;              /* ZERO PTR TO DUPLEXED IOE'S    */
         SLR   @12,@12                                             0629
