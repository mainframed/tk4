*       CALL QUEUEIOE;              /* QUEUE IOE'S TO PART WRITE Q   */
         BAL   @14,QUEUEIOE                                        0631
*       RFY                                                        0632
*        (IOEPTR,                                                  0632
*         QUEUEPTR) UNRSTD;         /* RELEASE REGS                  */
*     END;                          /* END OF SECTION TO QUEUE IOE'S */
*                                                                  0634
