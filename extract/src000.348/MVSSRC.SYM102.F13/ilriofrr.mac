*/*           LGEPROCQ AND IS FOUR IF THE AIA OR ACE IS NOT FOUND.   */
*/*                                                                  */
*/********************************************************************/
*                                                                  0702
*DEQPROCQ:                                                         0702
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0702
DEQPROCQ DS    0H                                                  0703
*   RFY                                                            0703
*    (AIAPTR,                                                      0703
*     R14,                                                         0703
*     RETCODE) RSTD;                /* RESTRICT REGS CONTAINING INPUT
*                                      PARAMETER AND RETURN ADDRESS
*                                      AND REG USED FOR RETURN CODE  */
*   RFY                                                            0704
*     R2 RSTD;                      /* RESTRICT REG RESTRICTED ACROSS
*                                      CALL TO THIS RTN              */
*   RETCODE=ZERO;                   /* INIT RETURN CODE              */
         SLR   RETCODE,RETCODE                                     0705
*   IF AIAPTR=LGEPROCF THEN         /* TEST IF THE INPUT AIA IS THE
*                                      1ST ELEMENT OF THE LGEPROCQ   */
         C     AIAPTR,LGEPROCF(,LGEPTR)                            0706
         BNE   @RF00706                                            0706
*     DO;                           /* IT IS LGEPROCF                */
*       LGEPROCF=AIAFQPA;           /* RESET LGEPROCF                */
         L     @05,AIAFQPA(,AIAPTR)                                0708
         ST    @05,LGEPROCF(,LGEPTR)                               0708
*       IF LGEPROCF=ZERO THEN       /* TEST IF QUEUE IS NOW EMPTY    */
         SLR   @07,@07                                             0709
         CR    @05,@07                                             0709
         BNE   @RF00709                                            0709
*         LGEPROCL=ZERO;            /* IF IT IS, INSURE LGEPROCL IS
*                                      SET TO 0                      */
         ST    @07,LGEPROCL(,LGEPTR)                               0710
*       ELSE                        /* IF THE QUEUE IS NOT EMPTY     */
*         LGEPROCF->AIABQPA=ZERO;   /* ZERO THE BACK PTR OF THE FIRST
*                                      ELEMENT                       */
         B     @RC00709                                            0711
@RF00709 L     @05,LGEPROCF(,LGEPTR)                               0711
         SLR   @00,@00                                             0711
         ST    @00,AIABQPA(,@05)                                   0711
*     END;                                                         0712
*   ELSE                            /* IF THE INPUT AIA IS NOT THE 0713
*                                      1ST ELEMENT OF THE LGEPROCQ   */
*     IF AIAPTR=LGEPROCL THEN       /* TEST IF IT IS THE LAST ELEMENT
*                                      OF THE LGEPROCQ               */
         B     @RC00706                                            0713
@RF00706 C     AIAPTR,LGEPROCL(,LGEPTR)                            0713
         BNE   @RF00713                                            0713
*       DO;                         /* IT IS LGEPROCL                */
*         LGEPROCL=AIABQPA;         /* RESET LGEPROCL                */
         L     @05,AIABQPA(,AIAPTR)                                0715
         ST    @05,LGEPROCL(,LGEPTR)                               0715
*         LGEPROCL->AIAFQPA=ZERO;   /* SET THE FORWARD PTR OF THE  0716
*                                      LAST ELEMENT TO 0             */
         SLR   @00,@00                                             0716
         ST    @00,AIAFQPA(,@05)                                   0716
*       END;                                                       0717
*     ELSE                          /* THE INPUT AIA IS NEITHER THE
*                                      1ST NOR THE LAST ELEMENT OF 0718
*                                      THE LGEPROCQ                  */
*       DO;                         /* SEARCH THE LGEPROCQ FOR THE 0718
*                                      INPUT AIA AND DEQUEUE IT IF 0718
*                                      FOUND                         */
         B     @RC00713                                            0718
@RF00713 DS    0H                                                  0719
*         CURRAIA=LGEPROCF;         /* START SEARCH AT BEGINNING OF
*                                      LGEPROCQ                      */
         L     CURRAIA,LGEPROCF(,LGEPTR)                           0719
*         FINISH=OFF;               /* INIT FLAG TO TERMINATE LOOP   */
         NI    FINISH,B'10111111'                                  0720
*         DO WHILE(CURRAIA^=LGEPROCL&FINISH=OFF);/* LOOP THRU AIA'S
*                                      ON LGEPROCQ UNTIL END OF QUEUE
*                                      IS REACHED OR INPUT AIA IS  0721
*                                      FOUND                         */
         B     @DE00721                                            0721
@DL00721 DS    0H                                                  0722
*           IF CURRAIA=AIAPTR THEN  /* IF INPUT AIA IS FOUND         */
         CR    CURRAIA,AIAPTR                                      0722
         BNE   @RF00722                                            0722
*             DO;                   /* DEQUEUE IT FROM LGEPROCQ AND
*                                      TERMINATE LOOP                */
*               AIAFQPA->AIABQPA=AIABQPA;/* RESET THE BACK PTR OF THE
*                                      NEXT ELEMENT                  */
         L     @07,AIAFQPA(,AIAPTR)                                0724
         L     @06,AIABQPA(,AIAPTR)                                0724
         ST    @06,AIABQPA(,@07)                                   0724
*               AIABQPA->AIAFQPA=AIAFQPA;/* RESET THE FORWARD PTR OF
*                                      THE PREVIOUS ELEMENT          */
         ST    @07,AIAFQPA(,@06)                                   0725
*               FINISH=ON;          /* FORCE LOOP TO TERMINATE       */
         OI    FINISH,B'01000000'                                  0726
*             END;                                                 0727
*           ELSE                    /* IF INPUT AIA IS NOT FOUND     */
*             CURRAIA=CURRAIA->AIAFQPA;/* UPDATE PTR TO CURRENT AIA  */
         B     @RC00722                                            0728
@RF00722 L     CURRAIA,AIAFQPA(,CURRAIA)                           0728
*         END;                      /* END OF LOOP TO FIND INPUT AIA */
@RC00722 DS    0H                                                  0729
@DE00721 C     CURRAIA,LGEPROCL(,LGEPTR)                           0729
         BE    @DC00721                                            0729
         TM    FINISH,B'01000000'                                  0729
         BZ    @DL00721                                            0729
@DC00721 DS    0H                                                  0730
*         IF FINISH=OFF THEN        /* IF FINISH IS NOT SET AND THE
*                                      END OF THE LOOP HAS BEEN    0730
*                                      REACHED, THE INPUT AIA WAS NOT
*                                      FOUND                         */
         TM    FINISH,B'01000000'                                  0730
         BNZ   @RF00730                                            0730
*           RETCODE=FOUR;           /* SET AIA NOT FOUND RETURN CODE */
         LA    RETCODE,4                                           0731
*       END;                                                       0732
@RF00730 DS    0H                                                  0733
*   IF RETCODE=ZERO THEN            /* TEST IF AIA WAS FOUND ON THE
*                                      LGEPROCQ                      */
@RC00713 DS    0H                                                  0733
@RC00706 SLR   @12,@12                                             0733
         CR    RETCODE,@12                                         0733
         BNE   @RF00733                                            0733
*     DO;                                                          0734
*       AIAFQPA=ZERO;               /* 0 THE FORWARD PTR OF INPUT AIA*/
         ST    @12,AIAFQPA(,AIAPTR)                                0735
*       AIABQPA=ZERO;               /* 0 THE BACK PTR OF INPUT AIA   */
         ST    @12,AIABQPA(,AIAPTR)                                0736
*     END;                                                         0737
*   RFY                                                            0738
*    (AIAPTR,                                                      0738
*     R2,                                                          0738
*     R14) UNRSTD;                  /* RELEASE REGS                  */
*   END DEQPROCQ;                                                  0739
@EL00005 DS    0H                                                  0739
@EF00005 DS    0H                                                  0739
@ER00005 BR    @14                                                 0739
*                                                                  0740
*/********************************************************************/
*/*                                                                  */
*/*  DEQSTAGQ IS AN INTERNAL ROUTINE THAT DEQUEUES AN AIA FROM THE   */
*/*           ASMSTAGQ.                                              */
*/*                                                                  */
*/*  INPUT - AIAPTR CONTAINS THE PTR TO THE AIA                      */
*/*                                                                  */
*/*  PROCESS - THE INPUT AIA IS TESTED TO SEE IF IT IS ON THE        */
*/*            ASMSTAGQ (IF IT IS, IT IS EQUAL TO ASMSTAGL).  IF     */
*/*            THE AIA IS ON THE ASMSTAGQ, IT IS THEN DEQUEUED AND   */
*/*            THE COUNT OF REQUESTS RECEIVED IS DECREMENTED.        */
*/*                                                                  */
*/********************************************************************/
*                                                                  0740
*DEQSTAGQ:                                                         0740
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0740
DEQSTAGQ DS    0H                                                  0741
*   RFY                                                            0741
*    (AIAPTR,                                                      0741
*     R14) RSTD;                    /* RESTRICT REGS CONTAINING INPUT
*                                      PARM AND RETURN ADDRESS       */
*   IF AIAPTR=ASMSTAGL THEN         /* TEST IF INPUT AIA IS ON THE 0742
*                                      ASMSTAGQ                      */
         C     AIAPTR,ASMSTAGL(,ASMVTPTR)                          0742
         BNE   @RF00742                                            0742
*     DO;                           /* DEQUEUE AIA FROM ASMSTAGQ     */
*       IF AIAPTR=ASMSTAGF THEN     /* TEST IF INPUT AIA IS ALSO THE
*                                      FIRST AIA ON ASMSTAGQ         */
         C     AIAPTR,ASMSTAGF(,ASMVTPTR)                          0744
         BNE   @RF00744                                            0744
*         DO;                       /* REMOVING INPUT AIA WILL LEAVE
*                                      AN EMPTY ASMSTAGQ             */
*           ASMSTAGF=ZERO;          /* ZERO PTR TO FIRST AIA         */
         SLR   @12,@12                                             0746
         ST    @12,ASMSTAGF(,ASMVTPTR)                             0746
*           ASMSTAGL=ZERO;          /* ZERO PTR TO LAST AIA          */
         ST    @12,ASMSTAGL(,ASMVTPTR)                             0747
*         END;                                                     0748
*       ELSE                        /* OTHERWISE, THE INPUT AIA IS 0749
*                                      NOT THE ONLY AIA ON ASMSTAGQ  */
*         DO;                       /* FIND THE AIA PRIOR TO THE   0749
*                                      INPUT AIA                     */
         B     @RC00744                                            0749
@RF00744 DS    0H                                                  0750
*           LASTAIA=ASMSTAGF;       /* INIT PTR TO NEW LAST AIA      */
         L     LASTAIA,ASMSTAGF(,ASMVTPTR)                         0750
*           DO WHILE(LASTAIA->AIANXAIA^=AIAPTR);/* LOOP THROUGH AIA'S
*                                      ON ASMSTAGQ TILL AIA PRIOR TO
*                                      INPUT AIA IS FOUND            */
         B     @DE00751                                            0751
@DL00751 DS    0H                                                  0752
*             LASTAIA=LASTAIA->AIANXAIA;/* UPDATE PTR TO LAST AIA    */
         L     LASTAIA,AIANXAIA(,LASTAIA)                          0752
*           END;                    /* END OF LOOP TO FIND PRIOR AIA */
@DE00751 C     AIAPTR,AIANXAIA(,LASTAIA)                           0753
         BNE   @DL00751                                            0753
*           ASMSTAGL=LASTAIA;       /* SET PTR TO LAST AIA           */
         ST    LASTAIA,ASMSTAGL(,ASMVTPTR)                         0754
*           ASMSTAGL->AIANXAIA=ZERO;/* DEQUEUE INPUT AIA             */
         L     @12,ASMSTAGL(,ASMVTPTR)                             0755
         SLR   @05,@05                                             0755
         ST    @05,AIANXAIA(,@12)                                  0755
*         END;                                                     0756
*       ASMIORQR=ASMIORQR-ONE;      /* DECREMENT THE COUNT OF      0757
*                                      REQUESTS RECEIVED - DONE TO 0757
*                                      KEEP THIS COUNT ACCURATE, THE
*                                      COUNT WAS INCREMENTED BY    0757
*                                      ILRPAGIO AND BY ILRPOS BEFORE
*                                      THE AIA WAS ADDED TO THE    0757
*                                      ASMSTAGQ                      */
@RC00744 L     @12,ASMIORQR(,ASMVTPTR)                             0757
         BCTR  @12,0                                               0757
         ST    @12,ASMIORQR(,ASMVTPTR)                             0757
*       IF AIAPRIV=ON              /* IF AIA IS FOR A PRIVATE AREA
*                                      PAGE, OR                      */
*           AIAVIO=ON              /* AIA IS FOR A VIO PAGE, OR     */
*           AIASWPFX=ON THEN        /* AIA IS FOR A PRIVATE AREA   0758
*                                      FIXED PAGE, THEN ASHIOCNT HAS
*                                      BEEN INCREMENTED BY ILRPAGIO
*                                      OR ILRPOS                     */
         TM    AIAPRIV(AIAPTR),B'01100010'                         0758
         BZ    @RF00758                                            0758
*         DO;                       /* DECREMENT ASHIOCNT            */
*           PCBPTR=AIAPTR-(LENGTH(PCB)-LENGTH(AIA));/* CALCULATE THE
*                                      ASSOCIATED PCB                */
         LR    PCBPTR,AIAPTR                                       0760
         SH    PCBPTR,@CH02178                                     0760
*           RFY                                                    0761
*             RSMHDPTR RSTD;        /* RESTRICT REG TO POINT TO RSMHD*/
*           RSMHDPTR=PCBASCB->ASCBRSM;/* GET ADDR OF RSMHD           */
         L     @12,PCBASCB(,PCBPTR)                                0762
         L     RSMHDPTR,ASCBRSM(,@12)                              0762
*           ASHIOCNT=ASHIOCNT-ONE;  /* DECREMENT THE COUNT OF PRIVATE
*                                      AREA I/O REQUESTS, INCREMENTED
*                                      BY ILRPAGIO AND BY ILRPOS   0763
*                                      BEFORE THE AIA WAS ADDED TO 0763
*                                      THE ASMSTAGQ                  */
         L     @12,ASHIOCNT(,RSMHDPTR)                             0763
         BCTR  @12,0                                               0763
         ST    @12,ASHIOCNT(,RSMHDPTR)                             0763
*           RFY                                                    0764
*             RSMHDPTR UNRSTD;      /* RELEASE REG                   */
*         END;                      /* END OF SECTION TO DECREMENT 0765
*                                      ASHIOCNT                      */
*     END;                          /* END OF SECTION TO DEQUEUE   0766
*                                      INPUT AIA FROM ASMSTAGQ       */
*   RFY                                                            0767
*    (AIAPTR,                                                      0767
*     R14) UNRSTD;                  /* RELEASE REGS                  */
*   END DEQSTAGQ;                                                  0768
@EL00006 DS    0H                                                  0768
@EF00006 DS    0H                                                  0768
@ER00006 BR    @14                                                 0768
