         TITLE 'ILRIOFRR, I/O CONTROL RECOVERY                         *
                        '
ILRIOFRR CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL2(@EP00001-@MAINENT)
         DC    AL1(16)                                             0001
         DC    C'ILRIOFRR  76.091'                                 0001
ILRCQIOE DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL2(@EP00572-ILRCQIOE)
         ENTRY ILRCQIOE
         DROP  @15
@PROLOG  BALR  @10,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@10                                         0001
         AH    @15,4(,@15)                                         0001
         BR    @15                                                 0001
@EP00001 DS    0H                                                  0002
*                                      AREA ADDR AND RETURN ADDRESS  */
*   WORKADDR=R0;                    /* SAVE WORKAREA ADDR            */
         LR    WORKADDR,R0                                         0071
*   RFY                                                            0072
*     R0 UNRSTD;                    /* RELEASE REG                   */
*   WORKADDR->FRRWORKA=''B;         /* CLEAR WORKAREA                */
         XC    FRRWORKA(200,WORKADDR),FRRWORKA(WORKADDR)           0073
*   WAREG=WORKADDR+LENGTH(WORKAREA);/* GET ADDR OF PORTION OF WORK 0074
*                                      AREA USED BY RECOVERY SUBRTNS */
         LA    WAREG,100                                           0074
         ALR   WAREG,WORKADDR                                      0074
*   GEN REFS(WAREG)(USING @DATD,WAREG);/* GET ADDRESSABILITY TO THIS
*                                      PORTION OF WORKAREA           */
         USING @DATD,WAREG
*   RTMRTRN=R14;                    /* SAVE RETURN ADDR              */
         ST    R14,RTMRTRN(,WORKADDR)                              0076
*   RFY                                                            0077
*     R14 UNRSTD;                   /* RELEASE REG                   */
*   BASEADDR=BASEREG;               /* SAVE VALUE OF BASE REG        */
         ST    BASEREG,BASEADDR(,WORKADDR)                         0078
*   ASMVTPTR=CVTASMVT;              /* INIT PTR TO ASMVT             */
         L     @12,CVTPTR                                          0079
         L     ASMVTPTR,CVTASMVT(,@12)                             0079
*   ATAPTR=SDWAPARM;                /* INIT PTR TO ATA               */
*                                                                  0080
         L     ATAPTR,SDWAPARM(,SDWAPTR)                           0080
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE PARAMETERS FOR RECORDING.                          */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0081
*   SDWAMODN=MODIDS(ATAMODID);      /* PLACE MODULE NAME IN SDWA FOR
*                                      RECORDING                     */
         SLR   @12,@12                                             0081
         IC    @12,ATAMODID(,ATAPTR)                               0081
         SLA   @12,3                                               0081
         LA    @11,MODIDS-8(@12)                                   0081
         MVC   SDWAMODN(8,SDWAPTR),0(@11)                          0081
*   SDWAREXN=RCVRYRTN;              /* PLACE RECOVERY RTN NAME IN  0082
*                                      SDWA FOR RECORDING            */
         MVC   SDWAREXN(8,SDWAPTR),RCVRYRTN                        0082
*   SDWACSCT=NONAME;                /* PLACE DEFAULT CSECT NAME IN 0083
*                                      SDWA FOR RECORDING            */
         MVC   SDWACSCT(8,SDWAPTR),NONAME                          0083
*   SDWAVRA(1:LENGTH(ATA))=ATA;     /* COPY ATA INTO VARIABLE      0084
*                                      RECORDING AREA                */
         MVC   SDWAVRA(24,SDWAPTR),ATA(ATAPTR)                     0084
*   SDWAURAL=LENGTH(ATA);           /* INIT COUNT OF USED PORTION OF
*                                      VARIABLE RECORDING AREA       */
         MVI   SDWAURAL(SDWAPTR),X'18'                             0085
*   SDWARCRD=ON;                    /* REQUEST RECORDING IN ALL    0086
*                                      CASES, EVEN IF PERCOLATED TO  */
         OI    SDWARCRD(SDWAPTR),B'10000000'                       0086
*                                                                  0087
*   /*****************************************************************/
*   /*                                                               */
*   /* PERFORM COMMON VERIFICATIONS.                                 */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0087
*   RFY                                                            0087
*     SAVEPTR RSTD;                 /* RESTRICT REG USED TO POINT TO
*                                      REG SAVE AREA                 */
*   SAVEPTR=ADDR(ASMSAVE);          /* REG SAVE AREA ONLY USED WHEN
*                                      SALLOC LOCK IS HELD SO USE  0088
*                                      ASMSAVE - INITIALIZED HERE FOR
*                                      USE BY NON-EXTERNAL RECOVERY
*                                      ROUTINES                      */
         LA    SAVEPTR,ASMSAVE(,ASMVTPTR)                          0088
*   IF ATASWPDR=OFF THEN            /* TEST IF SWAP DRIVER WAS IN  0089
*                                      CONTROL - IF NOT THE SALLOC 0089
*                                      LOCK IS HELD                  */
         TM    ATASWPDR(ATAPTR),B'00100000'                        0089
         BNZ   @RF00089                                            0089
*     DO;                           /* DO COMMON VERIFICATION        */
*       RFY                                                        0091
*        (R5,                                                      0091
*         R6,                                                      0091
*         R7) RSTD;                 /* RESTRICT REGS DESTROYED BY  0091
*                                      CALL TO Q VERIFICATION RTN    */
*       CALL ILRVASGQ;              /* VERIFY ASMSTAGQ - SALLOC LOCK
*                                      MUST BE HELD                  */
         L     @15,@CV02316                                        0092
         BALR  @14,@15                                             0092
*       RFY                                                        0093
*        (R5,                                                      0093
*         R6,                                                      0093
*         R7) UNRSTD;               /* RELEASE REGS                  */
*       IF ATAMODID=ATAMPGCM THEN   /* CONTINUE VERIFICATIONS IF   0094
*                                      ERROR OCCURRED DURING PAGE  0094
*                                      COMPLETION PROCESSING         */
         CLI   ATAMODID(ATAPTR),2                                  0094
         BNE   @RF00094                                            0094
*         DO;                                                      0095
*           RFY                                                    0096
*             PARMPTR RSTD;         /* RESTRICT REG USED AS PARM TO
*                                      VERIFICATIONS RTNS            */
*           PARMPTR=ADDR(ASM3PIOP); /* LOCATE QUEUE OF AIA'S TO BE 0097
*                                      RETURNED TO PIOP              */
         LA    PARMPTR,ASM3PIOP(,ASMVTPTR)                         0097
*           RFY                                                    0098
*            (R5,                                                  0098
*             R6,                                                  0098
*             R7) RSTD;             /* RESTRICT REGS DESTROYED BY  0098
*                                      CALL TO Q VERIFICATION RTNS   */
*           CALL ILRVAIAQ;          /* VERIFY THIS QUEUE             */
         L     @15,@CV02317                                        0099
         BALR  @14,@15                                             0099
*           PARMPTR=ADDR(ASM3GENQ); /* LOCATE QUEUE OF AIA'S FOR   0100
*                                      PAGECOMP SUBRTN               */
         LA    PARMPTR,ASM3GENQ(,ASMVTPTR)                         0100
*           CALL ILRVAIAQ;          /* VERIFY THIS QUEUE             */
         L     @15,@CV02317                                        0101
         BALR  @14,@15                                             0101
*           PARMPTR=ADDR(ASM3SWPQ); /* LOCATE QUEUE OF AIA'S FOR   0102
*                                      SWAPCOMP SUBRTN               */
         LA    PARMPTR,ASM3SWPQ(,ASMVTPTR)                         0102
*           CALL ILRVSPAQ;          /* VERIFY THIS QUEUE             */
         L     @15,@CV02318                                        0103
         BALR  @14,@15                                             0103
*           RFY                                                    0104
*            (R5,                                                  0104
*             R6,                                                  0104
*             R7) UNRSTD;           /* RELEASE REGS                  */
*           RFY                                                    0105
*             PARMPTR UNRSTD;       /* RELEASE REG                   */
*         END;                      /* END OF VERIFICATIONS FOR PAGE
*                                      COMPLETION                    */
*     END;                          /* END OF VERIFICATIONS          */
*                                                                  0107
@RF00094 DS    0H                                                  0108
*   /*****************************************************************/
*   /*                                                               */
*   /* SAVE REGS DESTROYED BY EXTERNAL ROUTINES. THIS COMMOM SAVING  */
*   /* OF REGISTERS IS USEFUL ONLY FOR NON-EXTERNAL RECOVERY         */
*   /* SUBROUTINES.                                                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0108
*   SAVESDWA=SDWAPTR;               /* SAVE ADDR OF SDWA             */
@RF00089 ST    SDWAPTR,SAVESDWA                                    0108
*   STM(ASMVTPTR,ATAPTR,REGSAVE);   /* SAVE ADDR OF ASMVT AND ADDR OF
*                                      ATA                           */
         STM   ASMVTPTR,ATAPTR,REGSAVE                             0109
*                                                                  0110
*   /*****************************************************************/
*   /*                                                               */
*   /* IF ILRQIOE WAS IN CONTROL AT THE TIME OF ERROR, PERFORM       */
*   /* ILRQIOE RECOVERY.                                             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0110
*   IF ATAQIOE=ON THEN                                             0110
         TM    ATAQIOE(ATAPTR),B'10000000'                         0110
         BNO   @RF00110                                            0110
*     DO;                           /* QIOE RECOVERY                 */
*       SDWACSCT=QIOENAME;          /* PLACE QIOE NAME IN SDWA FOR 0112
*                                      RECORDING                     */
         MVC   SDWACSCT(8,SDWAPTR),QIOENAME                        0112
*       CALL ILRCQIOE;              /* INVOKE QIOE RECOVERY RTN      */
         L     @15,@CA02336                                        0113
         BALR  @14,@15                                             0113
*       GOTO DONE;                  /* RESUME MAINLINE PROCESSING    */
         B     DONE                                                0114
*     END;                          /* END OF QIOE RECOVERY          */
*                                                                  0116
*   /*****************************************************************/
*   /*                                                               */
*   /* IF ILRSLSQA WAS IN CONTROL AT THE TIME OF ERROR, PERFORM      */
*   /* ILRSLSQA RECOVERY.                                            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0116
*   IF ATASLSQA=ON THEN                                            0116
@RF00110 TM    ATASLSQA(ATAPTR),B'01000000'                        0116
         BNO   @RF00116                                            0116
*     DO;                           /* SLSQA RECOVERY                */
*       SDWACSCT=SLSQNAME;          /* PLACE SLSQA NAME IN SDWA FOR
*                                      RECORDING                     */
         MVC   SDWACSCT(8,SDWAPTR),SLSQNAME                        0118
*       CALL ILRCSLSQ;              /* INVOKE SLSQA RECOVERY RTN     */
         L     @15,@CV02313                                        0119
         BALR  @14,@15                                             0119
*       BASEREG=BASEADDR;           /* RESTORE VALUE OF BASE REG     */
         L     BASEREG,BASEADDR(,WORKADDR)                         0120
*       GOTO DONE;                  /* RESUME MAINLINE PROCESSING    */
         B     DONE                                                0121
*     END;                          /* END OF SLSQA RECOVERY         */
*                                                                  0123
*   /*****************************************************************/
*   /*                                                               */
*   /* IF SWAPCOMP SUBROUTINE OF ILRPAGCM WAS IN CONTROL AT THE TIME */
*   /* OF ERROR, PERFORM SWAPCOMP RECOVERY.                          */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0123
*   IF ATASCOMP=ON THEN                                            0123
@RF00116 TM    ATASCOMP(ATAPTR),B'00100000'                        0123
         BNO   @RF00123                                            0123
*     DO;                           /* SWAPCOMP RECOVERY             */
*       SDWACSCT=SCMPNAME;          /* PLACE SWAPCOMP NAME IN SDWA 0125
*                                      FOR RECORDING                 */
         MVC   SDWACSCT(8,SDWAPTR),SCMPNAME                        0125
*       RFY                                                        0126
*        (R5,                                                      0126
*         R6,                                                      0126
*         R7) RSTD;                 /* RESTRICT REGS DESTROYED BY  0126
*                                      CALL TO Q VERIFICATION RTNS   */
*       CALL ILRVSWTQ;              /* VERIFY SARWAITQ               */
         L     @15,@CV02320                                        0127
         BALR  @14,@15                                             0127
*       RFY                                                        0128
*         PARMPTR RSTD;             /* RESTRICT REG USED AS PARM FOR
*                                      VERIFICATION ROUTINES         */
*       PARMPTR=ADDR(ASM3TMPA);     /* POINT TO QUEUE OF AIA'S STILL
*                                      TO BE PROCESSED BY SWAPCOMP   */
         LA    PARMPTR,ASM3TMPA(,ASMVTPTR)                         0129
*       CALL ILRVSPAQ;              /* VERIFY THIS QUEUE             */
         L     @15,@CV02318                                        0130
         BALR  @14,@15                                             0130
*       PARMPTR=ATAAIA;             /* POINT TO AIA TO BE VERIFIED   */
         L     PARMPTR,ATAAIA(,ATAPTR)                             0131
*       RFY                                                        0132
*         RETCODE RSTD;             /* RESTRICT REG USED FOR RETURN
*                                      CODE                          */
*       RFY                                                        0133
*        (R2,                                                      0133
*         R3,                                                      0133
*         R4) RSTD;                 /* RESTRICT REGS DESTROYED BY  0133
*                                      CALL TO ELEMENT VERIFICATION
*                                      RTN                           */
*       CALL ILRVAIA;               /* VERIFY THIS AIA               */
         L     @15,@CV02322                                        0134
         BALR  @14,@15                                             0134
*       RFY                                                        0135
*        (R2,                                                      0135
*         R3,                                                      0135
*         R4,                                                      0135
*         R5,                                                      0135
*         R6,                                                      0135
*         R7) UNRSTD;               /* RELEASE REGS                  */
*       LM(ASMVTPTR,ATAPTR,REGSAVE);/* RESTORE REGS DESTROYED BY THE
*                                      CALL TO ILRVAIA               */
         LM    ASMVTPTR,ATAPTR,REGSAVE                             0136
*       IF RETCODE^=ZERO THEN       /* IF ILRVAIA INDICATED ATAAIA 0137
*                                      WAS NOT A VALID AIA (NON-ZERO
*                                      RETURN CODE)                  */
         SLR   @14,@14                                             0137
         CR    RETCODE,@14                                         0137
         BE    @RF00137                                            0137
*         ATAAIA=ZERO;              /* ZERO THE AIA PTR IN THE ATA   */
         ST    @14,ATAAIA(,ATAPTR)                                 0138
*       RFY                                                        0139
*        (PARMPTR,                                                 0139
*         RETCODE) UNRSTD;          /* RELEASE REGS                  */
@RF00137 DS    0H                                                  0140
*                                                                  0140
*       /*************************************************************/
*       /*                                                           */
*       /* RECOVER FROM AN ERROR DURING SWAPCOMP'S PROCESSING OF A   */
*       /* SWAP IN REQUEST.                                          */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0140
*       IF ATAASCB=ZERO THEN        /* IF ATAASCB IS 0, SWAPCOMP WAS
*                                      PROCESSING A SWAP IN REQUEST  */
         SLR   @12,@12                                             0140
         C     @12,ATAASCB(,ATAPTR)                                0140
         BNE   @RF00140                                            0140
*         DO;                       /* RECOVERY FOR SWAP IN REQUEST  */
*           IF ATAAIA^=ZERO THEN    /* TEST IF THERE IS A VALID AIA
*                                      CHECKPOINTED IN THE ATA       */
         L     @11,ATAAIA(,ATAPTR)                                 0142
         CR    @11,@12                                             0142
         BE    @RF00142                                            0142
*             DO;                   /* PROCESS THIS AIA              */
*               PCBPTR=ATAAIA-(LENGTH(PCB)-LENGTH(AIA));/* CALCULATE
*                                      RELATED PCB ADDR      @ZM43070*/
         LR    PCBPTR,@11                                          0144
         SH    PCBPTR,@CH02178                                     0144
*               ATAASCB=PCBASCB;    /* GET THE ADDR OF THE ASCB BEING
*                                      SWAPPED IN            @ZM43070*/
         MVC   ATAASCB(4,ATAPTR),PCBASCB(PCBPTR)                   0145
*               SWAPIN=ON;          /* SET INTERNAL FLAG TO INDICATE
*                                      MEMTERM IS FOR FAILURE DURING
*                                      SWAP-IN PROCESSING    @ZM43070*/
         OI    SWAPIN,B'00100000'                                  0146
*               ATAAIA->AIAERROR=ON;/* TURN ON THE INDETERMINATE   0147
*                                      ERROR FLAG IN THIS AIA        */
         OI    AIAERROR(@11),B'00000010'                           0147
*               RFY                                                0148
*                 AIAPTR RSTD;      /* RESTRICT REG TO POINT TO AIA  */
*               AIAPTR=ASM3PIOP;    /* POINT TO THE FIRST AIA ON THE
*                                      CHAIN OF AIA'S TO BE RETURNED
*                                      TO RSM                        */
         L     AIAPTR,ASM3PIOP(,ASMVTPTR)                          0149
*               DO WHILE(AIAPTR^=ZERO&AIAPTR^=ATAAIA);/* LOOP THROUGH
*                                      THIS CHAIN UNTIL THE END OF 0150
*                                      THE CHAIN IS REACHED OR ATAAIA
*                                      IS FOUND ON THE CHAIN         */
         B     @DE00150                                            0150
@DL00150 DS    0H                                                  0151
*                 AIAPTR=AIANXAIA;  /* POINT TO THE NEXT AIA         */
         L     AIAPTR,AIANXAIA(,AIAPTR)                            0151
*               END;                                               0152
@DE00150 LTR   AIAPTR,AIAPTR                                       0152
         BZ    @DC00150                                            0152
         C     AIAPTR,ATAAIA(,ATAPTR)                              0152
         BNE   @DL00150                                            0152
@DC00150 DS    0H                                                  0153
*               IF AIAPTR=ZERO THEN /* IF AIAPTR IS 0, THE END OF THE
*                                      CHAIN WAS REACHED WITHOUT   0153
*                                      FINDING ATAAIA                */
         LTR   AIAPTR,AIAPTR                                       0153
         BNZ   @RF00153                                            0153
*                 DO;               /* PLACE ATAAIA ON THE CHAIN TO
*                                      BE RETURNED TO RSM            */
*                   RFY                                            0155
*                     AIAPTR UNRSTD;/* RELEASE REG                   */
*                   ATAAIA->AIANXAIA=ASM3PIOP;/* QUEUE THE EXISTING
*                                      CHAIN TO ATAAIA               */
         L     @12,ATAAIA(,ATAPTR)                                 0156
         MVC   AIANXAIA(4,@12),ASM3PIOP(ASMVTPTR)                  0156
*                   ASM3PIOP=ATAAIA;/* PLACE ATAAIA AT THE HEAD OF 0157
*                                      THE CHAIN                     */
         ST    @12,ASM3PIOP(,ASMVTPTR)                             0157
*                   IF ATAAIA->AIAPAGDS=ON THEN/* IF THE AIA WAS FOR
*                                      A PAGE DATA SET               */
         TM    AIAPAGDS(@12),B'00000100'                           0158
         BNO   @RF00158                                            0158
*                     ASMIORQC=ASMIORQC+ONE;/* INCREMENT THE COUNT OF
*                                      COMPLETED REQUESTS            */
         LA    @12,1                                               0159
         AL    @12,ASMIORQC(,ASMVTPTR)                             0159
         ST    @12,ASMIORQC(,ASMVTPTR)                             0159
*                 END;                                             0160
@RF00158 DS    0H                                                  0161
*             END;                  /* END OF SECTION TO PROCESS A 0161
*                                      VALID AIA IN THE ATA          */
@RF00153 DS    0H                                                  0162
*         END;                      /* END OF SECTION TO PROCESS A 0162
*                                      SWAP IN REQUEST               */
@RF00142 DS    0H                                                  0163
*                                                                  0163
*       /*************************************************************/
*       /*                                                           */
*       /* TERMINATE THE MEMORY INVOLVED WHEN AN ERROR OCCURRED      */
*       /* DURING SWAPCOMP'S PROCESSING OF EITHER A SWAP IN OR A SWAP*/
*       /* OUT REQUEST. ALSO PERFORM MORE RECOVERY FOR A SWAP OUT.   */
*       /*                                                   @ZM43070*/
*       /*                                                           */
*       /*************************************************************/
*                                                                  0163
*       IF ATAASCB^=ZERO THEN       /* IF ATAASCB IS NON-0, TERMINATE
*                                      THE MEMORY, IF VALID  @ZM43070*/
@RF00140 L     @12,ATAASCB(,ATAPTR)                                0163
         LTR   @12,@12                                             0163
         BZ    @RF00163                                            0163
*         DO;                       /* TERMINATE THE MEMORY  @ZM43070*/
*           RFY                                                    0165
*            (PARMPTR,                                             0165
*             RETCODE) RSTD;        /* RESTRICT REGS USED FOR CALL TO
*                                      IEAVEGAS                      */
*           PARMPTR=ATAASCB;        /* POINT TO ASCB TO BE VERIFIED  */
         LR    PARMPTR,@12                                         0166
*           RFY                                                    0167
*            (R2,                                                  0167
*             R3,                                                  0167
*             R4,                                                  0167
*             R5,                                                  0167
*             R6,                                                  0167
*             R7) RSTD;             /* RESTRICT REGS DESTROYED BY  0167
*                                      CALL TO ASCB VERIFICATION RTN */
*           CALL IEAVEGAS;          /* VERIFY THE ASCB               */
         L     @15,@CV02325                                        0168
         BALR  @14,@15                                             0168
*           RFY                                                    0169
*            (R2,                                                  0169
*             R3,                                                  0169
*             R4,                                                  0169
*             R5,                                                  0169
*             R6,                                                  0169
*             R7) UNRSTD;           /* RELEASE REGS                  */
*           LM(ASMVTPTR,ATAPTR,REGSAVE);/* RESTORE REGS DESTROYED BY
*                                      THE CALL TO IEAVEGAS          */
         LM    ASMVTPTR,ATAPTR,REGSAVE                             0170
*           IF RETCODE=ZERO THEN    /* TEST FOR A RETURN CODE OF 0 -
*                                      INDICATING A VALID ASCB       */
         LTR   RETCODE,RETCODE                                     0171
         BNZ   @RF00171                                            0171
*             DO;                   /* SINCE THE ERROR OCURRED WHILE
*                                      EITHER A SWAP OUT OR A SWAP IN
*                                      WAS IN PROCESS, SET UP TO   0172
*                                      TERMINATE THE MEMORY  @ZM43070*/
*               RFY                                                0173
*                 RETCODE UNRSTD;   /* RELEASE REG                   */
*               ASCBPTR=PARMPTR;    /* POINT TO ASCB TO BE TERMINATED*/
         LR    ASCBPTR,PARMPTR                                     0174
*               RFY                                                0175
*                 RSMHDPTR RSTD;    /* RESTRICT REG TO POINT TO RSMHD*/
*               RSMHDPTR=ASCBRSM;   /* GET ADDR OF RSMHD             */
         L     RSMHDPTR,ASCBRSM(,ASCBPTR)                          0176
*               IF SWAPIN=ON THEN   /* IF ERROR OCCURRED DURING A  0177
*                                      SWAP IN REQUEST       @ZM43070*/
         TM    SWAPIN,B'00100000'                                  0177
         BNO   @RF00177                                            0177
*                 RSMFAIL=ON;       /* TURN ON RSMFAIL TO INDICATE 0178
*                                      LSQA IS NOT AVAILABLE @ZM43070*/
         OI    RSMFAIL(RSMHDPTR),B'00000010'                       0178
*               ELSE                /* IF ERROR OCCURRED DURING A  0179
*                                      SWAP OUT REQUEST      @ZM43070*/
*                 DO;               /* PERFORM FURTHER RECOVERY FOR A
*                                      SWAP OUT REQUEST      @ZM43070*/
         B     @RC00177                                            0179
@RF00177 DS    0H                                                  0180
*                   PARMPTR=ADDR(ASHCAPQ);/* POINT TO THE CAPTURE Q
*                                      OF THE ADDRESS SPACE          */
         LA    PARMPTR,ASHCAPQ(,RSMHDPTR)                          0180
*                   RFY                                            0181
*                    (R5,                                          0181
*                     R6,                                          0181
*                     R7) RSTD;     /* RESTRICT REGS DESTROYED BY  0181
*                                      CALL TO Q VERIFICATION RTN    */
*                   CALL ILRVSPAQ;  /* VERIFY THIS QUEUE             */
         L     @15,@CV02318                                        0182
         BALR  @14,@15                                             0182
*                   RFY                                            0183
*                    (R5,                                          0183
*                     R6,                                          0183
*                     R7) UNRSTD;   /* RELEASE REGS                  */
*                   RFY                                            0184
*                     PARMPTR UNRSTD;/* RELEASE REG                  */
*                   IF ATAAIA^=ZERO THEN/* TEST IF THERE IS A VALID
*                                      AIA CHECKPOINTED IN THE ATA   */
         L     @11,ATAAIA(,ATAPTR)                                 0185
         LTR   @11,@11                                             0185
         BZ    @RF00185                                            0185
*                     DO;           /* PROCESS THIS AIA              */
*                       ATAAIA->AIAERROR=ON;/* TURN ON THE         0187
*                                      INDETERMINATE ERROR FLAG IN 0187
*                                      THIS AIA                      */
         OI    AIAERROR(@11),B'00000010'                           0187
*                       ATAAIA->AIANXAIA=ASHCAPQ;/* CHAIN THE CAPTURE
*                                      Q TO THIS AIA                 */
         MVC   AIANXAIA(4,@11),ASHCAPQ(RSMHDPTR)                   0188
*                       ASHCAPQ=ATAAIA;/* PLACE THIS AIA AT THE HEAD
*                                      OF THE CAPTURE Q              */
         ST    @11,ASHCAPQ(,RSMHDPTR)                              0189
*                     END;          /* END OF PROCESSING ATAAIA      */
*                   ASHPERME=ON;    /* TURN ON THE PERMANENT ERROR 0191
*                                      FLAG FOR THIS ADDRESS SPACE   */
@RF00185 OI    ASHPERME(RSMHDPTR),B'00100000'                      0191
*                 END;              /* END OF FURTHER RECOVERY FOR A
*                                      SWAP OUT REQUEST      @ZM43070*/
*               RFY                                                0193
*                 RSMHDPTR UNRSTD;  /* RELEASE REG                   */
@RC00177 DS    0H                                                  0194
*               RFY                                                0194
*                 WORK1 RSTD;       /* RESTRICT REG USED FOR CALLRTM */
*               WORK1=ASCBASID;     /* GET THE ASID OF THE ADDR    0195
*                                      SPACE, NEEDED FOR THE CALLRTM */
         SLR   WORK1,WORK1                                         0195
         ICM   WORK1,3,ASCBASID(ASCBPTR)                           0195
*               GEN REFS(CVTBTERM);                                0196
*                                  /* TERMINATE THE ADDRESS SPACE   */
         CALLRTM TYPE=MEMTERM,COMPCOD=X'028',ASID=(WORK1)
*               RFY                                                0197
*                 WORK1 UNRSTD;     /* RELEASE REG                   */
*               SDWAPTR=SAVESDWA;   /* RESTORE ADDR OF SDWA          */
