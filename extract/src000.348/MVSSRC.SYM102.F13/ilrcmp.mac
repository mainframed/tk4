IOCARCV1 DS    0H                                                  0412
         SETFRR A,FRRAD=(R1),PARMAD=(R4),WRKREGS=(R4,R5),              *
               RELATED=(PAGE/SWAP,ILRCMP(IOCDRCV1))
*   RFY                                                            0413
*    (R1,                                                          0413
*     R5) UNRSTD;                                                  0413
*   ATAMODID=R8;                    /* SET MODID IN ATA FOR RECOVERY */
         STC   R8,ATAMODID(,ATAPTR)                                0414
*   ATAIOSB=R9;                     /* SET IOSB ADDR FOR RECOVERY    */
         ST    R9,ATAIOSB(,ATAPTR)                                 0415
*   RFY                                                            0416
*     IOSB BASED(R9);                                              0416
*   RFY                                                            0417
*     IORB BASED(IOSUSE);                                          0417
*   IF R8=ATAMCMPD THEN             /* CALLED BY DIE ENTRY?          */
         C     R8,@CF00307                                         0418
         BNE   @RF00418                                            0418
*     R6=WSACASMD;                  /* SET ADDRESSABILITY TO DIE   0419
*                                      WORKAREA                      */
         L     @15,LCCAPTR                                         0419
         L     @15,LCCACPUS(,@15)                                  0419
         L     R6,WSACASMD(,@15)                                   0419
*   ELSE                            /* NO- POST STATUS DRIVEN        */
*     R6=WSACASMS;                  /* SET ADDRESSABILITY TO SRB   0420
*                                      WORKAREA                      */
         B     @RC00418                                            0420
@RF00418 L     @15,LCCAPTR                                         0420
         L     @15,LCCACPUS(,@15)                                  0420
         L     R6,WSACASMS(,@15)                                   0420
*   GEN REFS(R6)(USING @DATD,R6);                                  0421
@RC00418 DS    0H                                                  0421
         USING @DATD,R6
*   ZEROWORK=''B;                   /* ZERO ENTIRE IOC WORKAREA      */
*                                                                  0422
         XC    ZEROWORK(256,R6),ZEROWORK(R6)                       0422
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE REQUIRED VARIABLES                                 */
*   /*                                                               */
