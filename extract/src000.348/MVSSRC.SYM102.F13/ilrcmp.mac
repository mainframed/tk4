         L     PARTPTR,ASMPART(,ASMVTPTR)                          0429
*   SARTPTR=ASMSART;                /* SET SART BASE                 */
         L     @12,ASMSART(,ASMVTPTR)                              0430
         ST    @12,SARTPTR                                         0430
*   PAREPTR=IORPARTE;               /* SET PARTE BASE                */
         L     @15,IORPARTE(,@15)                                  0431
         ST    @15,PAREPTR                                         0431
*   RFY                                                            0432
*     R14 UNRSTD;                                                  0432
*   END SETUP;                                                     0433
@EL00005 DS    0H                                                  0433
@EF00005 DS    0H                                                  0433
@ER00005 BR    @14                                                 0433
