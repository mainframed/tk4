         L     @15,CVTPTR                                          0423
         L     ASMVTPTR,CVTASMVT(,@15)                             0423
*   IOSBPTR=R9;                     /* SET IOSB BASE                 */
         ST    R9,IOSBPTR                                          0424
*   IORBPTR=IOSUSE;                 /* SET IORB BASE                 */
         L     @15,IOSUSE(,R9)                                     0425
         ST    @15,IORBPTR                                         0425
*   RFY                                                            0426
*     IORB;                                                        0426
*   RFY                                                            0427
*     IOSB;                                                        0427
*   RFY                                                            0428
*    (R8,                                                          0428
*     R9) UNRSTD;                                                  0428
