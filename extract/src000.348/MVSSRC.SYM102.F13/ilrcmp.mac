         TITLE 'ILRCMP, I/O COMPLETE                                   *
                        '
ILRCMP   CSECT
         USING *,R15
         MODID
         BALR  R7,0
BASEADD  DS    0H
         USING *,R7
         DROP  R15
*   R9=R1;                          /* SAVE IOSB ADDRESS             */
         LR    R9,R1                                               0171
*   R8=ATAMCMP;                     /* TELL SETUP WHO WE ARE         */
         LA    R8,12                                               0172
*   R10=R14;                        /* SAVE RETURN ADDRESS           */
         LR    R10,R14                                             0173
*   RFY                                                            0174
*    (R1,                                                          0174
*     R8,                                                          0174
*     R9,                                                          0174
*     R14) UNRSTD;                                                 0174
*   CALL SETUP;                     /* SET RECOVERY AND INITIALIZE   */
         BAL   @14,SETUP                                           0175
*   GEN REFS(R6)(USING @DATD,R6);                                  0176
         USING @DATD,R6
*   RTNADRX=R10;                    /* SAVE RETURN ADDR              */
         ST    R10,RTNADRX                                         0177
*   RFY                                                            0178
*     R10 UNRSTD;                                                  0178
*   IF IOSCOD=IOSABNC THEN          /* 45- ABEND IN IOS OR APPENDAGE?*/
         L     @12,IOSBPTR                                         0179
         CLI   IOSCOD(@12),X'45'                                   0179
         BNE   @RF00179                                            0179
*     CALL ABNMTERM;                /* MARK EVERY AIA RETRY + FREE 0180
*                                      P/SCCWS                       */
*                                                                  0180
         BAL   @14,ABNMTERM                                        0180
*   /*****************************************************************/
*   /*                                                               */
*   /* NOT 45- THERE SHOULD BE NO P/SCCWS ON IORB AT THIS POINT. IF  */
*   /* THERE ARE IT MEANS ION WAS ENTERED WITH WHAT WE CONSIDER AN   */
*   /* ERROR (WRL OR UE) WITHOUT THE IOSEX BIT ON. THIS SHOULD NEVER */
*   /* HAPPEN- THEREFORE ABEND IF SO AND IOC WILL BE AGAIN ENTERED   */
*   /* THIS TIME WITH CODE 45 FROM IOS FRR + THESE AIAS WILL THEN BE */
*   /* MARKED RETRY                                                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0181
*   ELSE                                                           0181
*     IF IORPCCW^=0 THEN            /* DID THE CANT-HAPPEN HAPPEN?   */
         B     @RC00179                                            0181
@RF00179 L     @12,IORBPTR                                         0181
         L     @12,IORPCCW(,@12)                                   0181
         LTR   @12,@12                                             0181
         BZ    @RF00181                                            0181
*       DO;                         /* I STILL DONT BELIEVE IT       */
*         ATARCODE=CANTHAPN;        /* SET REASON CODE FOR ABEND     */
         MVI   ATARCODE(ATAPTR),X'04'                              0183
*         RFY                                                      0184
*           R1 RSTD;                                               0184
*         GEN(ABEND X'084',,,SYSTEM);/* IOC01 HERE I COME            */
         ABEND X'084',,,SYSTEM
*         RFY                                                      0186
*           R1 UNRSTD;                                             0186
*       END;                                                       0187
*     ELSE                                                         0188
*       ;                           /* THE CANT-HAPPEN DIDNT HAPPEN-
*                                      NOTHING LEFT ON IORB          */
*                                                                  0188
@RF00181 DS    0H                                                  0189
*   /*****************************************************************/
*   /*                                                               */
*   /* CLEAN UP AND UNCONDITIONALLY RESCHED PART MONITOR OR SWAP     */
*   /* DRIVER IF CAN. NOT TESTING FOR WORK TO DO SINCE THIS IS AN    */
*   /* ERROR PATH.                                                   */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0189
*   ATAIOSB=0;                      /* TELL RECOVERY WERE CLOSING  0189
*                                      DOWN                          */
@RC00179 SLR   @12,@12                                             0189
         ST    @12,ATAIOSB(,ATAPTR)                                0189
*   IORFUSE=OFF;                    /* MAKE THIS IORB AVAILABLE      */
         L     @12,IORBPTR                                         0190
         NI    IORFUSE(@12),B'01111111'                            0190
*   IF IORSWAP=ON THEN              /* WAS THAT A SWAP OPERATION?    */
         TM    IORSWAP(@12),B'00001000'                            0191
         BNO   @RF00191                                            0191
*     DO;                           /* YES- UNCOND SCHED SWAP DRVR IF
*                                      CAN                           */
*       RFY                                                        0193
*        (R0,                                                      0193
*         R1) RSTD;                                                0193
*       R0=ONE;                     /* VALUE TO REPLACE IF WE CAN  0194
*                                      SCHEDULE                      */
         LA    R0,1                                                0194
*       R1=0;                       /* VALUE TO CHECK TO ALLOW A   0195
*                                      SCHEDULE                      */
         SLR   R1,R1                                               0195
*       CS(R1,R0,SARSRBCT);                                        0196
         L     @12,SARTPTR                                         0196
         CS    R1,@00,SARSRBCT(@12)                                0196
*       BC(NE,SWPNOSC1);            /* IF ALREADY ONE OUT- CANT    0197
*                                      SCHEDULE                      */
         BC    7,SWPNOSC1                                          0197
*/* CAN SCHEDULE- DO IT                                              */
*                                                                  0198
*       R1=SARSRBP;                 /* SWAP DRIVER SRB               */
         L     @12,SARTPTR                                         0198
         L     R1,SARSRBP(,@12)                                    0198
*       RFY                                                        0199
*         R15 RSTD;                                                0199
*       GEN REFS(R1) SETS(R0,R15)(SCHEDULE SRB=(1),SCOPE=GLOBAL);  0200
         SCHEDULE SRB=(1),SCOPE=GLOBAL
*SWPNOSC1:                                                         0201
*       RFY                                                        0201
*        (R0,                                                      0201
*         R1,                                                      0201
*         R15) UNRSTD;                                             0201
SWPNOSC1 DS    0H                                                  0202
*     END;                          /* END SCHED SWAP DRIVER         */
*   ELSE                            /* NOT SWAP- PAGE                */
*     DO;                           /* DO SPECIAL PAGE CLEANUP AND 0203
*                                      UNCOND SCHED SWAP DRVR, IF CAN*/
         B     @RC00191                                            0203
@RF00191 DS    0H                                                  0204
*       RFY                                                        0204
*        (R0,                                                      0204
*         R1) RSTD;                                                0204
*       R0=ASMIOCNT;                /* GET CURRENT CT STARTIOS NOT 0205
*                                      COMPLETE                      */
         L     R0,ASMIOCNT(,ASMVTPTR)                              0205
*RETRYCT1:                                                         0206
*       R1=R0-ONE;                  /* DECR BY 1- THIS STARTIO     0206
*                                      COMPLETE                      */
RETRYCT1 LR    R1,R0                                               0206
         BCTR  R1,0                                                0206
*       CS(R0,R1,ASMIOCNT);         /* STORE NEW CT                  */
         CS    R0,@01,ASMIOCNT(ASMVTPTR)                           0207
*       BC(NE,RETRYCT1);                                           0208
         BC    7,RETRYCT1                                          0208
*       R0=ONE;                     /* VALUE TO REPLACE IF WE CAN  0209
*                                      SCHEDULE                      */
         LA    R0,1                                                0209
*       R1=0;                       /* VALUE TO CHECK TO ALLOW A   0210
*                                      SCHEDULE                      */
         SLR   R1,R1                                               0210
*       CS(R1,R0,ASMSRBCT);                                        0211
         CS    R1,@00,ASMSRBCT(ASMVTPTR)                           0211
*       BC(NE,PTMNOSC1);            /* IF ALREADY ONE OUT- CANT    0212
*                                      SCHEDULE                      */
         BC    7,PTMNOSC1                                          0212
*/* CAN SCHEDULE- DO IT                                              */
*                                                                  0213
*       R1=ASMPSRB;                 /* SET ADDR OF PTMS SRB          */
         L     R1,ASMPSRB(,ASMVTPTR)                               0213
*       RFY                                                        0214
*         R15 RSTD;                                                0214
*       GEN REFS(R1) SETS(R0,R15)(SCHEDULE SRB=(1),SCOPE=GLOBAL);  0215
         SCHEDULE SRB=(1),SCOPE=GLOBAL
*PTMNOSC1:                                                         0216
*       RFY                                                        0216
*        (R0,                                                      0216
*         R1,                                                      0216
*         R15) UNRSTD;                                             0216
PTMNOSC1 DS    0H                                                  0217
*     END;                          /* END SPECIAL PAGE CLEANUP AND
*                                      SCHED PTM                     */
*                                                                  0217
*   /*****************************************************************/
*   /*                                                               */
*   /* EXIT TO DISPATCHER FROM PGAD                                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0218
*   CALL CLEANUP;                   /* REMOVE RECOVERY               */
@RC00191 BAL   @14,CLEANUP                                         0218
*   RFY                                                            0219
*     R14 RSTD;                                                    0219
*   R14=RTNADRX;                    /* RESTORE RETURN ADDR           */
         L     R14,RTNADRX                                         0220
*   RFY                                                            0221
*     R14 UNRSTD;                                                  0221
*   RETURN;                         /* RETURN TO DISPATCHER          */
@EL00001 DS    0H                                                  0222
@EF00001 DS    0H                                                  0222
@ER00001 BR    @14                                                 0222
*                                                                  0223
*/********************************************************************/
*/*                                                                  */
*/*   ILRCMPDI - DISABLED INTERRUPT EXIT                             */
*/*                                                                  */
*/*   INPUT - R2 = IOSB ADDRESS                                      */
*/*           R7 = UCB ADDRESS (NOT USED)                            */
*/*           R13 = 64 BYTE SAVEAREA ADDRESS                         */
*/*           (REGISTERS 0, 3-6, 8-12 MUST NOT BE DESTROYED BY DIE)  */
*/*                                                                  */
*/*   PROCESS - PROCESS I/O COMPLETIONS ON ALL GOOD I/O. COMPUTE     */
*/*              A NEW SERVICE BURST IF POSSIBLE. RESCHEDULE SWAP    */
*/*              DRIVER OR PART MONITOR IF NECESSARY.                */
*/*                                                                  */
*/*   OUTPUT - NONE                                                  */
*/*                                                                  */
*/********************************************************************/
*/*ILRCMPDI: CHART (AREF)                                            */
*/* HEADER                                                         0223
*/*                               DISABLED INTERRUPT EXIT          0223
*/*ILRCMPDI                                                          */
*/*ILRCMPDI: E DISABLED INTERRUPT EXIT                               */
*/* N INPUT: R2= IOSB@, R7= UCB@,R13= SAVEAREA ADDR                  */
*/* P SAVE REGS IN 16 WORD SAVEAREA (R13)                            */
*/* P SET UP MODULE ADDRESSABILITY                                   */
*/* P SAVE IOSB ADDR                                                 */
*/* P SET MODID IN REG FOR SETUP                                     */
*/* S SETUP: SET FRR, WORKAREA + ADDRESSABILITY                      */
*/* D (YES,,NO,%F1) 1ST ENTRY TO DIE? IOSDIESE                       */
*/* P STORE IOC TOD VALUE                                            */
*/* D (YES,%T2,NO,) STORE CLOCK OK?                                  */
*/* P ZERO SRT TOD VALUE- NO NEW BURST                               */
*/*%T2: D (YES,%T3,NO,) I/O SUCCESSFUL? CE DE CUE & IOSERR=0         */
*/* P (,%CLEAN) SET RETURN VECTOR 0 IN R15                           */
*/*%T3: P INDICATE NO ERRORS IORERR =0                               */
*/* S PROCCCWS: PROCESS GOOD PCCWS                                   */
*/*%F2: S POSTCMP: RETURN P/SCCWS AND AIAS                           */
*/* S COMPBRST: COMPUTE NEW SERVICE BURST                            */
*/* P SET RETURN VECTOR TO 8 IN R15                                  */
*/*%CLEAN: S CLEANUP: REMOVE FRR FROM STACK                          */
*/* R RETURN ON R14 + R15                                            */
*/*%F1: N CLEANUP AND CHECK FOR SCHEDULE OF PART MON OR SWAP DRVR    */
*/* D (PAG,,SWP,%S1) PAGE OR SWAP? IORSWAP                           */
*/* P DEC CT OF STARTIOS NOT COMPLETE (C+S) ASMIOCNT                 */
*/* P *** FRR *** ZERO ATAIOSB                                       */
*/* P MARK IORB AVAILABLE (IORFUSE)                                  */
*/* N DETERMINE WHETHER NECESSARY TO RESCHED PTM-                    */
*/* D (YES,,NO,%FREE) ANOTHER SCHED OF PTM REQD?                     */
*/* D (YES,%FREE,NO,) PTM ALREADY SCHED? (C+S) ASMSRBCT              */
*/* L SCHEDULE: ILRPTM                                               */
*/*%FREE: P (,%CLEAN) SET VECTOR 0 IN R15                            */
*/*%S1: P *** FRR *** ZERO ATAIOSB                                   */
*/* P MARK IORB AVAILABLE (IORFUSE)                                  */
*/* N SWAP DATA SET- DETERMINE WHETHER NEED TO RESCHED SWAP DRVR     */
*/* D (YES,,NO,%FREE) WORK TO DO? IORSARTE-> SRESCCW                 */
*/* D (YES,%FREE,NO,) SWP ALREADY SCHED? (C+S) SARSRBCT              */
*/* L (,%FREE) SCHEDULE: ILRSWPDR                                    */
*/*ILRCMPDI: END                                                     */
*                                                                  0223
*ILRCMPDI:                                                         0223
*   PROC OPTIONS(NOSAVE,NOSAVEAREA,RETREG);                        0223
ILRCMPDI DS    0H                                                  0224
*   GENERATE;                                                      0224
         ENTRY ILRCMPDI
         STM   R0,R15,0(R13)
         BALR  R7,0
IODBASE  LA    R9,IODBASE-BASEADD
         SR    R7,R9
*   RFY                                                            0225
*    (R2,                                                          0225
*     R8,                                                          0225
*     R9) RSTD;                                                    0225
*   R9=R2;                          /* SAVE IOSB ADDR FOR SETUP    0226
*                                      SUBRTN                        */
         LR    R9,R2                                               0226
*   R8=ATAMCMPD;                    /* SET MODID FOR SETUP SUBRTN    */
         LA    R8,9                                                0227
*   RFY                                                            0228
*    (R2,                                                          0228
*     R8,                                                          0228
*     R9) UNRSTD;                                                  0228
*   CALL SETUP;                     /* SET RECOVERY AND INITIALIZE   */
         BAL   @14,SETUP                                           0229
*   GEN REFS(R6)(USING @DATD,R6);                                  0230
         USING @DATD,R6
*   DIEIOSSA=R13;                   /* SAVE IOS DIE SAVEAREA ADDR    */
         LR    DIEIOSSA,R13                                        0231
*   IF IOSDIESE=OFF THEN            /* 1ST ENTRY TO DIE?             */
*                                                                  0232
         L     @12,IOSBPTR                                         0232
         TM    IOSDIESE(@12),B'10000000'                           0232
         BNZ   @RF00232                                            0232
*     /***************************************************************/
*     /*                                                             */
*     /* FIRST ENTRY TO DIE                                          */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0233
*     DO;                                                          0233
*       GEN SETS(IOCTOD)(STCK IOCTOD);/* STORE CURRENT TOD           */
         STCK IOCTOD
*       BC(EQ,GOODTIME);            /* STCK OK?                      */
         BC    8,GOODTIME                                          0235
*       IORTSMP=''B;                /* NO- ZERO SLOTSORT TOD SO    0236
*                                      COMPBURST WONT COMPUTE NEW  0236
*                                      BURST                         */
         L     @12,IORBPTR                                         0236
         XC    IORTSMP(8,@12),IORTSMP(@12)                         0236
*GOODTIME:                                                         0237
*       ;                                                          0237
*                                                                  0237
GOODTIME DS    0H                                                  0238
*       /*************************************************************/
*       /*                                                           */
*       /* DETERMINE WHETHER I/O SUCCESSFUL- ONLY CE DE OR CUE CAN BE*/
*       /* ON IN CSWSTATUS                                           */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0238
*       IF IOSERR=OFF THEN          /* WAS ERP NOT IN PROCESS      0238
*                                                            @Z40WPWP*/
         L     @12,IOSBPTR                                         0238
         TM    IOSERR(@12),B'00100000'                             0238
         BNZ   @RF00238                                            0238
*         DO;                       /* YES, SEE IF I/O IS CLEAN    0239
*                                                            @Z40WPWP*/
*           RFY                                                    0240
*             R12 RSTD;             /*                       @Z40WPWP*/
*           R12=ADDR(IOSB);         /* SET UP TO TEST IOSTATUS     0241
*                                                            @Z40WPWP*/
         L     @08,IOSBPTR                                         0241
         LR    R12,@08                                             0241
*           GEN REFS(IOSTSA,IOSTSB) FLOWS(NOTCLEAN);               0242
*                                         /*@Z40WPWP*/
         TM    IOSTSA(R12),BADDEVST  BITS ON OTHER THAN CUE,CE @Z40WPWP
*                                    OR DE IN DEVICE STATUS    @Z40WPWP
         BNZ   NOTCLEAN       YES, LET POST STATUS HANDLE      @Z40WPWP
         TM    IOSTSB(R12),BADCHNST  ANY BITS ON IN CHANNEL    @Z40WPWP
*                                    STATUS                    @Z40WPWP
         BNZ   NOTCLEAN       YES, LET POST STATUS HANDLE      @Z40WPWP
*           RFY                                                    0243
*             R12 UNRSTD;           /*                       @Z40WPWP*/
*                                                                  0243
*           /*********************************************************/
*           /*                                                       */
*           /* THE I/O IS CLEAN SO PROCESS IT HERE           @Z40WPWP*/
*           /*                                                       */
*           /*********************************************************/
*                                                                  0244
*           IORFDI=ON;              /* INDICATE DIE IS PROCESSING  0244
*                                      THIS COMPLETION               */
         L     @12,IORBPTR                                         0244
         OI    IORFDI(@12),B'00000100'                             0244
*           IORERR=0;               /* INDICATE NO ERRORS            */
         SLR   @08,@08                                             0245
         ST    @08,IORERR(,@12)                                    0245
*           CALL PROCCCWS;          /* PROCESS GOOD P/SCCWS          */
         BAL   @14,PROCCCWS                                        0246
*           CALL POSTCMP;           /* FREE P/SCCWS AND POST BACK  0247
*                                      AIAS TO PAGE COMPLETION       */
         BAL   @14,POSTCMP                                         0247
*           CALL COMPBRST;          /* COMPUTE NEW SERVICE BURST     */
         BAL   @14,COMPBRST                                        0248
*           RFY                                                    0249
*             R15 RSTD;                                            0249
*           R15=IGNORE;             /* INDICATE FINISHED WITH THIS 0250
*                                      IOSB WHEN RETURN TO IOS       */
         LA    R15,8                                               0250
*         END;                      /* END SEE IF I/O IS CLEAN     0251
*                                                            @Z40WPWP*/
*       ELSE                        /* NOT CLEAN I/O-                */
*         DO;                       /* FIND ERROR P/SCCW AND UPDATE
*                                      SEEK ADDRESS IN IOSB IN CASE
*                                      THIS IS THE SECOND TEMPORARY
*                                      ERROR RECEIVED FOR THIS STRING
*                                      OF P/SCCWS            @ZA18116*/
         B     @RC00238                                            0252
@RF00238 DS    0H                                                  0253
*           IF IOSCC^=IOSCC1 THEN   /* IF CC=1 FROM SIO, DON'T UPDATE
*                                      SEEK ADDRESS          @ZA30982*/
         L     @14,IOSBPTR                                         0253
         CLI   IOSCC(@14),X'10'                                    0253
         BE    @RF00253                                            0253
*             DO;                   /*                       @ZA30982*/
*               CALL LOCATECP;      /* FIND ERROR P/SCCW     @ZA18116*/
         BAL   @14,LOCATECP                                        0255
*               IOSEEK=IORERR->PCCWCHHR;/* UPDATE IOSB STATIC SEEK 0256
*                                      ADDRESS               @ZA28705*/
         L     @14,IOSBPTR                                         0256
         L     @12,IORBPTR                                         0256
         L     @12,IORERR(,@12)                                    0256
         MVC   IOSEEK(8,@14),PCCWCHHR(@12)                         0256
*               IOSEEKA=IOSEEK;     /* UPDATE IOSB DYNAMIC SEEK    0257
*                                      ADDRESS               @ZA28705*/
         MVC   IOSEEKA(8,@14),IOSEEK(@14)                          0257
*             END;                  /*                       @ZA30982*/
*NOTCLEAN:                          /* CAN BRANCH HERE FROM GEND 'TM'
*                                      IN DO ABOVE           @Z40WPWP*/
*           R15=PURSUE;             /* INDICATE POST STATUS TO HANDLE
*                                      THIS ONE                      */
@RF00253 DS    0H                                                  0259
NOTCLEAN SLR   R15,R15                                             0259
*         END;                      /* END IOSB SEEK ADDRESS UPDATE
*                                                            @ZA18116*/
*     END;                          /* END FIRST ENTRY TO DIE        */
*                                                                  0262
*   /*****************************************************************/
*   /*                                                               */
*   /* SECOND ENTRY TO DIE- CLEANUP AND CHECK FOR POSSIBLE SCHEDULE  */
*   /* OF PART MONITOR OR SWAP DRIVER                                */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0262
*   ELSE                            /* NOT 1ST ENTRY TO DIE- SECOND
*                                      ENTRY                         */
*     DO;                                                          0262
         B     @RC00232                                            0262
@RF00232 DS    0H                                                  0263
*       IF IORSWAP=ON THEN          /* WAS THAT A SWAP OPERATION?    */
         L     @14,IORBPTR                                         0263
         TM    IORSWAP(@14),B'00001000'                            0263
         BNO   @RF00263                                            0263
*         DO;                       /* YES- UNCHECKPOINT AND SCHED 0264
*                                      SWAP DRVR IF NECESSARY        */
*           ATAIOSB=0;              /* INDICATE TO RECOVERY WERE   0265
*                                      CLOSING DOWN                  */
         SLR   @12,@12                                             0265
         ST    @12,ATAIOSB(,ATAPTR)                                0265
*           IORFUSE=OFF;            /* MAKE THIS IORB AVAILABLE- THIS
*                                      MUST BE DONE AFTER ZEROING  0266
*                                      ATAIOSB FOR RECOV             */
         NI    IORFUSE(@14),B'01111111'                            0266
*           IF SRESCCW^=0 THEN      /* WORK TO DO?                   */
         L     @14,SAREPTR                                         0267
         C     @12,SRESCCW(,@14)                                   0267
         BE    @RF00267                                            0267
*             DO;                   /* YES- SCHED SWAP DRVR IF CAN   */
*               RFY                                                0269
*                (R0,                                              0269
*                 R1) RSTD;                                        0269
*               R0=ONE;             /* VALUE TO REPLACE IF WE CAN  0270
*                                      SCHEDULE                      */
         LA    R0,1                                                0270
*               R1=0;               /* VALUE TO CHECK TO ALLOW A   0271
*                                      SCHEDULE                      */
         SLR   R1,R1                                               0271
*               CS(R1,R0,SARSRBCT);                                0272
         L     @14,SARTPTR                                         0272
         CS    R1,@00,SARSRBCT(@14)                                0272
*               BC(NE,SWPNOSCH);    /* IF ALREADY ONE OUT- CANT    0273
*                                      SCHEDULE                      */
         BC    7,SWPNOSCH                                          0273
*/* CAN SCHEDULE- DO IT                                              */
*                                                                  0274
*               R1=SARSRBP;         /* SET ADDR OF SWP DRVRS SRB     */
         L     @14,SARTPTR                                         0274
         L     R1,SARSRBP(,@14)                                    0274
*               RFY                                                0275
*                 R15 RSTD;                                        0275
*               GEN REFS(R1) SETS(R0,R15)                          0276
*                   (SCHEDULE SRB=(1),SCOPE=GLOBAL);               0276
         SCHEDULE SRB=(1),SCOPE=GLOBAL
*SWPNOSCH:                                                         0277
*               RFY                                                0277
*                (R0,                                              0277
*                 R1,                                              0277
*                 R15) UNRSTD;                                     0277
SWPNOSCH DS    0H                                                  0278
*             END;                  /* END SCHED SWP DRVR IF CAN     */
*           ELSE                                                   0279
*             ;                     /* NO WORK TO DO FOR SWAP DRVR   */
@RF00267 DS    0H                                                  0280
*         END;                      /* END UNCHECKPT AND SCHED SWAP
*                                      DRVR                          */
*       ELSE                        /* NOT SWAP- PAGING              */
*         DO;                       /* DEC CTS AND CHECK FOR SCHED OF
*                                      PTM                           */
*                                                                  0281
         B     @RC00263                                            0281
@RF00263 DS    0H                                                  0282
*           /*********************************************************/
*           /*                                                       */
*           /* DECREMENT COUNT OF STARTIOS NOT COMPLETE              */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0282
*           RFY                                                    0282
*            (R0,                                                  0282
*             R1) RSTD;                                            0282
*           R0=ASMIOCNT;            /* GET CURRENT COUNT             */
         L     R0,ASMIOCNT(,ASMVTPTR)                              0283
*RETRYCNT:                                                         0284
*           R1=R0-ONE;              /* DECREMENT BY 1- THIS STARTIO
*                                      COMPLETE                      */
RETRYCNT LR    R1,R0                                               0284
         BCTR  R1,0                                                0284
*           CS(R0,R1,ASMIOCNT);     /* STORE NEW CT                  */
         CS    R0,@01,ASMIOCNT(ASMVTPTR)                           0285
*           BC(NE,RETRYCNT);                                       0286
         BC    7,RETRYCNT                                          0286
*           ATAIOSB=0;              /* INDICATE TO RECOVERY WERE   0287
*                                      CLOSING DOWN                  */
         SLR   @12,@12                                             0287
         ST    @12,ATAIOSB(,ATAPTR)                                0287
*           IORFUSE=OFF;            /* MAKE THIS IORB AVAILABE- THIS
*                                      MUST BE DONE AFTER ZEROING  0288
*                                      ATAIOSB FOR RECOV             */
*                                                                  0288
         L     @08,IORBPTR                                         0288
         NI    IORFUSE(@08),B'01111111'                            0288
*           /*********************************************************/
*           /*                                                       */
*           /* IF THERE IS WORK TO DO- SCHEDULE PART MONITOR IF CAN. */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0289
*           IF ASMIORQR^=ASMIORQC&  /* ANY REQUESTS OUTSTANDING      */
*               ASMIOCNT=0 THEN     /* AND NO I/O IN PROGRESS        */
         CLC   ASMIORQR(4,ASMVTPTR),ASMIORQC(ASMVTPTR)             0289
         BE    @RF00289                                            0289
