*/*                                                                  */
*/*   LOCATECP- LOCATE PCCW IN ERROR                                 */
*/*                                                                  */
*/*   INPUT -  PTR TO FIRST P/SCCW (IORPCCW)                         */
*/*                                                                  */
*/*   PROCESS - THE P/SCCWS ARE SCANNED FOR THE CCW IN ERROR         */
*/*                                                                  */
*/*   OUTPUT - IORERR WILL CONTAIN ADDRESS OF ERROR P/SCCW           */
*/*                                                                  */
*/********************************************************************/
*/*LOCATECP: CHART (AREF)                                            */
*/* HEADER                                                         0439
*/*                                I/O COMPLETE -                  0439
*/*LOCATECP                                                          */
*/*LOCATECP: E LOCATE PCCW IN ERROR                                  */
*/* P SET UP ADDR OF ERROR CCW CSWCCWA -8                            */
*/* N DETERMINE WHETHER PAGE OR SWAP TO GET LENGTH                 0439
*/*   OF P/SCCW                                                      */
*/* N THEN DETERMINE WITHIN WHICH P/SCCW CCW IN ERROR FALLS          */
*/* D (PAG,,SWP,%F1) PAGE OR SWAP? IORSWAP                           */
*/* P (,%LOC20) SET LENGTH OF PCCW                                   */
*/*%F1: P SET LENGTH OF SCCW                                         */
*/*%LOC20: D (NO,,YES,%LOC50) ERROR CCW IN THIS P/SCCW?              */
*/* P UPDATE TO NEXT P/SCCW                                          */
*/* D (YES,%LOC80,NO,%LOC20) END OF P/SCCW CHAIN?                    */
*/*%LOC50: P MARK ERROR P/SCCW PCCWFERR                              */
*/*%LOC80: P SAVE ADDRESS OF ERROR P/SCCW IN IORB IORERR             */
*/*%N1: R RETURN                                                     */
*/*LOCATECP: END                                                     */
*                                                                  0439
*LOCATECP:                                                         0439
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0439
LOCATECP DS    0H                                                  0440
*   RFY                                                            0440
*     R14 RSTD;                                                    0440
*                                                                  0440
*/* SET UP FOR LOCATE PROCESSING                                     */
*                                                                  0441
*   CSWADDR=IOSCSWCA-CCWLEN;        /* SAVE ADDR OF CCW THAT FAILED  */
         L     @15,IOSBPTR                                         0441
         L     CSWADDR,IOSCSWCA-1(,@15)                            0441
         LA    CSWADDR,0(,CSWADDR)                                 0441
         SL    CSWADDR,@CF00040                                    0441
*   IF IORSWAP=ON THEN              /* WORKING WITH SCCWS?           */
         L     @15,IORBPTR                                         0442
         TM    IORSWAP(@15),B'00001000'                            0442
         BNO   @RF00442                                            0442
*     XCCWLEN=LENGTH(SCCW);         /* SAVE SCCW LEN FOR SEARCH      */
         LA    XCCWLEN,400                                         0443
*   ELSE                            /* NO- PCCWS.                    */
*     XCCWLEN=LENGTH(PCCW);         /* SAVE PCCW LEN FOR SEARCH      */
         B     @RC00442                                            0444
@RF00442 LA    XCCWLEN,72                                          0444
*   IORERR=0;                       /* INIT AS NO ERROR              */
*                                                                  0445
@RC00442 L     @15,IORBPTR                                         0445
         SLR   @12,@12                                             0445
         ST    @12,IORERR(,@15)                                    0445
*   /*****************************************************************/
*   /*                                                               */
*   /* LOOP THRU P/SCCWS UNTIL FIND THE ONE THAT CONTAINS FAILING CCW*/
*   /* OR UNTIL HIT END OF CHAIN                                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0446
*   PCCWPTR=IORPCCW;                                               0446
         L     @15,IORPCCW(,@15)                                   0446
         ST    @15,PCCWPTR                                         0446
*   DO WHILE(PCCWPTR^=0&IORERR=0);                                 0447
         B     @DE00447                                            0447
@DL00447 DS    0H                                                  0448
*     IF CSWADDR>=PCCWPTR&CSWADDR<=(PCCWPTR+XCCWLEN) THEN/* ERROR IN
*                                      THIS P/SCCW?                  */
         L     @15,PCCWPTR                                         0448
         CR    CSWADDR,@15                                         0448
         BL    @RF00448                                            0448
         LR    @12,XCCWLEN                                         0448
         ALR   @12,@15                                             0448
         CR    CSWADDR,@12                                         0448
         BH    @RF00448                                            0448
*       DO;                         /* YES- FOUND ERROR P/SCCW       */
*         IORERR=PCCWPTR;           /* SAVE ADDR BAD P/SCCW IN IORB  */
         L     @12,IORBPTR                                         0450
         ST    @15,IORERR(,@12)                                    0450
*         PCCWFERR=ON;              /* MARK P/SCCW IN ERROR          */
         OI    PCCWFERR(@15),B'10000000'                           0451
*       END;                                                       0452
*     ELSE                          /* ERROR NOT IN THIS P/SCCW      */
*       PCCWPTR=PCCWPCCW;           /* CHECK NEXT                    */
         B     @RC00448                                            0453
@RF00448 L     @15,PCCWPTR                                         0453
         L     @15,PCCWPCCW(,@15)                                  0453
         ST    @15,PCCWPTR                                         0453
*   END;                            /* END LOOP THRU P/SCCWS         */
@RC00448 DS    0H                                                  0454
@DE00447 SLR   @15,@15                                             0454
         C     @15,PCCWPTR                                         0454
         BE    @DC00447                                            0454
         L     @12,IORBPTR                                         0454
         C     @15,IORERR(,@12)                                    0454
         BE    @DL00447                                            0454
@DC00447 DS    0H                                                  0455
*   IF IORERR=0 THEN                /* IF ERROR NOT FOUND            */
         L     @15,IORBPTR                                         0455
         L     @12,IORERR(,@15)                                    0455
         LTR   @12,@12                                             0455
         BNZ   @RF00455                                            0455
*     IORERR=IORPCCW;               /* MARK 1ST P/SCCW IN ERROR      */
         L     @12,IORPCCW(,@15)                                   0456
         ST    @12,IORERR(,@15)                                    0456
*   ELSE                                                           0457
*     ;                             /* ERROR FOUND DURING SEARCH     */
@RF00455 DS    0H                                                  0458
*   RFY                                                            0458
*     R14 UNRSTD;                                                  0458
*   END LOCATECP;                                                  0459
@EL00007 DS    0H                                                  0459
@EF00007 DS    0H                                                  0459
@ER00007 BR    @14                                                 0459
