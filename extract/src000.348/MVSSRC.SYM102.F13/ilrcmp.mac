*/*                                                                  */
*/*   BADPACK -                                                      */
*/*                                                                  */
*/*   INPUT - NONE.                                                  */
*/*                                                                  */
*/*   PROCESS - BADPACK MARKS THE PAGE/SWAP SPACE AS UNUSABLE AFTER  */
*/*       OBTAINING THE SALLOC LOCK. ILRMSG00 IS CALLED TO INDICATE  */
*/*       SITUATION AND DETERMINE WHETHER THE SYSTEM CAN CONTINUE.   */
*/*       NOTE: CONTROL MAY NOT BE RETURNED FROM ILRMSG00.           */
*/*                                                                  */
*/*   OUTPUT - PAREDSBD ON FOR THIS PAGE/SWAP SPACE.                 */
*/*                                                                  */
*/********************************************************************/
*/*BADPACK: CHART (AREF)                                             */
*/* HEADER                                                         0719
*/*                                I/O COMPLETE -                  0719
*/*BADPACK                                                           */
*/*BADPACK: E MARK PAGE/SWAP SPACE UNUSABLE                          */
*/* L SETLOCK: OBTAIN SALLOC                                         */
*/* P MARK SWAP OR PAGE SPACE BAD PAREDSBD                           */
*/* P *** FRR *** ATABADPK ON                                        */
*/* P SET PARTE/SARTE @ IN MSG/TERM PLIST                            */
*/* D (PAG,,SWP,%S1) PAGE OR SWAP? IORSWAP                           */
*/* P INDICATE PAGE IN MSG/TERM PLIST                                */
*/*%MSG: P FINAL SET UP FOR CALL TO MSG00                            */
*/* N MAY NOT RETURN IF MSG00 TAKES SYS DOWN                         */
*/* P *** FRR *** ATABADPK OFF                                       */
*/* S ILRMSG00: INDICATE SITUATION                                   */
*/* L SETLOCK: RELEASE SALLOC                                        */
*/* R RETURN                                                         */
*/*%S1: P (,%MSG) INDICATE SWAP IN MSG PLIST                         */
*/*BADPACK: END                                                      */
*                                                                  0719
*BADPACK:                                                          0719
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0719
BADPACK  DS    0H                                                  0720
*   RFY                                                            0720
*     R14 RSTD;                                                    0720
*   RTNADDR=R14;                    /* SAVE RETURN ADDR              */
         ST    R14,RTNADDR                                         0721
*   RFY                                                            0722
*     R14 UNRSTD;                                                  0722
*                                                                  0722
*   /*****************************************************************/
*   /*                                                               */
*   /* SERIALIZE THIS PROCESSING WITH SALLOC LOCK                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0723
*   RFY                                                            0723
*    (R11,                                                         0723
*     R12,                                                         0723
*     R13,                                                         0723
*     R14) RSTD;                                                   0723
*IOCLK2:                                                           0724
*   GEN REFS(R12,R13,R14,PSALITA,FLC) SETS(R11,R12,R13,R14);       0724
IOCLK2   DS    0H                                                  0724
         SETLOCK OBTAIN,TYPE=SALLOC,MODE=UNCOND,                       *
               RELATED=(PAGE/SWAP,ILRCMP(IOCUNLK2))
*   RFY                                                            0725
*    (R11,                                                         0725
*     R12,                                                         0725
*     R14) UNRSTD;                                                 0725
*   IF PAREDSBD=OFF THEN            /* IS THE BIT STILL OFF  @Z40WPWP*/
         L     @12,PAREPTR                                         0726
         TM    PAREDSBD(@12),B'01000000'                           0726
         BNZ   @RF00726                                            0726
*     DO;                           /* YES, TRY TO ISSUE DATA SET BAD
*                                      MSG                   @Z40WPWP*/
*       PAREDSBD=ON;                /* MARK PAGE/SWAP SPACE BAD      */
         OI    PAREDSBD(@12),B'01000000'                           0728
*       ATABADPK=ON;                /* LET RECOVERY KNOW WE ARE    0729
*                                      TRYING TO WRITE MSG   @Z40WPWP*/
         OI    ATABADPK(ATAPTR),B'00100000'                        0729
*       MSGPLIST=''B;               /* INITIAL SETUP OF MSG00 PLIST  */
         XC    MSGPLIST(8),MSGPLIST                                0730
*       MSGCBADR=IORPARTE;          /* SET PARTE/SARTE ADDR IN PLIST */
         L     @12,IORBPTR                                         0731
         L     @05,IORPARTE(,@12)                                  0731
         ST    @05,MSGCBADR                                        0731
*       IF IORSWAP=ON THEN          /* SWAP?                         */
         TM    IORSWAP(@12),B'00001000'                            0732
         BNO   @RF00732                                            0732
*         MSGTYPE=MSGSRETP;         /* INDICATE MSGCBADR IS A SARTE  */
         MVC   MSGTYPE(2),@CH00043                                 0733
*       ELSE                                                       0734
*         ;                         /* PARTE - ALREADY SET TO ZERO   */
*                                                                  0734
@RF00732 DS    0H                                                  0735
*       /*************************************************************/
*       /*                                                           */
*       /* PUPOSELY LEFT MSGNUM FIELD ZERO- MSG00 WILL DETERMINE MSG */
*       /* TO ISSUE AND SYSTEM STATUS. MAY NOT RECEIVE CONTROL BACK  */
*       /* FROM MSG00- SYSTEM MAY BE TAKEN DOWN.                     */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0735
*       RFY                                                        0735
*         R1 RSTD;                                                 0735
*       R13=ADDR(SAVEREGS);         /* SET SAVE AREA FOR MSG00       */
         LA    R13,SAVEREGS                                        0736
*       R1=ADDR(MSGPLIST);          /* SET INTERFACE TO MSG00        */
         LA    R1,MSGPLIST                                         0737
*       ATABADPK=OFF;               /* DO NOT LET RECOVERY TRY ISSUE
*                                      MSG AGAIN             @Z40WPWP*/
         NI    ATABADPK(ATAPTR),B'11011111'                        0738
*       CALL ILRMSG00;                                             0739
         L     @15,@CV02489                                        0739
         BALR  @14,@15                                             0739
*       RFY                                                        0740
*         R1 UNRSTD;                                               0740
*     END;                          /* FOR TRY TO ISSUE MSG  @Z40WPWP*/
*   RFY                                                            0742
*    (R11,                                                         0742
*     R12,                                                         0742
*     R13,                                                         0742
*     R14) RSTD;                                                   0742
@RF00726 DS    0H                                                  0743
*IOCUNLK2:                                                         0743
*   GEN REFS(R12,R13,R14,PSALITA,FLC) SETS(R11,R12,R13,R14);       0743
IOCUNLK2 DS    0H                                                  0743
         SETLOCK RELEASE,TYPE=SALLOC,                                  *
               RELATED=(PAGE/SWAP,ILRCMP(IOCLK2))
*   RFY                                                            0744
*    (R11,                                                         0744
*     R12) UNRSTD;                                                 0744
*   R14=RTNADDR;                    /* RESTORE RETURN ADDRESS        */
         L     R14,RTNADDR                                         0745
*   RFY                                                            0746
*     R14 UNRSTD;                                                  0746
*   END BADPACK;                                                   0747
@EL00015 DS    0H                                                  0747
@EF00015 DS    0H                                                  0747
@ER00015 BR    @14                                                 0747
*   END ILRCMP                                                     0748
