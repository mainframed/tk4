*               RFY                                                0376
*                (R0,                                              0376
*                 R1) RSTD;                                        0376
*               R0=ASMERRS;         /* GET CURRENT CT                */
         L     R0,ASMERRS(,ASMVTPTR)                               0377
*RETRYERS:                                                         0378
*               R1=R0+ONE;          /* ADD THIS ERROR                */
RETRYERS LA    R1,1                                                0378
         ALR   R1,R0                                               0378
*               CS(R0,R1,ASMERRS);  /* UPDATE                        */
         CS    R0,@01,ASMERRS(ASMVTPTR)                            0379
*               BC(NE,RETRYERS);                                   0380
         BC    7,RETRYERS                                          0380
*               RFY                                                0381
*                (R0,                                              0381
*                 R1) UNRSTD;                                      0381
*             END;                  /* END UPDATE ASMERRS            */
*           ELSE                                                   0383
*             ;                     /* NOT A LOCAL PAGE D.S.         */
@RF00374 DS    0H                                                  0384
*/* UPDATE PARTE/SARTE ERROR CTS                                     */
*                                                                  0384
*           RFY                                                    0384
*            (R0,                                                  0384
*             R1) RSTD;                                            0384
*           R0=PARERRCT;            /* GET CURRENT PARTE/SARTE ERROR
*                                      CT                            */
         L     @12,PAREPTR                                         0385
         L     R0,PARERRCT(,@12)                                   0385
*RETRYER1:                                                         0386
*           R1=R0+ONE;              /* ADD THIS ERROR                */
RETRYER1 LA    R1,1                                                0386
         ALR   R1,R0                                               0386
*           CS(R0,R1,PARERRCT);     /* UPDATE PARTE/SARTE ERROR CT   */
         L     @12,PAREPTR                                         0387
         CS    R0,@01,PARERRCT(@12)                                0387
*           BC(NE,RETRYER1);                                       0388
         BC    7,RETRYER1                                          0388
*           RFY                                                    0389
*            (R0,                                                  0389
*             R1) UNRSTD;                                          0389
*           CALL RECERR;            /* RECORD THIS BAD SLOT ID       */
         BAL   @14,RECERR                                          0390
*         END;                      /* END SLOT ERROR                */
*                                                                  0392
*       /*************************************************************/
*       /*                                                           */
*       /* MARK BAD SLOT AIA(S) (MAYBE MORE THAN ONE IF PACK ERROR)  */
*       /* AS PRI OR SEC ERROR + PLACE ON INTERNAL POST QUEUE. PLACE */
*       /* P/SCCW ON INTERNAL FREE QUEUE.                            */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0392
*       CALL BADSLOT;               /* PROCESS BAD SLOTS             */
*                                                                  0392
@RC00359 BAL   @14,BADSLOT                                         0392
*       /*************************************************************/
*       /*                                                           */
*       /* DETERMINE WHETHER ANY P/SCCWS REMAIN AFTER THE BAD ONE    */
