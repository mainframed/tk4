*/****************************************************************** */
*/*                                                                  */
*/*   RECERR - BAD SLOT LIST ROUTINE                                 */
*/*                                                                  */
*/*   INPUT - PTR TO P/SCCW SUFFERING I/O ERROR- ATACPCCW            */
*/*            (MAY BE CHAIN OF P/SCCWS IF CALLED ON BADPACK         */
*/*             SITUATION, BUT ONLY 1ST IS RECORDED.)                */
*/*                                                                  */
*/*   PROCESS - THIS SUBROUTINE PLACES THE LSID OF A SLOT            */
*/*       WHICH SUFFERED AN I/O ERROR INTO AN SQA BUFFER LIST OF     */
*/*       BAD SLOTS.  THIS IS A WRAP AROUND BUFFER AND IS USED FOR   */
*/*       DEBUGGING PURPOSES.  IT IS NOT EXAMINED BY ANY ASM ROUTINE */
*/*       THIS ROUTINE OBTAINS THE BAD SLOT LIST ON FIRST ENTRY.     */
*/*                                                                  */
*/*                                                                  */
*/*   OUTPUT - ASMEREC UPDATED WITH ADDRESS OF BAD SLOT LIST WHEN    */
*/*              OBTAINED.                                           */
*/*            NEXT ENTRY IN TABLE UPDATED WITH NEWEST ENTRY.        */
*/*                                                                  */
*/****************************************************************** */
*/*                                                                  */
*/*RECERR: CHART (AREF)                                              */
*/*HEADER                                                          0460
*/*                               I/O COMPLETE -                   0460
*/*RECERR                                                            */
*/*RECERR: E BEGIN SEGMENT ADD SLOT TO LIST                          */
*/* N THIS RTN ONLY GETS BUFFER 1ST TIME THERES A BAD SLOT           */
*/* D (YES,%T1,NO,) PREV ERROR- BFR OBTAINED YET? ASMEREC            */
*/* N USE BRANCH ENTRY TO GETMAIN TO OBTAIN BAD SLOT LIST            */
*/* L SETLOCK: OBTAIN SALLOC (UNCOND)                                */
*/* N CHECK AGAIN- COULDVE BEEN GOTTEN WHILE WE LOCKED               */
*/* D (YES,,NO,%F1) STILL UNOBTAINED? ASMEREC                        */
*/* L GETMAIN: RC LV=1024 SP=245 BRANCH=(YES GLOBAL)                 */
*/* D (YES,,NO,%F1) GETMAIN OK? R15                                  */
*/* N FORMAT BAD SLOT LIST                                           */
*/* N SET UP BUFFER HEADER ENTRIES- CURRENT, 1ST, & LAST             */
*/* P SET CURRENT PTR TO ASMEREC+8                                   */
*/* P SET 1ST PTR TO ASMEREC + 12                                    */
*/* P SET LAST PTR TO ASMEREC + 1020                                 */
*/* P INIT BAD SLOT LIST TO ZERO                                     */
*/* P SET ADDR OF LIST IN ASMVT ASMEREC                              */
*/*%F1: L SETLOCK: RELEASE SALLOC                                    */
*/* D (YES,,NO,%P1) GETMAIN OK OR ALREADY THERE? ASMEREC             */
*/*%T1: D (YES,,NO,%F2) THIS ENTRY THE LAST ENTRY?                   */
*/* P (,%N1) SET REG X TO 1ST PTR                                    */
*/*%F2: P ADD 4 TO REG X                                             */
*/*%N1: P UPDATE CURR PTR WITH REG X (C+S)                           */
*/* D (YES,,NO,%T1) C + S SUCCESSFUL?                                */
*/* P PUT LSID IN CURRENT ENTRY USE REG X                            */
*/* D (PAG,%P1,SWP,) PAGE OR SWAP? IORSWAP                           */
*/* P SET HI ORDER BIT THIS ENTRY INDICATE SWAP USE REG X            */
*/*%P1: R RETURN                                                     */
*/*RECERR: END                                                       */
*                                                                  0460
*RECERR:                                                           0460
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0460
RECERR   DS    0H                                                  0461
*   RFY                                                            0461
*     R14 RSTD;                                                    0461
*   RTNADDR=R14;                    /* SAVE RETURN ADDRESS           */
         ST    R14,RTNADDR                                         0462
*   RFY                                                            0463
*     R14 UNRSTD;                                                  0463
*   IF ASMEREC=0 THEN               /* EVER GET THE BAD SLOT LIST?   */
         L     @12,ASMEREC(,ASMVTPTR)                              0464
         LTR   @12,@12                                             0464
         BNZ   @RF00464                                            0464
*     DO;                           /* NOT YET- GET IT NOW           */
*                                                                  0465
*       /*************************************************************/
*       /*                                                           */
*       /* USE BRANCH ENTRY TO GETMAIN. SALLOC LOCK REQUIRED         */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0466
*       RFY                                                        0466
*        (R11,                                                     0466
*         R12,                                                     0466
*         R13,                                                     0466
*         R14) RSTD;                                               0466
*IOCLK1:                                                           0467
*       GEN REFS(R12,R13,R14,PSALITA,FLC) SETS(R11,R12,R13,R14);   0467
IOCLK1   DS    0H                                                  0467
         SETLOCK OBTAIN,TYPE=SALLOC,MODE=UNCOND,                       *
               RELATED=(GETMAIN,ILRCMP(IOCUNLK1))
*       RFY                                                        0468
*        (R11,                                                     0468
*         R12,                                                     0468
*         R14) UNRSTD;                                             0468
*       IF ASMEREC=0 THEN           /* MAKE SURE STILL UNOBTAINED. 0469
*                                      COULDVE BEEN GOTTEN ON ANOTHER
*                                      CPU WHILE WE LOCKED. DONT WANT
*                                      TO GET IT TWICE.              */
         L     @12,ASMEREC(,ASMVTPTR)                              0469
         LTR   @12,@12                                             0469
         BNZ   @RF00469                                            0469
*         DO;                       /* NOPE- KEEP GOIN               */
*           STM(R3,R4,R34SAVE);     /* SAVE REGS ACROSS GETMAIN      */
         STM   R3,R4,R34SAVE                                       0471
*           RFY                                                    0472
*            (R0,                                                  0472
*             R1,                                                  0472
*             R3,                                                  0472
*             R4,                                                  0472
*             R14,                                                 0472
*             R15) RSTD;                                           0472
*           GEN SETS(R0,R1,R3,R4,R14,R15);                         0473
         GETMAIN RC,LV=1024,SP=245,BRANCH=(YES,GLOBAL)
*           RFY                                                    0474
*            (R0,                                                  0474
*             R1,                                                  0474
*             R14,                                                 0474
*             R15) UNRSTD;                                         0474
*           LM(R3,R4,R34SAVE);      /* RESTORE REGS                  */
         LM    R3,R4,R34SAVE                                       0475
*           RFY                                                    0476
*            (R1,                                                  0476
*             R15) RSTD;                                           0476
*           IF R15=0 THEN           /* DID WE SWING IT?              */
         LTR   R15,R15                                             0477
         BNZ   @RF00477                                            0477
*             DO;                   /* YUP- FORMAT BAD SLOT LIST   0478
*                                      HEADER                        */
*               R1->BSCURR=R1+BSCURPRM;/* PRIME CUR PTR TO ONE ENTRY
*                                      BEFORE PHYSICAL FIRST         */
         LA    @14,8                                               0479
         ALR   @14,R1                                              0479
         ST    @14,BSCURR(,R1)                                     0479
*               R1->BSFIRST=R1+BS1STPRM;/* PRIME PHYSICAL 1ST NTRY   */
         LA    @14,12                                              0480
         ALR   @14,R1                                              0480
         ST    @14,BSFIRST(,R1)                                    0480
*               R1->BSLAST=R1+BSLSTPRM;/* PRIME PHYSICAL LAST NTRY   */
*                                                                  0481
         LA    @14,1020                                            0481
         ALR   @14,R1                                              0481
         ST    @14,BSLAST(,R1)                                     0481
*               /*****************************************************/
*               /*                                                   */
*               /* USE MOVE CHAR LONG TO INITIALIZE TABLE TO ZERO    */
*               /*                                                   */
*               /*****************************************************/
*                                                                  0482
*               RFY                                                0482
*                (R8,                                              0482
*                 R9,                                              0482
*                 R10,                                             0482
*                 R11) RSTD;                                       0482
*               R8=ADDR(R1->BSLIST);                               0483
         LA    R8,BSLIST(,R1)                                      0483
*               R9=LENGTH(R1->BSLIST);                             0484
         LA    R9,1012                                             0484
*               R10=0;                                             0485
         SLR   R10,R10                                             0485
*               R11=0;              /* THIS SETS PAD CHAR TO ZERO    */
         SLR   R11,R11                                             0486
*               MVCL(R8,R10);       /* INIT TO ZEROS                 */
         MVCL  R8,R10                                              0487
*               RFY                                                0488
*                (R8,                                              0488
*                 R9,                                              0488
*                 R10,                                             0488
*                 R11) UNRSTD;                                     0488
*               ASMEREC=R1;         /* AND FINALLY, SET LIST ADDR IN
*                                      ASMVT                         */
         ST    R1,ASMEREC(,ASMVTPTR)                               0489
*             END;                  /* END FORMAT HEADER             */
*           ELSE                                                   0491
*             ;                     /* GETMAIN FAILED                */
@RF00477 DS    0H                                                  0492
*           RFY                                                    0492
*            (R1,                                                  0492
*             R15) UNRSTD;                                         0492
*         END;                      /* END KEEP GOIN- STILL        0493
*                                      UNOBTAINED                    */
*       ELSE                                                       0494
*         ;                         /* LIST ALREADY OBTAINED WHILE WE
*                                      LOCKED                        */
@RF00469 DS    0H                                                  0495
*       RFY                                                        0495
*        (R11,                                                     0495
*         R12,                                                     0495
*         R13,                                                     0495
*         R14) RSTD;                                               0495
*IOCUNLK1:                                                         0496
*       GEN REFS(R12,R13,R14,PSALITA,FLC) SETS(R11,R12,R13,R14);   0496
IOCUNLK1 DS    0H                                                  0496
         SETLOCK RELEASE,TYPE=SALLOC,RELATED=(GETMAIN,ILRCMP(IOCLK1))
*       RFY                                                        0497
*        (R11,                                                     0497
*         R12,                                                     0497
*         R14) UNRSTD;                                             0497
*                                                                  0497
*       /*************************************************************/
*       /*                                                           */
*       /* AT THIS POINT WE ARE ABOUT TO FALL INTO THE CODE TO USE   */
*       /* THE TABLE FOR THE FIRST TIME- MAKE SURE WE WERE SUCCESSFUL*/
*       /* IN GETTING THE TABLE ABOVE.                               */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0498
*       IF ASMEREC=0 THEN           /* SHOULD EXIST UNLESS OUR     0498
*                                      GETMAIN FAILED                */
         L     @12,ASMEREC(,ASMVTPTR)                              0498
         LTR   @12,@12                                             0498
         BZ    @RT00498                                            0498
*         GOTO RECXIT;                                             0499
*       ELSE                                                       0500
*         ;                         /* EVERYTHING OK- FALL INTO TABLE
*                                      UPDATE CODE. FROM NOW ON- WE
*                                      WILL ALWAYS GO DIRECTLY HERE  */
*     END;                          /* END OF GET THE BAD SLOT LIST-
*                                      FIRST TIME                    */
*   ELSE                                                           0502
*     ;                                                            0502
@RF00464 DS    0H                                                  0503
*                                                                  0503
*   /*****************************************************************/
*   /*                                                               */
*   /* BAD SLOT TABLE UPDATE CODE. UPDATE CURRENT PTR IN HEADER TO   */
*   /* NEXT TABLE ENTRY AND ADD NEW ENTRY TO TABLE. USE C+S TO UPDATE*/
*   /* CURR PTR SINCE ANOTHER CPU MAY BE DOING SAME THING.           */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0503
*   RFY                                                            0503
*     BSHEADER BASED(ASMEREC);                                     0503
*   RFY                                                            0504
*    (R0,                                                          0504
*     R1) RSTD;                                                    0504
*   R0=BSCURR;                      /* GET CURRENT CURRENT-PTR       */
         L     @12,ASMEREC(,ASMVTPTR)                              0505
         L     R0,BSCURR(,@12)                                     0505
*RETRYBS:                                                          0506
*   IF R0=BSLAST THEN               /* LAST ELEM USED?               */
RETRYBS  L     @12,ASMEREC(,ASMVTPTR)                              0506
         C     R0,BSLAST(,@12)                                     0506
         BNE   @RF00506                                            0506
*     R1=BSFIRST;                   /* YES- SET UP TO WRAP AROUND    */
         L     R1,BSFIRST(,@12)                                    0507
*   ELSE                            /* SOME LEFT-                    */
*     R1=R0+BSINCR;                 /* BUMP TO NEXT ELEM             */
         B     @RC00506                                            0508
@RF00506 LA    R1,4                                                0508
         ALR   R1,R0                                               0508
*   CS(R0,R1,BSCURR);               /* UPDATE HEADER CURRENT PTR     */
@RC00506 L     @12,ASMEREC(,ASMVTPTR)                              0509
         CS    R0,@01,BSCURR(@12)                                  0509
*   BC(NE,RETRYBS);                                                0510
*                                                                  0510
         BC    7,RETRYBS                                           0510
*   /*****************************************************************/
*   /*                                                               */
*   /* WHEN FALL HERE, REG1 CONTAINS OUR BSCURR VALUE. USE R1 TO     */
*   /* UPDATE TABLE SINCE BSCURR ITSELF MAY CHANGE AGAIN FOR OTHER   */
*   /* CPU                                                           */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0511
*   IF ATACPCCW->PCCWAIA->AIADUPLX=ON&/* IS THIS A                   */
*       PAREDPLX=ON THEN            /* SECONDARY OPERATION?          */
         L     @12,ATACPCCW(,ATAPTR)                               0511
         L     @12,PCCWAIA(,@12)                                   0511
         TM    AIADUPLX(@12),B'00010000'                           0511
         BNO   @RF00511                                            0511
         L     @05,PAREPTR                                         0511
         TM    PAREDPLX(@05),B'00100000'                           0511
         BNO   @RF00511                                            0511
*     R1->BSTABNTY=ATACPCCW->PCCWAIA->AIALSID2;/* PULL PROPER LSID   */
         L     @12,AIALSID2(,@12)                                  0512
         ST    @12,BSTABNTY(,R1)                                   0512
*   ELSE                            /* NOT SECONDARY OPERATION       */
*     R1->BSTABNTY=ATACPCCW->PCCWAIA->AIALSID;/* ADD BAD P/SCCW AIA
*                                      SLOT ID TO TABLE              */
         B     @RC00511                                            0513
@RF00511 L     @12,ATACPCCW(,ATAPTR)                               0513
         L     @12,PCCWAIA(,@12)                                   0513
         L     @12,AIALSID(,@12)                                   0513
         ST    @12,BSTABNTY(,R1)                                   0513
*   IF IORSWAP=ON THEN              /* WAS IT SWAP?                  */
@RC00511 L     @12,IORBPTR                                         0514
         TM    IORSWAP(@12),B'00001000'                            0514
         BNO   @RF00514                                            0514
*     R1->BSSPLSID=SWPLSID;         /* YES- INDICATE SUCH            */
         MVI   BSSPLSID(R1),X'80'                                  0515
*   ELSE                            /* NO-PAGE-                      */
*     R1->BSSPLSID=PAGLSID;         /* MAY HAVE BEEN ON LAST TIME  0516
*                                      USED                          */
         B     @RC00514                                            0516
@RF00514 MVI   BSSPLSID(R1),X'00'                                  0516
*   IF ATACPCCW->PCCWAIA->AIAWRITE^=ON THEN/* A READ ERROR   @Z40WPWP*/
@RC00514 L     @12,ATACPCCW(,ATAPTR)                               0517
         L     @12,PCCWAIA(,@12)                                   0517
         TM    AIAWRITE(@12),B'10000000'                           0517
         BO    @RF00517                                            0517
*     R1->BSSPLSID=R1->BSSPLSID'08'X;/* INDICATE A READ ERROR     0518
*                                                            @Z40WPWP*/
         OI    BSSPLSID(R1),X'08'                                  0518
*   RFY                                                            0519
*    (R0,                                                          0519
*     R1) UNRSTD;                                                  0519
@RF00517 DS    0H                                                  0520
*RECXIT:                                                           0520
*   RFY                                                            0520
*     R14 RSTD;                                                    0520
RECXIT   DS    0H                                                  0521
*   R14=RTNADDR;                    /* RESTORE RETURN ADDR           */
         L     R14,RTNADDR                                         0521
*   RFY                                                            0522
*     R14 UNRSTD;                                                  0522
*   END RECERR;                                                    0523
@EL00008 DS    0H                                                  0523
@EF00008 DS    0H                                                  0523
@ER00008 BR    @14                                                 0523
*                                                                  0524
*/********************************************************************/
*/*                                                                  */
*/*   BADSLOT - PROCESS ERROR P/SCCW(S)                              */
*/*                                                                  */
*/*   INPUT - POINTER TO THE FIRST P/SCCW OF THE CHAIN OR TO THE     */
*/*       P/SCCW OF THE OFFENDING I/O REQUEST - ATACPCCW             */
*/*                                                                  */
*/*   PROCESS - EACH AIA ASSOCIATED WITH THE INPUT P/SCCW(S) IS      */
*/*       MARKED PRIMARY OR SECONDARY ERROR. IF THE AIA IS FOR A     */
*/*       DUPLEX OPERATION, THEN DECREMENT THE DUPLEX COUNT IN       */
*/*       THE AIA BY ONE. IF THE DUPLEX COUNT IS NOW ZERO THEN       */
*/*       PUT THAT AIA ON THE INTERNAL COMPLETION QUEUE. NOTE        */
*/*       THAT AN AIA FOR A DUPLEX OPERATION WILL NEVER BE PLACED    */
*/*       ON THE INTERNAL COMPLETION QUEUE UNTIL BOTH WRITE          */
*/*       OPERATIONS HAVE COMPLETED. IF THE DUPLEXED WRITE WAS       */
*/*       FOR THE DUPLEX DATA SET THEN MARK THE SECONDARY ERROR      */
*/*       FLAG. OTHERWISE, MARK THE PRIMARY ERROR FLAG. IF THE       */
*/*       AIA IS NOT FOR A DUPLEX OPERATION, THEN PLACE THE AIA      */
*/*       ON THE INTERNAL COMPLETION QUEUE AND MARK THE PRIMARY      */
*/*       ERROR FLAG. EACH ASSOCIATED P/SCCW IS PLACED ON THE        */
*/*       INTERAL FREE QUEUE TO BE LATER FREED.              @ZA16409*/
*/*                                                                  */
*/*   OUTPUT - AIAPRIER OR AIASECER. ATACOMPQ AND ATAPCCWQ.          */
*/*                                                                  */
*/********************************************************************/
*/*                                                                  */
*/*BADSLOT: CHART (AREF)                                             */
*/* HEADER                                                         0524
*/*                               I/O COMPLETE -                   0524
*/*BADSLOT                                                           */
*/*BADSLOT: E PROCESS ERROR P/SCCW(S)                                */
*/* N CAN HAVE ONE, OR A CHAIN OF P/SCCWS.                           */
*/* P PICK UP PTR TO 1ST P/SCCW PCCWPTR                              */
*/*%BLOOP: D (YES,%BRET,NO,) END OF CHAIN?                           */
*/* D (YES,,NO,%S1) AIADUPLX ON ?                            @ZA16409*/
*/* P DECREMENT AIADPXCT BY 1                                @ZA16409*/
*/* D (YES,,NO,%D1) AIADPXCT = 0 ?                           @ZA16409*/
*/* P PUT ASSOC AIA ON TOP OF INTERNAL ATACOMPQ              @ZA16409*/
*/*%D1: D (YES,,NO,%F1) PARETYPE = DPLX ?                    @ZA16409*/
*/* P (,%N1) SET SECONDARY ERROR AIASECER                    @ZA16409*/
*/*%F1: P (,%N1) P SET PRIMARY ERROR AIAPRIER                @ZA16409*/
*/*%S1: P PUT ASSOC AIA ON TOP OF INTERNAL ATACOMPQ                  */
*/* P SET PRIMARY ERROR AIAPRIER                                     */
*/*%N1: P ZERO P/SCCW PTR TO AIA FOR RCV                             */
*/* P PUT P/SCCW ON TOP OF INTERNAL ATAPCCWQ                         */
*/* P (,%BLOOP) GET NEXT P/SCCW PCCWPCCW                             */
*/*%BRET: R RETURN                                                   */
*/*BADSLOT: END                                                      */
*                                                                  0524
*BADSLOT:                                                          0524
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0524
BADSLOT  DS    0H                                                  0525
*   RFY                                                            0525
*     R14 RSTD;                                                    0525
*                                                                  0525
*   /*****************************************************************/
*   /*                                                               */
*   /* PROCESS BAD P/SCCWS - COULD HAVE ONE OR A CHAIN OF BAD P/SCCWS*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0526
*   PCCWPTR=ATACPCCW;                                              0526
         L     @15,ATACPCCW(,ATAPTR)                               0526
         ST    @15,PCCWPTR                                         0526
*   DO WHILE(PCCWPTR^=0);                                          0527
         B     @DE00527                                            0527
@DL00527 DS    0H                                                  0528
*     ATACPCCW=ATACPCCW->PCCWPCCW;  /* IMMEDIATELY REMOVE ONE WERE 0528
*                                      WORKING ON FROM ATA           */
*                                                                  0528
         L     @15,ATACPCCW(,ATAPTR)                               0528
         L     @15,PCCWPCCW(,@15)                                  0528
         ST    @15,ATACPCCW(,ATAPTR)                               0528
*     /***************************************************************/
*     /*                                                             */
*     /* DETERMINE WHETHER TO MARK PRI OR SEC ERROR. TEST FOR DPLX   */
*     /* MUST BE MADE IN FOLLOWING ORDER: CHECK AIADUPLX BIT BEFORE  */
*     /* PAREDPLX IN CASE WE HAVE A SARTE INSTEAD OF A PARTE         */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0529
*     RFY                                                          0529
*       AIA BASED(PCCWAIA);         /*                       @ZA16409*/
*     IF AIADUPLX=ON THEN           /* DUPLEXED OPERATION ?  @ZA16409*/
         L     @15,PCCWPTR                                         0530
         L     @15,PCCWAIA(,@15)                                   0530
         TM    AIADUPLX(@15),B'00010000'                           0530
         BNO   @RF00530                                            0530
*       DO;                         /* YES,                  @ZA16409*/
*         RFY                                                      0532
*          (R0,                                                    0532
*           R1) RSTD;               /*                       @ZA16409*/
*         R0=AIADPXCT;              /* OBTAIN DUPLEX COUNT   @ZA16409*/
         L     R0,AIADPXCT(,@15)                                   0533
*REPEAT1:                                                          0534
*         R1=R0-1;                  /* DECREMENT DUPLEX COUNT      0534
*                                                            @ZA16409*/
REPEAT1  LR    R1,R0                                               0534
         BCTR  R1,0                                                0534
*         CS(R0,R1,AIADPXCT);       /* UPDATE DUPLEX COUNT IN AIA  0535
*                                                            @ZA16409*/
         L     @15,PCCWPTR                                         0535
         L     @15,PCCWAIA(,@15)                                   0535
         CS    R0,@01,AIADPXCT(@15)                                0535
*         BC(NE,REPEAT1);           /* UNSUCCESSFUL, TRY AGAIN     0536
*                                                            @ZA16409*/
         BC    7,REPEAT1                                           0536
*         IF R1=0 THEN              /* DUPLEX COUNT IS ZERO ?      0537
*                                                            @ZA16409*/
         LTR   R1,R1                                               0537
         BNZ   @RF00537                                            0537
*           DO;                     /* YES-                  @ZA16409*/
*             AIANXAIA=ATACOMPQ;    /* CHAIN IN EXISTING AIA'S     0539
*                                                            @ZA16409*/
         L     @15,PCCWPTR                                         0539
         L     @15,PCCWAIA(,@15)                                   0539
         L     @12,ATACOMPQ(,ATAPTR)                               0539
         ST    @12,AIANXAIA(,@15)                                  0539
*             ATACOMPQ=PCCWAIA;     /* PUT THIS AIA ON TOP OF      0540
*                                      INTERNAL POST QUEUE   @ZA16409*/
         ST    @15,ATACOMPQ(,ATAPTR)                               0540
*           END;                    /* END - DUPLEX COUNT IS ZERO  0541
*                                                            @ZA16409*/
*         RFY                                                      0542
*          (R0,                                                    0542
*           R1) UNRSTD;             /*                       @ZA16409*/
@RF00537 DS    0H                                                  0543
*         IF PAREDPLX=ON THEN       /* SECONDARY OPERATION ? @ZA16409*/
         L     @15,PAREPTR                                         0543
         TM    PAREDPLX(@15),B'00100000'                           0543
         BNO   @RF00543                                            0543
*           AIASECER=ON;            /* YES, SET SECONDARY ERROR BIT
*                                                            @ZA16409*/
         L     @15,PCCWPTR                                         0544
         L     @15,PCCWAIA(,@15)                                   0544
         OI    AIASECER(@15),B'00000100'                           0544
*         ELSE                      /* NO,                   @ZA16409*/
*           AIAPRIER=ON;            /* SET PRIMARY ERROR BIT @ZA16409*/
         B     @RC00543                                            0545
@RF00543 L     @15,PCCWPTR                                         0545
         L     @15,PCCWAIA(,@15)                                   0545
         OI    AIAPRIER(@15),B'00001000'                           0545
*       END;                        /* END DUPLEX PROCESSING @ZA16409*/
*     ELSE                          /* NO,                   @ZA16409*/
*       DO;                         /* NOT DUPLEXED OPERATION      0547
*                                                            @ZA16409*/
         B     @RC00530                                            0547
@RF00530 DS    0H                                                  0548
*         AIANXAIA=ATACOMPQ;        /* CHAIN IN EXISTING AIA'S     0548
*                                                            @ZA16409*/
         L     @15,PCCWPTR                                         0548
         L     @15,PCCWAIA(,@15)                                   0548
         L     @12,ATACOMPQ(,ATAPTR)                               0548
         ST    @12,AIANXAIA(,@15)                                  0548
*         ATACOMPQ=PCCWAIA;         /* PUT THIS AIA ON TOP OF      0549
*                                      INTERNAL POST QUEUE   @ZA16409*/
         ST    @15,ATACOMPQ(,ATAPTR)                               0549
*         AIAPRIER=ON;              /* SET PRIMARY ERROR BIT @ZA16409*/
         OI    AIAPRIER(@15),B'00001000'                           0550
*       END;                        /* END-NOT DUPLEXED OPERATION  0551
*                                                            @ZA16409*/
*     RFY                                                          0552
*       AIA BASED(AIAPTR);          /*                       @ZA16409*/
@RC00530 DS    0H                                                  0553
*     PCCWAIA=0;                    /* REMOVE FROM P/SCCW FOR      0553
*                                      RECOVERY                      */
         L     @15,PCCWPTR                                         0553
         SLR   @12,@12                                             0553
         ST    @12,PCCWAIA(,@15)                                   0553
*     PCCWPCCW=ATAPCCWQ;            /* CHAIN IN EXISTING P/SCCWS AND */
         L     @12,ATAPCCWQ(,ATAPTR)                               0554
         ST    @12,PCCWPCCW(,@15)                                  0554
*     ATAPCCWQ=PCCWPTR;             /* PUT THIS ONE ON TOP OF      0555
*                                      INTERNAL FREE QUE             */
         ST    @15,ATAPCCWQ(,ATAPTR)                               0555
*     PCCWPTR=ATACPCCW;             /* GET NEXT, IF ANY              */
         L     @15,ATACPCCW(,ATAPTR)                               0556
         ST    @15,PCCWPTR                                         0556
*   END;                            /* END PROCESS BAD P/SCCWS       */
@DE00527 L     @15,PCCWPTR                                         0557
         LTR   @15,@15                                             0557
         BNZ   @DL00527                                            0557
*   RFY                                                            0558
*     R14 UNRSTD;                                                  0558
*   END BADSLOT;                                                   0559
@EL00009 DS    0H                                                  0559
@EF00009 DS    0H                                                  0559
@ER00009 BR    @14                                                 0559
*                                                                  0560
*/****************************************************************** */
*/*                                                                  */
*/*   RECHAIN - RECHAIN P/SCCWS TO BE REDRIVEN THRU IOS.             */
*/*                                                                  */
*/*   INPUT - POINTER TO THE 1ST P/SCCW TO BE RECHAINED- IORPCCW     */
*/*                                                                  */
*/*   PROCESS - RESET IOSB AND IORB FIELDS NECESSARY TO REDRIVE      */
*/*       THE I/O OPERATION FROM THIS NEW POINT. ZERO THE STARTING   */
*/*       TIME STAMP VALUE IN THE IORB.                              */
*/*                                                                  */
*/*   OUTPUT - IOSVST, IOSRST, IOSEEK, IOSEEKA,                      */
*/*             IORQSZ, IORTSMP.                                     */
*/*                                                                  */
*/****************************************************************** */
*/*RECHAIN: CHART (AREF)                                             */
*/* HEADER                                                         0560
*/*                               I/O COMPLETE -                   0560
*/*RECHAIN                                                           */
*/*RECHAIN: E BEGIN SEGMENT PROCESS RECHAIN PCCWS                    */
*/* N THE IORB POINTS TO THE FIRST P/SCCW TO BE RETRIED              */
*/* COMMENT (12,7) ALL THE PCCWS/                                    */
*/* COMMENT (13,7) SCCWS BEFORE HAVE                                 */
*/* COMMENT (14,7) BEEN PROCESSED.                                   */
*/* COMMENT (17,7) REMAINING P/SCCWS                                 */
*/* COMMENT (18,7) ARE TO BE RETRIED.                                */
*/* N THIS P/SCCW IS THE FIRST ONE RETURNED TO IOS.                  */
*/* D (PAG,,SWP,%S1) PAGE OR SWAP? IORSWAP                           */
*/* P (,%N1) SET IOSVST=ADDR OF SEARCH CCW IN PCCW                   */
*/*%S1: P SET IOSVST=ADDR OF SET SECTOR CCW IN SCCW                  */
*/*%N1: P SET IOSRST = IOSVST                                        */
*/* P RESET IOSB FIELDS- SEE BELOW                                   */
*/* N IOSEEK, IOSEEKA.                                               */
*/* P SET CTR TO ONE                                                 */
*/*%CKCHN: D (NO,%ENDCHN,YES,) ANY MORE P/SCCWS IN CHAIN             */
*/* P ADD 1 TO CTR                                                   */
*/*%SELECT: P (,%CKCHN) SELECT NEXT P/SCCW                           */
*/*%ENDCHN: P SET CTR IN IORB IORQSZ                                 */
*/* P ZERO TIME STAMP VALUE IORTSMP                                  */
*/* R RETURN                                                         */
*/*RECHAIN: END                                                      */
*                                                                  0560
*RECHAIN:                                                          0560
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0560
RECHAIN  DS    0H                                                  0561
*   RFY                                                            0561
*     R14 RSTD;                                                    0561
*   PCCWPTR=IORPCCW;                /* SET PCCW MAP BASE             */
         L     @15,IORBPTR                                         0562
         L     @12,IORPCCW(,@15)                                   0562
         ST    @12,PCCWPTR                                         0562
*   IF IORSWAP=ON THEN              /* SWAP?                         */
         TM    IORSWAP(@15),B'00001000'                            0563
         BNO   @RF00563                                            0563
*     IOSVST=ADDR(SCCWSSEC);        /* YES- SET IOSB TO ADDR OF SET
*                                      SECTOR CCW- WE KNOW ITS VALID
*                                      FOR SWAP SINCE WE KNOW WERE 0564
*                                      PROCESSING FIRST OF SET       */
         L     @15,SCCWPTR                                         0564
         LA    @15,SCCWSSEC(,@15)                                  0564
         L     @12,IOSBPTR                                         0564
         ST    @15,IOSVST(,@12)                                    0564
*   ELSE                            /* NOT SWAP- PAGE                */
*     IOSVST=ADDR(PCCWSRCH);        /* FOR PAGING- DONT KNOW WHERE WE
*                                      ARE- SKIP SET SECTOR          */
         B     @RC00563                                            0565
@RF00563 L     @15,PCCWPTR                                         0565
         LA    @15,PCCWSRCH(,@15)                                  0565
         L     @12,IOSBPTR                                         0565
         ST    @15,IOSVST(,@12)                                    0565
*   IOSRST=IOSVST;                  /* OUR CCWS ARE IN V=R           */
@RC00563 L     @15,IOSBPTR                                         0566
         L     @12,IOSVST(,@15)                                    0566
         ST    @12,IOSRST(,@15)                                    0566
*   IOSEEK=PCCWCHHR;                /* SEEK ADDR- P/SCCW             */
         L     @12,PCCWPTR                                         0567
         MVC   IOSEEK(8,@15),PCCWCHHR(@12)                         0567
*   IOSEEKA=PCCWCHHR;               /* DYNAMIC SEEK ADDR- P/SCCW     */
         MVC   IOSEEKA(8,@15),PCCWCHHR(@12)                        0568
*   IORRQSZ=ONE;                    /* INITIALIZE IORB P/SCCW CT     */
         L     @15,IORBPTR                                         0569
         MVI   IORRQSZ(@15),X'01'                                  0569
*   DO WHILE(PCCWPCCW^=0);          /* COUNT NUMBER P/SCCWS          */
         B     @DE00570                                            0570
@DL00570 DS    0H                                                  0571
*     IORRQSZ=IORRQSZ+ONE;                                         0571
         L     @15,IORBPTR                                         0571
         LA    @12,1                                               0571
         SLR   @05,@05                                             0571
         IC    @05,IORRQSZ(,@15)                                   0571
         ALR   @12,@05                                             0571
         STC   @12,IORRQSZ(,@15)                                   0571
*     PCCWPTR=PCCWPCCW;             /* GET NEXT                      */
         L     @15,PCCWPTR                                         0572
         L     @15,PCCWPCCW(,@15)                                  0572
         ST    @15,PCCWPTR                                         0572
*   END;                                                           0573
@DE00570 L     @15,PCCWPTR                                         0573
         L     @15,PCCWPCCW(,@15)                                  0573
         LTR   @15,@15                                             0573
         BNZ   @DL00570                                            0573
*   IORTSMP=''B;                    /* ZERO SRT TIME STAMP VALUE     */
         L     @15,IORBPTR                                         0574
         XC    IORTSMP(8,@15),IORTSMP(@15)                         0574
*   RFY                                                            0575
*     R14 UNRSTD;                                                  0575
*   END RECHAIN;                                                   0576
@EL00010 DS    0H                                                  0576
@EF00010 DS    0H                                                  0576
@ER00010 BR    @14                                                 0576
*                                                                  0577
*/****************************************************************** */
*/*                                                                  */
*/*   PROCCCWS - PROCESS ALL SUCCESSFULLY COMPLETED I/O REQUESTS.    */
*/*                                                                  */
*/*   INPUT - POINTER TO THE FIRST P/SCCW TO BE PROCESSED- IORPCCW.  */
*/*           POINTER TO THE LAST P/SCCW TO BE PROCESSED- PRECEDES   */
*/*                IORERR.                                           */
*/*                                                                  */
*/*   PROCESS - SEARCH EACH GOOD AIA. IF THE AIA IS FOR A DUPLEX     */
*/*       OPERATION, THEN DECREMENT THE DUPLEX COUNT IN THE AIA      */
*/*       BY ONE. IF THE DUPLEX COUNT IS NOW ZERO THEN PUT THAT      */
*/*       AIA ON THE INTERNAL COMPLETION QUEUE. NOTE THAT AN AIA     */
*/*       FOR A DUPLEX OPERATION WILL NEVER BE PLACED ON THE         */
*/*       INTERNAL COMPLETION QUEUE UNTIL BOTH WRITE OPERATIONS      */
*/*       HAVE COMPLETED. IF THE AIA IS NOT FOR A DUPLEX             */
*/*       OPERATION, THEN PLACE THE AIA ON TOP OF THE INTERNAL       */
*/*       COMPLETION QUEUE TO BE LATER POSTED BACK TO PAGE           */
*/*       COMPLETION. EACH ASSOC P/SCCW IS PLACED ON TOP OF AN       */
*/*       INTERNAL FREE QUEUE TO BE LATER FREED ALL AT ONCE.         */
*/*                                                         @ZA16409 */
*/*                                                                  */
*/*   OUTPUT - ATACOMPQ, ATAPCCWQ, AND NUMREQ (NUMBER OF REQUESTS    */
*/*              FOR COMPBRST SUBROUTINE)                            */
*/*                                                                  */
*/****************************************************************** */
*/*                                                                  */
*/*PROCCCWS: CHART (AREF)                                            */
*/*HEADER                                                          0577
*/*                               I/O COMPLETE                     0577
*/*PROCCCWS                                                          */
*/*PROCCCWS: E PROCESS GOOD P/SCCWS                                  */
*/* N LOOP THRU P/SCCWS UNTIL HIT ERROR P/SCCW OR END (IORERR)       */
*/* N IORERR ALWAYS SET TO ERROR P/SCCW OR 0                         */
*/* P GET FIRST P/SCCW PTR                                           */
*/*%PLOOP: D (YES,%T1,NO,) HIT ERROR OR LAST? IORERR                 */
*/* D (YES,,NO,%F1) AIADUPLX ON ?                           @ZA16409 */
*/* P DECREMENT AIADPXCT BY 1                               @ZA16409 */
*/* D (YES,,NO,%S1) AIADPXCT = 0 ?                          @ZA16409 */
*/*%F1: P PUT THIS P/SCCWS AIA ON TOP OF INTERNAL ATACOMPQ  @ZA16409 */
*/*%S1: P ZERO P/SCCW AIA PTR FOR RCV                       @ZA16409 */
*/* P PUT THIS P/SCCW ON TOP OF INTERNAL ATAPCCWQ                    */
*/* P GET NEXT P/SCCW PTR                                            */
*/* P (,%PLOOP) INCR NUM REQUESTS FOR COMPBRST                       */
*/*%T1: R RETURN                                                     */
*                                                                  0577
*PROCCCWS:                                                         0577
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0577
PROCCCWS DS    0H                                                  0578
*   RFY                                                            0578
*     R14 RSTD;                                                    0578
*   RFY                                                            0579
*     PCCW BASED(ATACPCCW);                                        0579
*   ATACPCCW=IORPCCW;               /* PICK UP FIRST P/SCCW ON IORB  */
         L     @15,IORBPTR                                         0580
         L     @15,IORPCCW(,@15)                                   0580
         ST    @15,ATACPCCW(,ATAPTR)                               0580
*   DO WHILE(ATACPCCW^=IORERR);     /* PROCESS ALL PCCWS ON IORB     */
         B     @DE00581                                            0581
@DL00581 DS    0H                                                  0582
*     IORPCCW=PCCWPCCW;             /* IMMEDIATELY REMOVE IN-PROCESS
*                                      PCCW FROM IORB FOR RECOVERY   */
         L     @15,ATACPCCW(,ATAPTR)                               0582
         L     @12,PCCWPCCW(,@15)                                  0582
         L     @08,IORBPTR                                         0582
         ST    @12,IORPCCW(,@08)                                   0582
*     RFY                                                          0583
*       AIA BASED(PCCWAIA);         /*                       @ZA16409*/
*     IF AIADUPLX=ON THEN           /* DUPLEXED OPERATION ?  @ZA16409*/
         L     @15,PCCWAIA(,@15)                                   0584
         TM    AIADUPLX(@15),B'00010000'                           0584
         BNO   @RF00584                                            0584
*       DO;                         /* YES,                  @ZA16409*/
*         RFY                                                      0586
*          (R0,                                                    0586
*           R1) RSTD;               /*                       @ZA16409*/
*         R0=AIADPXCT;              /* OBTAIN DUPLEX COUNT   @ZA16409*/
         L     R0,AIADPXCT(,@15)                                   0587
*REPEAT2:                                                          0588
*         R1=R0-1;                  /* DECREMENT DUPLEX COUNT      0588
*                                                            @ZA16409*/
REPEAT2  LR    R1,R0                                               0588
         BCTR  R1,0                                                0588
*         CS(R0,R1,AIADPXCT);       /* UPDATE DUPLEX COUNT IN AIA  0589
*                                                            @ZA16409*/
         L     @15,ATACPCCW(,ATAPTR)                               0589
         L     @15,PCCWAIA(,@15)                                   0589
         CS    R0,@01,AIADPXCT(@15)                                0589
*         BC(NE,REPEAT2);           /* UNSUCCESSFUL, TRY AGAIN     0590
*                                                            @ZA16409*/
         BC    7,REPEAT2                                           0590
*         IF R1=0 THEN              /* DUPLEX COUNT IS ZERO ?      0591
*                                                            @ZA16409*/
         LTR   R1,R1                                               0591
         BNZ   @RF00591                                            0591
*           DO;                     /* YES-                  @ZA16409*/
*             AIANXAIA=ATACOMPQ;    /* CHAIN IN EXISTING AIA'S     0593
*                                                            @ZA16409*/
         L     @15,ATACPCCW(,ATAPTR)                               0593
         L     @15,PCCWAIA(,@15)                                   0593
         L     @12,ATACOMPQ(,ATAPTR)                               0593
         ST    @12,AIANXAIA(,@15)                                  0593
*             ATACOMPQ=PCCWAIA;     /* PUT THIS AIA ON TOP OF      0594
*                                      INTERNAL POST QUEUE   @ZA16409*/
         ST    @15,ATACOMPQ(,ATAPTR)                               0594
*           END;                    /* END - DUPLEX COUNT IS ZERO  0595
*                                                            @ZA16409*/
*         RFY                                                      0596
*          (R0,                                                    0596
*           R1) UNRSTD;             /*                       @ZA16409*/
@RF00591 DS    0H                                                  0597
*       END;                        /* END DUPLEX PROCESSING @ZA16409*/
*     ELSE                          /* NO,                   @ZA16409*/
*       DO;                         /* NOT DUPLEXED OPERATION      0598
*                                                            @ZA16409*/
         B     @RC00584                                            0598
@RF00584 DS    0H                                                  0599
*         AIANXAIA=ATACOMPQ;        /* CHAIN IN EXISTING AIA'S     0599
*                                                            @ZA16409*/
         L     @15,ATACPCCW(,ATAPTR)                               0599
         L     @15,PCCWAIA(,@15)                                   0599
         L     @12,ATACOMPQ(,ATAPTR)                               0599
         ST    @12,AIANXAIA(,@15)                                  0599
*         ATACOMPQ=PCCWAIA;         /* PUT THIS AIA ON TOP OF      0600
*                                      INTERNAL POST QUEUE   @ZA16409*/
         ST    @15,ATACOMPQ(,ATAPTR)                               0600
*       END;                        /* END-NOT DUPLEXED OPERATION  0601
*                                                            @ZA16409*/
*     RFY                                                          0602
*       AIA BASED(AIAPTR);          /*                       @ZA16409*/
@RC00584 DS    0H                                                  0603
*     PCCWAIA=0;                    /* REMOVE FROM P/SCCW FOR      0603
*                                      RECOVERY                      */
         L     @15,ATACPCCW(,ATAPTR)                               0603
         SLR   @12,@12                                             0603
         ST    @12,PCCWAIA(,@15)                                   0603
*     PCCWPCCW=ATAPCCWQ;            /* CHAIN IN EXISTING P/SCCWS AND */
         L     @12,ATAPCCWQ(,ATAPTR)                               0604
         ST    @12,PCCWPCCW(,@15)                                  0604
*     ATAPCCWQ=ATACPCCW;            /* PUT THIS ONE ON TOP OF      0605
*                                      INTERNAL FREE QUEUE           */
         ST    @15,ATAPCCWQ(,ATAPTR)                               0605
*     ATACPCCW=IORPCCW;             /* GET NEXT P/SCCW ON THIS IORB  */
         L     @15,IORBPTR                                         0606
         L     @15,IORPCCW(,@15)                                   0606
         ST    @15,ATACPCCW(,ATAPTR)                               0606
*     NUMREQ=NUMREQ+ONE;            /* INCR NUMBER REQUESTS FOR    0607
*                                      COMPBRST                      */
         LA    @15,1                                               0607
         AL    @15,NUMREQ                                          0607
         ST    @15,NUMREQ                                          0607
*   END;                                                           0608
@DE00581 L     @15,IORBPTR                                         0608
         CLC   ATACPCCW(4,ATAPTR),IORERR(@15)                      0608
         BNE   @DL00581                                            0608
*   RFY                                                            0609
*     PCCW;                                                        0609
*   RFY                                                            0610
*     R14 UNRSTD;                                                  0610
*   END PROCCCWS;                                                  0611
@EL00011 DS    0H                                                  0611
@EF00011 DS    0H                                                  0611
@ER00011 BR    @14                                                 0611
*                                                                  0612
*/********************************************************************/
*/*                                                                  */
*/*   POSTCMP _ RETURN AIA'S TO PAGE COMPLETION AND FREE PCCWS       */
*/*                                                                  */
*/*   INPUT - INTERNAL FREE QUE OF P/SCCWS - ATAPCCWQ                */
*/*           INTERNAL POST QUE OF AIAS    - ATACOMPQ                */
*/*                                                                  */
*/*   PROCESS - THE CHAIN OF P/SCCWS IS PLACED (C+S) ON TOP OF       */
*/*       ITS RESPECTIVE AVAILABLE QUEUE (ASMPCCWQ OR SARSCCWQ).     */
*/*       THE CHAIN OF AIAS IS PASSED TO PAGE COMPLETION.            */
*/*                                                                  */
*/*   OUTPUT - NONE.                                                 */
*/*                                                                  */
*/********************************************************************/
*/*POSTCMP: CHART (AREF)                                             */
*/*HEADER                                                          0612
*/*                               I/O COMPLETE                     0612
*/*POSTCMP                                                           */
*/*POSTCMP: E POST AIAS + FREE P/SCCWS                               */
*/* D (YES,,NO,%F2) ANY P/SCCWS TO RETURN? ATAPCCWQ                  */
*/* N LOOP THRU ALL P/SCCWS ON INTERNAL FREECCWQ AND GET           0612
*/*   PTR TO LAST                                                    */
*/* D (PAG,,SWP,%S1) PCCWS OR SCCWS? IORSWAP                         */
*/* P ZERO INTERNAL PTR ATAPCCWQ FOR RCV                             */
*/* P SET CHAIN FIELD OF LAST PCCW TO 1ST ON ASMPCCWQ                */
*/* P (,%F1) RETURN PCCWS TO ASMPCCWQ (C+S)                          */
*/*%S1: P ZERO INTERNAL PTR ATAPCCWQ FOR RCV                         */
*/* P SET CHAIN FIELD OF LAST SCCW TO 1ST ON SARSCCWQ                */
*/* P RETURN SCCWS TO SARSCCWQ (C+S)                                 */
*/*%F1: P SET R13 TO SAVEAREA ADDRESS                                */
*/* P SET R1 TO ADDRESS OF 1ST AIA ON ATACOMPQ                       */
*/* P ZERO INTERNAL PTR ATACOMPQ FOR RCV                             */
*/* S ILRPAGCM: POST BACK AIAS                                       */
*/*%F2: R RETURN                                                     */
*/*POSTCMP: END                                                      */
*                                                                  0612
*POSTCMP:                                                          0612
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0612
POSTCMP  DS    0H                                                  0613
*   RFY                                                            0613
*     R14 RSTD;                                                    0613
*   RETADDR1=R14;                   /* SAVE RETURN ADDRESS           */
         ST    R14,RETADDR1                                        0614
*   RFY                                                            0615
*     R14 UNRSTD;                                                  0615
*   IF ATAPCCWQ^=0 THEN             /* ANY P/SCCWS TO RETURN?        */
         L     @12,ATAPCCWQ(,ATAPTR)                               0616
         LTR   @12,@12                                             0616
         BZ    @RF00616                                            0616
*     DO;                           /* YES- FREE P/SCCWS AND POST  0617
*                                      BACK ASSOC AIAS               */
*       PCCWPTR=ATAPCCWQ;                                          0618
         ST    @12,PCCWPTR                                         0618
*       DO WHILE PCCWPCCW^=0;       /* FIND LAST P/SCCW ON INTERNAL
*                                      FREE QUEUE                    */
         B     @DE00619                                            0619
@DL00619 DS    0H                                                  0620
*         PCCWPTR=PCCWPCCW;         /* GET NEXT P/SCCW               */
         L     @12,PCCWPTR                                         0620
         L     @12,PCCWPCCW(,@12)                                  0620
         ST    @12,PCCWPTR                                         0620
*       END;                        /* PCCWPTR NOW PTS TO LAST P/SCCW*/
*                                                                  0621
@DE00619 L     @12,PCCWPTR                                         0621
         L     @12,PCCWPCCW(,@12)                                  0621
         LTR   @12,@12                                             0621
         BNZ   @DL00619                                            0621
*       /*************************************************************/
*       /*                                                           */
*       /* DETERMINE WHETHER PCCWS OR SCCWS TO RETURN TO APPROPRIATE */
*       /* AVAILABLE QUEUE.                                          */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0622
*       RFY                                                        0622
*         R2 RSTD;                                                 0622
*       IF IORSWAP=ON THEN          /* PROCESSING SCCWS?             */
         L     @12,IORBPTR                                         0623
         TM    IORSWAP(@12),B'00001000'                            0623
         BNO   @RF00623                                            0623
*         R2=ADDR(SARSCCWQ);        /* SETUP TO RETURN TO SART       */
         L     R2,SARTPTR                                          0624
         LA    R2,SARSCCWQ(,R2)                                    0624
*       ELSE                        /* NO- PAGING                    */
*         R2=ADDR(ASMPCCWQ);        /* SETUP TO RETURN TO ASMVT      */
         B     @RC00623                                            0625
@RF00623 LA    R2,ASMPCCWQ(,ASMVTPTR)                              0625
*       RFY                                                        0626
*        (R0,                                                      0626
*         R1) RSTD;                                                0626
@RC00623 DS    0H                                                  0627
*       R1=ATAPCCWQ;                /* PICK UP CHAIN OF P/SCCWS      */
         L     R1,ATAPCCWQ(,ATAPTR)                                0627
*       ATAPCCWQ=0;                 /* IMMEDIATELY ZERO FOR RECOVERY */
         SLR   @12,@12                                             0628
         ST    @12,ATAPCCWQ(,ATAPTR)                               0628
*       R0=R2->PORSCCWQ;            /* GET HEAD OF P/SCCW AVAILABLE
*                                      QUEUE                         */
         L     R0,PORSCCWQ(,R2)                                    0629
*REPLPSCW:                                                         0630
*       PCCWPCCW=R0;                /* CHAIN IN EXISTING P/SCCWS     */
REPLPSCW L     @12,PCCWPTR                                         0630
         ST    R0,PCCWPCCW(,@12)                                   0630
*       CS(R0,R1,R2->PORSCCWQ);     /* UPDATE AVAILABLE QUEUE        */
         CS    R0,@01,PORSCCWQ(R2)                                 0631
*       BC(NE,REPLPSCW);                                           0632
         BC    7,REPLPSCW                                          0632
*       RFY                                                        0633
*        (R0,                                                      0633
*         R2) UNRSTD;                                              0633
*                                                                  0633
*       /*************************************************************/
*       /*                                                           */
*       /* POST BACK AIAS TO ILRPAGCM                                */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0634
*       R13=ADDR(SAVEREGS);         /* SET UP SAVE AREA FOR PAGE   0634
*                                      COMPLETION                    */
         LA    R13,SAVEREGS                                        0634
*       R1=ATACOMPQ;                /* SET PARM REG TO PAGE        0635
*                                      COMPLETION                    */
         L     R1,ATACOMPQ(,ATAPTR)                                0635
*       ATACOMPQ=0;                 /* ZERO FOR RECOVERY             */
         SLR   @12,@12                                             0636
         ST    @12,ATACOMPQ(,ATAPTR)                               0636
*       CALL ILRPAGCM;              /* POST BACK AIAS                */
         L     @15,@CV02487                                        0637
         BALR  @14,@15                                             0637
*       RFY                                                        0638
*         R1 UNRSTD;                                               0638
*     END;                          /* END FREE P/SCCWS AND POST BACK
*                                      AIAS                          */
*   ELSE                                                           0640
*     ;                             /* NO P/SCCWS TO RETURN          */
@RF00616 DS    0H                                                  0641
*   RFY                                                            0641
*     R14 RSTD;                                                    0641
*   R14=RETADDR1;                   /* RESTORE RETURN ADDRESS        */
         L     R14,RETADDR1                                        0642
*   RFY                                                            0643
*     R14 UNRSTD;                                                  0643
*   END POSTCMP;                                                   0644
@EL00012 DS    0H                                                  0644
@EF00012 DS    0H                                                  0644
@ER00012 BR    @14                                                 0644
*                                                                  0645
*/********************************************************************/
*/*                                                                  */
*/* COMPBRST: COMPUTE SERVICE TIME TO BE USED IN COMPUTING THE       */
*/*           LENGTH OF A SERVICE BURST.                             */
*/*                                                                  */
*/* INPUT: THE TIME OF DAY AT ENTRY TO IOS WHICH INITIATED THE I/O   */
*/*        REQUEST, AND THE OLD SERVICE TIME FOR ONE PAGE REQUEST IN */
*/*        PART ENTRY                                                */
*/*                                                                  */
*/* PROCESS: THE CURRENT TIME OF DAY IS OBTAINED AND SUBTRACTED FROM */
*/*          THE TIME OF DAY WHEN IOS WAS CALLED.  THIS VALUE IS     */
*/*          DIVIDED BY THE NUMBER OF I/O REQUESTS JUST COMPLETED,   */
*/*          YIELDING THE SERVICE TIME FOR ONE PAGE REQUEST. THIS    */
*/*          VALUE IS AVERAGED INTO THE OLD VALUE (PARERQTM) AND     */
*/*          THE NEW VALUE IS SAVED IN THE PART ENTRY.               */
*/*          (IF AN I/O ERROR HAS OCCURRED, A NEW SERVICE TIME WILL  */
*/*          NOT BE COMPUTED- THIS SUBROUTINE WILL NOT BE CALLED.)   */
*/*                                                                  */
*/* OUTPUT: A NEW SERVICE TIME IS STORED IN THE PART ENTRY.          */
*/*                                                                  */
*/********************************************************************/
*/*COMPBRST: CHART (AREF)                                            */
*/* HEADING                                                        0645
*/*                               I/O COMPLETE -                   0645
*/*COMPBRST                                                          */
*/*COMPBRST: E COMPUTE I/O SERVICE TIME                              */
*/* N TIME WILL BE 0 IF COMPLETION FOR SWAP                          */
*/* D (YES,,NO,%CEX) TIME SAVED BY SLOT SORT?                        */
*/* D (YES,,NO,%ZERO) NUMBER OF REQUESTS NONZERO ?                   */
*/* P COMPUTE TIME IT TOOK TO PROCESS 1 REQUEST                      */
*/* P COMPUTE MOVING AVERAGE OF SERVICE TIME                         */
*/* D (YES,,NO,%CEX) IS NEW VALUE NON-ZERO?                          */
*/* P (,%CEX) STORE NEW AVERAGE IN PART ENTRY (PARERQTM)             */
*/*%ZERO: P ZERO SLOT SORT TIME                                      */
*/*%CEX: R RETURN                                                    */
*/*COMPBRST: END                                                     */
*                                                                  0645
*COMPBRST:                                                         0645
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0645
COMPBRST DS    0H                                                  0646
*   DCL                                                            0646
*     NOCARRY LABEL GENERATED;      /*                               */
*   RFY                                                            0647
*     R14 RSTD;                     /*                               */
*   RTNADDR=R14;                    /* SAVE RETURN ADDRESS           */
         ST    R14,RTNADDR                                         0648
*   RFY                                                            0649
*     R14 UNRSTD;                                                  0649
*   IF IORTSMP^=DZERO THEN          /* WAS TIME STORED PRIOR TO SRT
*                                      CALLING IOS AND NOT         0650
*                                      SUBSEQUENTLY ZEROED BECAUSE 0650
*                                      THIS IS SWAP OR STCK IN IOD 0650
*                                      FAILED...                     */
         L     @12,IORBPTR                                         0650
         CLC   IORTSMP(8,@12),@CB02414                             0650
         BE    @RF00650                                            0650
*     DO;                           /* YES, COMPUTE DIFFERENCE       */
*       IF NUMREQ^=CODE00 THEN                                     0652
         L     @08,NUMREQ                                          0652
         LTR   @08,@08                                             0652
         BZ    @RF00652                                            0652
*         DO;                                                      0653
*                                                                  0653
*           /*********************************************************/
*           /*                                                       */
*           /* COMPUTE THE SERVICE TIME (PARERQTM) USED IN COMPUTING */
*           /* THE THE NUMBER OF REQUESTS THAT CAN BE SERVICED IN ONE*/
*           /* SERVICE BURST. THE TIME IT ACTUALLY TOOK TO COMPLETE  */
*           /* THE I/O IS COMPUTED (IOCTOD - SRTTOD) AND DIVIDED BY  */
*           /* THE NUMBER OF REQUESTS PROCESSED. A MOVING AVERAGE OF */
*           /* THIS SERVICE TIME FOR ONE REQUEST IS THEN COMPUTED    */
*           /* USING 3/4 OF THE OLD PARERQTM AND 1/4 OF THE NEW      */
*           /* VALUE.                                                */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0654
*           PCTPTR=PAREPCTP;        /* SET UP ADDR OF PCT            */
         L     @08,PAREPTR                                         0654
         L     PCTPTR,PAREPCTP(,@08)                               0654
*           RFY                                                    0655
*            (R0,                                                  0655
*             R1,                                                  0655
*             R14,                                                 0655
*             R15) RSTD;                                           0655
*           LM(R14,R15,IOCTOD);     /* GET TIME AT ENTRY TO IOC      */
         LM    R14,R15,IOCTOD                                      0656
*           LM(R0,R1,IORTSMP);      /* GET TIME AT ENTRY TO IOS      */
         LM    R0,R1,IORTSMP(@12)                                  0657
*           GEN REFS(R0,R1,R14,R15) CODE DEFS(NOCARRY);            0658
            SLR R15,R1                  SUBTRACT LOW ORDER PORTION
            BC  3,NOCARRY               SUBTRACT 1 FROM HIGH REGISTER
            BCTR R14,0                   IF A CARRY
NOCARRY     SLR R14,R0                  SUBTRACT HIGH ORDER PORTION
            SRDA R14,12                 SHIFT OUT LOW 12 BITS
*           BC(4,ENDTIME);          /* IF NEGATIVE NUMBER - IGNORE   */
         BC    4,ENDTIME                                           0659
*           RFY                                                    0660
*            (R0,                                                  0660
*             R1) UNRSTD;                                          0660
*           IF R15>CODE00 THEN      /* Q. A POSITIVE NUMBER?         */
         LTR   R15,R15                                             0661
         BNP   @RF00661                                            0661
*SUBTR:                                                            0662
*             R15=R15/NUMREQ;       /* COMPUTE SERVICE TIME          */
SUBTR    LR    @00,R15                                             0662
         SRDA  @00,32                                              0662
         D     @00,NUMREQ                                          0662
         LR    R15,@01                                             0662
*           R14=PARERQTM;           /* GET CURRENT VALUE             */
@RF00661 L     @12,PAREPTR                                         0663
         L     R14,PARERQTM(,@12)                                  0663
*           GEN(SLA R14,1);         /* MULTIPLY BY TWO               */
         SLA R14,1
*           R14=R14+R15+PCTRQTIM;   /* ADD IN EXPECTED VALUE FROM PCT*/
         LR    @12,R14                                             0665
         ALR   @12,R15                                             0665
