         L     @12,IORBPTR                                         0393
         L     @12,IORPCCW(,@12)                                   0393
         LTR   @12,@12                                             0393
         BZ    @RF00393                                            0393
*         DO;                       /* YES-                          */
*           CALL RECHAIN;           /* RESET IOSB/IORB FIELDS TO   0395
*                                      REFLECT NEW STARTING POINT    */
         BAL   @14,RECHAIN                                         0395
*           RETVEC=REDRIVE;         /* INDICATE INVOKE CHAN SCHEDULER
*                                      TO REDRIVE THIS IOSB/SRB      */
         LA    RETVEC,8                                            0396
*         END;                      /* END WORK TO BE REDRIVEN       */
*       ELSE                        /* NOTHING TO REDRIVE            */
*         RETVEC=PURSUE;            /* INDICATE FINISHED + INVOKE  0398
