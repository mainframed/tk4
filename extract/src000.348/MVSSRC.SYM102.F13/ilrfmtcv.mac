         TITLE 'ILRFMTCV    COMMON, HEADER, AND VIO CONTROL-BLOCK FORMA*
               ATTING   '
ILRFMTCV CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL2(@EP00001-@MAINENT)
         DC    AL1(16)                                             0001
         DC    C'ILRFMTCV  76.169'                                 0001
ILRFMTC  DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL2(@EP00059-ILRFMTC)
         ENTRY ILRFMTC
ILRFMTH  DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL2(@EP00278-ILRFMTH)
         ENTRY ILRFMTH
ILRFMTV  DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL2(@EP00847-ILRFMTV)
         ENTRY ILRFMTV
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @05,0                                               0001
@PSTART  LA    @06,4095(,@05)                                      0001
         LA    @07,4095(,@06)                                      0001
         USING @PSTART,@05                                         0001
         USING @PSTART+4095,@06                                    0001
         USING @PSTART+8190,@07                                    0001
         ST    @13,@SA00001+4                                      0001
