         BR    @15                                                 0001
@EP00001 DS    0H                                                  0002
*                                                                  0059
*/********************************************************************/
*/*ILRFMTC:  CHART (SEQ)                                             */
*/*ILRFMTC:  E COMMON SEGMENT                                        */
*/*          P SAVE CALLERS REGS, ESTABLISH ADDRESSIBILITY           */
*/*          P OBTAIN ABDUMP PARM LIST PTR (R1)                      */
*/*          P OBTAIN CVT PTR FORM PARM LIST                         */
*/*          L ACCESS: PSASTOR MASTER SGT                            */
*/*          D (YES,,NO,%ERROR1) POINTER GOOD ?                      */
*/*          L ACCESS:      CVTPVTP OBTAIN PVT PTR                   */
*/*          D (YES,,NO,%ERROR2) POINTER GOOD ?                      */
*/*          L ACCESS:      PVTLCSA VBN OF FIRST COMMON PAGE         */
*/*          D (YES,,NO,%ERROR2) RETURN CODE GOOD ?                  */
*/*          P STARTVBN = PVTLCSA                                    */
*/*          L ACCESS:      PVTLQSA VBN FIRST QUICK START            */
*/*          D (YES,,NO,%ERROR2) RETURN CODE GOOD ?                  */
*/*          P CALCULATE NUMBER SEGMENTS, (PVTLQSA - PVTLCSA) / 64   */
*/*          S SEGFIND: CALL RTN TO FIND SGTES                       */
*/*          P MAINTAIN RETURN CODE TO PASS TO CALLER                */
*/*%RETURN:  P RESTORE CALLERS REGS                                  */
*/*          R RETURN                                                */
*/*%ERROR1:  S (,%RETURN) ERRMSG: R1=10 MASTER SGT INACCESSIBLE      */
*/*%ERROR2:  S (,%RETURN) ERRMSG: R1=6 PRINTS PVT INACCESSIBLE       */
*/*ILRFMTC:  END                                                     */
*/********************************************************************/
*                                                                  0059
*                                                                  0059
*/********************************************************************/
*/*                                                                  */
*/* COMMON:  SEGMENT TO CAUSE FORMATTING OF PAGE AND EXTERNAL        */
*/*           PAGE TABLES FOR THE COMMON SERVICE AREA                */
*/*                                                                  */
*/* INPUT:   R1=ABDPLPTR (ABDUMP PARM LIST)                          */
*/*                                                                  */
*/* PROCESS: (1) GET ADDRESS OF MASTER SEGMENT TABLE FROM PSA        */
*/*          (2) GET VBN OF FIRST CSA SEGMENT FROM PVT               */
*/*          (3) CALCULATE NUMBER OF CSA SEGMENTS                    */
*/*          (4) PASS THIS INFORMATION TO SEGFIND SEGMENT            */
*/*                                                                  */
*/* OUTPUT:  THE RETURN CODE IN R15 (0 OR 4)                         */
*/*                                                                  */
*/********************************************************************/
*                                                                  0059
*ILRFMTC:                                                          0059
*   ENTRY;                          /* FORMAT COMMMON ENTRY          */
@EP00059 DS    0H                                                  0060
*   ABDPLPTR=R1;                    /* GET ADDR OF PARMLIST          */
         LR    ABDPLPTR,R1                                         0060
*   CVTPTR=ADPLCVT;                 /* GET CVT PTR FROM PARM LIST    */
         MVC   CVTPTR(4),ADPLCVT(ABDPLPTR)                         0061
*   DO;                             /* ILRMACCS (SEGPTR)           0062
*                                      FROM(PSASTOR)                 */
*     RFY                                                          0063
*      (R0,                                                        0063
*       R1,                                                        0063
*       R14,                                                       0063
*       R15) RSTD;                                                 0063
*     R0=ADDR(PSASTOR);                                            0064
         LA    R0,PSASTOR                                          0064
*     R1=ABDPLPTR;                  /* POINT TO PARM LIST            */
         LR    R1,ABDPLPTR                                         0065
*     CALL ACCESS;                  /* ACCESS DATA                   */
         L     @15,ADPLMEMA(,ABDPLPTR)                             0066
         BALR  @14,@15                                             0066
*     R14=R0;                       /* CANNOT USE R0 AS BASE         */
         LR    R14,R0                                              0067
*     IF R15=GOOD THEN              /* CHECK ACCESS                  */
         LTR   R15,R15                                             0068
         BNZ   @RF00068                                            0068
*       SEGPTR=CURRENT;             /* GET REQUESTED FIELD           */
         MVC   SEGPTR(4),CURRENT(R14)                              0069
*     ELSE                          /* ACCESS NO GOOD                */
*       SEGPTR=ZERO;                /* ZERO FIELD                    */
         B     @RC00068                                            0070
@RF00068 SLR   @11,@11                                             0070
         ST    @11,SEGPTR                                          0070
*     RFY                                                          0071
*      (R0,                                                        0071
*       R1,                                                        0071
*       R14,                                                       0071
*       R15) UNRSTD;                                               0071
@RC00068 DS    0H                                                  0072
*   END;                                                           0072
*   IF SEGPTR^=ZERO THEN                                           0073
         ICM   @11,15,SEGPTR                                       0073
         BZ    @RF00073                                            0073
*     DO;                           /* ACCESS WAS GOOD               */
*       DO;                         /* ILRMACCS (PVTPTR)           0075
*                                      FROM(CVTPVTP)                 */
*         RFY                                                      0076
*          (R0,                                                    0076
*           R1,                                                    0076
*           R14,                                                   0076
*           R15) RSTD;                                             0076
*         R0=ADDR(CVTPVTP);                                        0077
         L     @11,CVTPTR                                          0077
         LA    R0,CVTPVTP(,@11)                                    0077
*         R1=ABDPLPTR;              /* POINT TO PARM LIST            */
         LR    R1,ABDPLPTR                                         0078
*         CALL ACCESS;              /* ACCESS DATA                   */
         L     @15,ADPLMEMA(,ABDPLPTR)                             0079
         BALR  @14,@15                                             0079
*         R14=R0;                   /* CANNOT USE R0 AS BASE         */
         LR    R14,R0                                              0080
*         IF R15=GOOD THEN          /* CHECK ACCESS                  */
         LTR   R15,R15                                             0081
         BNZ   @RF00081                                            0081
*           PVTPTR=CURRENT;         /* GET REQUESTED FIELD           */
         MVC   PVTPTR(4),CURRENT(R14)                              0082
*         ELSE                      /* ACCESS NO GOOD                */
*           PVTPTR=ZERO;            /* ZERO FIELD                    */
         B     @RC00081                                            0083
@RF00081 SLR   @11,@11                                             0083
         ST    @11,PVTPTR                                          0083
*         RFY                                                      0084
*          (R0,                                                    0084
*           R1,                                                    0084
*           R14,                                                   0084
*           R15) UNRSTD;                                           0084
@RC00081 DS    0H                                                  0085
*       END;                                                       0085
*       IF PVTPTR^=ZERO THEN                                       0086
         L     @11,PVTPTR                                          0086
         LTR   @11,@11                                             0086
         BZ    @RF00086                                            0086
*         DO;                       /* ACCESS WAS GOOD               */
*           R0=ADDR(PVTLCSA);                                      0088
         LA    R0,PVTLCSA(,@11)                                    0088
*           R1=ABDPLPTR;            /* POINT TO PARM LIST            */
         LR    R1,ABDPLPTR                                         0089
*           CALL ACCESS;            /* ACCESS DATA                   */
         L     @15,ADPLMEMA(,ABDPLPTR)                             0090
         BALR  @14,@15                                             0090
*           R14=R0;                 /* CANNOT USE R0 AS BASE         */
         LR    R14,R0                                              0091
*           IF R15=ZERO THEN                                       0092
         LTR   R15,R15                                             0092
         BNZ   @RF00092                                            0092
*             DO;                   /* ACCESS WAS GOOD               */
*               STARTVBN=CURRHALF;  /* GET REQUESTED FIELD HALFWORD  */
         SLR   @11,@11                                             0094
         ICM   @11,3,CURRHALF(R14)                                 0094
         ST    @11,STARTVBN                                        0094
*               R0=ADDR(PVTLQSA);                                  0095
         L     @11,PVTPTR                                          0095
         LA    R0,PVTLQSA(,@11)                                    0095
*               R1=ABDPLPTR;        /* POINT TO PARM LIST            */
         LR    R1,ABDPLPTR                                         0096
*               CALL ACCESS;        /* ACCESS DATA                   */
         L     @15,ADPLMEMA(,ABDPLPTR)                             0097
         BALR  @14,@15                                             0097
*               R14=R0;             /* CANNOT USE R0 AS BASE         */
         LR    R14,R0                                              0098
*               IF R15=ZERO THEN                                   0099
         LTR   R15,R15                                             0099
         BNZ   @RF00099                                            0099
*                 DO;               /* ACCESS WAS GOOD               */
*                   HIVBN=CURRHALF; /* GET REQUESTED FIELD HALFWORD  */
         SLR   @11,@11                                             0101
         ICM   @11,3,CURRHALF(R14)                                 0101
         ST    @11,HIVBN                                           0101
*                   NUMSEGS=(HIVBN-STARTVBN)/JUSTIFY;/* CLACULATE NO.
*                                      OF SEGMENTS                   */
         LR    @08,@11                                             0102
         SL    @08,STARTVBN                                        0102
         SRDA  @08,32                                              0102
         D     @08,@CF00389                                        0102
         ST    @09,NUMSEGS                                         0102
*                   R2=SEGPTR;      /* REAL ADDRESS FOR TRANSLATION  */
         L     R2,SEGPTR                                           0103
*                   CALL TRANSLAT;  /* TRANSLATE REAL TO VIRTUAL     */
         BAL   @14,TRANSLAT                                        0104
*                   IF R15=ZERO&R3^=ZERO THEN                      0105
         SLR   @11,@11                                             0105
         CR    R15,@11                                             0105
         BNE   @RF00105                                            0105
         CR    R3,@11                                              0105
         BE    @RF00105                                            0105
*                     DO;           /* TRANSLATION SUCCESSFUL        */
*                       SEGPTR=R3;  /* VIRTUAL SEGMENT TABLE ADDRESS */
         ST    R3,SEGPTR                                           0107
*                       CUMRC=ZERO; /* CLEAR CUMULATIVE RETURN CODE  */
         ST    @11,CUMRC                                           0108
*                       BUFFER(1:49)=                              0109
*'SYSTEM COMMON AREA PAGE AND EXTERNAL PAGE TABLES';               0109
         L     @11,ADPLBUF(,ABDPLPTR)                              0109
         MVI   BUFFER+48(@11),C' '                                 0109
         MVC   BUFFER(48,@11),@CC01927                             0109
*                       R1=ABDPLPTR;/* PTR TO INPUT PARM LIST        */
         LR    R1,ABDPLPTR                                         0110
*                       CALL PRINT; /* PRINT ONE LINE                */
         L     @15,ADPLPRNT(,ABDPLPTR)                             0111
         BALR  @14,@15                                             0111
*                       CALL PRINT; /* SPACE ONE LINE                */
         L     @15,ADPLPRNT(,ABDPLPTR)                             0112
         BALR  @14,@15                                             0112
*                       CALL SEGFIND;/* OBTAINS ADDRESS EACH SEGMENT
*                                      ENTRY                         */
         BAL   @14,SEGFIND                                         0113
*                       R15=CUMRC;  /* GET RETURN CODE               */
         L     R15,CUMRC                                           0114
*                     END;          /* TRANSLATION SUCCESSFUL        */
*                   ELSE                                           0116
*                     DO;           /* TRANSLATION FAILED            */
         B     @RC00105                                            0116
@RF00105 DS    0H                                                  0117
*                       R1=SGTERR;  /* MASTER SGT ERROR MESSAGE      */
         LA    R1,10                                               0117
*                       CALL ILRFMSG;/* PRINT MESSAGE                */
         L     @15,@CV01769                                        0118
         BALR  @14,@15                                             0118
*                       R15=ERROR;  /* SET ERROR RETURN CODE         */
         LA    R15,4                                               0119
*                     END;                                         0120
*                 END;                                             0121
*               ELSE                                               0122
*                 DO;               /* ACCESS FAILED                 */
         B     @RC00099                                            0122
@RF00099 DS    0H                                                  0123
*                   R1=PVTERR;      /* PVT ERROR MESSAGE             */
         LA    R1,6                                                0123
*                   CALL ILRFMSG;   /* PRINT MESSAGE                 */
         L     @15,@CV01769                                        0124
         BALR  @14,@15                                             0124
*                   R15=ERROR;      /* SET ERROR RETURN CODE         */
         LA    R15,4                                               0125
*                 END;                                             0126
*             END;                                                 0127
*           ELSE                                                   0128
*             DO;                   /* ACCESS FAILED                 */
         B     @RC00092                                            0128
@RF00092 DS    0H                                                  0129
*               R1=PVTERR;          /* PVT ERROR MESSAGE             */
         LA    R1,6                                                0129
*               CALL ILRFMSG;       /* PRINT MESSAGE                 */
         L     @15,@CV01769                                        0130
         BALR  @14,@15                                             0130
*               R15=ERROR;          /* SET ERROR RETURN CODE         */
         LA    R15,4                                               0131
*             END;                                                 0132
*         END;                                                     0133
*       ELSE                                                       0134
*         DO;                       /* ACCESS FAILED                 */
         B     @RC00086                                            0134
@RF00086 DS    0H                                                  0135
*           R1=PVTERR;              /* PVT ERROR MESSAGE             */
         LA    R1,6                                                0135
*           CALL ILRFMSG;           /* PRINT MESSAGE                 */
         L     @15,@CV01769                                        0136
         BALR  @14,@15                                             0136
*           R15=ERROR;              /* SET ERROR RETURN CODE         */
         LA    R15,4                                               0137
*         END;                                                     0138
*     END;                                                         0139
*   ELSE                                                           0140
*     DO;                           /* ACCESS FAILED                 */
         B     @RC00073                                            0140
@RF00073 DS    0H                                                  0141
*       R1=SGTERR;                  /* MASTER SGT ERROR MESSAGE      */
         LA    R1,10                                               0141
*       CALL ILRFMSG;               /* PRINT MESSAGE                 */
         L     @15,@CV01769                                        0142
         BALR  @14,@15                                             0142
*       R15=ERROR;                  /* SET ERROR RETURN CODE         */
         LA    R15,4                                               0143
*     END;                                                         0144
*FMTCEND:                                                          0145
*   RETURN CODE(R15);               /* RETURNS TO FMT00, ENDS COMMON */
@RC00073 DS    0H                                                  0145
FMTCEND  L     @13,4(,@13)                                         0145
         L     @14,12(,@13)                                        0145
         LM    @00,@12,20(@13)                                     0145
         BR    @14                                                 0145
*                                                                  0146
*/********************************************************************/
*/*SEGFIND:  CHART (SEQ)                                             */
*/*          E SEGFIND                                               */
*/*          P SAVE RETURN ADDRESS                                   */
*/*          P CLEAR CUMULATIVE RETURN CODE                          */
*/*          P SET COUNTER = STARTVBN                                */
*/*%SEGS:    D (NO,,YES,%RETURN) ALL SGTES DONE? (I = NUMSEGS)       */
*/*          P INCREMENT COUNTER                                     */
*/*          L ACCESS: GET SEGMENT ENTRY VIA COUNTER AND PSASTOR     */
*/*          D (YES,,NO,%ERRET) ACCESS GOOD ?                        */
*/*          D (YES,,NO,%INCR) SEGMENT VALID ? (SGTPAM)              */
*/*          P R2 = SGTPTO, REAL ADDR PAGE TABLE ORIGIN              */
*/*          S TRANSLAT: CALL SEGMENT TO TRANSLATE REAL TO VIRTUAL   */
*/*          D (YES,,NO,%ERRET) RETURN CODE GOOD ?                   */
*/*          P PGTPTR = R3 VIRTUAL ADDRESS OF PGT                    */
*/*          S FMTPGXPT: CALL RTN TO FORMAT PGT/XPT'S                */
*/*%ERRET:   P OR IN RETURN CODE                                     */
*/*%INCR:    P (,%SEGS) INCR I                                       */
*/*%RETURN:  P RESTORE RETURN ADDRESS                                */
*/*          R RETURN                                                */
*/*SEGFIND:  END                                                     */
*/********************************************************************/
*                                                                  0146
*                                                                  0146
*/********************************************************************/
*/*                                                                  */
*/* SEGFIND: LOCATES SEGMENTS                                        */
*/*                                                                  */
*/* INPUT:   STARTVBN = VIRTUAL BLOCK NUMBER OF FIRST SEGMENT        */
*/*                      TO BE LOCATED                               */
*/*          NUMSEGS  = THE NUMBER OF SEGMENTS TO BE LOCATED         */
*/*          SEGPTR   = ADDRESS OF THE SEGMENT TABLE                 */
*/*                                                                  */
*/* PROCESS: GET SEGMENT, CHECK ITS VALIDITY, IF VALID, CALL         */
*/*           SUBROUTINE TO FORMAT THE PAGE TABLES FOR THAT SEGMENT  */
*/*           FOLLOW THIS PROCEDURE UNTIL ALL SEGS REQUESTED HAVE    */
*/*           PROCESSED.                                             */
*/*                                                                  */
*/* OUTPUT:  THE CUMULATIVE RETURN CODE                              */
*/*                                                                  */
*/********************************************************************/
*                                                                  0146
*SEGFIND:                                                          0146
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0146
SEGFIND  DS    0H                                                  0147
*   RTNADDR1=R14;                   /* SAVE RETURN ADDRESS           */
         ST    R14,RTNADDR1                                        0147
*   CTR=STARTVBN/JUSTIFY;           /* SET CTR TO USE AS INDEX       */
         L     @08,STARTVBN                                        0148
         SRDA  @08,32                                              0148
         D     @08,@CF00389                                        0148
         ST    @09,CTR                                             0148
*   STEPTR=SEGPTR+(CTR*LENGTH(SGTSTE));/* OBTAIN ADDR FIRST ENTRY    */
         SLA   @09,2                                               0149
         AL    @09,SEGPTR                                          0149
         ST    @09,STEPTR                                          0149
*   DO I=1 TO NUMSEGS;              /* DO UNTIL ALL CSA SEGMENTS ARE
*                                      PROCESSED                     */
         LA    @11,1                                               0150
         B     @DE00150                                            0150
@DL00150 DS    0H                                                  0151
*     DO;                           /* ILRMACCS (SENTRY) FROM(SGTSTE)*/
*       RFY                                                        0152
*        (R0,                                                      0152
*         R1,                                                      0152
*         R14,                                                     0152
*         R15) RSTD;                                               0152
*       R0=ADDR(SGTSTE);                                           0153
         L     R0,STEPTR                                           0153
*       R1=ABDPLPTR;                /* POINT TO PARM LIST            */
         LR    R1,ABDPLPTR                                         0154
*       CALL ACCESS;                /* ACCESS DATA                   */
         L     @15,ADPLMEMA(,ABDPLPTR)                             0155
         BALR  @14,@15                                             0155
*       R14=R0;                     /* CANNOT USE R0 AS BASE         */
         LR    R14,R0                                              0156
*       IF R15=GOOD THEN            /* CHECK ACCESS                  */
         LTR   R15,R15                                             0157
         BNZ   @RF00157                                            0157
*         SENTRY=CURRENT;           /* GET REQUESTED FIELD           */
         MVC   SENTRY(4),CURRENT(R14)                              0158
*       ELSE                        /* ACCESS NO GOOD                */
*         SENTRY=ZERO;              /* ZERO FIELD                    */
         B     @RC00157                                            0159
@RF00157 SLR   @11,@11                                             0159
         ST    @11,SENTRY                                          0159
*       RFY                                                        0160
*        (R0,                                                      0160
*         R1,                                                      0160
*         R14,                                                     0160
*         R15) UNRSTD;                                             0160
@RC00157 DS    0H                                                  0161
*     END;                                                         0161
*     IF R15=ZERO THEN              /* ACCESS SUCCEEDED              */
         LTR   R15,R15                                             0162
         BNZ   @RF00162                                            0162
*       DO;                                                        0163
*         RFY                                                      0164
*           SGTSTE BASED(ADDR(SENTRY));                            0164
*         IF SGTPAM=OFF THEN        /* SEGMENT IS VALID              */
         LA    @11,SENTRY                                          0165
         TM    SGTPAM(@11),B'00000001'                             0165
         BNZ   @RF00165                                            0165
*           DO;                                                    0166
*             R2=SGTPTO;            /* REAL ADDRESS FOR TRANSLATION
*                                      PLUS 3 FLAG BITS              */
         SLR   R2,R2                                               0167
         ICM   R2,7,SGTPTO(@11)                                    0167
*             RFY                                                  0168
*               SGTSTE BASED(STEPTR);                              0168
*             SRL(R2,3);            /* TO ZERO THE                   */
         SRL   R2,3                                                0169
*             SLL(R2,3);            /* THREE FLAG BITS               */
         SLL   R2,3                                                0170
*             CALL TRANSLAT;        /* CALL SEGMENT TO TRANSLATE REAL
*                                      TO VIRTUAL ADDR               */
         BAL   @14,TRANSLAT                                        0171
*             IF R15=ZERO&R3^=ZERO THEN                            0172
         SLR   @11,@11                                             0172
         CR    R15,@11                                             0172
         BNE   @RF00172                                            0172
         CR    R3,@11                                              0172
         BE    @RF00172                                            0172
*               DO;                 /* TRANSLATION WAS SUCCESSFUL    */
*                 PGTPTR=R3;        /* GET VIRTUAL ADDRESS           */
         ST    R3,PGTPTR                                           0174
*                 CALL FMTPGXPT;    /* CALL SEGMENT TO FMT PGTES     */
         BAL   @14,FMTPGXPT                                        0175
*               END;                                               0176
*             ELSE                                                 0177
*               CUMRC=CUMRC×ERROR;  /* TRANSLAT FAILED               */
         B     @RC00172                                            0177
@RF00172 L     @11,CUMRC                                           0177
         O     @11,@CF00064                                        0177
         ST    @11,CUMRC                                           0177
*           END;                                                   0178
*       END;                        /* END SEGMENT PROCESSING        */
*     ELSE                                                         0180
*       DO;                         /* ACCESS FAILED                 */
         B     @RC00162                                            0180
@RF00162 DS    0H                                                  0181
*         CUMRC=CUMRC×ERROR;        /* OR IN ERROR                   */
         L     @11,CUMRC                                           0181
         O     @11,@CF00064                                        0181
         ST    @11,CUMRC                                           0181
*         I=NUMSEGS;                /* FORCE END OF LOOP             */
         MVC   I(4),NUMSEGS                                        0182
*       END;                                                       0183
*     STEPTR=STEPTR+LENGTH(SGTSTE); /* ADDR NEXT ENTRY               */
@RC00162 LA    @11,4                                               0184
         AL    @11,STEPTR                                          0184
         ST    @11,STEPTR                                          0184
*     CTR=CTR+ONE;                  /* INCR VBN SS                   */
         LA    @11,1                                               0185
         AL    @11,CTR                                             0185
         ST    @11,CTR                                             0185
*   END;                            /* END 1 TO NUMSEGS              */
         LA    @11,1                                               0186
         AL    @11,I                                               0186
@DE00150 ST    @11,I                                               0186
         C     @11,NUMSEGS                                         0186
         BNH   @DL00150                                            0186
*   R14=RTNADDR1;                   /* RESTORE RETURN ADDRESS        */
         L     R14,RTNADDR1                                        0187
*   END SEGFIND;                    /* RETURN TO CALLER              */
@EL00002 DS    0H                                                  0188
@EF00002 DS    0H                                                  0188
@ER00002 BR    @14                                                 0188
*                                                                  0189
*/********************************************************************/
*/*TRANSLAT:  CHART (SEQ)                                            */
*/*TRANSLAT:  E TRANSLAT                                             */
*/*           P SAVE RETURN ADDRESS                                  */
*/*           L ACCESS:      PVTPFTP  PAGE FRAME TABLE               */
*/*           D (YES,,NO,%ERRET) POINTER GOOD ?                      */
*/*           P R3 = R2 - SAVE REAL ADDR                             */
*/*           P SRL R2,12 - CHOP OFF DISPLACEMENT                    */
*/*           P SLL R2,4 - LEFT JUSTIFY TO HALF WORD (OFFSET IN PFT) */
*/*           P PFTEPTR = PFTPTR + R2                                */
*/*           P ACCESS: PFTVBN FROM PFTEPTR                          */
*/*           D (YES,,NO,%ERRET) ACCESS GOOD ?                       */
*/*           P R2 = PFTVBN                                          */
*/*           P SRL R2,4 - RIGHT JUSTIFY VBN                         */
*/*           P SLL R3,20 - MOVE DISPLACEMENT NEXT TO VBN            */
*/*           P SRDL R2,20 - GET VIRTUAL ADDRESS IN R3               */
*/*%RETURN:   P RESTORE RETURN ADDRESS                               */
*/*           R RETURN                                               */
*/*%ERRET:    P (,%RETURN) MAINTAIN ERROR RETURN CODE TO PASS CALLER */
*/*TRANSLAT:  END                                                    */
*/********************************************************************/
*                                                                  0189
*                                                                  0189
*/********************************************************************/
*/*                                                                  */
*/* TRANSLAT: SEGMENT TO TRANSLATE REAL ADDRESS TO VIRTUAL ADDRESS   */
*/*                                                                  */
*/* INPUT:    REAL ADDRESS IN REGISTER 2, PVTPTR                     */
*/*                                                                  */
*/* PROCESS:  ISOLATE SSP PASSED FROM CALLER,  GET CORRESPONDING     */
*/*            VBN FROM PFTE, SHIFT UNTIL VIRTUAL ADDRESS IS IN      */
*/*            REGISTER 3.                                           */
*/*                                                                  */
