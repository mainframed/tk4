         LR    R14,R0                                              0196
*     IF R15=GOOD THEN              /* CHECK ACCESS                  */
         LTR   R15,R15                                             0197
         BNZ   @RF00197                                            0197
*       PFTPTR=CURRENT;             /* GET REQUESTED FIELD           */
         MVC   PFTPTR(4),CURRENT(R14)                              0198
