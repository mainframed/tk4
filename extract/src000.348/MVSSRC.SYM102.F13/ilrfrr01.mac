*/********************************************************************/
*/*                                                                  */
*/*  ILRVASGQ IS THE ASMSTAGQ VERIFICATION ROUTINE.                  */
*/*                                                                  */
*/*  INPUT - REG 1 CONTAINS THE ADDRESS OF THE SDWA                  */
*/*          REG 3 CONTAINS THE ADDRESS OF THE ASMVT                 */
*/*          REG 8 CONTAINS THE ADDRESS OF THE WORKAREA              */
*/*          REG 13 CONTAINS THE ADDRESS OF A REG-SAVE AREA          */
*/*                                                                  */
*/********************************************************************/
*                                                                  0127
*ILRVASGQ:                                                         0127
*   ENTRY;                          /* ASMSTAGQ VERIFICATION ROUTINE */
@EP00127 DS    0H                                                  0128
*   RFY                                                            0128
*    (R2,                                                          0128
*     ASMVTPTR,                                                    0128
*     R4) RSTD;                     /* RESTRICT REGS THAT SHOULD NOT
*                                      BE DESTROYED OVER VERIFICATION
*                                      ROUTINE                       */
*   RFY                                                            0129
*     R14 RSTD;                     /* RESTRICT REG CONTAINING RETURN
*                                      ADDRESS                       */
*   GEN REFS(WAREG)(USING @DATD,WAREG);/* GET ADDRESSABILITY TO    0130
*                                      WORKAREA                      */
         USING @DATD,WAREG
*   WAREG->WORKA=''B;               /* CLEAR WORKAREA                */
         XC    WORKA(72,WAREG),WORKA(WAREG)                        0131
*   MAINRTRN=R14;                   /* SAVE RETURN ADDRESS           */
         ST    R14,MAINRTRN                                        0132
*   RFY                                                            0133
*     R14 UNRSTD;                   /* RELEASE REG                   */
*   QVPLEVR=ADDR(ILRVAIA);          /* INIT ADDR OF ELEMENT        0134
*                                      VERIFICATION ROUTINE          */
         L     @15,@CA02870                                        0134
         ST    @15,QVPLEVR                                         0134
*   QVPLHDR=ADDR(ASMSTAGF);         /* INIT ADDR OF QUEUE HEADER     */
         LA    @15,ASMSTAGF(,ASMVTPTR)                             0135
         ST    @15,QVPLHDR                                         0135
*   QVPLTRLR=ADDR(ASMSTAGL);        /* INIT ADDR OF QUEUE TRAILER    */
         LA    @15,ASMSTAGL(,ASMVTPTR)                             0136
         ST    @15,QVPLTRLR                                        0136
*   RFY                                                            0137
*     AIAPTR RSTD;                  /* RESTRICT REG TO POINT TO AIA  */
*   QVPLFPTR=ADDR(AIANXAIA)-ADDR(AIA);/* INIT OFFSET OF FORWARD PTR  */
         MVC   QVPLFPTR(2),@CH01945                                0138
*   RFY                                                            0139
*     AIAPTR UNRSTD;                /* RELEASE REG                   */
*   QTYPE=TWO;                      /* SET QTYPE FOR SINGLE-THREADED,
*                                      DOUBLE-HEADED QUEUE           */
         MVC   QTYPE(4),@CF00133                                   0140
*   CALL COMQRTN;                   /* INTERFACE WITH QUEUE VERIFIER */
         BAL   @14,COMQRTN                                         0141
*   RFY                                                            0142
*     R14 RSTD;                     /* RESTRICT REG FOR RETURN ADDR  */
*   R14=MAINRTRN;                   /* RESTORE RETURN ADDRESS        */
         L     R14,MAINRTRN                                        0143
*   GEN(BR    R14);                 /* RETURN TO CALLER              */
         BR    R14
*   RFY                                                            0145
*    (R2,                                                          0145
*     ASMVTPTR,                                                    0145
*     R4,                                                          0145
*     R14) UNRSTD;                  /* RELEASE REGS                  */
*                                                                  0146
*/********************************************************************/
*/*                                                                  */
*/*  ILRVLPRQ IS THE LGEPROCQ VERIFICATION ROUTINE.                  */
*/*                                                                  */
*/*  INPUT - REG 0 CONTAINS THE ADDRESS OF THE LGE                   */
*/*          REG 1 CONTAINS THE ADDRESS OF THE SDWA                  */
*/*          REG 8 CONTAINS THE ADDRESS OF THE WORKAREA              */
*/*          REG 13 CONTAINS THE ADDRESS OF A REG-SAVE AREA          */
*/*                                                                  */
*/********************************************************************/
*                                                                  0146
*ILRVLPRQ:                                                         0146
*   ENTRY;                          /* LGEPROCQ VERIFICATION ROUTINE */
@EP00146 DS    0H                                                  0147
*   RFY                                                            0147
*    (R2,                                                          0147
*     R3,                                                          0147
*     R4) RSTD;                     /* RESTRICT REGS THAT SHOULD NOT
*                                      BE DESTROYED BY VERIFICATION
*                                      ROUTINE                       */
*   RFY                                                            0148
*     R14 RSTD;                     /* RESTRICT REG CONTAINING RETURN
*                                      ADDRESS                       */
*   WAREG->WORKA=''B;               /* CLEAR WORKAREA                */
         XC    WORKA(72,WAREG),WORKA(WAREG)                        0149
*   MAINRTRN=R14;                   /* SAVE RETURN ADDRESS           */
         ST    R14,MAINRTRN                                        0150
*   RFY                                                            0151
*     R14 UNRSTD;                   /* RELEASE REG                   */
*   RFY                                                            0152
*     R7 RSTD;                      /* RESTRICT REG FOR STM          */
*   STM(R2,R7,REGSAVE);             /* SAVE REGS DESTROYED BY CALL TO
*                                      ILRVLGE                       */
         STM   R2,R7,REGSAVE(SAVEPTR)                              0153
*   CALL ILRVLGE;                   /* VERIFY LGE                    */
         L     @15,@CA02877                                        0154
         BALR  @14,@15                                             0154
*   LM(R2,R7,REGSAVE);              /* RESTORE REGS DESTROYED BY CALL
*                                      TO ILRVLGE                    */
         LM    R2,R7,REGSAVE(SAVEPTR)                              0155
*   RFY                                                            0156
*     R7 UNRSTD;                    /* RELEASE REG                   */
*   RFY                                                            0157
*     RETCODE RSTD;                 /* RESTRICT REG CONTAINING RETURN
*                                      CODE                          */
*   IF RETCODE=ZERO THEN            /* TEST FOR RETURN CODE OF 0 - 0158
*                                      INDICATING A VALID LGE        */
         LTR   RETCODE,RETCODE                                     0158
         BNZ   @RF00158                                            0158
*     DO;                           /* CONTINUE ONLY IF VALID LGE    */
*       RFY                                                        0160
*         RETCODE UNRSTD;           /* RELEASE REG                   */
*       QVPLEVR=ADDR(ILRVAIAC);     /* INIT ADDR OF ELEMENT        0161
*                                      VERIFICATION ROUTINE          */
         L     @15,@CA02879                                        0161
         ST    @15,QVPLEVR                                         0161
*       QVPLHDR=ADDR(INPARM->LGEPROCF);/* INIT ADDR OF QUEUE HEADER  */
         ST    INPARM,QVPLHDR                                      0162
*       QVPLTRLR=ADDR(INPARM->LGEPROCL);/* INIT ADDR OF QUEUE TRLR   */
         LR    @15,INPARM                                          0163
         LA    @15,LGEPROCL(,@15)                                  0163
         ST    @15,QVPLTRLR                                        0163
*       RFY                                                        0164
*         AIAPTR RSTD;              /* RESTRICT REG TO POINT TO AIA  */
*       QVPLFPTR=ADDR(AIAFQPA)-ADDR(AIA);/* INIT OFFSET OF FORWARD 0165
*                                      POINTER                       */
         LR    @06,AIAPTR                                          0165
         SLR   @06,AIAPTR                                          0165
         STH   @06,QVPLFPTR                                        0165
*       QVPLBPTR=ADDR(AIABQPA)-ADDR(AIA);/* INIT OFFSET OF BACKWARD
*                                      POINTER                       */
         MVC   QVPLBPTR(2),@CH00145                                0166
*       RFY                                                        0167
*         AIAPTR UNRSTD;            /* RELEASE REG                   */
*       QTYPE=THREE;                /* SET QTYPE FOR DOUBLE-THREADED,
*                                      DOUBLE-THREADED QUEUE         */
         MVC   QTYPE(4),@CF00083                                   0168
*       CALL COMQRTN;               /* INTERFACE WITH QUEUE VERIFIER */
         BAL   @14,COMQRTN                                         0169
*       RFY                                                        0170
*         RETCODE RSTD;             /* RESTRICT REG USED FOR RETURN
*                                      CODE                          */
*     END;                          /* END OF SECTION TO PROCESS A 0171
*                                      VALID LGE                     */
*   ELSE                            /* WHEN LGE IS NOT VALID         */
*     RETCODE=INVPARM;              /* SET RETURN CODE TO INDICATE A
*                                      BAD PARAMETER WAS PASSED      */
         B     @RC00158                                            0172
@RF00158 LA    RETCODE,24                                          0172
*   RFY                                                            0173
*     R14 RSTD;                     /* RESTRICT REG FOR RETURN ADDR  */
@RC00158 DS    0H                                                  0174
*   R14=MAINRTRN;                   /* RESTORE RETURN ADDRESS        */
         L     R14,MAINRTRN                                        0174
*   GEN(BR    R14);                 /* RETURN TO CALLER              */
         BR    R14
*   RFY                                                            0176
*    (R2,                                                          0176
*     R3,                                                          0176
*     R4,                                                          0176
*     R14,                                                         0176
*     RETCODE) UNRSTD;              /* RELEASE REGS                  */
*                                                                  0177
*/********************************************************************/
