*     R14,                                                         0533
*     RETCODE) UNRSTD;              /* RELEASE REGS                  */
*                                                                  0534
*/********************************************************************/
*/*                                                                  */
*/*  ILRVIOE IS THE ROUTINE TO VERIFY A POTENTIAL IOE                */
*/*                                                                  */
*/*  INPUT - REG 0 CONTAINS THE ADDRESS OF THE POTENTIAL IOE         */
*/*          REG 1 CONTAINS THE ADDRESS OF THE SDWA                  */
*/*                                                                  */
*/********************************************************************/
*                                                                  0534
*ILRVIOE:                                                          0534
*   ENTRY;                          /* IOE VERIFICATION ROUTINE      */
@EP00534 DS    0H                                                  0535
*   RFY                                                            0535
*    (IOEPTR,                                                      0535
*     RETURN1,                                                     0535
*     R14) RSTD;                    /* RESTRICT REGS TO SAVE INPUT R0
*                                      AND RETURN ADDRESS AND REG  0535
*                                      CONTAINING RETURN ADDRESS     */
*   IF INPARM^=ZERO THEN            /* TEST FOR INPUT VALUE OF 0     */
         LTR   INPARM,INPARM                                       0536
         BZ    @RF00536                                            0536
*     DO;                           /* CONTINUE PROCESSING IF NOT 0  */
*       RETURN1=R14;                /* SAVE RETURN ADDRESS           */
         LR    RETURN1,R14                                         0538
*       RFY                                                        0539
*         R14 UNRSTD;               /* RELEASE REG                   */
*       IOEPTR=INPARM;              /* SAVE VALUE OF INPUT R0        */
         LR    IOEPTR,INPARM                                       0540
*       RFY                                                        0541
*         CBPTR RSTD;               /* RESTRICT REG TO POINT TO CB TO
*                                      BE VERIFIED                   */
*       CBPTR=INPARM;               /* INIT ADDR OF CB TO BE VERIFIED*/
         LR    CBPTR,INPARM                                        0542
*       INPARM=LENGTH(IOE);         /* INIT LENGTH OF CB TO BE     0543
*                                      VERIFIED                      */
         LA    INPARM,16                                           0543
*       RFY                                                        0544
*        (R3,                                                      0544
*         RETCODE) RSTD;            /* RESTRICT REG DESTROYED BY CALL
*                                      TO ADDRESS VERIFIER AND REG 0544
*                                      FOR RETURN CODE               */
*       CALL IEAVEADV;              /* CALL ADDRESS VERIFIER         */
         L     @15,@CV02859                                        0545
         BALR  @14,@15                                             0545
*       RFY                                                        0546
*        (CBPTR,                                                   0546
*         R3) UNRSTD;               /* RELEASE REGS                  */
*       RFY                                                        0547
*         R14 RSTD;                 /* RESTRICT REG FOR RETURN ADDR  */
*       R14=RETURN1;                /* RESTORE RETURN ADDRESS        */
         LR    R14,RETURN1                                         0548
*       RFY                                                        0549
*         RETURN1 UNRSTD;           /* RELEASE REG                   */
*       IF RETCODE^=ZERO THEN       /* IF RETURN CODE IS NOT 0, IOE
*                                      IS NOT ADDRESSABLE            */
         LTR   RETCODE,RETCODE                                     0550
         BZ    @RF00550                                            0550
*         RETCODE=INVCB;            /* SET RETURN CODE TO INDICATE 0551
*                                      INVALID CB                    */
         LA    RETCODE,8                                           0551
*       ELSE                        /* IF RETURN CODE IS 0, IOE IS 0552
*                                      ADDRESSABLE                   */
*         IF IOEAIA^=ZERO THEN      /* IF THERE IS AN AIA ADDR IN THE
*                                      IOE                           */
         B     @RC00550                                            0552
@RF00550 L     @06,IOEAIA(,IOEPTR)                                 0552
         LTR   @06,@06                                             0552
         BZ    @RF00552                                            0552
*           DO;                     /* VERIFY THIS AIA               */
*             RFY                                                  0554
*              (PCBPTR,                                            0554
*               AIAPTR) RSTD;       /* RESTRICT REGS TO POINT TO PCB
*                                      AND AIA                       */
*             INPARM=IOEAIA-(LENGTH(PCB)-LENGTH(AIA));/* CALCULATE 0555
*                                      PCB ADDRESS                   */
         SH    @06,@CH01271                                        0555
         LR    INPARM,@06                                          0555
*             GOTO COMMPCBA;        /* BRANCH TO AIA VERIFICATION RTN*/
         B     COMMPCBA                                            0556
*             RFY                                                  0557
*              (PCBPTR,                                            0557
*               AIAPTR) UNRSTD;     /* RELEASE REGS                  */
*           END;                                                   0558
*       INPARM=IOEPTR;              /* RESTORE VALUE OF INPUT R0     */
@RF00552 DS    0H                                                  0559
@RC00550 LR    INPARM,IOEPTR                                       0559
*     END;                          /* END OF PROCESSING NON-0 INPUT */
*   ELSE                            /* IF INPUT VALUE IS 0           */
*     RETCODE=INVCB;                /* SET RETURN CODE TO INDICATE 0561
*                                      INVALID CB                    */
         B     @RC00536                                            0561
@RF00536 LA    RETCODE,8                                           0561
*   GEN(BR    R14);                 /* RETURN TO CALLER              */
@RC00536 DS    0H                                                  0562
         BR    R14
*   RFY                                                            0563
*    (IOEPTR,                                                      0563
*     R14,                                                         0563
*     RETCODE) UNRSTD;              /* RELEASE REGS                  */
*                                                                  0564
*/********************************************************************/
*/*                                                                  */
*/*  ILRVIORB IS THE ROUTINE TO VERIFY A POTENTIAL IORB-IOSB-SRB     */
*/*           COMBINATION                                            */
*/*                                                                  */
*/*  INPUT - REG 0 CONTAINS THE ADDRESS OF THE POTENTIAL COMBINATION */
*/*          REG 1 CONTAINS THE ADDRESS OF THE SDWA                  */
*/*                                                                  */
*/********************************************************************/
*                                                                  0564
*ILRVIORB:                                                         0564
*   ENTRY;                          /* IORB-IOSB-SRB VERIFICATION AND
*                                      REFRESH ROUTINE               */
@EP00564 DS    0H                                                  0565
*   RFY                                                            0565
*    (RETURN1,                                                     0565
*     IORBPTR,                                                     0565
*     R14) RSTD;                    /* RESTRICT REGS TO SAVE RETURN
*                                      ADDRESS AND INPUT R0 AND REG
*                                      CONTAINING RETURN ADDRESS     */
*   IF INPARM^=ZERO THEN            /* TEST FOR INPUT VALUE OF 0     */
         LTR   INPARM,INPARM                                       0566
         BZ    @RF00566                                            0566
*     DO;                           /* CONTINUE PROCESSING IF NOT 0  */
*       RETURN1=R14;                /* SAVE RETURN ADDRESS           */
         LR    RETURN1,R14                                         0568
*       RFY                                                        0569
*         R14 UNRSTD;               /* RELEASE REG                   */
*       IORBPTR=INPARM;             /* SAVE VALUE OF INPUT R0        */
         LR    IORBPTR,INPARM                                      0570
*       RFY                                                        0571
*         CBPTR RSTD;               /* RESTRICT REG TO POINT TO CB TO
*                                      BE VERIFIED                   */
*       CBPTR=INPARM;               /* INIT ADDR OF CB TO BE VERIFIED*/
         LR    CBPTR,INPARM                                        0572
*       INPARM=LENGTH(IORB)+        /* INIT LENGTH OF CB TO BE     0573
*                                      VERIFIED - LENGTH OF IORB     */
*           SALENGTH+               /* PLUS LENGTH OF A STANDARD SA  */
*           ((LENGTH(IOSB)+7)/8*8)+ /* PLUS LENGTH OF IOSB           */
*           ((LENGTH(SRB)+7)/8*8);  /* PLUS LENGTH OF SRB            */
         LA    INPARM,272                                          0573
*       RFY                                                        0574
*        (R3,                                                      0574
*         RETCODE) RSTD;            /* RESTRICT REG DESTROYED BY CALL
*                                      TO ADDRESS VERIFIER AND REG 0574
*                                      FOR RETURN CODE               */
*       CALL IEAVEADV;              /* CALL ADDRESS VERIFIER         */
         L     @15,@CV02859                                        0575
         BALR  @14,@15                                             0575
*       IF RETCODE=ZERO&            /* IF RETURN CODE IS 0, IORB+ IS
*                                      ADDRESSABLE                   */
*           IORID=IDIORB THEN       /* AND THE IORB IDENTIFIER IS  0576
*                                      PRESENT                       */
         LTR   RETCODE,RETCODE                                     0576
         BNZ   @RF00576                                            0576
         CLI   IORID(IORBPTR),X'88'                                0576
         BNE   @RF00576                                            0576
*         DO;                       /* CONTINUE VERIFICATION         */
*           CBPTR=IORPARTE;         /* INIT ADDR OF CB TO BE       0578
*                                      VERIFIED, NOW THE PARTE/SARTE */
         L     CBPTR,IORPARTE(,IORBPTR)                            0578
*           INPARM=LENGTH(SARTE);   /* INIT LENGTH OF CB TO BE     0579
*                                      VERIFIED, VERIFY STORAGE FOR
*                                      THE LENGTH OF THE SARTE SINCE
*                                      IT IS THE SHORTER CB          */
         LA    INPARM,48                                           0579
*           CALL IEAVEADV;          /* CALL ADDRESS VERIFIER         */
         L     @15,@CV02859                                        0580
         BALR  @14,@15                                             0580
*           RFY                                                    0581
*             R3 UNRSTD;            /* RELEASE REG                   */
*           IF RETCODE=ZERO THEN    /* IF RETURN CODE IS 0,        0582
*                                      PARTE/SARTE IS ADDRESSABLE    */
         LTR   RETCODE,RETCODE                                     0582
         BNZ   @RF00582                                            0582
*             DO;                   /* CONTINUE VERIFICATION         */
*               RFY                                                0584
*                (ENTRYPTR,                                        0584
*                 ASMVTPTR) RSTD;   /* RESTRICT REGS TO POINT TO   0584
*                                      PARTE/SARTE AND TO ASMVT      */
*               ENTRYPTR=CBPTR->PARENN+ONE;/* CALCULATE ENTRY NUM    */
         LA    @14,1                                               0585
         AH    @14,PARENN(,CBPTR)                                  0585
         LR    ENTRYPTR,@14                                        0585
*               RFY                                                0586
*                 CBPTR UNRSTD;     /* RELEASE REG                   */
*               ASMVTPTR=CVTASMVT;  /* GET ASMVT ADDR FROM CVT       */
         L     @14,CVTPTR                                          0587
         L     ASMVTPTR,CVTASMVT(,@14)                             0587
*               IF IORSWAP=OFF THEN /* IF THIS IORB IS NOT FOR A SWAP
*                                      DATA SET                      */
         TM    IORSWAP(IORBPTR),B'00001000'                        0588
         BNZ   @RF00588                                            0588
*                 ENTRYPTR=ADDR(PARTENTS(ENTRYPTR));/* CALCULATE ADDR
*                                      OF PARTE                      */
         LR    @14,ENTRYPTR                                        0589
         SLA   @14,6                                               0589
         L     @04,PARTPTR(,ASMVTPTR)                              0589
         LA    @14,PARTENTS-64(@14,@04)                            0589
         LR    ENTRYPTR,@14                                        0589
*               ELSE                /* IF IORB IS FOR A SWAP DATA SET*/
*                 ENTRYPTR=ADDR(SARENTS(ENTRYPTR));/* CALCULATE ADDR
*                                      OF SARTE                      */
         B     @RC00588                                            0590
@RF00588 LR    @14,ENTRYPTR                                        0590
         MH    @14,@CH00457                                        0590
         L     @04,SARTPTR(,ASMVTPTR)                              0590
         LA    @14,SARENTS-48(@14,@04)                             0590
         LR    ENTRYPTR,@14                                        0590
*               RFY                                                0591
*                 ASMVTPTR UNRSTD;  /* RELEASE REG                   */
@RC00588 DS    0H                                                  0592
*               RFY                                                0592
*                 IOSBPTR RSTD;     /* RESTRICT REG TO POINT TO IOSB */
*               IOSBPTR=IORBPTR+LENGTH(IORB)+SALENGTH;/* CALCULATE 0593
*                                      IOSB ADDRESS                  */
         LA    IOSBPTR,112                                         0593
         ALR   IOSBPTR,IORBPTR                                     0593
*               IF IORPARTE=ENTRYPTR&/* IF IORPARTE IS THE SAME AS 0594
*                                      THE CALCULATED PARTE ADDR     */
*                   IOSDVRID=IOSMISID&/* AND THE DRIVER ID IN THE  0594
*                                      IOSB IS THE MISC ID           */
*                   IOSASID=MASTERID THEN/* AND THE ASID IN THE IOSB
*                                      IS MASTER'S ASID              */
         C     ENTRYPTR,IORPARTE(,IORBPTR)                         0594
         BNE   @RF00594                                            0594
         CLI   IOSDVRID(IOSBPTR),X'01'                             0594
         BNE   @RF00594                                            0594
         CLC   IOSASID(2,IOSBPTR),@CH00055                         0594
         BNE   @RF00594                                            0594
*                 DO;               /* REFRESH IORB, IOSB, SRB FIELDS*/
*                   IF ENTRYPTR->PAREUCBP->UCBRPS=ON THEN/* IF UCB 0596
*                                      INDICATES DEVICE IS AN RPS  0596
*                                      DEVICE                        */
         L     @14,PAREUCBP(,ENTRYPTR)                             0596
         TM    UCBRPS(@14),B'00010000'                             0596
         BNO   @RF00596                                            0596
*                     IORFRPS=ON;   /* TURN ON RPS FLAG IN IORB      */
         OI    IORFRPS(IORBPTR),B'01000000'                        0597
*                   RFY                                            0598
*                     ENTRYPTR UNRSTD;/* RELEASE REG                 */
@RF00596 DS    0H                                                  0599
*                   IORSAVE=IORBPTR+LENGTH(IORB);/* REFRESH ADDR OF
*                                      SA IN IORB                    */
         LA    @14,40                                              0599
         ALR   @14,IORBPTR                                         0599
         ST    @14,IORSAVE(,IORBPTR)                               0599
*                   IORIOSB=IOSBPTR;/* REFRESH ADDR OF IOSB IN IORB  */
         ST    IOSBPTR,IORIOSB(,IORBPTR)                           0600
*                   IOSCC3WE=ON;    /* REFRESH FLAG IN IOSB TO     0601
*                                      REQUEST POST FOR SIO CC3      */
         OI    IOSCC3WE(IOSBPTR),B'00100000'                       0601
*                   IOSIDR=ON;      /* REFRESH FLAG IN IOSB TO     0602
*                                      REQUEST ASYNCHRONOUS        0602
*                                      SCHEDULING                    */
         OI    IOSIDR(IOSBPTR),B'00000100'                         0602
*                   IOSPSLL=ON;     /* REFRESH FLAG IN IOSB TO     0603
*                                      INDICATE LOCAL LOCK NOT WANTED
*                                      FOR POST STATUS PROCESSING    */
*                   IOSTSLL=ON;     /* REFRESH FLAG IN IOSB TO     0604
*                                      INDICATE LOCAL LOCK NOT WANTED
*                                      BY TERMINATION ROUTINE        */
         OI    IOSPSLL(IOSBPTR),B'00010100'                        0604
*                   IOSSRB=IOSBPTR+((LENGTH(IOSB)+7)/8*8);/* REFRESH
*                                      ADDR OF SRB IN IOSB           */
         LA    @14,112                                             0605
         ALR   @14,IOSBPTR                                         0605
         ST    @14,IOSSRB(,IOSBPTR)                                0605
*                   IOSUSE=IORBPTR; /* REFRESH ADDR OF IORB IN IOSB  */
         ST    IORBPTR,IOSUSE(,IOSBPTR)                            0606
*                   IOSFMSK=FILEMASK;/* REFRESH FILE MASK IN IOSB    */
         MVI   IOSFMSK(IOSBPTR),X'88'                              0607
*                   RFY                                            0608
*                     ASMVTPTR RSTD;/* RESTRICT REG TO POINT TO ASMVT*/
*                   ASMVTPTR=CVTASMVT;/* GET ASMVT ADDR FROM CVT     */
         L     @02,CVTPTR                                          0609
         L     ASMVTPTR,CVTASMVT(,@02)                             0609
*                   IOSPGAD=ASMPCMP;/* REFRESH ADDR OF ILRCMP IN IOSB*/
         MVC   IOSPGAD(4,IOSBPTR),ASMPCMP(ASMVTPTR)                0610
*                   IOSNRM=ASMPCMPN;/* REFRESH ADDR OF ILRCMPNE IN 0611
*                                      IOSB                          */
         MVC   IOSNRM(4,IOSBPTR),ASMPCMPN(ASMVTPTR)                0611
*                   IOSABN=ASMPCMPA;/* REFRESH ADDR OF ILRCMPAE IN 0612
*                                      IOSB                          */
         MVC   IOSABN(4,IOSBPTR),ASMPCMPA(ASMVTPTR)                0612
*                   IOSDIE=ASMPCMPD;/* REFRESH ADDR OF ILRCMPDI IN 0613
*                                      IOSB - HIGH ORDER BIT IS ON   */
         MVC   IOSDIE(4,IOSBPTR),ASMPCMPD(ASMVTPTR)                0613
*                   RFY                                            0614
*                     ASMVTPTR UNRSTD;/* RELEASE REG                 */
*                   RFY                                            0615
*                     SRBPTR RSTD;  /* RESTRICT REG TO POINT TO SRB  */
*                   SRBPTR=IOSSRB;  /* GET SRB ADDR JUST CALCULATED  */
         LR    SRBPTR,@14                                          0616
*                   SRBID=IDSRB;    /* REFRESH SRB IDENTIFIER        */
         MVC   SRBID(4,SRBPTR),@CC02853                            0617
*                   SRBPARM=IOSBPTR;/* REFRESH ADDR OF IOSB IN SRB   */
         ST    IOSBPTR,SRBPARM(,SRBPTR)                            0618
*                   SRBPRIOR=NONQUIES;/* REFRESH SRB PRIORITY FIELD  */
         MVI   SRBPRIOR(SRBPTR),X'04'                              0619
*                   RFY                                            0620
*                    (SRBPTR,                                      0620
*                     IOSBPTR) UNRSTD;/* RELEASE REGS                */
*                 END;              /* END OF REFRESH CODE           */
*               ELSE                /* IF ANY VERIFICATION FAILS     */
*                 RETCODE=INVCB;    /* SET RETURN CODE TO INDICATE 0622
*                                      INVALID CB                    */
         B     @RC00594                                            0622
@RF00594 LA    RETCODE,8                                           0622
*             END;                                                 0623
*           ELSE                    /* IF RETURN CODE IS NOT 0,    0624
*                                      PARTE/SARTE IS NOT ADDRESSABLE*/
*             RETCODE=INVCB;        /* SET RETURN CODE TO INDICATE 0624
*                                      INVALID CB                    */
         B     @RC00582                                            0624
@RF00582 LA    RETCODE,8                                           0624
*         END;                                                     0625
*       ELSE                        /* IF EITHER IORB VERIFICATION 0626
*                                      FAILS                         */
*         RETCODE=INVCB;            /* SET RETURN CODE TO INDICATE 0626
*                                      INVALID CB                    */
         B     @RC00576                                            0626
@RF00576 LA    RETCODE,8                                           0626
*       INPARM=IORBPTR;             /* RESTORE VALUE OF INPUT R0     */
@RC00576 LR    INPARM,IORBPTR                                      0627
*       RFY                                                        0628
*         R14 RSTD;                 /* RESTRICT REG FOR RETURN ADDR  */
*       R14=RETURN1;                /* RESTORE RETURN ADDRESS        */
         LR    R14,RETURN1                                         0629
*     END;                          /* END OF PROCESSING NON-0 INPUT */
*   ELSE                            /* IF INPUT VALUE IS 0           */
*     RETCODE=INVCB;                /* SET RETURN CODE TO INDICATE 0631
*                                      INVALID CB                    */
         B     @RC00566                                            0631
@RF00566 LA    RETCODE,8                                           0631
*   GEN(BR    R14);                 /* RETURN TO CALLER              */
@RC00566 DS    0H                                                  0632
         BR    R14
*   RFY                                                            0633
*    (RETURN1,                                                     0633
*     IORBPTR,                                                     0633
*     R14,                                                         0633
