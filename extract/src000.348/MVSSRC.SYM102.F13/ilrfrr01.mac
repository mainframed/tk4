*/********************************************************************/
*/*                                                                  */
*/*  COMQRTN IS AN INTERNAL ROUTINE THAT INTERFACES WITH THE GENERAL */
*/*          SUPERVISOR QUEUE VERIFICATION ROUTINES.                 */
*/*                                                                  */
*/*  INPUT - REG 0 CONTAINS AN INPUT PARAMETER THAT MUST BE SAVED    */
*/*          REG 1 CONTAINS THE ADDRESS OF THE SDWA                  */
*/*          REG 8 CONTAINS THE ADDRESS OF THE WORKAREA              */
*/*          REG 13 CONTAINS THE ADDRESS OF REG-SAVE AREA            */
*/*                                                                  */
*/*  OUTPUT - REG 15 CONTAINS A RETURN CODE                          */
*/*                                                                  */
*/********************************************************************/
*                                                                  0639
*COMQRTN:                                                          0639
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0639
COMQRTN  DS    0H                                                  0640
*   RFY                                                            0640
*    (R2,                                                          0640
*     R3,                                                          0640
*     R4,                                                          0640
*     R7) RSTD;                     /* RESTRICT REGS NOT TO BE     0640
*                                      DESTROYED BY THIS RTN         */
*   RFY                                                            0641
*    (RETURN1,                                                     0641
*     R14) RSTD;                    /* RESTRICT REG TO SAVE RETURN 0641
*                                      ADDRESS AND REG CONTAINING  0641
*                                      RETURN ADDRESS                */
*   RETURN1=R14;                    /* SAVE RETURN ADDRESS           */
         LR    RETURN1,R14                                         0642
*   RFY                                                            0643
*     R14 UNRSTD;                   /* RELEASE REG                   */
*                                                                  0643
*   /*****************************************************************/
*   /*                                                               */
*   /* PROCESS EMPTY DOUBLE-HEADED QUEUES.                           */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0644
*   IF QTYPE>ONE&                   /* IF QUEUE IS DOUBLE-HEADED     */
*       QVPLHDR->ELEMENT=ZERO THEN  /* AND EMPTY, DO NOT CALL QUEUE
*                                      VERIFIER ROUTINE              */
         CLC   QTYPE(4),@CF00055                                   0644
         BNH   @RF00644                                            0644
         SLR   @15,@15                                             0644
         L     @14,QVPLHDR                                         0644
         C     @15,ELEMENT(,@14)                                   0644
         BNE   @RF00644                                            0644
*     DO;                                                          0645
*       QVPLTRLR->ELEMENT=ZERO;     /* INSURE LAST ELEMENT IS 0      */
         L     @14,QVPLTRLR                                        0646
         ST    @15,ELEMENT(,@14)                                   0646
*       RFY                                                        0647
*         RETCODE RSTD;             /* RESTRICT REG USED FOR RETURN
*                                      CODE                          */
*       RETCODE=ZERO;               /* SET SUCCESSFUL RETURN CODE    */
         SLR   RETCODE,RETCODE                                     0648
*       RFY                                                        0649
*         RETCODE UNRSTD;           /* RELEASE REG                   */
*     END;                          /* END OF SECTION TO PROCESS   0650
*                                      EMPTY DOUBLE-HEADED QUEUE     */
*                                                                  0650
*   /*****************************************************************/
*   /*                                                               */
*   /* PROCESS ALL REMAINING QUEUES.                                 */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0651
*   ELSE                            /* QUEUE IS NOT DOUBLE-HEADED OR
*                                      EMPTY                         */
*     DO;                                                          0651
         B     @RC00644                                            0651
@RF00644 DS    0H                                                  0652
*       QVPLWKA=ADDR(QVWA);         /* INIT ADDR OF WORKAREA FOR   0652
*                                      QUEUE VERIFIER                */
         LA    @15,QVWA                                            0652
         ST    @15,QVPLWKA                                         0652
*       QVPLODA=ADDR(SDWARA);       /* INIT ADDR OF OUTPUT DATA AREA */
         LA    @15,SDWARA(,SDWAPTR)                                0653
         ST    @15,QVPLODA                                         0653
*       RFY                                                        0654
*         QVERPTR RSTD;             /* RESTRICT REG FOR ADDR OF QUEUE
*                                      VERIFIER                      */
*       IF QTYPE=ONE THEN           /* IF QUEUE IS SINGLE-THREADED 0655
*                                      AND SINGLE-HEADED             */
         CLC   QTYPE(4),@CF00055                                   0655
         BNE   @RF00655                                            0655
*         QVERPTR=ADDR(IEAVEQV1);   /* CALL QUEUE VERIFIER ENTRY 1   */
         L     QVERPTR,@CV02860                                    0656
*       ELSE                                                       0657
*         IF QTYPE=TWO THEN         /* IF QUEUE IS SINGLE-THREADED 0657
*                                      AND DOUBLE-HEADED             */
         B     @RC00655                                            0657
@RF00655 CLC   QTYPE(4),@CF00133                                   0657
         BNE   @RF00657                                            0657
*           QVERPTR=ADDR(IEAVEQV2); /* CALL QUEUE VERIFIER ENTRY 2   */
         L     QVERPTR,@CV02861                                    0658
*         ELSE                      /* QUEUE MUST BE DOUBLE-THREADED
*                                      AND DOUBLE-HEADED             */
*           QVERPTR=ADDR(IEAVEQV3); /* CALL QUEUE VERIFIER ENTRY 3   */
         B     @RC00657                                            0659
@RF00657 L     QVERPTR,@CV02862                                    0659
*       SAVERG=INPARM;              /* SAVE VALUE OF INPUT REG 0     */
@RC00657 DS    0H                                                  0660
@RC00655 ST    INPARM,SAVERG                                       0660
*       INPARM=SDWAPTR;             /* SET REG 0 TO ADDRESS OF SDWA,
*                                      PARM FOR EVR                  */
