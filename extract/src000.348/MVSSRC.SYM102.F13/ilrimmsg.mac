@RC00213 MVC   MSGOTEXT+22(19),TXT916E                             0216
*       MSGOTEXT(42:56)=PROMPT;                                    0217
         MVC   MSGOTEXT+41(15),PROMPT                              0217
*     END;                          /* 6 CHAR PARM                   */
*   ELSE                                                           0219
*     DO;                           /* 4 CHAR PARM                   */
         B     @RC00211                                            0219
@RF00211 DS    0H                                                  0220
*       IF SWAPEQ=YES THEN                                         0220
         TM    SWAPEQ(MSGPLPTR),B'01000000'                        0220
         BNO   @RF00220                                            0220
*         MSGOTEXT(17:20)=SWAP;     /* SWAP PARM                     */
         MVC   MSGOTEXT+16(4),SWAP                                 0221
*       ELSE                                                       0222
*         MSGOTEXT(17:20)=PAGE;     /* PUT IN PAGE                   */
         B     @RC00220                                            0222
@RF00220 MVC   MSGOTEXT+16(4),PAGE                                 0222
*       MSGOTEXT(21:39)=TXT916E;                                   0223
@RC00220 MVC   MSGOTEXT+20(19),TXT916E                             0223
*       MSGOTEXT(40:54)=PROMPT;                                    0224
         MVC   MSGOTEXT+39(15),PROMPT                              0224
*     END;                          /* 4 CHAR PARM                   */
*   END MSG916E;                    /* END OF SUBROUTINE MSG916E     */
@EL00005 DS    0H                                                  0226
@EF00005 DS    0H                                                  0226
@ER00005 BR    @14                                                 0226
