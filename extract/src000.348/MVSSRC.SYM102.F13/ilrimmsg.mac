         TITLE 'ILRIMMSG -- ASM RIM MESSAGE MODULE                     *
                        '
ILRIMMSG CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'ILRIMMSG  76.087'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @12,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@12                                         0001
         ST    @13,@SA00001+4                                      0001
         LA    @14,@SA00001                                        0001
         ST    @14,8(,@13)                                         0001
         LR    @13,@14                                             0001
*
* REFS ARE GENERATED FOR NVT FIELDS THAT CONTAIN ADDRESSES OF
* NIP SERVICE ROUTINES, SO IEAPMNIP MACRO CALLS TO THESE ROUTINES
* WILL ASSEMBLE PROPERLY.
*   MSGPLPTR=R1;                    /* GET ADDR OF MESSAGE PARM LIST */
         LR    MSGPLPTR,R1                                         0153
*   R2=NVTPTR;                      /* GET NVT ADDRESS               */
         L     R2,NVTPTR(,MSGPLPTR)                                0154
*   R3=CVTPTR;                      /* GET CVT ADDRESS               */
         L     R3,CVTPTR(,MSGPLPTR)                                0155
*   MSGOPREF=IEA;                   /* SET MSG ID PREFIX             */
         MVC   MSGOPREF(3),IEA                                     0156
*   MSGOID=MSGID;                   /* MOVE MSG ID TO MSG AREA       */
         MVC   MSGOID(4),MSGID(MSGPLPTR)                           0157
*   MSGOTEXT=BLANK;                 /* BLANK MESSAGE TEXT AREA       */
         MVI   MSGOTEXT+1,C' '                                     0158
         MVC   MSGOTEXT+2(62),MSGOTEXT+1                           0158
         MVI   MSGOTEXT,C' '                                       0158
*   VALFLGS=''B;                    /* INITLZ VALID REPLY FLGS (USED
*                                      WITH 'D' TYPE MSGS)           */
*                                                                  0159
         XC    VALFLGS(4),VALFLGS                                  0159
*   /*****************************************************************/
*   /*                                                               */
*   /* LOOP THROUGH BRANCH TABLE TO GET MESSAGE SUBROUTINE           */
*   /* CORRESPONDING TO MESSAGE ID RECEIVED                          */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0160
*   DO WRKCTR=DIM(MSGBTENT) TO ONE BY-1;                           0160
*                                                                  0160
         LA    WRKCTR,24                                           0160
@DL00160 DS    0H                                                  0161
*     /***************************************************************/
*     /*                                                             */
*     /* TEST FOR MSG ID MATCH                                       */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0161
*     IF MSGID=MSGBTID(WRKCTR) THEN /* MSG IDS MATCH ?               */
         LR    @07,WRKCTR                                          0161
         SLA   @07,3                                               0161
         LA    @04,MSGBTID-8(@07)                                  0161
         CLC   MSGID(4,MSGPLPTR),0(@04)                            0161
         BNE   @RF00161                                            0161
*       DO;                                                        0162
*                                                                  0162
*         /***********************************************************/
*         /*                                                         */
*         /* INVOKE PROPER SUBROUTINE TO BUILD MESSAGE               */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0163
*         MSGSPTR=MSGBTSUB(WRKCTR); /* GET MSG SUBRTN ADDRESS        */
         L     MSGSPTR,MSGBTSUB-8(@07)                             0163
*         CALL MSGNNNT;             /* BUILD MESSAGE                 */
         LR    @15,MSGSPTR                                         0164
         BALR  @14,@15                                             0164
*         WRKCTR=ONE;               /* END LOOP                      */
         LA    WRKCTR,1                                            0165
*       END;                                                       0166
*     ELSE                                                         0167
*       ;                           /* NO MATCH ?                    */
@RF00161 DS    0H                                                  0168
*   END;                            /* END LOOP THRU BRANCH TABLE    */
*                                                                  0168
         BCTR  WRKCTR,0                                            0168
         LTR   WRKCTR,WRKCTR                                       0168
         BP    @DL00160                                            0168
*   /*****************************************************************/
*   /*                                                               */
*   /* WTO OR WTOR MESSAGE, DEPENDING ON MESSAGE ID TYPE CODE. IF 'W'*/
*   /* TYPE WTO MESSAGE, THE SYSTEM IS PUT INTO A WAIT CONDITION     */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0169
*   IF MSGIDTYP=DECISION THEN       /* WTOR TYPE MESSAGE ?           */
         CLI   MSGIDTYP(MSGPLPTR),C'D'                             0169
         BNE   @RF00169                                            0169
*     DO;                                                          0170
*                                                                  0170
*       /*************************************************************/
*       /*                                                           */
*       /* LOOP UNTIL VALID REPLY RECEIVED FROM OPERATOR             */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0171
*       VALREPLY=NO;                /* INITLZ LOOP CONTROL VBL       */
         NI    VALREPLY,B'10111111'                                0171
*       DO WHILE(VALREPLY=NO);                                     0172
*                                                                  0172
         B     @DE00172                                            0172
@DL00172 DS    0H                                                  0173
*         /***********************************************************/
*         /*                                                         */
*         /* USE NIP SERVICE TO WTOR MESSAGE                         */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0173
*         GEN(IEAPMNIP TYPE=WTOR,PARAM=NWTORPL)                    0173
*         REFS(NVTWTOR,NWTORPL);                                   0173
         IEAPMNIP TYPE=WTOR,PARAM=NWTORPL
*         REPLYPTR=R1;              /* GET ADDR OF OPER REPLY        */
         LR    REPLYPTR,R1                                         0174
*         VNP04R1=REPLYPTR;         /* RETURN ADDR OF OPERATOR REPLY
*                                      AREA                          */
         L     @06,VNP04SAP(,R13)                                  0175
         ST    REPLYPTR,VNP04R1(,@06)                              0175
*         CALL SYNCHK;              /* CHECK FOR VALID REPLY         */
         BAL   @14,SYNCHK                                          0176
*       END;                        /* END WTOR LOOP                 */
@DE00172 TM    VALREPLY,B'01000000'                                0177
         BZ    @DL00172                                            0177
*     END;                          /* END WTOR PROCESSING           */
*   ELSE                                                           0179
*                                                                  0179
*     /***************************************************************/
*     /*                                                             */
*     /* WTO TYPE MESSAGE PROCESSING                                 */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0179
*     DO;                                                          0179
*                                                                  0179
         B     @RC00169                                            0179
@RF00169 DS    0H                                                  0180
*       /*************************************************************/
*       /*                                                           */
*       /* USE NIP SERVICE TO WTO MESSAGE                            */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0180
*       GEN(IEAPMNIP TYPE=WTO,PARAM=NWTOMHDR)                      0180
*       REFS(NVTWTO,NWTOMHDR);                                     0180
*                                                                  0180
         IEAPMNIP TYPE=WTO,PARAM=NWTOMHDR
*       /*************************************************************/
*       /*                                                           */
*       /* PUT SYSTEM INTO WAIT STATE IF MESSAGE ID SO INDICATES     */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0181
*       IF MSGIDTYP=SYSWAIT×MSGID=NORSTID THEN                     0181
         CLI   MSGIDTYP(MSGPLPTR),C'W'                             0181
         BE    @RT00181                                            0181
         CLC   MSGID(4,MSGPLPTR),NORSTID                           0181
         BNE   @RF00181                                            0181
@RT00181 DS    0H                                                  0182
*         GEN(IEAPMNIP TYPE=SWAIT)                                 0182
*         REFS(NVTSWAIT);                                          0182
         IEAPMNIP TYPE=SWAIT
*       ELSE                                                       0183
*         ;                                                        0183
@RF00181 DS    0H                                                  0184
*     END;                                                         0184
*RIMMEXIT:                                                         0185
*                                                                  0185
*   /*****************************************************************/
*   /*                                                               */
*   /* EXIT -- RETURN TO CALLER                                      */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0185
*   RETURN;                                                        0185
@EL00001 L     @13,4(,@13)                                         0185
@EF00001 DS    0H                                                  0185
@ER00001 LM    @14,@12,12(@13)                                     0185
         BR    @14                                                 0185
*                                                                  0186
*/********************************************************************/
*/*                                                                  */
*/* SYNCHK:    ROUTINE OF ILRIMMSG TO VALIDATE OPERATOR REPLY TO MSG */
*/*                                                                  */
*/*                                                                  */
*/* INPUT:     ARRAY OF VALID OPERATOR REPLIES                       */
*/*            SET OF FLAGS CORRESPONDING TO REPLY ARRAY ENTRIES     */
*/*                                                                  */
*/* PROCESS:   LOOP COMPARES OPERATOR REPLY WITH EACH ENTRY IN THE   */
*/*            REPLY ARRAY FOR WHICH THE CORRESPONDING FLAG IS SET   */
*/*            ON (SET BY MSGNNND SUBROUTINE).                       */
*/*            IF A VALID MATCH IS FOUND, THE WTOR MESSAGE CONDITION */
*/*            IS SET OFF.  OTHERWISE, A MESSAGE OF 'INVALID REPLY'  */
*/*            IS MOVED TO THE MESSAGE BUFFER WHICH THE WTOR LOOP    */
*/*            IN THE ILRIMMSG SEGMENT WILL CAUSE TO HAVE WRITTEN.   */
*/*                                                                  */
*/* OUTPUT:    RESET WTOR MESSAGE CONDITION, IF VALID REPLY OBTAINED */
*/*            ALTERED MESSAGE BUFFER, IF VALID REPLY NOT OBTAINED   */
*/*                                                                  */
*/********************************************************************/
*/*SYNCHK:   CHART                                                   */
*/* FOOTING                                                        0186
*/*                         ILRIMMSG           OS/VS 2 RELEASE 2:  0186
*/*                                                JANUARY 28, 1974  */
*/*SYNCHK:   E    ENTRY TO VALIDATE OPERATOR REPLY                   */
*/*          N    LOOP THRU VALID REPLY ARRAY                        */
*/*%SCK10:   D (YES,,NO,%SCK50) CONTINUE LOOPING ?                   */
*/*          N    LOOP CONTINUES UNTIL 1) END OF ARRAY IS REACHED OR */
*/*          N    2) UNTIL A VALID MATCH IS FOUND (ARRAY AND       0186
*/*               OPER REPLY                                         */
*/*          D (YES,,NO,%SCK30) TRY TO MATCH THIS ENTRY ?            */
*/*          D (YES,,NO,%SCK30) OPER REPLY MATCH ENTRY ?             */
*/*          P    INDICATE VALID REPLY RECEIVED                      */
*/*%SCK30:   N (,%SCK10) END OF LOOP THRU VALID REPLY ARRAY          */
*/*%SCK50:   D (YES,,NO,%SCK90) WTOR MSG COND STILL ON ?             */
*/*          P    MOVE 'INVALID REPLY' TO MESSAGE BUFFER             */
*/*%SCK90:   R    RETURN TO ILRIMMSG                                 */
*/*SYNCHK:   END                                                     */
*                                                                  0186
*SYNCHK:                            /* VALIDATE OPERATOR REPLY       */
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);                          0186
         B     @EL00001                                            0186
SYNCHK   DS    0H                                                  0187
*   RFY                                                            0187
*    (R2,                                                          0187
*     R3,                                                          0187
*     R14) RSTD;                    /* RESTRICT NVT,CVT AND RETURN 0187
*                                      ADDRESS REGISTERS             */
*                                                                  0187
*   /*****************************************************************/
*   /*                                                               */
*   /* LOOP THROUGH VALID REPLY ARRAY, TEST REPLY IF CORRESPONDING   */
*   /* REPLY FLAG IS SET.                                            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0188
*   VALMASK=WORDBIT1;               /* INITLZ VAL REPLY FLAG MASK    */
         L     VALMASK,WORDBIT1                                    0188
*   DO WRKCTR=ONE TO DIM(VALRTENT) WHILE(VALREPLY=NO);             0189
         LA    WRKCTR,1                                            0189
@DL00189 TM    VALREPLY,B'01000000'                                0189
         BNZ   @DC00189                                            0189
*     IF VALFLGS=(VALFLGS×VALMASK) THEN/* TEST THIS VALID REPLY ?    */
*                                                                  0190
         L     @15,VALFLGS                                         0190
         LR    @04,@15                                             0190
         OR    @04,VALMASK                                         0190
         CLR   @15,@04                                             0190
         BNE   @RF00190                                            0190
*       /*************************************************************/
*       /*                                                           */
*       /* TEST FOR VALID REPLY                                      */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0191
*       IF MSGREPLY(ONE:VALREPLS(WRKCTR))=VALREPS(WRKCTR) THEN     0191
         LR    @15,WRKCTR                                          0191
         SLA   @15,3                                               0191
         SLR   @04,@04                                             0191
         IC    @04,VALREPLS-8(@15)                                 0191
         BCTR  @04,0                                               0191
         LA    @01,VALREPS-8(@15)                                  0191
         EX    @04,@SC00854                                        0191
         BNE   @RF00191                                            0191
*         VALREPLY=YES;             /* INDICATE MATCH FOUND          */
         OI    VALREPLY,B'01000000'                                0192
*       ELSE                                                       0193
*         ;                                                        0193
@RF00191 DS    0H                                                  0194
*     ELSE                                                         0194
*       ;                                                          0194
@RF00190 DS    0H                                                  0195
*     VALMASK=VALMASK/SHIFTR1;      /* SHIFT MASK TO TEST NEXT FLG   */
         SRL   VALMASK,1                                           0195
*   END;                                                           0196
*                                                                  0196
         AL    WRKCTR,@CF00055                                     0196
         C     WRKCTR,@CF00067                                     0196
         BNH   @DL00189                                            0196
@DC00189 DS    0H                                                  0197
*   /*****************************************************************/
*   /*                                                               */
*   /* IF INVALID REPLY, MOVE MESSAGE TO OUTPUT BUFFER               */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0197
*   IF VALREPLY=NO THEN             /* NO VALID REPLY FOUND ?        */
         TM    VALREPLY,B'01000000'                                0197
         BNZ   @RF00197                                            0197
*     MSGOBUF=BADREPLY;             /* MOVE 'INVALID REPLY' TO OUTPUT
*                                      BUFFER                        */
         MVI   MSGOBUF+33,C' '                                     0198
         MVC   MSGOBUF+34(38),MSGOBUF+33                           0198
         MVC   MSGOBUF(33),BADREPLY                                0198
*   ELSE                                                           0199
*     ;                                                            0199
@RF00197 DS    0H                                                  0200
*   END SYNCHK;                     /* END OF SUBROUTINE SYNCHK      */
@EL00002 DS    0H                                                  0200
@EF00002 DS    0H                                                  0200
@ER00002 BR    @14                                                 0200
*                                                                  0201
*/********************************************************************/
*/*                                                                  */
*/* MSGNNNT: GENERIC FORMAT OF MESSAGE BUILDING SUBROUTINE           */
*/*                                                                  */
*/*                                                                  */
*/* INPUT:   PARAMETER LIST INPUT TO ILRIMMSG MAIN SEGMENT           */
*/*                                                                  */
*/* PROCESS: SINCE THERE IS A UNIQUE SUBROUTINE FOR ALL MESSAGES,    */
*/*          NO TESTS ARE NECESSARY, ONLY A SERIES OF                */
*/*          MOVES OF MSG DATA TO THE MESSAGE BUFFER.                */
*/*                                                                  */
*/*          MOST OF ANY MESSAGE IS FIXED.  HOWEVER, IN SOME CASES   */
*/*          A DATA SET NAME OR OTHER VARIABLE MAY BE REQUIRED. FOR  */
*/*          THIS REASON, THE POINTERS CONTAINED IN THE PARAMETER    */
*/*          LIST FROM THE CALLER WILL POINT TO VARYING DATA         */
*/*          DEPENDING ON THE SPECIFIC MESSAGE.  THESE POINTERS      */
*/*          ADDRESS THE ADDITIONAL INFORMATION, LOCATED IN THE      */
*/*          CALLING MODULE, REQUIRED TO COMPLETE BUILDING THE       */
*/*          MESSAGE.                                                */
*/*                                                                  */
*/*          IF THE MESSAGE IS MULTIPLE LINES, THE FIRST LINE(S)     */
*/*          IS (ARE) WRITTEN VIA NIPWTO (SEE SUBROUTINE WTOMSGC).   */
*/*          THE FINAL LINE OF THE MESSAGE IS SET UP TO BE WRITTEN   */
*/*          FROM THE MAIN ROUTINE (ILRIMMSG).                       */
*/*                                                                  */
*/*          IF THE MESSAGE THE SUBROUTINE BUILDS IS A TYPE 'D'      */
*/*          THE SUBROUTINE ALSO SETS A SERIES OF FLAGS              */
*/*          CORRESPONDING TO ENTRIES IN A 'VALID REPLY' ARRAY.      */
*/*          THIS IS DONE SO THAT THE SYNCHK SUBROUTINE WILL KNOW    */
*/*          WHICH VALID REPLIES ARE DESIRED FROM THE OPERATOR.      */
*/*                                                                  */
*/*          IF THE MESSAGE THE SUBROUTINE BUILDS IS A TYPE 'W'      */
*/*          MESSAGE, THE WAIT STATE CODE CORRESPONDING TO THE       */
*/*          ERROR CONDITION IS PLACED INTO THE NVT PRIOR TO         */
*/*          RETURNING TO ILRIMMSG.                                  */
*/*                                                                  */
*/* OUTPUT:  NONE SPECIFICALLY TO ILRIMMSG, MESSAGE BUFFER NOW       */
*/*          CONTAINS MESSAGE.                                       */
*/*                                                                  */
*/********************************************************************/
*/*MSGNNNT:  CHART                                                   */
*/* HEADER                                                         0201
*/*                        MSGNNNT: GENERIC FORMAT OF AN ILRIMMSG ME
*/*SSAGE BUILDING SUBROUTINE                                PAGE #1  */
*/* FOOTING                                                        0201
*/*                         ILRIMMSG           OS/VS 2 RELEASE 2:  0201
*/*                                                JANUARY 28, 1974  */
*/*MSGNNNT:  E    ENTRY TO BUILD MESSAGE                             */
*/*          N    NEXT 2 STEPS ARE REPEATED FOR 1ST LINE(S) OF     0201
*/*               MULTIPLE LINE MSGS                                 */
*/*          P    MOVE REQUIRED MSG TEXT FROM DATA AREA TO MSG     0201
*/*               BUFFER                                             */
*/*          S WTOMSGC: WTO LINE, BLANK MSG AREA                     */
*/*          P    MOVE MSG (OR FINAL LINE, IF MULTIPLE) TO MSGAREA   */
*/*          N    INCLUDE ANY DATA ADDRESSED IN PARAMETER LIST       */
*/*          N    ALSO SET VALID REPLY FLAGS FOR SYNCHK IF 'D'     0201
*/*               TYPE MESSAGE                                       */
*/*          N    ALSO MOVE ASSOCIATED WAIT STATE CODE TO NVT IF 'W'
*/*               TYPE MSG                                           */
*/*          R    RETURN TO ILRIMMSG TO WTO(R) MESSAGE               */
*/*MSGNNNT:  END                                                     */
