*MSG916E:                           /* BUILD MESSAGE IEA916E         */
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);                          0209
*                                                                  0209
MSG916E  DS    0H                                                  0210
*   /*****************************************************************/
*   /*                                                               */
*   /* BUILD MESSAGE IN OUTPUT AREA                                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0210
*   MSGOTEXT(1:16)=SYNTXERR;        /* 1ST MSG PART                  */
         MVC   MSGOTEXT(16),SYNTXERR                               0210
*   IF DUPEQ=YES×PAGEN=YES THEN                                    0211
         TM    DUPEQ(MSGPLPTR),B'00110000'                         0211
         BZ    @RF00211                                            0211
*     DO;                           /* 6 CHAR PARM?                  */
*       IF DUPEQ=YES THEN                                          0213
         TM    DUPEQ(MSGPLPTR),B'00100000'                         0213
         BNO   @RF00213                                            0213
*         MSGOTEXT(17:22)=DUPLEX;                                  0214
         MVC   MSGOTEXT+16(6),DUPLEX                               0214
*       ELSE                                                       0215
*         MSGOTEXT(17:22)=PAGNUM;   /* PUT IN PAGNUM                 */
         B     @RC00213                                            0215
@RF00213 MVC   MSGOTEXT+16(6),PAGNUM                               0215
