@DATA    DS    0H
@DATD    DSECT
         DS    0F
IGFPMTHA CSECT
         DS    0F
@CF01716 DC    BL4'00000000000000000000000010000000'
@CF01720 DC    BL4'00000000000000000000000000010000'
@DATD    DSECT
         DS    0D
         ORG   *+1-(*-@DATD)/(*-@DATD) INSURE DSECT DATA
@ENDDATD EQU   *
IGFPMTHA CSECT
         DS    0F
@SIZDATD DC    AL1(0)
         DC    AL3(@ENDDATD-@DATD)
         DS    0D
FRSDEL   DC    F'-1'
FRSADD   DS    CL8
         ORG   FRSADD
