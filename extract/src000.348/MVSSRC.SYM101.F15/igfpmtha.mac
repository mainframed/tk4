         TITLE 'IGFPMTHA - MACHINE CHECK THRESHOLD ANALYSIS            *
                        '
IGFPMTHA CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'IGFPMTHA  78.053'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @12,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@12                                         0001
*                                                                  0075
*/************************************************************       */
*/*                   STANDARD  LINKAGE                              */
*/************************************************************       */
*                                                                  0075
*   FWDPTR=ADDR(PWASA3);            /* CHAIN FOWARD POINTER          */
         L     @07,PCCAPTR                                         0075
         L     @07,PWAPTR(,@07)                                    0075
         LA    @15,PWASA3(,@07)                                    0075
         ST    @15,FWDPTR(,R13)                                    0075
*   ADDR(PWASA3)->BACKPTR=R13;      /* CHAIN BACK PTR THIS RTN       */
         ST    R13,BACKPTR+144(,@07)                               0076
*   R13=ADDR(PWASA3);               /* LOAD SAVE AREA POINTER        */
         LR    R13,@15                                             0077
*/************************************************************       */
*/*     SAVE THB AND MSG POINTERS AND SET UP FRR VECTOR              */
*/************************************************************       */
*                                                                  0078
*   PRMREG6=R1;                     /* SAVE INPUT POINTERS           */
         LR    PRMREG6,R1                                          0078
*   R1=ADDR(FRSADD);                /* ADD TO FRS STACK              */
         LA    R1,FRSADD                                           0079
*   CALL RVTFRS;                    /* GO ADJUST VECTOR TABLE        */
         L     @07,CVTPTR                                          0080
         L     @07,RVTPTR(,@07)                                    0080
         L     @15,RVTPMFRS(,@07)                                  0080
         BALR  @14,@15                                             0080
*/************************************************************       */
*/*   THIS ROUTINE SAVES THE THRESHOLD BLOCK AND INTERROGATES        */
*/*   IT TO DETERMINE IF A MESSAGE IS TO BE WRITTEN OR NOT.          */
*/*   BITS ARE SET TO INDICATE A MESSAGE OR NOT.                     */
*/************************************************************       */
*                                                                  0081
*LOOP:                                                             0081
*   ;                                                              0081
LOOP     DS    0H                                                  0082
*   R2=THB(1:4);                    /* LOAD FIRST HALF OF THB IN REG */
         L     @07,THBPTR(,PRMREG6)                                0082
         ICM   R2,15,THB(@07)                                      0082
*   R3=THB(5:8);                    /* LOAD SECOND HALF OF THB IN REG*/
         ICM   R3,15,THB+4(@07)                                    0083
*   MSGVAR1(1:4)=THB(1:4);          /* SAVE THRESHOLD STATUS         */
         L     @15,MSGPTR(,PRMREG6)                                0084
         MVC   MSGVAR1(4,@15),THB(@07)                             0084
*   MSGVAR2(1:4)=THB(5:8);          /* SAVE THRESHOLD COUNT          */
         MVC   MSGVAR2(4,@15),THB+4(@07)                           0085
*/********************************************************************/
*/*                 INCREMENT THRESHOLD COUNTER                      */
*/*                                                                  */
*/*  FOR HARD MACHINE CHECKS, INCREMENT COUNTER                      */
*/*  IF TIMING INTERVAL HAS NOT ELAPSED OR TOD CLOCK WAS NO GOOD.    */
*/*  OTHERWISE RESET COUNTER TO ZERO BEFORE INCREMENTING,RESET       */
*/*  BEGINNING OF TIMING INTERVAL.                                   */
*/*                                                                  */
*/*  FOR SOFT ERRORS INCREMENT COUNTER.                              */
*/*                                                                  */
*/*********************************************************  @G55CP2P*/
*                                                                  0086
*   IF THBEXT='1'B                  /* HARD THRESHOLD BLOCK ?      0086
*                                                            @G55CP2P*/
*     THEN                                                         0086
         TM    THBEXT(@07),B'00010000'                             0086
         BNO   @RF00086                                            0086
*     IF(LRBHDATE-THBOTIME>         /* YES - SEE IF INTERVAL @G55CP2P*/
*         THBNTRVL)&                /* HAS ELAPSED AND       @G55CP2P*/
*         LRBHDATE^=0               /* TOD CLOCK GOOD        @G55CP2P*/
*       THEN                                                       0087
         L     @14,PCCAPTR                                         0087
         L     @14,LRBPTR(,@14)                                    0087
         L     @11,LRBHDATE(,@14)                                  0087
         LR    @00,@11                                             0087
         SL    @00,THBOTIME(,@07)                                  0087
         L     @10,THBNTRVL-1(,@07)                                0087
         LA    @10,0(,@10)                                         0087
         CLR   @00,@10                                             0087
         BNH   @RF00087                                            0087
         SLR   @10,@10                                             0087
         CLR   @11,@10                                             0087
         BE    @RF00087                                            0087
*       DO;                         /* YES - SET COUNT TO 0 RESET  0088
*                                      BEGINNING OF TIMING INTERVAL
*                                                            @G55CP2P*/
*         MSGVAR1(2:4)=0;           /*                       @G55CP2P*/
         STCM  @10,7,MSGVAR1+1(@15)                                0089
*         THBOTIME=LRBHDATE;        /*                       @G55CP2P*/
         MVC   THBOTIME(4,@07),LRBHDATE(@14)                       0090
*       END;                        /*                       @G55CP2P*/
*   MSGVAR1(2:4)=MSGVAR1(2:4)+1;    /* INCREMENT COUNTER     @G55CP2P*/
@RF00087 DS    0H                                                  0092
@RF00086 L     @07,MSGPTR(,PRMREG6)                                0092
         LA    @00,1                                               0092
         SLR   @15,@15                                             0092
         ICM   @15,7,MSGVAR1+1(@07)                                0092
         ALR   @00,@15                                             0092
         STCM  @00,7,MSGVAR1+1(@07)                                0092
*/************************************************************       */
*/*  DETERMINE IF A THRESHOLD OF DEGRADATION MACHINE CHECKS          */
*/*  HAS OCCURED, BASED ON BUFFER ENABLEMENT THRESHOLD. IF           */
*/*  SO, AND THE MODEL SUPPORTS BUFFER ENABLEMENT, THEN SET          */
*/*  BIT IN PWA TO REQUEST ENABLEMENT.                               */
*/************************************************************       */
*                                                                  0093
*   IF LRBMFDG='1'B THEN            /* CHECK FOR DEGRAD.MACH.CK.   0093
*                                                            @G556P2P*/
         L     @07,PCCAPTR                                         0093
         L     @15,LRBPTR(,@07)                                    0093
         TM    LRBMFDG(@15),B'00000001'                            0093
         BNO   @RF00093                                            0093
*     IF PCCACPID(9:12)=MOD165×     /* IF A 165 OR           @G556P2P*/
*         PCCACPID(9:12)=MOD168×    /* 168                   @G556P2P*/
*         PCCACPID(9:12)=M168AP×    /* OR 168AP              @G556P2P*/
*         PCCACPID(9:12)=M3032×     /* OR 3032               @ZA30079*/
*         PCCACPID(9:12)=M3033 THEN /* OR 3033 THEN          @ZM45593*/
         CLC   PCCACPID+8(4,@07),MOD165                            0094
         BE    @RT00094                                            0094
         CLC   PCCACPID+8(4,@07),MOD168                            0094
         BE    @RT00094                                            0094
         CLC   PCCACPID+8(4,@07),M168AP                            0094
         BE    @RT00094                                            0094
         CLC   PCCACPID+8(4,@07),M3032                             0094
         BE    @RT00094                                            0094
         CLC   PCCACPID+8(4,@07),M3033                             0094
         BNE   @RF00094                                            0094
@RT00094 DS    0H                                                  0095
*       IF THBENABL^=0 THEN         /* IF THRESHOLD DEFINED, @G55CP2P*/
         L     @07,THBPTR(,PRMREG6)                                0095
         CLI   THBENABL(@07),0                                     0095
         BE    @RF00095                                            0095
*         IF MSGVAR1(2:4)//THBENABL=0/* CHECK INTEGRAL MULTIPLE    0096
*                                      BUFFER THRESHOLD      @G556P2P*/
*           THEN                                                   0096
         L     @15,MSGPTR(,PRMREG6)                                0096
         SLR   @09,@09                                             0096
         ICM   @09,7,MSGVAR1+1(@15)                                0096
         SLR   @08,@08                                             0096
         SLR   @00,@00                                             0096
         IC    @00,THBENABL(,@07)                                  0096
         DR    @08,@00                                             0096
         LTR   @08,@08                                             0096
         BNZ   @RF00096                                            0096
*           PWAENABL='1'B;          /* YES, SET BIT FOR BUFFER/TLB 0097
*                                      ENABLE                @G556P2P*/
         L     @07,PCCAPTR                                         0097
         L     @07,PWAPTR(,@07)                                    0097
         OI    PWAENABL(@07),B'00010000'                           0097
*/************************************************************       */
*/*  DETERMINE IF A THRESHOLD OF MACHINE CHECKS HAS OCCURED          */
*/*  BASED ON RECORDING THRESHOLD. IF SO TAKE THE PROPER             */
*/*  ACTION, BASED ON THE TYPE OF MACHINE CHECK BEING                */
*/*  PROCESSED.                                                      */
*/************************************************************       */
*                                                                  0098
*   IF MSGVAR1(2:4)//THBTHR=0       /* CHECK INTEGRAL MULTIPLE       */
*     THEN                                                         0098
@RF00096 DS    0H                                                  0098
@RF00095 DS    0H                                                  0098
@RF00094 DS    0H                                                  0098
@RF00093 L     @07,MSGPTR(,PRMREG6)                                0098
         L     @15,THBPTR(,PRMREG6)                                0098
         SLR   @09,@09                                             0098
         ICM   @09,7,MSGVAR1+1(@07)                                0098
         SLR   @08,@08                                             0098
         L     @07,THBTHR-1(,@15)                                  0098
         LA    @07,0(,@07)                                         0098
         DR    @08,@07                                             0098
         LTR   @08,@08                                             0098
         BNZ   @RF00098                                            0098
*     DO;                           /*                       @G556P2P*/
*/********************************************************************/
*/*   THE FOLLOWING SECTION OF CODE PROCESSES HARD                   */
*/*   THRESHOLDS.                                                    */
*/*           - - - - - - - - - - - - - - - - - - - - - - -          */
*/*                                                                  */
*/*   SET BIT IN LRB (LRBMTTHR) TO TELL MAINLINE                     */
*/*   MCH TO CALL IGFPTERM TO TAKE THIS PROCESSOR OFFLINE            */
*/*   UNLESS RECORD MODE IS RECORD=ALL                               */
*/***********************************************************@G55CP2P*/
*                                                                  0100
*       IF THBEXT='1'B              /* Q. HARD THRESHOLD     @G55CP2P*/
*         THEN                                                     0100
         TM    THBEXT(@15),B'00010000'                             0100
         BNO   @RF00100                                            0100
*         IF THBREC='0'B            /* YES - SET TERM BIT IF RECORD
*                                      MODE NOT RECORD=ALL   @G55CP2P*/
*           THEN                                                   0101
         TM    THBREC(@15),B'01000000'                             0101
         BNZ   @RF00101                                            0101
*           LRBMTTHR='1'B;          /*                       @G55CP2P*/
         L     @07,PCCAPTR                                         0102
         L     @07,LRBPTR(,@07)                                    0102
         OI    LRBMTTHR(@07),B'00100000'                           0102
*         ELSE                                                     0103
*           ;                       /*                       @G55CP2P*/
@RF00101 DS    0H                                                  0104
*/***********************************************************@G55CP2P*/
*/* THIS SECTION OF CODE PROCESSES SOFT MACHINE CHECK THRESHOLDS     */
*/*    - - - - - - - - - - - - - - - - - - - - - - - - - - -         */
*/*                                                                  */
*/*IF SYSTEM RECOVERY OR DEGRADATION THRESHOLD, SET PROPER           */
*/*MODE AND SCHEDULE MESSAGE                                         */
*/***********************************************************@G55CP2P*/
*                                                                  0104
*       ELSE                                                       0104
*         DO;                       /*                       @G55CP2P*/
         B     @RC00100                                            0104
@RF00100 DS    0H                                                  0105
*           IF THBREC='1'B          /* CHECK IF RECORD=ALL           */
*             THEN                                                 0105
         L     @07,THBPTR(,PRMREG6)                                0105
         TM    THBREC(@07),B'01000000'                             0105
         BNO   @RF00105                                            0105
*             MSGTAB2='04'X;        /* YES - RECORD MODE             */
         L     @07,MSGPTR(,PRMREG6)                                0106
         MVI   MSGTAB2(@07),X'04'                                  0106
*           ELSE                                                   0107
*             DO;                                                  0107
         B     @RC00105                                            0107
@RF00105 DS    0H                                                  0108
*               MSGVAR1(1:1)='80'X; /* YES - SET QUIET MODE          */
         L     @07,MSGPTR(,PRMREG6)                                0108
         MVI   MSGVAR1(@07),X'80'                                  0108
*               MSGTAB2='03'X;      /* INDICATE QUIET MODE           */
         MVI   MSGTAB2(@07),X'03'                                  0109
*             END;                                                 0110
*         END;                      /*                       @G55CP2P*/
*     END;                                                         0112
*   ELSE                                                           0113
*     MSGNUM=0;                     /* INDICATE NO MESSAGE REQUIRED  */
         B     @RC00098                                            0113
@RF00098 L     @07,MSGPTR(,PRMREG6)                                0113
         MVI   MSGNUM(@07),X'00'                                   0113
*/************************************************************       */
*/*   THIS ROUTINE SETS UP A CDS(COMPARE AND SWAP) SO THAT           */
*/*   THE  MODIFIED THB CAN BE STORED IN THE PROPER PROCESSOR.       */
*/************************************************************       */
*                                                                  0114
*   R4=MSGVAR1;                     /* SET UP REGISTERS              */
@RC00098 L     @07,MSGPTR(,PRMREG6)                                0114
         ICM   R4,15,MSGVAR1(@07)                                  0114
*   R5=MSGVAR2;                     /* FOR CDS INSTRUCTION           */
         ICM   R5,15,MSGVAR2(@07)                                  0115
*   CDS(R2,R4,THBBASE);             /* EXECUTE COMPARE AND SWAP      */
         L     @07,THBPTR(,PRMREG6)                                0116
         CDS   R2,R4,THBBASE(@07)                                  0116
*   BC(7,LOOP);                     /* TRY AGAIN                     */
         BC    7,LOOP                                              0117
*/************************************************************       */
*/*   THIS ROUTINE CHECKS TO SEE IF A MESSAGE IS TO BE               */
*/*   WRITTEN.  IF A MESSAGE IS TO BE WRITTEN                        */
*/*   WE CALL THE MESSAGE ROUTINE TO WRITE THE MESSAGE.              */
*/*   MSGNUM WILL ALWAYS CONTAIN THE CORRECT MESSAGE NUMBER          */
*/*   WHEN THIS MODULE GETS CONTROL, IF A MESSAGE IS TO BE           */
*/*   WRITTEN.                                                       */
*/************************************************************       */
*                                                                  0118
*   IF MSGNUM^=0                    /* CHECK IF MESSAGE IS REQUIRED
*                                                            @G556P2P*/
*     THEN                                                         0118
         L     @07,MSGPTR(,PRMREG6)                                0118
         CLI   MSGNUM(@07),0                                       0118
         BE    @RF00118                                            0118
*     DO;                           /*                       @G556P2P*/
*       MSGVAR2=PCCACPUA;           /* YES - GET CPU NUMBER FROM PCCA*/
         L     @15,PCCAPTR                                         0120
         LH    @00,PCCACPUA(,@15)                                  0120
         STCM  @00,15,MSGVAR2(@07)                                 0120
*       MSGVAR1(1:1)='00'X;         /* ZERO FIRST BYTE OF MSG STATUS */
         MVI   MSGVAR1(@07),X'00'                                  0121
*       R1=MSGADR;                  /* LOAD REG1 WITH MESSAGE BUF  0122
*                                      ADDR                          */
         L     R1,MSGADR(,PRMREG6)                                 0122
*       CALL RVTMSG;                /* CALL MESSAGE ROUTINE          */
         L     @07,CVTPTR                                          0123
         L     @07,RVTPTR(,@07)                                    0123
         L     @15,RVTPMMSG(,@07)                                  0123
         BALR  @14,@15                                             0123
*/************************************************************       */
*/*  UPON RETURN FROM THE MESSAGE WRITER THE RETURN CODE IS          */
*/*  CHECKED. IF THE RETURN CODE IS NOT ZERO WE SET AN               */
*/*  INDICATOR IN THE RWA TO INDICATE AN ERROR.                      */
*/************************************************************       */
*                                                                  0124
*       IF R15^=0                   /* CHECK RETURN CODE FOR NONZERO
*                                                            @G556P2P*/
*         THEN                                                     0124
         LTR   R15,R15                                             0124
         BZ    @RF00124                                            0124
*         MESS31='1'B;              /* SET ERROR INDICATOR BIT       */
         L     @15,CVTPTR                                          0125
         L     @15,RVTPTR(,@15)                                    0125
         L     @15,RWAPTR(,@15)                                    0125
         L     @07,MESS31-3(,@15)                                  0125
@GL00006 LR    @00,@07                                             0125
         O     @00,@CF01716                                        0125
         CS    @07,@00,MESS31-3(@15)                               0125
         BNE   @GL00006                                            0125
*     END;                          /*                       @G556P2P*/
@RF00124 DS    0H                                                  0127
*/********************************************************************/
*/* ISSUE BUFFER RESTORE MESSAGE IF REQUIRED                 @G556P2P*/
*/********************************************************************/
*                                                                  0127
*   IF PWAENABL='1'B                /* IS BUFFER ENABLE MESSAGE REQD
*                                                            @G556P2P*/
*     THEN                                                         0127
@RF00118 L     @07,PCCAPTR                                         0127
         L     @15,PWAPTR(,@07)                                    0127
         TM    PWAENABL(@15),B'00010000'                           0127
         BNO   @RF00127                                            0127
*     DO;                           /* YES                   @G556P2P*/
*       MSG=MSG&&MSG;               /* CLEAR MESSAGE         @G556P2P*/
         L     @15,MSGPTR(,PRMREG6)                                0129
         XC    MSG(12,@15),MSG(@15)                                0129
*       MSGNUM=IGF956I;             /* BUILD MESSAGE         @G556P2P*/
         MVI   MSGNUM(@15),X'56'                                   0130
*       MSGVAR1=PCCACPUA;           /* BUFFER                @G556P2P*/
         LH    @07,PCCACPUA(,@07)                                  0131
         STCM  @07,15,MSGVAR1(@15)                                 0131
*       R1=MSGADR;                  /* LOAD REG1 WITH MESSAGE BUF  0132
*                                      ADDR                  @G556P2P*/
         L     R1,MSGADR(,PRMREG6)                                 0132
*       CALL RVTMSG;                /* LINK TO MESSAGE SCHEDULER   0133
*                                                            @G556P2P*/
         L     @07,CVTPTR                                          0133
         L     @07,RVTPTR(,@07)                                    0133
         L     @15,RVTPMMSG(,@07)                                  0133
         BALR  @14,@15                                             0133
*       IF R15^=0                   /* CHECK RETURN CODE FROM MESSAGE
*                                                            @G55CP2P*/
*         THEN                      /* SCHEDULER - IF NONZERO      0134
*                                                            @G556P2P*/
         LTR   R15,R15                                             0134
         BZ    @RF00134                                            0134
*         RWAMSG56='1'B;            /* SET MESSAGE LOST INDICATOR  0135
*                                                            @G556P2P*/
         L     @07,CVTPTR                                          0135
         L     @07,RVTPTR(,@07)                                    0135
         L     @07,RWAPTR(,@07)                                    0135
         OI    RWAMSG56(@07),B'00001000'                           0135
*     END;                          /*                       @G556P2P*/
@RF00134 DS    0H                                                  0137
*/********************************************************************/
*/*  THIS ROUTINE SETS UP                                            */
*/*  TO ADJUST THE FRR STACK AND RETURN TO THE CALLER.               */
*/********************************************************************/
*                                                                  0137
*MCEXIT:                                                           0137
*   ;                                                              0137
@RF00127 DS    0H                                                  0137
MCEXIT   DS    0H                                                  0138
*   R1=ADDR(FRSDEL);                /* SET UP TO DELETE FROM STACKER */
         LA    R1,FRSDEL                                           0138
*   CALL RVTFRS;                    /* GO ADJUST VECTOR TABLE        */
         L     @07,CVTPTR                                          0139
         L     @07,RVTPTR(,@07)                                    0139
         L     @15,RVTPMFRS(,@07)                                  0139
         BALR  @14,@15                                             0139
*   R13=BACKPTR;                    /* RESTORE REG 13 SAVE AREA      */
         L     R13,BACKPTR(,R13)                                   0140
*   RETURN;                         /* RETURN TO CALLER              */
@EL00001 DS    0H                                                  0141
@EF00001 DS    0H                                                  0141
@ER00001 LM    @14,@12,12(@13)                                     0141
         BR    @14                                                 0141
*/************************************************************       */
*/*   THIS IS THE FRR ROUTINE OF IGFPMTHA. THIS ROUTINE IS           */
*/*   ENTERED WHEN A PROGRAM CHECK OR A MACHINE CHECK OCCURS         */
*/*   WHILE IGFPMTHA IS IN PROCESS. IT MERELY SETS AN                */
*/*   INDICATOR BIT IN THE RWA AND RETURNS.                          */
*/************************************************************       */
*                                                                  0142
*IGFPMTFR:                                                         0142
*   ;                                                              0142
IGFPMTFR DS    0H                                                  0143
*   MESSXX='1'B;                    /* MC,PC OR LOOP SET ERROR     0143
*                                      INDICATOR                     */
         L     @15,CVTPTR                                          0143
         L     @15,RVTPTR(,@15)                                    0143
         L     @15,RWAPTR(,@15)                                    0143
         L     @07,MESSXX-3(,@15)                                  0143
@GL00007 LR    @00,@07                                             0143
         O     @00,@CF01720                                        0143
         CS    @07,@00,MESSXX-3(@15)                               0143
         BNE   @GL00007                                            0143
*   R13=BACKPTR;                    /* RESTORE REG 13 SAVE AREA      */
         L     R13,BACKPTR(,R13)                                   0144
*   RETURN;                         /* RETURN TO CALLER              */
         B     @EL00001                                            0145
*   END IGFPMTHA                                                   0146
*                                                                  0146
*/* THE FOLLOWING INCLUDE STATEMENTS WERE FOUND IN THIS PROGRAM.     */
*/*%INCLUDE SYSLIB  (IHALRB  )                                       */
*/*%INCLUDE SYSLIB  (IHACSD  )                                       */
