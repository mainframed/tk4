*   DERPTR=ADDR(ESTPLIST);          /* SET DERPLIST PTR TO THE NEW 0053
*                                      GETMAINED AREA                */
         LA    @12,ESTPLIST(,ESTAIPTR)                             0053
         LR    DERPTR,@12                                          0053
*   DERPERK='1'B;                   /* PERCOLATE             @YM02581*/
         OI    DERPERK(DERPTR),B'01000000'                         0054
*   DERREC=ADDR(DERRECPM);          /*                       @YM02581*/
