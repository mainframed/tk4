*     RESPECIFY                                                    0049
*      (GPR01F,                                                    0049
*       GPR00F)UNRESTRICTED;                                       0049
*   END;                            /* GETMAIN (R)                 0050
*                                      LV(LENGTH(ESTAIARE))SP(245)   */
