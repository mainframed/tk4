         TITLE 'IGFDV1, DDR - COMMON VALIDATE ROUTINE                  *
                        '
IGFDV1   CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'IGFDV1    76.335'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @08,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@08                                         0001
         L     @00,@SIZDATD                                        0001
         GETMAIN  R,LV=(0)
         LR    @09,@01                                             0001
         USING @DATD,@09                                           0001
         ST    @13,@SA00001+4                                      0001
         LM    @00,@01,20(@13)                                     0001
         ST    @09,8(,@13)                                         0001
         LR    @13,@09                                             0001
*   DO;                             /* ?IGFDERR INIT                 */
*                                                                  0150
*     /***************************************************************/
*     /*                                                             */
*     /* IGFDERR - INITIALIZE RECOVERY ENVIRONMENT                   */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0151
*     DERPLIST=DERPLIST&&DERPLIST;  /* CLEAR PARAMETER LIST          */
         XC    DERPLIST(48),DERPLIST                               0151
*     DERFUNK=DERINIT;              /* INDICATE INIT FUNCTION        */
         MVI   DERFUNK,X'01'                                       0152
*     DERREC=ADDR(DERRECPM);        /* POINT TO REC PARMS            */
         LA    @14,DERRECPM                                        0153
         ST    @14,DERREC                                          0153
*     DERRETRY=ADDR(IGFDV1R);       /* POINT TO RETRY ROUTINE        */
         LA    @14,IGFDV1R                                         0154
         ST    @14,DERRETRY                                        0154
*     GEN SETS(DERDATD)(MVC DERDATD,@SIZDATD);/* SET UP SPN, SIZE    */
         MVC DERDATD,@SIZDATD
*     DERPFX=DERRMASK;              /* SET UP RUB MASK               */
         MVC   DERPFX(2),@CB00956                                  0156
*     DERRCODE=CODEBASE;            /* SAVE CODEREG                  */
         ST    CODEBASE,DERRCODE                                   0157
*     DERRDATA=DATABASE;            /* SAVE DATAREG                  */
         ST    DATABASE,DERRDATA                                   0158
*     DERRDDR=DDRPTR;               /* SAVE DDRCOM POINTER           */
         ST    DDRPTR,DERRDDR                                      0159
*     DERRSAVE=SAVEPTR;             /* SAVE REGISTER 13              */
         ST    SAVEPTR,DERRSAVE                                    0160
*     CALL IGFDE0(DERPLIST);        /* ISSUE ESTAE                   */
         LA    @14,DERPLIST                                        0161
         ST    @14,@AL00001                                        0161
         L     @15,@CV00075                                        0161
         LA    @01,@AL00001                                        0161
         BALR  @14,@15                                             0161
*   END;                            /* ?IGFDERR INIT                 */
*   DERLRC=INVALID;                 /* INITIALIZE EXTERNAL RETURN  0163
*                                      CODE                          */
         MVI   DERLRC,X'04'                                        0163
*   IF DDRINV2=DDRVFM THEN          /* TEST FOR 'FROM' DEVICE CHECK  */
         CLI   DDRINV2(DDRPTR),X'01'                               0164
         BNE   @RF00164                                            0164
*     DO;                           /* YES, VALIDATE 'FROM' DEVICE   */
*                                                                  0165
*       /*************************************************************/
*       /*                                                           */
*       /* CHECK THE SPECIFIED 'FROM' DEVICE FOR VALIDITY            */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0166
*       DV1CUA=DDRFMCUA;            /* PICK UP 'FROM' DEVICE ADDRESS */
         MVC   DV1CUA(3),DDRFMCUA(DDRPTR)                          0166
*       CALL IGFDV2;                /* FIND THE UCB FOR THE 'FROM' 0167
*                                      DEVICE                        */
         BAL   @14,IGFDV2                                          0167
*       IF DV2RC^=VALID THEN        /* WAS THE UCB FOUND             */
         C     DV2RC,@CF00037                                      0168
         BE    @RF00168                                            0168
*         DO;                       /* NO, THE SPECIFIED DEVICE DOES
*                                      NOT EXIST                     */
*           DDRINV1=DDRIUF;         /* INDICATE 'UNIT REF INVALID'   */
         MVI   DDRINV1(DDRPTR),X'02'                               0170
*           DDRTER1=DDRTID;         /* INDICATE 'INVALID DEVICE'     */
         MVI   DDRTER1(DDRPTR),X'08'                               0171
*         END;                                                     0172
*       ELSE                        /* YES, THE SPECIFIED DEVICE HAS
*                                      A UCB - CONTINUE TESTING      */
*         DO;                                                      0173
         B     @RC00168                                            0173
@RF00168 DS    0H                                                  0174
*           DDRFMUCB=UCBPTR;        /* PUT UCB ADDRESS INTO DDRCOM   */
         ST    UCBPTR,DDRFMUCB(,DDRPTR)                            0174
*           CALL IGFDV3;            /* LOOK UP DEVICE TYPE IN DDR  0175
*                                      DEVICE CHARACTERISTICS TABLE  */
         BAL   @14,IGFDV3                                          0175
*           IF DV3RC^=VALID THEN    /* WAS DEVICE FOUND IN TABLE     */
         C     DV3RC,@CF00037                                      0176
         BE    @RF00176                                            0176
*             DO;                   /* NO, DEVICE NOT SUPPORTED BY 0177
*                                      DDR                           */
*               DDRINV1=DDRIDT;     /* INDICATE 'INV DEVICE TYPE'    */
         MVI   DDRINV1(DDRPTR),X'07'                               0178
*               DDRTER1=DDRTID;     /* INDICATE 'INVALID DEVICE'     */
         MVI   DDRTER1(DDRPTR),X'08'                               0179
*             END;                                                 0180
*           ELSE                    /* YES, DEVICE FOUND IN TABLE -
*                                      CONTINUE TESTING              */
*             DO;                                                  0181
         B     @RC00176                                            0181
@RF00176 DS    0H                                                  0182
*               DDRDCHAR=DV1DCHAR;  /* COPY DEVICE CHARACTERISTICS TO
*                                      DDRCOM                        */
         MVC   DDRDCHAR(2,DDRPTR),DV1DCHAR                         0182
*               IF UCBNALOC='1'B THEN/* IS DEVICE NOT ALLOCATABLE    */
         TM    UCBNALOC(UCBPTR),B'00000100'                        0183
         BNO   @RF00183                                            0183
*                 DO;               /* YES, DEVICE CANNOT BE SWAPPED */
*                   DDRINV1=DDRINS; /* INDICATE 'NOT SUPPORTED'      */
         MVI   DDRINV1(DDRPTR),X'03'                               0185
*                   DDRTER1=DDRTID; /* INDICATE 'INVALID DEVICE'     */
         MVI   DDRTER1(DDRPTR),X'08'                               0186
*                 END;                                             0187
*               ELSE                /* NO, DEVICE IS NOT UNDER     0188
*                                      CONTROL OF OLTEP - CONTINUE 0188
*                                      TESTING                       */
*                 IF UCBPRES='1'B THEN/* IS DEVICE PERM RESID      0188
*                                                            @OZ09341*/
         B     @RC00183                                            0188
@RF00183 TM    UCBPRES(UCBPTR),B'00000100'                         0188
         BNO   @RF00188                                            0188
*                   DO;             /* YES, DEVICE CAN'T BE SWAPPED
*                                                            @OZ09341*/
*                     DDRINV1=DDRINS;/* INDICATE 'NOT SUPPORTED'     */
         MVI   DDRINV1(DDRPTR),X'03'                               0190
*                     DDRTER1=DDRTID;/* INDICATE 'INVALID DEVICE'    */
         MVI   DDRTER1(DDRPTR),X'08'                               0191
*                   END;                                           0192
*                 ELSE              /* NO, DEVICE IS NOT PERMANENTLY
*                                      RESIDENT - CONTINUE TESTING   */
*                   IF UCB3DACC='1'B&UCB2OPT4='1'B THEN/* AN SS1   0193
*                                      DEVICE                @Y30LKVD*/
         B     @RC00188                                            0193
@RF00188 TM    UCB3DACC(UCBPTR),B'00100000'                        0193
         BNO   @RF00193                                            0193
         TM    UCB2OPT4(UCBPTR),B'00001000'                        0193
         BNO   @RF00193                                            0193
*                     DO;           /* YES,DEVICE CANNOT BE SWAPPED
*                                                            @Y30LKVD*/
*                       DDRINV1=DDRINS;/* INDICATE NOT SUPPORTED   0195
*                                                            @Y30LKVD*/
         MVI   DDRINV1(DDRPTR),X'03'                               0195
*                       DDRTER1=DDRTID;/* INDICATE INVALID DEVICE  0196
*                                                            @Y30LKVD*/
         MVI   DDRTER1(DDRPTR),X'08'                               0196
*                     END;          /*                       @Y30LKVD*/
*                   ELSE            /* NO, DEVICE IS NOT A VIRTUAL 0198
*                                      SS1 DEVICE            @YM0LKVD*/
*                     IF UCBALOC='0'B/* IS DEVICE ALLOCATED          */
*                         ×UCBONLI='0'B THEN/* AND ONLINE            */
         B     @RC00193                                            0198
@RF00193 TM    UCBALOC(UCBPTR),B'10001000'                         0198
         BO    @RF00198                                            0198
*                       DO;         /* NO, DEVICE CANNOT BE SWAPPED  */
*                         DDRINV1=DDRINA;/* INDICATE 'NOT ALLOCATED' */
         MVI   DDRINV1(DDRPTR),X'06'                               0200
*                         DDRTER1=DDRTNF;/* INDICATE 'NO USER FOUND' */
         MVI   DDRTER1(DDRPTR),X'01'                               0201
*                       END;                                       0202
*                     ELSE          /* ALL TESTS HAVE BEEN PASSED -
*                                      THE DEVICE IS ACCEPTABLE      */
*                       DERLRC=VALID;/* RESET THE EXTERNAL RETURN  0203
*                                      CODE TO INDICATE THAT THE   0203
*                                      DEVICE IS VALID TO BE SWAPPED
*                                      FROM                          */
         B     @RC00198                                            0203
@RF00198 MVI   DERLRC,X'00'                                        0203
*             END;                                                 0204
*         END;                                                     0205
*     END;                                                         0206
*   ELSE                            /* NOT 'FROM' VALIDATE REQUEST   */
*     IF DDRINV2=DDRVCM THEN        /* TEST FOR 'TO' VALIDATE REQUEST*/
         B     @RC00164                                            0207
@RF00164 CLI   DDRINV2(DDRPTR),X'02'                               0207
         BNE   @RF00207                                            0207
*       DO;                         /* 'TO' VALIDATE REQUEST         */
*                                                                  0208
*         /***********************************************************/
*         /*                                                         */
*         /* CHECK THE SPECIFIED 'TO' DEVICE FOR VALIDITY AND FOR    */
*         /* COMPATABILITY WITH THE 'FROM' DEVICE                    */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0209
*         DV1CUA=DDRTOCUA;          /* PICK UP 'TO' DEVICE ADDRESS   */
         MVC   DV1CUA(3),DDRTOCUA(DDRPTR)                          0209
*         CALL IGFDV2;              /* FIND THE UCB FOR THE 'TO'   0210
*                                      DEVICE                        */
         BAL   @14,IGFDV2                                          0210
*         IF DV2RC^=VALID           /* WAS THE UCB FOUND             */
*           THEN                    /* NO, THE SPECIFIED DEVICE DOES
*                                      NOT EXIST                     */
         C     DV2RC,@CF00037                                      0211
         BE    @RF00211                                            0211
*           DDRINV1=DDRIUF;         /* INDICATE 'UNIT REF INVALID'   */
         MVI   DDRINV1(DDRPTR),X'02'                               0212
*         ELSE                      /* YES, THE SPECIFIED DEVICE HAS
*                                      A UCB - CONTINUE TESTING      */
*           DO;                                                    0213
         B     @RC00211                                            0213
@RF00211 DS    0H                                                  0214
*             DDRTOUCB=UCBPTR;      /* COPY UCB ADDRESS INTO DDRCOM  */
         ST    UCBPTR,DDRTOUCB(,DDRPTR)                            0214
*             IF DDRTOUCB->UCBTBYT3^=/* ARE THE 'TO' AND THE         */
*                 DDRFMUCB->UCBTBYT3/* 'FROM' DEVICES IN THE SAME  0215
*                                      DEVICE CLASS                  */
*               THEN                /* NO, DEVICES ARE IN DIFFERENT
*                                      CLASSES                       */
         L     @14,DDRTOUCB(,DDRPTR)                               0215
         L     @12,DDRFMUCB(,DDRPTR)                               0215
         CLC   UCBTBYT3(1,@14),UCBTBYT3(@12)                       0215
         BE    @RF00215                                            0215
*               DDRINV1=DDRIIN;     /* INDICATE 'INCOMPATIBLE'       */
         MVI   DDRINV1(DDRPTR),X'04'                               0216
*             ELSE                  /* YES, THE 'TO' AND 'FROM'    0217
*                                      DEVICES ARE IN THE SAME DEVICE
*                                      CLASS                         */
*               IF UCBPRES='1'B THEN/* IS DEVICE PREMANENTLY RESIDENT
*                                                            @OZ15427*/
         B     @RC00215                                            0217
@RF00215 TM    UCBPRES(UCBPTR),B'00000100'                         0217
         BNO   @RF00217                                            0217
*                 DO;               /* YES, DEVICE CAN'T BE SWAPPED
*                                                            @OZ15427*/
*                   DDRINV1=DDRINS; /* INDICATE 'NOT SUPPORTED'      */
         MVI   DDRINV1(DDRPTR),X'03'                               0219
*                   DDRTER1=DDRTID; /* INDICATE 'INVALID DEVICE'     */
         MVI   DDRTER1(DDRPTR),X'08'                               0220
*                 END;                                             0221
*               ELSE                /* IF NOT, CONTINUE      @OZ15427*/
*                 DO;                                              0222
         B     @RC00217                                            0222
@RF00217 DS    0H                                                  0223
*                   CALL IGFDV3;    /* LOOK UP DEVICE TYPE IN DDR  0223
*                                      DEVICE CHARACTERISTICS TABLE  */
         BAL   @14,IGFDV3                                          0223
*                   IF DV3RC^=VALID /* WAS DEVICE FOUND IN TABLE     */
*                     THEN          /* NO, DEVICE NOT SUPPORTED BY 0224
*                                      DDR                           */
         C     DV3RC,@CF00037                                      0224
         BE    @RF00224                                            0224
*                     DDRINV1=DDRIDT;/* INDICATE 'INV DEVICE TYPE'   */
         MVI   DDRINV1(DDRPTR),X'07'                               0225
*                   ELSE            /* YES, 'TO' DEVICE IS SUPPORTED */
*                     IF UCBNALOC='1'B/* IS THE 'TO' DEVICE NOT    0226
*                                      ALLOCATABLE                   */
*                       THEN        /* YES, IT CANNOT BE SWAPPED TO  */
         B     @RC00224                                            0226
@RF00224 TM    UCBNALOC(UCBPTR),B'00000100'                        0226
         BNO   @RF00226                                            0226
*                       DDRINV1=DDRINS;/* INDICATE 'NOT SUPPORTED'   */
         MVI   DDRINV1(DDRPTR),X'03'                               0227
*                     ELSE          /* NO, IT IS STILL VALID         */
*                       IF UCB3DACC='1'B&UCB2OPT4='1'B THEN/* AN SS1
*                                      DEVICE                @Y30LKVD*/
         B     @RC00226                                            0228
@RF00226 TM    UCB3DACC(UCBPTR),B'00100000'                        0228
         BNO   @RF00228                                            0228
         TM    UCB2OPT4(UCBPTR),B'00001000'                        0228
         BNO   @RF00228                                            0228
*                         DO;       /* YES,DEVICE CANNOT BE SWAPPED
*                                                            @Y30LKVD*/
*                           DDRINV1=DDRINS;/* INDICATE NOT SUPPORTED
*                                                            @Y30LKVD*/
         MVI   DDRINV1(DDRPTR),X'03'                               0230
*                           DDRTER1=DDRTID;/* INDICATE INVALID DEVICE
*                                                            @Y30LKVD*/
         MVI   DDRTER1(DDRPTR),X'08'                               0231
*                         END;      /*                       @Y30LKVD*/
*                       ELSE        /* NO, DEVICE IS NOT A VIRTUAL 0233
*                                      SS1 DEVICE            @YM0LKVD*/
*                         DO;                                      0233
*                                                                  0233
         B     @RC00228                                            0233
@RF00228 DS    0H                                                  0234
*                           /*****************************************/
*                           /*                                       */
*                           /* CALL THE RECONFIGURATION SUBROUTINE TO*/
*                           /* DETERMINE WHETHER OR NOT THE 'TO'     */
*                           /* DEVICE HAS AT LEAST ONE GOOD PATH     */
*                           /* AVAILABLE                             */
*                           /*                                       */
*                           /*****************************************/
*                                                                  0234
*                           IF UCBONLI='0'B THEN/* IS THE 'TO' DEVICE
*                                      ONLINE                @Y02BKCI*/
         TM    UCBONLI(UCBPTR),B'10000000'                         0234
         BNZ   @RF00234                                            0234
*                             DO;   /* NO, PATH MUST BE VERIFIED   0235
*                                                            @Y02BKCI*/
*                               PLSTUCB=DDRTOUCB;/* PUT UCB ADDRESS
*                                      INTO PARAMETER LIST           */
         L     @14,DDRTOUCB(,DDRPTR)                               0236
         ST    @14,PLSTUCB                                         0236
*                               PLSTFLAG=FCNUCBCK;/* PUT FUNCTION  0237
*                                      CODE INTO PARAMETER LIST      */
         MVC   PLSTFLAG(4),@CB00050                                0237
*                               DO; /* LINK EP('IEEVDEV')          0238
*                                      PARAM(PLIST)                  */
*                                 I00601='00'X;/* EP/DE FLAG         */
         MVI   I00601,X'00'                                        0239
*                                 I00602=ADDR('IEEVDEV ');/* ADDR OF
*                                      EP/DE                         */
         LA    @14,@CC01656                                        0240
         STCM  @14,7,I00602                                        0240
*                                 I00604=0;/* DCB PTR                */
         SLR   @14,@14                                             0241
         STCM  @14,7,I00604                                        0241
*                                 I00603='00'X;/* ERRET FLAG         */
         MVI   I00603,X'00'                                        0242
*                                 IOPTLIST(1)=ADDR(PLIST);/* ADDR OF
*                                      PROB PROG PARM                */
         LA    @14,PLIST                                           0243
         ST    @14,IOPTLIST                                        0243
*                                 IOPTLIST(1)=IOPTLIST(1)×'80000000'X;
         O     @14,@CF01660                                        0244
         ST    @14,IOPTLIST                                        0244
*                                   /* SET VL BIT                    */
*                                 RESPECIFY                        0245
*                                  (GPR01P) RESTRICTED;            0245
*                                 GPR01P=ADDR(IOPTLIST);/* ADDR OF 0246
*                                      OPTLIST                       */
         LA    GPR01P,IOPTLIST                                     0246
*                                 RESPECIFY                        0247
*                                  (GPR15P) RESTRICTED;            0247
*                                 GPR15P=ADDR(I006);/* ADDR OF LINK
*                                      LIST                          */
         LA    GPR15P,I006                                         0248
*                                 SVC(6);/* ISSUE LINK SVC           */
         SVC   6                                                   0249
*                                 RESPECIFY                        0250
*                                  (GPR01P) UNRESTRICTED;          0250
*                                 RESPECIFY                        0251
*                                  (GPR15P) UNRESTRICTED;          0251
*                               END;/* LINK EP('IEEVDEV')          0252
*                                      PARAM(PLIST)                  */
*                             END;                                 0253
*                           IF UCBONLI='0'B&RTNCODE^=PATHOK THEN/* Q.
*                                      PATH EXISTS TO 'TO' DEVICE  0254
*                                                            @Y02BKCI*/
@RF00234 TM    UCBONLI(UCBPTR),B'10000000'                         0254
         BNZ   @RF00254                                            0254
         LTR   RTNCODE,RTNCODE                                     0254
         BZ    @RF00254                                            0254
*                             DDRINV1=DDRINO;/* NO, INDICATE SO    0255
*                                                            @Y02BKCI*/
         MVI   DDRINV1(DDRPTR),X'05'                               0255
*                           ELSE                                   0256
*                             DO;                                  0256
         B     @RC00254                                            0256
@RF00254 DS    0H                                                  0257
*                               CALL IGFDVJES;/* SEE IF JES3 LIKES 0257
*                                      THIS DEVICE           @Y02BKCI*/
         BAL   @14,IGFDVJES                                        0257
*                               IF DVJESRC^=VALID THEN/* Q. DEVICE 0258
*                                      VALID FROM JES3 POINT OF VIEW
*                                                            @Y02BKCI*/
         C     DVJESRC,@CF00037                                    0258
         BE    @RF00258                                            0258
*                                 DDRINV1=DDRIJ3;/* NO, INDICATE SO
*                                                            @Y02BKCI*/
         MVI   DDRINV1(DDRPTR),X'08'                               0259
*                               ELSE/* YES, ALL TESTS PASSED - DEVICE
*                                      CAN BE SWAPPED TO             */
*                                 DERLRC=VALID;/* RESET THE EXTERNAL
*                                      RETURN CODE TO INDICATE THAT
*                                      THE DEVICE IS VALID TO BE   0260
*                                      SWAPPED TO                    */
         B     @RC00258                                            0260
@RF00258 MVI   DERLRC,X'00'                                        0260
*                             END;                                 0261
*                         END;                                     0262
*                 END;                                             0263
*           END;                                                   0264
*       END;                                                       0265
*     ELSE                          /* NOT 'TO' OR 'FROM' REQUEST    */
*       IF DDRINV2=DDRVUS           /* TEST FOR 'USER' VALIDATE    0266
*                                      REQUEST                       */
*         THEN                      /* YES, VALIDATE USER            */
*                                                                  0266
         B     @RC00207                                            0266
@RF00207 CLI   DDRINV2(DDRPTR),X'03'                               0266
         BNE   @RF00266                                            0266
*         /***********************************************************/
*         /*                                                         */
*         /* CHECK TO SEE IF THE USER REQUEST WHICH INITIATED DDR IS */
*         /* STILL IN THE SYSTEM                                     */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0267
*         IF^(DDRSYS='1'B&          /* IS THIS A SYSTEM INITIATED  0267
*                                      SWAP                          */
*             DDRPRG='1'B)          /* IN WHICH THE USER REQUEST HAS
*                                      ALREADY BEEN PURGED           */
*           THEN                    /* NO, CONTINUE WITH SWAP        */
         TM    DDRSYS(DDRPTR),B'01000000'                          0267
         BNO   @RT00267                                            0267
         TM    DDRPRG(DDRPTR),B'00001000'                          0267
         BO    @RF00267                                            0267
@RT00267 DS    0H                                                  0268
*           DERLRC=VALID;           /* RESET THE EXTERNAL RETURN CODE
*                                      TO INDICATE THAT USER REQUEST
*                                      HAS NOT BEEN PURGED           */
         MVI   DERLRC,X'00'                                        0268
*         ELSE                                                     0269
*           ;                       /* YES, SWAP WILL BE TERMINATED
*                                      SINCE EXTERNAL RETURN CODE  0269
*                                      STILL INDICATES INVALID SWAP  */
@RF00267 DS    0H                                                  0270
*       ELSE                        /* NOT 'TO', 'FROM' OR 'USER'  0270
*                                      REQUEST                       */
*                                                                  0270
*         /***********************************************************/
*         /*                                                         */
*         /* INVALID PARAMETER IN THE DDRCOM FOR VALIDATE REQUEST    */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0270
*         DO;                                                      0270
         B     @RC00266                                            0270
@RF00266 DS    0H                                                  0271
*           DDRTER1=DDRTCE;         /* INDICATE 'CATASTROPHIC ERROR' */
         MVI   DDRTER1(DDRPTR),X'07'                               0271
*           DERLRC=BADPARM;         /* RESET EXTERNAL RETURN CODE TO
*                                      INDICATE INVALID REQUEST      */
         MVI   DERLRC,X'0C'                                        0272
*         END;                                                     0273
*                                                                  0273
*   /*****************************************************************/
*   /*                                                               */
*   /* COMMON EXIT FROM DEVICE INDEPENDENT VALIDATION - AT THIS      */
*   /* POINT, THE EXTERNAL RETURN CODE VARIABLE (DERLRC) CONTAINS THE*/
*   /* RETURN CODE TO BE PASSED BACK TO THE CALLER                   */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0274
*IGFDV1R:                           /* ESTAE RETRY ENTRY POINT       */
*   DO;                             /* ?IGFDERR CANCEL               */
*                                                                  0274
@RC00266 DS    0H                                                  0274
@RC00207 DS    0H                                                  0274
@RC00164 DS    0H                                                  0274
IGFDV1R  DS    0H                                                  0275
*     /***************************************************************/
*     /*                                                             */
*     /* IGFDERR - CANCEL RECOVERY ENVIRONMENT                       */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0275
*     DERFUNK=DERCANC;              /* INDICATE CANCEL               */
         MVI   DERFUNK,X'02'                                       0275
*     CALL IGFDE0(DERPLIST);        /* CANCEL ESTAE                  */
         LA    @14,DERPLIST                                        0276
         ST    @14,@AL00001                                        0276
         L     @15,@CV00075                                        0276
         LA    @01,@AL00001                                        0276
         BALR  @14,@15                                             0276
*   END;                            /* ?IGFDERR CANCEL               */
*   RETURN CODE(DERLRC);            /* RETURN TO CALLER              */
         SLR   @12,@12                                             0278
         IC    @12,DERLRC                                          0278
         L     @13,4(,@13)                                         0278
         L     @00,@SIZDATD                                        0278
         LR    @01,@09                                             0278
         FREEMAIN R,LV=(0),A=(1)
         LR    @15,@12                                             0278
         L     @14,12(,@13)                                        0278
         LM    @00,@12,20(@13)                                     0278
         BR    @14                                                 0278
*   GENERATE NOREFS NOSETS(EJECT);                                 0279
         EJECT
*                                                                  0280
*   /*****************************************************************/
*   /*                                                               */
*   /* SUBROUTINE IGFDV2 - FINDS THE UCB FOR A GIVEN DEVICE ADDRESS  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0280
*IGFDV2:                            /* SUBROUTINE ENTRY POINT        */
