         N     CHANAD,@CF03138                                     0584
*         IF CHANAD=FAILCHAN(1)&UCBBUSYD=BINARY1 THEN              0585
         SLR   @15,@15                                             0585
         IC    @15,FAILCHAN(,PCCAPTR)                              0585
         CR    CHANAD,@15                                          0585
         BNE   @RF00585                                            0585
         TM    UCBBUSYD(UCBADDR),B'10000000'                       0585
         BNO   @RF00585                                            0585
*           DO;                                                    0586
*                                                                  0586
*             /*******************************************************/
*             /*                                                     */
*             /* IF THE RESULTS OF THE ABOVE TEST ARE TRUE A TEST    */
