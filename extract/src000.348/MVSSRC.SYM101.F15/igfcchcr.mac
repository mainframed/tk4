         TITLE 'IGFCCHCR- MVM CENTRAL CHANNEL CHECK HANDLER            *
                        '
*                                                                  0208
*                                      /*GENERATE ENTRANCE CODE*/
IGFCCHCR CSECT
CRSTART STM     0,15,0(13)             SAVE ALL THE IOS REGS
        LR      11,15                  GET ADDRESSABILITY
        USING   CRSTART,11             SET THE BASE
        B       PROLOG                 BRANCH AROUND IDENTIFIERS
        MODID BR=NO                    MODULE ID & LEVEL      @ZA31594
PROLOG  EQU     *
*   /*****************************************************************/
*   /*                                                               */
*   /* IF AN FRR ENTRY THE PCCAPTR WILL BE LOADED FROM IOS REG 8,    */
*   /* OTHERWISE IT WILL BE LOADED FROM THE PSA. THE FOOTPRINTS ARE  */
*   /* CLEARED AND FOOTPRINT FOR CCH CENTRAL IS SET                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0209
*   IF FRRBIT='1'B                  /* CHECK FOR FRR ENTRY   @YM00991*/
*     THEN                                                         0209
         TM    FRRBIT(SAVREG),B'10000000'                          0209
         BNO   @RF00209                                            0209
*     DO;                                                          0210
*       PCCAPTR=SAVREG8;            /* LOAD PCCAPTR FROM THE IOS SAVE
*                                      AREA                  @YM00991*/
         L     PCCAPTR,SAVREG8(,SAVREG)                            0211
*       SAVREG8=PCCACHSV(3);        /* RESTORE REG 8 IN IOS SAVE AREA
*                                      FROM PCCA             @YM00991*/
         L     @15,PCCACHSV+8(,PCCAPTR)                            0212
         ST    @15,SAVREG8(,SAVREG)                                0212
*       PCCASLCK='0'B;              /* CLEAR LOCK HELD       @G51BPLC*/
         NI    PCCASLCK(PCCAPTR),B'10111111'                       0213
*     END;                          /*                       @YM00991*/
*   ELSE                                                           0215
*     PCCAPTR=PSAPCCAV;             /* IF NOT FRR ENTRY LOAD PCCAPTR
*                                      FROM PSA              @YM00991*/
         B     @RC00209                                            0215
@RF00209 L     PCCAPTR,PSAPCCAV                                    0215
*   PCCACHF1=X0;                    /* CLEAR CCH FOOTPRINTS          */
@RC00209 MVI   PCCACHF1(PCCAPTR),X'00'                             0216
*   PCCACHF2=X0;                    /* CLEAR FOOTPRINT               */
         MVI   PCCACHF2(PCCAPTR),X'00'                             0217
*   PCCACHF3=X0;                    /* CLEAR FOOTPRINTS      @G51BPLC*/
         MVI   PCCACHF3(PCCAPTR),X'00'                             0218
*   PCCACF11='1'B;                  /* SET CCH CENTRAL FOOTPRINT     */
         OI    PCCACF11(PCCAPTR),B'10000000'                       0219
*   PCCACHSV(3)=SAVREG;             /* SAVE THE SAVE AREA ADDR       */
         ST    SAVREG,PCCACHSV+8(,PCCAPTR)                         0220
*/********************************************************************/
*/*                                                                  */
*/* CHECK TO SEE IF THIS IS AN ENTRY FROM THE CCH FRR (THE HIGH ORDER*/
*/* BIT IN THE LAST BYTE OF REGISTER 1 IS 1). IF IT IS 1, WE WANT TO */
*/* TO LEAVE THE PCCACNRE & PCCACIOR SET FROM THR FRR ROUTINE. WE    */
*/* ALSO HAVE TO CLEAR THE HIGH ORDER BIT IN REGISTER 1 SAVE AREA.   */
*/*                                                                  */
*/* IF THE ENTRY IS FROM IOS, WE MUST PLACE THE CCH FRR ON THE STACK */
*/* VIA THE SETFRR MACRO. ALSO WE HAVE TO CLEAR THE PCCACHS1 & PCCA- */
*/* CHS2 FIELDS AND SET PCCACFRR.                                    */
*/*                                                                  */
*/********************************************************************/
*                                                                  0221
*   RESPECIFY                                                      0221
*    (WKREG1,                                                      0221
*     WKREG3,                                                      0221
*     WKREG4,                                                      0221
*     WKREG5,                                                      0221
*     WKREG6) RSTD;                 /* RESTRICT REGS 1,3,4,5, & 6 FOR
*                                      USE                   @YM00991*/
*   IF FRRBIT='1'B THEN                                            0222
         TM    FRRBIT(SAVREG),B'10000000'                          0222
         BNO   @RF00222                                            0222
*     DO;                           /* FRR ENTRY ?                   */
*       PCCACHS1=(PCCACHS1&'40'X);  /* CLEAR PCCACHS1 EXCEPT FOR THE
*                                      PCCACNRE BIT                  */
         NI    PCCACHS1(PCCAPTR),X'40'                             0224
*       PCCACHS2=(PCCACHS2&'80'X);  /* CLEAR PCCACHS2 EXCEPT FOR THE
*                                      PCCACIOR BIT                  */
         NI    PCCACHS2(PCCAPTR),X'80'                             0225
*       FRRBIT='0'B;                /* CLEAR THE FRR BIT IN SAVE     */
         NI    FRRBIT(SAVREG),B'01111111'                          0226
*       SAVREG0=PCCACHSV(2);        /* RESTORE REG 0 IN SAVE AREA    */
         L     @15,PCCACHSV+4(,PCCAPTR)                            0227
         ST    @15,SAVREG0(,SAVREG)                                0227
*     END;                                                         0228
*   ELSE                                                           0229
*     DO;                           /* NO FRR ENTRY. SET UP FRR      */
         B     @RC00222                                            0229
@RF00222 DS    0H                                                  0230
*       WKREG3=ADDR(IGFCCHFR);      /* GET THE ADDRESS OF THE FRR    */
*                                                                  0230
         LA    WKREG3,IGFCCHFR                                     0230
*       /*************************************************************/
*       /*                                                           */
*       /* ADD THE CCH FUNCTIONAL RECOVERY ROUTINE TO THE FRR STACK  */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0231
*CCHFRR:                                                           0231
*       GEN REFS(PSACSTK,PSALSFCC,FRRSCURR,FRRSLAST,FRRSELEN,FRRSENTR,
*           FRRSFRRA,FRRSPARM,FRRSEMP,FRRS)                        0231
*           (SETFRR A,PARMAD=(6),FRRAD=(3),WRKREGS=(4,5));         0231
CCHFRR   SETFRR A,PARMAD=(6),FRRAD=(3),WRKREGS=(4,5)
*       PCCACHS1=X0;                /* CLEAR PCCACHS1 FIELD          */
         MVI   PCCACHS1(PCCAPTR),X'00'                             0232
*       PCCACHS2=X0;                /* CLEAR PCCACHS2 FIELD          */
         MVI   PCCACHS2(PCCAPTR),X'00'                             0233
*       PCCACFRR='1'B;              /* SET FRR IN STACK BIT          */
         OI    PCCACFRR(PCCAPTR),B'00100000'                       0234
*       FRRPCCA=PCCAPTR;            /* SAVE PCCAPTR IN FRR WORK AREA
*                                                            @YM02589*/
         ST    PCCAPTR,FRRPCCA(,WKREG6)                            0235
*       FRRBASE=BASEREG;            /* SAVE BASE REG IN FRR WORK AREA
*                                                            @YM02589*/
         ST    BASEREG,FRRBASE(,WKREG6)                            0236
*       FRRSAVE=SAVREG;             /* SAVE IOS SAVE AREA POINTER  0237
*                                                            @YM02589*/
         ST    SAVREG,FRRSAVE(,WKREG6)                             0237
*     END;                                                         0238
*   RESPECIFY                                                      0239
*    (WKREG3,                                                      0239
*     WKREG4,                                                      0239
*     WKREG5,                                                      0239
*     WKREG6) UNRSTD;               /*                       @YM00991*/
*                                                                  0239
@RC00222 DS    0H                                                  0240
*   /*****************************************************************/
*   /*                                                               */
*   /* THE RECORD BYTE MUST BE CLEARED IN THE PCCAWERP AND THE THE   */
*   /* ENTRY CODE MOVED FROM THE REG 1 SAVE LOCATION IN THE IOS SAVE */
*   /* AREA TO THE PCCAIOSI BYTE.                                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0240
*CCHCLRB:                                                          0240
*   PCCACHRB=X0;                    /* CLEAR CCH RECORD BYTE         */
CCHCLRB  MVI   PCCACHRB(PCCAPTR),X'00'                             0240
*   PCCAWERP=PCCAWERP&&PCCAWERP;    /* ZERO THE WORK ERPIB           */
         XC    PCCAWERP(8,PCCAPTR),PCCAWERP(PCCAPTR)               0241
*   PCCAIOSI=IOSBYTE;               /* MOVE IOS ENTRY CODE TO PCCA   */
         MVC   PCCAIOSI(1,PCCAPTR),IOSBYTE(SAVREG)                 0242
*   IF FLCLCL^=FOXES&HUNGINF='1'B   /* HUNG INTERFACE?       @G51BPLC*/
*     THEN                                                         0243
         CLC   FLCLCL(4),@CB03136                                  0243
         BE    @RF00243                                            0243
         TM    HUNGINF,B'00010000'                                 0243
         BNO   @RF00243                                            0243
*     PCCAISRB='1'B;                /* INDIC SCHED IECVIRST  @G51BPLC*/
*                                                                  0244
         OI    PCCAISRB(PCCAPTR),B'10000000'                       0244
*   /*****************************************************************/
*   /*                                                               */
*   /* A CHECK IS MADE TO SEE IF THE ERROR WAS ENCOUNTERED DURING A  */
*   /* START I/O OPERATION. IF SO THE PCCACSIB BIT IS SET IN THE     */
*   /* PCCA. THE ENTRY CODES FOR SIO ARE 0,8,16,&28.                 */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0245
*   IF PCCAIOSI=X0×PCCAIOSI=X8×PCCAIOSI=X10×PCCAIOSI=X1C THEN      0245
@RF00243 CLI   PCCAIOSI(PCCAPTR),X'00'                             0245
         BE    @RT00245                                            0245
         CLI   PCCAIOSI(PCCAPTR),X'08'                             0245
         BE    @RT00245                                            0245
         CLI   PCCAIOSI(PCCAPTR),X'10'                             0245
         BE    @RT00245                                            0245
         CLI   PCCAIOSI(PCCAPTR),X'1C'                             0245
         BNE   @RF00245                                            0245
@RT00245 DS    0H                                                  0246
*     PCCACSIB='1'B;                                               0246
*                                                                  0246
         OI    PCCACSIB(PCCAPTR),B'10000000'                       0246
*   /*****************************************************************/
*   /*                                                               */
*   /* IF ERROR WAS DURING A TIO OPERATION ,SET PCCACTIB IN THE PCCA.*/
*   /* THE CODE FOR A TIO IS 4.                                      */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0247
*   ELSE                                                           0247
*     IF PCCAIOSI=X4 THEN                                          0247
         B     @RC00245                                            0247
@RF00245 CLI   PCCAIOSI(PCCAPTR),X'04'                             0247
         BNE   @RF00247                                            0247
*       PCCACTIB='1'B;              /* TIO OPERATION ?               */
*                                                                  0248
         OI    PCCACTIB(PCCAPTR),B'00100000'                       0248
*   /*****************************************************************/
*   /*                                                               */
*   /* IF THE ERROR WAS DURING AN I/0 INTERRUPT, SET PCCACINB. THE   */
*   /* ENTRY CODE FOR AN INTERRUPT IS 20.                            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0249
*     ELSE                                                         0249
*       IF PCCAIOSI=X14 THEN                                       0249
         B     @RC00247                                            0249
@RF00247 CLI   PCCAIOSI(PCCAPTR),X'14'                             0249
         BNE   @RF00249                                            0249
*         PCCACINB='1'B;            /* INTERRUPT INTERCEPT ?         */
*                                                                  0250
         OI    PCCACINB(PCCAPTR),B'01000000'                       0250
*   /*****************************************************************/
*   /*                                                               */
*   /* IF THE ERROR OCCURRED DURING A HALT I/O OPERATION ,SET        */
*   /* PCCACHIB. ALSO SET THE RECORD ONLY BIT IN THE PCCA. THE THE   */
*   /* ENTRY CODE FOR THE HIO INTERCEPT IS 24.                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0251
*       ELSE                                                       0251
*         IF PCCAIOSI=X18 THEN                                     0251
         B     @RC00249                                            0251
@RF00249 CLI   PCCAIOSI(PCCAPTR),X'18'                             0251
         BNE   @RF00251                                            0251
*           DO;                     /* HIO INTERCEPT ?               */
*             PCCACHIB='1'B;        /* YES, SET HIO BIT              */
         OI    PCCACHIB(PCCAPTR),B'00010000'                       0253
*             PCCACNLS='1'B;        /* SET RECORD ONLY               */
         OI    PCCACNLS(PCCAPTR),B'00010000'                       0254
*           END;                                                   0255
*                                                                  0255
*   /*****************************************************************/
*   /*                                                               */
*   /* A CHECK IS MADE FOR A ZERO UCB ADDRESS. THE ADDRESS WAS PASSED*/
*   /* IN REGISTER 7 FROM IOS. IF THE ADDRESS IS ZERO THE UCB ADDRESS*/
*   /* IS ZERO FOOTPRINT IS SET IN THE PCCA, THE RECORD ONLY BIT IS  */
*   /* SET ,AND THE UCB INVALID BIT IS SET (PCCACUCB). THEN WE WILL  */
*   /* CHECK FOR I/O RESTART.                                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0256
*CCHUCBT:                                                          0256
*   IF UCBPTR=0 THEN                                               0256
@RF00251 DS    0H                                                  0256
@RC00249 DS    0H                                                  0256
@RC00247 DS    0H                                                  0256
@RC00245 DS    0H                                                  0256
CCHUCBT  LTR   UCBPTR,UCBPTR                                       0256
         BNZ   @RF00256                                            0256
*     DO;                           /* UCB ADDRESS ZERO ?            */
*CCHUCB0:                                                          0258
*       PCCACF12='1'B;              /* UCB INVALID FOOTPRINT         */
CCHUCB0  OI    PCCACF12(PCCAPTR),B'01000000'                       0258
*       PCCACNLS='1'B;              /* RECORD ONLY SET               */
*       PCCACUCB='1'B;              /* SET UCB INVALID               */
         OI    PCCACNLS(PCCAPTR),B'00010010'                       0260
*       GOTO CCHTSIOR;              /* CK FOR I/O RESTART            */
         B     CCHTSIOR                                            0261
*     END;                                                         0262
*   UCBPTR=UCBPTR+LENGTH(UCB);      /* OBTAIN UCB COMMON ADDRESS     */
*                                                                  0263
@RF00256 LA    UCBPTR,512(,UCBPTR)                                 0263
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK FOR SENSE OR ASYNCHRONOUS SENSE INTERCEPTS.             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0264
*   IF PCCAIOSI^=X8&PCCAIOSI^=X1C THEN                             0264
*                                                                  0264
         CLI   PCCAIOSI(PCCAPTR),X'08'                             0264
         BE    @RF00264                                            0264
         CLI   PCCAIOSI(PCCAPTR),X'1C'                             0264
         BE    @RF00264                                            0264
*     /***************************************************************/
*     /*                                                             */
*     /* CHECK FOR CHANNEL PROGRAM COMPLETE OR DEVICE BUSY. IF       */
*     /* NEITHER, SET SPECIAL HANDLING AND GO CHECK FOR I/O RESTART  */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0265
*     IF UCBUSING='0'B&UCBBUSYD='0'B THEN                          0265
         TM    UCBUSING(UCBPTR),B'10100000'                        0265
         BNZ   @RF00265                                            0265
*       DO;                                                        0266
*CCHSPCL:                                                          0267
*         PCCACAND='1'B;            /* SET SPECIAL HANDLING          */
CCHSPCL  OI    PCCACAND(PCCAPTR),B'00001000'                       0267
*         GOTO CCHTSIOR;            /* CK FOR I/O RESTART            */
         B     CCHTSIOR                                            0268
*       END;                                                       0269
*     ELSE                                                         0270
*       ;                                                          0270
*                                                                  0270
@RF00265 DS    0H                                                  0271
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK FOR A CHANNEL DATA CHECK. IF CDC SET RECORD ONLY OR DOES*/
*   /* THE EWA EXIST ? (IS THE ADDRESS IN REG 12=0?) IF SO, THE EWA  */
*   /* DOES NOT EXIST, AND RECORD ONLY MUST BE SET.                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0271
*   IF CSWCCC='0'B&CSWICC='0'B×ERPIBREG=0 THEN                     0271
@RF00264 TM    CSWCCC,B'00000110'                                  0271
         BZ    @RT00271                                            0271
         LTR   ERPIBREG,ERPIBREG                                   0271
         BNZ   @RF00271                                            0271
@RT00271 DS    0H                                                  0272
*     DO;                           /* CDC ONLY OR NO EWA EXIST      */
*       PCCACNLS='1'B;              /* SET RECORD ONLY               */
         OI    PCCACNLS(PCCAPTR),B'00010000'                       0273
*       GOTO CCHTSIOR;              /* CK FOR I/O RESTART            */
         B     CCHTSIOR                                            0274
*     END;                                                         0275
*                                                                  0275
*   /*****************************************************************/
*   /*                                                               */
*   /* IF THIS IS AN ASYNCHRONOUS SENSE INTERCEPT SET THE FOLLOWING  */
*   /* 1. ERPIB ALREADY FOOTPRINT 2. ERPIB NO RETRY FLAG             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0276
*CCHOLDEP:                                                         0276
*   IF PCCAIOSI=X1C THEN                                           0276
@RF00271 DS    0H                                                  0276
CCHOLDEP CLI   PCCAIOSI(PCCAPTR),X'1C'                             0276
         BNE   @RF00276                                            0276
*     DO;                           /* ASYNCHRONOUS SENSE ?          */
*       PCCACIBC='1'B;              /* ERPIB ALREADY EXIST FLAG      */
         OI    PCCACIBC(PCCAPTR),B'00000100'                       0278
*       PCCACF13='1'B;              /* ERPIB EXIST FOOTPRINT         */
         OI    PCCACF13(PCCAPTR),B'00100000'                       0279
*       ERPNOR='1'B;                /* SET NO RETRY IN ERPIB         */
         OI    ERPNOR(ERPIBREG),B'00000001'                        0280
*     END;                                                         0281
*   ELSE                                                           0282
*     DO;                           /* NO ASYNCHRONOUS SENSE         */
*                                                                  0282
         B     @RC00276                                            0282
@RF00276 DS    0H                                                  0283
*       /*************************************************************/
*       /*                                                           */
*       /* INVOKE THE RECONFIGURATION VERIFICATION SUBROUTINE TO     */
*       /* DETERMINE IF RECONFIGURATION HARDWARE IS ACTIVE ON THIS   */
*       /* CHANNEL                                                   */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0283
*CCHTSIOR:                                                         0283
*       CALL IGFCCHRV;              /*                       @Y30CJJE*/
*                                                                  0283
CCHTSIOR BAL   @14,IGFCCHRV                                        0283
*       /*************************************************************/
*       /*                                                           */
*       /* CHECK TO SEE IF I/O RESTART HAS BEEN SET BY THE FRR. IF IT*/
*       /* HAS THE ERPIB IS NOT TO BE CREATED.                       */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0284
*       IF PCCACIOR='0'B THEN                                      0284
         TM    PCCACIOR(PCCAPTR),B'10000000'                       0284
         BNZ   @RF00284                                            0284
*         DO;                       /* IOR SET ? YES NO FE           */
*                                                                  0285
*           /*********************************************************/
*           /*                                                       */
*           /* CHECK TO SEE IF THIS IS A SENSE INTERCEPT. IF SO CALL */
*           /* SENSE INTERCEPT ROUTINE.                              */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0286
*           IF PCCAIOSI=X8 THEN                                    0286
         CLI   PCCAIOSI(PCCAPTR),X'08'                             0286
         BNE   @RF00286                                            0286
*CCHSI:                                                            0287
*             CALL IGFCCHSI;        /* SENSE? CALL SI                */
CCHSI    BAL   @14,IGFCCHSI                                        0287
*CCHFE:                                                            0288
*           CALL IGFCCHFE;          /* NO INTER. CALL FE             */
@RF00286 DS    0H                                                  0288
CCHFE    BAL   @14,IGFCCHFE                                        0288
*         END;                                                     0289
*CCHTSTRE:                                                         0290
*       IF PCCACNRE='0'B THEN                                      0290
@RF00284 DS    0H                                                  0290
CCHTSTRE TM    PCCACNRE(PCCAPTR),B'01000000'                       0290
         BNZ   @RF00290                                            0290
*CCHRE:                                                            0291
*         CALL IGFCCHRD;            /* GO BUILD THE RECORD           */
*                                                                  0291
CCHRE    BAL   @14,IGFCCHRD                                        0291
*       /*************************************************************/
*       /*                                                           */
*       /* CALL THE ASYNCHRONOUS RECORDER TO SCHEDULE RECORDING.     */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0292
*CCHAS:                                                            0292
*       CALL IGFCCHAS;              /* CALL THE RECORDER ROUTINE     */
@RF00290 DS    0H                                                  0292
CCHAS    BAL   @14,IGFCCHAS                                        0292
*/* IF INTERRUPT INTERCEPT AND SPECIAL HANDLING THEN RESET CCC+ICC   */
*                                                                  0293
*       IF PCCAIOSI=X14&PCCACAND='1'B THEN                         0293
         CLI   PCCAIOSI(PCCAPTR),X'14'                             0293
         BNE   @RF00293                                            0293
         TM    PCCACAND(PCCAPTR),B'00001000'                       0293
         BNO   @RF00293                                            0293
*         DO;                       /*                       @ZA16569*/
*           PCCACF15='1'B;          /* SET FOOTPRINT         @ZA16569*/
         OI    PCCACF15(PCCAPTR),B'00001000'                       0295
*           CALL IGFPSAD0;          /* OBTAIN AUTHORIZATION@G51BPLC  */
         L     @15,@CV03088                                        0296
         BALR  @14,@15                                             0296
*           CSWICC='0'B;            /* TURN OFF INTERFACE CTL CHK  0297
*                                                            @ZA16569*/
*           CSWCCC='0'B;            /* TURN OFF CHAN CTL CHK @ZA16569*/
         NI    CSWICC,B'11111001'                                  0298
*           CALL IGFPSAE0;          /* RELSE AUTHORIZATION@G51BPLC   */
         L     @15,@CV03089                                        0299
         BALR  @14,@15                                             0299
*         END;                      /*                       @ZA16569*/
*     END;                                                         0301
@RF00293 DS    0H                                                  0302
*CCHEX:                                                            0302
*   CALL IGFCCHEX;                  /* CALL EXIT                     */
@RC00276 DS    0H                                                  0302
CCHEX    BAL   @14,IGFCCHEX                                        0302
*   RETURN;                                                        0303
@EL00001 DS    0H                                                  0303
@EF00001 DS    0H                                                  0303
@ER00001 BR    @14                                                 0303
