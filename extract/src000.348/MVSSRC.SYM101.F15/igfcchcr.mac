*   LRBCMPNO=CSDCPUOL;                                             0622
*                                                                  0622
         L     @15,CVTPTR                                          0622
         L     @10,CVTCSD(,@15)                                    0622
         MVC   LRBCMPNO(2,MLRBCVPT),CSDCPUOL(@10)                  0622
*   /*****************************************************************/
*   /*                                                               */
*   /* SET THE ALTERNATE PCCA POINTER EQUAL TO THE VALUE OF THE      */
*   /* POINTER PASSED TO THIS SUBROUTINE IN REGISTER 8               */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0623
*   IF PCCACCRA='1'B THEN           /*                       @Y30CJJE*/
         TM    PCCACCRA(PCCAPTR),B'00000100'                       0623
         BNO   @RF00623                                            0623
*     MPCCAPTR=CRCAPCCA;            /*                       @Y30CJJE*/
         L     @15,CRCAPTR(,@15)                                   0624
