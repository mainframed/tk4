         MVC   LRBCAIO-2(2,@15),CHANNEL(UCBADDR)                   0593
         NC    LRBCAIO-2(2,@15),@CB03132                           0593
*                     COUNTER=COUNTER+ONE;                         0594
         AL    COUNTER,@CF00063                                    0594
*                   END;                                           0595
*               END;                                               0596
@RF00591 DS    0H                                                  0597
*           END;                                                   0597
@RF00589 DS    0H                                                  0598
*       END;                                                       0598
*                                                                  0598
@RF00585 DS    0H                                                  0599
*     /***************************************************************/
