@RC00541 DS    0H                                                  0549
*   RLRBCVPT=ADDR(LRBCCLOG)+LRBCLOGL;/* PTR PTS TO END OF LOG        */
         LA    RLRBCVPT,LRBCCLOG(,RDLRBPTR)                        0549
         LH    @15,LRBCLOGL(,RDLRBPTR)                             0549
         N     @15,@CF03517                                        0549
         ALR   RLRBCVPT,@15                                        0549
*   RESPECIFY                                                      0550
*     LRBCVAR BASED(RLRBCVPT);                                     0550
*   LRBCVAR(1:LENGTH(LRBCVAR)+16*LENGTH(LRBCMP))=LRBCVAR(1:LENGTH( 0551
