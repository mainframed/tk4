*       /* UPDATE MPCATPTR TO LOCATE NEXT CHANNEL ENTRY              */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0634
*       MPCATPTR=MPCATPTR+LENGTH(CAT);                             0634
         AL    MPCATPTR,@CF00056                                   0634
*     END;                                                         0635
*                                                                  0635
         AL    N,@CF00063                                          0635
         C     N,@CF00051                                          0635
         BNH   @DL00630                                            0635
*     /***************************************************************/
*     /*                                                             */
*     /* SHIFT WORKREG RIGHT BY ONE POSITION TO CORRECTLY POSITION   */
*     /* THE CHANNEL STATUS BITS AND STORE THE CHANNEL STATUS IN THE */
*     /* LRBCMPCS FIELD INDEXED BY INDEX                             */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0636
*     LRBCMPCS(INDEX)=MPCSREG/TWO;                                 0636
*                                                                  0636
         LR    @15,INDEX                                           0636
         SLA   @15,2                                               0636
         LR    @10,MPCSREG                                         0636
         SRL   @10,1                                               0636
         ALR   @15,MLRBCVPT                                        0636
         STCM  @10,3,LRBCMPCS-4(@15)                               0636
*     /***************************************************************/
*     /*                                                             */
*     /* DETERMINE IF THE CPU WITH THE CHANNEL ERROR IS THE ONLY CPU */
*     /* ON LINE. IF IT IS RETURN TO IGFCCHRD, IF NOT PROCESS ALL    */
*     /* OTHER CPU'S ON LINE                                         */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0637
*     IF(INDEX=ONE&PCCACCRA='1'B) THEN                             0637
         LA    @15,1                                               0637
         CR    INDEX,@15                                           0637
         BNE   @RF00637                                            0637
         TM    PCCACCRA(PCCAPTR),B'00000100'                       0637
         BNO   @RF00637                                            0637
*       DO;                         /*                       @Y30CJJE*/
*         MPCCAPTR=PCCAPTR;                                        0639
         LR    MPCCAPTR,PCCAPTR                                    0639
*         INDEX=INDEX+ONE;                                         0640
         ALR   INDEX,@15                                           0640
*       END;                                                       0641
*     ELSE                                                         0642
*       ;                                                          0642
@RF00637 DS    0H                                                  0643
*     IF LRBCMPNO<TWO THEN                                         0643
         CLC   LRBCMPNO(2,MLRBCVPT),@CH00223                       0643
         BNL   @RF00643                                            0643
*       M=SEVNTEEN;                 /* IF ONLY 1 CPU IS ON LINE FORCE
*                                      THE LOOP TO TERMINATE BY    0644
*                                      SETTING M = 17 (UPPER LIMIT OF
*                                      LOOP IS 16)                   */
*                                                                  0644
         LA    M,17                                                0644
*     /***************************************************************/
*     /*                                                             */
*     /* THIS LOOP WILL BE ENTERED ONLY WHEN MORE THAN 1 CPU IS ON   */
*     /* LINE. WHENEVER THIS LOOP IS ENTERED N SHOULD EQUAL 17       */
*     /* BECAUSE OF PREVIOUS LOOP USING N FOR CHANNEL STATUS INDEX   */
*     /* (VALUE OF N IS NOT IMPORTANT, ONLY THE FACT THAT IT IS      */
*     /* GREATER THAN 1)                                             */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0645
*     ELSE                                                         0645
*       DO WHILE N>ONE&M<SEVNTEEN;                                 0645
*                                                                  0645
         B     @RC00643                                            0645
@RF00643 B     @DE00645                                            0645
@DL00645 DS    0H                                                  0646
*         /***********************************************************/
*         /*                                                         */
*         /* UPDATE CPU ACTIVE POINTER (M) TO POINT TO THE CPU TO BE */
*         /* TESTED (M WAS INITIALLY SET TO 0). THEN ISOLATE THE CPU */
*         /* ACTIVE BIT FOR THIS CPU BY SHIFTING THE CPU ACTIVE BITS */
*         /* IN THE WORKREG (CPUALREG) TO THE LEFT 15 + M POSITIONS  */
*         /* TO MAKE BIT TO BE TESTED THE REGISTER SIGN BIT.         */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0646
*         M=M+ONE;                                                 0646
         AL    M,@CF00063                                          0646
*         CPUALREG=CSDCPUAL;        /* LOAD ACTIVE BITS FOR ALL 16 0647
*                                      CPU'S                         */
         L     @15,CVTPTR                                          0647
         L     @15,CVTCSD(,@15)                                    0647
         LH    CPUALREG,CSDCPUAL(,@15)                             0647
         N     CPUALREG,@CF03517                                   0647
*         GEN REFS(CPUALREG,M)(SLL CPUALREG,15(M));                0648
*                                                                  0648
         SLL CPUALREG,15(M)
*         /***********************************************************/
*         /*                                                         */
*         /* IF THE CPU ACTIVE WORKREG (CPUALREG) CONTAINS A NEGATIVE*/
*         /* NUMBER IT INDACATES THAT THE CPU BEING TESTED IS ON     */
*         /* LINE. IF THIS CPU IS NOT ON LINE THEN THE NEXT ONE WILL */
*         /* BE TESTED. IF IT IS ON LINE THEN A TEST WILL BE MADE TO */
*         /* DETERMINE IF THIS IS THE CPU WITH THE ORIGINAL CHANNEL  */
*         /* ERROR. IF THIS IS THE CPU WITH THE ORIGINAL CHANNEL     */
*         /* ERROR THEN SKIP THIS ONE AS IT IS ALREADY RECORDED.     */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0649
*         IF CPUALREG<ZERO THEN                                    0649
         LTR   CPUALREG,CPUALREG                                   0649
         BNM   @RF00649                                            0649
*           DO;                     /* IS CPU ON LINE?               */
*             MPCCAPTR=PCCAT00P(M); /* YES, LOCATE IT'S PCCA         */
*                                                                  0651
         LR    @15,M                                               0651
         SLA   @15,2                                               0651
         L     @10,CVTPTR                                          0651
         L     @09,CVTPCCAT(,@10)                                  0651
         AL    @09,@CF03524                                        0651
         L     MPCCAPTR,PCCAT00P(@15,@09)                          0651
*             /*******************************************************/
*             /*                                                     */
*             /* IF THIS IS NOT THE CPU WITH THE ORIGINAL CHANNEL    */
