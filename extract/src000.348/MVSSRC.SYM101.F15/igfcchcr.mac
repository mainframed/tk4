*/********************************************************************/
*/*                                                                  */
*/*      IF THE CAT ENTRY IS VALID FOR THE CHANNEL IN ERROR, THEN    */
*/*      THEN CHANNEL IDENTIFICATION IS MOVED INTO THE RECORD.       */
*/*      THE VALID CHANNEL IDS ARE:                                  */
*/*        1. 00- UNKNOWN              2. 01- INT. BYTE MPX          */
*/*        3. 02- INT. SELECTOR        4. 03- INT. BLOCK MPX         */
*/*        5. 05- 2860 SELECTOR        6. 06- 2870 BYTE MPX.         */
*/*        7. 07- 2880 BLOCK MPX       8. 0A- INT. FILE ADAPTOR      */
*/*        9. 0F- UNRECOGNIZABLE CHANNEL                             */
*/*      THE UNIT ADDR. REG (REG 6) WAS SAVED FROM IGFCCHFE AND STILL*/
*/*      HAS THE UNIT ADDRESS IN IT.                                 */
*/*                                                                  */
*/********************************************************************/
*                                                                  0523
*   RESPECIFY                                                      0523
*     CATPTR RSTD;                                                 0523
@RF00518 DS    0H                                                  0524
*   CATPTR=((UAREG/256)*8)+ADDR(PCCACAT);/* GET THE CAT ENTRY        */
         LR    @04,UAREG                                           0524
         SRDA  @04,32                                              0524
         D     @04,@CF00986                                        0524
         SLA   CATPTR,3                                            0524
         LA    @15,PCCACAT(,PCCAPTR)                               0524
         ALR   CATPTR,@15                                          0524
*   IF CATNID='1'B THEN                                            0525
         TM    CATNID(CATPTR),B'00001000'                          0525
         BNO   @RF00525                                            0525
*     LRBCCHID=X0;                  /* SET CHANNEL ID INVAL          */
         MVI   LRBCCHID(RDLRBPTR),X'00'                            0526
*   ELSE                                                           0527
*     DO;                           /* CAT GOOD, FIND THE ID         */
         B     @RC00525                                            0527
@RF00525 DS    0H                                                  0528
*       WKREG1=CATMODL&RDCMAS0F;    /* GET THE CAT MODEL             */
         SLR   WKREG1,WKREG1                                       0528
         IC    WKREG1,CATMODL(,CATPTR)                             0528
         N     WKREG1,@CF03138                                     0528
*       WKREG2=CATTYPE&RDCMASF0;    /* GET THE CAT TYPE              */
         SLR   WKREG2,WKREG2                                       0529
         IC    WKREG2,CATTYPE(,CATPTR)                             0529
         N     WKREG2,@CF03306                                     0529
*       WKREG1=WKREG1+WKREG2;       /* REG1 HAS THE INDEX            */
*                                                                  0530
         ALR   WKREG1,WKREG2                                       0530
*       /*************************************************************/
*       /*                                                           */
*       /* IF THE INDEX IN REG 1 IS GREATER THAN THE LENGTH OF THE   */
*       /* TABLE, THEN MOVE IN AN ID OF '0F'X.                       */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0531
*       IF WKREG1>RDCONT2F THEN                                    0531
         C     WKREG1,@CF03302                                     0531
         BNH   @RF00531                                            0531
*         LRBCCHID='0F'X;           /* INDEX>LEN TAB                 */
         MVI   LRBCCHID(RDLRBPTR),X'0F'                            0532
*       ELSE                                                       0533
*         DO;                       /* NOT GREATER THAN TABLE        */
         B     @RC00531                                            0533
@RF00531 DS    0H                                                  0534
*           WKREG1=WKREG1+ADDR(RDCCHTAB);/* REG 1 PTS TO ID          */
         LA    @15,RDCCHTAB                                        0534
         ALR   WKREG1,@15                                          0534
*           LRBCCHID=RDTABINX;      /* GET THE ID IN RECORD          */
         MVC   LRBCCHID(1,RDLRBPTR),RDTABINX(WKREG1)               0535
*         END;                                                     0536
*     END;                                                         0537
*                                                                  0537
@RC00531 DS    0H                                                  0538
*   /*****************************************************************/
*   /*                                                               */
*   /* MOVE THE UNIT ADDRESS PASSED FROM IGFCCHFE( FILL ERPIB)       */
