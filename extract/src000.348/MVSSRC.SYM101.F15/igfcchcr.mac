*     GOTO RDMOVCSW;                /* CSW @ INVALID                 */
*   WKREG1=WKREG1-8;                /* DECREMENT THE CSW CMD ADDR    */
*                                                                  0498
         SL    WKREG1,@CF00056                                     0498
*   /*****************************************************************/
*   /*                                                               */
*   /* AT THIS POINT WE WILL DISABLE DAT AND USE REAL ADDRESS OF CCW */
*   /* BECAUSE THE FAILING CCW MAY BE IN ANOTHER MEMORY THAN ONE WE  */
*   /* ARE CURRENTLY RUNNING IN, AND OUR SEGMENT TABLE MAY NOT BE    */
*   /* VALID FOR THAT MEMORY. NOTE -- DYNAMIC ADDRESS TRANSLATION    */
*   /* (DAT) IS DISABLED BETWEEN LABELS 'RDRSMXL' AND 'RDMOVCSW'.    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0499
*   RESPECIFY                                                      0499
*     WKREG5 RSTD;                  /*                       @YM06943*/
*RDRSMXL:                                                          0500
*   WKREG5=PSAPCCAR;                /* GET REAL ADDR OF PCCA @YM06943*/
RDRSMXL  L     WKREG5,PSAPCCAR                                     0500
*   RFY                                                            0501
*    (WKREG14) RSTD;                /*                       @G51BPLC*/
*   GEN SETS(WKREG14) REFS(RDLRBPTR)(LRA 14,0(RDLRBPTR));/* GET ADDR
*                                      OF LRB                @G51BPLC*/
         LRA 14,0(RDLRBPTR)
*   RFY                                                            0503
*     LRB BASED(WKREG14);           /* REBASE LRB            @G51BPLC*/
*   GENERATE REFS(PCCACHID,PCCAPTR)(STNSM PCCACHID(PCCAPTR),X'BB');/*
*                                      DISABLE DAT           @YM06943*/
         STNSM PCCACHID(PCCAPTR),X'BB'
*   RESPECIFY                                                      0505
*     PCCA BASED(WKREG5);           /* USE REAL ADDR OF PCCA @YM06943*/
*   LRBCFCCW=RDCFCCW;               /* MOVE CCW INTO RECORD  @YM06943*/
         MVC   LRBCFCCW(8,WKREG14),RDCFCCW(WKREG1)                 0506
*   IF PER='1'B THEN                                               0507
         TM    PER(WKREG5),B'01000000'                             0507
         BNO   @RF00507                                            0507
*     GENERATE REFS(PCCACHID,WKREG5)(STOSM PCCACHID(WRKREG5),X'44');
         STOSM PCCACHID(WRKREG5),X'44'
*   ELSE                                                           0509
*     GENERATE REFS(PCCACHID,WKREG5)(STOSM PCCACHID(WKREG5),X'04');/*
*                                      RE-ENABLE DAT         @YM06943*/
         B     @RC00507                                            0509
@RF00507 DS    0H                                                  0509
         STOSM PCCACHID(WKREG5),X'04'
*   RESPECIFY                                                      0510
*     PCCA BASED(PCCAPTR);          /* GO BACK TO VIRTUAL ADDR OF  0510
*                                      PCCA                  @YM06943*/
@RC00507 DS    0H                                                  0511
*   RESPECIFY                                                      0511
*     WKREG5 UNRSTD;                /*                       @YM06943*/
*   RFY                                                            0512
*     LRB BASED(RDLRBPTR);          /* REBASE LRB ON VIRTUAL ADDRESS
