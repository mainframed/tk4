*   WKREG1=RDFLCCAW;                /* GET THE CAW ADDRESS           */
         L     WKREG1,RDFLCCAW-1                                   0490
         LA    WKREG1,0(,WKREG1)                                   0490
*   IF PCCACINB='0'B THEN                                          0491
         TM    PCCACINB(PCCAPTR),B'01000000'                       0491
         BZ    @RT00491                                            0491
*     GOTO RDRSMXL;                 /* INTERRUPT INTERCEPT?          */
*                                                                  0492
*   /*****************************************************************/
*   /*                                                               */
*   /* THIS IS AN INTERRUPT, IF THE COMMAND ADDRESS IN THE CSW IS    */
*   /* VALID, THEN THIS IS THE ADDRESS WE MUST HAVE TRANSLATED AND   */
*   /* MOVED INTO THE RECORD.                                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0493
*   IF PCCACCMD='0'B THEN                                          0493
         TM    PCCACCMD(PCCAPTR),B'00000100'                       0493
