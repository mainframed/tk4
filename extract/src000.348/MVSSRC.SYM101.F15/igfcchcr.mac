*   MLRBCVPT=ADDR(LRBCCLOG)+LRBCLOGL;                              0619
*                                                                  0619
         L     @15,LRBPTR(,PCCAPTR)                                0619
         LA    MLRBCVPT,LRBCCLOG(,@15)                             0619
         LH    @15,LRBCLOGL(,@15)                                  0619
         N     @15,@CF03517                                        0619
         ALR   MLRBCVPT,@15                                        0619
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE THE INDEX AND THE LOOP CONTROL REGISTER, M         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0620
*   INDEX=ONE;                                                     0620
         LA    INDEX,1                                             0620
*   M=ZERO;                                                        0621
*                                                                  0621
         SLR   M,M                                                 0621
*   /*****************************************************************/
*   /*                                                               */
*   /* SET LRBCMPNO FIELD EQUAL TO THE NUMBER OF CURRENTLY ACTIVE    */
*   /* PROCESSORS                                                    */
*   /*                                                               */
*   /*****************************************************************/
