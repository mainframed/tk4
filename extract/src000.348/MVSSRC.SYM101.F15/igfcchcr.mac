         LH    UCBADDR,UCBADTAB(,ADLSTPTR)                         0600
         N     UCBADDR,@CF03517                                    0600
*   END;                                                           0601
*                                                                  0601
@DE00580 C     COUNTER,@CF03119                                    0601
         BNL   @DC00580                                            0601
         C     UCBADDR,@CF03134                                    0601
         BNE   @DL00580                                            0601
@DC00580 DS    0H                                                  0602
