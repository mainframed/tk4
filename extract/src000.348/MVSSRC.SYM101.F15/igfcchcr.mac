*   IF PCCACUCB='0'B THEN                                          0518
         TM    PCCACUCB(PCCAPTR),B'00000010'                       0518
         BNZ   @RF00518                                            0518
*     DO;                                                          0519
*       UCBPTR=SAVUCB+LENGTH(UCB);  /* OBTAIN UCB COMMON ADDRESS     */
         L     @15,SAVUCB(,SAVREG)                                 0520
         LA    @15,512(,@15)                                       0520
         LR    UCBPTR,@15                                          0520
*       LRBCDEVT=UCBTYP;            /* VALID UCB                     */
         MVC   LRBCDEVT(4,RDLRBPTR),UCBTYP(UCBPTR)                 0521
*     END;                                                         0522
