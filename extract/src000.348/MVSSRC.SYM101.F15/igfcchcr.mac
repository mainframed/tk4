         BNO   @GL00011                                            0591
         TM    UCBIORST(UCBADDR),B'10000000'                       0591
         BO    @RT00591                                            0591
@GL00011 TM    PCCACCRA(PCCAPTR),B'00000100'                       0591
         BO    @RF00591                                            0591
         TM    UCBIORST(UCBADDR),B'10000000'                       0591
         BO    @RF00591                                            0591
         CLC   UCBCPU(1,UCBADDR),CPUAD(PCCAPTR)                    0591
         BNE   @RF00591                                            0591
@RT00591 DS    0H                                                  0592
*                   DO;             /*                       @Y30CJJE*/
