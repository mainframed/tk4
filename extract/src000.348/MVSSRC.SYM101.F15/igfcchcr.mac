         MVC   LRBCHCUA(2,RDLRBPTR),RDUNIT                         0540
*   /*****************************************************************/
*   /*                                                               */
*   /* DETERMINE THE LOGOUT LENGTH & ADDRESS FROM THE PCCA AND IF    */
*   /* NEITHER IS ZERO, MOVE THE LOGOUT TO THE RECORD BUFFER.        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0541
*   IF PCCALOGL=0×PCCALOGA=0 THEN                                  0541
         SLR   @15,@15                                             0541
         CH    @15,PCCALOGL(,PCCAPTR)                              0541
         BE    @RT00541                                            0541
         C     @15,PCCALOGA(,PCCAPTR)                              0541
         BNE   @RF00541                                            0541
@RT00541 DS    0H                                                  0542
*     LRBCLOGL=0;                   /* SET LOG LEN=0                 */
         SLR   @15,@15                                             0542
         STH   @15,LRBCLOGL(,RDLRBPTR)                             0542
*   ELSE                                                           0543
*     DO;                           /* LOGOUT EXIST, MOVE INTO REC   */
         B     @RC00541                                            0543
@RF00541 DS    0H                                                  0544
*       IF PCCAELAD^=PCCALOGA THEN  /*                       @G51BPLC*/
         L     @15,PCCALOGA(,PCCAPTR)                              0544
         C     @15,PCCAELAD(,PCCAPTR)                              0544
         BE    @RF00544                                            0544
*         LRBCCLOG(1:PCCALOGL)=RDLOGOUT(1:PCCALOGL);/* MOVE THE    0545
*                                      LOGOUT TO THE RECORD          */
         LH    @12,PCCALOGL(,PCCAPTR)                              0545
         BCTR  @12,0                                               0545
         EX    @12,@SM03522                                        0545
*       LRBCLOGL=PCCALOGL;          /* MOVE THE LOG LENGTH TO REC    */
@RF00544 LH    @15,PCCALOGL(,PCCAPTR)                              0546
         STH   @15,LRBCLOGL(,RDLRBPTR)                             0546
*     END;                          /*                       @G51BPLC*/
*                                                                  0547
*   /*****************************************************************/
*   /*                                                               */
*   /* ADJUST THE LRB PTR TO ACCOMMADATE THE LOGOUT(VARAIBLE LENGTH  */
*   /* AND CLEAR REMAINDER OF LRB AFTER IOEL                 @G51BPLC*/
*   /*                                                               */
