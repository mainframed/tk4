*       /*                                                           */
*       /*************************************************************/
*                                                                  0631
*       IF CATNOP=BINARY1 THEN                                     0631
         TM    CATNOP(MPCATPTR),B'01000000'                        0631
         BNO   @RF00631                                            0631
*         MPCSREG=MPCSREG+ONE;                                     0632
*                                                                  0632
         AL    MPCSREG,@CF00063                                    0632
*       /*************************************************************/
*       /*                                                           */
*       /* SHIFT WORKREG LEFT BY ONE POSITION TO PREPARE FOR NEXT    */
