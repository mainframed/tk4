SGTE     TITLE 'BLSSSGTE--FIND SEGMENT TABLE (SGT)                     *
                        '
*/* CHANGE ACTIVITY -------------------------------------------------*/
*/*   THIS MODULE WAS WRITTEN FOR @G57LPRW                           */
*/*   THIS MODULE WAS CHANGED FOR @ZA31997                   @ZA31997*/
*/*------------------------------------------------------------------*/
BLSSSGTE CSECT ,                                                01S0002
@MAINENT DS    0H                                               01S0002
         USING *,@15                                            01S0002
         B     @PROLOG                                          01S0002
         DC    AL1(16)                                          01S0002
         DC    C'BLSSSGTE  78.117'                              01S0002
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                  01S0002
         BALR  @12,0                                            01S0002
@PSTART  DS    0H                                               01S0002
         USING @PSTART,@12                                      01S0002
         L     @00,@SIZDATD                                     01S0002
         BLSUALLS R,LV=(0)
         LR    @11,@01                                          01S0002
         USING @DATD,@11                                        01S0002
         ST    @13,@SA00001+4                                   01S0002
         LM    @00,@01,20(@13)                                  01S0002
         ST    @11,8(,@13)                                      01S0002
         LR    @13,@11                                          01S0002
         MVC   @PC00001(8),0(@01)                               01S0002
*   ESBAPTR=ADDR(ESPA);             /* ->ESBA (ALIAS FOR ESPA)       */
         L     ESBAPTR,@PC00001+4                               01S0151
*   ASID=0;                         /* ASID                          */
         SLR   ASID,ASID                                        01S0152
*   RETC=ZZZFLAGS;                  /* RETURN CODE                   */
         LA    RETC,12                                          01S0153
*   IF SYMBA^='SGT'×ESBASYM(LENGTH(SYMB)+1:ZZZSYML)^=BLANKSUF THEN
         CLC   SYMBA(3,ESBAPTR),@CC01658                        01S0154
         BNE   @RT00154                                         01S0154
         CLC   ESBASYM+6(25,ESBAPTR),BLANKSUF                   01S0154
         BNE   @RT00154                                         01S0154
*     GO TO EXIT;                   /* COMMON EXIT PATH              */
*   DO J1=1 TO LENGTH(SYMBB) WHILE(SYMBB(J1:J1)>='0'&SYMBB(J1:J1)<='9')
*         ;                         /* VERIFY NUMERIC DIGITS         */
         LA    J1,1                                             01S0156
@DL00156 LR    @14,ESBAPTR                                      01S0156
         ALR   @14,J1                                           01S0156
         CLI   SYMBB-1(@14),C'0'                                01S0156
         BL    @DC00156                                         01S0156
         LR    @14,ESBAPTR                                      01S0156
         ALR   @14,J1                                           01S0156
         CLI   SYMBB-1(@14),C'9'                                01S0156
         BH    @DC00156                                         01S0156
*   END;                            /* VERIFY NUMERIC DIGITS         */
         AH    J1,@CH00046                                      01S0157
         CH    J1,@CH00158                                      01S0157
         BNH   @DL00156                                         01S0157
@DC00156 DS    0H                                               01S0158
*   IF J1<=LENGTH(SYMBB) THEN       /* NON-NUMERIC DIGIT             */
         CH    J1,@CH00158                                      01S0158
         BNH   @RT00158                                         01S0158
*     GO TO EXIT;                   /* COMMON EXIT PATH              */
*                                                               01S0159
*    DO;/*BLRCNVCU(SYMBB) FROM(CHARACTER)TO(UNSIGNED BINARY)SET(ASID)RE
*ARK(ADDRESS SPACE IDENTIFIER)*/                                01S0160
*      /*ADDRESS SPACE IDENTIFIER*/                             01S0161
*      PACK(PACKED,SYMBB);/*EBCDIC=>PACKED DECIMAL*/            01S0161
         PACK  PACKED(8),SYMBB(3,ESBAPTR)                       01S0161
*      CVB(ASID,PACKED);/*PACKED DECIMAL=>UNSIGNED BINARY*/     01S0162
         CVB   ASID,PACKED                                      01S0162
*    END;/*BLRCNVCU(SYMBB) FROM(CHARACTER)TO(UNSIGNED BINARY)SET(ASID)R
*MARK(ADDRESS SPACE IDENTIFIER)*/                               01S0163
*   IF ASID=0 THEN                  /* ABSURD ADDRESS SPACE          */
         SLR   @14,@14                                          01S0164
         CR    ASID,@14                                         01S0164
         BE    @RT00164                                         01S0164
*     GO TO EXIT;                   /* COMMON EXIT PATH              */
*   ESBAAS=DATS;                    /* REAL STORAGE                  */
         MVC   ESBAAS(16,ESBAPTR),DATS                          01S0166
*   ESBAAS1=ZZ6DQA1;                /* PREFERRED CPU ADDRESS         */
         L     @10,ZZ2AZZ6P(,R9)                                01S0167
         MVC   ESBAAS1(4,ESBAPTR),ZZ6DQA1(@10)                  01S0167
*   RETC=ZZZFLAGI;                  /* RETURN CODE                   */
         SLR   RETC,RETC                                        01S0168
*   IF                              /* CRITICAL TABLE FOR DAT        */
*       ASID=ZZZAS2M&               /* MASTER SEGMENT TABLE          */
*       ZZ6DFAB=ZZZ1 THEN           /* ABSOLUTE STORAGE DUMP         */
*                                                               01S0169
         CH    ASID,@CH00046                                    01S0169
         BNE   @RF00169                                         01S0169
         TM    ZZ6DFAB(@10),B'00010000'                         01S0169
         BNO   @RF00169                                         01S0169
*     /***************************************************************/
*     /*                                                             */
*     /* CRITICAL SEGMENT TABLE FOR DYNAMIC ADDRESS TRANSLATION      */
*     /*                                                             */
*     /***************************************************************/
*                                                               01S0170
*     DO;                           /* CRITICAL TABLE FOR DAT        */
*       ESAU(1:ESAULRL)=ESBA(1:ESBALRL);/* INITIALIZE ESAU           */
         MVC   ESAU(182),ESBA(ESBAPTR)                          01S0171
*       ESAUSYM(1:7)='PSASTOR';     /* SYMBOLIC BASE                 */
         MVC   ESAUSYM(7),@CC01740                              01S0172
*       ESAULAD=ZZZZNULL;           /* ->PSA (LOCATION 0.)           */
         ST    @14,ESAULAD                                      01S0173
*       ESAUD=DATCP;                /* STORAGE CHARACTERISTICS       */
         MVC   ESAUD(60),DATCP                                  01S0174
*       ESAURL=0;                   /* NO REMARK                     */
         STH   @14,ESAURL                                       01S0175
*       CALL BLSRACC(ZZ2,ESAU,STOR);/* RETRIEVE STORAGE              */
         ST    R9,@AL00001                                      01S0176
         LA    @14,ESAU                                         01S0176
         ST    @14,@AL00001+4                                   01S0176
         LA    @14,STOR                                         01S0176
         ST    @14,@AL00001+8                                   01S0176
         L     @10,ZZ2RVTP(,R9)                                 01S0176
         L     @15,RVTRACCP(,@10)                               01S0176
         LA    @01,@AL00001                                     01S0176
         BALR  @14,@15                                          01S0176
*       IF RF^=ZZZFLAGI THEN        /* MASTER STOR NOT IN DUMP       */
*                                                               01S0177
         LTR   RF,RF                                            01S0177
         BZ    @RF00177                                         01S0177
*         /***********************************************************/
*         /*                                                         */
*         /* MASTER SEGMENT TABLE ORIGIN REGISTER NOT IN DUMP        */
*         /*                                                         */
*         /***********************************************************/
*                                                               01S0178
*         DO;                       /* MASTER STOR NOT IN DUMP       */
*           RETC=MAX(RF,ZZZFLAGS);  /* RETURN CODE                   */
         LA    RETC,12                                          01S0179
         CR    RETC,RF                                          01S0179
         BNL   *+6
         LR    RETC,RF                                          01S0179
*           GO TO EXIT;             /* COMMON EXIT PATH              */
         B     EXIT                                             01S0180
*         END;                      /* MASTER STOR NOT IN DUMP       */
*       CALL E100;                  /* VALIDATE STOR                 */
@RF00177 BAL   @14,E100                                         01S0182
*       IF RETC>ZZZFLAGE THEN       /* SGT001 VALIDATION FAILED      */
*                                                               01S0183
         CH    RETC,@CH00054                                    01S0183
         BNH   @RF00183                                         01S0183
*         /***********************************************************/
*         /*                                                         */
*         /* MASTER SEGMENT TABLE VALIDATION FAILED                  */
*         /*                                                         */
*         /***********************************************************/
*                                                               01S0184
*         DO;                       /* SGT001 VALIDATION FAILED      */
*           ASID=0;                 /* ASID                          */
         SLR   ASID,ASID                                        01S0185
*           GO TO EXIT;             /* COMMON EXIT PATH              */
         B     EXIT                                             01S0186
*         END;                      /* SGT001 VALIDATION FAILED      */
*                                                               01S0187
*        DO;/*BLSRARQ(ADD) RECORD(ESBA)ANCHOR(ZZ2VRESP)MF(EXECUTE,ARAU,
*OMPLETE)*/                                                     01S0188
@RF00183 DS    0H                                               01S0189
*        ARAURCDP=ADDR(ESBA);/*->RECORD*/                       01S0189
         ST    ESBAPTR,ARAURCDP                                 01S0189
*        ARAUARQP=ZZ2VRESP;/*->PREVIOUS ELEMENT*/               01S0190
         MVC   ARAUARQP(4),ZZ2VRESP(R9)                         01S0190
*        ZZ2VRESP=ADDR(ARAU);/*ADD ELEMENT*/                    01S0191
         LA    @14,ARAU                                         01S0191
         ST    @14,ZZ2VRESP(,R9)                                01S0191
*        END;/*BLSRARQ(ADD) RECORD(ESBA)ANCHOR(ZZ2VRESP)MF(EXECUTE,ARAU
*COMPLETE)*/                                                    01S0192
*     END;                          /* CRITICAL TABLE FOR DAT        */
*   SYMAA='ASCB';                   /* SYMBOL--PREFIX                */
@RF00169 MVC   SYMAA(4),@CC01653                                01S0194
*   SYMAB=SYMBB;                    /* SYMBOL--SUFFIX                */
         MVC   SYMAB(3),SYMBB(ESBAPTR)                          01S0195
*   ESAUSYM(LENGTH(SYMA)+1:ZZZSYML)=BLANKA;/* PAD WITH BLANKS        */
         MVC   ESAUSYM+7(24),BLANKA                             01S0196
*   ESAUDT=DATCAT;                  /* DATA TYPE                     */
         MVC   ESAUDT(34),DATCAT                                01S0197
*   CALL BLSRESGU(ZZ2,ESAU);        /* LOCATE ASCB                   */
         ST    R9,@AL00001                                      01S0198
         LA    @14,ESAU                                         01S0198
         ST    @14,@AL00001+4                                   01S0198
         L     @10,ZZ2RVTP(,R9)                                 01S0198
         L     @15,RVTESGUP(,@10)                               01S0198
         LA    @01,@AL00001                                     01S0198
         BALR  @14,@15                                          01S0198
*   IF RF>ZZZFLAGE THEN             /* ASCB NOT FOUND                */
*                                                               01S0199
         CH    RF,@CH00054                                      01S0199
         BNH   @RF00199                                         01S0199
*     /***************************************************************/
*     /*                                                             */
*     /* ASCB NOT FOUND                                              */
*     /*                                                             */
*     /***************************************************************/
*                                                               01S0200
*     DO;                           /* ASCB NOT FOUND                */
*       IF ASID^=ZZZAS2M THEN       /* NOT THE MASTER SEGMENT TABLE  */
         CH    ASID,@CH00046                                    01S0201
         BE    @RF00201                                         01S0201
*         RETC=RF;                  /* RETURN CODE                   */
         LR    RETC,RF                                          01S0202
*       GO TO EXIT;                 /* COMMON EXIT PATH              */
         B     EXIT                                             01S0203
*     END;                          /* ASCB NOT FOUND                */
*   ESAUD=DATCA;                    /* STORAGE ATTRIBUTES            */
@RF00199 MVC   ESAUD(60),DATCA                                  01S0205
*   ASCBPTR=ADDR(MYASCB);           /* ->ASCB IMAGE                  */
         LA    ASCBPTR,MYASCB                                   01S0206
*   CALL BLSRACC(ZZ2,ESAU,ASCB);    /* RETRIEVE IMAGE OF ASCB        */
         ST    R9,@AL00001                                      01S0207
         LA    @14,ESAU                                         01S0207
         ST    @14,@AL00001+4                                   01S0207
         ST    ASCBPTR,@AL00001+8                               01S0207
         L     @10,ZZ2RVTP(,R9)                                 01S0207
         L     @15,RVTRACCP(,@10)                               01S0207
         LA    @01,@AL00001                                     01S0207
         BALR  @14,@15                                          01S0207
*   IF RF^=ZZZFLAGI THEN            /* ASCB NOT IN THE DUMP          */
*                                                               01S0208
         LTR   RF,RF                                            01S0208
         BZ    @RF00208                                         01S0208
*     /***************************************************************/
*     /*                                                             */
*     /* ASCB NOT IN THE DUMP                                        */
*     /*                                                             */
*     /***************************************************************/
*                                                               01S0209
*     DO;                           /* ASCB NOT IN THE DUMP          */
*       IF ASID^=ZZZAS2M THEN       /* NOT THE MASTER SEGMENT TABLE  */
         CH    ASID,@CH00046                                    01S0210
         BE    @RF00210                                         01S0210
*         RETC=ZZZFLAGS;            /* SERIOUS ERROR                 */
         LA    RETC,12                                          01S0211
*       GO TO EXIT;                 /* COMMON EXIT PATH              */
         B     EXIT                                             01S0212
*     END;                          /* ASCB NOT IN THE DUMP          */
*   IF ASCBSEQN='7FFF'X THEN        /* ASCB NOT ON DISPATCHING QUEUE */
*                                                               01S0214
@RF00208 CLC   ASCBSEQN(2,ASCBPTR),@CH01743                     01S0214
         BNE   @RF00214                                         01S0214
*     /***************************************************************/
*     /*                                                             */
*     /* ASCB NOT ON DISPATCHING QUEUE                               */
*     /*                                                             */
*     /***************************************************************/
*                                                               01S0215
*     DO;                           /* ASCB NOT ON DISPATCHING QUEUE */
*       RETC=ZZZFLAGS;              /* SERIOUS ERROR                 */
         LA    RETC,12                                          01S0216
*       GO TO EXIT;                 /* COMMON EXIT PATH              */
         B     EXIT                                             01S0217
*     END;                          /* ASCB NOT ON DISPATCHING QUEUE */
*   IF                              /* CRITICAL TABLE FOR DAT        */
*       ASID=ZZZAS2M&               /* MASTER ADDRESS SPACE          */
*       ZZ6DFAB=ZZZ1 THEN           /* ABSOLUTE STORAGE DUMP         */
*                                                               01S0219
@RF00214 CH    ASID,@CH00046                                    01S0219
         BNE   @RF00219                                         01S0219
         L     @14,ZZ2AZZ6P(,R9)                                01S0219
         TM    ZZ6DFAB(@14),B'00010000'                         01S0219
         BNO   @RF00219                                         01S0219
*     /***************************************************************/
*     /*                                                             */
*     /* CRITICAL SEGMENT TABLE FOR DYNAMIC ADDRESS TRANSLATION      */
*     /*                                                             */
*     /***************************************************************/
*                                                               01S0220
*     DO;                           /* CRITICAL TABLE FOR DAT        */
*       IF ASCBSTOR^=STOR THEN      /* DISCREPENCY                   */
*                                                               01S0221
         CLC   ASCBSTOR(4,ASCBPTR),STOR                         01S0221
         BE    @RF00221                                         01S0221
*         /***********************************************************/
*         /*                                                         */
*         /* DISCREPENCY BETWEEN PSA AND ASCB                        */
*         /*                                                         */
*         /***********************************************************/
*                                                               01S0222
*         DO;                       /* DISCREPENCY                   */
*           IF ZZ2ASE<=ZZZFLAGE THEN/* ERROR MESSAGES PERMITTED      */
         CLI   ZZ2ASE(R9),8                                     01S0223
         BH    @RF00223                                         01S0223
*             CALL BLSUPUTA(ZZ2,M005);/* TRANSMIT MESSAGE M005       */
         ST    R9,@AL00001                                      01S0224
         LA    @14,M005                                         01S0224
         ST    @14,@AL00001+4                                   01S0224
         L     @10,ZZ2BVTP(,R9)                                 01S0224
         L     @15,BVTPUTAP(,@10)                               01S0224
         LA    @01,@AL00001                                     01S0224
         BALR  @14,@15                                          01S0224
*           RETC=ZZZFLAGE;          /* ERROR CONDITION               */
@RF00223 LA    RETC,8                                           01S0225
*         END;                      /* DISCREPENCY                   */
*     END;                          /* CRITICAL TABLE FOR DAT        */
*   ELSE                            /* NOT A CRITICAL SEGMENT TABLE  */
*                                                               01S0228
*     /***************************************************************/
*     /*                                                             */
*     /* NOT A CRITICAL SEGMENT TABLE                                */
*     /*                                                             */
*     /***************************************************************/
*                                                               01S0228
*     DO;                           /* NOT A CRITICAL SEGMENT TABLE  */
         B     @RC00219                                         01S0228
@RF00219 DS    0H                                               01S0229
*       STOR=ASCBSTOR;              /* SEGMENT TABLE ORIGIN REGISTER */
         MVC   STOR(4),ASCBSTOR(ASCBPTR)                        01S0229
*       CALL E100;                  /* VALIDATE STOR                 */
         BAL   @14,E100                                         01S0230
*     END;                          /* NOT A CRITICAL SEGMENT TABLE  */
*   IF RETC<ZZZFLAGS THEN           /* NO SERIOUS ERROR(S) DETECTED  */
@RC00219 CH    RETC,@CH00036                                    01S0232
         BNL   @RF00232                                         01S0232
*     RMKB=RMKS;                    /* REMARK                        */
         MVC   RMKB(15,ESBAPTR),RMKS                            01S0233
*EXIT:                              /* COMMON EXIT PATH              */
*   IF ZZ2VRESP=ADDR(ARAU) THEN     /* ACTIVE RECORD QUEUED  @ZA31997*/
*                                                               01S0234
@RF00232 DS    0H                                               01S0234
EXIT     LA    @14,ARAU                                         01S0234
         CL    @14,ZZ2VRESP(,R9)                                01S0234
         BNE   @RF00234                                         01S0234
*      DO;/*BLSRARQ(DELETE) ANCHOR(ZZ2VRESP)MF(EXECUTE,ARAU)*/  01S0235
*      ZZ2VRESP=ARAUARQP;/*DELETE ELEMENT*/                     01S0236
         MVC   ZZ2VRESP(4,R9),ARAUARQP                          01S0236
*      END;/*BLSRARQ(DELETE) ANCHOR(ZZ2VRESP)MF(EXECUTE,ARAU)*/ 01S0237
*   RETURN CODE(RETC);                                          01S0238
@RF00234 L     @13,4(,@13)                                      01S0238
         L     @00,@SIZDATD                                     01S0238
         LR    @01,@11                                          01S0238
         BLSUFRES R,LV=(0),A=(1)
         LR    @15,@04                                          01S0238
         L     @14,12(,@13)                                     01S0238
         LM    @00,@12,20(@13)                                  01S0238
         BR    @14                                              01S0238
         EJECT
*                                                               01S0239
*/*------------------------------------------------------------------*/
*/* E100--VALIDATE SEGMENT TABLE ORIGIN REGISTER                     */
*/*------------------------------------------------------------------*/
*                                                               01S0239
*E100:                              /* VALIDATE STOR                 */
*   PROCEDURE;                                                  01S0239
E100     STM   @14,@03,@SA00002                                 01S0239
         STM   @05,@12,@SA00002+24                              01S0239
*   IF                              /* VALID STOR                    */
*       STORL^=15×                  /* MVS USES FULL SEGMENT TABLES  */
*       STORP^=(STORP&'FFFFC0'X) THEN/* HARDWARE-REQUIRED ALIGNMENT  */
*                                                               01S0240
         CLI   STORL,15                                         01S0240
         BNE   @RT00240                                         01S0240
         SLR   @14,@14                                          01S0240
         ICM   @14,7,STORP                                      01S0240
         L     @10,@CF01748                                     01S0240
         NR    @10,@14                                          01S0240
         CR    @14,@10                                          01S0240
         BE    @RF00240                                         01S0240
@RT00240 DS    0H                                               01S0241
*     /***************************************************************/
*     /*                                                             */
*     /* INVALID MVS SEGMENT TABLE ORIGIN REGISTER                   */
*     /*                                                             */
*     /***************************************************************/
*                                                               01S0241
*     DO;                           /* INVALID STOR                  */
*       RETC=ZZZFLAGS;              /* RETURN CODE                   */
         LA    RETC,12                                          01S0242
*       GO TO L190;                 /* COMMON EXIT PATH              */
         B     L190                                             01S0243
*     END;                          /* INVALID STOR                  */
*   ESBALAD=STORP;                  /* ->SEGMENT TABLE (REAL STORAGE)*/
@RF00240 SLR   @14,@14                                          01S0245
         ICM   @14,7,STORP                                      01S0245
         ST    @14,ESBALAD(,ESBAPTR)                            01S0245
*   ESBADFA=ZZZ1;                   /* ARRAY                         */
         OI    ESBADFA(ESBAPTR),B'10000000'                     01S0246
*   ESBADIM=256;                    /* MVS==>256-ENTRY SEGMENT TABLE */
         MVC   ESBADIM(4,ESBAPTR),@CF00398                      01S0247
*   ESBADIL=0;                      /* SEGMENT 0=>ENTRY 0, ....      */
         SLR   @14,@14                                          01S0248
         ST    @14,ESBADIL(,ESBAPTR)                            01S0248
*   CALL BLSRSAGU(ZZ2,SAAU,ESBA);   /* SCAN SEGMENT TABLE            */
         ST    R9,@AL00001                                      01S0249
         LA    @14,SAAU                                         01S0249
         ST    @14,@AL00001+4                                   01S0249
         ST    ESBAPTR,@AL00001+8                               01S0249
         L     @10,ZZ2RVTP(,R9)                                 01S0249
         L     @15,RVTSAGUP(,@10)                               01S0249
         LA    @01,@AL00001                                     01S0249
         BALR  @14,@15                                          01S0249
*   RETC=MAX(RETC,RF,SAAUSRC);      /* RETURN CODE                   */
         LR    @14,RF                                           01S0250
         CR    @14,RETC                                         01S0250
         BNL   *+6
         LR    @14,RETC                                         01S0250
         SLR   @10,@10                                          01S0250
         IC    @10,SAAUSRC                                      01S0250
         CR    @14,@10                                          01S0250
         BNL   *+6
         LR    @14,@10                                          01S0250
         LR    RETC,@14                                         01S0250
*L190:                              /* COMMON EXIT PATH              */
*   RETURN;                                                     01S0251
@EL00002 DS    0H                                               01S0251
@EF00002 DS    0H                                               01S0251
@ER00002 LM    @14,@03,@SA00002                                 01S0251
         LM    @05,@12,@SA00002+24                              01S0251
         BR    @14                                              01S0251
*   END E100;                                                   01S0252
*                                                               01S0252
         B     @EL00002                                         01S0252
*/*BLSSPEND--FIND EXIT*/                                        01S0253
