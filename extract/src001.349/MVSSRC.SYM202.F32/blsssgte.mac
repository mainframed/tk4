*/*%INCLUDE SYSLIB  (IHAPSA  )                                        *
*;                                                              01S0256
@EL00001 L     @13,4(,@13)                                      01S0256
@EF00001 L     @00,@SIZDATD                                     01S0256
         LR    @01,@11                                          01S0256
         BLSUFRES R,LV=(0),A=(1)
@ER00001 LM    @14,@12,12(@13)                                  01S0256
         BR    @14                                              01S0256
@DATA    DS    0H
@CH00046 DC    H'1'
@CH00158 DC    H'3'
@CH00054 DC    H'8'
@CH00036 DC    H'12'
@CH01743 DC    XL2'7FFF'
@DATD    DSECT
         DS    0F
@SA00001 DS    18F
@PC00001 DS    2F
@SA00002 DS    14F
@AL00001 DS    3A
BLSSSGTE CSECT
         DS    0F
