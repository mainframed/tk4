*    DECLARE /* COMMON VARIABLES */                             01S0254
*      I256C CHAR(256) BASED,                                   01S0254
*      I031F FIXED(31) BASED,                                   01S0254
*      I031P PTR(31)   BASED,                                   01S0254
*      I015F FIXED(15) BASED,                                   01S0254
*      I015P PTR(15)   BASED,                                   01S0254
*      I008P PTR(8)    BASED,                                   01S0254
*      I001C CHAR(1)   BASED;                                   01S0254
