         TITLE '**********  ICKWI01:  WRITE IPL RECORDS  ***************
               *********'
        MACRO
        GETMAIN &R,&LV=
        L     REG15,0(REG1)           GDTTBL ADDRESS
        L     REG15,GDTPRL(REG15)     PROL ROUTINE ADDRESS
        CNOP  2,4                     BOUNDARY ALIGNMENT
        LA    REG14,*+14              RETURN ADDRESS
        BALR  REG1,REG15              LINKAGE TO PROL ROUTINE
        DC    A(MODID)                MODULE ID ADDRESS
        DC    A(@SIZDATD)             AUTOMATIC STORAGE SIZE ADDRESS
        MEND
ICKWI01  CSECT ,                                                   0002
@MAINENT DS    0H                                                  0002
         USING *,@15                                               0002
         B     @PROLOG                                             0002
         DC    AL1(16)                                             0002
         DC    C'ICKWI01   78.192'                                 0002
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0002
         BALR  @12,0                                               0002
@PSTART  DS    0H                                                  0002
         USING @PSTART,@12                                         0002
         L     @00,@SIZDATD                                        0002
         GETMAIN  R,LV=(0)
         LR    @11,@01                                             0002
         USING @DATD,@11                                           0002
         ST    @13,@SA00001+4                                      0002
         LM    @00,@01,20(@13)                                     0002
         ST    @11,8(,@13)                                         0002
         LR    @13,@11                                             0002
         XC    @ZTEMPS(@ZLEN),@ZTEMPS
         MVC   @PC00001(24),0(@01)                                 0002
*                                                                  0176
         EJECT
*/*****  B E G I N    E X E C U T I O N  *****************************/
*                                                                  0177
*    IF GDTDBG = NULLPTR                                           0177
*      THEN;                                                       0177
         L     @10,@PC00001                                        0177
         L     @10,GDTDBG(,@10)                                    0177
         LTR   @10,@10                                             0177
         BZ    @RT00177                                            0177
*      ELSE                                                        0179
*        CALL ICKDB010 (GDTTBL                                     0179
*          ,'WI00');                                               0179
*                                                                  0179
*                                                                  0179
         L     @10,@PC00001                                        0179
         ST    @10,@AL00001                                        0179
         LA    @15,@CC00588                                        0179
         ST    @15,@AL00001+4                                      0179
         MVI   @AL00001+4,X'80'                                    0179
         L     @15,GDTDBG(,@10)                                    0179
         LA    @01,@AL00001                                        0179
         BALR  @14,@15                                             0179
*/*  ESTABLISH UPRINT ARGUMENT LIST                                  */
*    PRTHDR = 'DARGLIST';                                          0180
@RT00177 MVC   PRTHDR(8),@CC00589                                  0180
*    DDSTRU = ADDR(PRTARGS);                                       0181
         LA    @10,PRTARGS                                         0181
         ST    @10,DDSTRU                                          0181
*    PRTARGS = ''B;                                                0182
         XC    PRTARGS(20),PRTARGS                                 0182
*    DARGSMOD = MSGMODZ;                                           0183
         MVC   DARGSMOD(3,@10),MSGMODZ                             0183
*/*  SET CONDCODE INITIALLY TO INDICATE SUCCESSFUL COMPLETION        */
*    CONDCODE = SUCCEEDS;                                          0184
         L     @10,@PC00001+20                                     0184
         SLR   @15,@15                                             0184
         ST    @15,CONDCODE(,@10)                                  0184
*    TRKCCHH = ''B;                                                0185
         XC    TRKCCHH(4),TRKCCHH                                  0185
*/*  OBTAIN BOOTSTRAP RECORDS, MAXIMUM ALLOWABLE LENGTH OF THE IPL   */
*/*   PROGRAM RECORD, AND THE RECORD OVERHEAD FOR THE VOLUME         */
*/*   (UDEVINFO)                                                     */
*    DO I = 1 TO INFOSET;                                          0186
*                                                                  0186
         LA    I,1                                                 0186
@DL00186 DS    0H                                                  0187
*      CALL ICKDVIN0 (GDTTBL                                       0187
*               ,VOLIB                                             0187
*               ,INFOVECT (I)                                      0187
*               ,POOLID                                            0187
*               ,INFOPTR (I)                                       0187
*               ,INFOLEN (I)                                       0187
*               ,CCODE(I));                                        0187
         L     @10,@PC00001                                        0187
         ST    @10,@AL00001                                        0187
         L     @03,@PC00001+4                                      0187
         ST    @03,@AL00001+4                                      0187
         LR    @03,I                                               0187
         SLA   @03,2                                               0187
         LA    @15,INFOVECT-4(@03)                                 0187
         ST    @15,@AL00001+8                                      0187
         LA    @15,POOLID                                          0187
         ST    @15,@AL00001+12                                     0187
         LA    @15,INFOPTR-4(@03)                                  0187
         ST    @15,@AL00001+16                                     0187
         LA    @15,INFOLEN-4(@03)                                  0187
         ST    @15,@AL00001+20                                     0187
         LA    @03,CCODE-4(@03)                                    0187
         ST    @03,@AL00001+24                                     0187
         L     @15,GDTDIN(,@10)                                    0187
         LA    @01,@AL00001                                        0187
         BALR  @14,@15                                             0187
*    END;                                                          0188
         AH    I,@CH00167                                          0188
         CH    I,@CH00046                                          0188
         BNH   @DL00186                                            0188
*    BOLENGTH(1) = LENGTH(BOOTSTR1);                               0189
         MVC   BOLENGTH(4),@CF00512                                0189
*    BOLENGTH(2) = LENGTH(BOOTSTR2);                               0190
         MVC   BOLENGTH+4(4),@CF00514                              0190
*/*  READ_VOLUME_LABEL                                               */
*    CALL ICKRL01                                                  0191
*         (GDTTBL                                                  0191
*         ,VOLIB                                                   0191
*         ,VLNIMAGE                                                0191
*         ,LABELS                                                  0191
*         ,RETCODE);                                               0191
         L     @10,@PC00001                                        0191
         ST    @10,@AL00001                                        0191
         L     @10,@PC00001+4                                      0191
         ST    @10,@AL00001+4                                      0191
         LA    @10,VLNIMAGE                                        0191
         ST    @10,@AL00001+8                                      0191
         LA    @10,LABELS                                          0191
         ST    @10,@AL00001+12                                     0191
         LA    @10,RETCODE                                         0191
         ST    @10,@AL00001+16                                     0191
         L     @15,@CV00592                                        0191
         LA    @01,@AL00001                                        0191
         BALR  @14,@15                                             0191
*/*  IF UNABLE TO READ VOLUME LABEL                                  */
*    IF RETCODE = FAILURE                                          0192
*/*    THEN                                                          */
*      THEN DO;                                                    0192
*                                                                  0192
         CLC   RETCODE(4),@CF00167                                 0192
         BNE   @RF00192                                            0192
*/*      ISSUE ERROR MESSAGE (MSGVOL) (UPRINT)                       */
*        DARGSENT = MSGVOL;                                        0194
         L     @10,DDSTRU                                          0194
         MVI   DARGSENT(@10),X'28'                                 0194
*        CALL ICKTPPR0 (GDTTBL                                     0195
*               ,PRTFILE                                           0195
*               ,DDSTRU);                                          0195
         L     @10,@PC00001                                        0195
         ST    @10,@AL00001                                        0195
         LA    @15,@CF00096                                        0195
         ST    @15,@AL00001+4                                      0195
         LA    @15,DDSTRU                                          0195
         ST    @15,@AL00001+8                                      0195
         MVI   @AL00001+8,X'80'                                    0195
         L     @15,GDTPRT(,@10)                                    0195
         LA    @01,@AL00001                                        0195
         BALR  @14,@15                                             0195
*/*      SET CONDCODE TO INDICATE UNSUCCESSFUL COMPLETION (1)        */
*        CONDCODE = FAILS;                                         0196
         L     @10,@PC00001+20                                     0196
         MVC   CONDCODE(4,@10),@CF00167                            0196
*/*      END-THEN                                                    */
*        END;                                                      0197
*/*  IF NO ERRORS HAVE BEEN ENCOUNTERED (CONDCODE = 0)               */
*    IF CONDCODE = SUCCEEDS                                        0198
*/*    THEN                                                          */
*      THEN DO;                                                    0198
*                                                                  0198
@RF00192 SLR   @10,@10                                             0198
         L     @15,@PC00001+20                                     0198
         C     @10,CONDCODE(,@15)                                  0198
         BNE   @RF00198                                            0198
*/*      IF USER VOLUME LABELS EXIST ON THE VOLUME                   */
*        IF LABELS > 0                                             0200
*/*        THEN                                                      */
*          THEN DO;                                                0200
*                                                                  0200
         L     @15,LABELS                                          0200
         CR    @15,@10                                             0200
         BNH   @RF00200                                            0200
*/*          IF PURGING IS NOT PERMITTED                             */
*            IF PRGFLG = NOPURGE                                   0202
*/*            THEN                                                  */
*              THEN DO;                                            0202
*                                                                  0202
         L     @10,@PC00001+12                                     0202
         TM    PRGFLG(@10),B'10000000'                             0202
         BNZ   @RF00202                                            0202
*/*              COMPUTE THE ALLOWABLE LENGTH FOR THE IPL PROGRAM    */
*                PROGLEN = PROGLEN - LABELS * (UVLENGTH + OVERHEAD);
         L     @10,INFOPTR+8                                       0204
         L     @01,INFOPTR+12                                      0204
         LA    @03,84                                              0204
         AL    @03,OVERHEAD(,@01)                                  0204
         MR    @02,@15                                             0204
         LCR   @03,@03                                             0204
         AL    @03,PROGLEN(,@10)                                   0204
         ST    @03,PROGLEN(,@10)                                   0204
*/*              SET THE PROPER RECORD NUMBER FOR THE IPL PROGRAM    */
*                IPLRECNO = LABELS + 4;                            0205
         AH    @15,@CH00046                                        0205
         ST    @15,IPLRECNO                                        0205
*/*              SET THE PROPER RECORD NUMBER IN BOOTSTRAP RECORD 2  */
*/*               SEARCH ADDRESS                                     */
*                B2SRCHR = IPLRECNO;                               0206
         L     @10,INFOPTR+4                                       0206
         STC   @15,B2SRCHR(,@10)                                   0206
*/*              END-THEN (PURGING IS NOT PERMITTED)                 */
*                END;                                              0207
*/*            ELSE                                                  */
*              ELSE DO;                                            0208
*                                                                  0208
         B     @RC00202                                            0208
@RF00202 DS    0H                                                  0209
*/*              SET RECORD NUMBER TO 4                              */
*                IPLRECNO = 4;                                     0209
         MVC   IPLRECNO(4),@CF00046                                0209
*/*              END-ELSE                                            */
*                END;                                              0210
*/*          END-THEN (USER VOLUME LABELS EXIST ON THE VOLUME)       */
*            END;                                                  0211
*/*        ELSE (NO VOLUME LABELS EXIST ON THE VOLUME)               */
*          ELSE DO;                                                0212
*                                                                  0212
         B     @RC00200                                            0212
@RF00200 DS    0H                                                  0213
*/*          SET RECORD NUMBER FOR THE IPL RECORD TO 4               */
*            IPLRECNO = 4;                                         0213
         MVC   IPLRECNO(4),@CF00046                                0213
*/*          END-ELSE                                                */
*            END;                                                  0214
*/*      SET ALLOWABLE PROGRAM LENGTH IN THE BOOTSTRAP RECORD 2      */
*        BO2DCCNT = PROGLEN;                                       0215
@RC00200 L     @10,INFOPTR+4                                       0215
         L     @01,INFOPTR+8                                       0215
         MVC   BO2DCCNT(2,@10),PROGLEN+2(@01)                      0215
*/*      END-THEN (NO ERROR INDICATED)                               */
*        END;                                                      0216
*/*  IF NO ERRORS HAVE BEEN ENCOUNTERED (CONDCODE = 0)               */
*    IF CONDCODE = SUCCEEDS                                        0217
*/*    THEN                                                          */
*      THEN DO;                                                    0217
*                                                                  0217
@RF00198 L     @10,@PC00001+20                                     0217
         L     @10,CONDCODE(,@10)                                  0217
         LTR   @10,@10                                             0217
         BNZ   @RF00217                                            0217
*/*      OPEN THE IPL DATA SET (UOPEN)                               */
*        OPNPTR = ADDR(OPENLIST);                                  0219
         LA    OPNPTR,OPENLIST                                     0219
*        OPENLIST = ''B;                                           0220
         XC    OPENLIST(4),OPENLIST                                0220
*        OPNOPTIN = YES;                                           0221
         OI    OPNOPTIN(OPNPTR),B'10000000'                        0221
*        OPNIOC = ADDR(IOCSPTR);                                   0222
         LA    @10,IOCSPTR                                         0222
         ST    @10,OPNIOC(,OPNPTR)                                 0222
*        OPNDDN = ADDR(DDNAME);                                    0223
         L     @10,@PC00001+8                                      0223
         ST    @10,OPNDDN(,OPNPTR)                                 0223
*        CALL ICKIOOP0 (GDTTBL                                     0224
*              ,OPENLIST);                                         0224
         L     @10,@PC00001                                        0224
         ST    @10,@AL00001                                        0224
         LA    @15,OPENLIST                                        0224
         ST    @15,@AL00001+4                                      0224
         MVI   @AL00001+4,X'80'                                    0224
         L     @15,GDTOPN(,@10)                                    0224
         LA    @01,@AL00001                                        0224
         BALR  @14,@15                                             0224
*        RESPECIFY RTNREG RSTD;                                    0225
*        TESTRC = RTNREG;                                          0226
         STH   RTNREG,TESTRC                                       0226
*        RESPECIFY RTNREG UNRSTD;                                  0227
*/*      IF UNABLE TO OPEN THE DATA SET                              */
*        IF TESTRC = 4                                             0228
*/*        THEN                                                      */
*          THEN DO;                                                0228
*                                                                  0228
         LH    @10,TESTRC                                          0228
         CH    @10,@CH00046                                        0228
         BNE   @RF00228                                            0228
*/*          INDICATE UNSUCCESSFUL COMPLETION                        */
*            CONDCODE = FAILS;                                     0230
         L     @10,@PC00001+20                                     0230
         MVC   CONDCODE(4,@10),@CF00167                            0230
*/*          ISSUE ERROR MESSAGE (MSGOPEN) (UPRINT)                  */
*            DARGSENT = MSGOPEN;                                   0231
         L     @10,DDSTRU                                          0231
         MVI   DARGSENT(@10),X'29'                                 0231
*            CALL ICKTPPR0 (GDTTBL                                 0232
*                   ,PRTFILE                                       0232
*                   ,DDSTRU);                                      0232
         L     @10,@PC00001                                        0232
         ST    @10,@AL00001                                        0232
         LA    @15,@CF00096                                        0232
         ST    @15,@AL00001+4                                      0232
         LA    @15,DDSTRU                                          0232
         ST    @15,@AL00001+8                                      0232
         MVI   @AL00001+8,X'80'                                    0232
         L     @15,GDTPRT(,@10)                                    0232
         LA    @01,@AL00001                                        0232
         BALR  @14,@15                                             0232
*/*          END-THEN                                                */
*            END;                                                  0233
*/*      END-THEN                                                    */
*        END;                                                      0234
@RF00228 DS    0H                                                  0235
*/*  IF NO ERRORS HAVE BEEN ENCOUNTERED                              */
*    IF CONDCODE = SUCCEEDS                                        0235
*/*    THEN                                                          */
*      THEN DO;                                                    0235
*                                                                  0235
@RF00217 L     @10,@PC00001+20                                     0235
         L     @10,CONDCODE(,@10)                                  0235
         LTR   @10,@10                                             0235
         BNZ   @RF00235                                            0235
*/*      IF THE BOOTSTRAP RECORDS ARE SUPPLIED BY THE USER           */
*        IF BOOTFLG = BOOTYES                                      0237
*/*        THEN                                                      */
*          THEN DO;                                                0237
*                                                                  0237
         L     @10,@PC00001+16                                     0237
         TM    BOOTFLG(@10),B'10000000'                            0237
         BNO   @RF00237                                            0237
*/*          READ IN THE FIRST IPL BOOTSTRAP RECORD (UGET)           */
*            CALL ICKIOGT0 (GDTTBL                                 0239
*                 ,IOCSPTR);                                       0239
         L     @10,@PC00001                                        0239
         ST    @10,@AL00001                                        0239
         LA    @02,IOCSPTR                                         0239
         ST    @02,@AL00001+4                                      0239
         MVI   @AL00001+4,X'80'                                    0239
         L     @15,GDTGET(,@10)                                    0239
         LA    @01,@AL00001                                        0239
         BALR  @14,@15                                             0239
*            INRECPTR = IOCDAD;                                    0240
         L     @10,IOCSPTR                                         0240
         L     INRECPTR,IOCDAD(,@10)                               0240
*/*          IF IDENTIFYING CHARACTERS ARE WRONG                     */
*            IF INREC (1:4) ^= BSKEYV (1)                          0241
*/*            THEN                                                  */
*              THEN DO;                                            0241
*                                                                  0241
         CLC   INREC(4,INRECPTR),BSKEYV                            0241
         BE    @RF00241                                            0241
*/*              ISSUE ERROR MESSAGE (MSGBOOT(1)) (UPRINT)           */
*                DARGSENT = MSGBOOT(1);                            0243
         L     @10,DDSTRU                                          0243
         MVC   DARGSENT(1,@10),MSGBOOT                             0243
*                CALL ICKTPPR0 (GDTTBL                             0244
*                       ,PRTFILE                                   0244
*                       ,DDSTRU);                                  0244
         L     @10,@PC00001                                        0244
         ST    @10,@AL00001                                        0244
         LA    @15,@CF00096                                        0244
         ST    @15,@AL00001+4                                      0244
         LA    @15,DDSTRU                                          0244
         ST    @15,@AL00001+8                                      0244
         MVI   @AL00001+8,X'80'                                    0244
         L     @15,GDTPRT(,@10)                                    0244
         LA    @01,@AL00001                                        0244
         BALR  @14,@15                                             0244
*/*              SET CONDCODE TO INDICATE UNSUCCESSFUL COMPLETION    */
*                CONDCODE = FAILS;                                 0245
         L     @10,@PC00001+20                                     0245
         MVC   CONDCODE(4,@10),@CF00167                            0245
*/*              END-THEN                                            */
*                END;                                              0246
*/*            ELSE SAVE THE RECORD                                  */
*              ELSE DO;                                            0247
*                                                                  0247
         B     @RC00241                                            0247
@RF00241 DS    0H                                                  0248
*                BOOTSTR1 (13:36) = INREC (5:28);                  0248
         L     @10,INFOPTR                                         0248
         MVC   BOOTSTR1+12(24,@10),INREC+4(INRECPTR)               0248
*                END;                                              0249
*/*          END-THEN                                                */
*            END;                                                  0250
@RC00241 DS    0H                                                  0251
*/*      END-THEN                                                    */
*        END;                                                      0251
@RF00237 DS    0H                                                  0252
*/*  IF NO ERRORS HAVE BEEN ENCOUNTERED                              */
*    IF CONDCODE = SUCCEEDS                                        0252
*/*    THEN                                                          */
*      THEN DO;                                                    0252
*                                                                  0252
@RF00235 L     @10,@PC00001+20                                     0252
         L     @10,CONDCODE(,@10)                                  0252
         LTR   @10,@10                                             0252
         BNZ   @RF00252                                            0252
*/*      IF THE BOOTSTRAP RECORDS ARE SUPPLIED BY THE USER           */
*        IF BOOTFLG = BOOTYES                                      0254
*/*        THEN                                                      */
*          THEN DO;                                                0254
*                                                                  0254
         L     @10,@PC00001+16                                     0254
         TM    BOOTFLG(@10),B'10000000'                            0254
         BNO   @RF00254                                            0254
*/*          DO-FOR THE NEXT THREE RECORDS                           */
*            DO I = 1 TO 3;                                        0256
         LA    I,1                                                 0256
@DL00256 DS    0H                                                  0257
*/*            READ IN THE SECOND IPL BOOTSTRAP RECORD (UGET)        */
*              CALL ICKIOGT0 (GDTTBL                               0257
*                   ,IOCSPTR);                                     0257
         L     @10,@PC00001                                        0257
         ST    @10,@AL00001                                        0257
         LA    @15,IOCSPTR                                         0257
         ST    @15,@AL00001+4                                      0257
         MVI   @AL00001+4,X'80'                                    0257
         L     @15,GDTGET(,@10)                                    0257
         LA    @01,@AL00001                                        0257
         BALR  @14,@15                                             0257
*              INRECPTR = IOCDAD;                                  0258
         L     @10,IOCSPTR                                         0258
         L     INRECPTR,IOCDAD(,@10)                               0258
*/*            IF IDENTIFYING CHARACTERS ARE WRONG                   */
*              IF INREC (1:4) ^= BSKEYV (2)                        0259
*/*              THEN                                                */
*                THEN DO;                                          0259
*                                                                  0259
         CLC   INREC(4,INRECPTR),BSKEYV+4                          0259
         BE    @RF00259                                            0259
*/*                ISSUE ERROR MESSAGE (MSGBOOT(2)) (UPRINT)         */
*                  DARGSENT = MSGBOOT(2);                          0261
         L     @10,DDSTRU                                          0261
         MVC   DARGSENT(1,@10),MSGBOOT+1                           0261
*                  CALL ICKTPPR0 (GDTTBL                           0262
*                         ,PRTFILE                                 0262
*                         ,DDSTRU);                                0262
         L     @10,@PC00001                                        0262
         ST    @10,@AL00001                                        0262
         LA    @15,@CF00096                                        0262
         ST    @15,@AL00001+4                                      0262
         LA    @15,DDSTRU                                          0262
         ST    @15,@AL00001+8                                      0262
         MVI   @AL00001+8,X'80'                                    0262
         L     @15,GDTPRT(,@10)                                    0262
         LA    @01,@AL00001                                        0262
         BALR  @14,@15                                             0262
*/*                SET CONDCODE TO INDICATE UNSUCCESSFUL COMPLETION  */
*                  CONDCODE = FAILS;                               0263
         L     @10,@PC00001+20                                     0263
         MVC   CONDCODE(4,@10),@CF00167                            0263
*/*                LEAVE                                             */
*                  GOTO ENDFOR1;                                   0264
         B     ENDFOR1                                             0264
*/*                END-THEN                                          */
*                  END;                                            0265
*/*              ELSE SAVE THE RECORD                                */
*                ELSE DO;                                          0266
*                                                                  0266
@RF00259 DS    0H                                                  0267
*                  IF I = 1                                        0267
*                    THEN DO;                                      0267
*                                                                  0267
         CH    I,@CH00167                                          0267
         BNE   @RF00267                                            0267
*                      BO2DATA (1:68) = INREC (5:72);              0269
         L     @10,INFOPTR+4                                       0269
         MVC   BO2DATA(68,@10),INREC+4(INRECPTR)                   0269
*                      END;                                        0270
*                  IF I = 2                                        0271
*                    THEN DO;                                      0271
*                                                                  0271
@RF00267 CH    I,@CH00118                                          0271
         BNE   @RF00271                                            0271
*                      BO2DATA (69:136) = INREC (5:72);            0273
         L     @10,INFOPTR+4                                       0273
         MVC   BO2DATA+68(68,@10),INREC+4(INRECPTR)                0273
*                      END;                                        0274
*                  IF I = 3                                        0275
*                    THEN DO;                                      0275
*                                                                  0275
@RF00271 CH    I,@CH00138                                          0275
         BNE   @RF00275                                            0275
*                      BO2DATA (137:144) = INREC (5:12);           0277
         L     @10,INFOPTR+4                                       0277
         MVC   BO2DATA+136(8,@10),INREC+4(INRECPTR)                0277
*                      END;                                        0278
*                  END;                                            0279
@RF00275 DS    0H                                                  0280
*/*            END-FOR                                               */
*              END;                                                0280
         AL    I,@CF00167                                          0280
         CH    I,@CH00138                                          0280
         BNH   @DL00256                                            0280
*/*          END-THEN                                                */
*            ENDFOR1:                                              0281
*            END;                                                  0281
ENDFOR1  DS    0H                                                  0282
*/*      END-THEN                                                    */
*        END;                                                      0282
@RF00254 DS    0H                                                  0283
*/*  DO-FOR BOTH THE BOOT-STRAP RECORDS                              */
*    DO I = 1 TO 2;                                                0283
@RF00252 LA    I,1                                                 0283
@DL00283 DS    0H                                                  0284
*/*    IF NO ERRORS HAVE BEEN ENCOUNTERED (CONDCODE = 0)             */
*      IF CONDCODE = SUCCEEDS                                      0284
*/*      THEN                                                        */
*        THEN DO;                                                  0284
*                                                                  0284
         L     @10,@PC00001+20                                     0284
         L     @10,CONDCODE(,@10)                                  0284
         LTR   @10,@10                                             0284
         BNZ   @RF00284                                            0284
*/*        WRITE THE IPL BOOTSTRAP RECORD ON THE VOLUME              */
*          DATAPTR = INFOPTR (I) + 8;                              0286
         LA    @10,8                                               0286
         LR    @03,I                                               0286
