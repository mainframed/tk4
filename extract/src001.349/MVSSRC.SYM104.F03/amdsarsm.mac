         MVC   BCTINDEX(1,BCTPTR),SPCTSSID(R1)                  01S0403
*   BCTRBA=R1->SPCTSSID(2:3);       /* RBA PART OF LSID      @Z40WI3F*/
         MVC   BCTRBA(2,BCTPTR),SPCTSSID+1(R1)                  01S0404
*   BCTKEY1='01'X;                  /* DUMMY PROTECT KEY     @Z40WI3F*/
         MVI   BCTKEY1(BCTPTR),X'01'                            01S0405
*   BCTKEY2='01'X;                  /* DUMMY PROTECT KEY     @Z40WI3F*/
         MVI   BCTKEY2(BCTPTR),X'01'                            01S0406
*   IF SPCTPSET=ON THEN             /* ON PAGE DATA SET?     @Z40WI3F*/
         L     @14,ASCBPTR(,VCCTREG)                            01S0407
         L     @14,RSMHDPTR(,@14)                               01S0407
         L     @14,SPCTPTR(,@14)                                01S0407
         TM    SPCTPSET(@14),B'00001000'                        01S0407
         BNO   @RF00407                                         01S0407
