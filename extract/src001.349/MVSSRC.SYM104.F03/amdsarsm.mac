@ER00001 LM    @14,@12,12(@13)                                  01S0417
         BR    @14                                              01S0417
*                                                               01S0418
*   /*****************************************************************/
*   /*                                                               */
*   /* REAL - TO - VIRTUAL CONVERSION ROUTINE. THIS ROUTINE USES THE */
*   /* PAGE FRAME TABLE TO CONVERT A REAL ADDRESS, PASSED IN REGISTER*/
*   /* 1, TO THE CORRESPONDING VIRTUAL ADDRESS, RETURNED IN REGISTER */
*   /* 1. IF THE RBN IS INVALID, I.E. NOT DEFINED BY THE PAGE FRAME  */
*   /* TABLE, A RETURN CODE OF 4 IS SET. OTHERWISE A RETURN CODE OF 0*/
*   /* IS SET.                                                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0418
*/*RSMVIRT: E CONVERT REAL ADDR                                      */
*/* N REAL ADDRESS IN REGISTER 1                                     */
*/* P SAVE DISPLACEMENT PORTION OF ADDR                              */
*/* P GET RBN                                                        */
*/* S RSMRBN: CHECK FOR VALID RBN                                    */
*/* D (YES,%RBNVLD,NO,) RBN VALID                                    */
*/* P SET RETURN CODE 4                                              */
*/* R RETURN                                                         */
*/*%RBNVLD: P COMPUTE PFTE ADDRESS                                   */
*/* P GET VBN FROM PFTE                                              */
*/* P RETURN VIRTUAL ADDRESS IN REGISTER 1                           */
*/* P SET RETURN CODE 0                                              */
*/* R RETURN                                                         */
*                                                               01S0418
*RSMVIRT:                                                       01S0418
*   PROC OPTIONS(NOSAVE(1));                                    01S0418
RSMVIRT  STM   @14,@00,@SA00002                                 01S0418
         STM   @02,@12,@SA00002+12                              01S0418
*   RESPECIFY                                                   01S0419
*    (R1,                                                       01S0419
*     RETCODE) RESTRICTED;                                      01S0419
*   RSMVADDR=R1&DISPLMNT;           /* GET DISPLACEMENT PORTION OF
*                                      REAL ADDRESS                  */
         LR    @14,R1                                           01S0420
         N     @14,@CF01848                                     01S0420
         ST    @14,RSMVADDR                                     01S0420
*   R1=EVAL((R1&ADDRMASK)/PAGESIZE)*SHF4BITS;/* GET RBN WITH 4 LOW
*                                      ORDER ZEROES                  */
         LR    @02,R1                                           01S0421
         N     @02,@CF01837                                     01S0421
         SRL   @02,12                                           01S0421
         SLA   @02,4                                            01S0421
         LR    R1,@02                                           01S0421
*   CALL RSMRBN;                    /* CHECK FOR VALID RBN           */
         BAL   @14,RSMRBN                                       01S0422
*   IF RETCODE^=0 THEN              /* RBN INVALID                   */
         LTR   RETCODE,RETCODE                                  01S0423
         BZ    @RF00423                                         01S0423
*     RETURN CODE(RETCODE);         /* YES - ERROR RETURN            */
         L     @14,@SA00002                                     01S0424
         L     @00,@SA00002+8                                   01S0424
         LM    @02,@12,@SA00002+12                              01S0424
         BR    @14                                              01S0424
*   PFTEPTR=PVTPFTP+R1*(LENGTH(PFTE)/SHF4BITS);/* COMPUTE PFTE  01S0425
*                                      ADDRESS                       */
@RF00423 L     @14,CVTPTR                                       01S0425
         L     @14,PVTPTR(,@14)                                 01S0425
         LR    PFTEPTR,R1                                       01S0425
         AL    PFTEPTR,PVTPFTP(,@14)                            01S0425
*   RSMVADDR(2:3)=RSMVADDR(2:3)PFTVBN;/* CONCATENATE VBN WITH  01S0426
*                                      DISPLACEMENT                  */
         OC    RSMVADDR+1(2),PFTVBN(PFTEPTR)                    01S0426
*   R1=RSMVADDR;                    /* GET VIRTUAL ADDRESS           */
         L     R1,RSMVADDR                                      01S0427
*   RETURN CODE(0);                 /* GOOD RETURN                   */
         SLR   @15,@15                                          01S0428
         L     @14,@SA00002                                     01S0428
         L     @00,@SA00002+8                                   01S0428
         LM    @02,@12,@SA00002+12                              01S0428
         BR    @14                                              01S0428
*   RESPECIFY                                                   01S0429
*    (R1,                                                       01S0429
*     RETCODE) UNRESTRICTED;                                    01S0429
*   END RSMVIRT;                                                01S0430
*                                                               01S0431
*   /*****************************************************************/
*   /*                                                               */
*   /* VALIDITY CHECK RBN - CHECK IF RBN (WITH 4 LOW ORDER ZEROES) IS*/
*   /* IN RANGE DEFINED BY THE PAGE FRAME TABLE. IF NOT, SET ERROR   */
*   /* RETURN CODE, OTHERWISE SET 0 RETURN CODE. RBN IS PASSED IN    */
*   /* REGISTER 1.                                                   */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0431
*/*RSMRBN: E CHECK RBN                                               */
*/* N RBN IN REGISTER 1                                              */
*/* P SET UP RETURN CODE 4                                           */
*/* D (YES,,NO,%RBNRET) RBN >= FIRST RBN IN PFT                      */
*/* D (YES,,NO,%RBNRET) RBN <= LAST RBN IN PFT                       */
*/* P SET UP RETURN CODE 0                                           */
*/*%RBNRET: R RETURN                                                 */
*                                                               01S0431
*RSMRBN:                                                        01S0431
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                            01S0431
RSMRBN   DS    0H                                               01S0432
*   RESPECIFY                                                   01S0432
*    (R0) RESTRICTED;               /* ALTHOUGH R0 IS NOT USED IN
*                                      THIS ROUTINE, ITS VALUE MUST
*                                      BE PRESERVED. IT IS CALCULATED
*                                      BEFORE THIS ROUTINE IS CALLED
*                                      AND USED AFTER THIS ROUTINE IS
*                                      CALLED, IN SOME CASES. WITHOUT
*                                      THIS RESPECIFY, THE COMPILER
*                                      MIGHT PICK R0 TO USE AS A WORK
*                                      REGISTER IN THIS ROUTINE. THIS
*                                      WOULD RESULT IN THE VALUE IN
*                                      R0 BEING DESTROYED.   @Z40WI3F*/
*   RESPECIFY                                                   01S0433
*    (R1,                                                       01S0433
*     RETREG,                                                   01S0433
*     RETCODE) RESTRICTED;                                      01S0433
*   RETCODE=4;                      /* SET UP ERROR RETURN CODE      */
         LA    RETCODE,4                                        01S0434
*   IF(R1>=PVTFPFN)&(R1<=PVTLPFN) THEN/* RBN IN RANGE                */
         L     @06,CVTPTR                                       01S0435
         L     @06,PVTPTR(,@06)                                 01S0435
         SLR   @04,@04                                          01S0435
         ICM   @04,3,PVTFPFN(@06)                               01S0435
         CR    R1,@04                                           01S0435
         BL    @RF00435                                         01S0435
         SLR   @04,@04                                          01S0435
         ICM   @04,3,PVTLPFN(@06)                               01S0435
         CR    R1,@04                                           01S0435
         BH    @RF00435                                         01S0435
*     RETCODE=0;                    /* YES - SET GOOD RETURN CODE    */
         SLR   RETCODE,RETCODE                                  01S0436
*   RETURN;                         /* RETURN TO CALLER              */
@EL00003 DS    0H                                               01S0437
@EF00003 DS    0H                                               01S0437
@ER00003 BR    @14                                              01S0437
*   RESPECIFY                                                   01S0438
*    (R0) UNRESTRICTED;             /*                       @Z40WI3F*/
*   RESPECIFY                                                   01S0439
*    (R1,                                                       01S0439
*     RETREG,                                                   01S0439
*     RETCODE) UNRESTRICTED;                                    01S0439
*   END RSMRBN;                                                 01S0440
         B     @EL00003                                         01S0440
*SRCHSPCT:                                                      01S0441
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);/*                       @Z40WI3F*/
SRCHSPCT DS    0H                                               01S0442
*/*   THIS SUBROUTINE SEARCHES THE SPCT LSQA ENTRIES FOR ONE WHICH
* MATCHES THE VBN PASSED IN R0 AS INPUT. IF AN ENTRY IS FOUND, ITS
* ADDRESS IS RETURNED IN R1, WITH A RETURN CODE OF ZERO. OTHERWISE
* A RETURN CODE OF FOUR IS RETURNED.                         @Z40WI3F*/
*                                                               01S0442
*   DCL                                                         01S0442
*    (SWAPPTR,                                                  01S0442
*     SWP) PTR(31);                 /* PTR TO LSQA ENTRY AND SPCT
*                                      EXTENSION             @Z40WI3F*/
*   DCL                                                         01S0443
*     SRCHI FIXED(31);              /* LOOP COUNTER          @Z40WI3F*/
*   RESPECIFY                                                   01S0444
*    (R0,                                                       01S0444
*     R1,                                                       01S0444
*     RETREG) RESTRICTED;           /*                       @Z40WI3F*/
*   RESPECIFY                                                   01S0445
*     SPCTEXTM BASED(SWP);          /* SPCT EXTENSION MAP    @Z40WI3F*/
*   IF RSMSPCT=0 THEN                                           01S0446
         L     @15,ASCBPTR(,VCCTREG)                            01S0446
         L     @15,RSMHDPTR(,@15)                               01S0446
         ICM   @06,15,RSMSPCT(@15)                              01S0446
         BNZ   @RF00446                                         01S0446
*     RETURN CODE(4);               /* ERROR IF NO SPCT      @Z40WI3F*/
         LA    @15,4                                            01S0447
@EL00004 DS    0H                                               01S0447
@EF00004 DS    0H                                               01S0447
@ER00004 BR    @14                                              01S0447
*   SWP=ADDR(SPCTSWAP);             /* ADDR 1ST EXTENSION    @Z40WI3F*/
@RF00446 L     @15,ASCBPTR(,VCCTREG)                            01S0448
         L     @15,RSMHDPTR(,@15)                               01S0448
         L     @15,SPCTPTR(,@15)                                01S0448
         LA    SWP,SPCTSWAP(,@15)                               01S0448
*   SWAPPTR=ADDR(SPCTENT);          /* ADDR 1ST ENTRY        @Z40WI3F*/
         LA    SWAPPTR,SPCTENT(,SWP)                            01S0449
*   DO SRCHI=SPCTLSQA TO 1 BY-1 WHILE/* SEARCH ALL LSQA ENTRIES      */
*         (SWP^=0)&SPCTLSQ=ON;      /* UNTIL COUNT OR EXTENSIONS ARE
*                                      DONE OR NON-LSAQ ENTRY FOUND
*                                                            @Z40WI3F*/
         LH    @15,SPCTLSQA(,@15)                               01S0450
