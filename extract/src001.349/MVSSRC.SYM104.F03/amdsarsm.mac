         LH    @14,PFTRCLM                                      01S0400
         LA    @14,1(,@14)                                      01S0400
         STH   @14,PFTRCLM                                      01S0400
*   GO TO SEGEXIT;                  /* RETURN                @ZF00407*/
         B     SEGEXIT                                          01S0401
*                                                               01S0402
*/*   COPY LSID TO BCT FROM SPCT LSQA ENTRY POINTED TO BY R1. INDICATE
