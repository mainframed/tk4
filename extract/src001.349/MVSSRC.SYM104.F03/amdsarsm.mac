*CHKPFTE:                                                       01S0466
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);/*                       @Z40WI3F*/
CHKPFTE  DS    0H                                               01S0467
*/*   THIS SUBROUTINE CHECKS THE PAGE FRAME TABLE ENTRY (PFTE)  01S0467
* CORRESPONDING TO THE RBN IN R1 FOR RECLAIM FOR THE VBN PASSED IN R0.
* FOR A PRIVATE AREA PAGE, THE ASID IN THE PFTE MUST MATCH THE ASID
* IN THE ASCB POINTED TO BY VCTASCB. IF NO MATCH IS FOUND, A RETURN
* CODE OF 0 IS SET, INDICATING A PAGE-IN IS NEEDED. IF A MATCH IS
* FOUND AND NO PCB EXISTS (NO PAGING ACTIVITY FOR THIS FRAME), PCBPTR
* IS SET TO 0 AND A RETURN CODE OF 8 IS SET, INDICATING RECLAIM. IF A
* MATCH IS FOUND BUT A PCB EXISTS, THEN FOR A COMMON AREA PAGE, PCBPTR
* IS SET  TO THE ADDRESS OF THE COMMON I/O QUEUE AND A RETURN CODE OF
* 8 IS SET AS ABOVE. FOR A PRIVATE AREA PAGE, PCBPTR IS SET TO THE
* ADDRESS OF THE LOCAL I/O QUEUE IF THE RSM HEADER IS VALID AND A
* RETURN CODE OF 8 IS SET AS ABOVE. IF THE RSM HEADER IS NOT VALID,
* A RETURN CODE OF 4 IS SET INDICATING AN ERROR.             @Z40WI3F*/
*                                                               01S0467
*   RESPECIFY                                                   01S0467
*    (R0,                                                       01S0467
*     R1,                                                       01S0467
*     RETREG) RESTRICTED;           /*                       @Z40WI3F*/
*   PFTEPTR=PVTPFTP+R1*(LENGTH(PFTE)/SHF4BITS);/* GET PFTE ADDRESS
*                                                            @Z40WI3F*/
         L     @15,CVTPTR                                       01S0468
         L     @15,PVTPTR(,@15)                                 01S0468
         LR    PFTEPTR,R1                                       01S0468
         AL    PFTEPTR,PVTPFTP(,@15)                            01S0468
*   PCBPTR=0;                       /* INITIALIZE FOR RETURN @Z40WI3F*/
         SLR   PCBPTR,PCBPTR                                    01S0469
*   IF(PFTIRRG=ON)(PFTVBN^=R0) THEN/* VIO PAGE OR WRONG VBN?   01S0470
*                                                            @Z40WI3F*/
         TM    PFTIRRG(PFTEPTR),B'00010000'                     01S0470
         BO    @RT00470                                         01S0470
         SLR   @15,@15                                          01S0470
         ICM   @15,3,PFTVBN(PFTEPTR)                            01S0470
         CR    R0,@15                                           01S0470
         BE    @RF00470                                         01S0470
@RT00470 DS    0H                                               01S0471
*     RETURN CODE(0);               /* YES - PAGE-IN NEEDED  @Z40WI3F*/
         SLR   @15,@15                                          01S0471
@EL00005 DS    0H                                               01S0471
@EF00005 DS    0H                                               01S0471
@ER00005 BR    @14                                              01S0471
*   IF R0<PVTLCSA THEN              /* PRIVATE OR COMMON AREA PAGE?
*                                                            @Z40WI3F*/
@RF00470 L     @15,CVTPTR                                       01S0472
         L     @15,PVTPTR(,@15)                                 01S0472
         SLR   @06,@06                                          01S0472
         ICM   @06,3,PVTLCSA(@15)                               01S0472
         CR    R0,@06                                           01S0472
         BNL   @RF00472                                         01S0472
*     DO;                           /* PRIVATE AREA PAGE     @Z40WI3F*/
*       IF PFTASID^=ASCBASID THEN   /* ASID MATCH?           @Z40WI3F*/
         L     @15,ASCBPTR(,VCCTREG)                            01S0474
         CLC   PFTASID(2,PFTEPTR),ASCBASID(@15)                 01S0474
         BE    @RF00474                                         01S0474
*         RETURN CODE(0);           /* NO - PAGE-IN NEEDED   @Z40WI3F*/
         SLR   @15,@15                                          01S0475
         BR    @14                                              01S0475
*       IF PFTPCBSI=OFF THEN        /* I/O IN PROGRESS?      @Z40WI3F*/
@RF00474 TM    PFTPCBSI(PFTEPTR),B'00001000'                    01S0476
         BNZ   @RF00476                                         01S0476
*         RETURN CODE(8);           /* NO - RECLAIM OK       @Z40WI3F*/
         LA    @15,8                                            01S0477
         BR    @14                                              01S0477
*       IF VCTLRCLM=ON THEN         /* RSM HEADER VALID?     @Z40WI3F*/
@RF00476 TM    VCTLRCLM(VCCTREG),B'00001000'                    01S0478
         BNO   @RF00478                                         01S0478
*         RETURN CODE(4);           /* ERROR - CANNOT CHECK PCBS
*                                                            @Z40WI3F*/
         LA    @15,4                                            01S0479
         BR    @14                                              01S0479
*       PCBPTR=RSMLIOQF;            /* LOCAL I/O QUEUE       @Z40WI3F*/
@RF00478 L     @15,ASCBPTR(,VCCTREG)                            01S0480
         L     @15,RSMHDPTR(,@15)                               01S0480
         L     PCBPTR,RSMLIOQF(,@15)                            01S0480
*     END;                          /*                       @Z40WI3F*/
*   ELSE                                                        01S0482
*     DO;                           /* COMMON AREA PAGE      @Z40WI3F*/
         B     @RC00472                                         01S0482
@RF00472 DS    0H                                               01S0483
*       IF PFTPCBSI=ON THEN         /* I/O IN PROGRESS?      @Z40WI3F*/
         TM    PFTPCBSI(PFTEPTR),B'00001000'                    01S0483
         BNO   @RF00483                                         01S0483
*         PCBPTR=PVTCIOQF;          /* YES - GET COMMON I/O QUEUE PTR
*                                                            @Z40WI3F*/
         L     @15,CVTPTR                                       01S0484
         L     @15,PVTPTR(,@15)                                 01S0484
         L     PCBPTR,PVTCIOQF(,@15)                            01S0484
*     END;                          /*                       @Z40WI3F*/
@RF00483 DS    0H                                               01S0486
*   RETURN CODE(8);                 /* RECLAIM OK            @Z40WI3F*/
@RC00472 LA    @15,8                                            01S0486
         BR    @14                                              01S0486
*   RESPECIFY                                                   01S0487
*    (R0,                                                       01S0487
*     R1,                                                       01S0487
*     RETREG) UNRESTRICTED;         /*                       @Z40WI3F*/
*   END CHKPFTE;                    /*                       @Z40WI3F*/
*                                                               01S0489
*   /*****************************************************************/
*   /*                                                               */
*   /* PCB QUEUE SEARCH ROUTINE - THIS ROUTINE SEARCHES THE PCB QUEUE*/
*   /* BEGINNING WITH THE PCB POINTED TO BY PCBPTR. R0 CONTAINS THE  */
*   /* DESIRED VBN AND R1 CONTAINS THE RBN, BOTH WITH 4 LOW ORDER    */
*   /* ZEROES. A LIMIT OF 200 PCBS IS PERMITTED ON ANY QUEUE TO      */
*   /* PREVENT A LOOP. IF A PCB WITH THE VBN AND RBN MATCHING IS     */
*   /* FOUND, A RETURN CODE OF 0 IS SET AND PCBPTR POINTS TO THAT    */
*   /* PCB. IF THE LIMIT IS EXCEEDED, OR IF NO PCB IS FOUND WITH BOTH*/
*   /* VBN AND RBN MATCHING, A RETURN CODE OF 4 IS SET.              */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0489
*/*PCBSRCH: E SEARCH PCB QUEUE                                       */
*/* N R0 CONTAINS VBN, R1 CONTAINS RBN                               */
*/* P SET LIMIT OF 200 PCBS                                          */
*/*%CHKPCB: D (YES,,NO,%PCBNXT) PCBVBN = R0                          */
*/* D (YES,,NO,%PCBERR) PCBRBN = R1                                  */
*/* P SET RETURN CODE 0                                              */
*/* R RETURN                                                         */
*/*%PCBNXT: P DECREMENT PCB COUNT                                    */
*/* D (NO,,YES,%PCBERR) PCB COUNT = 0                                */
*/* D (NO,,YES,%PCBERR) PCBFQPA = 0                                  */
*/* P (,%CHKPCB) GET NEXT PCB ADDRESS                                */
*/*%PCBERR: P SET RETURN CODE 4                                      */
*/* R RETURN                                                         */
*/*AMDSARSM: END                                                     */
*                                                               01S0489
*PCBSRCH:                                                       01S0489
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                            01S0489
PCBSRCH  DS    0H                                               01S0490
*   RESPECIFY                                                   01S0490
*    (R0,                                                       01S0490
*     R1,                                                       01S0490
*     RETREG,                                                   01S0490
*     RETCODE) RESTRICTED;                                      01S0490
*   RETCODE=4;                      /* SET UP ERROR RETURN CODE      */
         LA    RETCODE,4                                        01S0491
*   DO NUMPCB=DEFLTPCB TO 1 BY-1 WHILE PCBPTR^=0;/* SEARCH PCB QUEUE
*                                      IN FORWARD DIRECTION          */
         LA    @06,400                                          01S0492
         STH   @06,NUMPCB                                       01S0492
@DL00492 SLR   @06,@06                                          01S0492
         CR    PCBPTR,@06                                       01S0492
         BE    @DC00492                                         01S0492
*     IF PCBVBN=R0 THEN             /* VBN MATCH                     */
         SLR   @02,@02                                          01S0493
         ICM   @02,3,PCBVBN(PCBPTR)                             01S0493
         CR    R0,@02                                           01S0493
         BNE   @RF00493                                         01S0493
*       IF R1=0PCBRBN=R1 THEN      /* RBN MATCH OR 0 (DON'T CARE)
*                                                            @Z40WI3F*/
         CR    R1,@06                                           01S0494
         BE    @RT00494                                         01S0494
         SLR   @06,@06                                          01S0494
         ICM   @06,3,PCBRBN(PCBPTR)                             01S0494
         CR    R1,@06                                           01S0494
         BNE   @RF00494                                         01S0494
@RT00494 DS    0H                                               01S0495
*         DO;                                                   01S0495
*           RETCODE=0;              /* YES - GOOD RETURN             */
         SLR   RETCODE,RETCODE                                  01S0496
*           RETURN;                 /* RETURN TO CALLER              */
@EL00006 DS    0H                                               01S0497
@EF00006 DS    0H                                               01S0497
@ER00006 BR    @14                                              01S0497
*         END;                                                  01S0498
*     PCBPTR=PCBFQPA;               /* GET NEXT PCB                  */
@RF00494 DS    0H                                               01S0499
@RF00493 SLR   @06,@06                                          01S0499
         ICM   @06,7,PCBFQPA(PCBPTR)                            01S0499
         LR    PCBPTR,@06                                       01S0499
*   END;                            /* CONTINUE PCB SEARCH           */
         SLR   @06,@06                                          01S0500
         BCTR  @06,0                                            01S0500
         AH    @06,NUMPCB                                       01S0500
         STH   @06,NUMPCB                                       01S0500
         LTR   @06,@06                                          01S0500
         BP    @DL00492                                         01S0500
@DC00492 DS    0H                                               01S0501
*   RETURN;                         /* LIMIT EXCEEDED OR END OF QUEUE*/
         B     @EL00006                                         01S0501
*   END PCBSRCH;                                                01S0502
*   END AMDSARSM                                                01S0503
