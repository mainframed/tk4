         CR    SWP,@15                                          01S0450
         BE    @DC00450                                         01S0450
         TM    SPCTLSQ(SWAPPTR),B'01000000'                     01S0450
         BNO   @DC00450                                         01S0450
*     IF(SPCTVBN&PRBNMASK)=R0 THEN  /* MATCH FOUND?          @Z40WI3F*/
         L     @06,@CF01850                                     01S0451
         SLR   @04,@04                                          01S0451
         ICM   @04,3,SPCTVBN(SWAPPTR)                           01S0451
         NR    @06,@04                                          01S0451
         CR    @06,R0                                           01S0451
         BNE   @RF00451                                         01S0451
*       DO;                         /* YES - RETURN ADDRESS OF ENTRY
*                                                            @Z40WI3F*/
*         R1=SWAPPTR;               /* ADDR OF SPCT LSQA ENTRY  01S0453
*                                                            @Z40WI3F*/
         LR    R1,SWAPPTR                                       01S0453
*         RETURN CODE(0);           /* RETURN SUCCESSFULLY   @Z40WI3F*/
         BR    @14                                              01S0454
*       END;                        /*                       @Z40WI3F*/
*     SWAPPTR=SWAPPTR+LENGTH(SPCTENTS);/* NEXT LSQA ENTRY    @Z40WI3F*/
@RF00451 AH    SWAPPTR,@CH01214                                 01S0456
*     IF SWAPPTR=ADDR(SPCTXEND) THEN/* END OF CURRENT EXTENSION?
*                                                            @Z40WI3F*/
         LA    @15,SPCTXEND(,SWP)                               01S0457
         CR    SWAPPTR,@15                                      01S0457
         BNE   @RF00457                                         01S0457
*       DO;                         /* YES - GET NEXT EXTENSION 01S0458
*                                                            @Z40WI3F*/
*         SWP=SPCTEXT;              /* NEXT EXTENSION ADDR   @Z40WI3F*/
         L     SWP,SPCTEXT(,SWP)                                01S0459
*         SWAPPTR=ADDR(SPCTENT);    /* ADDR 1ST LSQA ENTRY IN NEW
*                                      EXTENSION             @Z40WI3F*/
         LA    SWAPPTR,SPCTENT(,SWP)                            01S0460
*       END;                        /*                       @Z40WI3F*/
*   END;                            /* END FOR MAIN LOOP     @Z40WI3F*/
@RF00457 SLR   @15,@15                                          01S0462
         BCTR  @15,0                                            01S0462
         AL    @15,SRCHI                                        01S0462
         ST    @15,SRCHI                                        01S0462
@DE00450 LTR   @15,@15                                          01S0462
         BP    @DL00450                                         01S0462
@DC00450 DS    0H                                               01S0463
