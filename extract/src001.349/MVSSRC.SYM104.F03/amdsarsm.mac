*RECLAIM:                           /* PAGE RECLAIMED        @ZF00407*/
*   BCTRECLM=ON;                    /* BCT RECLAIMED         @ZF00407*/
RECLAIM  DS    0H                                               01S0383
*   BCTCOMP=ON;                     /*                       @ZF00407*/
         OI    BCTRECLM(BCTPTR),B'10010000'                     01S0383
*   PGTRSA=R1;                      /* PAGE NOW VALID        @ZF00407*/
         STH   R1,PGTRSA(,PTEPTR)                               01S0384
*   GENERATE(PTLB)                                              01S0385
*   NOREFS NOSETS;                  /*                       @Z40WI3F*/
         PTLB
*   R1=BCTVADDR;                    /* GET VIRTUAL ADDR      @ZF00407*/
         L     R1,BCTVADDR(,BCTPTR)                             01S0386
*   GENERATE REFS(R1) SETS(R0,R1);                              01S0387
*                                                         /*@ZF00407*/
        LRA   R1,0(R1)             GET REAL ADDR              @ZF00407
        ISK   R0,R1                GET STORAGE KEY            @ZF00407
*   BCTKEY1=R0;                     /* SAVE KEY FOR FIRST 2K @ZF00407*/
         STC   R0,BCTKEY1(,BCTPTR)                              01S0388
*   R1=R1+PAGESIZE/2;               /* GET NEXT 2K REAL ADDR @ZF00407*/
         AH    R1,@CH01926                                      01S0389
*   GENERATE REFS(R1) SETS(R0);                                 01S0390
*                                                         /*@ZF00407*/
        ISK   R0,R1                GET STORAGE KEY            @ZF00407
*   BCTKEY2=R0;                     /* SAVE KEY FOR SECOND 2K   01S0391
*                                                            @ZF00407*/
         STC   R0,BCTKEY2(,BCTPTR)                              01S0391
*   RESPECIFY                                                   01S0392
*    (R2,                                                       01S0392
*     R3) RESTRICTED;               /*                       @ZF00407*/
*   R0=ADDR(RSMBUFFR);              /* POINT TO BUFFER       @ZF00407*/
         L     R0,BCTRADDR(,BCTPTR)                             01S0393
         AL    R0,VCTLPA(,VCCTREG)                              01S0393
*   R2=ADDR(RSMBUFFV);              /* POINT TO RECLAIMED PAGE  01S0394
*                                                            @ZF00407*/
         L     R2,BCTVADDR(,BCTPTR)                             01S0394
*   R1=PAGESIZE;                    /*                       @ZF00407*/
         LH    @14,@CH01833                                     01S0395
         LR    R1,@14                                           01S0395
*   R3=PAGESIZE;                    /*                       @ZF00407*/
         LR    R3,@14                                           01S0396
*   MVCL(R0,R2);                    /* MOVE PAGE TO BUFFER   @ZF00407*/
         MVCL  R0,R2                                            01S0397
*   RESPECIFY                                                   01S0398
*    (R0,                                                       01S0398
*     R1,                                                       01S0398
*     R2,                                                       01S0398
*     R3) UNRESTRICTED;             /*                       @ZF00407*/
*   SAVEBACK->SAVER15=8;            /* INDICATE RECLAIM      @ZF00407*/
         L     @14,SAVEBACK(,R13)                               01S0399
         MVC   SAVER15(4,@14),@CF00059                          01S0399
*   PFTRCLM=PFTRCLM+1;              /* INCREMENT RECLAIM COUNT IN
