         TITLE 'AMDSARSM REAL STORAGE MANAGEMENT FUNCTION              *
                        '
AMDSARSM CSECT ,                                                01S0001
@MAINENT DS    0H                                               01S0001
         USING *,@15                                            01S0001
         B     @PROLOG                                          01S0001
         DC    AL2(@EP00001-@MAINENT)
         DC    AL1(16)                                          01S0001
         DC    C'AMDSARSM  77.053'                              01S0001
AMDSAVRT DS    0H                                               01S0001
         USING *,@15                                            01S0001
         B     @PROLOG                                          01S0001
         DC    AL2(@EP00232-AMDSAVRT)
         ENTRY AMDSAVRT
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                  01S0001
         BALR  @11,0                                            01S0001
@PSTART  DS    0H                                               01S0001
         USING @PSTART,@11                                      01S0001
         ST    @13,@SA00001+4                                   01S0001
         LA    @14,@SA00001                                     01S0001
         ST    @14,8(,@13)                                      01S0001
         LR    @13,@14                                          01S0001
         AH    @15,4(,@15)                                      01S0001
         BR    @15                                              01S0001
@EP00001 DS    0H                                               01S0002
*   MODESW=OFF;                     /* INDICATE RSM MODE             */
         NI    MODESW,B'01111111'                               01S0230
*   GO TO RECSNCHK;                 /* CHECK FOR RECURSION           */
         B     RECSNCHK                                         01S0231
*AMDSAVRT:                                                      01S0232
*   ENTRY;                                                      01S0232
@EP00232 DS    0H                                               01S0233
*   MODESW=ON;                      /* INDICATE VRT MODE             */
         OI    MODESW,B'10000000'                               01S0233
*RECSNCHK:                                                      01S0234
*   IF VCTRSMR=ON THEN              /* RECURSION THROUGH AMDSARSM    */
RECSNCHK TM    VCTRSMR(VCCTREG),B'00010000'                     01S0234
         BO    @RT00234                                         01S0234
*     GO TO EREXIT;                 /* YES - ERROR                   */
*   SAVEFORM(1:4)='LRSM';           /* SET UP THE RSM SAVE AREA ID   */
         MVC   SAVEFORM(4,R13),@CC01871                         01S0236
*   VCTRSMR=ON;                     /* SET RECURSION FLAG            */
         OI    VCTRSMR(VCCTREG),B'00010000'                     01S0237
*   IODBPTR=ADDR(IODBDA)+VCTLPA;    /* GET VIRTUAL ADDRESS OF IODB   */
         L     IODBPTR,@CA01819                                 01S0238
         L     @14,VCTLPA(,VCCTREG)                             01S0238
         ALR   IODBPTR,@14                                      01S0238
*   BCTPTR=IODBBCT+VCTLPA;          /* GET VIRTUAL ADDRESS OF BCT    */
         AL    @14,IODBBCT(,IODBPTR)                            01S0239
         LR    BCTPTR,@14                                       01S0239
*                                                               01S0240
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK FOR MODE OF ENTRY. IF ENTERED AT AMDSAVRT, R1 CONTAINS  */
*   /* THE REAL ADDRESS TO BE CONVERTED TO VIRTUAL.                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0240
*/* D (YES,,NO,%RSMCONT) VIRTUAL ADDR CONVERT                        */
*/* S RSMVIRT: CONVERT TO VIRTUAL ADDRESS                            */
*/* D (YES,,NO,EREXIT) RC  =  0                                      */
*/* P (,NORMEXIT) SAVE VIRTUAL ADDRESS FOR CALLER                    */
*                                                               01S0240
*   IF MODESW=ON THEN               /* CONVERT TO VIRTUAL ADDRESS    */
         TM    MODESW,B'10000000'                               01S0240
         BNO   @RF00240                                         01S0240
*     DO;                           /* YES -                         */
*       CALL RSMVIRT;               /* PERFORM CONVERSION            */
         BAL   @14,RSMVIRT                                      01S0242
*       IF RETCODE^=0 THEN          /* ERROR                         */
         LTR   RETCODE,RETCODE                                  01S0243
         BNZ   @RT00243                                         01S0243
*         GO TO EREXIT;             /* YES - INDICATE ERROR          */
*       SAVEBACK->SAVER1=R1;        /* SAVE VIRTUAL ADDRESS          */
         L     @14,SAVEBACK(,R13)                               01S0245
         ST    R1,SAVER1(,@14)                                  01S0245
*       GO TO NORMEXIT;             /* SUCCESSFUL                    */
         B     NORMEXIT                                         01S0246
*     END;                                                      01S0247
*   RESPECIFY                                                   01S0248
*    (R1,                                                       01S0248
*     RETCODE) UNRESTRICTED;                                    01S0248
*                                                               01S0248
@RF00240 DS    0H                                               01S0249
*   /*****************************************************************/
*   /*                                                               */
*   /* ISOLATE INPUT REQUEST TYPE - RSM HANDLES SEGMENT EXCEPTIONS IN*/
*   /* THIS SECTION                                                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0249
*/*%RSMCONT: P OBTAIN FIRST BCT FROM IODBBCT                         */
*/* P OBTAIN CURRENT MDCB FROM BCTMDCB                               */
*/* P OBTAIN INTERRUPT CODE FROM MDCBPI                              */
*/* D (YES,,NO,PGEXPT) MDCBPI = X'10' (SEG EXCEPTION )?              */
*/* P OBTAIN ADDRESS OF THE PTRT FROM VCTPTRT                        */
*/* N ADDRESS IS IN FORM OF SSPDDD                                   */
*/* P OBTAIN SS PORTION OF ADDRESS FROM BCTVADDR                     */
*/* P INDEX TO PROPER PTRT ENTRY                                     */
*/* D (YES,EREXIT,NO,) PTRTRSA FOR ENTRY = -1 ?                      */
*                                                               01S0249
*   PTRTNDX=(BCTVADDR&SEGMASK)/SEGSIZE;/* COMPUTE INDEX VALUE        */
         L     @00,BCTVADDR(,BCTPTR)                            01S0249
         N     @00,@CF01825                                     01S0249
         SRL   @00,16                                           01S0249
         LR    PTRTNDX,@00                                      01S0249
*   IF MDCBPI=SGTEXCPT THEN                                     01S0250
         L     @10,VCTLPA(,VCCTREG)                             01S0250
         L     @08,BCTMDCB(,BCTPTR)                             01S0250
         ALR   @08,@10                                          01S0250
         CLC   MDCBPI(2,@08),@CH01821                           01S0250
         BNE   @RF00250                                         01S0250
*     DO;                           /* IF SEGMENT EXCEPTION HANDLE   */
*       PTRTC=ADDR(PTRTRS(PTRTNDX+1));/* INDEX TO THE PROPER PTRT
*                                      ENTRY                         */
         LR    @08,PTRTNDX                                      01S0252
         SLA   @08,2                                            01S0252
         AL    @10,@CF01495                                     01S0252
         LA    PTRTC,PTRTRS(@08,@10)                            01S0252
*       RESPECIFY                                               01S0253
*         PTRTRSA BASED(PTRTC);                                 01S0253
*       IF PTRTRSA='FFFFFFFF'X THEN                             01S0254
         CLC   PTRTRSA(4,PTRTC),@CF01875                        01S0254
         BE    @RT00254                                         01S0254
*         GO TO EREXIT;             /* IF RSA = -1 THEN THIS IS AN
*                                      INVALID SEGMENT EXCEPTION     */
*                                                               01S0256
*       /*************************************************************/
*       /*                                                           */
*       /* WE HAVE A VALID PAGE TABLE TO OBTAIN. CONVERT THE REQUEST */
*       /* TO A READ FOR THE PAGE TABLE. USE THE PTRT TO COMPUTE THE */
*       /* LPID AND VIRTUAL ADDRESS OF THE PAGE TABLE.               */
*       /*                                                           */
*       /*************************************************************/
*                                                               01S0256
*/* P CONSTRUCT LPID FROM PTRTLGID AND PTRTVSA                       */
*/* P SET BCTPGTF (FIXED PAGE TABLE) AND BCTPGT (PAGE TABLE) FLAGS   */
*/* P (,NORMEXIT) SET BCTVADDR TO PTRTVSA ROUNDED DOWN TO PAGE BDY   */
*                                                               01S0256
*       BCTVADDR=PTRTVSA&PGERND;    /* SET VIRTUAL ADDRESS           */
         L     @10,PTRTVSA+1024(,PTRTC)                         01S0256
         LR    @06,@10                                          01S0256
         N     @06,@CF01829                                     01S0256
         ST    @06,BCTVADDR(,BCTPTR)                            01S0256
*       RESPECIFY                                               01S0257
*        (R0,                                                   01S0257
*         R1) RESTRICTED;           /*                       @Z40WI3F*/
*       R0=EVAL((PTRTVSA&ADDRMASK)/PAGESIZE)*SHF4BITS;/*     @Z40WI3F
*                                      GET VBN               @Z40WI3F*/
         LR    @04,@10                                          01S0258
         N     @04,@CF01837                                     01S0258
         SRL   @04,12                                           01S0258
         SLA   @04,4                                            01S0258
         LR    R0,@04                                           01S0258
*       CALL SRCHSPCT;              /* GET SPCT LSQA ENTRY IN R1
*                                                            @Z40WI3F*/
         BAL   @14,SRCHSPCT                                     01S0259
*       RESPECIFY                                               01S0260
*         RETCODE RESTRICTED;       /*                       @Z40WI3F*/
*       IF RETCODE^=0 THEN          /* LSQA ENTRY FOUND?     @Z40WI3F*/
         LTR   RETCODE,RETCODE                                  01S0261
         BNZ   @RT00261                                         01S0261
*         GOTO EREXIT;              /* NO - ERROR            @Z40WI3F*/
*       RESPECIFY                                               01S0263
*         RETCODE UNRESTRICTED;     /*                       @Z40WI3F*/
*       BCTFLGT=OFF;                /* RESET BUFFER FLAGS            */
         MVI   BCTFLGT(BCTPTR),B'00000000'                      01S0264
*       BCTPGTF=ON;                 /* MARK BUFFER AS FIXED PAGE
*                                      TABLE BUFFER TO PREVENT USE
*                                      BEFORE SUBSEQUENT PAGE FAULT  */
*       BCTPGT=ON;                  /* INDICATE PAGE TABLE BUFFER    */
         OI    BCTPGTF(BCTPTR),B'00100010'                      01S0266
*       GOTO LSQALSID;              /* GET LSID FROM SPCT LSQA ENTRY
*                                                            @Z40WI3F*/
         B     LSQALSID                                         01S0267
*     END;                                                      01S0268
*                                                               01S0269
*   /*****************************************************************/
*   /*                                                               */
*   /* PROCESS THE PAGE FAULT CONDITIONS NEXT                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0269
*   IF MDCBPI^=PGEXCPT THEN                                     01S0269
@RF00250 L     @10,BCTMDCB(,BCTPTR)                             01S0269
         AL    @10,VCTLPA(,VCCTREG)                             01S0269
         CLC   MDCBPI(2,@10),@CH01823                           01S0269
         BNE   @RT00269                                         01S0269
*     GO TO EREXIT;                 /*                       @ZF00407*/
*   STEPTR=MDCBSTOR+PTRTNDX*LENGTH(PTRTRSA);/*               @ZF00407*/
         L     @10,MDCBSTOR(,@10)                               01S0271
         LR    STEPTR,PTRTNDX                                   01S0271
         SLA   STEPTR,2                                         01S0271
         ALR   STEPTR,@10                                       01S0271
*   PTRTNDX=(BCTVADDR&PAGEMASK)/PAGESIZE;/* PAGE NUMBER      @ZF00407*/
         L     @08,BCTVADDR(,BCTPTR)                            01S0272
         N     @08,@CF01846                                     01S0272
         SRL   @08,12                                           01S0272
         LR    PTRTNDX,@08                                      01S0272
*   IF(MDCBSTOR&ADDRMASK)^=ADDR(PTRTREAL->PTRTRS(1)) THEN       01S0273
         N     @10,@CF01837                                     01S0273
         LH    @08,@CH01495                                     01S0273
         LA    @08,PTRTRS(,@08)                                 01S0273
         CR    @10,@08                                          01S0273
         BE    @RF00273                                         01S0273
*     DO;                           /* MEMORY IN IF USING REAL SEGTAB
*                                                            @ZF00407*/
*       RESPECIFY                                               01S0275
*        (R0,                                                   01S0275
*         R1,                                                   01S0275
*         RETCODE) RESTRICTED;      /*                       @ZF00407*/
*       R1=STEPTR;                  /*                       @ZF00407*/
         LR    R1,STEPTR                                        01S0276
*       CALL RSMVIRT;               /* CONVERT REAL ADDR TO VIRTUAL
*                                                            @ZF00407*/
         BAL   @14,RSMVIRT                                      01S0277
*       IF RETCODE^=0 THEN                                      01S0278
         LTR   RETCODE,RETCODE                                  01S0278
         BNZ   @RT00278                                         01S0278
*         GO TO EREXIT;             /* INVALID VIRTUAL ADDR          */
*       STEPTR=R1;                  /*                       @ZF00407*/
         LR    STEPTR,R1                                        01S0280
*       R1=SGTPTO;                  /* REAL PAGE TABLE ADDR  @ZF00407*/
         SLR   R1,R1                                            01S0281
         ICM   R1,7,SGTPTO(STEPTR)                              01S0281
*       CALL RSMVIRT;               /* CONVERT REAL ADDR TO VIRTUAL
*                                                            @ZF00407*/
         BAL   @14,RSMVIRT                                      01S0282
*       IF RETCODE^=0 THEN                                      01S0283
         LTR   RETCODE,RETCODE                                  01S0283
         BNZ   @RT00283                                         01S0283
*         GO TO EREXIT;             /* INVALID VIRTUAL ADDR          */
*       SAVEPTE=R1;                 /* VIRT ADDR OF PAGE TABLE START
*                                                            @ZF00407*/
         LR    SAVEPTE,R1                                       01S0285
*       PTEPTR=SAVEPTE+PTRTNDX*LENGTH(PGTPTE);/*             @ZF00407*/
         LR    PTEPTR,PTRTNDX                                   01S0286
         ALR   PTEPTR,PTEPTR                                    01S0286
         ALR   PTEPTR,SAVEPTE                                   01S0286
*       R1=PGTRSA&PRBNMASK;         /* R1 = RBN * 16         @ZF00407*/
         L     R1,@CF01850                                      01S0287
         SLR   @14,@14                                          01S0287
         ICM   @14,3,PGTRSA(PTEPTR)                             01S0287
         NR    R1,@14                                           01S0287
*       R0=EVAL((BCTVADDR&ADDRMASK)/PAGESIZE)*SHF4BITS;/* R0 = VBN   */
         L     @02,BCTVADDR(,BCTPTR)                            01S0288
         N     @02,@CF01837                                     01S0288
         SRL   @02,12                                           01S0288
         SLA   @02,4                                            01S0288
         LR    R0,@02                                           01S0288
*       CALL RSMRBN;                /* CHECK FOR VALID RBN   @ZF00407*/
         BAL   @14,RSMRBN                                       01S0289
*       IF RETCODE^=0 THEN                                      01S0290
         LTR   RETCODE,RETCODE                                  01S0290
         BNZ   @RT00290                                         01S0290
*         GO TO PAGEIN;             /* INVALID - PAGE IN     @ZF00407*/
*       CALL CHKPFTE;               /* CHECK FOR RECLAIM     @Z40WI3F*/
         BAL   @14,CHKPFTE                                      01S0292
*       IF RETCODE=4 THEN           /* ERROR?                @Z40WI3F*/
         CH    RETCODE,@CH00053                                 01S0293
         BE    @RT00293                                         01S0293
*         GOTO EREXIT;              /* YES - EXIT            @Z40WI3F*/
*       IF RETCODE=0 THEN           /* PAGE-IN NEEDED?       @Z40WI3F*/
         SLR   @14,@14                                          01S0295
         CR    RETCODE,@14                                      01S0295
         BE    @RT00295                                         01S0295
*         GOTO PAGEIN;              /* YES                   @Z40WI3F*/
*/*   THE PFTE INDICATES RECLAIM IS POSSIBLE. CHECK IF THERE IS ANY
* I/O ACTIVITY. IF SO, SEARCH APPROPRIATE PCB QUEUE TO DETERMINE IF
* A PAGE-IN OR PAGE-OUT IS IN PROCESS. IF A PAGE-IN IS IN PROCESS,
* THE EXTERNAL COPY IS STILL VALID AND MUST BE USED SINCE THE   01S0297
* OPERATION MAY NOT HAVE COMPLETED. SIMILARLY, IF A PAGE-OUT IS IN
* PROCESS, THE IN-STORAGE COPY IS STILL VALID AND MUST BE USED.      */
*                                                               01S0297
*       IF PCBPTR^=0 THEN           /* ANY PAGING ACTIVITY FOR FRAME?
*                                                            @Z40WI3F*/
         CR    PCBPTR,@14                                       01S0297
         BE    @RF00297                                         01S0297
*         DO;                       /* YES - CHECK PAGE-IN OR   01S0298
*                                      PAGE-OUT              @Z40WI3F*/
*           CALL PCBSRCH;           /* FIND PCB ON I/O QUEUE @Z40WI3F
*                                                            @Z40WI3F*/
         BAL   @14,PCBSRCH                                      01S0299
*           IF RETCODE^=0 THEN      /* PCB FOUND?            @Z40WI3F*/
         LTR   RETCODE,RETCODE                                  01S0300
         BNZ   @RT00300                                         01S0300
*             GOTO EREXIT;          /* NO - ERROR            @Z40WI3F*/
*           IF PCBIOI=OFF THEN      /* PAGE-IN OR -OUT IN PROCESS?
*                                                            @Z40WI3F*/
         TM    PCBIOI(PCBPTR),B'00000010'                       01S0302
         BZ    @RT00302                                         01S0302
*             GOTO PAGEIN;          /* PAGE-IN               @Z40WI3F*/
*         END;                      /*                       @Z40WI3F*/
*       GOTO RECLAIM;               /* RECLAIM FROM IN-STORAGE  01S0305
*                                                            @Z40WI3F*/
         B     RECLAIM                                          01S0305
*     END;                          /*                       @Z40WI3F*/
*   ELSE                                                        01S0307
*     DO;                           /* MEMORY ALL OR PARTLY OUT 01S0307
*                                                            @ZF00407*/
@RF00273 DS    0H                                               01S0308
*       IF VCTLRCLM=ON THEN                                     01S0308
         TM    VCTLRCLM(VCCTREG),B'00001000'                    01S0308
         BO    @RT00308                                         01S0308
*         GOTO EREXIT;              /* CANNOT CHECK FOR RECLAIM 01S0309
*                                      WITHOUT RSM HEADER    @Z40WI3F*/
*       STEPTR=STEPTR+VCTLPA;       /* SEGTAB IS IN SADMP    @ZF00407*/
         L     @14,VCTLPA(,VCCTREG)                             01S0310
         ALR   STEPTR,@14                                       01S0310
*       SAVEPTE=SGTSTE&PTOMASK;     /* REAL ADDR OF PAGE TABLE  01S0311
*                                                            @ZF00407*/
         L     SAVEPTE,SGTSTE(,STEPTR)                          01S0311
         N     SAVEPTE,@CF01839                                 01S0311
*       RESPECIFY                                               01S0312
*         BCTH BASED(VCTBCTH+VCTLPA);/*                      @ZF00407*/
*       RESPECIFY                                               01S0313
*         BCT BASED(BCTHFRST+VCTLPA);/*                      @ZF00407*/
*       IF SAVEPTE<BCTRADDR THEN                                01S0314
         L     @10,VCTBCTH(,VCCTREG)                            01S0314
         ALR   @10,@14                                          01S0314
         L     @06,BCTHFRST(,@10)                               01S0314
         ALR   @06,@14                                          01S0314
         C     SAVEPTE,BCTRADDR(,@06)                           01S0314
         BL    @RT00314                                         01S0314
*         GO TO PGTRCLD;            /* RECLAIMED PGT                 */
*       RESPECIFY                                               01S0316
*         BCT BASED(BCTHLAST+VCTLPA);/*                      @ZF00407*/
*       IF SAVEPTE>=BCTRADDR+PAGESIZE THEN                      01S0317
         L     @10,BCTHLAST(,@10)                               01S0317
         ALR   @10,@14                                          01S0317
         LH    @06,@CH01833                                     01S0317
         AL    @06,BCTRADDR(,@10)                               01S0317
         CR    SAVEPTE,@06                                      01S0317
         BNL   @RT00317                                         01S0317
*         GOTO PGTRCLD;             /*                       @Z40WI3F
*                                      RECLAIMED PGT         @Z40WI3F*/
*       SAVEPTE=SAVEPTE+VCTLPA;     /* NOT RECLAIMED - IN BUFFER     */
         ALR   SAVEPTE,@14                                      01S0319
*       GO TO SRCHPFT;              /*                       @ZF00407*/
         B     SRCHPFT                                          01S0320
*       RESPECIFY                                               01S0321
*         BCT BASED(BCTPTR);        /*                       @ZF00407*/
*PGTRCLD:                                                       01S0322
*       RESPECIFY                                               01S0322
*         PGTPTE BASED(VCTPGT+VCTLPA);/*                     @ZF00407*/
PGTRCLD  DS    0H                                               01S0323
*       PGTRSA=EVAL(SAVEPTE/PAGESIZE)*SHF4BITS;/* USE DUMMY PGT      */
         L     @14,VCTPGT(,VCCTREG)                             01S0323
         AL    @14,VCTLPA(,VCCTREG)                             01S0323
         LR    @10,SAVEPTE                                      01S0323
         SRL   @10,12                                           01S0323
         SLA   @10,4                                            01S0323
         STH   @10,PGTRSA(,@14)                                 01S0323
*       GENERATE(PTLB)                                          01S0324
*       NOREFS NOSETS;              /*                       @ZF00407*/
         PTLB
*       SAVEPTE=(SAVEPTE&DISPLMNT)+VCTLPA;/*                 @ZF00407*/
         LR    @14,SAVEPTE                                      01S0325
         N     @14,@CF01848                                     01S0325
         AL    @14,VCTLPA(,VCCTREG)                             01S0325
         LR    SAVEPTE,@14                                      01S0325
*       RESPECIFY                                               01S0326
*         PGTPTE BASED(PTEPTR);     /*                       @ZF00407*/
*SRCHPFT:                           /* SEARCH PFTE'S FOR RECLAIMABLE
*                                      PAGES                 @ZF00407*/
*       PTEPTR=SAVEPTE+PTRTNDX*LENGTH(PGTPTE);/*             @ZF00407*/
SRCHPFT  LR    PTEPTR,PTRTNDX                                   01S0327
         ALR   PTEPTR,PTEPTR                                    01S0327
         ALR   PTEPTR,SAVEPTE                                   01S0327
*       R0=EVAL((BCTVADDR&ADDRMASK)/PAGESIZE)*SHF4BITS;/*    @ZF00407*/
         L     @02,BCTVADDR(,BCTPTR)                            01S0328
         N     @02,@CF01837                                     01S0328
         SRL   @02,12                                           01S0328
         SLA   @02,4                                            01S0328
         LR    R0,@02                                           01S0328
*       RESPECIFY                                               01S0329
*         R1 RESTRICTED;            /*                       @Z40WI3F*/
*       CALL SRCHSPCT;              /* CHECK IF THIS IS AN LSQA PAGE
*                                                            @Z40WI3F*/
         BAL   @14,SRCHSPCT                                     01S0330
*       IF RETCODE^=0 THEN          /* PRIVATE AREA PAGE?    @Z40WI3F*/
         LTR   RETCODE,RETCODE                                  01S0331
         BNZ   @RT00331                                         01S0331
*         GOTO PAGEIN;              /* YES - PAGE IT IN      @Z40WI3F*/
*       IF R1->SPCTLVAL=ON THEN     /* LSID STILL VALID IS SPCT?
*                                                            @Z40WI3F*/
         TM    SPCTLVAL(R1),B'10000000'                         01S0333
         BO    @RT00333                                         01S0333
*         GOTO LSQALSID;            /* YES - GET LSID        @Z40WI3F*/
*/*   LSID IN SPCT LSQA ENTRY NO LONGER VALID. I/O MUST HAVE @Z40WI3F
* COMPLETED. THEREFORE, THE PAGE SHOULD BE RECLAIMABLE. WE HAVE TO
* FIND THE REAL ADDRESS. SEARCH THE RSM LSQA FRAME QUEUE TO FIND IT.
* WHEN FOUND, THE PFTE MUST INDICATE RECLAIM OR WE HAVE AN ERROR.    */
*                                                               01S0335
*       R1=RSMLSQAF;                /* GET STARTING RBN      @Z40WI3F*/
         L     @14,ASCBPTR(,VCCTREG)                            01S0335
         L     @14,RSMHDPTR(,@14)                               01S0335
         SLR   R1,R1                                            01S0335
         ICM   R1,3,RSMLSQAF(@14)                               01S0335
*       DO WHILE R1^=0;             /* SEARCH UNITL END OF FRAME
*                                      QUEUE                 @Z40WI3F*/
         B     @DE00336                                         01S0336
@DL00336 DS    0H                                               01S0337
*         CALL RSMRBN;              /* ENSURE RBN IS VALID INDEX INTO
*                                      PFTE                  @Z40WI3F*/
         BAL   @14,RSMRBN                                       01S0337
*         IF RETCODE^=0 THEN        /* RBN VALID?            @Z40WI3F*/
         LTR   RETCODE,RETCODE                                  01S0338
         BNZ   @RT00338                                         01S0338
*           GOTO EREXIT;            /* NO - ERROR            @Z40WI3F*/
*         CALL CHKPFTE;             /* CHECK PFTE - ENSURE RECLAIM
*                                                            @Z40WI3F*/
         BAL   @14,CHKPFTE                                      01S0340
*         IF RETCODE=8 THEN         /* RECLAIM OK?           @Z40WI3F*/
         CH    RETCODE,@CH00059                                 01S0341
         BE    @RT00341                                         01S0341
*           GOTO RECLAIM;           /* YES                   @Z40WI3F*/
*         R1=PFTFQPTR;              /* GET NEXT RBN ON FRAME QUEUE
*                                                            @Z40WI3F*/
         SLR   R1,R1                                            01S0343
         ICM   R1,3,PFTFQPTR(PFTEPTR)                           01S0343
*       END;                        /*                       @Z40WI3F*/
@DE00336 LTR   R1,R1                                            01S0344
         BNZ   @DL00336                                         01S0344
*       GOTO EREXIT;                /* FRAME NOT FOUND - ERROR  01S0345
*                                                            @Z40WI3F*/
         B     EREXIT                                           01S0345
*     END;                          /*                       @Z40WI3F*/
*PAGEIN:                            /* TRY TO PAGE IN        @ZF00407*/
*   XPTEPTR=(SAVEPTE+(PGENO*LENGTH(PGTPTE)))+(PTRTNDX*LENGTH(XPTE));
PAGEIN   LA    XPTEPTR,32                                       01S0347
         ALR   XPTEPTR,SAVEPTE                                  01S0347
         LR    @14,PTRTNDX                                      01S0347
