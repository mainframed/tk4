*                                   /* FIND XPTE             @ZF00407*/
*   IF XPTVALID=ON THEN             /* LSID IN XPTE VALID?   @Z40WI3F*/
         TM    XPTVALID(XPTEPTR),B'10000000'                    01S0348
         BNO   @RF00348                                         01S0348
*     DO;                           /* YES - USE IT          @Z40WI3F*/
*       BCTKEY1=XPTPROT;            /* GET PROTECT KEY (1ST 2K) 01S0350
*                                                            @Z40WI3F*/
         MVC   BCTKEY1(1,BCTPTR),XPTPROT(XPTEPTR)               01S0350
*       BCTKEY2=XPTPROT;            /* GET PROTECT KEY (2ND 2K) 01S0351
*                                                            @Z40WI3F*/
         MVC   BCTKEY2(1,BCTPTR),XPTPROT(XPTEPTR)               01S0351
