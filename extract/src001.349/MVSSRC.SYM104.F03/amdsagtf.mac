         TITLE 'AMDSAGTF    GTF VIRTUAL BUFFER DUMP MODULE             *
                       '
AMDSAGTF CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'AMDSAGTF  73.230'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @11,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@11                                         0001
         ST    @13,@SA00001+4                                      0001
         LA    @14,@SA00001                                        0001
         ST    @14,8(,@13)                                         0001
         LR    @13,@14                                             0001
*   SAVEFORM(1:4) = GTFHEAD;        /* MOVE MODULE ID TO SAVE AREA   */
         MVC   SAVEFORM(4,R13),GTFHEAD                             0208
*   RCBNEXT = VCTRCB;               /* LINK TO RCB CHAIN             */
         LA    @10,GTFRCB                                          0209
         MVC   RCBNEXT(4,@10),VCTRCB(VCCTREG)                      0209
*   RCBEXIT = ADDR(GTFEXIT1);       /* POINT TO ERROR EXIT           */
         LA    @15,GTFEXIT1                                        0210
         ST    @15,RCBEXIT(,@10)                                   0210
*   RCBSAVE = R13;                  /* SAVE AREA ADDR TO RCB         */
         ST    R13,RCBSAVE(,@10)                                   0211
*   RCBBASE = BASEREG;              /* BASE ADDRESS TO RCB           */
         ST    BASEREG,RCBBASE(,@10)                               0212
*   GENERATE REFS(R1) SETS(GTFSTOR) (STCTL R1,R1,GTFSTOR); /* SEGMENT
*                                      TABLE ORIGIN TO RCB           */
         STCTL R1,R1,GTFSTOR
*   VCTRCB = ADDR(GTFRCB);          /* MAKE RCB ACTIVE               */
         LA    @10,GTFRCB                                          0214
         ST    @10,VCTRCB(,VCCTREG)                                0214
*   SAVEBACK -> SAVER15 = 0;        /* SET UP RETURN CODE OF 0       */
*                                                                  0215
         SR    @10,@10                                             0215
         L     @01,SAVEBACK(,R13)                                  0215
         ST    @10,SAVER15(,@01)                                   0215
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK FOR PROCESSING OR FIRST PASS COMPLETE                   */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0216
*/* D (NO,,YES,GTFRET1) PROCESSING COMPLETE                          */
*/* D (NO,,YES,GTFENT2) FIRST PASS COMPLETE                          */
*   IF GTFCOMPF = ON THEN           /* PROCESSING COMPLETE           */
         TM    GTFCOMPF,B'10000000'                                0216
         BO    @RT00216                                            0216
*     GO TO GTFRET1;                /* YES - TAKE RETURN PATH        */
*   IF GTFPASS1 = ON THEN           /* FIRST PASS COMPLETE           */
         TM    GTFPASS1,B'01000000'                                0218
         BO    @RT00218                                            0218
*     GO TO GTFENT2;                /* YES - SKIP INITIALIZATION     */
*                                                                  0220
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZATION PASS IS REQUIRED TO DETERMINE THE GTF MEMORY.  */
*   /* VALIDIITY CHECK THE CURRENT MCCE POINTED TO BY MCHEAD. MCHEAD */
*   /* IS POINTED TO BY THE CVT.                                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0220
*/* P OBTAIN CVT ADDRESS FROM X'10' (FLCCVT)                         */
*/* P OBTAIN MCHEAD ADDRESS FROM CVTGTF                              */
*/* D (NO,,YES,%GTFCOMP) MCHCUR ADDR IN MCHEAD = 0                   */
*/* P SET VCTCBADR TO MCHCUR TO CHECK MCCE                           */
*/* P SET VCTCBID TO MCCE ID                                         */
*/* L AMDSAVCK: VALIDITY CHECK MCCE                                  */
*/* D (YES,,NO,GTFEXIT1) RC = 0                                      */
*   RESPECIFY MCCE BASED(MCHCUR);                                  0220
*   IF MCHCUR = 0 THEN              /* IS THERE A CURRENT MCCE       */
         L     @01,CVTPTR                                          0221
         L     @01,CVTGTF(,@01)                                    0221
         L     @15,MCHCUR(,@01)                                    0221
         CR    @15,@10                                             0221
         BE    @RT00221                                            0221
*     GO TO GTFCOMP;                /* NO - NOTHING ACTIVE           */
*   VCTCBADR = MCHCUR;              /* CONTROL BLOCK ADDRESS         */
         ST    @15,VCTCBADR(,VCCTREG)                              0223
*   VCTCBIDN = VMCCEID;             /* CONTROL BLOCK ID FOR VALIDITY
*                                      CHECK                         */
         MVI   VCTCBIDN(VCCTREG),X'09'                             0224
*   SVC(VCKSVC);                    /* CALL AMDSAVCK TO VALIDITY   0225
*                                      CHECK THE MCCE                */
         SVC   25                                                  0225
*   RESPECIFY RETCODE RESTRICTED;                                  0226
*   IF RETCODE ^= 0 THEN            /* IS MCCE VALID                 */
         LTR   RETCODE,RETCODE                                     0227
         BNZ   @RT00227                                            0227
*     GO TO GTFEXIT1;               /* NO - ERROR                    */
*   RESPECIFY RETCODE UNRESTRICTED;                                0229
*                                                                  0230
*   /*****************************************************************/
*   /*                                                               */
*   /* CURRENT MCCE IS VALID. PROCEED TO MCQE CHAIN TO DETERMINE IF  */
*   /* GTF IS ACTIVE. ONLY 15 MCQES ARE PERMITTED ON THE CHAIN.      */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0230
*/* P GET MCQE CHAIN FROM MCCEQEA                                    */
*/* P INITIALIZE MCQE LOOP COUNTER TO 15                             */
*/*%GTFMCQE: P SET VCTCBADR TO MCQE ADDR                             */
*/* P SET VCTCBID TO MCQE ID                                         */
*/* L AMDSAVCK: VALIDITY CHECK MCQE                                  */
*/* D (YES,,NO,GTFEXIT1) RC = 0                                      */
*/* D (NO,,YES,%GTFFND) MCQE FOR GTF                                 */
*/* P DECREMENT MCQE COUNTER                                         */
*/* P GET NEXT MCQE ADDR                                             */
*/* D (NO,,YES,%GTFCOMP) NEXT ADDR = 0                               */
*/* D (NO,%GTFMCQE,YES,%GTF18I1) MCQE COUNTER = 0                    */
*   RESPECIFY MCQE BASED(GTFMCQEA);                                0230
*   GTFMCQEA = MCCEQEA;             /* GET ADDRESS OF MCQE CHAIN     */
         L     @10,CVTPTR                                          0231
         L     @10,CVTGTF(,@10)                                    0231
         L     @10,MCHCUR(,@10)                                    0231
         MVC   GTFMCQEA(4),MCCEQEA(@10)                            0231
*   DO I = 1 TO NUMMCQE;            /* SEARCH ALL MCQES              */
         LA    I,1                                                 0232
@DL00232 DS    0H                                                  0233
*     VCTCBADR = GTFMCQEA;          /* MCQE ADDRESS FOR VALIDITY   0233
*                                      CHECK                         */
         MVC   VCTCBADR(4,VCCTREG),GTFMCQEA                        0233
*     VCTCBIDN = VMCQEID;           /* MCQE CONTROL BLOCK ID         */
         MVI   VCTCBIDN(VCCTREG),X'0A'                             0234
*     SVC(VCKSVC);                  /* CALL AMDSAVCK TO VALIDITY   0235
*                                      CHECK THE MCQE                */
         SVC   25                                                  0235
*     RESPECIFY RETCODE RESTRICTED;                                0236
*     IF RETCODE ^= 0 THEN          /* MCQE VALID                    */
         SR    @14,@14                                             0237
         CR    RETCODE,@14                                         0237
         BNE   @RT00237                                            0237
*       GO TO GTFEXIT1;             /* NO - ERROR                    */
*     RESPECIFY RETCODE UNRESTRICTED;                              0239
*     IF MCQENAME = GTFNAME THEN    /* IS THIS MCQE FOR GTF          */
         L     @10,GTFMCQEA                                        0240
         CLC   MCQENAME(8,@10),GTFNAME                             0240
         BE    @RT00240                                            0240
*       GO TO GTFFND;               /* YES - CONTINUE PROCESSING     */
*     GTFMCQEA = MCQEMCQE;          /* GET NEXT MCQE                 */
         L     @10,MCQEMCQE(,@10)                                  0242
         ST    @10,GTFMCQEA                                        0242
*     IF GTFMCQEA = 0 THEN          /* END OF CHAIN REACHED          */
         CR    @10,@14                                             0243
         BE    @RT00243                                            0243
*       GO TO GTFCOMP;              /* YES - GTF NOT ACTIVE          */
*   END;                            /* CONTINUE SEARCH OF MCQES      */
         AH    I,@CH00061                                          0245
         CH    I,@CH00109                                          0245
         BNH   @DL00232                                            0245
*   GO TO GTF18I1;                  /* ERROR - MORE THAN 15 MCQES  0246
*                                      (POSSIBLE LOOP)               */
         B     GTF18I1                                             0246
*                                                                  0247
*   /*****************************************************************/
*   /*                                                               */
*   /* MCQE FOR GTF HAS BEEN FOUND. SAVE THE MCQE ADDRESS AND THE GTF*/
*   /* ASCB ADDRESS.                                                 */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0247
*/*%GTFFND: P SAVE MCQEASCB AND MCQE ADDRESS                         */
*/* P (,GTFENT2) INDICATE FIRST PASS COMPLETE                        */
*                                                                  0247
*GTFFND:                                                           0247
*   GTFASCBA = MCQEASCB;            /* SAVE ASCB ADDRESS             */
GTFFND   L     @10,GTFMCQEA                                        0247
         MVC   GTFASCBA(4),MCQEASCB(@10)                           0247
*   GTFPASS1 = ON;                  /* INDICATE FIRST PASS COMPLETE  */
         OI    GTFPASS1,B'01000000'                                0248
*                                                                  0249
*   /*****************************************************************/
*   /*                                                               */
*   /* FIRST PASS COMPLETE. CHECK IF THE CURRENT ASCB IS FOR GTF.    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0249
*/*GTFENT2: D (YES,,NO,GTFRET1) VCTASCB = GTF ASCB                   */
*                                                                  0249
*GTFENT2:                                                          0249
*   IF VCTASCB ^= GTFASCBA THEN     /* PROCESSING GTF MEMORY         */
GTFENT2  CLC   VCTASCB(4,VCCTREG),GTFASCBA                         0249
         BNE   @RT00249                                            0249
*     GO TO GTFRET1;                /* NO - RETURN                   */
*                                                                  0250
*   /*****************************************************************/
*   /*                                                               */
*   /* PROCESSING GTF MEMORY. MODIFY RCB EXIT ADDRESS. CHECK THE     */
*   /* GTFPCT TO GET TO THE HISTORY QUEUE.                           */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0251
*/* P (NORM,,RCB,GTFEXIT2) POINT RCB TO GTFEXIT2                     */
*/* P GET GTFPCT ADDR FROM MCQEAT                                    */
*/* D (NO,,YES,GTFEXIT1) MCQEAT = 0                                  */
*/* P GET HISTORY QUEUE BUFFER COUNT (HQLENGTH)                      */
*/* D (NO,%GTFHQST,YES,%GTFCNT0) COUNT = 0                           */
*/*%GTFCNT0: P (,%GTFHQST) SET HISTORY QUEUE COUNT TO 256            */
*/*%GTFHQST: P GET HISTORY QUEUE ADDRESS (HQHEAD)                    */
*/* D (NO,,YES,GTFEXIT1) QUEUE ADDR = 0                              */
*   RCBEXIT = ADDR(GTFEXIT2);       /* POINT TO 2ND PASS EXIT        */
         LA    @10,GTFRCB                                          0251
         LA    @15,GTFEXIT2                                        0251
         ST    @15,RCBEXIT(,@10)                                   0251
*   GENERATE FLOWS(GTFEXIT2);                                      0252
*   PCTPTR = MCQEAT;                /* GET GTFPCT ADDRESS            */
         L     @10,GTFMCQEA                                        0253
         L     PCTPTR,MCQEAT(,@10)                                 0253
*   IF PCTPTR = 0 THEN              /* IS THERE A GTFPCT             */
         SR    @10,@10                                             0254
         CR    PCTPTR,@10                                          0254
         BE    @RT00254                                            0254
*     GO TO GTFEXIT1;               /* NO - BYPASS GTF DUMP          */
*   IF HQLENGTH = 0 THEN            /* ANY BUFFERS ON HISTORY QUEUE  */
         C     @10,HQLENGTH(,PCTPTR)                               0256
         BNE   @RF00256                                            0256
*     HQLENGTH = DEFLTBFR;          /* NO - SET DEFAULT              */
         MVC   HQLENGTH(4,PCTPTR),@CF00697                         0257
*   GBCBPTR = HQHEAD;               /* GET HISTORY QUEUE ADDRESS     */
@RF00256 L     @10,HQHEAD(,PCTPTR)                                 0258
         ST    @10,GBCBPTR                                         0258
*   IF GBCBPTR = 0 THEN             /* ANY HISTORY QUEUE             */
         LTR   @10,@10                                             0259
         BZ    @RT00259                                            0259
*     GO TO GTFEXIT1;               /* NO - BYPASS GTF DUMP          */
*                                                                  0261
*   /*****************************************************************/
*   /*                                                               */
*   /* BEGIN SEARCH OF GTFBCBS ON HISTORY QUEUE.                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0261
*/*%GTFBCB1: P SET VCTCBADR TO ADDR OF GTFBCB                        */
*/* P SET VCTCBID TO GTFBCB ID                                       */
*/* L AMDSAVCK:  VALIDITY CHECK GTFBCB                               */
*/* D (YES,,NO,GTFEXIT2) RC  =  0                                    */
*/* P PLACE BUFFER ADDR (GBUFRPTR) IN REG 1                          */
*/* P PLACE X'1000' (BUFFER SIZE) IN REG 0                           */
*/* L AMDSAARD: BUILD ARB FOR ADDRESS RANGE                          */
*/*%GTFNEXT: P DECREMENT HISTORY QUEUE COUNT                         */
*/* D (NO,,YES,GTFRET2) HQCHAIN = 0                                  */
*/* D (NO,%GTFBCB1,YES,%GTF18I2) HISTORY QUEUE COUNT = 0             */
*   RESPECIFY GTFBCB BASED(GBCBPTR);                               0261
*   RESPECIFY (R0,R1) RESTRICTED;                                  0262
*   DO I = HQLENGTH TO 1 BY -1;     /* SEARCH ALL GTFBCBS ON HISTORY
*                                      QUEUE                         */
         L     I,HQLENGTH(,PCTPTR)                                 0263
         B     @DE00263                                            0263
@DL00263 DS    0H                                                  0264
*     VCTCBADR = GBCBPTR;           /* GTFBCB ADDRESS FOR VALIDITY 0264
*                                      CHECK                         */
         MVC   VCTCBADR(4,VCCTREG),GBCBPTR                         0264
*     VCTCBIDN = VBCBID;            /* GTFBCB CONTROL BLOCK ID       */
         MVI   VCTCBIDN(VCCTREG),X'08'                             0265
*     SVC(VCKSVC);                  /* CALL AMDSAVCK TO VALIDITY   0266
*                                      CHECK THE GTFBCB              */
         SVC   25                                                  0266
*     RESPECIFY RETCODE RESTRICTED;                                0267
*     IF RETCODE ^= 0 THEN          /* IS GTFBCB VALID               */
         LTR   RETCODE,RETCODE                                     0268
         BNZ   @RT00268                                            0268
*       GO TO GTFEXIT2;             /* NO - TAKE ERROR EXIT          */
*     RESPECIFY RETCODE UNRESTRICTED;                              0270
*     R1 = GBUFRPTR;                /* GET BUFFER ADDRESS            */
         L     @10,GBCBPTR                                         0271
         L     R1,GBUFRPTR(,@10)                                   0271
*     R0 = PAGESIZE;                /* SET BUFFER SIZE               */
         LH    R0,@CH00956                                         0272
*     SVC(ARDSVC);                  /* CALL AMDSAARD TO BUILD AN ARB */
*                                                                  0273
         SVC   16                                                  0273
*GTFNEXT:                                                          0274
*     GBCBPTR = HQCHAIN;            /* GET NEXT GTFBCB               */
GTFNEXT  L     @10,GBCBPTR                                         0274
         L     @10,HQCHAIN(,@10)                                   0274
         ST    @10,GBCBPTR                                         0274
*     IF GBCBPTR = 0 THEN           /* REACHED END OF CHAIN          */
         LTR   @10,@10                                             0275
         BZ    @RT00275                                            0275
*       GO TO GTFRET2;              /* YES - DUMP ARBS               */
*   END;                            /* CONTINUE GTFBCB SEARCH        */
         BCTR  I,0                                                 0277
@DE00263 LTR   I,I                                                 0277
         BP    @DL00263                                            0277
*   GO TO GTF18I2;                  /* DID NOT REACH END OF CHAIN  0278
*                                      WHEN COUNT RAN OUT - POSSIBLE
*                                      LOOP                          */
         B     GTF18I2                                             0278
*                                                                  0279
*   /*****************************************************************/
*   /*                                                               */
*   /* ADDRESS RANGE BLOCKS (ARBS) HAVE BEEN BUILT. REQUEST DUMPING  */
*   /* OF THE ARBS. INITIALIZE AND ISSUE MESSAGE AMD023I FOR GTF.    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0279
*/*GTFRET2: P SET REG 1 TO 0                                         */
*/* L AMDSAARD: DUMP ARBS TO OUTPUT                                  */
*/* P INITIALIZE MESSAGE AMD023I FOR GTF                             */
*/* N THIS REQUIRES SETTING COMP23I AND LNG23I                       */
*/* L AMDSACON: ISSUE MESSAGE AMD023I                                */
*                                                                  0279
*GTFRET2:                                                          0279
*   R1 = 0;                         /* REQUEST DUMPING               */
GTFRET2  SR    R1,R1                                               0279
*   SVC(ARDSVC);                    /* CALL AMDSAARD TO DUMP ARBS    */
         SVC   16                                                  0280
*   VCOMP23I = GTFNAME;             /* MOVE GTF NAME TO MESSAGE      */
         L     @10,@CA01560                                        0281
         L     @15,VCTLPA(,VCCTREG)                                0281
         AR    @10,@15                                             0281
         MVC   VCOMP23I(7,@10),GTFNAME                             0281
*   VLNG23I = LENGTH(COMP23I);      /* SET UP SECONDARY LENGTH,    0282
*                                      INCLUDING 'FOR'               */
         L     @10,@CA01556                                        0282
         AR    @10,@15                                             0282
         MVI   VLNG23I(@10),X'07'                                  0282
*   R1 = AMD023I;                   /* REQUEST MESSAGE AMD023I       */
         LA    R1,23                                               0283
*   SVC(CONSVC);                    /* CALL AMDSACON TO ISSUE MESSAGE*/
*                                                                  0284
         SVC   3                                                   0284
*   /*****************************************************************/
*   /*                                                               */
*   /* RETURN PATH - DEQUEUE RCB AND RETURN                          */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0285
*/*%GTFCOMP: P (,GTFRET1) INDICATE PROCESSING COMPLETE               */
*/*GTFRET1: P DEQUEUE RCB                                            */
*/* P RESTORE REGISTERS                                              */
*/* P SET RETURN CODE                                                */
*/* R RETURN TO CALLER                                               */
*                                                                  0285
*GTFCOMP:                                                          0285
*   GTFCOMPF = ON;                  /* MARK PROCESSING COMPLETE      */
*                                                                  0285
GTFCOMP  OI    GTFCOMPF,B'10000000'                                0285
*GTFRET1:                                                          0286
*   VCTRCB = RCBNEXT;               /* DEQUEUE RCB                   */
GTFRET1  LA    @10,GTFRCB                                          0286
         MVC   VCTRCB(4,VCCTREG),RCBNEXT(@10)                      0286
*   RETURN;                         /* RETURN TO CALLER              */
@EL00001 L     @13,4(,@13)                                         0287
@EF00001 DS    0H                                                  0287
@ER00001 LM    @14,@12,12(@13)                                     0287
         BR    @14                                                 0287
*                                                                  0288
*   /*****************************************************************/
*   /*                                                               */
*   /* FIRST PASS ERROR EXIT - ISSUE MESSAGE AMD025I (GTF DUMP       */
*   /* BYPASSED) IF ERROR OCCURS DURING FIRST PASS.                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0288
*/*GTFEXIT1: P SET UP RETURN CODE = 4                                */
*/* L (,%GTFCOMP) AMDSACON:  ISSUE MESSAGE AMD025I                   */
*                                                                  0288
*GTFEXIT1:                                                         0288
*   SAVEBACK -> SAVER15 = 4;        /* SET UP RETURN CODE OF 4       */
GTFEXIT1 L     @10,SAVEBACK(,R13)                                  0288
         MVC   SAVER15(4,@10),@CF00053                             0288
*   R1 = AMD025I;                   /* REQUEST MESSAGE AMD025I       */
         LA    R1,25                                               0289
*   SVC(CONSVC);                    /* CALL AMDSACON TO ISSUE MESSAGE*/
         SVC   3                                                   0290
*   GO TO GTFCOMP;                  /* MARK PROCESSING COMPLETE      */
*                                                                  0291
         B     GTFCOMP                                             0291
*   /*****************************************************************/
*   /*                                                               */
*   /* SECOND PASS ERROR EXIT - ISSUE MESSAGE AMD022I (ERROR DUMPING */
*   /* GTF) IF ERROR OCCURS DURING PROCESSING BUFFER CHAIN. RETURN TO*/
*   /* CALLER                                                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0292
*/*GTFEXIT2: P SET UP RETURN CODE = 4                                */
*/* P INITIALIZE MESSAGE AMD022I FOR GTF                             */
*/* N THIS REQUIRES SETTING LNG22I AND DMP22I                        */
*/* L (,GTFRET2) AMDSACON: ISSUE MESSAGE AMD022I                     */
*                                                                  0292
*GTFEXIT2:                                                         0292
*   SAVEBACK -> SAVER15 = 4;        /* SET UP RETURN CODE OF 4       */
GTFEXIT2 L     @10,SAVEBACK(,R13)                                  0292
         MVC   SAVER15(4,@10),@CF00053                             0292
*   VDMP22I = GTFNAME;              /* MOVE GTF NAME TO MESSAGE      */
         L     @10,@CA01561                                        0293
         L     @15,VCTLPA(,VCCTREG)                                0293
         AR    @10,@15                                             0293
         MVC   VDMP22I(8,@10),GTFNAME                              0293
*   VLNG22I = LENGTH(GTFNAME);      /* SET UP SECONDARY MESSAGE    0294
*                                      LENGTH                        */
         L     @10,@CA01555                                        0294
         AR    @10,@15                                             0294
         MVI   VLNG22I(@10),X'08'                                  0294
*   R1 = AMD022I;                   /* REQUEST MESSAGE AMD022I       */
         LA    R1,22                                               0295
*   SVC(CONSVC);                    /* CALL AMDSACON TO ISSUE MESSAGE*/
         SVC   3                                                   0296
*   GO TO GTFRET2;                  /* PREPARE TO EXIT               */
         B     GTFRET2                                             0297
*                                                                  0298
*   /*****************************************************************/
*   /*                                                               */
*   /* PROBABLE LOOP IN MCQE CHAIN - ISSUE MESSAGE AMD018I           */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0298
*/*%GTF18I1: P SET UP RETURN CODE OF 4                               */
*/* P INITIALIZE MESSAGE AMD018I FOR MCQE LOOP                       */
*/* N THIS REQUIRES SETTING CBN18I AND CBER18I                       */
*/* L AMDSABIN: CONVERT ADDR TO EBCDIC                               */
*/* P INITIALIZE MESSAGE AMD018I                                     */
*/* N SET ADDR18I                                                    */
*/* L (,GTFEXIT1) AMDSACON: ISSUE MESSAGE AMD018I                    */
*                                                                  0298
*GTF18I1:                                                          0298
*   SAVEBACK -> SAVER15 = 4;        /* SET UP RETURN CODE OF 4       */
GTF18I1  L     @10,SAVEBACK(,R13)                                  0298
         MVC   SAVER15(4,@10),@CF00053                             0298
*   VCBN18I = VCTCBID;              /* MOVE CONTROL BLOCK ID TO    0299
*                                      MESSAGE                       */
         L     @10,@CA01558                                        0299
         L     @15,VCTLPA(,VCCTREG)                                0299
         AR    @10,@15                                             0299
         MVC   VCBN18I(8,@10),VCTCBID(VCCTREG)                     0299
*   VCBER18I = LOOPMSG;             /* MOVE LOOP ERROR INDICATOR     */
         L     @10,@CA01559                                        0300
         AR    @10,@15                                             0300
         MVC   VCBER18I(7,@10),@CC01575                            0300
*   GTFBINA = ADDR(GTFMCQEA) + 1;   /* ADDRESS OF MCQE               */
         LA    @10,GTFMCQEA                                        0301
         AH    @10,@CH00061                                        0301
         ST    @10,GTFBINA                                         0301
*   GTFEBCA = ADDR(VADDR18I);       /* ADDRESS OF OUTPUT AREA        */
         L     @10,@CA01557                                        0302
         AR    @10,@15                                             0302
         ST    @10,GTFEBCA                                         0302
*   GTFBLEN = LENGTH(GTFMCQEA) - 1; /* LENGTH OF ADDRESS             */
         MVC   GTFBLEN(2),@CH00287                                 0303
*   R1 = ADDR(GTFBINL);             /* POINT TO CONVERT PARAMETER  0304
*                                      LIST                          */
         LA    R1,GTFBINL                                          0304
*   SVC(BINSVC);                    /* CALL AMDSABIN TO CONVERT TO 0305
*                                      EBCDIC                        */
         SVC   28                                                  0305
*   R1 = AMD018I;                   /* REQUEST MESSAGE AMD018I       */
         LA    R1,18                                               0306
*   SVC(CONSVC);                    /* CALL AMDSACON TO ISSUE MESSAGE*/
         SVC   3                                                   0307
*   GO TO GTFEXIT1;                 /* FINISHED PROCESSING           */
*                                                                  0308
         B     GTFEXIT1                                            0308
*   /*****************************************************************/
*   /*                                                               */
*   /* PROBABLE LOOP IN GTFBCB CHAIN. ISSUE MESSAGE AMD018I AND      */
*   /* DISCONTINUE GTFBCB SEARCH.                                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0309
*/*%GTF18I2: P SET UP RETURN CODE OF 4                               */
*/* P INITIALIZE MESSAGE AMD018I FOR GTFBCB LOOP                     */
*/* N THIS REQUIRES SETTING CBN18I AND CBER18I                       */
*/* L AMDSABIN: CONVERT ADDR TO EBCDIC                               */
*/* P INITIALIZE MESSAGE AMD018I                                     */
*/* N SET ADDR18I                                                    */
*/* L (,GTFEXIT2) AMDSACON: ISSUE MESSAGE AMD018I                    */
*/*AMDSAGTF: END                                                     */
*                                                                  0309
*GTF18I2:                                                          0309
*   SAVEBACK -> SAVER15 = 4;        /* SET UP RETURN CODE OF 4       */
GTF18I2  L     @10,SAVEBACK(,R13)                                  0309
         MVC   SAVER15(4,@10),@CF00053                             0309
*   VCBN18I = VCTCBID;              /* MOVE CONTROL BLOCK ID TO    0310
*                                      MESSAGE                       */
         L     @10,@CA01558                                        0310
         L     @15,VCTLPA(,VCCTREG)                                0310
         AR    @10,@15                                             0310
         MVC   VCBN18I(8,@10),VCTCBID(VCCTREG)                     0310
*   VCBER18I = LOOPMSG;             /* MOVE LOOP ERROR INDICATOR     */
         L     @10,@CA01559                                        0311
         AR    @10,@15                                             0311
         MVC   VCBER18I(7,@10),@CC01575                            0311
*   GTFBINA = ADDR(GBCBPTR) + 1;    /* ADDRESS OF GTFBCB             */
         LA    @10,GBCBPTR                                         0312
         AH    @10,@CH00061                                        0312
         ST    @10,GTFBINA                                         0312
*   GTFEBCA = ADDR(VADDR18I);       /* ADDRESS OF OUTPUT AREA        */
         L     @10,@CA01557                                        0313
         AR    @10,@15                                             0313
         ST    @10,GTFEBCA                                         0313
*   GTFBLEN = LENGTH(GBCBPTR) - 1;  /* LENGTH OF ADDRESS             */
         MVC   GTFBLEN(2),@CH00287                                 0314
*   R1 = ADDR(GTFBINL);             /* POINT TO CONVERT PARAMETER  0315
*                                      LIST                          */
         LA    R1,GTFBINL                                          0315
*   SVC(BINSVC);                    /* CALL AMDSABIN TO CONVERT TO 0316
*                                      EBCDIC                        */
         SVC   28                                                  0316
*   R1 = AMD018I;                   /* REQUEST MESSAGE AMD018I       */
         LA    R1,18                                               0317
*   SVC(CONSVC);                    /* CALL AMDSACON TO ISSUE MESSAGE*/
         SVC   3                                                   0318
*   GO TO GTFEXIT2;                 /* DUMP ARBS AND EXIT            */
         B     GTFEXIT2                                            0319
*   END AMDSAGTF                                                   0320
