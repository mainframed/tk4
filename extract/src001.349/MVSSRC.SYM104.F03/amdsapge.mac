*   /*****************************************************************/
*   /*                                                               */
*   /* EXTERNAL INTERRUPT HANDLER AND TERMINATION ROUTINE - RECEIVES */
*   /* CONTROL FOLLOWING AN EXTERNAL INTERRUPT FROM THE CONSOLE, FROM*/
*   /* AMDSAPGE TO TERMINATE VIRTUAL DUMP OPERATIONS, OR FROM        */
*   /* AMDSAAUD THROUGH THE RCB EXIT. THE OUTPUT TAPE , AND THE      */
*   /* WORKFILE TAPE IF TAPE IPL WAS USED, IS TAPE MARKED, REWOUND,  */
*   /* AND UNLOADED. PRIOR TO DOING THIS, ALL I/O IS ALLOWED TO      */
*   /* QUIESCE. MESSAGE AMD023I IS ISSUED AND A WAIT STATE IS ENTERED*/
*   /* WITH CTWAIT AS THE IC.  ADDRESSABILITY IS SET UP HERE BECAUSE */
*   /* THIS ROUTINE MAY RECEIVE CONTROL ASYNCHRONOUSLY VIA AN        */
*   /* EXTERNAL INTERRUPTION.                                        */
*   /*                                                               */
*   /*****************************************************************/
*/*AMDSAEXI: E AMDSAEXI                                              */
*/* N EXTERNAL INTERRUPT HANDLER AND TERMINATION RTN                 */
*/* P SET UP ADDRESSABILITY                                          */
*/* P DISABLE FOR EXTERNAL INTERRUPTS                                */
*/* P REFRESH PSWS IN LOW CORE                                       */
*/* P ZERO VCTSTOR TO PREVENT MEMORY SWITCHING                       */
*/* D (NO,%EXIERR,YES,) VCTEXIR ON                                   */
*/* N INVALID RECURSION OCCURRING                                    */
*/* N ENTER WAIT STATE WITH IC OF X'2C'                              */
*/* R LPSW WAIT STATE                                                */
*/*%EXIERR: P SET VCTEXIR FOR RECURSION                              */
*/* D (YES,,NO,%NOCNCEL) OPERATOR CANCEL                             */
*/* P (,%EXICONT) SET WAIT STATE CODE FOR OPERATOR CANCEL            */
*/*%NOCNCEL: P SAVE CTWAIT (FINAL WAIT STATE VALUE)                  */
*                                                               01S0473
*                                                               01S0473
*AMDSAEXI:                                                      01S0473
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                            01S0473
AMDSAEXI DS    0H                                               01S0474
*   GENERATE REFS(BASEREG,INTEXSM) SETS(BASEREG);               01S0474
         BALR  BASEREG,0               SET UP BASE REGISTER
         USING *,BASEREG
         ENTRY AMDSAEXI
*   SSM (INTEXSM);                  /* DISABLE FOR EXTERNAL INTERRUPT*/
         SSM   INTEXSM                                          01S0475
*   NEWPSWS = INTHPSWS;             /* REFRESH PSWS IN LOW CORE      */
         MVC   NEWPSWS(40),INTHPSWS                             01S0476
*   VCCTREG = ADDR(VCCT);           /* RE-ESTABLISH ADDRESSABILITY TO
*                                      VCCT                          */
         LA    VCCTREG,VCCT                                     01S0477
*   VCTSTOR = 0;                    /* ENSURE NO MEMORY SWITCHING    */
         SLR   @14,@14                                          01S0478
         ST    @14,VCTSTOR                                      01S0478
*   IF VCTEXIR = ON THEN            /* RECURSION THROUGH AMDSAEXI    */
         TM    VCTEXIR,B'00100000'                              01S0479
         BNO   @RF00479                                         01S0479
*     SACTWAIT = EXRECSN;           /* YES - WAIT STATE CODE X'2C'   */
         MVI   SACTWAIT,X'2C'                                   01S0480
*   ELSE                                                        01S0481
*     DO;                                                       01S0481
         B     @RC00479                                         01S0481
@RF00479 DS    0H                                               01S0482
*       VCTEXIR = ON;               /* SET RECURSION BIT             */
         OI    VCTEXIR,B'00100000'                              01S0482
*       IF FLCEICOD ^= 0 THEN       /* DID OPERATOR CANCEL DUMP      */
         ICM   @14,3,FLCEICOD                                   01S0483
         BZ    @RF00483                                         01S0483
*         SACTWAIT = OPCANCEL;      /* SET OPERATOR CANCEL WAIT STATE*/
         MVI   SACTWAIT,X'2E'                                   01S0484
*       ELSE                                                    01S0485
*         SACTWAIT = CTWAIT;        /* SAVE CTWAIT ACROSS TERMINATION
*                                      PROCESSING                    */
         B     @RC00483                                         01S0485
@RF00483 LH    @14,@CH01539                                     01S0485
         MVC   SACTWAIT(1),CTWAIT(@14)                          01S0485
*       RESPECIFY RCB BASED(ADDR(PGERCB));                      01S0486
@RC00483 DS    0H                                               01S0487
*       R13 = RCBSAVE;              /* GET LOCAL SAVE AREA POINTER   */
         LA    @14,PGERCB                                       01S0487
         L     R13,RCBSAVE(,@14)                                01S0487
*                                                               01S0488
*   /*****************************************************************/
*   /*                                                               */
*   /* ALLOW I/O ON OUTPUT TAPE, AND ON THE WORKFILE TAPE IF TAPE    */
*   /* IPL, TO QUIESCE                                               */
*   /*                                                               */
*   /*****************************************************************/
*/*%EXICONT: D (YES,,NO,%PGETERM) IPL FROM TAPE                      */
*/* P SET CTIOTYPE TO CTIOWK                                         */
*/* D (NO,,YES,%NIOTERM) NORMAL TERMINATION                          */
*/* P SET CTIOTERM ON TO IGNORE BUFFERS                              */
*/*%NIOTERM: L  AMDSAWAT:  WAIT FOR I/O COMPLETION ON WORKFILE       */
*/*%PGETERM: P SET CTIOTYPE TO CTIOOUT                               */
*/* L  AMDSAWAT:  WAIT FOR I/O COMPLETION ON OUTPUT                  */
*                                                               01S0488
*       RESPECIFY IODB BASED(ADDR(IODBDA));                     01S0488
*       IODBDEV = 0;                /* IGNORE DEVICE DURING WAIT     */
         LA    @14,IODBDA                                       01S0489
         SLR   @04,@04                                          01S0489
         STH   @04,IODBDEV(,@14)                                01S0489
*       RESPECIFY IODB BASED(ADDR(IODBWORK));                   01S0490
*       CTIOTYPE = 0;               /* ZERO I/O REQUEST FLAGS        */
         LH    @14,@CH01539                                     01S0491
         MVI   CTIOTYPE(@14),X'00'                              01S0491
*       IF VCTNTERM = OFF THEN      /* NORMAL TERMINATION            */
         TM    VCTNTERM,B'00000100'                             01S0492
         BNZ   @RF00492                                         01S0492
*         CTIOTERM = ON;            /* NO - INDICATE BUFFERS SHOULD BE
*                                      IGNORED SINCE I/O PROCESSING MAY
*                                      BE INCOMPLETE DUE TO ERROR OR
*                                      OPERATOR CANCELLING           */
         OI    CTIOTERM(@14),B'10000000'                        01S0493
*       IF CTDEVTYP = IPLTAPE THEN  /* IPL FROM TAPE                 */
@RF00492 LH    @14,@CH01539                                     01S0494
         CLI   CTDEVTYP(@14),1                                  01S0494
         BNE   @RF00494                                         01S0494
*         DO;                       /* YES                           */
*           CTIOWK = ON;            /* REQUEST I/O TO WORKFILE       */
         OI    CTIOWK(@14),B'00010000'                          01S0496
*           SVC(WATSVC);            /* CALL AMDSAWAT TO WAIT FOR I/O
*                                      TO COMPLETE                   */
         SVC   1                                                01S0497
*         END;                                                  01S0498
*       ELSE                                                    01S0499
*         IODBDEV = 0;              /* IGNORE DEVICE DURING WAIT     */
         B     @RC00494                                         01S0499
@RF00494 LA    @14,IODBWORK                                     01S0499
         SLR   @04,@04                                          01S0499
         STH   @04,IODBDEV(,@14)                                01S0499
*       CTIOWK = OFF;               /* RESET WORKFILE REQUEST        */
@RC00494 DS    0H                                               01S0501
*       CTIOOUT = ON;               /* REQUEST I/O TO OUTPUT TAPE    */
         LH    @14,@CH01539                                     01S0501
         OI    CTIOOUT(@14),B'01000000'                         01S0501
         NI    CTIOWK(@14),B'11101111'                          01S0501
*       SVC(WATSVC);                /* CALL AMDSAWAT TO WAIT FOR I/O
*                                      TO COMPLETE                   */
         SVC   1                                                01S0502
*                                                               01S0503
*   /*****************************************************************/
*   /*                                                               */
*   /* REQUEST TERMINATION I/O ON OUTPUT, AND WORKFILE IF TAPE IPL   */
*   /*                                                               */
*   /*****************************************************************/
*/* P SET CTIOTERM IN CTIOTYPE                                       */
*/* L  AMDSASIO:  TERMINATION I/O                                    */
*/* D (YES,,NO,PGEDONE) RC  =  0                                     */
*/* P SET CTIOOUT IN CTIOTYPE                                        */
*/* L  AMDSAWAT:  WAIT FOR COMPLETION ON OUTPUT TAPE                 */
*/* D (YES,,NO,PGEDONE) IPL FROM TAPE                                */
*/* P RESET CTIOOUT                                                  */
*/* P SET CTIOWK IN CTIOTYPE                                         */
*/* L  AMDSAWAT:  WAIT FOR COMPLETION ON IPL/WORKFILE TAPE           */
*                                                               01S0503
*       CTIOTYPE = 0;               /* RESET I/O REQUEST FLAGS       */
         LH    @14,@CH01539                                     01S0503
         MVI   CTIOTYPE(@14),X'00'                              01S0503
*       CTIOTERM = ON;              /* REQUEST TERMINATION I/O       */
         OI    CTIOTERM(@14),B'10000000'                        01S0504
*       SVC(SIOSVC);                /* CALL AMDSASIO TO INITIATE I/O */
         SVC   0                                                01S0505
*       RESPECIFY RETCODE RESTRICTED;                           01S0506
*       IF RETCODE = 0 THEN         /* I/O INITIATED SUCCESSFULLY    */
         LTR   RETCODE,RETCODE                                  01S0507
         BNZ   @RF00507                                         01S0507
*         DO;                       /* YES -                         */
*           CTIOOUT = ON;           /* INDICATE OUTPUT               */
         LH    @14,@CH01539                                     01S0509
         OI    CTIOOUT(@14),B'01000000'                         01S0509
*           SVC(WATSVC);            /* WAIT FOR TERMINATION I/O ON
*                                      OUTPUT TAPE                   */
         SVC   1                                                01S0510
*           IF CTDEVTYP = IPLTAPE THEN /* IPL FROM TAPE              */
         LH    @14,@CH01539                                     01S0511
         CLI   CTDEVTYP(@14),1                                  01S0511
         BNE   @RF00511                                         01S0511
*             DO;                                               01S0512
*               CTIOOUT = OFF;      /* RESET OUTPUT FLAG             */
*               CTIOWK = ON;        /* INDICATE WORKFILE             */
         OI    CTIOWK(@14),B'00010000'                          01S0514
         NI    CTIOOUT(@14),B'10111111'                         01S0514
*               SVC(WATSVC);        /* WAIT FOR TERMINATION I/O ON
*                                      WORKFILE                      */
         SVC   1                                                01S0515
*             END;                                              01S0516
*         END;                                                  01S0517
@RF00511 DS    0H                                               01S0518
*                                                               01S0518
*   /*****************************************************************/
*   /*                                                               */
*   /* ISSUE MESSAGE AMD023I                                         */
*   /*                                                               */
*   /*****************************************************************/
*/*PGEDONE: P INITIALIZE MESSAGE AMD023I                             */
*/* N THIS REQUIRES SETTING LNG23I AND FOR23I                        */
*/* L  AMDSABIN:  CONVERT WAIT STATE CODE TO EBCDIC                  */
*/* L  AMDSACON:  ISSUE MESSAGE AMD023I                              */
*                                                               01S0518
*       FOR23I(1:4) = '-   ';       /* SET UP COMPLETION MSG         */
@RF00507 L     @14,@CA01748                                     01S0518
         MVC   FOR23I-FOR23I(4,@14),@CC01888                    01S0518
*       BINADDR = ADDR(SACTWAIT);   /* VALUE TO BE CONVERTED         */
         LA    @04,SACTWAIT                                     01S0519
         ST    @04,BINADDR                                      01S0519
*       BINEBCA = ADDR(FOR23I) + 2; /* OUTPUT AREA IN MSG           */
         AH    @14,@CH00037                                     01S0520
         ST    @14,BINEBCA                                      01S0520
*       BINLENG = 1;                /* LENGTH OF WAIT CODE           */
         MVC   BINLENG(2),@CH00039                              01S0521
*       RESPECIFY R1 RESTRICTED;                                01S0522
*       R1 = ADDR(BINPARM);         /* POINT TO CONVERT PARM LIST    */
         LA    R1,BINPARM                                       01S0523
*       SVC(BINSVC);                /* CALL AMDSABIN TO CONVERT VALUE*/
         SVC   28                                               01S0524
*       LNG23I = 4;                 /* SET UP SECONDARY MESSAGE 01S0525
*                                      LENGTH                        */
         L     @14,@CA01749                                     01S0525
         MVI   LNG23I-LNG23I(@14),X'04'                         01S0525
*       R1 = AMD023I;               /* REQUEST MESSAGE AMD023I TO BE
*                                      ISSUED                        */
         LA    R1,23                                            01S0526
*       SVC(CONSVC);                /* CALL AMDSACON TO ISSUE MESSAGE
*                                      TO CONSOLE                    */
         SVC   3                                                01S0527
*     END;                                                      01S0528
*                                                               01S0528
