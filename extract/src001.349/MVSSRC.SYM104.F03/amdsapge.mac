         IC    @04,SACTWAIT                                     01S0531
         ST    @04,PSWIA(,@14)                                  01S0531
*   LPSW (FLCRNPSW);                /* LOAD WAIT STATE PSW           */
         LPSW  FLCRNPSW                                         01S0532
