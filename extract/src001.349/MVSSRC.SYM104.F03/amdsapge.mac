         TITLE 'AMDSAPGE  VIRTUAL DUMP INITIALIZATION AND CONTROL      *
                        '
AMDSAPGE CSECT ,                                                01S0001
@MAINENT DS    0H                                               01S0001
         USING *,@15                                            01S0001
         B     @PROLOG                                          01S0001
         DC    AL2(@EP00001-@MAINENT)
         DC    AL1(16)                                          01S0001
         DC    C'AMDSAPGE  77.011'                              01S0001
AMDSABIN DS    0H                                               01S0001
         USING *,@15                                            01S0001
         B     @PROLOG                                          01S0001
         DC    AL2(@EP00450-AMDSABIN)
         ENTRY AMDSABIN
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                  01S0001
         BALR  @11,0                                            01S0001
@PSTART  LA    @10,4095(,@11)                                   01S0001
         USING @PSTART,@11                                      01S0001
         USING @PSTART+4095,@10                                 01S0001
         ST    @13,@SA00001+4                                   01S0001
         LA    @14,@SA00001                                     01S0001
         ST    @14,8(,@13)                                      01S0001
         LR    @13,@14                                          01S0001
         AH    @15,4(,@15)                                      01S0001
         BR    @15                                              01S0001
@EP00001 DS    0H                                               01S0002
*   VCCTREG = ADDR(VCCT);           /* ESTABLISH ADDRESSABILITY TO
*                                      VCCT                          */
         LA    VCCTREG,VCCT                                     01S0258
*   CTVCCT = VCCTREG;               /* POINT CCT TO VCCT             */
         LH    @01,@CH01539                                     01S0259
         ST    VCCTREG,CTVCCT(,@01)                             01S0259
*   RCBBASE = BASEREG;              /* SET UP BASE ADDRESS IN RCB    */
         LA    @15,PGERCB                                       01S0260
         ST    BASEREG,RCBBASE(,@15)                            01S0260
*   RCBSAVE = R13;                  /* SET UP SAVE AREA POINTER IN
*                                      RCB                           */
         ST    R13,RCBSAVE(,@15)                                01S0261
*   VCTRCB = ADDR(PGERCB);          /* PLACE RCB ON RCB QUEUE        */
         ST    @15,VCTRCB                                       01S0262
*   VCTSADMP = ON;                  /* SET ERROR DUMP REQUEST BIT    */
         OI    VCTSADMP,B'10000000'                             01S0263
*   FLCEICOD = 0;                   /* ZERO EXTERNAL INTERRUPT CODE  */
         SLR   @15,@15                                          01S0264
         STH   @15,FLCEICOD                                     01S0264
*   SAVEFORM(1:4) = 'LPGE';         /* MODULE ID TO SAVE AREA        */
         MVC   SAVEFORM(4,R13),@CC01848                         01S0265
*   NEWPSWS = INTHPSWS;             /* MOVE PSWS TO LOW CORE         */
         MVC   NEWPSWS(40),INTHPSWS                             01S0266
*   FLCRNPSW = RSTRTPSW;            /* MOVE PSW FOR PSWRESTART       */
         MVC   FLCRNPSW(8),RSTRTPSW                             01S0267
*   LPSW (ENMCPSW);                 /* ENABLE FOR MACHINE CHECKS     */
*                                                               01S0268
         LPSW  ENMCPSW                                          01S0268
*LOADCR0:                                                       01S0269
*   GENERATE REFS(PGECR0) SETS(R0) (LCTL R0,R0,PGECR0); /* SET UP
*                                      CONTROL REGISTER 0 FOR PAGE
*                                      SIZE, SEGMENT SIZE, ALLOW SSM
*                                      INSTRUCTIONS, ALLOW EXTERNAL
*                                      INTERRUPTS ONLY FROM CONSOLE. */
LOADCR0  LCTL R0,R0,PGECR0
*   PTRTPTRT = PTRTACRO;            /* MOVE PTRT ACRONYM             */
         LH    @01,@CH01373                                     01S0270
         MVC   PTRTPTRT(4,@01),PTRTACRO                         01S0270
*   PSWSADMP = 'PSWSADMP';          /* MOVE ID TO LOW CORE           */
         MVC   PSWSADMP(8),@CC01851                             01S0271
*   SAVEDUMP = 'SAVEDUMP';          /* MOVE ID TO LOW CORE           */
         MVC   SAVEDUMP(8),@CC01852                             01S0272
*   RESPECIFY I RESTRICTED;                                     01S0273
*   DO I = 6 TO 1 BY -1;            /* SET UP SAVE AREA IDS          */
         LA    I,6                                              01S0274
@DL00274 DS    0H                                               01S0275
*     LOWNAME = LOWINIT(I);         /* MOVE NAME                     */
         L     @03,LOWNPTR                                      01S0275
         LR    @15,I                                            01S0275
         SLL   @15,2                                            01S0275
         LA    @14,LOWINIT-4(@15)                               01S0275
         MVC   LOWNAME(4,@03),0(@14)                            01S0275
*     LOWNPTR = LOWNPTR + LOWSVLEN; /* POINT TO NEXT AREA            */
         AH    @03,LOWSVLEN                                     01S0276
         ST    @03,LOWNPTR                                      01S0276
*   END;                                                        01S0277
         BCTR  I,0                                              01S0277
         CL    I,@CF00039                                       01S0277
         BNL   @DL00274                                         01S0277
*   RESPECIFY I UNRESTRICTED;                                   01S0278
*   CTWAIT = 0;                     /* SET UP 0 WAIT STATE CODE      */
         LH    @15,@CH01539                                     01S0279
         MVI   CTWAIT(@15),X'00'                                01S0279
*   TRCTBL = TRCTBL && TRCTBL;      /* ZERO TRACE TABLE              */
         L     @01,DSCETBEG                                     01S0280
         XC    TRCTBL(256,@01),TRCTBL(@01)                      01S0280
*   DEBUGLSD = DEBUGLSD && DEBUGLSD; /* ZERO DEBUG COUNTERS          */
         XC    DEBUGLSD(26),DEBUGLSD                            01S0281
*                                                               01S0282
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE VCCT WITH WORKFILE AND CPU INFORMATION             */
*   /*                                                               */
*   /*****************************************************************/
*/* N INITIALIZE WORKFILE AND CPU INFORMATION                        */
*/* P SET VCTWKHI TO BITS 12-19 OF CTLWKAD                           */
*/* P SET VCTWKC TO VCTWKHI                                          */
*/* P SET VCTCPUC TO CTCPUI                                          */
*                                                               01S0282
*   VCTWKHI = CTLWKAD / PAGESIZE;   /* LAST BLOCK ON WORK FILE       */
         L     @14,CTLWKAD(,@15)                                01S0282
         SRL   @14,12                                           01S0282
         STC   @14,VCTWKHI                                      01S0282
*   VCTWKC = VCTWKHI;               /* INDICATE CURRENTLY AT END OF
*                                      WORKFILE                      */
         STC   @14,VCTWKC                                       01S0283
*   VCTCPUC = CTCPUI;               /* INDICATE CURRENTLY ON IPLED
*                                      CPU                           */
         MVC   VCTCPUC(2),CTCPUI(@15)                           01S0284
*                                                               01S0285
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE BUFFER AREA - DEFAULT TO 4 BUFFERS IF LESS THAN 64K*/
*   /* WAS WRITTEN TO WORKFILE                                       */
*   /*                                                               */
*   /*****************************************************************/
*/* N INITIALIZE BUFFER AREA                                         */
*/* D (YES,%PGECVTD,NO,) TAPE IPL                                    */
*/* P COMPUTE X'8000' + X'1800' * CTPGECNT                           */
*/*%PGECVTD: N THIS GIVES ENDING ADDRESS OF VIRTUAL DUMP             */
*/* P COMPUTE NUMBER OF EXTRA PAGES ABOVE CODE                       */
*/* D (YES,,NO,%PGEPGT1) VALUE > 0                                   */
*/* P INITIALIZE BCTVADDR AND BCTRADDR FOR EXTRA BCTS                */
*/* P INCREMENT BCTHLAST TO REFLECT EXTRA BUFFERS                    */
*                                                               01S0285
*   RESPECIFY SGTSTE BASED(ADDR(VCTPGT));                       01S0285
*   SGTLK = VALIDSGT;               /* MAKE VCTPGT LOOK LIKE VALID
*                                      SEGMENT TABLE ENTRY           */
         LA    @14,VCTPGT                                       01S0286
         MVI   SGTLK(@14),X'F0'                                 01S0286
*   RESPECIFY SGTSTE BASED(STEPTR);                             01S0287
*   RESPECIFY I RESTRICTED;                                     01S0288
*   IF CTDEVTYP ^= IPLTAPE THEN     /* TAPE IPL                      */
         CLI   CTDEVTYP(@15),1                                  01S0289
         BE    @RF00289                                         01S0289
*     CODEENDS = ADDR(AMDSAPGE) + DARCDSZ * CTPGECNT; /* COMPUTE
*                                      ENDING ADDRESS OF VIRTUAL DUMP
*                                      CODE                          */
         L     @03,@CA00028                                     01S0290
         SLR   @14,@14                                          01S0290
         IC    @14,CTPGECNT(,@15)                               01S0290
         MH    @14,@CH01814                                     01S0290
         ALR   @03,@14                                          01S0290
         ST    @03,CODEENDS                                     01S0290
*   CODEENDS = (CODEENDS + DISPLMNT) & PAGERND; /* ROUND ADDRESS UP TO
*                                      PAGE BOUNDARY                 */
@RF00289 LA    @03,4095                                         01S0291
         AL    @03,CODEENDS                                     01S0291
         N     @03,@CF01816                                     01S0291
         ST    @03,CODEENDS                                     01S0291
*   RESPECIFY BCT BASED(BCTHLAST);                              01S0292
*   IF ((CTLWKAD + PAGESIZE) > CODEENDS) & (CODEENDS > SEGSIZE) THEN /*
*                                      HAS MORE THAN CODE BEEN WRITTEN
*                                      TO WORKFILE                   */
         LH    @01,@CH01539                                     01S0293
         LH    @00,@CH01806                                     01S0293
         AL    @00,CTLWKAD(,@01)                                01S0293
         CR    @00,@03                                          01S0293
         BNH   @RF00293                                         01S0293
         C     @03,@CF01804                                     01S0293
         BNH   @RF00293                                         01S0293
*     DO I = CODEENDS TO CTLWKAD BY PAGESIZE; /* GET EXTRA BUFFERS   */
         LR    I,@03                                            01S0294
         B     @DE00294                                         01S0294
@DL00294 DS    0H                                               01S0295
*       BCTHLAST = BCTHLAST + LENGTH(BCT); /* SET LAST BUFFER POINTER
*                                      TO INCLUDE EXTRA BUFFER       */
         LA    @03,48                                           01S0295
         AL    @03,BCTHLAST                                     01S0295
         ST    @03,BCTHLAST                                     01S0295
*       BCTVADDR = I;               /* SET VIRTUAL ADDRESS           */
         ST    I,BCTVADDR(,@03)                                 01S0296
*       BCTRADDR = I;               /* SET REAL ADDRESS              */
         ST    I,BCTRADDR(,@03)                                 01S0297
*     END;                                                      01S0298
*                                                               01S0298
         AL    I,@CF01806                                       01S0298
@DE00294 LH    @03,@CH01539                                     01S0298
         CL    I,CTLWKAD(,@03)                                  01S0298
         BNH   @DL00294                                         01S0298
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE RELOCATION PAGE TABLES FOR ADDITIONAL BUFFER SPACE */
*   /*                                                               */
*   /*****************************************************************/
*/*%PGEPGT1: N INITIALIZE RELOCATION PAGE TABLES                     */
*/* P DETERMINE MAXIMUM OVERLAID ADDRESS                             */
*/* N THIS IS EITHER END OF CODE OR BUFFERS                          */
*/* P COMPUTE NUMBER OF EXTRA PAGES                                  */
*/* D (YES,,NO,PGEBCT1) ANY EXTRA PAGES                              */
*/*PGEPGTE: P TURN OFF INVALID BIT IN PGTE FOR NEXT PAGE             */
*/* P DECREMENT NUMBER OF REMAINING PAGES TO BE UPDATED              */
*/* D (YES,PGEPGTE,NO,) ANY MORE PAGES TO UPDATE                     */
*                                                               01S0299
*   I = (MAX(CODEENDS,CTLWKAD+PAGESIZE) - SEGSIZE) / PAGESIZE; /*
*                                      COMPUTE NUMBER OF PAGES ABOVE 1
*                                      SEGMENT                       */
@RF00293 LH    @15,@CH01806                                     01S0299
         LH    @01,@CH01539                                     01S0299
         L     @00,CTLWKAD(,@01)                                01S0299
         ALR   @00,@15                                          01S0299
         L     I,CODEENDS                                       01S0299
         CR    I,@00                                            01S0299
         BNL   *+6
         LR    I,@00                                            01S0299
         SL    I,@CF01804                                       01S0299
         SRDA  I,32                                             01S0299
         DR    I,@15                                            01S0299
         LR    I,@03                                            01S0299
*   IF I > 0 THEN                   /* USING MORE THAN 1 SEGMENT     */
         CL    I,@CF00073                                       01S0300
         BNH   @RF00300                                         01S0300
*     DO;                           /* YES -                         */
*       VCTEXTRA = I;               /* SET NUMBER OF EXTRA PAGES     */
         STH   I,VCTEXTRA                                       01S0302
*       PTEPTR = VCTPGT + LENGTH(PGTPTE) * PGEPRSEG; /* POINT TO
*                                      SECOND PAGE TABLE             */
         LA    PTEPTR,32                                        01S0303
         AL    PTEPTR,VCTPGT                                    01S0303
*       DO I = VCTEXTRA TO 1 BY -1; /* UPDATE EXTRA PAGES            */
         LH    @15,VCTEXTRA                                     01S0304
         LR    I,@15                                            01S0304
         B     @DE00304                                         01S0304
@DL00304 DS    0H                                               01S0305
*         PGTPVM = OFF;             /* INDICATE PAGE IS VALID        */
         NI    PGTPVM(PTEPTR),B'11110111'                       01S0305
*         PTEPTR = PTEPTR + LENGTH(PGTPTE); /* POINT TO NEXT PAGE
*                                      TABLE ENTRY                   */
         AH    PTEPTR,@CH00037                                  01S0306
*       END;                                                    01S0307
         BCTR  I,0                                              01S0307
@DE00304 CL    I,@CF00039                                       01S0307
         BNL   @DL00304                                         01S0307
*     END;                                                      01S0308
*   RESPECIFY I UNRESTRICTED;                                   01S0309
@RF00300 DS    0H                                               01S0310
*                                                               01S0310
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE IODBS TO REFLECT DEVICE ADDRESSES, CPU ADDRESSES,  */
*   /* AND DEVICE ATTRIBUTES                                         */
*   /*                                                               */
*   /*****************************************************************/
*/*PGEBCT1: N INITIALIZE CONSOLE IODB                                */
*/* P SET IODBCLAS AND IODBTYPE TO CTCONTYP                          */
*/* P SET IODBCPU TO CTCPUI                                          */
*/* N INITIALIZE OUTPUT IODB                                         */
*/* P SET IODBDEV TO CTOUTAD                                         */
*/* P SET IODBCPU TO CTCPUI                                          */
*/* N INITIALIZE WORKFILE IODB                                       */
*/* P SET IODBDEV TO CTINADDR                                        */
*/* P SET IODBCPU TO CTCPUI                                          */
*/* P SET IODBTYPE TO CTDEVTYP                                       */
*/* D (YES,,NO,%PGEWK1) CTDEVTYP FOR TAPE                            */
*/* P (,%PGEWK2) SET IODBCLAS TO TAPE DEVICE CLASS                   */
*/*%PGEWK1: P SET IODBCLAS TO DIRECT ACCESS DEVICE CLASS             */
*                                                               01S0310
*   RESPECIFY IODB BASED(ADDR(IODBCON));                        01S0310
*   IODBCLAS = CTCONTYP;            /* SET CONSOLE TYPE IN CONSOLE
*                                      IODB                          */
         LA    @15,IODBCON                                      01S0311
         LH    @14,@CH01539                                     01S0311
         MVC   IODBCLAS(1,@15),CTCONTYP(@14)                    01S0311
*   IODBTYPE = CTCONTYP;            /* SET CONSOLE TYPE IN CONSOLE
*                                      IODB                          */
         MVC   IODBTYPE(1,@15),CTCONTYP(@14)                    01S0312
*   IODBCPU = CTCPUI;               /* CONSOLE ACCESSIBLE FROM IPLED
*                                      CPU                           */
         LH    @09,CTCPUI(,@14)                                 01S0313
         STH   @09,IODBCPU(,@15)                                01S0313
*   RESPECIFY IODB BASED(ADDR(IODBOUT));                        01S0314
*   IODBDEV = CTOUTAD;              /* OUTPUT DEVICE ADDRESS TO 01S0315
*                                      OUTPUT IODB                   */
         LA    @15,IODBOUT                                      01S0315
         MVC   IODBDEV(2,@15),CTOUTAD(@14)                      01S0315
*   IODBCPU = CTCPUI;               /* OUTPUT TAPE ACCESSIBLE FROM
*                                      IPLED CPU                     */
         STH   @09,IODBCPU(,@15)                                01S0316
*   IODBTYPE = IPLTAPE;             /* OUTPUT IS TO TAPE             */
         MVI   IODBTYPE(@15),X'01'                              01S0317
*   IODBCLAS = TAPECLAS;            /* TAPE CLASS                    */
         MVI   IODBCLAS(@15),X'80'                              01S0318
*   RESPECIFY IODB BASED(ADDR(IODBWORK));                       01S0319
*   IODBDEV = CTINADDR;             /* WORKFILE (IPL) DEVICE ADDRESS
*                                      TO WORKFILE IODB              */
         LA    @15,IODBWORK                                     01S0320
         MVC   IODBDEV(2,@15),CTINADDR(@14)                     01S0320
*   IODBCPU = CTCPUI;               /* WORKFILE ACCESSIBLE FROM IPLED
*                                      CPU                           */
         STH   @09,IODBCPU(,@15)                                01S0321
*   IODBTYPE = CTDEVTYP;            /* SET WORKFILE DEVICE TYPE      */
         MVC   IODBTYPE(1,@15),CTDEVTYP(@14)                    01S0322
*   IF CTDEVTYP = IPLTAPE THEN      /* WORKFILE A TAPE DEVICE        */
         CLI   CTDEVTYP(@14),1                                  01S0323
         BNE   @RF00323                                         01S0323
*     IODBCLAS = TAPECLAS;          /* YES - SET TAPE CLASS          */
         MVI   IODBCLAS(@15),X'80'                              01S0324
*   ELSE                                                        01S0325
*     IODBCLAS = DACLASS;           /* NO - SET DIRECT ACCESS CLASS  */
         B     @RC00323                                         01S0325
@RF00323 LA    @15,IODBWORK                                     01S0325
         MVI   IODBCLAS(@15),X'20'                              01S0325
*                                                               01S0326
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK FOR INCOMPLETE WORKFILE. IF WORKFILE IS INCOMPLETE,     */
*   /* INITIALIZE MESSAGE AMD030I WITH THE REASON AND CALL AMDSACON. */
*   /*                                                               */
*   /*****************************************************************/
*/* D (NO,,YES,%PGEWK2) WORKFILE COMPLETE                            */
*/* D (YES,,NO,%WKIOER) INSUFF SPACE                                 */
*/* P (,%WK30I) INIT. AMD030I FOR INSUFFICIENT SPACE                 */
*/*%WKIOER: P INIT. AMD030I FOR I/O ERROR                            */
*/*%WK30I: L  AMDSACON:  ISSUE AMD030I                               */
*                                                               01S0326
*   IF CTLWKAD < MAXWKFL THEN       /* IS WORKFILE COMPLETE          */
@RC00323 LH    @15,@CH01539                                     01S0326
         CLC   CTLWKAD(4,@15),@CF01777                          01S0326
         BNL   @RF00326                                         01S0326
*     DO;                           /* NO - FIND OUT REASON          */
*       IF CTWKSTUS = ON THEN       /* INSUFFICIENT SPACE            */
         TM    CTWKSTUS(@15),B'00010000'                        01S0328
         BNO   @RF00328                                         01S0328
*         ERR30I = WKINSUSP;        /* YES - INIT. MESSAGE           */
         L     @15,@CA01750                                     01S0329
         MVC   ERR30I-ERR30I(18,@15),WKINSUSP                   01S0329
*       ELSE                        /* NO - I/O ERROR                */
*         ERR30I = WKIOERR;         /* INIT. MESSAGE                 */
         B     @RC00328                                         01S0330
@RF00328 L     @15,@CA01750                                     01S0330
         MVC   ERR30I-ERR30I(18,@15),WKIOERR                    01S0330
*       RESPECIFY R1 RESTRICTED;                                01S0331
@RC00328 DS    0H                                               01S0332
*       R1 = AMD030I;               /* MESSAGE I.D.                  */
         LA    R1,30                                            01S0332
*       SVC (CONSVC);               /* CALL AMDSACON TO ISSUE MESSAGE *
         SVC   3                                                01S0333
*       RESPECIFY R1 UNRESTRICTED;                              01S0334
*     END;                                                      01S0335
*                                                               01S0335
*   /*****************************************************************/
*   /*                                                               */
*   /* ENABLE FOR EXTERNAL INTERRUPTS. CONTROL REGISTER 0 MASKS      */
*   /* EVERYTHING EXCEPT INTERRUPTS FROM THE CONSOLE INTERRUPT KEY.  */
*   /* TERMINATION IS INVOKED ON EXTERNAL INTERRUPTS.                */
*   /*                                                               */
*   /*****************************************************************/
*/*%PGEWK2: P (,,EXT,AMDSAEXI) ENABLE FOR EXTERNAL INTERRUPTS FROM
*/*CONSOLE ONLY                                                      */
*                                                               01S0336
*   SSM (PGEMASK);                  /* ENABLE FOR EXTERNAL INTERRUPTS*/
@RF00326 SSM   @CB01541                                         01S0336
*                                                               01S0337
*   /*****************************************************************/
*   /*                                                               */
*   /* DETERMINE LINK PACK AREA ADDRESS FOR RELOCATING VIRTUAL DUMP  */
*   /* ON MEMORY SWITCHES. OBTAIN CVT AND CHECK IF OVERLAID BY       */
*   /* VIRTUAL DUMP CODE.                                            */
*   /*                                                               */
*   /*****************************************************************/
*/* N DETERMINE LPA ADDRESS FOR RELOCATION                           */
*/* P OBTAIN CVT ADDRESS FROM FLCCVT2 (X'4C')                        */
*/* P PLACE CVT ADDRESS IN FLCCVT (X'10')                            */
*/* D (YES,,NO,%PGECVTI) CVT ADDR < COMPUTED VALUE                   */
*                                                               01S0337
*   CVTPTR = FLCCVT2;               /* GET ADDRESS OF CVT            */
         L     CVTPTR,FLCCVT2                                   01S0337
*   FLCCVT = CVTPTR;                /* REFRESH CVT POINTER AT FLCCVT */
         ST    CVTPTR,FLCCVT                                    01S0338
*   IF CVTPTR < CODEENDS THEN       /* WAS CVT OVERLAID              */
         C     CVTPTR,CODEENDS                                  01S0339
         BNL   @RF00339                                         01S0339
*     DO;                           /* YES - BRING IT IN FROM   01S0340
*                                      WORKFILE                      */
*                                                               01S0341
*   /*****************************************************************/
*   /*                                                               */
*   /* CVT OVERLAID BY CODE - READ IT INTO THE FIRST BUFFER          */
*   /*                                                               */
*   /*****************************************************************/
*/*%PGECVTO: N CVT OVERLAID BY SADMP CODE - BRING INTO FIRST BUFFER  */
*/* P SET BCTVADDR                                                   */
*/* P SET BCTALLOC                                                   */
*/* P SET IODBBCT TO ADDR OF BCT                                     */
*/* P SET IODBCCHH TO VBN OF CVT                                     */
*/* P SET CTIOTYPE TO CTIOWK                                         */
*/* L  AMDSASIO:  READ IN CVT                                        */
*/* D (NO,%PGECVTE,YES,) RC  =  0                                    */
*/* L  AMDSAWAT:  WAIT FOR I/O COMPLETION                            */
*/* D (NO,%PGECVTE,YES,) RC  =  0                                    */
*/* L  AMDSAUPD:  UPDATE I/O QUEUE                                   */
*/* D (YES,,NO,%PGECVTE) RC  =  0                                    */
*                                                               01S0341
*       BCTPTR = BCTHFRST;          /* POINT TO FIRST BCT            */
         L     @04,BCTHFRST                                     01S0341
         ST    @04,BCTPTR                                       01S0341
*       RESPECIFY BCT BASED(BCTPTR);                            01S0342
*       RESPECIFY IODB BASED(ADDR(IODBWORK));                   01S0343
*       BCTALLOC = ON;              /* INDICATE BCT ALLOCATED FOR I/O*/
         OI    BCTALLOC(@04),B'01000000'                        01S0344
*       BCTVADDR = CVTPTR & PAGERND; /* GET PAGE NUMBER FOR CVT      */
         LR    @15,CVTPTR                                       01S0345
         N     @15,@CF01816                                     01S0345
         ST    @15,BCTVADDR(,@04)                               01S0345
*       IODBR = BCTVADDR / PAGESIZE; /* GET VBN OF CVT               */
         LA    @14,IODBWORK                                     01S0346
         SRL   @15,12                                           01S0346
         STC   @15,IODBR(,@14)                                  01S0346
*       IODBBCT = BCTPTR;           /* QUEUE BCT TO WORKFILE IODB FOR
*                                      I/O                           */
         ST    @04,IODBBCT(,@14)                                01S0347
*       CTIOWK = ON;                /* REQUEST I/O TO WORKFILE       */
         LH    @04,@CH01539                                     01S0348
         OI    CTIOWK(@04),B'00010000'                          01S0348
*       SVC(SIOSVC);                /* CALL AMDSASIO TO INITIATE I/O */
         SVC   0                                                01S0349
*       RESPECIFY RETCODE RESTRICTED;                           01S0350
*       IF RETCODE ^= 0 THEN        /* I/O INITIATED SUCCESSFULLY    */
         LTR   RETCODE,RETCODE                                  01S0351
         BNZ   @RT00351                                         01S0351
*         GO TO CVTINVLD;           /* NO - CVT UNAVAILABLE          */
*       SVC(WATSVC);                /* CALL AMDSAWAT TO WAIT FOR I/O
*                                      TO COMPLETE                   */
         SVC   1                                                01S0353
*       IF RETCODE ^= 0 THEN        /* ERROR READING CVT             */
         LTR   RETCODE,RETCODE                                  01S0354
         BNZ   @RT00354                                         01S0354
*         GO TO CVTINVLD;           /* YES - CVT UNAVAILABLE         */
*       SVC(UPDSVC);                /* CALL AMDSAUPD TO UPDATE I/O
*                                      REQUEST QUEUE                 */
         SVC   9                                                01S0356
*       IF RETCODE ^= 0 THEN        /* ERROR UPDATING QUEUE          */
         LTR   RETCODE,RETCODE                                  01S0357
         BNZ   @RT00357                                         01S0357
