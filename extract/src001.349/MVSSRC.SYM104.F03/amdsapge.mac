         LH    I,BINBINLN(,R1)                                  01S0454
         B     @DE00454                                         01S0454
@DL00454 DS    0H                                               01S0455
*     BINLOOP = OFF;                /* FIRST HALF OF BYTE PROCESSED  */
         NI    BINLOOP,B'01111111'                              01S0455
*     BINWORK = BINICHAR / 16;      /* GET FIRST HALF OF BYTE        */
*                                                               01S0456
         SLR   @14,@14                                          01S0456
         IC    @14,BINICHAR(,BINIPTR)                           01S0456
         SRL   @14,4                                            01S0456
         STC   @14,BINWORK                                      01S0456
*CONLP2:                                                        01S0457
*     IF BINWORK > MAXNUM THEN      /* IS IT A NUMERIC               */
CONLP2   CLI   BINWORK,9                                        01S0457
         BNH   @RF00457                                         01S0457
