         TITLE 'AMDSAAMR   ASM/VSAM CONVERSION ROUTINE                 *
                       '
AMDSAAMR CSECT ,                                                01S0001
@MAINENT DS    0H                                               01S0001
         USING *,@15                                            01S0001
         B     @PROLOG                                          01S0001
         DC    AL1(16)                                          01S0001
         DC    C'AMDSAAMR  73.353'                              01S0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                  01S0001
         BALR  @11,0                                            01S0001
@PSTART  DS    0H                                               01S0001
         USING @PSTART,@11                                      01S0001
*   RAIE = R1;                      /* SAVE AIE ADDRESS              */
         LR    RAIE,R1                                          01S0233
*   SVC(GTMSVC);                    /* CALL AMDSAGTM FOR STORAGE     */
         SVC   4                                                01S0234
*   IF RETCODE ^= 0 THEN            /* STORAGE OBTAINED              */
         LTR   RETCODE,RETCODE                                  01S0235
         BZ    @RF00235                                         01S0235
*     DO;                                                       01S0236
*        GENERATE REFS(R13,RETREG,RETCODE) SETS(R14,R12);       01S0237
         LM    R14,R12,12(R13)         RESTORE REGISTERS
         LA    RETCODE,4               SET ERROR RETURN CODE
         BR    RETREG                  RETURN TO CALLER
*     END;                                                      01S0238
*   R8 = R1;                        /* GET ADDRESS OF WORK AREA      */
@RF00235 LR    R8,R1                                            01S0239
*   GENERATE REFS(R8,R13) SETS(R15);                            01S0240
         USING @DATD,R8                MAKE WORK AREA ADDRESSABLE
         ST    R13,@SA00001+4          SAVE ADDR OF CALLERS SAVE AREA
         LA    R15,@SA00001            POINT TO SAVE AREA
*   SAVENEXT = R15;                 /* PLACE ADDRESS OF SAVE AREA IN
*                                      CALLER'S AREA                 */
         ST    R15,SAVENEXT(,R13)                               01S0241
*   R13 = R15;                      /* POINT TO SAVE AREA            */
         LR    R13,R15                                          01S0242
*   RESPECIFY (R1,RETCODE) UNRESTRICTED;                        01S0243
*   SAVENAME = 'AMR';               /* PLACE MODULE ID IN SAVE AREA  */
         MVC   SAVENAME(3,R13),@CC01734                         01S0244
*   AMRBUFT = CTBUFTYP;             /* SAVE BUFFER REQUEST FLAGS     */
         L     @09,VCTCCTV(,VCCTREG)                            01S0245
         SR    AMRBUFT,AMRBUFT                                  01S0245
         IC    AMRBUFT,CTBUFTYP(,@09)                           01S0245
*   CTBUFTYP = OFF;                 /* RESET BUFFER REQUEST FLAGS    */
         MVI   CTBUFTYP(@09),B'00000000'                        01S0246
*   CTASM = ON;                     /* REQUEST ASM BUFFERS           */
         OI    CTASM(@09),B'00010000'                           01S0247
*   RESPECIFY RCB BASED(ADDR(AMRRCB));                          01S0248
*   RCBNEXT = VCTRCB;               /* GET RCB CHAIN ADDRESS         */
         LA    @09,AMRRCB                                       01S0249
         MVC   RCBNEXT(4,@09),VCTRCB(VCCTREG)                   01S0249
*   RCBEXIT = ADDR(AMREXIT);        /* POINT TO EXIT ADDRESS         */
         LA    @05,AMREXIT                                      01S0250
         ST    @05,RCBEXIT(,@09)                                01S0250
*   RCBBASE = BASEREG;              /* GET BASE REGISTER VALUE       */
         ST    BASEREG,RCBBASE(,@09)                            01S0251
*   RCBSAVE = R13;                  /* SAVE AREA ADDRESS IN RCB      */
         ST    R13,RCBSAVE(,@09)                                01S0252
*   GENERATE REFS(R1) SETS(AMRWORK) (STCTL R1,R1,AMRWORK); /* GET
*                                      SEGMENT TABLE ADDRESS         */
         STCTL R1,R1,AMRWORK
*   RCBSTOR = AMRWORK;              /* SEGMENT TABLE ADDRESS TO RCB  */
         LA    @09,AMRRCB                                       01S0254
         MVC   RCBSTOR(4,@09),AMRWORK                           01S0254
*   VCTRCB = ADDR(AMRRCB);          /* MAKE RCB ACTIVE               */
         ST    @09,VCTRCB(,VCCTREG)                             01S0255
*                                                               01S0256
*   /*****************************************************************/
*   /*                                                               */
*   /* VALIDITY CHECK THE ASPCT POINTED TO BY THE LGVT ENTRY.        */
*   /* THE CHECK MUST BE MADE IN THE ASM MEMORY SINCE THE ASPCT IS   */
*   /* NOT IN ANY COMMON AREA.                                       */
*   /*                                                               */
*   /*****************************************************************/
*/* P SET VCTCBADR TO LGVENASP                                       */
*/* P SET VCTCBID TO ASPCT ID                                        */
*/* L  AMDSAVCK:  VALIDITY CHECK ASPCT                               */
*/* D (YES,,NO,AMREXIT) RC  =  0                                     */
*                                                               01S0256
*   RESPECIFY AIE BASED(RAIE);                                  01S0256
*   RESPECIFY AIA BASED(RAIA);                                  01S0257
*   RESPECIFY RETCODE RESTRICTED;                               01S0258
*   VCTCBADR = LGVENASP;            /* GET ASPCT ADDR FROM LGVT ENTRY*/
         L     @14,LGVTEPTR(,RAIE)                              01S0259
         MVC   VCTCBADR(4,VCCTREG),LGVENASP(@14)                01S0259
*   VCTCBIDN = VASPCTID;            /* SET ASPCT CONTROL BLOCK ID    */
         MVI   VCTCBIDN(VCCTREG),X'13'                          01S0260
*   SVC (VCKSVC);                   /* CALL AMDSAVCK TO CHECK ASPCT  */
         SVC   25                                               01S0261
*   IF RETCODE ^= 0 THEN            /* ASPCT VALID                   */
         LTR   RETCODE,RETCODE                                  01S0262
         BNZ   @RT00262                                         01S0262
*     GO TO AMREXIT;                /* NO - TAKE ERROR PATH          */
*                                                               01S0263
*   /*****************************************************************/
*   /*                                                               */
*   /* CALL AALSUB SUBROUTINE TO CONVERT LGVT ENTRY ADDRESS TO LPME  */
*   /* ADDRESSES.                                                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0264
*/* P SET ASM IN CONTROL FLAG                                        */
*/* S AALSUB: OBTAIN LPME ADDRESSES                                  */
*/* P RESET ASM IN CONTROL FLAG                                      */
*/* D (YES,,NO,AMREXIT) RC  =  0                                     */
*/* P OBTAIN PART ENTRY USING PRIMARY LPME                           */
*/* P GET DIRECT ACCESS IODB                                         */
*/* P GET BCT ADDRESS FROM IODBBCT                                   */
*/* P PLACE AMB ADDRESS FROM PART ENTRY IN BCTAMB                    */
*/* P PLACE RBA FROM LPME IN BCTRBA                                  */
*   VCTASMIC = ON;                  /* INDICATE ASM IN CONTROL       */
         OI    VCTASMIC(VCCTREG),B'00100000'                    01S0264
*   CALL AALSUB;                    /* CONVERT LGVT ENTRY TO LPME
*                                      ADDRESSES                     */
         BAL   @14,AALSUB                                       01S0265
*   RESPECIFY (R1,RETCODE) RESTRICTED;                          01S0266
*   VCTASMIC = OFF;                 /* ASM NOT IN CONTROL            */
         NI    VCTASMIC(VCCTREG),B'11011111'                    01S0267
*   IF RETCODE ^= 0 THEN            /* CONVERSION SUCCESSFUL         */
         LTR   RETCODE,RETCODE                                  01S0268
         BNZ   @RT00268                                         01S0268
*     GO TO AMREXIT;                /* NO - TAKE ERROR EXIT          */
*   AIEPRME = RALSECTP + RALPMEDX * LENGTH(ASPLPME); /* COMPUTE THE
*                                      PRIMARY LPME ADDRESS USING THE
*                                      VALUES RETURNED FROM AALSUB   */
         L     @14,RALPMEDX(,R1)                                01S0270
         SLA   @14,2                                            01S0270
         A     @14,RALSECTP(,R1)                                01S0270
         ST    @14,AIEPRME(,RAIE)                               01S0270
*   PAREPTR = ADDR(PARTENTS(ASPPRTNN+1)); /* COMPUTE PART ENTRY ADDR */
         L     @14,LPMEPTR(,RAIE)                               01S0271
         SR    @09,@09                                          01S0271
         IC    @09,ASPPRTNN(,@14)                               01S0271
         SLA   @09,6                                            01S0271
         L     @05,CVTPTR                                       01S0271
         L     @05,ASMVTPTR(,@05)                               01S0271
         L     PAREPTR,PARTPTR(,@05)                            01S0271
         LA    PAREPTR,PARTENTS(@09,PAREPTR)                    01S0271
*   BCTAMB = PAREAMBP;              /* GET AMB ADDR FROM PART ENTRY  */
         L     @09,@CA01700                                     01S0272
         L     @05,VCTLPA(,VCCTREG)                             01S0272
         AR    @09,@05                                          01S0272
         A     @05,IODBBCT(,@09)                                01S0272
         L     @09,PAREAMBP(,PAREPTR)                           01S0272
         ST    @09,BCTAMB(,@05)                                 01S0272
*   BCTRBA = ASPRSLOT;              /* GET RBA FROM LPME             */
         SR    @00,@00                                          01S0273
         ICM   @00,3,ASPRSLOT(@14)                              01S0273
         ST    @00,BCTRBA(,@05)                                 01S0273
*   RESPECIFY (R1,RETCODE) UNRESTRICTED;                        01S0274
*                                                               01S0275
*   /*****************************************************************/
*   /*                                                               */
*   /* OBTAIN AMB ADDRESS AND VALIDITY CHECK THE AMB                 */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0275
*/* P GET AMB ADDRESS FROM BCTAMB                                    */
*/* P SET VCTCBADR TO AMB ADDRESS                                    */
*/* P SET VCTCBID TO AMB ID                                          */
*/* L  AMDSAVCK:  VALIDITY CHECK AMB                                 */
*/* D (YES,,NO,AMREXIT) RC  =  0                                     */
*   VCTCBADR = BCTAMB;              /* ADDRESS OF AMB FOR VALIDITY
*                                      CHECK                         */
         ST    @09,VCTCBADR(,VCCTREG)                           01S0275
*   VCTCBIDN = VAMBID;              /* AMB CONTROL BLOCK ID          */
         MVI   VCTCBIDN(VCCTREG),X'01'                          01S0276
*   SVC(VCKSVC);                    /* CALL AMDSAVCK TO CHECK AMB    */
         SVC   25                                               01S0277
*   RESPECIFY RETCODE RESTRICTED;                               01S0278
*   IF RETCODE ^= 0 THEN            /* AMB VALID                     */
         LTR   RETCODE,RETCODE                                  01S0279
         BNZ   @RT00279                                         01S0279
*     GO TO AMREXIT;                /* NO - TAKE ERROR EXIT          */
*                                                               01S0280
*   /*****************************************************************/
*   /*                                                               */
*   /* GET EDB HEADER ADDRESS FROM AMB AND VALIDITY CHECK THE EDB    */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0281
*/* P OBTAIN EDB HEADER ADDRESS FROM AMBEDB                          */
*/* P SET VCTCBADR TO AMBEDB                                         */
*/* P SET VCTCBID TO EDB HEADER ID                                   */
*/* L  AMDSAVCK:  VALIDITY CHECK EDB HEADER                          */
*/* D (YES,,NO,AMREXIT) RC  =  0                                     */
*/* D (NO,,YES,AMREXIT) EDBNO = 0                                    */
*   RESPECIFY IDAAMB BASED(BCTAMB);                             01S0281
*   RESPECIFY IDAEDBHD BASED(AMBEDB);                           01S0282
*   VCTCBADR = AMBEDB;              /* ADDRESS OF EDB HEADER FOR
*                                      VALIDITY CHECK                */
         L     @14,@CA01700                                     01S0283
         L     @09,VCTLPA(,VCCTREG)                             01S0283
         AR    @14,@09                                          01S0283
         A     @09,IODBBCT(,@14)                                01S0283
         L     @14,BCTAMB(,@09)                                 01S0283
         MVC   VCTCBADR(4,VCCTREG),AMBEDB(@14)                  01S0283
*   VCTCBIDN = VEDBID;              /* EDB HEADER CONTROL BLOCK ID   */
         MVI   VCTCBIDN(VCCTREG),X'06'                          01S0284
*   SVC(VCKSVC);                    /* CALL AMDSAVCK TO CHECK EDB
*                                      HEADER                        */
         SVC   25                                               01S0285
*   IF RETCODE ^= 0 × EDBNO = 0 THEN /* EDB HEADER VALID OR CONTAIN
*                                      ANY ENTRIES                   */
         LTR   RETCODE,RETCODE                                  01S0286
         BNZ   @RT00286                                         01S0286
         L     @14,@CA01700                                     01S0286
         L     @09,VCTLPA(,VCCTREG)                             01S0286
         AR    @14,@09                                          01S0286
         A     @09,IODBBCT(,@14)                                01S0286
         L     @14,BCTAMB(,@09)                                 01S0286
         L     @14,AMBEDB(,@14)                                 01S0286
         CLI   EDBNO(@14),0                                     01S0286
         BE    @RT00286                                         01S0286
*     GO TO AMREXIT;                /* NO - TAKE ERROR EXIT          */
*   RESPECIFY RETCODE UNRESTRICTED;                             01S0288
*                                                               01S0289
*   /*****************************************************************/
*   /*                                                               */
*   /* EDB HEADER IS VALID. SAVE NUMBER AND LENGTH OF EDB ENTRIES AND*/
*   /* SEARCH FOR AN EDB WHICH CONTAINS THE REQUESTED RBA.           */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0289
*/* P GET RBA FROM BCTRBA                                            */
*/* P GET FIRST EDB                                                  */
*/*AMREDBS: D (NO,,YES,AMREDBN) RBA < EDBLORBA                       */
*/* D (NO,AMREDBF,YES,AMREDBN) RBA > EDBHIRBA                        */
*/*AMREDBN: P DECREMENT EDB COUNT                                    */
*/* D (NO,,YES,AMREXIT) LAST EDB                                     */
*/* P (,AMREDBS) GET NEXT EDB                                        */
*   RESPECIFY CONVOUT BASED(RPLH);                              01S0289
*   RESPECIFY RPLH RESTRICTED;                                  01S0290
*   RPLH = ADDR(CNVRTOUT);          /* SET UP DUMMY PLH POINTER      */
         LA    RPLH,CNVRTOUT                                    01S0291
*   CONBB = EDBNO;                  /* SAVE NUMBER OF EDB ENTRIES    */
         SR    @05,@05                                          01S0292
         IC    @05,EDBNO(,@14)                                  01S0292
         STH   @05,CONBB(,RPLH)                                 01S0292
*   CONCC = EDBLEN;                 /* SAVE LENGTH OF EDB ENTRIES    */
         MVC   CONCC(2,RPLH),EDBLEN(@14)                        01S0293
*   RESPECIFY IDAEDB BASED(RWORK2);                             01S0294
*   RESPECIFY (RWORK0,RWORK2) RESTRICTED;                       01S0295
*   RWORK2 = AMBEDB + LENGTH(IDAEDBHD); /* POINT TO FIRST EDB ENTRY  */
         AH    @14,@CH00060                                     01S0296
         LR    RWORK2,@14                                       01S0296
*   RWORK0 = BCTRBA * PAGESIZE;     /* GET REQUESTED RBA             */
         L     RWORK0,BCTRBA(,@09)                              01S0297
         SLA   RWORK0,12                                        01S0297
*   DO I = 1 TO CONBB;              /* SEARCH ALL EDB ENTRIES        */
         LA    I,1                                              01S0298
         B     @DE00298                                         01S0298
@DL00298 DS    0H                                               01S0299
*     IF (RWORK0 >= EDBLORBA) & (RWORK0 <= EDBHIRBA) THEN /* IS RBA
*                                      CONTAINED IN RANGE COVERED BY
*                                      THIS EDB                      */
         CL    RWORK0,EDBLORBA(,RWORK2)                         01S0299
         BL    @RF00299                                         01S0299
         CL    RWORK0,EDBHIRBA(,RWORK2)                         01S0299
         BNH   @RT00299                                         01S0299
*       GO TO AMREDBF;              /* YES - PROPER EDB FOUND        */
*     RWORK2 = RWORK2 + CONCC;      /* POINT TO NEXT EDB             */
@RF00299 SR    @09,@09                                          01S0301
         ICM   @09,3,CONCC(RPLH)                                01S0301
         ALR   RWORK2,@09                                       01S0301
*   END;                            /* CONTINUE EDB SEARCH           */
         AH    I,@CH00062                                       01S0302
@DE00298 SR    @09,@09                                          01S0302
         ICM   @09,3,CONBB(RPLH)                                01S0302
         CR    I,@09                                            01S0302
         BNH   @DL00298                                         01S0302
*   GO TO AMREXIT;                  /* NO EDB CONTAINS REQUESTED RBA */
         B     AMREXIT                                          01S0303
*                                                               01S0304
*   /*****************************************************************/
*   /*                                                               */
*   /* EDB FOR REQUESTED RBA FOUND. CHECK THE LPMB IT POINTS TO FOR  */
*   /* VALIDITY.                                                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0304
*/*AMREDBF: P GET LPMB ADDRESS FROM EDBLPMBA                         */
*/* P SET VCTCBADR TO LPMB ADDRESS                                   */
*/* P SET VCTCBID TO LPMB ID                                         */
*/* L  AMDSAVCK:  VALIDITY CHECK LPMB                                */
*/* D (YES,,NO,AMREXIT) RC  =  0                                     */
*                                                               01S0304
*AMREDBF:                                                       01S0304
*   VCTCBADR = EDBLPMBA;            /* LPMB ADDRESS FOR VALIDITY
*                                      CHECK                         */
AMREDBF  MVC   VCTCBADR(4,VCCTREG),EDBLPMBA(RWORK2)             01S0304
*   VCTCBIDN = VLPMBID;             /* LPMB CONTROL BLOCK ID         */
         MVI   VCTCBIDN(VCCTREG),X'0F'                          01S0305
*   SVC(VCKSVC);                    /* CALL AMDSAVCK TO CHECK LPMB   */
         SVC   25                                               01S0306
*   RESPECIFY RETCODE RESTRICTED;                               01S0307
*   IF RETCODE ^= 0 THEN            /* LPMB VALID                    */
         LTR   RETCODE,RETCODE                                  01S0308
         BNZ   @RT00308                                         01S0308
*     GO TO AMREXIT;                /* NO - TAKE ERROR EXIT          */
*   RESPECIFY RETCODE UNRESTRICTED;                             01S0310
*                                                               01S0311
*   /*****************************************************************/
*   /*                                                               */
*   /* LPMB VALID. SET UP INTERFACE TO VSAM CONVERT ROUTINE AND CALL */
*   /* IT TO CONVERT THE RBA TO AN MBBCCHHR.                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0311
*/* P SET UP INTERFACE TO VSAM CONVERT ROUTINE                       */
*/* S CONVERT: CONVERT RBA TO MBBCCHHR                               */
*/* N RETURN CODE FROM CONVERT IS IN 'BB' IN WORK AREA               */
*/* D (YES,,NO,AMREXIT) RC  =  0                                     */
*   RESPECIFY (RAMB,RPLH) RESTRICTED;                           01S0311
*   RAMB = BCTAMB;                  /* GET AMB ADDRESS               */
         L     @09,@CA01700                                     01S0312
         L     @07,VCTLPA(,VCCTREG)                             01S0312
         AR    @09,@07                                          01S0312
         A     @07,IODBBCT(,@09)                                01S0312
         L     RAMB,BCTAMB(,@07)                                01S0312
*   CALL CONVERT;                   /* CALL VSAM CONVERT ROUTINE     */
         BAL   @14,CONVERT                                      01S0313
*   IF CONBB ^= '0000'X THEN        /* CONVERT SUCCESSFUL            */
         CLC   CONBB(2,RPLH),@CB01748                           01S0314
         BNE   @RT00314                                         01S0314
*     GO TO AMREXIT;                /* NO - TAKE ERROR EXIT          */
*   RESPECIFY (RWORK0,RWORK2,RAMB) UNRESTRICTED;                01S0316
*                                                               01S0317
*   /*****************************************************************/
*   /*                                                               */
*   /* MBBCCHHR OBTAINED SUCCESSFULLY. GET CPU ADDRESS, AND DEVICE   */
*   /* ADDRESS AND ATTRIBUTES.                                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0317
*/* P SET UP EXTENT NUMBER FOR AMDSAUCB                              */
*/* L  AMDSAUCB:  GET CPU ADDR, DEVICE ADDR AND ATTRIBUTES           */
*/* D (YES,,NO,AMREXIT) RC  =  0                                     */
*   IODBCCHH = CONVOUT;             /* MOVE MBBCCHHR TO IODB         */
         L     @09,@CA01700                                     01S0317
         A     @09,VCTLPA(,VCCTREG)                             01S0317
         MVC   IODBCCHH(8,@09),CONVOUT(RPLH)                    01S0317
*   SVC(UCBSVC);                    /* CALL AMDSAUCB TO GET ADDRESS
*                                      AND ATTRIBUTES                */
         SVC   24                                               01S0318
*   RESPECIFY RETCODE RESTRICTED;                               01S0319
*   IF RETCODE ^= 0 THEN            /* PROCESSING SUCCESSFUL         */
         SR    @14,@14                                          01S0320
         CR    RETCODE,@14                                      01S0320
         BNE   @RT00320                                         01S0320
*     GO TO AMREXIT;                /* NO - TAKE ERROR EXIT          */
*   RESPECIFY RETCODE UNRESTRICTED;                             01S0322
*                                                               01S0322
*   /*****************************************************************/
*   /*                                                               */
*   /* RE-INITIALIZE IODB WITH MBBCCHHR IN CASE OF PAGE FAULT IN     */
*   /* AMDSAUCB. SET UP RETURN CODE OF 0.                            */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0323
*/* P MOVE MBBCCHHR TO D.A. IODBCCHH FROM WORK AREA                  */
*/* P SET UP RETURN CODE OF 0                                        */
*   IODBCCHH = CONVOUT;             /* MOVE MBBCCHHR TO IODB         */
         L     @09,@CA01700                                     01S0323
         A     @09,VCTLPA(,VCCTREG)                             01S0323
         MVC   IODBCCHH(8,@09),CONVOUT(RPLH)                    01S0323
*   SAVEBACK -> SAVER15 = 0;        /* INDICATE SUCCESSFUL PROCESSING*/
         L     @09,SAVEBACK(,R13)                               01S0324
         ST    @14,SAVER15(,@09)                                01S0324
*   RESPECIFY RPLH UNRESTRICTED;                                01S0325
*                                                               01S0326
*   /*****************************************************************/
*   /*                                                               */
*   /* DEQUEUE RCB, FREE SAVE AREA, AND RETURN TO CALLER.            */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0326
*/*AMRRET: P DEQUEUE RCB                                             */
*/* P RESET ASM IN CONTROL FLAGS                                     */
*/* P RESTORE ORIGINAL BUFFER REQUEST FLAGS                          */
*/* L  AMDSAFRM:  FREE SAVE/WORK AREA                                */
*/*AMRRETG: P RESTORE REGISTERS                                      */
*/* R RETURN TO CALLER                                               */
*                                                               01S0326
*AMRRET:                                                        01S0326
*   VCTRCB = RCBNEXT;               /* DEQUEUE RCB                   */
AMRRET   LA    @09,AMRRCB                                       01S0326
         MVC   VCTRCB(4,VCCTREG),RCBNEXT(@09)                   01S0326
*   VCTASMIC = OFF;                 /* RESET ASM IN CONTROL FLAGS    */
         NI    VCTASMIC(VCCTREG),B'11011111'                    01S0327
*   CTBUFTYP = AMRBUFT;             /* RESTORE BUFFER REQUEST FLAGS  */
         L     @09,VCTCCTV(,VCCTREG)                            01S0328
         STC   AMRBUFT,CTBUFTYP(,@09)                           01S0328
*   RESPECIFY R1 RESTRICTED;                                    01S0329
*   R1 = R8;                        /* POINT TO AREA TO BE FREED     */
         LR    R1,R8                                            01S0330
*   SVC(FRMSVC);                    /* CALL AMDSAFRM TO FREE SAVE
*                                      AREA                          */
         SVC   5                                                01S0331
*   RETURN;                         /* RETURN TO CALLER              */
*                                                               01S0332
@EL00001 L     @13,4(,@13)                                      01S0332
@EF00001 DS    0H                                               01S0332
@ER00001 LM    @14,@12,12(@13)                                  01S0332
         BR    @14                                              01S0332
*   /*****************************************************************/
*   /*                                                               */
*   /* RCB EXIT - SET UP RETURN CODE OF 4 AND TAKE RETURN PATH       */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0333
*/*AMREXIT: P (,AMRRET) SET UP RETURN CODE OF 4                      */
*                                                               01S0333
*AMREXIT:                                                       01S0333
*   SAVEBACK -> SAVER15 = 4;        /* INDICATE ERROR DURING    01S0333
*                                      PROCESSING                    */
AMREXIT  L     @09,SAVEBACK(,R13)                               01S0333
         MVC   SAVER15(4,@09),@CF00054                          01S0333
*   GO TO AMRRET;                   /* TAKE RETURN PATH              */
         B     AMRRET                                           01S0334
*   /*****************************************************************/
*   /*                                                               */
*   /* AALSUB SUBROUTINE - THIS SUBROUTINE PERFORMS THE AUXILIARY    */
*   /* STORAGE MANAGEMENT (ASM) FUNCTION OF CONVERTING AN LGVT ENTRY */
*   /* ADDRESS TO AN LPME INDEX VALUE AND SECTOR POINTER. THESE      */
*   /* MAY THEN BE USED TO COMPUTE THE LPME ADDRESS(ES) CORRESPONDING*/
*   /* TO THE REQUESTED LPID.                                        */
*   /*                                                               */
*   /* INPUT - ASPCT ADDRESS IN ASPCTPTR                             */
*   /*         AIE ADDRESS IN RAIE                                   */
*   /*                                                               */
*   /* OUTPUT - RETURN CODE IN REGISTER 15.                          */
*   /*            00 - NO ERRORS, ADDRESS OF PARAMETER IN REGISTER 1.*/
*   /*            60 - RPN OUT OF ADDRESS SPACE                      */
*   /*            64 - LPME SECTOR NOT ALLOCATED                     */
*   /*          OUTPUT PARAMETER CONSISTS OF THE LPME INDEX AND THE  */
*   /*          SECTOR ADDRESS.                                      */
*   /*                                                               */
*   /* EXIT-NORMAL - RETURN TO CALLER WITH RETURN CODE               */
*   /*                                                               */
*   /* EXIT-ERROR - NONE                                             */
*   /*                                                               */
*   /* EXTERNAL-REFERENCES - NONE                                    */
*   /*                                                               */
*   /* MAPPING-MACROS-USED - ILRAIE,ILRAIA,ILRASPCT                  */
*   /*                                                               */
*   /* REGISTERS-MODIFIED - 1,15                                     */
*   /*                                                               */
*   /* NOTES - THE CALLING MODULE MUST INCLUDE THE MAPPING MACROS    */
*   /*         LISTED ABOVE. A BASE POINTER FOR THE ASPCT MUST BE    */
*   /*         SET BY THE CALLING ROUTINE. ALL LABELS AND DATA AREAS */
*   /*         CREATED BY THIS SUBROUTINE BEGIN WITH 'AAL'. THIS     */
*   /*         ROUTINE ALSO REQUIRES THAT THE CALLING MODULE         */
*   /*         DECLARES REGISTERS R0-R15 AND POINTERS RAIE AND RAIA. */
*   /*         THE AIE ADDRESS MUST BE IN RAIE.                      */
*   /*                                                               */
*   /*****************************************************************/
*/*AALSUB: CHART                                                     */
*/*HEADER                                                       02S0335
*/*AALSUB        LGVT ENTRY TO LPME CONVERT SUBROUTINE          02S0335
*/*                                               PAGE #             */
*/*AALSUB: E AALSUB                                                  */
*/* P GET AIA ADDRESS FROM AIE                                       */
*/* D (YES,,NO,%AALRPN) RPN > MAX. RPN                               */
*/*%AALR60: P (,%AALRET) SET RETURN CODE 60                          */
*/*%AALRPN: P COMPUTE ASST INDEX                                     */
*/* P COMPUTE LPME INDEX                                             */
*/* D (YES,%AALR60,NO,) RPN > 16 MEG                                 */
*/* P INDICATE BASE ASPCT                                            */
*/* D (NO,,YES,%AALR60) EXTENSION ASPCT NEEDED                       */
*/* P COMPUTE ASST POINTER USING INDEX                               */
*/* D (NO,,YES,%AALSTE) LPME SECTOR ALLOCATED                        */
*/* P (,%AALRET) SET RETURN CODE 64                                  */
*/*%AALSTE: P GET LPME SECTOR ADDRESS                                */
*/* P SET RETURN CODE 0                                              */
*/*%AALRET: R RETURN TO CALLER                                       */
*/*AALSUB: END                                                       */
*                                                               02S0335
*AALSUB:                                                        02S0335
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                            02S0335
AALSUB   DS    0H                                               02S0336
*   DCL                                                         02S0336
*         AAL0 CONSTANT(0);                                     02S0336
*   DCL                                                         02S0337
*         AAL32 CONSTANT(32);                                   02S0337
*   DCL                                                         02S0338
*         AAL60 CONSTANT(60);                                   02S0338
*   DCL                                                         02S0339
*         AAL64 CONSTANT(64);                                   02S0339
*   DCL                                                         02S0340
*         AAL128 CONSTANT(128);                                 02S0340
*   DCL                                                         02S0341
*         AALONE CONSTANT(1);                                   02S0341
*   DCL                                                         02S0342
*         AALASSTX PTR(31);                                     02S0342
*   DCL                                                         02S0343
*         AALEXTDX PTR(31);                                     02S0343
*   DCL                                                         02S0344
*         AALASSTP PTR(31);                                     02S0344
*   DCL                                                         02S0345
*         AALASSTE PTR(31) BASED;                               02S0345
*   DCL                                                         02S0346
*         1 AALRPARM,               /* RETURN PARAMETER              */
*             3 AALPMEDX PTR(31),   /* LPME INDEX VALUE              */
*             3 AALSECTP PTR(31);   /* SECTOR POINTER                */
*   RESPECIFY (R14,R15) RESTRICTED;                             02S0347
*   RAIA = RAIE -> AIEAIAP;         /* GET AIA ADDR FROM AIE         */
         L     RAIA,AIEAIAP(,RAIE)                              02S0348
*   IF RAIA -> AIARPN > ASPMAXPN THEN /* REQUESTED RPN > MAXIMUM RPN */
         L     @09,LGVTEPTR(,RAIE)                              02S0349
         L     @09,ASPCTPTR(,@09)                               02S0349
         CLC   AIARPN(4,RAIA),ASPMAXPN(@09)                     02S0349
         BNH   @RF00349                                         02S0349
*     R15 = AAL60;                  /* YES - RPN OUT OF ADDRESS SPACE*/
         LA    R15,60                                           02S0350
*   ELSE                            /* NO -                          */
*     DO;                                                       02S0351
         B     @RC00349                                         02S0351
@RF00349 DS    0H                                               02S0352
*       AALASSTX = (RAIA -> AIARPN / AAL64) + AALONE; /* ASST INDEX =
*                                      HIGH ORDER 27 BITS OF RPN     */
         L     @09,AIARPN(,RAIA)                                02S0352
         LR    AALASSTX,@09                                     02S0352
         SRL   AALASSTX,6                                       02S0352
         AL    AALASSTX,@CF00062                                02S0352
*       AALPMEDX = RAIA -> AIARPN & '0000003F'X; /* LPME INDEX =
*                                      LOW ORDER 5 BITS OF RPN       */
         N     @09,@CF01770                                     02S0353
         ST    @09,AALPMEDX                                     02S0353
*       IF AALASSTX > AAL64  THEN   /* RPN FOR ADDRESS > 16 MEG      */
         CH    AALASSTX,@CH00801                                02S0354
         BNH   @RF00354                                         02S0354
*         R15 = AAL60;              /* YES - OUT OF ADDRESS SPACE    */
         LA    R15,60                                           02S0355
*       ELSE                                                    02S0356
*         DO;                                                   02S0356
         B     @RC00354                                         02S0356
@RF00354 DS    0H                                               02S0357
*           AALEXTDX = AAL0;        /* INDICATE BASE ASPCT           */
         SR    @09,@09                                          02S0357
         LR    AALEXTDX,@09                                     02S0357
*           IF ASPAXPGS => AALEXTDX THEN /* ANY ASST EXTENSIONS      */
         L     @05,LGVTEPTR(,RAIE)                              02S0358
         L     @05,ASPCTPTR(,@05)                               02S0358
         CH    AALEXTDX,ASPAXPGS(,@05)                          02S0358
         BH    @RF00358                                         02S0358
*             DO;                                               02S0359
*                 AALASSTP = ADDR(ASPASSTE(AALASSTX)); /* SET UP ASST
*                                      ADDRESS                       */
         LR    @07,AALASSTX                                     02S0360
         SLA   @07,2                                            02S0360
         LA    AALASSTP,ASPASSTE-4(@07,@05)                     02S0360
*               IF AALASSTP -> AALASSTE = AAL0 THEN /* ASST ENTRY 0  */
         C     @09,AALASSTE(,AALASSTP)                          02S0361
         BNE   @RF00361                                         02S0361
*                 R15 = AAL64;      /* YES - SECTOR NOT ALLOCATED    */
         LA    R15,64                                           02S0362
*               ELSE                                            02S0363
*                 DO;                                           02S0363
         B     @RC00361                                         02S0363
@RF00361 DS    0H                                               02S0364
*                   AALSECTP = AALASSTP -> AALASSTE; /* GET LPME
*                                      SECTOR ADDRESS                */
         MVC   AALSECTP(4),AALASSTE(AALASSTP)                   02S0364
*                   R15 = AAL0;     /* SET UP 0 RETURN CODE          */
         SR    R15,R15                                          02S0365
*                 END;                                          02S0366
*             END;                                              02S0367
*           ELSE                                                02S0368
*             R15 = AAL60;          /* ONLY BASE ASPCT USED FOR 02S0368
*                                      16 MEGABTYE AREA              */
         B     @RC00358                                         02S0368
@RF00358 LA    R15,60                                           02S0368
*         END;                                                  02S0369
@RC00358 DS    0H                                               02S0370
*     END;                                                      02S0370
@RC00354 DS    0H                                               02S0371
*   RESPECIFY R1 RESTRICTED;                                    02S0371
@RC00349 DS    0H                                               02S0372
*   R1 = ADDR(AALRPARM);            /* SET UP RETURN PARAMETER       */
         LA    R1,AALRPARM                                      02S0372
*   RETURN;                         /* RETURN TO CALLER              */
@EL00002 DS    0H                                               02S0373
@EF00002 DS    0H                                               02S0373
@ER00002 BR    @14                                              02S0373
*   RESPECIFY (R1,R14,R15) UNRESTRICTED;                        02S0374
*   END AALSUB;                                                 02S0375
