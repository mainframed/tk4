*   /*                                                               */
*   /* THIS IS THE VSAM RBA TO MBBCCHHR CONVERT ROUTINE. THE CODE IS */
*   /* IDENTICAL WITH THAT IN MODULE IDA019R3, THE VSAM I/O MANAGER. */
*   /* INPUT CONSISTS OF THE REQUESTED RBA IN RWORK0 (GPR 0), THE AMB*/
*   /* ADDRESS IN RAMB (GPR 3), A DUMMY PLH POINTER IN RPLH (GPR 2), */
*   /* OUTPUT IS PLACED IN THE DUMMY PLH (CNVRTOUT). THE 'BB' FIELD  */
*   /* OF THE MBBCCHHR IS USED AS A RETURN CODE.                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0376
*/*CONVERT: E CONVERT                                                */
*/* N VSAM RBA TO MBBCCHHR CONVERT RTN                               */
*/* P SAVE NUMBER OF EDB ENTRIES                                     */
*/* P SAVE EDB ENTRY LENGTH                                          */
*/* P SET BLOCKSIZE TO 0                                             */
*/* P GET FIRST EDB                                                  */
*/*CONEDBS: D (NO,,YES,CONEDBN) RBA < EDBLORBA                       */
*/* D (NO,,YES,CONEDBN) RBA > EDBHIRBA                               */
*/* P MOVE 'M' FROM EDBM TO OUTPUT AREA                              */
*/* P SET RC TO 0 IN OUTPUT AREA ('BB')                              */
*/* P SAVE STARTING TRACK (EDBSTTRK)                                 */
*/* P COMPUTE WORK1 = (RBA - EDBLORBA) (REL. OFFSET)                 */
*/* P GET LPMB ADDRESS FROM EDBLPMBA                                 */
*/* P SET 'CC' = WORK1 / LPMAUSZ                                     */
*/* P SET WORK2 = REM( WORK1 / LPMAUSZ)                              */
*/* P SET 'HH' = WORK2 / LPMBPTRK                                    */
*/* P SET WORK1 = REM( WORK2 / LPMBPTRK)                             */
*/* P SET 'R' = (WORK1 / LPMBLKSZ) + 1                               */
*/* P SET WORK1 = CC * LPMTRKAU + HH + STARTING TRACK                */
*/* P SET CC = WORK1 / LPMTPC                                        */
*/* P SET HH = REM( WORK1 / LPMTPC)                                  */
*/* P SET BLOCKSIZE = LPMBLKSZ                                       */
*/*CONEDBN: D (YES,,NO,CONRET) BLOCKSIZE = 0                         */
*/* P DECREMENT EDB COUNT                                            */
*/* D (NO,,YES,%CONCHK) COUNT = 0                                    */
*/* P (,CONEDBS) GET NEXT EDB                                        */
*/*CONRET: P RESTORE REGISTERS                                       */
*/* R RETURN TO CALLER                                               */
*/*%CONCHK: D (YES,,NO,CONRET) BLOCKSIZE = 0                         */
*/* P (,CONRET) SET RETURN CODE OF 4                                 */
*                                                               01S0376
*CONVERT:                                                       01S0376
*   PROC OPTIONS(SAVE,NOSAVEAREA);                              01S0376
CONVERT  STM   @14,@12,12(@13)                                  01S0376
*   RESPECIFY (RWORK0,RWORK2,RAMB,RPLH) RESTRICTED;             01S0377
*   RESPECIFY IDAEDBHD BASED(RWORK2);                           01S0378
*   RWORK2 = AMBEDB;                /* GET EDB ADDRESS FROM AMB      */
         L     @09,@CA01700                                     01S0379
         L     @01,VCTLPA(,VCCTREG)                             01S0379
         AR    @09,@01                                          01S0379
         A     @01,IODBBCT(,@09)                                01S0379
         L     @09,BCTAMB(,@01)                                 01S0379
         L     RWORK2,AMBEDB(,@09)                              01S0379
*   CONBB = EDBNO;                  /* SAVE NUMBER OF EDB ENTRIES    */
         SR    @09,@09                                          01S0380
         IC    @09,EDBNO(,RWORK2)                               01S0380
         STH   @09,CONBB(,RPLH)                                 01S0380
*   CONCC = EDBLEN;                 /* SAVE EDB ENTRY LENGTH         */
         MVC   CONCC(2,RPLH),EDBLEN(RWORK2)                     01S0381
*   RESPECIFY IDAEDB BASED(RWORK2);                             01S0382
*   RWORK2 = RWORK2 + LENGTH(IDAEDBHD);                         01S0383
*                                                               01S0383
         AL    RWORK2,@CF00060                                  01S0383
*   /*****************************************************************/
*   /*                                                               */
*   /* TEST INPUT RBA AGAINST LOW AND HIGH RBA RANGE FOR EACH EDB    */
*   /* ENTRY TO FIND CORRECT ONE.                                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0384
*   CPABLKSZ = ZERO;                /* CLEAR                         */
         SR    @09,@09                                          01S0384
         ST    @09,CPABLKSZ                                     01S0384
*   DO PLHSAVE3 = ONE TO CONBB WHILE (CPABLKSZ=ZERO);           01S0385
         LA    @09,1                                            01S0385
         B     @DE00385                                         01S0385
@DL00385 SR    @09,@09                                          01S0385
         C     @09,CPABLKSZ                                     01S0385
         BNE   @DC00385                                         01S0385
*     IF RWORK0 >= EDBLORBA & RWORK0 <= EDBHIRBA THEN /* RBA IN RANGE*/
         L     @07,EDBLORBA(,RWORK2)                            01S0386
         CLR   RWORK0,@07                                       01S0386
         BL    @RF00386                                         01S0386
         CL    RWORK0,EDBHIRBA(,RWORK2)                         01S0386
         BH    @RF00386                                         01S0386
*       DO;                         /* EDB FOUND - NOW SET UP   01S0387
*                                      MBBCCHHR                      */
*         CONM = EDBM;              /* M FROM EDB                    */
         MVC   CONM(1,RPLH),EDBM(RWORK2)                        01S0388
*         CONBB = ZERO;             /* BB SET TO ZERO                */
         STH   @09,CONBB(,RPLH)                                 01S0389
*         PLHSAVE3 = EDBSTTRK;      /* SAVE STARTING TRACK           */
         MVC   PLHSAVE3(4),EDBSTTRK(RWORK2)                     01S0390
*         RESPECIFY RWORK3 RESTRICTED;                          01S0391
*         RWORK3 = RWORK0 - EDBLORBA; /* RBA RELATIVE TO EXTENT      */
         L     @09,EDBLORBA(,RWORK2)                            01S0392
         LCR   @09,@09                                          01S0392
         ALR   @09,RWORK0                                       01S0392
         LR    RWORK3,@09                                       01S0392
*         RESPECIFY IDALPMB BASED(RWORK2);                      01S0393
*         RWORK2 = EDBLPMBA;        /* GET LPMB ADDRESS FROM EDB     */
*                                                               01S0394
         L     RWORK2,EDBLPMBA(,RWORK2)                         01S0394
*   /*****************************************************************/
*   /*                                                               */
*   /* OPTIMIZE CODE BY USING ASSEMBLER CODE FOR DIVISION            */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0395
*         RESPECIFY (RWORK4,RWORK5) RESTRICTED;                 01S0395
*         GENERATE REFS(LPMAUSZ,LPMBPTRK);                      01S0396
*        CONCC = RWORK3 / LPMAUSZ
*        RWORK5 = RWORK3 // LPMAUSZ
         LR    RWORK5,RWORK3           RBA RELATIVE TO EXTENT IN ODD
         SR    RWORK4,RWORK4           ZERO EVEN REGISTER
         D     RWORK4,LPMAUSZ(,RWORK2) RELATIVE CYLINDER VALUE
         STH   RWORK5,CONCC(,RPLH)     STORE RELATIVE CYLINDER VALUE
         LR    RWORK5,RWORK4           REMAINDER INTO ODD REGISTER
*        CONHH = RWORK5 / LPMBPTRK
*        RWORK3 = RWORK5 // LPMBPTRK
         SR    RWORK4,RWORK4           ZERO EVEN REGISTER
         D     RWORK4,LPMBPTRK(,RWORK2) RELATIVE TRACK VALUE
         STH   RWORK5,CONHH(,RPLH)     STORE RELATIVE TRACK VALUE
         LR    RWORK3,RWORK4           REMAINDER INTO RWORK3
*         RESPECIFY (RWORK4,RWORK5) UNRESTRICTED;               01S0397
*         CONR = (RWORK3+LPMBLKSZ-ONE) / LPMBLKSZ + ONE; /* R        */
         L     @09,LPMBLKSZ(,RWORK2)                            01S0398
         LR    @01,RWORK3                                       01S0398
         AR    @01,@09                                          01S0398
         BCTR  @01,0                                            01S0398
         LR    @04,@01                                          01S0398
         SRDA  @04,32                                           01S0398
         DR    @04,@09                                          01S0398
         AL    @05,@CF00062                                     01S0398
         STC   @05,CONR(,RPLH)                                  01S0398
*         RWORK3 = CONCC * LPMTRKAU + CONHH + PLHSAVE3;         01S0399
         SR    RWORK3,RWORK3                                    01S0399
         ICM   RWORK3,3,CONCC(RPLH)                             01S0399
         MH    RWORK3,LPMTRKAU(,RWORK2)                         01S0399
         SR    @09,@09                                          01S0399
         ICM   @09,3,CONHH(RPLH)                                01S0399
         AR    RWORK3,@09                                       01S0399
         A     RWORK3,PLHSAVE3                                  01S0399
*         RESPECIFY (RWORK4,RWORK5,R15) RESTRICTED;             01S0400
*         GENERATE REFS(LPMTPC);                                01S0401
*        CONCC = RWORK3 / LPMTPC
*        CONHH = RWORK3 // LPMTPC
         LR    RWORK5,RWORK3           LOAD ODD REGISTER
         SR    RWORK4,RWORK4           ZERO EVEN REGISTER
         LH    R15,LPMTPC(,RWORK2)
         DR    RWORK4,R15              DETERMINE REAL CCHH
         STH   RWORK5,CONCC(,RPLH)     REAL CYLINDER VALUE - CC
         STH   RWORK4,CONHH(,RPLH)     REAL TRACK VALUE - HH
*         RESPECIFY (RWORK3,RWORK4,RWORK5,R15) UNRESTRICTED;    01S0402
*         CPABLKSZ = LPMBLKSZ;                                  01S0403
         MVC   CPABLKSZ(4),LPMBLKSZ(RWORK2)                     01S0403
*       END;                                                    01S0404
*     ELSE                                                      01S0405
*       RWORK2 = RWORK2 + CONCC;    /* NEXT EDB ENTRY                */
         B     @RC00386                                         01S0405
@RF00386 SR    @09,@09                                          01S0405
         ICM   @09,3,CONCC(RPLH)                                01S0405
         ALR   RWORK2,@09                                       01S0405
*   END;                                                        01S0406
@RC00386 LA    @09,1                                            01S0406
         A     @09,PLHSAVE3                                     01S0406
@DE00385 ST    @09,PLHSAVE3                                     01S0406
         SR    @07,@07                                          01S0406
         ICM   @07,3,CONBB(RPLH)                                01S0406
         CR    @09,@07                                          01S0406
         BNH   @DL00385                                         01S0406
@DC00385 DS    0H                                               01S0407
*   IF CPABLKSZ = ZERO              /* IF EDB NOT FOUND FOR INPUT RBA*/
*         THEN                                                  01S0407
         ICM   @09,15,CPABLKSZ                                  01S0407
         BNZ   @RF00407                                         01S0407
*     CONBB = FOUR;                 /* SET ERROR                     */
         MVC   CONBB(2,RPLH),@CH00054                           01S0408
*   RESPECIFY (RWORK0,RWORK2) UNRESTRICTED;                     01S0409
*   RETURN;                                                     01S0410
@EL00003 DS    0H                                               01S0410
@EF00003 DS    0H                                               01S0410
@ER00003 LM    @14,@12,12(@13)                                  01S0410
         BR    @14                                              01S0410
*   END CONVERT;                                                01S0411
         B     @EL00003                                         01S0411
*   END AMDSAAMR                                                01S0412
