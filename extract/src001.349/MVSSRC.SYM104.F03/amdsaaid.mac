         TITLE 'AMDSAAID ASID SEARCH ROUTINE                           *
                       '
AMDSAAID CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'AMDSAAID  73.230'                                 0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0001
         BALR  @11,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@11                                         0001
         ST    @13,@SA00001+4                                      0001
         LA    @14,@SA00001                                        0001
         ST    @14,8(,@13)                                         0001
         LR    @13,@14                                             0001
*   SAVEFORM(1:4) = 'LAID';         /* PLACE THE MODULE IDENTIFIER IN
*                                      THE FIRST WORD OF THE SAVE  0193
*                                      AREA. THIS IS A DEBUGGING AID */
         MVC   SAVEFORM(4,R13),@CC01420                            0193
*   RESPECIFY RCB BASED (ADDR(AIDRCB));                            0194
*   RCBNEXT = VCTRCB;               /* SAVE BACKWARD RCB CHAIN     0195
*                                      POINTER                       */
         LA    @10,AIDRCB                                          0195
         MVC   RCBNEXT(4,@10),VCTRCB(VCCTREG)                      0195
*   GEN REFS(R1) SETS (AIDSTOR) (STCTL R1,R1,AIDSTOR); /* SAVE     0196
*                                      SEGMENT TABLE ORGIN FOR THE 0196
*                                      RCB EXIT ROUTINE              */
         STCTL R1,R1,AIDSTOR
*   EXITADDR = ADDR(NEXTASID);      /* ERROR EXIT ADDR               */
         LA    @10,NEXTASID                                        0197
         ST    @10,EXITADDR                                        0197
*   RCBSAVE = R13;                  /* AID'S SAVE AREA POINTER       */
         LA    @10,AIDRCB                                          0198
         ST    R13,RCBSAVE(,@10)                                   0198
*   RCBBASE = BASEREG;              /* BASE REGISTER OF AID          */
         ST    BASEREG,RCBBASE(,@10)                               0199
*   VCTRCB = ADDR(AIDRCB);          /* PLACE AID RCB ON THE VCCT   0200
*                                      CHAIN                         */
         ST    @10,VCTRCB(,VCCTREG)                                0200
*   ASCBPTR = 0;                    /* SET UP INITIAL ASCB POINTER   */
         SR    @10,@10                                             0201
         LR    ASCBPTR,@10                                         0201
*   AIDASVTA = 0;                   /* INITIALIZE INITIAL ASVT     0202
*                                      ADDRESS                       */
         LR    AIDASVTA,@10                                        0202
*   AIDMAX = 0;                     /* SET UP MAXIMUM COUNTER        */
         LR    AIDMAX,@10                                          0203
*   CTBUFTYP = OFF;                 /* TURN OFF BUFFER SWITCH        */
         L     @10,VCTCCTV(,VCCTREG)                               0204
         MVI   CTBUFTYP(@10),B'00000000'                           0204
*   CTLSQA = ON;                    /* TURN ON LSQA SWITCH           */
         OI    CTLSQA(@10),B'00001000'                             0205
*                                                                  0206
*   /*****************************************************************/
*   /*                                                               */
*   /* OBTAIN THE ADDRESS OF THE ASVT AND VALIDITY CHECK THE ASVT. IF*/
*   /* THE ASVT IS VALID, THE ASCB'S WILL BE OBTAINED FROM THE ASVT. */
*   /* IF THE ASVT IS INVALID, AN ALTERNATE SEARCH USING THE         */
*   /* DISPATCHABILITY QUEUE WILL BE UTILIZED                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0206
*/* P OBTAIN ASVT ADDRESS FROM CVTASVT                               */
*/* L AMDSAVCK: SVC 25 VALIDITY CHECK THE ASVT                       */
*/* D (YES,,NO,ALTASCB) ASVT VALID ?                                 */
*/* P AIDSVTA POINTS TO FIRST ASVT ENTRY                             */
*/*ENTYQ: P (,VCKASCB) VCTASCB = AIDASCBA                            */
*/*ALTASCB: P AIDALTSW = 1                                           */
*/* P USE CVTASCBH QUEUE TO OBTAIN ASCB'S                            */
*/* P ASVTMAXU = X'7FFF'                                             */
*/* P (,VCKASCB) VCTASCB = CVTASCBH                                  */
*   ASVTPTR = CVTASVT;              /* SET UP POINTER TO ASVT FROM 0206
*                                      THE CVT                       */
         L     @10,CVTPTR                                          0206
         L     @10,CVTASVT(,@10)                                   0206
         LR    ASVTPTR,@10                                         0206
*   VCTCBADR = CVTASVT;             /* SET UP POINTER TO ASVT SO IT
*                                      CAN BE VALIDITY CHECKED       */
         ST    @10,VCTCBADR(,VCCTREG)                              0207
*   VCTCBIDN = VASVTID;             /* SET UP ASVT ID FOR VALIDITY 0208
*                                      CHECK                         */
         MVI   VCTCBIDN(VCCTREG),X'04'                             0208
*   SVC (VCKSVC);                   /* VALIDITY CHECK THE ASVT       */
         SVC   25                                                  0209
*   RESPECIFY RETCODE RESTRICTED;                                  0210
*   IF RETCODE = 0 THEN                                            0211
         SR    @14,@14                                             0211
         CR    RETCODE,@14                                         0211
         BNE   @RF00211                                            0211
*     DO;                           /* IF VALID THEN DO THE FOLLOWING*/
*       RESPECIFY RETCODE UNRESTRICTED;                            0213
*       AIDASVTA = ADDR(ASVTENTY(1)); /* POINT TO FIRST ENTRY        */
         LA    AIDASVTA,ASVTENTY(,ASVTPTR)                         0214
*       AIDMAX = ASVTMAXU;          /* SET UP LIMITS ON ASCB'S DUMPED*/
         L     AIDMAX,ASVTMAXU(,ASVTPTR)                           0215
*       IF AIDMAX=0 THEN                                           0216
         CR    AIDMAX,@14                                          0216
         BNE   @RF00216                                            0216
*         AIDMAX=DFLTASCB;          /* SET TO MAX IF 0               */
*                                                                  0217
         LH    AIDMAX,@CH01416                                     0217
*ENTYQ:                                                            0218
*       VCTASCB = AIDASCBA;         /* POINT TO CURRENT ASCB ENTRY   */
@RF00216 DS    0H                                                  0218
ENTYQ    MVC   VCTASCB(4,VCCTREG),AIDASCBA(AIDASVTA)               0218
*     END;                                                         0219
*   ELSE                                                           0220
*     DO;                                                          0220
         B     @RC00211                                            0220
@RF00211 DS    0H                                                  0221
*       AIDALTSW = ON;              /* USE DISPATCHABILITY QUEUE     */
         OI    AIDALTSW,B'10000000'                                0221
*       AIDMAX = DFLTASCB;          /* LIMITS THE ASCB SEARCHING     */
         LH    AIDMAX,@CH01416                                     0222
*       VCTASCB = CVTASCBH;         /* SET TO TOP ASCB ON DISPATCHER
*                                      CHAIN                         */
         L     @10,CVTPTR                                          0223
         MVC   VCTASCB(4,VCCTREG),CVTASCBH(@10)                    0223
*     END;                                                         0224
*                                                                  0225
*   /*****************************************************************/
*   /*                                                               */
*   /* VALIDITY CHECK THE ASCB. IF IT IS INVALID, TRY AN ALTERNATE   */
*   /* SEARCH PATH (IF AVAILABLE)                                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0225
*/*VCKASCB: L AMDSAVCK: SVC 25 TO VALIDITY CHECK ASCB                */
*/* D (YES,%AIDS,NO,) ASCB VALID ?                                   */
*/* D (YES,NEXTASID,NO,) AIDALTSW = OFF ?                            */
*/* D (YES,DEQRCB,NO,) AIDBACKS = ON ?                               */
*/* P AIDBACKS = ON                                                  */
*/* P OBTAIN CVT LOWER ASCB CHAIN                                    */
*/* D (YES,DEQRCB,NO,) VCTASCB = 0                                   */
*/* D (YES,DEQRCB,NO,VCKASCB) VCTASCB = AIDASCBL ?                   */
*                                                                  0225
*VCKASCB:                                                          0225
*   VCTCBADR = VCTASCB;             /* PLACE ASCB ADDRESS INTO THE 0225
*                                      VALIDITY CHECK INPUT PARAMETER
*                                      DATA                          */
@RC00211 DS    0H                                                  0225
VCKASCB  MVC   VCTCBADR(4,VCCTREG),VCTASCB(VCCTREG)                0225
*   VCTCBIDN = VASCBID;             /* ASCB IDENTIFIER FOR VALIDITY
*                                      CHECK                         */
         MVI   VCTCBIDN(VCCTREG),X'02'                             0226
*   SVC (VCKSVC);                   /* VALIDITY CHECK THE ASCB       */
         SVC   25                                                  0227
*   RESPECIFY RETCODE RESTRICTED;                                  0228
*   IF RETCODE ^= 0 THEN                                           0229
         SR    @14,@14                                             0229
         CR    RETCODE,@14                                         0229
         BE    @RF00229                                            0229
*     DO;                           /* IF ASCB INVALID USE AN      0230
*                                      ALTERNATE SEARCH              */
*       RESPECIFY RETCODE UNRESTRICTED;                            0231
*       IF AIDALTSW = OFF THEN                                     0232
         TM    AIDALTSW,B'10000000'                                0232
         BZ    @RT00232                                            0232
*         GO TO NEXTASID;           /* IF IN PRIMARY SEARCH SKIP THIS
*                                      MEMORY AND CONTINUE WITH THE
*                                      NEXT MEMORY                   */
*       IF AIDBACKS = ON THEN                                      0234
         TM    AIDBACKS,B'01000000'                                0234
         BO    @RT00234                                            0234
*         GO TO DEQRCB;             /* WE ARE AT THE END OF ALL    0235
*                                      RECOVERY. THE ASVT IS INVALID.
*                                      THE ASCB FORWARD AND BACKWARD
*                                      SEARCHES HAVE GONE ASTRAY--OUR
*                                      ONLY RECOURSE IS TO GRACEFULLY
*                                      EXIT                          */
*       AIDBACKS = ON;              /* SET BACKWARD SEARCH ON        */
         OI    AIDBACKS,B'01000000'                                0236
*       VCTASCB = CVTASCBL;         /* PICK UP BACKWARD CVT CHAIN    */
         L     @10,CVTPTR                                          0237
         L     @10,CVTASCBL(,@10)                                  0237
         ST    @10,VCTASCB(,VCCTREG)                               0237
*       IF VCTASCB = 0 × VCTASCB = AIDASCBL THEN                   0238
         CR    @10,@14                                             0238
         BE    @RT00238                                            0238
         C     @10,AIDASCBL                                        0238
         BE    @RT00238                                            0238
*         GO TO DEQRCB;             /* IF AT END OF CHAIN, TERMINATE */
*       GO TO VCKASCB;              /* OTHERWISE RESTART WITH THIS 0240
*                                      ASCB CHAIN                    */
         B     VCKASCB                                             0240
*     END;                                                         0241
*                                                                  0242
*   /*****************************************************************/
*   /*                                                               */
*   /* THE ASCB IS VALID. OBTAIN THE JOBNAME ASSOCIATED WITH THE     */
*   /* MEMORY                                                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0242
*/*%AIDS: P SET UP ASCBPTR                                           */
*/* D (YES,,NO,%NOSAID) AIDBACKS = OFF ?                             */
*/* P SAVE LAST ASCB PROCESED                                        */
*/*%NOSAID: P VCTASID = ASCBASID                                     */
*/* D (YES,JBNS,NO,JBNI) ASCBJBNI = 0 ?                              */
*/*JBNI: P (,CKJOB) VCTJBNME = ASCBJBNI                              */
*/*JBNS: D (YES,%JOB,NO,) ASCBJBNI = 0 ?                             */
*/* P (,CKJOB) VCTJBNME = ASCBJBNS                                   */
*/*%JOB: P VCKJBNME = '*UNKNOWN'                                     */
*/*CKJOB: P VALIDITY CHECK JOBNAME                                   */
*/* D (YES,%MSG,NO,) JOBNAME VALID ?                                 */
*/* P VCTJBNME = '*UNKNOWN'                                          */
*   ASCBPTR = VCTASCB;              /* UPDATE THE POINTER TO THE ASCB*/
@RF00229 L     @10,VCTASCB(,VCCTREG)                               0242
         LR    ASCBPTR,@10                                         0242
*   IF AIDBACKS = OFF THEN                                         0243
         TM    AIDBACKS,B'01000000'                                0243
         BNZ   @RF00243                                            0243
*     AIDASCBL = VCTASCB;           /* SAVE ADDRESS OF ASCB BEING  0244
*                                      PROCESSED                     */
         ST    @10,AIDASCBL                                        0244
*   VCTASID = ASCBASID;             /* SET UP ASID BEING PROCESSED   */
@RF00243 MVC   VCTASID(2,VCCTREG),ASCBASID(ASCBPTR)                0245
*   RESPECIFY AIDJOBN BASED (ASCBJBNI);                            0246
*   IF ASCBJBNI ^= 0 THEN                                          0247
         L     @10,ASCBJBNI(,ASCBPTR)                              0247
         LTR   @10,@10                                             0247
         BZ    @RF00247                                            0247
*     VCTJBNME = AIDJOBN;           /* SET TO PROGRAM JOBNAME        */
         MVC   VCTJBNME(8,VCCTREG),AIDJOBN(@10)                    0248
*   ELSE                                                           0249
*     IF ASCBJBNS ^= 0 THEN                                        0249
         B     @RC00247                                            0249
@RF00247 L     @10,ASCBJBNS(,ASCBPTR)                              0249
         LTR   @10,@10                                             0249
         BZ    @RF00249                                            0249
*       VCTJBNME = ASCBJBNS -> AIDJOBN; /* SET TO START/MOUNT/LOGON
*                                      JOBNAME                       */
         MVC   VCTJBNME(8,VCCTREG),AIDJOBN(@10)                    0250
*     ELSE                                                         0251
*       VCTJBNME ='*UNKNOWN';       /* JOBNAME IS UNAVAILABLE        */
         B     @RC00249                                            0251
@RF00249 MVC   VCTJBNME(8,VCCTREG),@CC01431                        0251
*   IF VCTJBNME(1) ^= '*' & VCTJBNME(1) ^= '$' & VCTJBNME(1) ^= '@' &
*         VCTJBNME(1) ^= '#' & ((VCTJBNME(1) < 'A') × (VCTJBNME(1) >
*         'I' & VCTJBNME(1) < 'J') × (VCTJBNME(1) > 'R' & VCTJBNME(1)
*         < 'S') × (VCTJBNME(1) > 'Z')) THEN                       0252
@RC00249 DS    0H                                                  0252
@RC00247 CLI   VCTJBNME(VCCTREG),C'*'                              0252
         BE    @RF00252                                            0252
         CLI   VCTJBNME(VCCTREG),C'$'                              0252
         BE    @RF00252                                            0252
         CLI   VCTJBNME(VCCTREG),C'@'                              0252
         BE    @RF00252                                            0252
         CLI   VCTJBNME(VCCTREG),C'#'                              0252
         BE    @RF00252                                            0252
         CLI   VCTJBNME(VCCTREG),C'A'                              0252
         BL    @RT00252                                            0252
         CLI   VCTJBNME(VCCTREG),C'I'                              0252
         BNH   @GL00007                                            0252
         CLI   VCTJBNME(VCCTREG),C'J'                              0252
         BL    @RT00252                                            0252
@GL00007 CLI   VCTJBNME(VCCTREG),C'R'                              0252
         BNH   @GL00006                                            0252
         CLI   VCTJBNME(VCCTREG),C'S'                              0252
         BL    @RT00252                                            0252
@GL00006 CLI   VCTJBNME(VCCTREG),C'Z'                              0252
         BNH   @RF00252                                            0252
@RT00252 DS    0H                                                  0253
*     VCTJBNME = '*UNKNOWN';        /* INVALID JOBNAME PROVIDED IF 0253
*                                      FIRST CHARACTER IS NOT A    0253
*                                      LETTER                        */
         MVC   VCTJBNME(8,VCCTREG),@CC01431                        0253
*                                                                  0254
*   /*****************************************************************/
*   /*                                                               */
*   /* OUTPUT A MESSAGE INDICATING WHAT MEMORY WE ARE CURRENTLY      */
*   /* PROCESSING                                                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0254
*/*%MSG: P MOVE ASID INTO ASID10I FROM VCTASID FOR MESSAGE           */
*/* P MOVE JOBNAME INTO JOBN10I FROM VCTJBNME FOR MESSAGE            */
*/* L AMDSACON: SVC 3 TO ISSUE MESSAGE AMD010I                       */
*   CVTADDR = ADDR(VCTASID);        /* ASID TO BE CONVERTED          */
@RF00252 LA    @10,VCTASID(,VCCTREG)                               0254
         ST    @10,CVTADDR                                         0254
*   CVTOUTA = ADDR(ASIDNUM);        /* ADDRESS OF OUTPUT AREA        */
         LA    @10,ASIDNUM                                         0255
         ST    @10,CVTOUTA                                         0255
*   CVTLNG = LENGTH(VCTASID);       /* LENGTH OF ASID                */
         MVC   CVTLNG(2),@CH00188                                  0256
*   RESPECIFY R1 RESTRICTED;                                       0257
*   R1 = ADDR(CVTBLK);              /* CONVERT PARAMETER LIST ADDRESS*/
         LA    R1,CVTBLK                                           0258
*   SVC (BINSVC);                   /* CONVERT ASID TO EBCDIC        */
         SVC   28                                                  0259
*   VASID10I=ASIDNUM;               /* SET UP MSG 10I WITH ASID      */
         L     @10,@CA01407                                        0260
         L     @02,VCTLPA(,VCCTREG)                                0260
         AR    @10,@02                                             0260
         MVC   VASID10I(4,@10),ASIDNUM                             0260
*   VJOBN10I = VCTJBNME;            /* MOVE IN JOBNAME               */
         L     @10,@CA01410                                        0261
         AR    @10,@02                                             0261
         MVC   VJOBN10I(8,@10),VCTJBNME(VCCTREG)                   0261
*   CVTADDR=ADDR(VCTASCB)+1;        /* FOR ASCB CONVERT              */
         LA    @10,VCTASCB(,VCCTREG)                               0262
         AH    @10,@CH00074                                        0262
         ST    @10,CVTADDR                                         0262
*   CVTOUTA=ADDR(ASCB10I)+VCTLPA;   /* WHERE TO PUT THE ADDR         */
         L     @10,@CA01409                                        0263
         AR    @10,@02                                             0263
         ST    @10,CVTOUTA                                         0263
*   CVTLNG=LENGTH(VCTASCB)-1;       /* LENGTH TO BE CONVERTED        */
         MVC   CVTLNG(2),@CH00401                                  0264
*   R1=ADDR(CVTBLK);                /* CONVERT PARAMETER LIST        */
         LA    R1,CVTBLK                                           0265
*   SVC(BINSVC);                    /* CONVERT ASCB                  */
         SVC   28                                                  0266
*   R1 = AMD010I;                   /* SET UP MESSAGE ID FOR CONSOLE */
         LA    R1,10                                               0267
*   SVC (CONSVC);                   /* ISSUE MESSAGE INDICATING THE
*                                      CURRENT MEMORY BEING PROCESSED*/
         SVC   3                                                   0268
*   RESPECIFY R1 UNRESTRICTED;                                     0269
*                                                                  0270
*   /*****************************************************************/
*   /*                                                               */
*   /* DUMP THE LOCAL MOMORY'S STORAGE                               */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0270
*/*SIN: L AMDSASIN: SVC 8 TO SWAP-IN MEMORY                          */
*/* D (YES,,NO,NEXTASID) MEMORY SWAPPED IN ?                         */
*/* P SAVE VCTSTOR IN AIDMEM FOR FOLLOWING CALLS                     */
*   SVC (SINSVC);                   /* SWAP IN THE MEMORY. THE ASCB
*                                      IS IN VCTASCB                 */
         SVC   8                                                   0270
*   RESPECIFY RETCODE RESTRICTED;                                  0271
*   IF RETCODE ^= 0 THEN                                           0272
         LTR   RETCODE,RETCODE                                     0272
         BNZ   @RT00272                                            0272
*     GO TO NEXTASID;               /* IF MEMORY CAN NOT BE OBTAINED,
*                                      SKIP TO THE NEXT MEMORY       */
*   AIDMEM = VCTSTOR;               /* SAVE SEGMENT TABLE ADDRESS FOR
*                                      FUTURE CALLS                  */
         L     AIDMEM,VCTSTOR(,VCCTREG)                            0274
*   RESPECIFY RETCODE UNRESTRICTED;                                0275
*                                                                  0276
*   /*****************************************************************/
*   /*                                                               */
*   /* SWAP IN THE MEMORY                                            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0276
*/* P RESTORE VCTSTOR FOR MDM CALL                                   */
*/* L AMDSAMDM: SVC 17 TO DUMP MEMORY                                */
*   VCTSTOR = AIDMEM;               /* SET UP TO ENTER LOCAL MEMORY  */
         ST    AIDMEM,VCTSTOR(,VCCTREG)                            0276
*   SVC (MDMSVC);                   /* DUMP THE MOMORY'S AREA        */
*                                                                  0277
         SVC   17                                                  0277
*   /*****************************************************************/
*   /*                                                               */
*   /* DUMP THE GTF BUFFERS IF THIS MEMORY IS GTF                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0278
*/* P RESTORE VCTSTOR FOR GTF CALL                                   */
*/* L AMDSAGTF: SVC 22 TO DUMP GTF BUFFERS                           */
*   VCTSTOR = AIDMEM;               /* RESTORE FOR MEMORY SWITCH     */
         ST    AIDMEM,VCTSTOR(,VCCTREG)                            0278
*   SVC (GTFSVC);                   /* DUMP THE GTF VIRTUAL BUFFER   */
         SVC   22                                                  0279
*   /*****************************************************************/
*   /*                                                               */
*   /* OBTAIN THE NEXT LOCAL MEMORY. SEARCH THE PROPER CHAIN TO      */
*   /* OBTAIN THE NEXT ASCB POINTER                                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0280
*/*NEXTASID: P AIDMAX = AIDMAX - 1                                   */
*/* D (YES,DEQRCB,NO,) AIDMAX <= 0 ?                                 */
*/* D (YES,ALTRCB,NO,) AIDALTSW = 1 ?                                */
*/* P AIDASVTA = AIDASVTA + 4                                        */
*/* D (YES,DEQRCB,NO,) AIDASCBA = 0 ?                                */
*/* D (YES,NEXTASID,NO,ENTYQ) ASVTAVAI = OFF ?                       */
*                                                                  0280
*NEXTASID:                                                         0280
*   AIDMAX = AIDMAX - 1;            /* FINISHED WITH PREVIOUS LOCAL
*                                      MEMORY                        */
NEXTASID BCTR  AIDMAX,0                                            0280
*   IF AIDMAX <= 0 THEN                                            0281
         SR    @10,@10                                             0281
         CR    AIDMAX,@10                                          0281
         BNH   @RT00281                                            0281
*     GO TO DEQRCB;                 /* IF ALL MEMORIES HAVE BEEN   0282
*                                      COMPLETED--EXIT               */
*   IF AIDALTSW = OFF THEN                                         0283
         TM    AIDALTSW,B'10000000'                                0283
         BNZ   @RF00283                                            0283
*     DO;                           /* PRIMARY ASVT CHAIN SEARCH     */
*       AIDASVTA = AIDASVTA + LENGTH(ASVTENTY); /* BUMP TO NEXT ASVT
*                                      ENTRY                         */
         AH    AIDASVTA,@CH00038                                   0285
*       IF AIDASCBA = 0 THEN                                       0286
         C     @10,AIDASCBA(,AIDASVTA)                             0286
         BE    @RT00286                                            0286
*         GO TO DEQRCB;             /* IF AT END OF CHAIN- WE HAVE 0287
*                                      COMPLETED PROCESSING          */
*       IF AIDAVAL = OFF THEN                                      0288
         TM    AIDAVAL(AIDASVTA),B'10000000'                       0288
         BZ    @RT00288                                            0288
*         GO TO ENTYQ;              /* IF THE ASID IS ACTIVE, DUMP IT*/
*       GO TO NEXTASID;             /* SKIP TO NEXT MEMORY           */
         B     NEXTASID                                            0290
*     END;                                                         0291
*                                                                  0292
*   /*****************************************************************/
*   /*                                                               */
*   /* USING FORWARD OR BACKWARD CHAIN SEARCH. GET THE NEXT ASCB FROM*/
*   /* THE APPROPRIATE ASCB QUQUE                                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0292
*/*ALTRCB: D (YES,,NO,%QCK) AIDBACKS = OFF                           */
*/* D (YES,DEQRCB,NO,) ASCBFWDP = 0 ?                                */
*/* P (,VCKASCB) VCTASCB = ASCBFWDP                                  */
*/*%QCK: P VCTASCB = ASCBBWDP                                        */
*/* D (YES,VCKASCB,NO,) VCTASCB ^= AIDASCBL                          */
*/* P (,VCKASCB) VCTASCB = ASCBFWDP                                  */
*   IF AIDBACKS = OFF THEN                                         0292
@RF00283 TM    AIDBACKS,B'01000000'                                0292
         BNZ   @RF00292                                            0292
*     DO;                           /* IN FORWARD SEARCH             */
*       IF ASCBFWDP = 0 THEN                                       0294
         L     @10,ASCBFWDP(,ASCBPTR)                              0294
         LTR   @10,@10                                             0294
         BZ    @RT00294                                            0294
*         GO TO DEQRCB;             /* AT END OF CHAIN--EXIT         */
*       VCTASCB = ASCBFWDP;         /* PICK UP NEXT ASCB POINTER     */
         ST    @10,VCTASCB(,VCCTREG)                               0296
*       GO TO VCKASCB;              /* PROCESS NEW LOCAL MEMORY      */
         B     VCKASCB                                             0297
*     END;                                                         0298
*   IF ASCBBWDP = 0 THEN                                           0299
@RF00292 L     @10,ASCBBWDP(,ASCBPTR)                              0299
         LTR   @10,@10                                             0299
         BZ    @RT00299                                            0299
*     GO TO DEQRCB;                 /* AT END OF BACKWARD CHAIN      */
*   VCTASCB = ASCBBWDP;             /* SET UP TO NEW LOCAL MEMORY    */
         ST    @10,VCTASCB(,VCCTREG)                               0301
*   IF VCTASCB ^= AIDASCBL THEN                                    0302
         C     @10,AIDASCBL                                        0302
         BNE   @RT00302                                            0302
*     GO TO VCKASCB;                /* IF HAVE NOT GONE AS FAR BACK
*                                      DOWN THE CHAIN AS WE CAME UP
*                                      THE CHAN, THEN WE ARE STILL 0303
*                                      DUMPING UNIQUE LOCAL MEMORIES */
*                                                                  0304
*   /*****************************************************************/
*   /*                                                               */
*   /* WE HAVE FINISHED DUMPING ALL LOCAL MEMORYS--DEQUEUE THE RCB   */
*   /* AND EXIT TO CALLER                                            */
