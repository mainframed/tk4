         L     @10,VCTCCTV(,VCCTREG)                            01S0249
         SLR   ASMBUFTP,ASMBUFTP                                01S0249
         IC    ASMBUFTP,CTBUFTYP(,@10)                          01S0249
*   CTBUFTYP=OFF;                   /* RESET BUFFER REQUEST FLAGS    */
         MVI   CTBUFTYP(@10),B'00000000'                        01S0250
*   CTASM=ON;                       /* REQUEST ASM BUFFERS           */
         OI    CTASM(@10),B'00010000'                           01S0251
*   RCBNEXT=VCTRCB;                 /* GET CURRENT RCB               */
         LA    @10,ASMRCB                                       01S0252
         MVC   RCBNEXT(4,@10),VCTRCB(VCCTREG)                   01S0252
*   RCBBASE=BASEREG;                /* SAVE BASE REG IN RCB          */
         ST    BASEREG,RCBBASE(,@10)                            01S0253
*   RCBEXIT=ADDR(ASMEXIT);          /* EXIT ADDR TO RCB              */
         LA    @09,ASMEXIT                                      01S0254
         ST    @09,RCBEXIT(,@10)                                01S0254
*   RCBSAVE=R13;                    /* SAVE AREA ADDR TO RCB         */
         ST    R13,RCBSAVE(,@10)                                01S0255
*   GENERATE REFS(R1) SETS(ASMRSTOR)(STCTL R1,R1,ASMRSTOR);/* CURRENT
*                                      SEGMENT TABLE ADDRESS TO RCB  */
         STCTL R1,R1,ASMRSTOR
*   VCTRCB=ADDR(ASMRCB);            /* MAKE RCB ACTIVE               */
         LA    @10,ASMRCB                                       01S0257
         ST    @10,VCTRCB(,VCCTREG)                             01S0257
*                                                               01S0258
*/*  CHECK IF ASM INITIALIZATION IS REQUIRED. IF SO, VALIDITY CHECK *
*  * THE ASMVT AND PART. IF EITHER IS INVALID, TAKE ERROR PATH *
*  * SINCE PAGING CANNOT BE DONE WITHOUT THEM. IF BOTH ARE VALID, *
*  * VALIDITY CHECK THE SART. IF IT IS INVALID, INDICATE SO IN THE *
*  * VCCT. THIS WILL ONLY AFFECT SWAPPED-OUT MEMORIES FOR WHICH THE *
*  * LSQA IS ON A SWAP DATA SET.                             @Z40WI3F*/
*                                                               01S0258
*   IF ASMINIT=OFF THEN             /* ASM INITIALIZATION REQUIRED?
*                                                            @Z40WI3F*/
         TM    ASMINIT,B'10000000'                              01S0258
         BNZ   @RF00258                                         01S0258
*     DO;                           /* YES -                 @Z40WI3F*/
*       VCTCBADR=CVTASMVT;          /* ASMVT ADDR FROM CVT   @Z40WI3F*/
         L     @10,CVTPTR                                       01S0260
         MVC   VCTCBADR(4,VCCTREG),CVTASMVT(@10)                01S0260
*       VCTCBIDN=VASMVTID;          /* ASMVT CNTRL BLOCK ID  @Z40WI3F*/
         MVI   VCTCBIDN(VCCTREG),X'11'                          01S0261
*       SVC(VCKSVC);                /* CALL AMDSAVCK TO VALIDITY
*                                      CHECK THE ASMVT       @Z40WI3F*/
         SVC   25                                               01S0262
*       RESPECIFY                                               01S0263
*         RETCODE RESTRICTED;       /*                       @Z40WI3F*/
*       IF RETCODE^=0 THEN          /* ASMVT NOT VALID?      @Z40WI3F*/
         LTR   RETCODE,RETCODE                                  01S0264
         BNZ   @RT00264                                         01S0264
*         GOTO ASMEXIT;             /* YES - TAKE ERROR PATH @Z40WI3F*/
*       VCTCBADR=ASMPART;           /* PART ADDR FROM ASMVT  @Z40WI3F*/
         L     @14,CVTPTR                                       01S0266
         L     @14,ASMVTPTR(,@14)                               01S0266
         MVC   VCTCBADR(4,VCCTREG),ASMPART(@14)                 01S0266
*       VCTCBIDN=VPARTID;           /* PART CNTRL BLOCK ID   @Z40WI3F*/
         MVI   VCTCBIDN(VCCTREG),X'0C'                          01S0267
*       SVC(VCKSVC);                /* CALL AMDSAVCK TO VALIDITY
*                                      CHECK THE PART        @Z40WI3F*/
         SVC   25                                               01S0268
*       IF RETCODE^=0 THEN          /* PART NOT VALID?       @Z40WI3F*/
         LTR   RETCODE,RETCODE                                  01S0269
         BNZ   @RT00269                                         01S0269
*         GOTO ASMEXIT;             /* YES - TAKE ERROR PATH @Z40WI3F*/
*       VCTCBADR=ASMSART;           /* SART ADDR FROM ASMVT  @Z40WI3F*/
         L     @14,CVTPTR                                       01S0271
         L     @14,ASMVTPTR(,@14)                               01S0271
         MVC   VCTCBADR(4,VCCTREG),ASMSART(@14)                 01S0271
*       VCTCBIDN=VSARTID;           /* SARD CNTRL BLOCK ID   @Z40WI3F*/
         MVI   VCTCBIDN(VCCTREG),X'13'                          01S0272
*       SVC(VCKSVC);                /* CALL AMDSAVCK TO VALIDITY
*                                      CHECK THE SART        @Z40WI3F*/
         SVC   25                                               01S0273
*       IF RETCODE^=0 THEN          /* SART NOT VALID?       @Z40WI3F*/
         LTR   RETCODE,RETCODE                                  01S0274
         BZ    @RF00274                                         01S0274
*         VCTSARTB=ON;              /* YES INDICATE SART BAD @Z40WI3F*/
         OI    VCTSARTB(VCCTREG),B'00010000'                    01S0275
*       ASMINIT=ON;                 /* ASM ININTIALIZATION COMPLETE
*                                                            @Z40WI3F*/
@RF00274 OI    ASMINIT,B'10000000'                              01S0276
*       SAVEBACK->SAVER15=0;        /* 0 RETURN CODE         @Z40WI3F*/
         L     @14,SAVEBACK(,R13)                               01S0277
         SLR   @10,@10                                          01S0277
         ST    @10,SAVER15(,@14)                                01S0277
*       GO TO ASMRET;               /* TAKE RETURN PATH      @Z40WI3F*/
         B     ASMRET                                           01S0278
*     END;                          /*                       @Z40WI3F*/
*   RESPECIFY                                                   01S0280
*     RETCODE UNRESTRICTED;         /*                       @Z40WI3F*/
@RF00258 DS    0H                                               01S0281
*                                                               01S0281
