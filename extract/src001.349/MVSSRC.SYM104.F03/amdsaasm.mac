*  * OF AN LSID TO A SEEK ADDRESS, DEVICE ADDRESS, AND CPU ADDRESS   */
*                                                               01S0281
*   IF BCTSWPDS=OFF THEN            /* ON SWAP OR PAGE DATA SET?
*                                                            @Z40WI3F*/
         L     @10,@CA01592                                     01S0281
         L     @09,VCTLPA(,VCCTREG)                             01S0281
         ALR   @10,@09                                          01S0281
         AL    @09,IODBBCT(,@10)                                01S0281
         TM    BCTSWPDS(@09),B'00001000'                        01S0281
         BNZ   @RF00281                                         01S0281
*     DO;                           /* PAGE D.S. - USE PART  @Z40WI3F*/
*       IF BCTINDEX>=PARTEUSE THEN  /* IF THE PART INDEX IS GREATER
*                                      THAN THE NUMBER OF PART  01S0283
*                                      ENTRIES IN USE, THEN THE PART
*                                      ENTRY CORRESPONDING TO THE
*                                      INDEX IS INVALID. NOTE THAT
*                                      INDICES RUN 0,1,2,3,... THUS,
*                                      WHEN PARTEUSE IS 1, THE ONLY
*                                      VALID INDEX IS 0. HENCE, THE
*                                      USE OF >= INSTEAD OF >   01S0283
*                                                            @Z40WI3F*/
         SLR   @10,@10                                          01S0283
         IC    @10,BCTINDEX(,@09)                               01S0283
         L     @09,CVTPTR                                       01S0283
         L     @09,ASMVTPTR(,@09)                               01S0283
         L     @09,PARTPTR(,@09)                                01S0283
         C     @10,PARTEUSE(,@09)                               01S0283
         BNL   @RT00283                                         01S0283
*         GOTO ASMEXIT;             /*                       @Z40WI3F*/
*       PAREPTR=ADDR(PARTENTS(BCTINDEX+1));/* COMPUTE PART ENTRY
*                                      ADDRESS               @Z40WI3F*/
         SLA   @10,6                                            01S0285
         LA    PAREPTR,PARTENTS(@10,@09)                        01S0285
*       IF PAREDSBD=ON THEN         /* DATA SET BAD          @Z40WI3F*/
         TM    PAREDSBD(PAREPTR),B'01000000'                    01S0286
         BO    @RT00286                                         01S0286
*         GOTO ASMEXIT;             /* YES - ERROR           @Z40WI3F*/
*       EDBPTR=PAREEDBP;            /* EDB ADDR FROM PART    @Z40WI3F*/
         L     EDBPTR,PAREEDBP(,PAREPTR)                        01S0288
*       UCBPTR=PAREUCBP;            /* UCB ADDR FROM PART    @Z40WI3F*/
         L     UCBPTR,PAREUCBP(,PAREPTR)                        01S0289
*     END;                          /*                       @Z40WI3F*/
*   ELSE                                                        01S0291
*     DO;                           /* PAGE ON SWAP DATA SET - USE
*                                      SART                  @Z40WI3F*/
         B     @RC00281                                         01S0291
@RF00281 DS    0H                                               01S0292
*       IF VCTSARTB=ON THEN         /* IS THE SART BAD?      @Z40WI3F*/
         TM    VCTSARTB(VCCTREG),B'00010000'                    01S0292
         BO    @RT00292                                         01S0292
*         GOTO ASMEXIT;             /* YES - ERROR           @Z40WI3F*/
*       IF BCTINDEX>=SARUSE THEN    /* IF THE SART INDEX IS GREATER
*                                      THAN THE NUMBER OF SART  01S0294
*                                      ENTRIES IN USE, THEN THE SART
