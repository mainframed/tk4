*/*AMDSAASM: CHART (SEQ)                                             */
*/*HEADER                                                       01S0233
*/*AMDSAASM   AUXILIARY STORAGE MANAGEMENT (ASM) INTERFACE ROUTINE
*/*                                                     PAGE #       */
*/*AMDSAASM: E AMDSAASM                                              */
*/* N EXECUTES IN MASTER MEMORY                                      */
*/* N RECEIVES NO INPUT                                              */
*/* P OBSERVE STANDARD LINKAGE CONVENTIONS (NO SAVE AREA)            */
*/* L  AMDSAGTM:  OBTAIN SAVE/WORK AREA                              */
*/* D (NO,,YES,%ASMSTOR) RC  =  0                                    */
*/* P SET RETURN CODE OF 4                                           */
*/* R RETURN TO CALLER                                               */
*/*%ASMSTOR: P INITIALIZE SAVE AREA                                  */
*/* P SAVE ORIGINAL BUFFER REQUEST                                   */
*/* P SET ASM BUFFER FLAGS                                           */
*/* P CREATE RCB IN WORK AREA                                        */
*/* P (NORM,,RCB,ASMEXIT) QUEUE RCB TO VCTRCB                        */
*                                                               01S0233
*   RESPECIFY                                                   01S0233
*     CCT BASED(VCTCCTV);                                       01S0233
*   RESPECIFY                                                   01S0234
*     RCB BASED(ADDR(ASMRCB));                                  01S0234
*   RESPECIFY                                                   01S0235
*     IODB BASED(ADDR(IODBDA)+VCTLPA);                          01S0235
*   RESPECIFY                                                   01S0236
*     BCT BASED(IODBBCT+VCTLPA);                                01S0236
*   RESPECIFY                                                   01S0237
*    (R1,                                                       01S0237
*     R8,                                                       01S0237
*     R13,                                                      01S0237
*     RETCODE) RESTRICTED;                                      01S0237
*   SVC(GTMSVC);                    /* CALL AMDSAGTM FOR STORAGE     */
         SVC   4                                                01S0238
*   IF RETCODE^=0 THEN              /* STORAGE OBTAINED              */
         LTR   RETCODE,RETCODE                                  01S0239
         BZ    @RF00239                                         01S0239
*     DO;                           /* NO - ERROR                    */
*       GENERATE REFS(R13,RETREG,RETCODE) SETS(R14,R12);        01S0241
         LM    R14,R12,12(R13)         RESTORE REGISTERS
         LA    RETCODE,4               SET ERROR RETURN CODE
         BR    RETREG                  RETURN TO CALLER
*     END;                                                      01S0242
*   R8=R1;                          /* SAVE ADDRESS OF STORAGE       */
@RF00239 LR    R8,R1                                            01S0243
*   GENERATE REFS(R8,R13) SETS(R15);                            01S0244
         USING @DATD,R8
         ST    R13,@SA00001+4         SAVE CALLERS SAVE AREA
         LA    R15,@SA00001           GET ADDRESS OF SAVE AREA
*   SAVENEXT=R15;                   /* LINK TO CALLERS SAVE AREA     */
         ST    R15,SAVENEXT(,R13)                               01S0245
*   R13=R15;                        /* MAKE SAVE AREA CURRENT        */
         LR    R13,R15                                          01S0246
*   SAVENAME='ASM';                 /* MODULE ID TO SAVE AREA        */
