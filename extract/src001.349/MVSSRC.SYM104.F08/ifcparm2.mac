*/*                                                          @Z427VLS*/
*/*  CONVERT THE HEXADECIMAL VALUES IN THE BYTES STRAIGHT TO @Z427VLS*/
*/*  CHARACTER REPRESENTATION.                               @Z427VLS*/
*/********************************************************************/
*UNPACK: PROC (OPLEN) OPTIONS(SAVE(1,14));                 /*@Z427VLS*/
UNPACK   ST    @14,@SA00005                                        0144
         ST    @01,@SA00005+4                                      0144
         MVC   @PC00005(4),0(@01)                                  0144
*      DCL OPLEN   FIXED(31);            /* NUMBER OF CHAR TO CONVERT*/
*      DO J = 1 TO OPLEN BY 1;                             /*@Z427VLS*/
         LA    J,1                                                 0146
         B     @DE00146                                            0146
@DL00146 DS    0H                                                  0147
*        HOLDAREA = HOLDAREA && HOLDAREA;  /* CHEAR HOLD AREA        */
         XC    HOLDAREA(5),HOLDAREA                                0147
*        HOLDAREA(2) = PARMFLD(J);       /* CHARACTERS ARE IN PARMFLD*/
         LA    @08,PARMFLD-1(J)                                    0148
         MVC   HOLDAREA+1(1),0(@08)                                0148
*        SLL (HOLDAREA(1:2),FOUR);       /* SHIFT FIRST TO FIRST BYTE*/
         SLR   @08,@08                                             0149
         ICM   @08,3,HOLDAREA                                      0149
         SLL   @08,4                                               0149
         STCM  @08,3,HOLDAREA                                      0149
*        SRL (HOLDAREA(2:2),FOUR);       /* SECOND TO SECOND BYTE    */
         SLR   @08,@08                                             0150
         IC    @08,HOLDAREA+1                                      0150
         SRL   @08,4                                               0150
         STC   @08,HOLDAREA+1                                      0150
*        INDEX3 = HOLDAREA(1) + 1;       /* GET VALUE AS INDEX       */
         LA    @08,1                                               0151
         SLR   INDEX3,INDEX3                                       0151
         IC    INDEX3,HOLDAREA                                     0151
         ALR   INDEX3,@08                                          0151
*        PARMPT(2*J+1) = NUMTAB(INDEX3);  /* STORE CHARACTER @Z427VLS*/
         LR    @07,J                                               0152
         ALR   @07,@07                                             0152
         LA    @03,PARMPT(@07)                                     0152
         LA    @01,NUMTAB-1(INDEX3)                                0152
         MVC   0(1,@03),0(@01)                                     0152
*        INDEX3 = HOLDAREA(2) + 1;       /* GET VALUE AS INDEX       */
         SLR   INDEX3,INDEX3                                       0153
         IC    INDEX3,HOLDAREA+1                                   0153
         ALR   @08,INDEX3                                          0153
         LR    INDEX3,@08                                          0153
*        PARMPT(2*J+2) = NUMTAB(INDEX3);  /* STORE CHARACTER @Z427VLS*/
         LA    @08,PARMPT+1(@07)                                   0154
         LA    @07,NUMTAB-1(INDEX3)                                0154
         MVC   0(1,@08),0(@07)                                     0154
*      END;                                                /*@Z427VLS*/
         AL    J,@CF00058                                          0155
@DE00146 L     @08,@PC00005                                        0155
         C     J,OPLEN(,@08)                                       0155
         BNH   @DL00146                                            0155
*      PRINTADR = ADDR(PARMPT);                            /*@Z427VLS*/
         LA    @08,PARMPT                                          0156
         ST    @08,PRINTADR(,R2)                                   0156
*      CALL IOHND(RITEPRTR);                               /*@Z427VLS*/
         L     @15,ADIOHND(,R2)                                    0157
         LA    @01,@AL00157                                        0157
         BALR  @14,@15                                             0157
*   END;                                                   /*@Z427VLS*/
@EL00005 DS    0H                                                  0158
@EF00005 DS    0H                                                  0158
@ER00005 L     @14,@SA00005                                        0158
         L     @01,@SA00005+4                                      0158
         BR    @14                                                 0158
