*/*              LOCATE EPTABLE                                     */
*/*******************************************************************/
*CEPTPTR = RPHSRPRM;                   /* EPTBL HDR ADDR FROM RPH    */
         L     CEPTPTR,RPHSRPRM(,RPHPTR)                           0030
*IF CEPTPTR = ZERO                     /* IS EPTABLE ADDR PROVIDED?  */
*   THEN DO;                           /* NO,FIND VIA MAJ. CTL. BLOCK*/
         LTR   CEPTPTR,CEPTPTR                                     0031
         BNZ   @RF00031                                            0031
*        CFMCPTR = RPHMAJCB;           /* LOCATE FMCB */           0033
         L     CFMCPTR,RPHMAJCB-1(,RPHPTR)                         0033
         LA    CFMCPTR,0(,CFMCPTR)                                 0033
*        CEPTPTR = FMCDVTA;            /* LOCATE EPTABLE HEADER */ 0034
         L     CEPTPTR,FMCDVTA(,CFMCPTR)                           0034
*        END;                                                      0035
*   ELSE;                                                          0036
@RF00031 DS    0H                                                  0037
*CEPTPTR = ADDR(EPTENTRY);            /* LOOK AT EPT ENTRY */      0037
         LA    @05,EPTENTRY(,CEPTPTR)                              0037
         LR    CEPTPTR,@05                                         0037
*/*******************************************************************/
*/*              SEARCH FOR NAME IN EPTABLE                         */
*/*******************************************************************/
*GENERATE SETS(CNAMPTR) REFS(EMCR0)  (LPR   CNAMPTR,EMCR0);/*GET NAME*/
         LPR   CNAMPTR,EMCR0
*CRETCODE = FOUR;                     /* RETURN CODE */            0039
         LA    CRETCODE,4                                          0039
*DO WHILE (EPTENDF = OFFB & CRETCODE = FOUR); /* SEARCH FOR NAME */
         B     @DE00040                                            0040
@DL00040 DS    0H                                                  0041
*   IF EPTNAME = CSRCHNAM             /* ARE NAMES EQUAL? */       0041
*      THEN DO;                       /* YES */                    0041
         CLC   EPTNAME(8,CEPTPTR),CSRCHNAM(CNAMPTR)                0041
         BNE   @RF00041                                            0041
*           RPHDVTA = EPTDVT;         /* PUT DVT ADDR IN RPH */    0043
         L     @05,EPTDVT(,CEPTPTR)                                0043
         ST    @05,RPHDVTA(,RPHPTR)                                0043
*           CRETCODE = ZERO;          /* INDICATE NAME FOUND */    0044
         SLR   CRETCODE,CRETCODE                                   0044
*           END;                                                   0045
*      ELSE CEPTPTR = CEPTPTR + LENGTH(ISTEPTE); /* NO,CHK NEXT NAME */
         B     @RC00041                                            0046
@RF00041 AL    CEPTPTR,@CF01092                                    0046
*   END;                                                           0047
@RC00041 DS    0H                                                  0047
@DE00040 TM    EPTENDF(CEPTPTR),B'10000000'                        0047
         BNZ   @DC00040                                            0047
         C     CRETCODE,@CF00052                                   0047
         BE    @DL00040                                            0047
@DC00040 DS    0H                                                  0048
*/*******************************************************************/
*/*                     RETURN TO USER OR CALL ESC                  */
*/*******************************************************************/
*IF REG0<0×CRETCODE=FOUR              /* RETURN TO CALLER ? @DM11264 */
*   THEN DO;                          /* YES */                    0048
         LTR   REG0,REG0                                           0048
         BM    @RT00048                                            0048
         C     CRETCODE,@CF00052                                   0048
         BNE   @RF00048                                            0048
@RT00048 DS    0H                                                  0049
