         BZ    @RF01093                                            1093
*                                      NCSVBF03=ON;/*SET VTAM TR BIT*/
         L     @08,NCSPLPTR                                        1095
         OI    NCSVBF03(@08),B'00100000'                           1095
*                                      NCSPLRID='VTAM';/*SET ID*/  1096
         MVI   NCSPLRID+4(@08),C' '                                1096
         MVC   NCSPLRID+5(3,@08),NCSPLRID+4(@08)                   1096
         MVC   NCSPLRID(4,@08),@CC03102                            1096
*                                      LOOPSW=ON;/*CONT      @DL32HBN*/
         OI    LOOPSW,B'00000010'                                  1097
*                                    END;   /*END VTAM TR    @DL32HBN*/
*                                                                  1098
*                                    ELSE DO;/*NOT VTAM TR   @X50BHYB*/
         B     @RC01093                                            1099
@RF01093 DS    0H                                                  1100
*                                     IF PHRF2VAL(1)=5  /*IS TYPE=SMS*/
*                                      THEN DO;   /*YES*/          1100
         L     @08,RPHSAVE                                         1100
         CLC   PHRF2VAL(4,@08),@CF00701                            1100
         BNE   @RF01100                                            1100
*                                       NCSVBF04=ON;  /*SET SMS BIT*/
         L     @08,NCSPLPTR                                        1102
         OI    NCSVBF04(@08),B'00010000'                           1102
*                                       IDREQ=ON;   /*SET ID REQRD*/
*                                       LOOPSW=ON; /*TO CONTINUE SCAN*/
         OI    IDREQ,B'00000011'                                   1104
*                                       END;      /*END TYPE=SMS*/ 1105
*                                                                  1105
*                                      ELSE DO;       /*NOT SMS TRACE*/
         B     @RC01100                                            1106
@RF01100 DS    0H                                                  1107
*                                       TYPESPC=OFF;/*NO VALID TYPE*/
         NI    TYPESPC,B'10111111'                                 1107
*                                       MSGCODE='043I';/*SET MSGID*/
         MVC   MSGCODE(4),@CC03496                                 1108
*                                       MSGTXT(9:16)='TYPE    ';   1109
         MVC   MSGTXT+8(8),@CC03448                                1109
*                                       CALL MSGS; /*CALL MSG RTN*/
         BAL   @14,MSGS                                            1110
*                                    END;       /*END NOT TR @DL32HBN*/
*                                 END;    /*TYPE NOT VTAM TR @X50BHYB*/
@RC01100 DS    0H                                                  1113
*                              END;          /*END TYPE NOT LINE*/ 1113
@RC01093 DS    0H                                                  1114
*                        END;                /*END TYPE NOT BUF*/  1114
@RC01086 DS    0H                                                  1115
*                  END;                      /*END TYPE NOT IO*/   1115
@RC01079 DS    0H                                                  1116
*      END;                            /*END NO PRIOR TYPE*/       1116
@RC01072 DS    0H                                                  1117
*   END;                                                   /*@X50BHEE*/
@RC01065 DS    0H                                                  1118
*   R14=R14SVKWD;                      /*RESTORE RETURN REG  @D32CKBN*/
@RC01056 L     R14,R14SVKWD                                        1118
*END TYPRTN;                                                       1119
