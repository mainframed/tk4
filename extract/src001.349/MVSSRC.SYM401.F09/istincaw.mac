         OI    NCSVBF07(@08),B'00000010'                           1081
*                           IDREQ=ON; /*SET ID REQRD         @DL32HBN*/
*                           LOOPSW=ON;       /*CONTINUE SCAN*/     1083
         OI    IDREQ,B'00000011'                                   1083
*                        END;                /*END TYPE=BUF*/      1084
*                        ELSE DO;            /*NOT TYPE=BUF*/      1085
         B     @RC01079                                            1085
@RF01079 DS    0H                                                  1086
