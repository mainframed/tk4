*                                    ATCDVTPT^=0 /*AND TR ALLOWED*/
*                                    THEN DO;  /*VALID VTAM TR*/   1093
         L     @08,RPHSAVE                                         1093
         CLC   PHRF2VAL(4,@08),@CF00055                            1093
         BNE   @RF01093                                            1093
         L     @08,ISTACVT(,AVTPTR)                                1093
         L     @08,ATCDVTPT(,@08)                                  1093
