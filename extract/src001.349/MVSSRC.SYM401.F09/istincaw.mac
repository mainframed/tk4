*/*             TYPRTN:  THIS ROUTINE HANDLES THE TYPE PARM OF THE   */
*/*                      TRACE COMMAND.  IT TEST FOR MULTIPLE TYPE   */
*/*                      PARM SPECIFIED AND ISSUES A MSG IF SO.      */
*/*                      OTHERWISE IT TEST FOR A VALID TYPE SPECIFIED*/
*/*                      AND SETS THE APPROPRIATE BIT IN THE NCSPL   */
*/*                      OR ISSUES A MSG IF THE TYPE IS NOT VALID.   */
*/*                      IT ALSO TURNS ON THE TYPE SPECIFIED BIT.    */
*/*                                                                  */
*/********************************************************************/
*                                                                  1054
*TYPRTN: PROC OPTIONS(NOSAVE,NOSAVEAREA);                          1054
TYPRTN   DS    0H                                                  1055
*   R14SVKWD=R14;                      /*SAVE REG 14         @D32CKBN*/
         ST    R14,R14SVKWD                                        1055
*   IF PHRF2NVL > 1                                        /*@X50BHEE*/
*      THEN DO;                                            /*@X50BHEE*/
         L     @08,RPHSAVE                                         1056
         L     @08,PHRF2NVL(,@08)                                  1056
         C     @08,@CF00063                                        1056
         BNH   @RF01056                                            1056
*        MSGCODE='403I';                                   /*@X50BHEE*/
         MVC   MSGCODE(4),@CC03494                                 1058
*        MSGTXT(9:16)='TYPE    ';                          /*@X50BHEE*/
         MVC   MSGTXT+8(8),@CC03448                                1059
*        CALL MSGS;                                        /*@X50BHEE*/
         BAL   @14,MSGS                                            1060
*      END;                                                /*@X50BHEE*/
*    ELSE DO;                                              /*@X50BHEE*/
         B     @RC01056                                            1062
@RF01056 DS    0H                                                  1063
*   TYPESPC=ON;                        /*INIT TYPE SPECIFIED ON*/  1063
*   TRREQ=ON;                          /*REQUIRES TRACE KWD*/      1064
         OI    TYPESPC,B'01001000'                                 1064
*   IF NCSVBF05=ON ×                   /*PRIOR TYPE=LINE */        1065
*      NCSVBF04=ON ×                   /*OR PRIOR TYPE=SMS   @X50BHYB*/
*      NCSVBF06=ON ×                   /*OR PRIOR TYPE=IO*/        1065
*      NCSVBF07=ON                     /*OR PRIOR TYPE=BUF*/       1065
*      THEN DO;                        /*YES PRIOR TYPE*/          1065
         L     @08,NCSPLPTR                                        1065
         TM    NCSVBF05(@08),B'00011110'                           1065
         BZ    @RF01065                                            1065
*         MSGCODE='096I';              /*SET ID FOR MULTI MSG*/    1067
         MVC   MSGCODE(4),@CC03495                                 1067
*         MSGTXT(9:16)='TYPE    ';     /*SET MSG TEXT*/            1068
         MVC   MSGTXT+8(8),@CC03448                                1068
*         CALL MSGS;                   /*CALL MSG ROUTINE*/        1069
         BAL   @14,MSGS                                            1069
*      END;                            /*END PRIOR TYPE*/          1070
*      ELSE DO;                        /*NO PRIOR TYPE*/           1071
         B     @RC01065                                            1071
@RF01065 DS    0H                                                  1072
*               IF PHRF2VAL(1)=1             /*IS IT TYPE=IO*/     1072
*                  THEN DO;                  /* YES */             1072
         L     @08,RPHSAVE                                         1072
         CLC   PHRF2VAL(4,@08),@CF00063                            1072
         BNE   @RF01072                                            1072
*                     NCSVBF06=ON;           /*SET IO BIT*/        1074
         L     @08,NCSPLPTR                                        1074
         OI    NCSVBF06(@08),B'00000100'                           1074
*                     IDREQ=ON; /*SET ID REQUIRED            @DL32HBN*/
*                     LOOPSW=ON;             /*CONTINUE SCAN*/     1076
         OI    IDREQ,B'00000011'                                   1076
*                  END;                      /*END TYPE=IO*/       1077
*                  ELSE DO;                  /*NOT TYPE=IO*/       1078
         B     @RC01072                                            1078
@RF01072 DS    0H                                                  1079
*                     IF PHRF2VAL(1)=2       /*IS IT TYPE=BUF*/    1079
*                        THEN DO;            /* YES */             1079
         L     @08,RPHSAVE                                         1079
