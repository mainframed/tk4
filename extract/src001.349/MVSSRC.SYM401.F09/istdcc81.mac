*/*  TPQUE (FSBPTR,FMCPAB2) NORM */                                0996
*                                                                  0996
*             DO;                                                  0996
*             RFY (GPR00P,GPR01P,GPR04P,GPR05P,GPR15P) RSTD;       0997
*             GEN REFS(GPR01P) SETS(RPHWORK)                       0998
*        (STM GPR02P,GPR08P,RPHWORK(GPR01P));                      0998
         STM GPR02P,GPR08P,RPHWORK(GPR01P)
*             GPR00P=0;                                            0999
         SLR   GPR00P,GPR00P                                       0999
*             GPR15P=ADDR(FMCPAB2); /*HOLD PAB PTR*/               1000
         L     GPR15P,CRRFMCB(,CRRPTR)                             1000
         LA    GPR15P,FMCPAB2(,GPR15P)                             1000
*             GPR04P=FSBPTR; /*WORK ELE PTR*/                      1001
         LR    GPR04P,FSBPTR                                       1001
*             GPR05P=GPR15P; /*PASS PAB PTR*/                      1002
         LR    GPR05P,GPR15P                                       1002
*             CALL ATCLCPTR->ATCAP37->ISTAPC37;                    1003
         L     @10,ATCLCPTR                                        1003
         L     @15,ATCAP37(,@10)                                   1003
         BALR  @14,@15                                             1003
*             GEN(NOPR  0)NOSETS NOREFS;                           1004
         NOPR  0
*             RFY (GPR00P,GPR01P,GPR04P,GPR05P,GPR15P) UNRSTD;     1005
*             END; /**** END OF TPQUE GENERATION ****/  /* QUEUE FSB TO
*CLIP */                                                           1006
*            CRRSW0=ON;                    /* SCHEDULED LCPB          *
         OI    CRRSW0(CRRPTR),B'10000000'                          1007
*          RFY ISTDCLCP BASED(CURRPTRB);                           1008
*                                                                  1008
*/* TPDEQ (,LCPRSTQH) RETPTR(WRKREGD) */                           1009
*           DO; /* BEGIN TPDEQ CODE GENERATION */                  1009
*           RFY (GPR00P,GPR01P,GPR14P,GPR15P) RSTD;                1010
*                                                                  1010
*           GPR15P=ADDR(LCPRSTQH); /*ADDRESS QUEUE POINTER */      1011
         L     GPR15P,CURRPTRB(,CRRPTR)                            1011
         LA    GPR15P,LCPRSTQH(,GPR15P)                            1011
*           GPR14P=GPR15P->PABWEQA×FMZCA108;                       1012
         L     @10,@CF01149                                        1012
         L     GPR14P,PABWEQA(,GPR15P)                             1012
         OR    GPR14P,@10                                          1012
*           IF GPR14P=FMZCA108 THEN;                               1013
         CLR   GPR14P,@10                                          1013
         BE    @RT01013                                            1013
*           ELSE DO;                                               1015
*FMZL1198:;                                                        1016
FMZL1198 DS    0H                                                  1017
*             DO WHILE(GPR14P->WKECHAIN^=FMZCA108);                1017
         B     @DE01017                                            1017
@DL01017 DS    0H                                                  1018
*               GPR15P=ADDR(GPR14P->WKECHAIN);                     1018
         LA    GPR15P,WKECHAIN(,GPR14P)                            1018
*               GPR14P=GPR15P->PABWEQA;                            1019
         L     GPR14P,PABWEQA(,GPR15P)                             1019
*             END;                                                 1020
@DE01017 L     @10,@CF01149                                        1020
         CL    @10,WKECHAIN(,GPR14P)                               1020
         BNE   @DL01017                                            1020
*             GPR00P=FMZCA108;                                     1021
         LR    GPR00P,@10                                          1021
*             CS(GPR14P,GPR00P,GPR15P->PABWEQA);                   1022
         CS    GPR14P,@00,PABWEQA(GPR15P)                          1022
*             BC(7,FMZL1198);                                      1023
         BC    7,FMZL1198                                          1023
*             GPR14P->WKECHAIN=0;                                  1024
         SLR   @10,@10                                             1024
         ST    @10,WKECHAIN(,GPR14P)                               1024
*           END;                                                   1025
*ISTA0198:;                                                        1026
@RT01013 DS    0H                                                  1026
ISTA0198 DS    0H                                                  1027
*           GPR14P=GPR14P&FMZCB108;                                1027
         N     GPR14P,@CF01147                                     1027
*           WRKREGD=GPR14P;                                        1028
         LR    WRKREGD,GPR14P                                      1028
*           RFY (GPR00P,GPR01P,GPR14P,GPR15P) UNRSTD;              1029
*           END; /* END TPDEQ GENERATION */  /* GET LAST ELEMENT*/ 1030
*          END;                            /*END-ELEMENT NOT RPL      *
*          END;                            /* END-INNER DO WHILE      *
@RC00933 DS    0H                                                  1032
@DE00932 LTR   WRKREGD,WRKREGD                                     1032
         BZ    @DC00932                                            1032
         CL    WRKREGD,@CF01149                                    1032
         BNE   @DL00932                                            1032
@DC00932 DS    0H                                                  1033
*         END;                             /* END-NOT PURGE LCCW      *
*      END;                                /* END- OUTER DO WHILE     *
@RC00765 DS    0H                                                  1034
@DE00755 TM    CRRSW1(CRRPTR),B'01000000'                          1034
         BZ    @DL00755                                            1034
*     CURRPTRB=QUEPTRA;                    /* SET CURRENT PTR TO     *
*                                           *HEAD OF PURGE RESET QUE  *
         L     @15,QUEPTRA(,CRRPTR)                                1035
         ST    @15,CURRPTRB(,CRRPTR)                               1035
*     RFY ISTDCLCP BASED(QUEPTRA);         /* COVER HEAD OF RESET Q*/
*                                                                  1036
*/* TPFEL (,LCPRSTQH) RETPTR(WRKREGD) */                           1037
*      DO; /* BEGIN TPFEL CODE GENERATION */                       1037
*      RFY (GPR00P,GPR01P,GPR14P,GPR15P) RSTD;                     1038
*                                                                  1038
*      GPR15P=ADDR(LCPRSTQH); /*ADDRESS QUEUE POINTER */           1039
         L     @10,QUEPTRA(,CRRPTR)                                1039
         LA    GPR15P,LCPRSTQH(,@10)                               1039
*      GPR14P=GPR15P->PABWEQA×FMZCA108;                            1040
         L     @10,@CF01149                                        1040
         L     GPR14P,PABWEQA(,GPR15P)                             1040
         OR    GPR14P,@10                                          1040
*      IF GPR14P=FMZCA108 THEN;                                    1041
         CLR   GPR14P,@10                                          1041
         BE    @RT01041                                            1041
*      ELSE DO;                                                    1043
*      GPR00P=FMZCA108; /* INITIALIZE QUIT FLAG */                 1044
         L     GPR00P,@CF01149                                     1044
*      DO WHILE (GPR00P ^= GPR14P->WKECHAIN);                      1045
         B     @DE01045                                            1045
@DL01045 DS    0H                                                  1046
*         GPR14P=GPR14P->WKECHAIN; /* SEARCH FOR END OF CHAIN */   1046
         L     GPR14P,WKECHAIN(,GPR14P)                            1046
*         END;                                                     1047
@DE01045 C     GPR00P,WKECHAIN(,GPR14P)                            1047
         BNE   @DL01045                                            1047
*      END;                                                        1048
*      GPR14P=GPR14P&FMZCB108;                                     1049
@RT01041 N     GPR14P,@CF01147                                     1049
*      WRKREGD=GPR14P;                                             1050
         LR    WRKREGD,GPR14P                                      1050
*      RFY (GPR00P,GPR01P,GPR14P,GPR15P) UNRSTD;                   1051
*      END; /* END TPFEL GENERATION */ /* GET ADDR LAST ELT ON Q */
*     CRRSW1=OFF;                         /* RESET LOOP CONTROL SWITCH*
         NI    CRRSW1(CRRPTR),B'10111111'                          1053
*     RTNREG=CRRRTN4;                     /* RESTORE RETURN ADDRESS */
         L     RTNREG,CRRRTN4(,CRRPTR)                             1054
*     RETURN;                                                      1055
@EL00005 DS    0H                                                  1055
@EF00005 DS    0H                                                  1055
@ER00005 BR    @14                                                 1055
*END  CLNPURGQ;                                                    1056
*POSTELEM: PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA,NOPARMREG);         1057
POSTELEM DS    0H                                                  1058
*/********************************************************************/
*/*                                                                  */
*/* ROUTINE NAME :  POSTELEM - POST ELEMENTS ON LCPB RESET QUEUE     */
*/*                                                                  */
*/* FUNCTION :                                                       */
*/*    POSTELEM EXAMINES THE LCPB'S PURGE RESET QUEUE AND CALLS      */
*/*    CLNPURGQ TO POST ANY ELEMENTS ON THE QUEUE. IF THE            */
*/*    CURRENT RPL IS BEING OR HAS BEEN SOFT PURGED, THE             */
*/*    CLOP ERROR LOCK IS RELEASED AND THE CLOP IS SCHEDULED.        */
*/*                                                                  */
*/* INPUT :  FSBPTR (REG 6) - ADDRESS OF FSB                         */
*/*          LCPBPTR (REG 4) - REQUESTOR'S PURGE LCPB                */
*/*                                                                  */
*/* OUTPUT :  LCPB RESET QUEUE ELEMENTS POSTED                       */
*/*                                                                  */
*/* SUBROUTINES CALLED :                                             */
*/*                      ISTAICPT - POST USER RPL COMPLETE           */
*/*                      CLNPURGQ - CLEAN OFF RESET QUEUE            */
*/*                                                                  */
*/*  MACROS USED :  TPFEL, TPSCHED                                   */
*/*                                                                  */
*/********************************************************************/
*     CRRRTN5=RTNREG;                      /* SAVE RETURN REG         *
         ST    RTNREG,CRRRTN5(,CRRPTR)                             1058
*     CRRSW0=OFF;                          /* INITIALIZE LOOP CONTROL *
*                                           *SWITCHES                 *
*     CRRSW1=OFF;                                                  1060
*     CRRSW2=OFF;                                                  1061
         NI    CRRSW0(CRRPTR),B'00011111'                          1061
*     RFY IFGRPL BASED(RPLPTR);            /* COVER RPL          */
*     QUEPTRA=CRRARPL;                     /* POINT TO HEAD OF LCPB Q*/
         L     @15,CRRARPL(,CRRPTR)                                1063
         ST    @15,QUEPTRA(,CRRPTR)                                1063
*     CURRPTRB=QUEPTRA;                    /*MAKE IT CURRENT LCPB     *
         ST    @15,CURRPTRB(,CRRPTR)                               1064
*     RFY ISTDCLCP BASED(QUEPTRA);                                 1065
*                                                                  1065
*/* TPFEL (,LCPRSTQH) RETPTR(WRKREGD) */                           1066
*      DO; /* BEGIN TPFEL CODE GENERATION */                       1066
*      RFY (GPR00P,GPR01P,GPR14P,GPR15P) RSTD;                     1067
*                                                                  1067
*      GPR15P=ADDR(LCPRSTQH); /*ADDRESS QUEUE POINTER */           1068
         L     @10,QUEPTRA(,CRRPTR)                                1068
         LA    GPR15P,LCPRSTQH(,@10)                               1068
*      GPR14P=GPR15P->PABWEQA×FMZCA108;                            1069
         L     @10,@CF01149                                        1069
         L     GPR14P,PABWEQA(,GPR15P)                             1069
         OR    GPR14P,@10                                          1069
*      IF GPR14P=FMZCA108 THEN;                                    1070
         CLR   GPR14P,@10                                          1070
         BE    @RT01070                                            1070
*      ELSE DO;                                                    1072
*      GPR00P=FMZCA108; /* INITIALIZE QUIT FLAG */                 1073
         L     GPR00P,@CF01149                                     1073
*      DO WHILE (GPR00P ^= GPR14P->WKECHAIN);                      1074
         B     @DE01074                                            1074
@DL01074 DS    0H                                                  1075
*         GPR14P=GPR14P->WKECHAIN; /* SEARCH FOR END OF CHAIN */   1075
         L     GPR14P,WKECHAIN(,GPR14P)                            1075
*         END;                                                     1076
@DE01074 C     GPR00P,WKECHAIN(,GPR14P)                            1076
         BNE   @DL01074                                            1076
*      END;                                                        1077
*      GPR14P=GPR14P&FMZCB108;                                     1078
@RT01070 N     GPR14P,@CF01147                                     1078
*      WRKREGD=GPR14P;                                             1079
         LR    WRKREGD,GPR14P                                      1079
*      RFY (GPR00P,GPR01P,GPR14P,GPR15P) UNRSTD;                   1080
*      END; /* END TPFEL GENERATION */                             1081
*     DO WHILE WRKREGD^=ZERO;              /* DO WHILE Q NOT EMPTY    *
         B     @DE01082                                            1082
@DL01082 DS    0H                                                  1083
*       CALL CLNPURGQ;                     /* CLEAN OFF PURGED ELEMTS*/
         BAL   @14,CLNPURGQ                                        1083
*       END;                               /* END DO WHILE            *
@DE01082 LTR   WRKREGD,WRKREGD                                     1084
         BNZ   @DL01082                                            1084
*     IF FMCVSP2=ON                        /* IF RPL WAS SOFT PURGED*/
*        THEN DO;                          /* GET RPL FROM FMCB       *
         L     @15,CRRFMCB(,CRRPTR)                                1085
         TM    FMCVSP2(@15),B'10000000'                            1085
         BNO   @RF01085                                            1085
*        RPLPTR=FMCRPLH;                   /* GET RPL                 *
         L     RPLPTR,FMCRPLH(,@15)                                1087
*        RPLRTNCD='1C'X;                   /*SET PURGE RTN CODE       *
         MVI   RPLRTNCD(RPLPTR),X'1C'                              1088
*        RPLFDB2=FRPARSSL;                 /*INDICATE SOFT PURGE      *
         MVI   RPLFDB2(RPLPTR),X'81'                               1089
*        RPLUSFLD=FMCUSFLD;                /* GET USER INFO   @XM09597*
         MVC   RPLUSFLD(4,RPLPTR),FMCUSFLD(@15)                    1090
*        RPHWEA=RPLPTR;                    /*RPL TO RPH FOR POST      *
         ST    RPLPTR,RPHWEA(,RPHPTR)                              1091
*        CALL ATCAPOST;                    /*CALL USER POST RTN       *
         L     @10,CRRATCVT(,CRRPTR)                               1092
         L     @15,ATCAPOST(,@10)                                  1092
         BALR  @14,@15                                             1092
*        ADDR(FMCPAB3)->PABERRLK=ON;       /*TURN OF ERR LOCK IN TOP */
         L     @15,CRRFMCB(,CRRPTR)                                1093
         OI    PABERRLK+96(@15),B'00000100'                        1093
*        END;                              /*END-RPL SOFT PRGED       *
*     ELSE;                                /*RPL NOT SOFT PRGED      */
@RF01085 DS    0H                                                  1096
*     IF FMCVSP1=ON × FMCVSP2=ON           /*IF SOFT PRG IN PROG      *
*        THEN DO;                          /* OR RPL IS SOFT PURGED   *
         L     @15,CRRFMCB(,CRRPTR)                                1096
         TM    FMCVSP1(@15),B'00000001'                            1096
         BO    @RT01096                                            1096
         TM    FMCVSP2(@15),B'10000000'                            1096
         BNO   @RF01096                                            1096
@RT01096 DS    0H                                                  1097
*        ADDR(FMCPAB1)->PABERRLK=OFF;      /*TURN OFF ERR LOCK(CLOP)*/
         L     @15,CRRFMCB(,CRRPTR)                                1098
         NI    PABERRLK+64(@15),B'11111011'                        1098
*        FMCVSP1=OFF;                      /*CLEAR SOFT PURGE FLAGS*/
         NI    FMCVSP1(@15),B'11111110'                            1099
*        FMCVSP2=OFF;                                              1100
         NI    FMCVSP2(@15),B'01111111'                            1100
*        FMCRPLH=ZERO;                     /*CLEAR RPL PTR IN FMCB    *
         SLR   @10,@10                                             1101
         ST    @10,FMCRPLH(,@15)                                   1101
*        RFY REG6  RSTD;                                           1102
*        REG6=ADDR(FMCPAB1);                                       1103
*                                                                  1103
         LA    REG6,FMCPAB1(,@15)                                  1103
*/* TPSCHED (REG6)  */                                             1104
*                                                                  1104
*         DO;                                                      1104
*         RFY (GPR00P,GPR01P,GPR05P) RSTD;                         1105
*         GEN REFS(GPR01P) SETS(RPHWORK)                           1106
*         (STM   GPR02P,GPR08P,RPHWORK(GPR01P));                   1106
         STM   GPR02P,GPR08P,RPHWORK(GPR01P)
*         GPR00P=0;                                                1107
         SLR   GPR00P,GPR00P                                       1107
*         GPR05P = REG6;                                           1108
         LR    GPR05P,REG6                                         1108
*         CALL ATCLCPTR->ATCAP38->ISTAPC38;                        1109
         L     @10,ATCLCPTR                                        1109
         L     @15,ATCAP38(,@10)                                   1109
         BALR  @14,@15                                             1109
*         GEN(NOPR 0)NOSETS NOREFS;                                1110
         NOPR 0
*         RFY (GPR00P,GPR01P,GPR05P) UNRSTD;                       1111
*         END; /***** END TPSCHED *****/                  /* SCHEDULE C
*L OUTBOUND*/                                                      1112
*        RFY REG6 UNRSTD;                                          1113
*        END;                              /*ENDDSOFT PURG IN PROG*/
*     ELSE;                                /*SOFT PRG NOT IN PROG*/
@RF01096 DS    0H                                                  1116
*     RTNREG=CRRRTN5;                      /* RESTORE RETURN REG*/ 1116
         L     RTNREG,CRRRTN5(,CRRPTR)                             1116
*     RETURN;                                                      1117
@EL00006 DS    0H                                                  1117
@EF00006 DS    0H                                                  1117
@ER00006 BR    @14                                                 1117
*END POSTELEM;                                                     1118
*/********************************************************************/
*/*                                                                  */
*/* ROUTINE NAME :  XFRFSB  - TRANSFER FSB TO PAGEABLE STORAGE       */
*/*                                                                  */
*/* FUNCTION :                                                       */
*/*    THE FSB/FDB CHAIN IS MOVED TO PAGEABLE STORAGE VIA CALL       */
*/*    TO ISTDCC61(MOVE ROUTINE). THE NUMBER OF FDB'S IN THE         */
*/*    CHAIN IS ADDED TO THE BUFFER COUNT IN THE FMCB. THE FSB IS    */
*/*    QUEUED TO FMCB DATA QUEUE.                                    */
*/*                                                                  */
*/* INPUT :  FSBPTR (REG 6) - ADDRESS OF FSB                         */
*/*          LCPBPTR (REG 4) - REQUESTOR'S PURGE LCPB                */
*/*                                                                  */
*/* OUTPUT :  A PAGEABLE FSB QUEUED TO FMCB DATA QUEUE               */
*/*                                                                  */
*/* SUBROUTINES CALLED :                                             */
*/*                      ISTDCC61 - MOVE FSB TO PAGEABLE STORAGE     */
*/*                                                                  */
*/*  MACROS USED :  TPQUE,                                           */
*/*                                                                  */
*/********************************************************************/
*XFRFSB: PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA,NOPARMREG);           1119
*                                                                  1119
XFRFSB   DS    0H                                                  1120
*     CRRRTN6=RTNREG;                      /* SAVE RETURN ADDRESS     *
         ST    RTNREG,CRRRTN6(,CRRPTR)                             1120
*     FSBELI=OFF;                          /* TURN OFF END DATA       *
*     FSBECI=OFF;                          /* TURN OFF END COMMUN.    *
         NI    FSBELI(FSBPTR),B'00111111'                          1122
*     RFY CRRSAREA BASED(ADDR(CRRSPACE));                          1123
*     CRRPTR=ADDR(CRRSAVE1);               /* POINT TO EXT SAVE AREA  *
         LA    @15,324                                             1124
         AL    @15,RPHCRR(,RPHPTR)                                 1124
         LA    CRRPTR,CRRSAVE1(,@15)                               1124
*     CALL ATCDCC61;                   /* CALL MOVE ROUTINE          */
         L     @10,CRRATCVT(,@15)                                  1125
         L     @15,ATCDCC61(,@10)                                  1125
         BALR  @14,@15                                             1125
*     CRRPTR=ADDR(CRRSPACE);                /* ORIGINAL CRR COVER    */
         L     CRRPTR,RPHCRR(,RPHPTR)                              1126
         LA    CRRPTR,CRRSPACE+312(,CRRPTR)                        1126
*     RFY CRRSAREA BASED(CRRPTR);                                  1127
*     FSBPTR=CRRFSB;               /* GET NEW (PAGEABLE) FSB ADDRESS */
         L     FSBPTR,CRRFSB(,CRRPTR)                              1128
*     RFY COUNTER RSTD;                                            1129
*     COUNTER=ZERO;                        /* TO COUNT FDS'IN CHAIN   *
         SLR   COUNTER,COUNTER                                     1130
*     RFY FDBREG RSTD;                                             1131
*     FDBREG=ADDR(FSBFDB);                 /*ADDRESS FIRST FDB        *
         LA    FDBREG,FSBFDB(,FSBPTR)                              1132
*     DO WHILE FDBFDBA^=0;                 /* UNTIL THIS FDB IS LAST  *
         B     @DE01133                                            1133
@DL01133 DS    0H                                                  1134
*       COUNTER =COUNTER+1;                /* COUNT THIS FDB          *
         AL    COUNTER,@CF00062                                    1134
*       FDBREG=FDBFDBA;                    /* GET THE NEXT ONE        *
         L     FDBREG,FDBFDBA(,FDBREG)                             1135
*       END;                                                       1136
@DE01133 L     @15,FDBFDBA(,FDBREG)                                1136
         LTR   @15,@15                                             1136
         BNZ   @DL01133                                            1136
*     RFY FDBREG UNRSTD;                                           1137
*     COUNTER=COUNTER+1;                   /* COUNT THE LAST FDB      *
         AL    COUNTER,@CF00062                                    1138
*     RFY (REG3,REG6) RSTD;                                        1139
*     REG3=FMCCMPS1;                       /* GET OLD VALUE OF COUNTER*
         L     @15,CRRFMCB(,CRRPTR)                                1140
         L     REG3,FMCCMPS1(,@15)                                 1140
*     REG6=REG3+COUNTER;                   /* ADD IN NEW BUFFERS      *
         LR    REG6,REG3                                           1141
         ALR   REG6,COUNTER                                        1141
*C81: CS(REG3,REG6,FMCCMPS1);              /* STORE NEW VALUE         *
C81      L     @15,CRRFMCB(,CRRPTR)                                1142
         CS    REG3,@06,FMCCMPS1(@15)                              1142
*     BC (7,C81);                                                  1143
         BC    7,C81                                               1143
*     RFY (REG3,REG6,COUNTER) UNRSTD;                              1144
*     FSBPTR=CRRFSB;          /* RESTORE FSB ADDRESS (R6 USED IN C&S) *
*                                                                  1145
         L     FSBPTR,CRRFSB(,CRRPTR)                              1145
*/*  TPQUE (FSBPTR,FMCFBA)  */                                     1146
*                                                                  1146
*      DO;                                                         1146
*      RFY (GPR00P,GPR01P,GPR02P,GPR14P,GPR15P) RSTD;              1147
*      GEN REFS(GPR01P) SETS(RPHWORK)                              1148
*        (STM GPR02P,GPR08P,RPHWORK(GPR01P));                      1148
         STM GPR02P,GPR08P,RPHWORK(GPR01P)
*      GPR15P=FSBPTR; /* GET WORK ELEM POINTER */                  1149
         LR    GPR15P,FSBPTR                                       1149
*      GPR14P=ADDR(FMCFBA); /* ADDRESS PAB/QUEUE POINTER */        1150
         L     GPR14P,CRRFMCB(,CRRPTR)                             1150
         LA    GPR14P,FMCFBA(,GPR14P)                              1150
*      GPR15P=GPR15P×PABWEQG1;                                     1151
         O     GPR15P,@CF01149                                     1151
*      GPR02P=GPR14P->PABWEQA;/* INITIALIZE WITH QUEUE HEADER */   1152
         L     GPR02P,PABWEQA(,GPR14P)                             1152
*CMZA0216:; /* CS FAIL TARGET */                                   1153
CMZA0216 DS    0H                                                  1154
*      GPR00P=GPR02P; /* SAVE QUEUE CONTENTS */                    1154
         LR    GPR00P,GPR02P                                       1154
*      GPR15P->PABCHAIN=GPR02P×PABWEQG1; /*FORWARD CHAIN WORK ELEMENT*/
         LR    @10,GPR02P                                          1155
         O     @10,@CF01149                                        1155
         ST    @10,PABCHAIN(,GPR15P)                               1155
*      CS   (GPR02P,GPR15P,GPR14P->PABWEQA); /* CHECK FOR END OF QUEUE
*/     BC (7,CMZA0216);                                            1156
         CS    GPR02P,@15,PABWEQA(GPR14P)                          1156
         BC    7,CMZA0216                                          1157
*      GEN REFS(RPHWORK,GPR01P) NOSETS                             1158
*        (LM    GPR02P,GPR08P,RPHWORK(GPR01P));                    1158
         LM    GPR02P,GPR08P,RPHWORK(GPR01P)
*      RFY (GPR00P,GPR01P,GPR02P,GPR14P,GPR15P) UNRSTD;            1159
*      END; /**** END OF TPQUE GENERATION ****/                    1160
*     RTNREG=CRRRTN6;                      /* RESTORE RETURN ADDRESS  *
         L     RTNREG,CRRRTN6(,CRRPTR)                             1161
*     RETURN;                                                      1162
@EL00007 DS    0H                                                  1162
@EF00007 DS    0H                                                  1162
@ER00007 BR    @14                                                 1162
*END    XFRFSB;                                                    1163
*/********************************************************************/
*/*                                                                  */
*/* ROUTINE NAME :  LSTRMEXT - SCHEDULE LOST TERMINAL EXIT           */
*/*                                                                  */
*/* FUNCTION :                                                       */
*/*    IF THIS IS NOT A VARY SOFT PURGE, LSTRMEXT BUILDS A UECB AND  */
*/*    SCHEDULES A LOST TERMINAL EXIT.                               */
*/*                                                                  */
*/* INPUT :  FSBPTR (REG 6) - ADDRESS OF FSB                         */
*/*          LCPBPTR (REG 4) - REQUESTOR'S PURGE LCPB                */
*/*                                                                  */
*/* OUTPUT :  LOST TERMINAL EXIT MAY BE SCHEDULED                    */
*/*                                                                  */
*/* SUBROUTINES CALLED : NONE                                        */
*/*                                                                  */
*/*  MACROS USED :  TPQUE, REQSTORE, TPWAIT                          */
*/*                                                                  */
*/********************************************************************/
*LSTRMEXT:  PROC OPTIONS(NOSAVE,NOSAVEAREA,NOPARMREG);             1164
LSTRMEXT DS    0H                                                  1165
*     CRRRTN7=RTNREG;                      /* SAVE RETURN ADDRESS     *
         ST    RTNREG,CRRRTN7(,CRRPTR)                             1165
*     IF FMCVSP1=ON                       /* VARY SOFT PURGE COND.   */
*        THEN ;                            /* RETURN-NO SCHED.        *
         L     @15,CRRFMCB(,CRRPTR)                                1166
         TM    FMCVSP1(@15),B'00000001'                            1166
         BO    @RT01166                                            1166
*     ELSE DO;                             /* NOT VARY SOFT PRG       *
*        RFY (UECBPTR,RCREG) RSTD;                                 1169
* /*REQSTORE CBID(UECB)RPH(RPHPTR)CLEAR(YES)SAVE(YES)PRIORITY(YES)*/
*  DO;                                                             1170
*/* ATBUILD (ISTDYPAB,ISTATCVT,ISTRPH)  */                         1171
*/* END OF ATBUILD */                                              1171
*                                                                  1171
* RFY (GPR02P,GPR03P,GPR04P,GPR15P) RSTD;                          1171
*                                                                  1171
* RFY (GPR01P,GPR13P) RSTD;                                        1172
*          GENERATE REFS(GPR01P,GPR02P,GPR13P) SETS(RPHWORK);      1173
*                                                                  1173
           STM GPR02P,GPR13P,RPHWORK(GPR01P)
* RFY (GPR01P,GPR13P) UNRSTD;                                      1174
*                                                                  1174
* DO; /* LISATCVT CVTBASE(PAGEABLE,(GPR15P)) */                    1175
*   GPR15P = ATCLCPTR; /*SET UP ATCVT POINTER*/                    1176
         L     GPR15P,ATCLCPTR                                     1176
* END; /*LISATCVT*/                                                1177
*          GPR02P=GPR15P->ATCBPDA × '80000000'X;                   1178
         L     GPR02P,ATCBPDA(,GPR15P)                             1178
         O     GPR02P,@CF01149                                     1178
*          DCL 1 FMZA0222 FIXED(31) STATIC LOCAL,                  1179
*              2 *        FIXED(8) INIT(248),                      1179
*              2 *        PTR(24) INIT(0);                         1179
*          GPR03F=FMZA0222;                                        1180
         L     GPR03F,FMZA0222                                     1180
*          GPR04F=17;                                              1181
         LA    GPR04F,17                                           1181
*          GPR15P=GPR15P->ATCSMRQ;                                 1182
         L     GPR15P,ATCSMRQ(,GPR15P)                             1182
*          DCL FMZL0222 BASED(GPR15P) LABEL VALUERANGE(*);         1183
*          CALL FMZL0222;                                          1184
*                                                                  1184
         BALR  @14,@15                                             1184
* RFY (GPR13P) RSTD;                                               1185
*          GENERATE REFS(GPR01P,RPHWORK) SETS(GPR02P,GPR13P);      1186
*                                                                  1186
           LM GPR02P,GPR13P,RPHWORK(GPR01P)
* RFY (GPR13P) UNRSTD;                                             1187
*                                                                  1187
* RFY (GPR02P,GPR03P,GPR04P,GPR15P) UNRSTD;                        1188
* END ;    /********* END OF REQSTORE EXPANSION **********/        1189
*        IF RCREG=FAIL                     /* NO STORAGE              *
*         THEN                             /* WAIT FOR IT             *
*                                                                  1190
         C     RCREG,@CF00060                                      1190
         BNE   @RF01190                                            1190
*/*  TPWAIT   */                                                   1191
*           DO; /* BEGIN TPWAIT GENERATION*/                       1191
*           RFY (GPR01P,GPR14P,GPR15P) RSTD;                       1192
*           RFY (GPR00P) RSTD;                                     1193
*           GEN (STM   GPR00P,GPR15P,RPHWORK(GPR01P))              1194
*           REFS(GPR00P,GPR01P,GPR15P) SETS(RPHWORK);              1194
         STM   GPR00P,GPR15P,RPHWORK(GPR01P)
*           RFY (GPR00P) UNRSTD;                                   1195
*           GPR15P=ADDR(LHAA0241);                                 1196
         LA    GPR15P,LHAA0241                                     1196
*           GPR14P=0;                                              1197
         SLR   GPR14P,GPR14P                                       1197
*           GPR15P=GPR15P & '00FFFFFF'X;                           1198
         N     GPR15P,@CF04346                                     1198
*           GPR15P=GPR15P × RPHWT1;                                1199
         O     GPR15P,@CF01173                                     1199
*           CS (GPR14P,GPR15P,GPR01P->RPHRESMA);                   1200
         CS    GPR14P,@15,RPHRESMA(GPR01P)                         1200
*           BC (7,LHAB0241);                                       1201
*                                                                  1201
         BC    7,LHAB0241                                          1201
*/* TPEXIT  NONE */                                                1202
*           DO;                                                    1202
*             RESPECIFY GPR15P RSTD;                               1203
*                                                                  1203
*/* ATBUILD (ISTPAB,ISTDYPAB,ISTATCVT)  */                         1204
*/* END OF ATBUILD */                                              1204
*                                                                  1204
*           DO; /* LISATCVT CVTBASE(PAGEABLE,(GPR15P)) */          1204
*             GPR15P = ATCLCPTR; /*SET UP ATCVT POINTER*/          1205
         L     GPR15P,ATCLCPTR                                     1205
*           END; /*LISATCVT*/                                      1206
*             RESPECIFY GPR00F RSTD;                               1207
*             GPR00F=0;     /*SET PARAMETER REGISTER TO 0*/        1208
         SLR   GPR00F,GPR00F                                       1208
*             GPR15P=GPR15P->ATCAP31;                              1209
         L     GPR15P,ATCAP31(,GPR15P)                             1209
*             DCL FMZL0246 BASED(GPR15P) LABEL VALUERANGE(*);      1210
*             CALL FMZL0246;                                       1211
         BALR  @14,@15                                             1211
*             RESPECIFY (GPR00F,GPR15P) UNRSTD;                    1212
*           END; /***** END OF TPEXIT  *****/                      1213
*LHAB0241:; GPR01P->RPHRESMA = 0;                                  1214
LHAB0241 DS    0H                                                  1215
         SLR   @10,@10                                             1215
         ST    @10,RPHRESMA(,GPR01P)                               1215
*           RFY (GPR00P) RSTD;                                     1216
*LHAA0241:; GEN (LM    GPR00P,GPR15P,RPHWORK(GPR01P))              1217
LHAA0241 DS    0H                                                  1218
*           REFS(RPHWORK,GPR01P) SETS(GPR00P,GPR15P);              1218
         LM    GPR00P,GPR15P,RPHWORK(GPR01P)
*           RFY (GPR00P,GPR01P,GPR14P,GPR15P) UNRSTD;              1219
*           END; /*****END OF TPWAIT *****/  /*WAIT FOR STORAGE   */
*        ELSE;                             /* HAVAE STORAGE           *
@RF01190 DS    0H                                                  1222
*       RFY RCREG UNRSTD;                                          1222
*        UECBPTR=RPHSRPRM;                 /* COVER UECB STORAGE      *
         L     UECBPTR,RPHSRPRM(,RPHPTR)                           1223
*        RFY ISTUECB BASED(UECBPTR);       /* COVER UECB              *
*        RFY ISTPAB BASED(ADDR(UECPAB));                           1225
*                                          /* COVER PAB IN UECB  */
*        UECID=UEID;                       /* BLOCK ID                *
         MVI   UECID(UECBPTR),X'10'                                1226
*        UECLEN=LENGTH(ISTUECB);           /* CB LENGTH               *
         MVI   UECLEN(UECBPTR),X'54'                               1227
*        UECACB=FMCDEBA->ACDACB;           /* ADDRESS OF ACB          *
         L     @15,CRRFMCB(,CRRPTR)                                1228
         L     @10,FMCDEBA(,@15)                                   1228
         L     @10,ACDACB-1(,@10)                                  1228
         LA    @10,0(,@10)                                         1228
         ST    @10,UECACB(,UECBPTR)                                1228
*        UECSAF=FMCSAF;                    /* GET SOURCE ID           *
         MVC   UECSAF(2,UECBPTR),FMCSAF(@15)                       1229
*        UECDAF=NCBDAF;                    /* GAET DESTINATION ID     *
         L     @10,FMCDNCBA(,@15)                                  1230
         MVC   UECDAF(2,UECBPTR),NCBDAF(@10)                       1230
*        UECUSFLD=FMCUSFLD;                /* COPY USER FIELD         *
         MVC   UECUSFLD(4,UECBPTR),FMCUSFLD(@15)                   1231
*        UECTSKID=FMCTSKID;                /* COPY TASK ID            *
         L     @10,FMCTSKID(,@15)                                  1232
         ST    @10,UECTSKID(,UECBPTR)                              1232
*        UECDVTPT=ATCUEP;                  /* USER EXIT ADDR CVT      *
         L     @10,CRRATCVT(,CRRPTR)                               1233
         L     @10,ATCUEP(,@10)                                    1233
         ST    @10,UECDVTPT(,UECBPTR)                              1233
*        PABNORPR=ON;                      /* SET NORMAL PRIORITY     *
         LA    @10,16                                              1234
         ALR   @10,UECBPTR                                         1234
         OI    PABNORPR(@10),B'00100000'                           1234
*        PABOFFST=ADDR(UECPAB)-ADDR(ISTUECB);                      1235
         MVI   PABOFFST(@10),X'10'                                 1235
*                                               /* SET PAB OFFSET  */
*        PABDVTA=ADDR(UECDVTPT);           /* POINT TO UECB PAB DVT   *
         LA    @09,UECDVTPT(,UECBPTR)                              1236
         STCM  @09,7,PABDVTA(@10)                                  1236
*        PABAPYP=ON;                       /* SET USER EXIT FLAG      *
         OI    PABAPYP(@10),B'10000000'                            1237
*        UECTYPE=UECLOGOF;                 /* SET LOST TERMINAL ID    *
         MVI   UECTYPE(UECBPTR),X'09'                              1238
*         IF FMCPRMRC=FRPDISCO             /* IF DISCONNECT OCCURRED  *
*          THEN                                                    1239
         CLI   FMCPRMRC(@15),X'08'                                 1239
         BNE   @RF01239                                            1239
*           UECRSNCD=ZERO;                 /* REASON CODE IN PARM LIST*
         SLR   @15,@15                                             1240
         ST    @15,UECRSNCD(,UECBPTR)                              1240
*          ELSE IF FMCPRMRC=FRMRUTRM THEN UECRSNCD=UECRTRM;/*@XM10597*/
         B     @RC01239                                            1241
@RF01239 L     @15,CRRFMCB(,CRRPTR)                                1241
         CLI   FMCPRMRC(@15),X'81'                                 1241
         BNE   @RF01241                                            1241
         MVC   UECRSNCD(4,UECBPTR),@CF02854                        1242
*            ELSE                                                  1243
*              UECRSNCD=LTERM;                 /* REASON CODE IN UECB*/
*                                                                  1243
         B     @RC01241                                            1243
@RF01241 MVC   UECRSNCD(4,UECBPTR),@CF00032                        1243
*/*  TPQUE (UECBPTR,UECPAB) NORM */                                1244
*                                                                  1244
*         DO;                                                      1244
@RC01241 DS    0H                                                  1244
@RC01239 DS    0H                                                  1245
*         RFY (GPR00P,GPR01P,GPR04P,GPR05P,GPR15P) RSTD;           1245
*         GEN REFS(GPR01P) SETS(RPHWORK)                           1246
*        (STM GPR02P,GPR08P,RPHWORK(GPR01P));                      1246
         STM GPR02P,GPR08P,RPHWORK(GPR01P)
*         GPR00P=0;                                                1247
         SLR   GPR00P,GPR00P                                       1247
*         GPR15P=ADDR(UECPAB); /*HOLD PAB PTR*/                    1248
         LA    GPR15P,UECPAB(,UECBPTR)                             1248
*         GPR04P=UECBPTR; /*WORK ELE PTR*/                         1249
         LR    GPR04P,UECBPTR                                      1249
*         GPR05P=GPR15P; /*PASS PAB PTR*/                          1250
         LR    GPR05P,GPR15P                                       1250
*         CALL ATCLCPTR->ATCAP37->ISTAPC37;                        1251
         L     @10,ATCLCPTR                                        1251
         L     @15,ATCAP37(,@10)                                   1251
         BALR  @14,@15                                             1251
*         GEN(NOPR  0)NOSETS NOREFS;                               1252
         NOPR  0
*         RFY (GPR00P,GPR01P,GPR04P,GPR05P,GPR15P) UNRSTD;         1253
*         END; /**** END OF TPQUE GENERATION ****/                 1254
*        RFY UECBPTR UNRSTD;                                       1255
*        END;                              /* END-NOT VARY SOFT PURGE *
*     RTNREG=CRRRTN7;                      /* RESTORE RETURN ADDRESS  *
@RT01166 L     RTNREG,CRRRTN7(,CRRPTR)                             1257
*     RETURN;                                                      1258
@EL00008 DS    0H                                                  1258
@EF00008 DS    0H                                                  1258
@ER00008 BR    @14                                                 1258
*END    LSTRMEXT;                                                  1259
*/********************************************************************/
*/*                                                                  */
*/* ROUTINE NAME : SETRPL - SET UP RPL FOR POSTING                   */
*/*                                                                  */
*/* FUNCTION :                                                       */
*/*    IF A VARY SOFT PURGE IS IN PROGRESS THE RPL IS POSTED         */
*/*    COMPLETE IF NO OTHER RPL IS CURRENTLY BEING HELD IN THE       */
*/*    FMCB FOR POSTING. IF THERE IS ONE BEING HELD, IT IS POSTED    */
*/*    AND THE CURRENT RPL IS THEN HELD. IF THIS IS NOT VARY SOFT    */
*/*    PURGE, THE CURRENT RPL IS POSTED WITH PURGE RETURN CODES      *.
*/*                                                                  */
*/* INPUT :  FSBPTR (REG 6) - ADDRESS OF FSB                         */
*/*          LCPBPTR (REG 4) - REQUESTOR'S PURGE LCPB                */
*/*                                                                  */
*/* OUTPUT :  THE OLDEST RPL IS POSTED COMPLETE                      */
*/*                                                                  */
*/* SUBROUTINES CALLED :                                             */
*/*                      ISTAICPT - POST USER RPL COMPLETE           */
*/*                                                                  */
*/*  MACROS USED :  NONE                                             */
*/*                                                                  */
*/********************************************************************/
*SETRPL:  PROC OPTIONS(NOSAVE,NOSAVEAREA,NOPARMREG);               1260
SETRPL   DS    0H                                                  1261
*     CRRRTN8=RTNREG;                      /* SAVE RETURN ADDRESS     *
         ST    RTNREG,CRRRTN8(,CRRPTR)                             1261
*    IF FMCVSP1=ON                            /* IF A VARY SOFT PURGE *
*    THEN DO;                               /*CHECK FOR AN OLDER RPL */
         L     @15,CRRFMCB(,CRRPTR)                                1262
         TM    FMCVSP1(@15),B'00000001'                            1262
         BNO   @RF01262                                            1262
*     FMCVSP2=ON;                          /*SET REQUEST SOFT PURGED  *
         OI    FMCVSP2(@15),B'10000000'                            1264
*     IF FMCRPLH=ZERO                      /*IF NO RPL IN FMCB        *
*        THEN FMCRPLH=RPLPTR;  /* PUT THIS RPL IN FMCB                *
         L     @10,FMCRPLH(,@15)                                   1265
         LTR   @10,@10                                             1265
         BNZ   @RF01265                                            1265
         ST    RPLPTR,FMCRPLH(,@15)                                1266
*     ELSE DO;                            /* IF FMCB HAS AN RPL       *
         B     @RC01265                                            1267
@RF01265 DS    0H                                                  1268
*        RFY IFGRPL BASED(RPLPTR);        /* COVER THIS RPL          */
*        WRKREGD=RPLPTR;                   /* SAVE CURRENT RPL        *
         LR    WRKREGD,RPLPTR                                      1269
*        RPLPTR=FMCRPLH;                   /* GET HELD RPL            *
         L     @15,CRRFMCB(,CRRPTR)                                1270
         L     RPLPTR,FMCRPLH(,@15)                                1270
*        FMCRPLH=WRKREGD;                  /* HOLD THIS CURRENT RPL   *
         ST    WRKREGD,FMCRPLH(,@15)                               1271
*        IF FMCPRMRC='81'X THEN RPLRTNCD='18'X;/*            @XM7981K*/
         CLI   FMCPRMRC(@15),X'81'                                 1272
         BNE   @RF01272                                            1272
         MVI   RPLRTNCD(RPLPTR),X'18'                              1273
*         ELSE                             /*                @XM7981K*/
*        RPLRTNCD='1C'X;                   /* SET RTN CODE TO PURGE   *
         B     @RC01272                                            1274
@RF01272 MVI   RPLRTNCD(RPLPTR),X'1C'                              1274
*        RPLFDB2=FMCPRMRC;                 /*  MINOR CODE FROM FMCB   *
@RC01272 L     @15,CRRFMCB(,CRRPTR)                                1275
         IC    @10,FMCPRMRC(,@15)                                  1275
         STC   @10,RPLFDB2(,RPLPTR)                                1275
*        RPLUSFLD=FMCUSFLD;                /* GET USER INFO   @XM09597*
         MVC   RPLUSFLD(4,RPLPTR),FMCUSFLD(@15)                    1276
*        RPHWEA=RPLPTR;                   /* SET UP RPH FOR POST    */
         ST    RPLPTR,RPHWEA(,RPHPTR)                              1277
*        CALL ATCAPOST;                    /* CALL USER POST RTN      *
         L     @10,CRRATCVT(,CRRPTR)                               1278
         L     @15,ATCAPOST(,@10)                                  1278
         BALR  @14,@15                                             1278
*        END;                                                      1279
*       END;                                     /* END SOFT PURGE   */
*    ELSE DO;                                   /*NOT SOFT PURGE    */
         B     @RC01262                                            1281
@RF01262 DS    0H                                                  1282
*      RPLUSFLD=FMCUSFLD;                  /* GET USER INFO   @XM09597*
         L     @15,CRRFMCB(,CRRPTR)                                1282
         MVC   RPLUSFLD(4,RPLPTR),FMCUSFLD(@15)                    1282
*      IF RPLFDB2='05'X THEN ;            /* USER ABEND-DON'T SET CODE*
         CLI   RPLFDB2(RPLPTR),5                                   1283
         BE    @RT01283                                            1283
*      ELSE DO;                                                    1285
*        IF FMCPRMRC='81'X THEN RPLRTNCD='18'X;/*            @XM7981K*/
         L     @15,CRRFMCB(,CRRPTR)                                1286
         CLI   FMCPRMRC(@15),X'81'                                 1286
         BNE   @RF01286                                            1286
         MVI   RPLRTNCD(RPLPTR),X'18'                              1287
*         ELSE                             /*                @XM7981K*/
*       RPLRTNCD='1C'X;                    /* SET PURGE RETURN CODE   *
         B     @RC01286                                            1288
@RF01286 MVI   RPLRTNCD(RPLPTR),X'1C'                              1288
*       RPLFDB2=FMCPRMRC;                  /* COPY REASON CODE        *
@RC01286 L     @15,CRRFMCB(,CRRPTR)                                1289
         IC    @10,FMCPRMRC(,@15)                                  1289
         STC   @10,RPLFDB2(,RPLPTR)                                1289
*       END;                                                       1290
*     RPHWEA=RPLPTR;                     /* PUT RPL ADDR TO RPH     */
@RT01283 ST    RPLPTR,RPHWEA(,RPHPTR)                              1291
*     CALL ATCAPOST;                     /* CALL USER POST ROUTINE  */
         L     @10,CRRATCVT(,CRRPTR)                               1292
         L     @15,ATCAPOST(,@10)                                  1292
         BALR  @14,@15                                             1292
*     END;                               /* END -NOT SOFT PURGE     */
*     RTNREG=CRRRTN8;                      /* RESTORE RETURN ADDRESS  *
@RC01262 L     RTNREG,CRRRTN8(,CRRPTR)                             1294
*     RETURN;                                                      1295
@EL00009 DS    0H                                                  1295
@EF00009 DS    0H                                                  1295
@ER00009 BR    @14                                                 1295
*END  SETRPL;                                                      1296
*/********************************************************************/
*/*                                                                  */
*/* ROUTINE NAME :  POSTRPL - POST RPL COMPLETE OR HOLD IT.          */
*/*                                                                  */
*/* FUNCTION :                                                       */
*/*    POSTRPL POSTS THE RPL COMPLETE UNLESS A VARY SOFT PURGE       */
*/*    IS IN PROGRESS, IN WHICH CASE THE RPL ADDRESS IS MOVED        */
*/*    TO THE FMCB HOLD QUEUE                                        */
*/*                                                                  */
*/* INPUT :  FSBPTR (REG 6) - ADDRESS OF FSB                         */
*/*          LCPBPTR (REG 4) - REQUESTOR'S PURGE LCPB                */
*/*                                                                  */
*/* OUTPUT :  RPL IS POSTED OR PUT IN THE FMCB HOLD QUEUE            */
*/*                                                                  */
*/* SUBROUTINES CALLED :                                             */
*/*                      ISTAICPT - USER POST ROUTINE                */
*/*                                                                  */
*/*  MACROS USED : TPDEQ                                             */
*/*                                                                  */
*/********************************************************************/
*POSTRPL:  PROC OPTIONS(NOSAVE,NOSAVEAREA,NOPARMREG);              1297
POSTRPL  DS    0H                                                  1298
*     CRRRTN9=RTNREG;                      /* SAVE RETURN ADDRESS     *
         ST    RTNREG,CRRRTN9(,CRRPTR)                             1298
*     RFY IFGRPL BASED(WRKREGD);           /* WRKREGD HAS CURR RPL    *
*     IF FMCPRMRC='05'X                    /* APPLICATION ABEND SPEC  *
*     THEN DO;                             /*POST THIS RPL            *
         L     @15,CRRFMCB(,CRRPTR)                                1300
         CLI   FMCPRMRC(@15),X'05'                                 1300
         BNE   @RF01300                                            1300
*        RPLUSFLD=FMCUSFLD;                /* GET USER INFO   @XM09597*
         MVC   RPLUSFLD(4,WRKREGD),FMCUSFLD(@15)                   1302
*        RPHWEA=WRKREGD;                   /* SET UP RPLH FOR POST    *
         ST    WRKREGD,RPHWEA(,RPHPTR)                             1303
*        CALL ATCAPOST;                    /* CALL USER POST ROUTINE  *
         L     @10,CRRATCVT(,CRRPTR)                               1304
         L     @15,ATCAPOST(,@10)                                  1304
         BALR  @14,@15                                             1304
*        END;                              /* END-APPL. ABEND         *
*     ELSE DO;                             /* APPL ABEND NOT SPECIFIED*
         B     @RC01300                                            1306
@RF01300 DS    0H                                                  1307
*        IF FMCVSP2=ON                     /* RPL WAS SOFT PURGED     *
*        THEN DO;                          /*  SAVE IT-POST OLD ONE   *
         L     @15,CRRFMCB(,CRRPTR)                                1307
         TM    FMCVSP2(@15),B'10000000'                            1307
         BNO   @RF01307                                            1307
*          RPLPTR=FMCRPLH;                 /* GET ADDR SAVED RPL      *
         L     RPLPTR,FMCRPLH(,@15)                                1309
*          FMCRPLH=WRKREGD;                /*  SAVE THE CURR ONE      *
         ST    WRKREGD,FMCRPLH(,@15)                               1310
*          RPLPTR->RPLRTNCD='1C'X;         /* SET RTN CODE-NEW RPL    *
         MVI   RPLRTNCD(RPLPTR),X'1C'                              1311
*          RPLPTR->RPLFDB2=FMCPRMRC;       /* AND THE REASON CODE     *
         IC    @10,FMCPRMRC(,@15)                                  1312
         STC   @10,RPLFDB2(,RPLPTR)                                1312
*          RPLPTR->RPLUSFLD=FMCUSFLD;      /* GET USER INFO   @XM09579*
         MVC   RPLUSFLD(4,RPLPTR),FMCUSFLD(@15)                    1313
*          RPHWEA=RPLPTR;                  /* SET RPH FOR POSTING     *
         ST    RPLPTR,RPHWEA(,RPHPTR)                              1314
*          CALL ATCAPOST;                  /* CALL USER POST          *
         L     @10,CRRATCVT(,CRRPTR)                               1315
         L     @15,ATCAPOST(,@10)                                  1315
         BALR  @14,@15                                             1315
*          END;                            /* END-RPL SOFT PURGED     *
*        ELSE DO;                          /* RPL NOT SOFT PURGED     *
         B     @RC01307                                            1317
@RF01307 DS    0H                                                  1318
*          IF FMCVSP1=ON                   /* SOFT PRG IN PROGRESS    *
*          THEN DO;                        /* DON'T POST COMPLETE     *
         L     @15,CRRFMCB(,CRRPTR)                                1318
         TM    FMCVSP1(@15),B'00000001'                            1318
         BNO   @RF01318                                            1318
*            FMCRPLH=WRKREGD;              /* HOLD THIS RPL           *
         ST    WRKREGD,FMCRPLH(,@15)                               1320
