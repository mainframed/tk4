*/*                                                                  */
*/* ROUTINE NAME : CLNPURGQ - CLEAN OFF THE PURGE RESET QUEUE        */
*/*                                                                  */
*/* FUNCTION :                                                       */
*/*    THE LCPB RESET QUEUE MAY HAVE RPLS OR LCPBS ON IT. RPLS ARE   */
*/*    POSTED WITH PURGE RETURN CODES AND THE LCPBS ARE QUEUED AND   */
*/*    SCHEDULED ON THE CONTROL LAYER INBOUND PAB. IF THE LCPB IS    */
*/*    A PURGE LCPB IS'S OWN LCPB PURGE RESET QUEUE (LCPRSTQH) IS    */
*/*    PROCESSED. FSB'S ARE BUILT FOR ANY LCPB THAT IS POSTED OR FED */
*/*    BACK TO CONTROL LAYER.                                        */
*/*                                                                  */
*/* INPUT :  FSBPTR (REG 6) - ADDRESS OF FSB                         */
*/*          LCPBPTR (REG 4) - REQUESTOR'S PURGE LCPB                */
*/*                                                                  */
*/* OUTPUT :  ALL ELEMENTS ON THE RESET QUEUE PROCESSED              */
*/*                                                                  */
*/* SUBROUTINES CALLED :                                             */
*/*                      PRGEPOST - POST PURGE OPERATION COMPLETE    */
*/*                      POSTRPL - - POST RPL STATUS                 */
*/*                                                                  */
*/*  MACROS USED :   TPDEQ,TPFEL,REQSTORE,TPWAIT,TPQUE               */
*/*                                                                  */
*/*******************************************************************/
*CLNPURGQ: PROC OPTIONS(NOSAVE,NOSAVEAREA,NOPARMREG);              0753
*                                                                  0753
CLNPURGQ DS    0H                                                  0754
*     CRRRTN4=RTNREG;                      /*SAVE RETURN ADDRESS      *
         ST    RTNREG,CRRRTN4(,CRRPTR)                             0754
*     DO WHILE CRRSW1=OFF;                 /* ONE COMPLETE CYCLE SW   *
         B     @DE00755                                            0755
@DL00755 DS    0H                                                  0756
*      CRRSW1=ON;                          /* SWITCH TO STOP DO WHILE*/
*      CRRSW2=OFF;                         /*INITIALIZE IS-PURGE SW   *
         OI    CRRSW1(CRRPTR),B'01000000'                          0757
         NI    CRRSW2(CRRPTR),B'11011111'                          0757
*      IF WRKREGD->RPLID='00'X             /* IS THIS ELEMENT AN RPL  *
*        THEN;                             /* DON'T CHECK LCCW        *
         CLI   RPLID(WRKREGD),X'00'                                0758
         BE    @RT00758                                            0758
*      ELSE DO;                                                    0760
*        IF WRKREGD->LCPNXLCW->LCCWOP=LCCWPRGE /* IF A PURGE LCPB     *
*          THEN CRRSW2=ON;                 /*SET PURGE INDICATOR      *
         L     @15,LCPNXLCW(,WRKREGD)                              0761
         CLI   LCCWOP(@15),X'6B'                                   0761
         BNE   @RF00761                                            0761
         OI    CRRSW2(CRRPTR),B'00100000'                          0762
*        ELSE;                             /* NOT A PURGE LCPB        *
@RF00761 DS    0H                                                  0764
*        END;                              /*END-ELEMENT NOT RPL      *
*      IF CRRSW2=ON                        /* IF A PURGE LCPB         *
*        THEN DO;                                                  0765
*                                                                  0765
@RT00758 TM    CRRSW2(CRRPTR),B'00100000'                          0765
         BNO   @RF00765                                            0765
*   /* THIS IS A PURGE LCPB. ITS PURGED QUEUE MUST BE CLEANED OFF *
*    * ALSO   */                                                   0767
*                                                                  0767
*          PRGPTRC=CURRPTRB;               /*SET UP PTR FOR PURGE LCPB*
         L     @15,CURRPTRB(,CRRPTR)                               0767
         ST    @15,PRGPTRC(,CRRPTR)                                0767
*          CURRPTRB=WRKREGD;               /* CURRENT ELT IS WORK ELT *
         ST    WRKREGD,CURRPTRB(,CRRPTR)                           0768
*          RFY ISTDCLCP  BASED(CURRPTRB);  /*COVER CURRENT LCPB       *
*                                                                  0769
*/* TPFEL (,LCPRSTQH) RETPTR(WRKREGD) */                           0770
*           DO; /* BEGIN TPFEL CODE GENERATION */                  0770
*           DCL FMZCA108 BIT(32) CONSTANT('80000000'X);            0771
*           DCL FMZCB108 BIT(32) CONSTANT('7FFFFFFF'X);            0772
*           RFY (GPR00P,GPR01P,GPR14P,GPR15P) RSTD;                0773
*                                                                  0773
*           GPR15P=ADDR(LCPRSTQH); /*ADDRESS QUEUE POINTER */      0774
         L     @10,CURRPTRB(,CRRPTR)                               0774
         LA    GPR15P,LCPRSTQH(,@10)                               0774
*           GPR14P=GPR15P->PABWEQA×FMZCA108;                       0775
         L     @10,@CF01149                                        0775
         L     GPR14P,PABWEQA(,GPR15P)                             0775
         OR    GPR14P,@10                                          0775
*           IF GPR14P=FMZCA108 THEN;                               0776
         CLR   GPR14P,@10                                          0776
         BE    @RT00776                                            0776
*           ELSE DO;                                               0778
*           GPR00P=FMZCA108; /* INITIALIZE QUIT FLAG */            0779
         L     GPR00P,@CF01149                                     0779
*           DO WHILE (GPR00P ^= GPR14P->WKECHAIN);                 0780
         B     @DE00780                                            0780
@DL00780 DS    0H                                                  0781
*              GPR14P=GPR14P->WKECHAIN; /* SEARCH FOR END OF CHAIN */
         L     GPR14P,WKECHAIN(,GPR14P)                            0781
*              END;                                                0782
@DE00780 C     GPR00P,WKECHAIN(,GPR14P)                            0782
         BNE   @DL00780                                            0782
*           END;                                                   0783
*           GPR14P=GPR14P&FMZCB108;                                0784
@RT00776 N     GPR14P,@CF01147                                     0784
*           WRKREGD=GPR14P;                                        0785
         LR    WRKREGD,GPR14P                                      0785
*           RFY (GPR00P,GPR01P,GPR14P,GPR15P) UNRSTD;              0786
*           END; /* END TPFEL GENERATION */ /*GET LAST ELEMENT ADDR*/
*          IF WRKREGD^=ZERO               /* IF Q IS EMPTY           */
*            THEN CRRSW1=OFF;            /*ANOTHER CYCLE IS NEEDED*/
         LTR   WRKREGD,WRKREGD                                     0788
         BZ    @RF00788                                            0788
         NI    CRRSW1(CRRPTR),B'10111111'                          0789
*          ELSE DO;                        /* RESET Q IS EMPTY        *
         B     @RC00788                                            0790
@RF00788 DS    0H                                                  0791
*            RFY ISTDCLCP BASED(PRGPTRC);                          0791
*            WRKREGD=CURRPTRB;              /*            @DM09165*/
         L     WRKREGD,CURRPTRB(,CRRPTR)                           0792
*            /*GET PURGE LCPB */                                   0793
*                                                                  0793
*/* TPDEQ (WRKREGD,LCPRSTQH)  */                                   0793
*             DO; /* BEGIN TPDEQ CODE GENERATION */                0793
*             RFY (GPR00P,GPR01P,GPR14P,GPR15P) RSTD;              0794
