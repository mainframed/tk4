*/*                                                                  */
*/* ROUTINE NAME : PRGEPOST - POST PURGE COMPLETE                    */
*/*                                                                  */
*/* FUNCTION :                                                       */
*/*    PRGE POST POSTS THE PURGE OPERATION COMPLETE BY POSTING THE   */
*/*    REQUESTOR'S RPH PASSED IN THE PURGE LCCW'S LCCWDATA FIELD.    */
*/*    IF THAT FIELD CONTAINS A PAB ADDRESS INSTEAD, THE FSB IS      */
*/*    QUEUED TO THAT PAB AND SCHEDULED. FOR A VARY PURGE, A LOST    */
*/*    TERMINAL EXIT IS SCHEDULED.                                   */
*/*                                                                  */
*/* INPUT :  FSBPTR (REG 6) - ADDRESS OF FSB                         */
*/*          LCPBPTR (REG 4) - REQUESTOR'S PURGE LCPB                */
*/*                                                                  */
*/* OUTPUT :  PURGE POSTED, LOST TERM EXIT MAY BE SCHEDULED          */
*/*                                                                  */
*/* SUBROUTINES CALLED :                                             */
*/*                      LSTRMEXT - SCHEDULE LOST TERMINAL EXIT      */
*/*                                                                  */
*/*                                                                  */
*/*  MACROS USED :  TPQUE, TPPOST                                    */
*/*                                                                  */
*/********************************************************************/
*PRGEPOST: PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA,NOPARMREG);         0601
PRGEPOST DS    0H                                                  0602
*     CRRRTN2=RTNREG;                      /* SAVE RETURN REG         *
         ST    RTNREG,CRRRTN2(,CRRPTR)                             0602
*     RFY ISTDCLCP BASED(FSBLCPB);                                 0603
*     RFY ISTLCCW BASED(LCPNXLCW);                                 0604
*                /* COVER FIRST LCCW        */                     0605
*     IF FSBMJRCD=FRLOGICE &              /* IF LOGIC ERROR          */
*        FSBMNRCD=FRMCDITS THEN          /* AND CONFLICTING SESSION  */
         CLI   FSBMJRCD(FSBPTR),X'08'                              0605
         BNE   @RF00605                                            0605
         CLI   FSBMNRCD(FSBPTR),X'A6'                              0605
         BNE   @RF00605                                            0605
*        DO;                                                       0606
*         FSBMJRCD=FRNORM;          /* ZERO MAJOR AND MINOR CODES*/
         MVI   FSBMJRCD(FSBPTR),X'00'                              0607
*         FSBMNRCD=FRMOK;                                          0608
         MVI   FSBMNRCD(FSBPTR),X'00'                              0608
*        END;                                                      0609
*      ELSE;                                                       0610
@RF00605 DS    0H                                                  0611
*      IF FSBMJRCD=ZERO THEN                                       0611
         CLI   FSBMJRCD(FSBPTR),0                                  0611
         BNE   @RF00611                                            0611
*       DO;                                /* NOT AN ERROR CONDITION*/
*       RFY (REG2,REG3) RSTD;   /* REGS FOR COMPARE AND SWAP*/     0613
*C81PST: REG2=FMCCMPS2;                   /* GET CURRENT FLAG SETTING *
C81PST   L     @15,CRRFMCB(,CRRPTR)                                0614
         L     REG2,FMCCMPS2(,@15)                                 0614
*       REG3=REG2 & FLG04OFF;              /*PURGE IN PROGRESS OFF*/
         LR    REG3,REG2                                           0615
         N     REG3,@CF04247                                       0615
*                                          /* AND DATA EXPECTED OFF*/
*       CS (REG2,REG3,FMCCMPS2);           /* USE COMPARE AND SWAP*/
         CS    REG2,@03,FMCCMPS2(@15)                              0616
*       BC (7,C81PST);                     /* TRY AGAIN IF CS FAILS   *
         BC    7,C81PST                                            0617
*       RFY (REG2,REG3) UNRSTD;                                    0618
*       END;                               /* END NO ERR IN FSB       *
*      ELSE;                               /* IS ERROR CASE           *
@RF00611 DS    0H                                                  0621
*    IF LCCWDATA^= 0 THEN               /* NOT AN END CTL CMD@DM13003*/
         L     @15,FSBLCPB(,FSBPTR)                                0621
         L     @15,LCPNXLCW(,@15)                                  0621
         L     @10,LCCWDATA(,@15)                                  0621
         LTR   @10,@10                                             0621
         BZ    @RF00621                                            0621
*     IF  ADDR(LCCWDATA)->PABWEQG=ON THEN                          0622
         TM    PABWEQG+4(@15),B'10000000'                          0622
         BNO   @RF00622                                            0622
*         DO;                              /* IF LCCW HAS RPH         *
*         RFY REG2 RSTD;                                           0624
*         REG2=LCCWDATA;                 /* GET ADDR OF LCCW RPH    */
         LR    REG2,@10                                            0625
*         REG2=REG2 & '7FFFFFFF'X;        /* TURN OFF HIGH ORDER BIT */
         N     REG2,@CF01147                                       0626
*         REG2->RPHWEA=FSBPTR;            /* SET WORK ELT TO @ZA15130*/
         ST    FSBPTR,RPHWEA(,REG2)                                0627
*                                         /* FSB ADDRESS     @ZA15130*/
*                                                                  0628
*/*TPPOST  ((REG2))  */                                            0628
*          DO;                                                     0628
*                                       /*DECLARE AND RESTRICT REGS*/
*            RESPECIFY (GPR00F,GPR01F,GPR04F,GPR05F,               0629
*                      GPR06F,GPR14F,GPR15F) RSTD;                 0629
*                                       /*0,1,4,5,6,14, AND 15   */
*                                                                  0630
*/* ATBUILD (ISTPAB,ISTDYPAB,ISTRPH)  */                           0630
*/* END OF ATBUILD */                                              0630
*            GEN REFS(GPR01P,GPR02F,GPR06F)SETS(RPHWORK);          0630
        STM GPR02F,GPR06F,RPHWORK(GPR01P)
*            GPR04F=REG2;               /*LOAD RPH PNTR INTO REG 4*/
         LR    GPR04F,REG2                                         0631
*            RFY GPR03F RSTD;                                      0632
*            GPR03F=0;                                             0633
*                                                                  0633
         SLR   GPR03F,GPR03F                                       0633
*          DO; /*MODESET  SAVEKEY(GPR06F) KEYADDR(GPR03F) WORKREG(GPR05
*)*/         RESPECIFY (GPR02F) RESTRICTED;                        0634
*            GPR05F = GPR02F; /*SAVE REGISTER 2*/                  0636
         LR    GPR05F,GPR02F                                       0636
*            IPK; /*SAVE CURRENT PSW KEY*/                         0637
         IPK                                                       0637
*            GPR06F = GPR02F;                                      0638
         LR    GPR06F,GPR02F                                       0638
*            GPR02F = GPR05F; /*RESTORE REGISTER 2*/               0639
         LR    GPR02F,GPR05F                                       0639
*            SPKA (GPR03F); /*SET PSW KEY*/                        0640
         SPKA  0(GPR03F)                                           0640
*            RESPECIFY (GPR02F) UNRESTRICTED;                      0641
*          END; /*MODESET  SAVEKEY(GPR06F) KEYADDR(GPR03F) WORKREG(GPR0
*F)*/        RFY GPR03F UNRSTD;                                    0642
*            GPR05P=GPR04P->RPHMAJCB+GPR04P->RPHPABOF;             0644
         L     GPR05P,RPHMAJCB-1(,GPR04P)                          0644
         LA    GPR05P,0(,GPR05P)                                   0644
         SLR   @10,@10                                             0644
         IC    @10,RPHPABOF(,GPR04P)                               0644
         ALR   GPR05P,@10                                          0644
*                                       /*LOAD REG 5  WITH ADDR OF*/
*                                       /*MAJOR CONTROL BLOCK PLUS*/
*                                       /*OFFSET OF PAB IN BLOCK  */
*            GPR14P=0;                  /*LOAD 0 INTO REG 14      */
         SLR   GPR14P,GPR14P                                       0645
*            GPR15P='FFFFFFFF'X;        /*LOAD REG 15 WITH DUMMY  */
         SLR   GPR15P,GPR15P                                       0646
         BCTR  GPR15P,0                                            0646
*                                       /*ADDRESS                 */
*            CS (GPR14P,GPR15P,GPR04P->RPHRESMA);                  0647
         CS    GPR14P,@15,RPHRESMA(GPR04P)                         0647
*                                       /*COMPARE RESUME ADDRESS  */
*                                       /*WITH 0 AND,IF EQUAL, PUT*/
*                                       /*DUMMY ADDRESS IN CONTROL*/
*                                       /*BLOCK RESUME ADDRESS    */
*            BC (8,FMZT0058);           /*IF EQUAL, BYPASS REMAINDER*/
         BC    8,FMZT0058                                          0648
*                                       /*OF EXPANSION              */
*FMZL0058:   IF GPR04P->RPHWT='1'B THEN /*WT/EXT CONTENTION FLAG ON */
FMZL0058 TM    RPHWT(GPR04P),B'00001000'                           0649
         BNO   @RF00649                                            0649
*            DO;                        /*IN RPH                    */
*              GPR14P=GPR04P->RPHRESMA×RPHWT1;                     0651
         L     GPR14P,RPHRESMA(,GPR04P)                            0651
         O     GPR14P,@CF01173                                     0651
*                                       /*LOAD REG 14 WITH FLAG FIELD*/
*                                       /*AND MAKE CERTAIN WT/EXT    */
*                                       /*CONTENTION FLAG STILL ON   */
*              GPR15P=GPR14P×RPHPT1;    /*LOAD REG 15 WITH FLAG FIELD*/
         LR    GPR15P,GPR14P                                       0652
         O     GPR15P,@CF01163                                     0652
*                                       /*AND TURN POST FLAG ON      */
*              CS (GPR14P,GPR15P,GPR04P->RPHRESMA);                0653
         CS    GPR14P,@15,RPHRESMA(GPR04P)                         0653
*                                       /*COMPARE REG 14 WITH FLAG */
*                                       /*FIELD--IF WT/EXT BIT ON, */
*                                       /*TURN ON POST FLAG        */
*              BC (4,FMZL0058);         /*IF WT/EXT BIT WAS OFF, LOOP*/
         BC    4,FMZL0058                                          0654
*                                       /*TO TEST CONTENTION FLAG    */
*              BC (8,FMZT0058);         /*IF WT/EXT BIT WAS OFF,     */
         BC    8,FMZT0058                                          0655
*                                       /*BRANCH TO BYPASS CODE GEN  */
*            END;                       /*IF POST FLAG IS ON, FALL*/
*                                       /*THRU BOTTOM OF LOOP     */
*            ELSE;                      /*WT/EXT CONTENTION FLAG OFF*/
@RF00649 DS    0H                                                  0658
*            IF GPR04P->RPHTSKID=GPR01P->RPHTSKID                  0658
*            THEN DO;                                              0658
         CLC   RPHTSKID(4,GPR04P),RPHTSKID(GPR01P)                 0658
         BNE   @RF00658                                            0658
*              GPR05P->PABCHNGP=GPR01P->RPHPABQP;                  0660
         MVC   PABCHNGP(3,GPR05P),RPHPABQP(GPR01P)                 0660
*                                       /*PUT PAB QUEUE ADDRESS*/  0661
*                                       /*IN SECOND WORD OF PAB     */
*              GPR01P->RPHPABQP=GPR05P; /*PUT PAB ADDRESS (IN MAJOR */
         STCM  GPR05P,7,RPHPABQP(GPR01P)                           0661
*                                       /*CONTROL BLOCK) IN PAB */ 0662
*                                       /*QUEUE ADRESS IN RPH    */
*              END;                                                0662
*            ELSE DO;                                              0663
         B     @RC00658                                            0663
@RF00658 DS    0H                                                  0664
*              GPR00F=0;                /*SET REG 0 TO INDICATE ENA*/
*                                                                  0664
         SLR   GPR00F,GPR00F                                       0664
*          DO; /* LISATCVT CVTBASE(PAGEABLE,(GPR15P)) */           0665
*            GPR15P = ATCLCPTR; /*SET UP ATCVT POINTER*/           0666
         L     GPR15P,ATCLCPTR                                     0666
*          END; /*LISATCVT*/            /*PUT POINTER TO ATCVT IN */
*                                       /*  IN REG 15             */
*              CALL GPR15P->ATCAP32;    /* CALL ISTAPC32: TPSCHED */
         L     @10,ATCAP32(,GPR15P)                                0668
         LR    @15,@10                                             0668
         BALR  @14,@15                                             0668
*              END;                                                0669
*FMZT0058:                                                         0670
*          DO; /*MODESET  KEYADDR(GPR06F) WORKREG(GPR05F)*/        0670
@RC00658 DS    0H                                                  0670
FMZT0058 DS    0H                                                  0671
*            GPR05F = GPR06F; /*GET KEY FORM SAVE LOCATION*/       0671
         LR    GPR05F,GPR06F                                       0671
*            SPKA (GPR05F); /*SET PSW KEY*/                        0672
         SPKA  0(GPR05F)                                           0672
*          END; /*MODESET  KEYADDR(GPR06F) WORKREG(GPR05F)*/       0673
*            GEN REFS(GPR04P,                                      0674
*            RPHWORK)                                              0674
*                SETS(GPR02F,GPR03F,                               0674
*                      GPR04F,GPR05F,GPR06F);                      0674
        LM GPR02F,GPR06F,RPHWORK(GPR01P)
*            RESPECIFY (GPR00F,GPR01F,GPR04F,GPR05F,GPR06F,        0675
*            GPR14F,GPR15F) UNRSTD;     /*UNRESTRICT REGS 0,1,    */
*                                       /*4,5,14, AND 15          */
*          END;  /****END OF TPPOST GENERATION****/              /* POS
* THE RPH            */                                            0676
*         RFY REG2 UNRSTD;                                         0677
*         END;                                                     0678
*     ELSE                                                         0679
*         DO;                              /* LCCW DOES NOT HAVE AN RPH
*                                           * IT HAS ADDRESS OF A PAB *
         B     @RC00622                                            0679
@RF00622 DS    0H                                                  0680
*         RFY ISTPAB BASED(LCCWDATA);      /* COVER PAB IN LCCW       *
*                                                                  0680
*/*  TPQUE (FSBPTR,PABWEQA) NORM */                                0681
*                                                                  0681
*          DO;                                                     0681
*          RFY (GPR00P,GPR01P,GPR04P,GPR05P,GPR15P) RSTD;          0682
*          GEN REFS(GPR01P) SETS(RPHWORK)                          0683
*        (STM GPR02P,GPR08P,RPHWORK(GPR01P));                      0683
         STM GPR02P,GPR08P,RPHWORK(GPR01P)
*          GPR00P=0;                                               0684
         SLR   GPR00P,GPR00P                                       0684
*          GPR15P=ADDR(PABWEQA); /*HOLD PAB PTR*/                  0685
         L     @10,FSBLCPB(,FSBPTR)                                0685
         L     @10,LCPNXLCW(,@10)                                  0685
         L     GPR15P,LCCWDATA(,@10)                               0685
*          GPR04P=FSBPTR; /*WORK ELE PTR*/                         0686
         LR    GPR04P,FSBPTR                                       0686
*          GPR05P=GPR15P; /*PASS PAB PTR*/                         0687
         LR    GPR05P,GPR15P                                       0687
*          CALL ATCLCPTR->ATCAP37->ISTAPC37;                       0688
         L     @10,ATCLCPTR                                        0688
         L     @15,ATCAP37(,@10)                                   0688
         BALR  @14,@15                                             0688
*          GEN(NOPR  0)NOSETS NOREFS;                              0689
         NOPR  0
*          RFY (GPR00P,GPR01P,GPR04P,GPR05P,GPR15P) UNRSTD;        0690
*          END; /**** END OF TPQUE GENERATION ****/                0691
*         END;                                                     0692
*    ELSE;                              /* END CTL NO POSTING@DM13003*/
@RF00621 DS    0H                                                  0694
*     IF FMCPRMRC^='05'X &FMCPRMRC^='06'X  /* FMCB INDICATES VARY     *
*        THEN CALL LSTRMEXT;               /*LOST TERM EXIT           *
@RC00621 L     @15,CRRFMCB(,CRRPTR)                                0694
         CLI   FMCPRMRC(@15),X'05'                                 0694
         BE    @RF00694                                            0694
         CLI   FMCPRMRC(@15),X'06'                                 0694
         BE    @RF00694                                            0694
         BAL   @14,LSTRMEXT                                        0695
*     ELSE;                                /* NOT A VARY CASE         *
@RF00694 DS    0H                                                  0697
*     RTNREG=CRRRTN2;                                              0697
         L     RTNREG,CRRRTN2(,CRRPTR)                             0697
*     RETURN;                                                      0698
@EL00003 DS    0H                                                  0698
@EF00003 DS    0H                                                  0698
@ER00003 BR    @14                                                 0698
*END PRGEPOST;                                                     0699
