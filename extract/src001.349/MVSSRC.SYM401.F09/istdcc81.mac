*/* ROUTINE NAME : FREEFSB - FREE FSB/FDB STORAGE                    */
*/*                                                                  */
*/* FUNCTION :                                                       */
*/*    FREEFSB FREES ALL STORAGE FOR FSB AND FDB BUFFERS             */
*/*                                                                  */
*/* INPUT :  FSBPTR (REG 6) - ADDRESS OF FSB                         */
*/*          LCPBPTR (REG 4) - REQUESTOR'S PURGE LCPB                */
*/*                                                                  */
*/* OUTPUT :   FSB/FDB STORAGE FREED                                 */
*/*                                                                  */
*/* SUBROUTINES CALLED : NONE                                        */
*/*                                                                  */
*/*  MACROS USED :  RELSTORE                                         */
*/*                                                                  */
*/********************************************************************/
*FREEFSB: PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA,NOPARMREG);          0700
FREEFSB  DS    0H                                                  0701
*     CRRRTN3=RTNREG;                      /*SAVE RETURN REG          *
*                                                                  0701
         ST    RTNREG,CRRRTN3(,CRRPTR)                             0701
*   /********************************************************      0702
*    * THIS ROUTINE RELEASES THE STORAGE FOR THE FSB AND ALL*      0702
*    * FDBS IN THE CHAIN                                    *      0702
*    *******************************************************/      0702
*                                                                  0702
*         RFY FDBREG RSTD;                                         0702
*         FDBREG=ADDR(FSBFDB);             /* GET ADDR FIRST FDB      *
         LA    FDBREG,FSBFDB(,FSBPTR)                              0703
*         RFY ISTFDB BASED(FDBREG);        /* COVER FDB               *
*         FSBPTR=FDBREG-FDBOFSET;          /* POINT TO BEGINNING OF   *
*                                           * BLOCK TO BE FREED   */
         LR    FSBPTR,FDBREG                                       0705
         SH    FSBPTR,FDBOFSET(,FDBREG)                            0705
*         FSBPTR->FDBFDBA=FDBFDBA;         /* MAKE 1ST WORD NXT FDB @ *
         L     @15,FDBFDBA(,FDBREG)                                0706
         ST    @15,FDBFDBA(,FSBPTR)                                0706
*         RFY ISTPROCD BASED(ADDR(FMCPROCD));                      0707
*         RFY FDBREG UNRSTD;                                       0708
*         IF PROCFTX=ON THEN              /* IF CONFIDENTIAL TEXT    */
         L     @15,CRRFMCB(,CRRPTR)                                0709
         TM    PROCFTX+48(@15),B'00000001'                         0709
         BNO   @RF00709                                            0709
*             /*RELSTORE BUFADDR(FSBPTR)CLEAR(YES)CHAIN(YES)*/     0710
*              DO;                                                 0710
*/* ATBUILD (ISTATCVT,ISTRPH)  */                                  0711
*/* END OF ATBUILD */                                              0711
*             RFY (GPR02P,GPR03P,GPR04P,GPR15P) RSTD;              0711
*         RFY (GPR01P,GPR13P) RSTD;                                0712
*         GENERATE REFS(GPR01P,GPR02P,GPR13P) SETS(RPHWORK);       0713
*                                                                  0713
          STM GPR02P,GPR13P,RPHWORK(GPR01P)
*         RFY GPR13P UNRSTD;                                       0714
*         GPR04P=FSBPTR × '80000000'X;                             0715
         LR    GPR04P,FSBPTR                                       0715
         O     GPR04P,@CF01149                                     0715
*         DCL 1 FMZA0076 FIXED(31) STATIC LOCAL,                   0716
*             2 * FIXED(8) INIT(80),                               0716
*             2 * PTR(24) INIT(0);                                 0716
*         GPR03F = FMZA0076;                                       0717
*                                                                  0717
         L     GPR03F,FMZA0076                                     0717
*             DO; /* LISATCVT CVTBASE(PAGEABLE,(GPR15P)) */        0718
*               GPR15P = ATCLCPTR; /*SET UP ATCVT POINTER*/        0719
         L     GPR15P,ATCLCPTR                                     0719
*             END; /*LISATCVT*/                                    0720
*         GPR02P = GPR15P->ATCBPDA;                                0721
         L     GPR02P,ATCBPDA(,GPR15P)                             0721
*         GPR15P=GPR15P->ATCSMRS;                                  0722
         L     GPR15P,ATCSMRS(,GPR15P)                             0722
*         DCL FMZL0076 BASED(GPR15P) LABEL VALUERANGE(*);          0723
*         CALL FMZL0076;                                           0724
         BALR  @14,@15                                             0724
*         RFY GPR13P RSTD;                                         0725
*         GENERATE REFS(GPR01P,RPHWORK) SETS(GPR02P,GPR13P);       0726
*                                                                  0726
          LM GPR02P,GPR13P,RPHWORK(GPR01P)
*         RFY (GPR01P,GPR13P) UNRSTD;                              0727
*             RFY (GPR02P,GPR03P,GPR04P,GPR15P) UNRSTD;            0728
*             END ;/**********END OF RELSTORE EXPANSION**********/ 0729
*         ELSE                             /* IF NOT CONFIDENTIAL     *
*             /*RELSTORE BUFADDR(FSBPTR)CLEAR(NO)CHAIN(YES)*/      0730
*              DO;                                                 0730
         B     @RC00709                                            0730
@RF00709 DS    0H                                                  0731
*/* ATBUILD (ISTATCVT,ISTRPH)  */                                  0731
*/* END OF ATBUILD */                                              0731
*             RFY (GPR02P,GPR03P,GPR04P,GPR15P) RSTD;              0731
*         RFY (GPR01P,GPR13P) RSTD;                                0732
*         GENERATE REFS(GPR01P,GPR02P,GPR13P) SETS(RPHWORK);       0733
*                                                                  0733
          STM GPR02P,GPR13P,RPHWORK(GPR01P)
*         RFY GPR13P UNRSTD;                                       0734
*         GPR04P=FSBPTR;                                           0735
         LR    GPR04P,FSBPTR                                       0735
*         DCL 1 FMZA0092 FIXED(31) STATIC LOCAL,                   0736
*             2 * FIXED(8) INIT(80),                               0736
*             2 * PTR(24) INIT(0);                                 0736
*         GPR03F = FMZA0092;                                       0737
*                                                                  0737
         L     GPR03F,FMZA0092                                     0737
*             DO; /* LISATCVT CVTBASE(PAGEABLE,(GPR15P)) */        0738
*               GPR15P = ATCLCPTR; /*SET UP ATCVT POINTER*/        0739
         L     GPR15P,ATCLCPTR                                     0739
*             END; /*LISATCVT*/                                    0740
*         GPR02P = GPR15P->ATCBPDA;                                0741
         L     GPR02P,ATCBPDA(,GPR15P)                             0741
*         GPR15P=GPR15P->ATCSMRS;                                  0742
         L     GPR15P,ATCSMRS(,GPR15P)                             0742
*         DCL FMZL0092 BASED(GPR15P) LABEL VALUERANGE(*);          0743
*         CALL FMZL0092;                                           0744
         BALR  @14,@15                                             0744
*         RFY GPR13P RSTD;                                         0745
*         GENERATE REFS(GPR01P,RPHWORK) SETS(GPR02P,GPR13P);       0746
*                                                                  0746
          LM GPR02P,GPR13P,RPHWORK(GPR01P)
*         RFY (GPR01P,GPR13P) UNRSTD;                              0747
*             RFY (GPR02P,GPR03P,GPR04P,GPR15P) UNRSTD;            0748
*             END ;/**********END OF RELSTORE EXPANSION**********/ 0749
*     RTNREG=CRRRTN3;                      /*RESTORE RETURN REG       *
@RC00709 L     RTNREG,CRRRTN3(,CRRPTR)                             0750
*     RETURN;                                                      0751
@EL00004 DS    0H                                                  0751
@EF00004 DS    0H                                                  0751
@ER00004 BR    @14                                                 0751
