         L     LCCWPTR,LCCWDATA(,@15)                              1420
*          STRTLCCW=LCCWPTR;               /* THIS IS CURRENT LCCW   */
         LR    STRTLCCW,LCCWPTR                                    1421
*          CRRSW1=ON;                      /* MORE LCCWS              *
         OI    CRRSW1(CRRPTR),B'01000000'                          1422
