         TITLE 'IECVDERP - DIRECT ACCESS ERROR RECOVERY PROCEEDURES    *
                        '
IECVDERP CSECT ,                                                   0001
@PROLOG  DS    0H                                                  0002
         USING IECVDERP,BASERG,BASEREG2                    @ZA15984
         LR    BASERG,@15          SET UP BASE REG
         LR    @06,@14             SAVE R14 FOR RETURN     @ZA15984
         STM   @02,@06,0(@13)      SAVE REGS               @ZA15984
         L     BASEREG2,NEWBASE    SET UP SECOND BASE      @ZA15984
         B     FIRST                                       @ZA15056
NEWBASE  DC    A(IECVDERP+4096)                            @ZA15056
*FIRST:                                                            0160
*   EWARG=IOSERP;                   /* INIT EWA BASE                 */
FIRST    L     EWARG,IOSERP(,IOSBRG)                               0160
*   IOSERR='1'B;                    /* INDICATE ERP IN CTL           */
*   IOSEX='1'B;                     /* INDICATE EXCEP COND           */
         OI    IOSERR(IOSBRG),B'00100100'                          0162
*   IF IOSBYP='1'B                  /* BYPASS IOS SEEK       @ZA17978*/
*     THEN                          /* YES                   @ZA17978*/
         TM    IOSBYP(IOSBRG),B'10000000'                          0163
         BNO   @RF00163                                            0163
*     IF ERPRDLOG='0'B&ERPRECAL='0'B/* ERP PGM               @ZA17978*/
*       THEN                                                       0164
         TM    ERPRDLOG+6(EWAPTR),B'11000000'                      0164
         BZ    @RT00164                                            0164
*       GOTO CCNOT0;                /* NO, DONT RESTORE REAL @ZA17978*/
*     ELSE                                                         0166
*       ;                           /* YES, TEST CC          @ZA15059*/
@RF00164 DS    0H                                                  0167
*   ELSE                                                           0167
*     ;                             /* NO, TEST CC           @ZA15059*/
*                                                                  0167
@RF00163 DS    0H                                                  0168
*   /*****************************************************************/
*   /*                                                               */
*   /* RESTORE THE ORIGINAL START ADDRESS INTO THE IOSRST FIELD UNDER*/
*   /* THE FOLLOWING CONDITIONS.......... 1.CC=0-NOT VIRTUAL DEV     */
*   /* 2.CC=0-VIRTUALDEV-NO UNIT CHECK 3.CC=0-VIRTUAL DEV-UNIT       */
*   /* CHECK-NO ENV DATA                                     @ZA15998*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0168
*   IF ECC1OR3='0'B THEN            /* TEST FOR SIO=0        @ZA15695*/
         TM    ECC1OR3+20(IOSBRG),B'00010000'                      0168
         BNZ   @RF00168                                            0168
*     DO;                                                          0169
*       IF UCBRVDEV='1'B THEN       /* VIRTUAL DEV           @ZA15998*/
         TM    UCBRVDEV(UCBRG),B'00001000'                         0170
         BNO   @RF00170                                            0170
*         IF IUNITCK='1'B THEN      /* UNIT CHECK            @ZA15998*/
         TM    IUNITCK+24(IOSBRG),B'00000010'                      0171
         BNO   @RF00171                                            0171
*           IF EWDEDAT='1'B THEN    /* ENV DATA              @ZA15998*/
         TM    EWDEDAT(EWAPTR),B'00010000'                         0172
         BO    @RT00172                                            0172
*             GO TO CCNOT0;         /*                       @ZA15998*/
*           ELSE                                                   0174
*             ;                     /*                       @ZA15998*/
*         ELSE                                                     0175
*           ;                       /*                       @ZA15998*/
@RF00171 DS    0H                                                  0176
*       ELSE                                                       0176
*         ;                         /*                       @ZA15998*/
*                                                                  0176
@RF00170 DS    0H                                                  0177
*       /*************************************************************/
*       /*                                                           */
*       /* SUBROUTINE TO RESTORE IOSRST                      @ZA04222*/
*       /*                                                           */
*       /*************************************************************/
*                                                                  0177
*       RFY                                                        0177
*        (WKRG1) RSTD;              /*                       @ZA04222*/
*       WKRG1=IOSVST;               /* VIRT START ADDR       @ZA04222*/
         L     WKRG1,IOSVST(,IOSBRG)                               0178
*       GEN;                                                       0179
*                                  /*                       @ZA04222 */
         LRA    WKRG1,0(WKRG1)      CONVERT TO REAL         @ZA04222
*       IOSRST=WKRG1;               /* RESTORE REAL START ADDR     0180
*                                                            @ZA04222*/
         ST    WKRG1,IOSRST(,IOSBRG)                               0180
*       RFY                                                        0181
*        (WKRG1) UNRSTD;            /*                       @ZA04222*/
*     END;                          /*                       @ZA15998*/
*CCNOT0:                            /*                       @ZA17978*/
*   IF ERPREV='0'B                  /* FIRST ENTRY?                  */
*     THEN                                                         0183
@RF00168 DS    0H                                                  0183
CCNOT0   TM    ERPREV+6(EWAPTR),B'00000010'                        0183
         BNZ   @RF00183                                            0183
*     DO;                           /* YES,                          */
*       EWDSAVKY=IOSCKEY;           /* SAVE CP PROT KEY              */
         MVC   EWDSAVKY(1,EWAPTR),IOSCKEY(IOSBRG)                  0185
*       EWDSAVOP=IOSOPT;            /* SAVE USER'S OPTS BYTE         */
         MVC   EWDSAVOP(1,EWAPTR),IOSOPT(IOSBRG)                   0186
*       IOSLOG='0'B;                /* INITIAL NO LOGOUT             */
*       IOSMSG='0'B;                /* INITIAL NO MESSAGE            */
         NI    IOSLOG(IOSBRG),B'11111010'                          0188
*     END;                                                         0189
*   ELSE                                                           0190
*     ;                             /* NO                            */
@RF00183 DS    0H                                                  0191
*   IF IUNITCK='0'B                 /* UNIT CHECK?                   */
*     THEN                                                         0191
         TM    IUNITCK+24(IOSBRG),B'00000010'                      0191
         BNZ   @RF00191                                            0191
*     DO;                           /* NO, CLEAR SNS                 */
*       IOSSNS=IOSSNS&&IOSSNS;                                     0193
         XC    IOSSNS(2,IOSBRG),IOSSNS(IOSBRG)                     0193
*       GOTO INTCK;                 /* GO CHECK FOR INTERCEPT        */
         B     INTCK                                               0194
*     END;                                                         0195
*   ELSE                                                           0196
*     ;                             /* YES                           */
@RF00191 DS    0H                                                  0197
*   IF EWABDSNS='1'B                /* SENSE FAIL?                   */
*     THEN                                                         0197
         TM    EWABDSNS(EWAPTR),B'00000001'                        0197
         BO    @RT00197                                            0197
*     GOTO SNSERR;                  /* YES, TREAT AS SEEK CK         */
*   ELSE                                                           0199
*     ;                             /* NO                            */
*   IF UCBTBYT4='08'X               /* 2314?                         */
*     THEN                                                         0200
         CLI   UCBTBYT4(UCBRG),X'08'                               0200
         BE    @RT00200                                            0200
*     GOTO STATCK;                  /* YES, CHK RHA STATUS           */
*   ELSE                                                           0202
*     ;                             /* NO                            */
*   IF EWDEDAT='1'B                 /* LOG FULL?                     */
*     THEN                                                         0203
         TM    EWDEDAT(EWAPTR),B'00010000'                         0203
         BNO   @RF00203                                            0203
*     DO;                           /* YES, LOG & RTY                */
*       GEN;                                                       0205
*                                         /*
                 BAL   @14,LOGSAVE         SAVE LOG INFO
*       GOTO RETRY;                 /* RETRY ORIG REQ                */
         B     RETRY                                               0206
*     END;                                                         0207
*   ELSE                                                           0208
*     ;                             /* NO                            */
@RF00203 DS    0H                                                  0209
*   IF UCBTBYT4='0A'X×UCBRVDEV='1'B /* 3340 OR MSS?          @ZA05468*/
*     THEN                                                         0209
         CLI   UCBTBYT4(UCBRG),X'0A'                               0209
         BE    @RT00209                                            0209
         TM    UCBRVDEV(UCBRG),B'00001000'                         0209
         BO    @RT00209                                            0209
*     GOTO INTCK;                   /* YES, SKIP ZLOG CHK            */
*   ELSE                                                           0211
*     ;                             /* NO                            */
*   IF EWDZLOG='0'B                 /* ZEUS LOG FULL?                */
*     THEN                                                         0212
         TM    EWDZLOG(EWAPTR),B'10000000'                         0212
         BZ    @RT00212                                            0212
*     GOTO INTCK;                   /* NO                            */
*   ELSE                                                           0214
*     ;                             /* YES                           */
*   ERPRDLOG='1'B;                  /* SET RD LOG IND                */
         OI    ERPRDLOG+6(EWAPTR),B'10000000'                      0215
*   GEN;                                                           0216
*                                 /*
         BAL   @14,LOGSAVE         SAVE LOG INFO
*   IOSBYP='1'B;                    /* IND NO SEEK/SFM               */
         OI    IOSBYP(IOSBRG),B'10000000'                          0217
*   IOSCKEY=0;                      /* SET KEY TO X'00'      @ZA13517*/
         MVI   IOSCKEY(IOSBRG),X'00'                               0218
*   RFY                                                            0219
*    (WKRG1,                                                       0219
*     CCWRG1,                                                      0219
*     WKRG2) RSTD;                                                 0219
*   CCWRG1=ADDR(DACP7);             /* READ LOG                      */
         LA    CCWRG1,DACP7                                        0220
*   GEN;                                                           0221
*                                  /*
         BAL   @14,CPBLD           BLD CHAN PGM
*   WKRG2=ADDR(MDRZLOG);            /* RD LOG ADDR           @VS30022*/
         L     WKRG2,EWADDISP-1(,EWAPTR)                           0222
         LA    WKRG2,0(,WKRG2)                                     0222
         LA    WKRG2,MDRZLOG(,WKRG2)                               0222
*   GEN;                                                           0223
         LRA   WKRG2,0(WKRG2)      REAL ADDRESS            @VS30022
*   WKRG1->CCWDATA=WKRG2;           /* INSERT DATE ADDR      @VS30022*/
         STCM  WKRG2,7,CCWDATA(WKRG1)                              0224
*   GOTO RESTART;                   /* GO START ERP CP               */
         B     RESTART                                             0225
*   RFY                                                            0226
*    (WKRG1,                                                       0226
*     CCWRG1,                                                      0226
*     WKRG2) UNRSTD;                                               0226
*                                                                  0226
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK SNS STATUS TO VERIFY SUCCESSFUL RHA/RR0 IGNORE          */
*   /* UNSUCCESSFUL RHA/RR0 FOR INTERVENTION REQUIRED                */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0227
*STATCK:                                                           0227
*   IF EUNITCK='1'B                 /* UNIT CHECK?                   */
*     THEN                                                         0227
STATCK   TM    EUNITCK+8(EWAPTR),B'00000010'                       0227
         BNO   @RF00227                                            0227
*     IF EWDINT='1'B                /* YES, IS INT REQUIRED ALSO     */
*       THEN                                                       0228
         TM    EWDINT(EWAPTR),B'01000000'                          0228
         BO    @RT00228                                            0228
*       GOTO SETBADHA;              /* YES, IGNORE                   */
*     ELSE                                                         0230
*       GOTO SNSRTY;                /* NO, RETRY SENSE               */
         B     SNSRTY                                              0230
*   ELSE                                                           0231
*     ;                             /* NO, CHK FOR OTHER BAD STATUS  */
@RF00227 DS    0H                                                  0232
*   IF EPCI='1'B×EINCLGTH='1'B×EPGMCK='1'B×EPROTCK='1'B×ECDATACK='1'B×
*       EINTCCK='1'B×ECHCK='1'B THEN                               0232
         TM    EPCI+8(EWAPTR),B'11111011'                          0232
         BZ    @RF00232                                            0232
*     ;                             /* UNSUCCESSFUL, RETRY           */
*   ELSE                                                           0234
*     GOTO INTCK;                   /* SUCCESSFUL, SKIP RETRY        */
*SNSRTY:                                                           0235
*   IF EWACNTR3=EWACNT01            /* RETRY CNT MAX?        @ZA05546*/
*     THEN                                                         0235
SNSRTY   CLI   EWACNTR3(EWAPTR),1                                  0235
         BE    @RT00235                                            0235
*     GOTO SETBADHA;                /* YES,PROCEED WITHOUT HA      0236
*                                                            @ZA05546*/
*   ELSE                                                           0237
*     ;                             /* NO,RETRY              @ZA05546*/
*                                                                  0237
*   /*****************************************************************/
*   /*                                                               */
*   /* A RHA/RR0 FAILURE HAS OCCURRED, OVERLAY IOSB STATUS WITH SENSE*/
*   /* STATUS FROM ERWA                                              */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0238
*STATSAV:                           /*                       @ZA05546*/
*   IOSTATUS=EWASSTAT;              /*                       @ZA05546*/
STATSAV  MVC   IOSTATUS(2,IOSBRG),EWASSTAT(EWAPTR)                 0238
*   EWACNTR3=EWACNTR3+EWACNT01;     /* INCREMENT CNTR        @ZA05546*/
         LA    @12,1                                               0239
         SLR   @11,@11                                             0239
         IC    @11,EWACNTR3(,EWAPTR)                               0239
         ALR   @12,@11                                             0239
         STC   @12,EWACNTR3(,EWAPTR)                               0239
*   GOTO RETRY;                     /* RETRY                 @ZA05546*/
         B     RETRY                                               0240
*SETBADHA:                                                         0241
*   ERPBADHA='1'B;                  /* IND NO HA                     */
SETBADHA OI    ERPBADHA+6(EWAPTR),B'00001000'                      0241
*   EWDCCHH=IOSCCHH;                /* SAVE SEEK ADDR FOR MSG        */
*                                                                  0242
         MVC   EWDCCHH(4,EWAPTR),IOSCCHH(IOSBRG)                   0242
*   /*****************************************************************/
*   /*                                                               */
*   /* FOR INTERCEPT CONDITIONS, LOG THE ERROR, RESET IOSCOD TO      */
*   /* NORMAL, AND RESTART USER CH PGM                               */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0243
*INTCK:                                                            0243
*   IF IOSCOD=IOSFINTC              /* INTERCEPT?                    */
*     THEN                                                         0243
INTCK    CLI   IOSCOD(IOSBRG),X'7E'                                0243
         BNE   @RF00243                                            0243
*     DO;                           /* YES,                          */
*       IOSCOD=IOSNRMC;             /* RESET CODE TO NORMAL          */
         MVI   IOSCOD(IOSBRG),X'7F'                                0245
*       GEN;                                                       0246
*                                          /*
                 BAL   @14,LOGSAVE         SAVE LOG INFO
*       GOTO RETRY;                 /* RESTART USER CP               */
         B     RETRY                                               0247
*     END;                                                         0248
*   ELSE                                                           0249
*     ;                             /* NO                            */
@RF00243 DS    0H                                                  0250
*   CCWRG=IOSCSWCA-CCWLNGTH;        /* POINT TO ERR CCW              */
*                                                                  0250
         L     CCWRG,IOSCSWCA-1(,IOSBRG)                           0250
         LA    CCWRG,0(,CCWRG)                                     0250
         SL    CCWRG,@CF00073                                      0250
*   /*****************************************************************/
*   /*                                                               */
*   /* SET UP TO CALL RESIDENT ERROR INTERPRETER                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0251
*   CALLRG=CVTXITP;                 /* ADDR OF ERR INTERP            */
         L     @12,CVTPTR                                          0251
         L     CALLRG,CVTXITP(,@12)                                0251
*   GEN;                                                           0252
*                                                                  0252
         BALR  @14,CALLRG          GOT TO ERR INTERP
* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
*        ERROR INTERPRETER VECTOR TABLE
*
         DC    X'1D',AL1(CCC-*+1)  CHAN CTRL CHK
*
         DC    X'1E',AL1(ICC-*+1)  INTERFACE CTRL CHK
*
         DC    X'1C',AL1(CDC-*+1)  CHAN DATA CHK
*
         DC    X'1F',AL1(CCHN-*+1) CHAINING CHK
*
         DC    X'05',AL1(OVRN-*+1) OVERRUN                     @ZA25317
*
         DC    X'08',AL1(PERM-*+1) PERM ERR
*
         DC    X'03',AL1(EQUP-*+1) EQUIP CHK
*
         DC    X'00',AL1(CREJ-*+1) COMMAND REJECT
*
         DC    X'02',AL1(BUSO-*+1) BUSOUT
*
         DC    X'01',AL1(INT-*+1)  INT REQ'D
*
         DC    X'07',AL1(SEEK-*+1) SEEK CHK
*
         DC    X'0E',AL1(MAM-*+1)  MISS ADDR MARK
*
         DC    X'09',AL1(TVRN-*+1) TRK OVRN / INVALID TRK FMT
*
         DC    X'0C',AL1(NRF-*+1)  NO RECORD FOUND
*
         DC    X'06',AL1(TCC-*+1)  TRK COND CHK
*
         DC    X'04',AL1(DCHK-*+1) DATA CHK
*
         DC    X'0A',AL1(EOC-*+1)  END OF CYLINDER
*
         DC    X'0D',AL1(FPRT-*+1) FILE PROT
*
         DC    X'0F',AL1(OFLO-*+1) OP INCOMPLETE
*
         DC    X'1A',AL1(CPCK-*+1) CHAN PGM CHK
*
         DC    X'1B',AL1(CPRT-*+1) CHAN PROT CHK
*
         DC    X'17',AL1(UNEX-*+1) UNIT EXCEPTION
*
         DC    X'19',AL1(LGTH-*+1) INCORRECT LENGTH
*
         DC    X'16',AL1(UCHK-*+1) UNIT CHK - NO SNS
*
         DC    X'2F',AL1(ENDI-*+1) END OF TEST
*
* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
*
*        BRANCH TABLE FOR RETURN VECTORS FROM ERR INTERP
*
CCC      DS    0H                  CHAN CTRL CHK
ICC      DS    0H                  INTERFACE CTRL CHK
         B     ERCHAN
*
CDC      DS    0H                  CHAN DATA CHK
CCHN     DS    0H                  CHAINING CHK
         B     RTY10D              RETRY 10, NO LOG            @ZA00137
EQUP     B     SAFCHK              CHECK FOR MSS 'SAF'         @G24LSFS
BUSO     DS    0H                  BUSOUT
MAM      DS    0H                  MISS ADDR MK
UCHK     DS    0H                  UNIT CHECK - NO SNS
         B     RTY10               RETRY 10
*
TVRN     DS    0H                  TRK OVERRUN
CPCK     DS    0H                  CH PGM CHK
CPRT     DS    0H                  CH PROT CHK
         B     ERHARD              HARD ERRS
*
PERM     B     PERMCK              PERM ERR
*
SEEK     B     ERSEK               SEEK CHK
*
INT      B     ERINT               INT REQ'D
*
CREJ     B     ERMSG               CMD REJECT
*
OVRN     B     RTY10A              OVERRUN
*
NRF      B     NRFCK               NO RECORD OUUND
*
TCC      B     ERTCC               TRK COND CHK
*
DCHK     B     ERDATA              DATA CHK
*
EOC      B     EREOC               END OF CYLINDER
*
FPRT     B     ERFP                FILE PROTECT
*
OFLO     B     EROFLO              OP INCOMPLETE
*
UNEX     B     EREOF               UNIT EXECPTION
*
LGTH     B     ERLEN               INCORRECT LENGTH
*
ENDI     B     ERNOER              NO ABNORMAL STATUS OR SENSE
*
*   /*****************************************************************/
*   /*                                                               */
*   /* IF EQUIPMENT CHECK ON VIRTUAL 3330, CHECK FOR STAGING ADAPTER */
*   /* FAILURE (SAF).IF SO SET FOR WTO MSG,LOG AND RETRY             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0253
*SAFCHK:                                                           0253
*   IF UCBRVDEV='1'B                /* VIRTUAL 3330          @G24LSFS*/
*       &MSSERMSG='25'X THEN        /* SAF CONDITION         @G24LSFS*/
SAFCHK   TM    UCBRVDEV(UCBRG),B'00001000'                         0253
         BNO   @RF00253                                            0253
         CLI   MSSERMSG+32(EWAPTR),X'25'                           0253
         BNE   @RF00253                                            0253
*     DO;                           /* WTO MSG AND LOG BITS  @G24LSFS*/
*       GEN;                                                       0255
*                 /*
                 BAL  @14,LOGSAVE     SAVE LOG INFO          @G24LSFS
*       IOSMSG='1'B;                /* IND. I/O ERROR MSG    @G24LSFS*/
         OI    IOSMSG(IOSBRG),B'00000100'                          0256
*       EWAFLG3=0;                  /* CLR ERP FLAGS         @G24LSFS*/
         MVI   EWAFLG3(EWAPTR),X'00'                               0257
*       ERPREV='1'B;                /* IND. PREVIOUS ENTRY   @G24LSFS*/
         OI    ERPREV+6(EWAPTR),B'00000010'                        0258
*       EWADDMSG='1'B;              /* INDICATE CCHH AVAIL.  @G24LSFS*/
         OI    EWADDMSG(EWAPTR),B'00000010'                        0259
*       GOTO XCTLWTO;               /* GO TO WTO EXIT        @G24LSFS*/
         B     XCTLWTO                                             0260
*     END;                          /* END SETUP FOR WTO,LOG @G24LSFS*/
*   ELSE                                                           0262
*     GOTO RTY10;                   /* REAL EQUIPMENT CHECK  @G24LSFS*/
*                                                                  0262
*   /*****************************************************************/
*   /*                                                               */
*   /* RETRY 10, LOG AFTER FINAL RETRY (HARDLOG)                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0263
*RTY10B:                                                           0263
*   IF EWACNTR1<EWACNT10            /* CNT MAX?                      */
*     THEN                                                         0263
RTY10B   CLI   EWACNTR1(EWAPTR),10                                 0263
         BL    @RT00263                                            0263
*     GOTO CNTINCR1;                /* NO, GO INCR & RTY             */
*   ELSE                                                           0265
*     ;                             /* YES, FALL THRU                */
*                                                                  0265
*   /*****************************************************************/
*   /*                                                               */
*   /* RETRY 10, LOG BEFORE EACH RETRY (SOFTLOG)                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0266
*RTY10:                                                            0266
*   ;                                                              0266
RTY10    DS    0H                                                  0267
*   GEN;                                                           0267
*                                                                  0267
*                                  /*
         BAL   @14,LOGSAVE         SAVE LOG INFO
*   /*****************************************************************/
*   /*                                                               */
*   /* RETRY 10, NO LOG ACTION                                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0268
*RTY10C:                                                           0268
*   IF EWACNTR1<EWACNT10            /* CNT MAX?                      */
*     THEN                                                         0268
RTY10C   CLI   EWACNTR1(EWAPTR),10                                 0268
         BL    @RT00268                                            0268
*     GOTO CNTINCR1;                /* NO, INCR & RTY                */
*   ELSE                                                           0270
*     ;                             /* YES                           */
*                                                                  0270
*   /*****************************************************************/
*   /*                                                               */
*   /* SET FOR IO ERR MSG                                            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0271
*ERMSG:                                                            0271
*                                                                  0271
*   /*****************************************************************/
*   /*                                                               */
*   /* TEST SENSE BYTE 7 AND 18 FOR UCB LOCKOUT COND.        @G24LSFS*/
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0271
*   IF MSSERMSG='0F'X               /* MSS ERROR SEE BYTE 18 @G24LSFS*/
*       &MSSERRCD='03'X             /* EMBEDDED SEEK-RST CP  @G24LSFS*/
*     THEN                                                         0271
ERMSG    LA    @12,32                                              0271
         ALR   @12,EWAPTR                                          0271
         CLI   MSSERMSG(@12),X'0F'                                 0271
         BNE   @RF00271                                            0271
         CLI   MSSERRCD(@12),X'03'                                 0271
         BE    @RT00271                                            0271
*     GOTO ERFP;                    /* RESTART CHAN PGM      @G24LSFS*/
*   ELSE                                                           0273
*     ;                             /* NOT LOCKOUT CONDITION @G24LSFS*/
@RF00271 DS    0H                                                  0274
*   IOSMSG='1'B;                    /* IND I/O ERR MSG               */
         OI    IOSMSG(IOSBRG),B'00000100'                          0274
*   EWADDMSG='1'B;                  /* INDICATE CCHH AVAILABLE       */
*                                                                  0275
         OI    EWADDMSG(EWAPTR),B'00000010'                        0275
*   /*****************************************************************/
*   /*                                                               */
*   /* SET FOR XCTL TO WRITE TO OPERATOR (WTO)                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0276
*XCTLWTO:                                                          0276
*   IOSPROC=IOSAWTO;                /* IND WTO                       */
XCTLWTO  MVI   IOSPROC(IOSBRG),X'14'                               0276
*   EWDCCW9(1:6)=UCBVOLI;           /* VOL SER FOR WTO               */
         MVC   EWDCCW9(6,EWAPTR),UCBVOLI(UCBRG)                    0277
