DSPL     TITLE 'BLSR3270--IPCS DSPL3270 SUBCOMMAND                     *
                        '
*/* CHANGE ACTIVITY -------------------------------------------------*/
*/*   THIS MODULE WAS WRITTEN FOR @G57LPKB                           */
*/*   THIS MODULE WAS CHANGED FOR @ZA31999                   @ZA31999*/
*/*   THIS MODULE WAS CHANGED FOR @ZA32643                   @ZA32643*/
*/*   THIS MODULE WAS CHANGED FOR @ZA32853                   @ZA32853*/
*/*------------------------------------------------------------------*/
BLSR3270 CSECT ,                                                01S0002
@MAINENT DS    0H                                               01S0002
         USING *,@15                                            01S0002
         B     @PROLOG                                          01S0002
         DC    AL1(16)                                          01S0002
         DC    C'BLSR3270  78.158'                              01S0002
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                  01S0002
         BALR  @12,0                                            01S0002
@PSTART  DS    0H                                               01S0002
         USING @PSTART,@12                                      01S0002
         L     @00,@SIZDATD                                     01S0002
         BLSUALLR R,LV=(0)
         LR    @11,@01                                          01S0002
         USING @DATD,@11                                        01S0002
         ST    @13,@SA00001+4                                   01S0002
         LM    @00,@01,20(@13)                                  01S0002
         ST    @11,8(,@13)                                      01S0002
         LR    @13,@11                                          01S0002
*   RETC=ZZZFLAGI;                  /* ANTICIPATE SUCCESS            */
         SLR   RETC,RETC                                        01S0165
*   SPD3270=0;                      /* NO SUSPEND REQUEST    @ZA31999*/
         MVI   SPD3270,X'00'                                    01S0166
*   ZZ2F3270='0'B;                  /* INFORM MONITOR        @ZA31999*/
*                                                               01S0167
*                                                               01S0167
         NI    ZZ2F3270(R9),B'01111111'                         01S0167
*    DO;/*BLSRPARS MF(EXECUTE,PX)*/                             01S0168
*                                                               01S0168
*    DO;/*BLSRPARD CSECT()DSECT(PD)EQUATE(ESAU)EXIT(EXITRLSA,)MF(EXECUT
*,PX)*/                                                         01S0169
*                                                               01S0169
*    DO;/*BLSUPARS CSECT(PC)DSECT(PD)EXIT(EXITRLSA,)MF(EXECUTE,PX)*/
*      CALL BLSUPARI(ZZ2,PCCSECT,PDAPTR);/*PARSE OPERANDS*/     01S0171
         ST    R9,@AL00001                                      01S0171
         L     @14,PCPTR                                        01S0171
         ST    @14,@AL00001+4                                   01S0171
         LA    @14,PDAPTR                                       01S0171
         ST    @14,@AL00001+8                                   01S0171
         L     @10,ZZ2BVTP(,R9)                                 01S0171
         L     @15,BVTPARIP(,@10)                               01S0171
         LA    @01,@AL00001                                     01S0171
         BALR  @14,@15                                          01S0171
*      RESPECIFY                                                01S0172
*       (GPR15F) RESTRICTED;                                    01S0172
*      IF GPR15F^=ZZZFLAGI THEN                                 01S0173
         LTR   GPR15F,GPR15F                                    01S0173
         BZ    @RF00173                                         01S0173
*        /*UNABLE TO PARSE*/                                    01S0174
*        DO;/*UNABLE TO PARSE*/                                 01S0174
*          RETC=GPR15F;/*RETURN CODE*/                          01S0175
         LR    RETC,GPR15F                                      01S0175
*          GO TO EXITNORL;/*EXIT WITHOUT PDL*/                  01S0176
         B     EXITNORL                                         01S0176
*        END;                                                   01S0177
*      RESPECIFY                                                01S0178
*       (GPR15F) UNRESTRICTED;                                  01S0178
@RF00173 DS    0H                                               01S0179
*      RESPECIFY                                                01S0179
*       (PDRPTR) RESTRICTED;                                    01S0179
*      PDRPTR=PDAPTR;/*->PDL*/                                  01S0180
         L     PDRPTR,PDAPTR                                    01S0180
*      GENERATE REFS(PD,PDRPTR) CODE(USING PD,PDRPTR);          01S0181
         USING PD,PDRPTR
*      IF PXTEST=PXTEST1 THEN                                   01S0182
         CLC   PXTEST(2),@CH00043                               01S0182
         BNE   @RF00182                                         01S0182
*        ZZ2AFX=ZZZ1;/*TEST*/                                   01S0183
         OI    ZZ2AFX(R9),B'10000000'                           01S0183
*      ELSE                                                     01S0184
*        IF PXTEST=PXTEST0 THEN                                 01S0184
         B     @RC00182                                         01S0184
@RF00182 CLC   PXTEST(2),@CH00068                               01S0184
         BNE   @RF00184                                         01S0184
*          ZZ2AFX=ZZZ0;/*NOTEST*/                               01S0185
         NI    ZZ2AFX(R9),B'01111111'                           01S0185
*    END;/*BLSUPARS CSECT(PC)DSECT(PD)EXIT(EXITRLSA,)MF(EXECUTE,PX)*/
@RF00184 DS    0H                                               01S0186
@RC00182 DS    0H                                               01S0187
*    END;/*BLSRPARD CSECT()DSECT(PD)EQUATE(ESAU)EXIT(EXITRLSA,)MF(EXECU
*E,PX)*/                                                        01S0187
*    END;/*BLSRPARS MF(EXECUTE,PX)*/                            01S0188
*   VPASP=ADDR(BLSRVPAS);           /* ->BLSRVPAS                    */
         L     @14,@CV01219                                     01S0189
         ST    @14,VPASP                                        01S0189
*   IOPLECB=ADDR(ZZ1EVE);           /* ->ECB                         */
         L     @14,ZZ2ZZ1P(,R9)                                 01S0190
         LA    @08,ZZ1EVE(,@14)                                 01S0190
         ST    @08,IOPLECB                                      01S0190
*   IOPLUPT=ADDR(UPT);              /* ->UPT                         */
         L     @08,CPPLUPT(,R9)                                 01S0191
         ST    @08,IOPLUPT                                      01S0191
*   IOPLECT=ADDR(ECT);              /* ->ECT                         */
         L     @08,CPPLECT(,R9)                                 01S0192
         ST    @08,IOPLECT                                      01S0192
*   IF ZZ1TPAGE^=24 THEN            /* NUMBER OF SCREEN LINES        */
*                                                               01S0193
         CLC   ZZ1TPAGE(4,@14),@CF00217                         01S0193
         BE    @RF00193                                         01S0193
*     /***************************************************************/
*     /*                                                             */
*     /* BLSR3270 REQUIRES A 3277 TSO TERMINAL--DIAGNOSE AND QUIT    */
*     /*                                                             */
*     /***************************************************************/
*                                                               01S0194
*     DO;                           /* NONGRAPHIC TSO TERMINAL       */
*       CALL BLSUTRMV(ZZ2,MSG2);    /* TRANSMIT MSG2                 */
         ST    R9,@AL00001                                      01S0195
         LA    @14,MSG2                                         01S0195
         ST    @14,@AL00001+4                                   01S0195
         L     @15,ZZ2TRMVP(,R9)                                01S0195
         LA    @01,@AL00001                                     01S0195
         BALR  @14,@15                                          01S0195
*       RETC=MAX(RETC,RF,ZZZFLAGT); /* ERROR RETURN CODE             */
         LR    @14,RF                                           01S0196
         CR    @14,RETC                                         01S0196
         BNL   *+6
         LR    @14,RETC                                         01S0196
         LA    @08,16                                           01S0196
         CR    @14,@08                                          01S0196
         BNL   *+6
         LR    @14,@08                                          01S0196
         LR    RETC,@14                                         01S0196
*       GO TO EXITRLSA;             /* EXIT WITH PDL                 */
         B     EXITRLSA                                         01S0197
*     END;                          /* NONGRAPHIC TSO TERMINAL       */
*   IF PDPAUSE=PDPAUSE1 THEN        /* PAUSE OPERAND ENTERED         */
*                                                               01S0199
@RF00193 CLC   PDPAUSE(2),@CH00043                              01S0199
         BNE   @RF00199                                         01S0199
*     /***************************************************************/
*     /*                                                             */
*     /* PAUSE BEFORE SCREEN RE-WRITE                                */
*     /*                                                             */
*     /***************************************************************/
*                                                               01S0200
*     DO;                           /* PAUSE BEFORE SCREEN RE-WRITE  */
*       RESPECIFY                                               01S0201
*        (R0,                       /* TPUT INTERFACE REGISTER       */
*         R1) RESTRICTED;           /* TPUT INTERFACE REGISTER       */
*       IF UPTMID='1'B THEN         /* MESSAGES ID'S WANTED  @ZA31999*/
         L     @14,CPPLUPT(,R9)                                 01S0202
         TM    UPTMID(@14),B'00100000'                          01S0202
         BNO   @RF00202                                         01S0202
*         LM(R0,R1,TPUTPMID);       /* PAUSE WITH MESSAGE ID @ZA31999*/
         LM    R0,R1,TPUTPMID                                   01S0203
*       ELSE                        /* NO MESSAGE ID'S       @ZA31999*/
*         LM(R0,R1,TPUTP);          /* PAUSE WITHOUT MSGID'S @ZA31999*/
         B     @RC00202                                         01S0204
@RF00202 LM    R0,R1,TPUTP                                      01S0204
*       RF=8;                       /* ATTENTION RETURN CODE         */
@RC00202 LA    RF,8                                             01S0205
*       DO WHILE(RF=8);             /* TPUT/TGET                     */
         B     @DE00206                                         01S0206
@DL00206 DS    0H                                               01S0207
*         IF ZZ1EVEP='1'B THEN      /* ATTENTION INTERRUPT   @ZA31999*/
         L     @14,ZZ2ZZ1P(,R9)                                 01S0207
         TM    ZZ1EVEP(@14),B'01000000'                         01S0207
         BNO   @RF00207                                         01S0207
*           DO;                     /* EXIT DSPL3270         @ZA31999*/
*             RETC=ZZZFLAGS;        /* SERIOUS CONDITION     @ZA31999*/
         LA    RETC,12                                          01S0209
*             GO TO EXITRLSA;       /* EXIT WITH PDL         @ZA31999*/
         B     EXITRLSA                                         01S0210
*           END;                    /* EXIT DSPL3270         @ZA31999*/
*         CALL E130;                /* DISPLAY PROMPTING MESSAGE     */
@RF00207 BAL   @14,E130                                         01S0212
*         IF RF^=8 THEN                                         01S0213
         C     RF,@CF00051                                      01S0213
         BE    @RF00213                                         01S0213
*           CALL E120;              /* GET BUT IGNORE RESPONSE       */
         BAL   @14,E120                                         01S0214
*       END;                        /* TPUT/TGET                     */
@RF00213 DS    0H                                               01S0215
@DE00206 C     RF,@CF00051                                      01S0215
         BE    @DL00206                                         01S0215
*       RESPECIFY                                               01S0216
*        (R0,                       /* TPUT INTERFACE REGISTER       */
*         R1) UNRESTRICTED;         /* TPUT INTERFACE REGISTER       */
*       RETC=MAX(RETC,RF);          /* RETURN CODE                   */
         CR    RETC,RF                                          01S0217
         BNL   *+6
         LR    RETC,RF                                          01S0217
*     END;                          /* PAUSE BEFORE SCREEN RE-WRITE  */
*                                                               01S0218
*   /*****************************************************************/
*   /*                                                               */
*   /* LOCATE THE SYSTEM CVT                                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0219
*   ESAUSYM='CVT';                  /* SYMBOL                        */
@RF00199 MVI   ESAUSYM+3,C' '                                   01S0219
         MVC   ESAUSYM+4(27),ESAUSYM+3                          01S0219
         MVC   ESAUSYM(3),@CC01017                              01S0219
*   ESAUDT=DTCVT;                   /* DATA TYPE                     */
         MVC   ESAUDT(34),DTCVT                                 01S0220
*   CALL BLSRESGU(ZZ2,ESAU);        /* LOCATE THE SYSTEM CVT         */
         ST    R9,@AL00001                                      01S0221
         LA    @14,ESAU                                         01S0221
         ST    @14,@AL00001+4                                   01S0221
         L     @08,ZZ2RVTP(,R9)                                 01S0221
         L     @15,RVTESGUP(,@08)                               01S0221
         LA    @01,@AL00001                                     01S0221
         BALR  @14,@15                                          01S0221
*   RETC=MAX(RETC,RF);              /* RETURN CODE                   */
         CR    RETC,RF                                          01S0222
         BNL   *+6
         LR    RETC,RF                                          01S0222
*   IF RF>ZZZFLAGE THEN             /* SERIOUS ERROR                 */
         C     RF,@CF00051                                      01S0223
         BH    @RT00223                                         01S0223
*     GO TO EXITRLSA;               /* EXIT WITH PDL                 */
*   ZZ2AMDC=ESAULAD;                /* ->CVT                         */
*                                                               01S0225
         L     @14,ESAULAD                                      01S0225
         ST    @14,ZZ2AMDC(,R9)                                 01S0225
*   /*****************************************************************/
*   /*                                                               */
*   /* GET CURRENT SYMBOL                                            */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0226
*   ESAUSYM='X';                    /* SYMBOL                        */
         MVI   ESAUSYM+1,C' '                                   01S0226
         MVC   ESAUSYM+2(29),ESAUSYM+1                          01S0226
         MVI   ESAUSYM,C'X'                                     01S0226
*   CALL BLSRESGE(ZZ2,ESAU);        /* RETRIEVE ZZZSYMCU RECORD      */
         ST    R9,@AL00001                                      01S0227
         LA    @14,ESAU                                         01S0227
         ST    @14,@AL00001+4                                   01S0227
         L     @08,ZZ2RVTP(,R9)                                 01S0227
         L     @15,RVTESGEP(,@08)                               01S0227
         LA    @01,@AL00001                                     01S0227
         BALR  @14,@15                                          01S0227
*   RETC=MAX(RETC,RF);              /* RETURN CODE                   */
         CR    RETC,RF                                          01S0228
         BNL   *+6
         LR    RETC,RF                                          01S0228
*   IF RF>ZZZFLAGE THEN             /* SERIOUS ERROR                 */
         C     RF,@CF00051                                      01S0229
         BH    @RT00229                                         01S0229
*     GO TO EXITRLSA;               /* EXIT WITH PDL                 */
*   IF RF=0&ESAUAST=ZZZASTCV THEN   /* SUCCESSFUL AND VIRTUAL        */
         LTR   RF,RF                                            01S0231
         BNZ   @RF00231                                         01S0231
         CLC   ESAUAST(2),@CC00075                              01S0231
         BNE   @RF00231                                         01S0231
*     ESAULAD=ESAULAD+ESAUDOF;      /* OFFSET NOT SUPPORTED          */
         L     @14,ESAULAD                                      01S0232
         AL    @14,ESAUDOF                                      01S0232
         ST    @14,ESAULAD                                      01S0232
*   ELSE                                                        01S0233
*                                                               01S0233
*     /***************************************************************/
*     /*                                                             */
*     /* DEFAULT CURRENT SYMBOL REQUIRED                             */
*     /*                                                             */
*     /***************************************************************/
*                                                               01S0233
*     DO;                           /* PROVIDE DEFAULT TO DSPL3270   */
         B     @RC00231                                         01S0233
@RF00231 DS    0H                                               01S0234
*       ESAUDTY=ZZZDTYB;            /* DATA FORMAT HEX               */
         MVI   ESAUDTY,C'B'                                     01S0234
*       ESAULAD=0;                  /* ADDRESS(0.)                   */
         SLR   @14,@14                                          01S0235
         ST    @14,ESAULAD                                      01S0235
*       ESAUAS=ZZ6DQA;              /* ADDRESS SPACE                 */
         L     @14,ZZ2AZZ6P(,R9)                                01S0236
         MVC   ESAUAS(16),ZZ6DQA(@14)                           01S0236
*       RETC=MAX(RETC,ZZZFLAGW);    /* RETURN CODE                   */
         LA    @14,4                                            01S0237
         CR    RETC,@14                                         01S0237
         BNL   *+6
         LR    RETC,@14                                         01S0237
*     END;                          /* PROVIDE DEFAULT TO DSPL3270   */
*   SESAUDTY=ESAUDTY;               /* SAVE INCOMING DATA TYPE       */
@RC00231 MVC   SESAUDTY(1),ESAUDTY                              01S0239
*   ESAUD=DESCR;                    /* STORAGE ATTRIBUTES            */
         MVC   ESAUD(60),DESCR                                  01S0240
*   ESAUDLE=MIN(ZZ2AQDLE,ZZZP24P1-ESAULAD);/* MAKE SURE ADDR+LEN DOES
*                                      NOT EXCEED MAX VALID ADR      */
         L     @14,@CF00121                                     01S0241
         SL    @14,ESAULAD                                      01S0241
         L     @08,ZZ2AQDLE(,R9)                                01S0241
         CLR   @08,@14                                          01S0241
         BNH   *+6
         LR    @08,@14                                          01S0241
         ST    @08,ESAUDLE                                      01S0241
*   ESAUDTY=SESAUDTY;               /* RESET TO ORIGINAL VALUE       */
         MVC   ESAUDTY(1),SESAUDTY                              01S0242
*   CALL BLSRESAR(ZZ2,ESAU);        /* UPDATE ZZZSYMCU RECORD        */
         ST    R9,@AL00001                                      01S0243
         LA    @14,ESAU                                         01S0243
         ST    @14,@AL00001+4                                   01S0243
         L     @08,ZZ2RVTP(,R9)                                 01S0243
         L     @15,RVTESARP(,@08)                               01S0243
         LA    @01,@AL00001                                     01S0243
         BALR  @14,@15                                          01S0243
*   RETC=MAX(RETC,RF);              /* RETURN CODE                   */
         CR    RETC,RF                                          01S0244
         BNL   *+6
         LR    RETC,RF                                          01S0244
*   IF RF>ZZZFLAGE THEN             /* SERIOUS ERROR                 */
         C     RF,@CF00051                                      01S0245
         BH    @RT00245                                         01S0245
*     GO TO EXITRLSA;               /* EXIT WITH PDL                 */
*                                                               01S0246
*   /*****************************************************************/
*   /*                                                               */
*   /* ESTABLISH SCREEN DEFAULT VALUES                               */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0247
*   WKRESET='1'B;                   /* INITIALIZE WORK AREA          */
         MVI   WKRESET,X'01'                                    01S0247
*   PHSY=PHCO;                      /* INITIALIZE PH RECORD  @ZA32853*/
         L     @14,ZZ2PHSYP(,R9)                                01S0248
         MVC   PHSY(48),PHCO(@14)                               01S0248
*   PHSYPGM='BLSR3270';             /* PROGRAM NAME                  */
         MVC   PHSYPGM(8),@CC01230                              01S0249
*   PHSYMRX='0'B;                   /* MULTIPLE RECORD INDEX         */
         SLR   @14,@14                                          01S0250
         ST    @14,PHSYMRX                                      01S0250
*   PHSYPL=ADDR(WKALENG);           /* WKALENG=EQUATED LENGTH        */
         LA    @14,WKALENG                                      01S0251
         STH   @14,PHSYPL                                       01S0251
*   IF PDRESET^=PDRESET1 THEN       /* NO RESET                      */
*                                                               01S0252
         CLC   PDRESET(2),@CH00043                              01S0252
         BE    @RF00252                                         01S0252
*     /***************************************************************/
*     /*                                                             */
*     /* USE PREVIOUS VALUES, IF ANY                                 */
*     /*                                                             */
*     /***************************************************************/
*                                                               01S0253
*     DO;                                                       01S0253
*                                                               01S0253
*        DO;/*BLSULINK(ZZ2,PHSY) EP('BLSRPHGE')*/               01S0254
*          ZZ2PGM='BLSRPHGE';/*ENTRY POINT NAME*/               01S0255
         MVC   ZZ2PGM(8,R9),@CC01231                            01S0255
*          CALL BLSUPGMC(ZZ2,PHSY);/*CALL NON-RESIDENT IPCS PROGRAM*/
         ST    R9,@AL00001                                      01S0256
         LA    @14,PHSY                                         01S0256
         ST    @14,@AL00001+4                                   01S0256
         L     @08,ZZ2BVTP(,R9)                                 01S0256
         L     @15,BVTPGMCP(,@08)                               01S0256
         LA    @01,@AL00001                                     01S0256
         BALR  @14,@15                                          01S0256
*        END;/*BLSULINK(ZZ2,PHSY) EP('BLSRPHGE')*//* GET HISTORY RECORD
*    */                                                         01S0257
*       IF RF>ZZZFLAGW THEN         /* ERROR CONDITION               */
*                                                               01S0258
         C     RF,@CF00040                                      01S0258
         BNH   @RF00258                                         01S0258
*         /***********************************************************/
*         /*                                                         */
*         /* HISTORY RECORD RETRIEVAL FAILED BADLY                   */
*         /*                                                         */
*         /***********************************************************/
*                                                               01S0259
*         DO;                       /* TERMINATE PROCESSING          */
*           RETC=ZZZFLAGS;          /* RETURN CODE                   */
         LA    RETC,12                                          01S0260
*           GO TO EXITRLSA;         /* EXIT WITH PDL                 */
         B     EXITRLSA                                         01S0261
*         END;                      /* TERMINATE PROCESSING          */
*       ELSE                        /* NO UNEXPECTED ERROR           */
*         IF RF<ZZZFLAGW THEN       /* RECORD AVAILABLE              */
@RF00258 C     RF,@CF00040                                      01S0263
         BNL   @RF00263                                         01S0263
*           WKRESET='0'B;           /* INDICATE WORK AREA INITIALIZED*/
         MVI   WKRESET,X'00'                                    01S0264
*     END;                          /* GET HISTORY RECORD            */
@RF00263 DS    0H                                               01S0266
*   IF ZZ1EVEP='1'B THEN            /* ATTENTION INTERRUPTION        */
@RF00252 L     @14,ZZ2ZZ1P(,R9)                                 01S0266
         TM    ZZ1EVEP(@14),B'01000000'                         01S0266
         BNO   @RF00266                                         01S0266
*     DO;                           /* EXIT DSPL3270                 */
*       RETC=ZZZFLAGS;              /* SERIOUS CONDITION             */
         LA    RETC,12                                          01S0268
*       GO TO EXITRLSA;             /* EXIT WITH PDL                 */
         B     EXITRLSA                                         01S0269
*     END;                          /* EXIT DSPL3270                 */
*                                                               01S0270
*   /*****************************************************************/
*   /*                                                               */
*   /* GIVE BLSR327A CONTROL                                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0271
*   ZZ2AFFS='1'B;                   /* INDICATE IN FULL SCREEN MODE  */
@RF00266 OI    ZZ2AFFS(R9),B'00100000'                          01S0271
*   IF ZZ1FACCM='0'B×ZZ1FTCAM='0'B THEN /* DON'T KNOW OR HAVE VTAM   */
         L     @14,ZZ2ZZ1P(,R9)                                 01S0272
         TM    ZZ1FACCM(@14),B'11000000'                        01S0272
         BO    @RF00272                                         01S0272
*     DO;                           /* PROCESS FOR VTAM POSSIBLE
*                                                            @ZA31999*/
*       CALL E150;                  /* ISSUE VTAM STFSMODE   @ZA31999*/
         BAL   @14,E150                                         01S0274
*       IF RF>8 THEN                /* UNEXPECTED RET CODE   @ZA32643*/
         C     RF,@CF00051                                      01S0275
         BNH   @RF00275                                         01S0275
*         DO;                       /* EXIT DSPL3270         @ZA31999*/
*           RETC=ZZZFLAGT;          /* ENVIRONMENTAL ERROR   @ZA31999*/
         LA    RETC,16                                          01S0277
*           GO TO EXITRLSA;         /* EXIT                  @ZA31999*/
         B     EXITRLSA                                         01S0278
*         END;                      /* EXIT DSPL3270         @ZA31999*/
*       IF ZZ1FACCM='0'B THEN       /* ACCESS MODE HAS NOT BEEN SET  */
@RF00275 L     @14,ZZ2ZZ1P(,R9)                                 01S0280
         TM    ZZ1FACCM(@14),B'10000000'                        01S0280
         BNZ   @RF00280                                         01S0280
*         DO;                       /* SET INDICATORS        @ZA31999*/
*           IF RF^=0 THEN           /* DON'T HAVE VTAM       @ZA32643*/
         LTR   RF,RF                                            01S0282
         BZ    @RF00282                                         01S0282
*             ZZ1FTCAM='1'B;        /* SET INDICATOR - HAVE TCAM
*                                                            @ZA31999*/
         OI    ZZ1FTCAM(@14),B'01000000'                        01S0283
*           ZZ1FACCM='1'B;          /* INDICATE MODE SET     @ZA31999*/
@RF00282 L     @14,ZZ2ZZ1P(,R9)                                 01S0284
         OI    ZZ1FACCM(@14),B'10000000'                        01S0284
*         END;                      /* SET INDICATORS        @ZA31999*/
*     END;                          /* PROCESS FOR VTAM POSSIBLE
*                                                            @ZA31999*/
@RF00280 DS    0H                                               01S0287
*   ZZ1P=ADDR(ZZ1);                 /* ->ZZ1                         */
@RF00272 L     @14,ZZ2ZZ1P(,R9)                                 01S0287
         ST    @14,ZZ1P                                         01S0287
*   ZZ2P=ADDR(ZZ2);                 /* ->ZZ2                         */
         ST    R9,ZZ2P                                          01S0288
*   CALL DSPL3270;                  /* ASMXF SECTION                 */
         L     @15,D3270P                                       01S0289
         BALR  @14,@15                                          01S0289
*   RETC=MAX(RETC,RF);              /* RETURN CODE                   */
         CR    RETC,RF                                          01S0290
         BNL   *+6
         LR    RETC,RF                                          01S0290
