*/* BLSRSAPU--ADD A STORAGE ADDRESS RECORD                           */
*/*------------------------------------------------------------------*/
*                                                               01S0132
*   SAPAGMT=ZZ2TOD;                 /* UPDATE TIME STAMP             */
         L     @10,@PC00001+4                                   01S0132
         MVC   SAPAGMT(8,@10),ZZ2TOD(R9)                        01S0132
*   RESPECIFY                                                   01S0133
*    (R2,                           /* RECORD LENGTH                 */
*     R3,                           /* ->ACB                         */
*     R4,                           /* ->RECORD AREA AND KEY         */
*     R5,                           /* RECORD LENGTH                 */
*     RF) RESTRICTED;               /* GENCB RETURN CODE             */
*   R2=SAPALKL;                     /* KEY LENGTH                    */
         LA    R2,66                                            01S0134
*   R3=ZZ1ACBP;                     /* ->ACB                         */
         L     @14,ZZ2ZZ1P(,R9)                                 01S0135
         L     R3,ZZ1ACBP(,@14)                                 01S0135
*   R4=ADDR(SAPA);                  /* ->RECORD AREA AND KEY         */
         LR    R4,@10                                           01S0136
*   R5=SAPALRL+SAPACL;              /* RECORD LENGTH                 */
         LA    R5,256                                           01S0137
         SLR   @14,@14                                          01S0137
         ICM   @14,3,SAPACL(@10)                                01S0137
         ALR   R5,@14                                           01S0137
*   GENERATE SETS(GENPARM) CODE;                                01S0138
*---------------------------------------------------------------------*
*   CREATE AN RPL TO ADD THE EQUATE SYMBOL RECORD                     *
*---------------------------------------------------------------------*
         GENCB BLK=RPL,WAREA=(S,MYRPL),LENGTH=256,KEYLEN=(R2),         *
               ACB=(R3),AREA=(R4),RECLEN=(R5),OPTCD=(DIR,MVE),         *
               MF=(L,GENPARM,GENPARML)
*   RESPECIFY                                                   01S0139
*    (R2,                           /* RECORD LENGTH                 */
*     R3,                           /* ->ACB                         */
*     R4,                           /* ->RECORD AREA AND KEY         */
*     R5) UNRESTRICTED;             /* KEY LENGTH                    */
*   CALL BLSUVSCR(ZZ2,GENPARM);     /* CREATE AN RPL                 */
         ST    R9,@AL00001                                      01S0140
         LA    @14,GENPARM                                      01S0140
         ST    @14,@AL00001+4                                   01S0140
         L     @10,ZZ2BVTP(,R9)                                 01S0140
         L     @15,BVTVSCRP(,@10)                               01S0140
         LA    @01,@AL00001                                     01S0140
         BALR  @14,@15                                          01S0140
*   IF RF=ZZZFLAGI THEN             /* RPL SUCCESSFULLY CREATED      */
         LTR   RF,RF                                            01S0141
         BNZ   @RF00141                                         01S0141
*     CALL BLSUVSPU(ZZ2,MYRPL);     /* ADD THE RECORD                */
         ST    R9,@AL00001                                      01S0142
         LA    @14,MYRPL                                        01S0142
         ST    @14,@AL00001+4                                   01S0142
         L     @10,ZZ2BVTP(,R9)                                 01S0142
         L     @15,BVTVSPUP(,@10)                               01S0142
         LA    @01,@AL00001                                     01S0142
         BALR  @14,@15                                          01S0142
*   RETURN CODE(RF);                                            01S0143
*                                                               01S0143
@RF00141 LR    @10,@15                                          01S0143
         L     @13,4(,@13)                                      01S0143
         L     @00,@SIZDATD                                     01S0143
         LR    @01,@11                                          01S0143
         BLSUFRES R,LV=(0),A=(1)
         LR    @15,@10                                          01S0143
         L     @14,12(,@13)                                     01S0143
         LM    @00,@12,20(@13)                                  01S0143
