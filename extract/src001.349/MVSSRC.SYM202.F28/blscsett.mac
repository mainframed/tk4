         TITLE 'BLSCSETT -- IPCS DATA ACCESS SERVICES MODCB EXECUTOR   *
                        '
*     /* CHANGE ACTIVITY
*    THIS MODULE IS WRITTEN FOR @G57LPSR
BLSCSETT CSECT ,                                                   0002
@MAINENT DS    0H                                                  0002
         USING *,@15                                               0002
         B     @PROLOG                                             0002
         DC    AL1(16)                                             0002
         DC    C'BLSCSETT  78.062'                                 0002
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0002
         BALR  @12,0                                               0002
@PSTART  DS    0H                                                  0002
         USING @PSTART,@12                                         0002
         L     @00,@SIZDATD                                        0002
         BLSCGETS R,LV=(0)
         LR    @11,@01                                             0002
         USING @DATD,@11                                           0002
         ST    @13,@SA00001+4                                      0002
         LM    @00,@01,20(@13)                                     0002
         ST    @11,8(,@13)                                         0002
         LR    @13,@11                                             0002
*   CBPTR=GPR01P;                   /* ESTABLISH ADDRESSIBILITY TO 0022
*                                      DMCB                          */
         LR    CBPTR,GPR01P                                        0022
*   RFY                                                            0023
*     GPR01P UNRSTD;                                               0023
*   MODNAME=LMODNMC;                /* MARK AUTOMATIC AREA           */
         MVC   MODNAME(8),@CC00919                                 0024
*   SUBCODE=F0C;                    /* SUB-ROUTINE RETURN CODE       */
         SLR   @10,@10                                             0025
         ST    @10,SUBCODE                                         0025
*   DMCBRTC=F0C;                    /* WE ARE SUCCESSFUL UNTIL WE  0026
*                                      FAIL                          */
         ST    @10,DMCBRTC(,CBPTR)                                 0026
         EJECT
*                                                                  0027
*   /*****************************************************************/
*   /*                                                               */
*   /* IF THE DMCB IS NOT OPEN THEN REJECT THE REQUEST               */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0027
*   IF DMCBOPN=LHEX0C THEN                                         0027
         CLI   DMCBOPN(CBPTR),X'00'                                0027
         BNE   @RF00027                                            0027
*     DO;                           /* DMCB NOT OPEN                 */
*       DMCBREJ=F0C;                                               0029
         MVI   DMCBREJ(CBPTR),X'00'                                0029
*       DMCBRTC=INVRQ;                                             0030
         MVC   DMCBRTC(4,CBPTR),@CF00037                           0030
*       RETURN;                                                    0031
@EL00001 L     @13,4(,@13)                                         0031
@EF00001 L     @00,@SIZDATD                                        0031
         LR    @01,@11                                             0031
         BLSCFRES R,LV=(0),A=(1)
@ER00001 LM    @14,@12,12(@13)                                     0031
         BR    @14                                                 0031
*     END;                          /* DMCB NOT OPEN                 */
*                                                                  0032
*   /*****************************************************************/
*   /*                                                               */
*   /* IF THE DMCB REPRESENTS A QSAM DATA SET THEN NOTHING SHOULD BE */
*   /* DONE                                                          */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0033
*   IF DMCBVSF=OFF THEN                                            0033
@RF00027 TM    DMCBVSF(CBPTR),B'00000001'                          0033
         BZ    @RT00033                                            0033
*     RETURN;                                                      0034
*                                                                  0034
*   /*****************************************************************/
*   /*                                                               */
*   /* INITIALIZE THE PLACEHOLDER. THE FIRST KEYWORD ELEMENT WILL BE */
*   /* THE SECOND PARAMETER IN THE LIST. THE HEADER WILL ONLY BE     */
*   /* BUILT IF THERE IS AT LEAST ONE MODIFICATION TO BE MADE TO THE */
*   /* RPL                                                           */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0035
*   RFY                                                            0035
*     ELEM BASED(ADDR(ELEMS(PLACEHDR)));                           0035
*   PLACEHDR=F1C;                                                  0036
*                                                                  0036
         LA    PLACEHDR,1                                          0036
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK THE SIX ITEMS IN THE DMCB (BUFP, BLEN, KEYP, KEYL,      */
*   /* RECLEN, AND OPTS) WHICH CAUSE MODIFICATION TO THE DMCB        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0037
*   IF DMCBBFPC=OFF THEN            /* DID BUFFER POINTER CHANGE     */
         TM    DMCBBFPC(CBPTR),B'10000000'                         0037
         BNZ   @RF00037                                            0037
*     CALL MOD(AREA,DMCBBUFP);                                     0038
         LA    @10,@CH00119                                        0038
         ST    @10,@AL00001                                        0038
         LA    @10,DMCBBUFP(,CBPTR)                                0038
         ST    @10,@AL00001+4                                      0038
         LA    @01,@AL00001                                        0038
         BAL   @14,MOD                                             0038
*   IF DMCBBLEN>=F0C THEN           /* DID THE BUFFER LENGTH CHANGE  */
@RF00037 L     @10,DMCBBLEN(,CBPTR)                                0039
         LTR   @10,@10                                             0039
         BM    @RF00039                                            0039
*     CALL MOD(AREALEN,DMCBBLEN);                                  0040
         LA    @10,@CH00876                                        0040
         ST    @10,@AL00001                                        0040
         LA    @10,DMCBBLEN(,CBPTR)                                0040
         ST    @10,@AL00001+4                                      0040
         LA    @01,@AL00001                                        0040
         BAL   @14,MOD                                             0040
*   IF DMCBOPTS^=DMCBLOPT           /* HAVE OPTIONS CHANGED          */
*       &DMCBKYD=OFF&DMCBRNO=ON THEN/* TO RELATIVE RECORD            */
@RF00039 CLC   DMCBOPTS(1,CBPTR),DMCBLOPT(CBPTR)                   0041
         BE    @RF00041                                            0041
         TM    DMCBRNO(CBPTR),B'01000000'                          0041
         BNO   @RF00041                                            0041
         TM    DMCBKYD(CBPTR),B'10000000'                          0041
         BNZ   @RF00041                                            0041
*     CALL MOD(ARG,ADDR(DMCBRBA));  /* USE RBA FIELD FOR ARGUMENT    */
         LA    @10,@CH00878                                        0042
         ST    @10,@AL00001                                        0042
         LA    @10,DMCBRBA(,CBPTR)                                 0042
         ST    @10,@AFTEMPS                                        0042
         LA    @10,@AFTEMPS                                        0042
         ST    @10,@AL00001+4                                      0042
         LA    @01,@AL00001                                        0042
         BAL   @14,MOD                                             0042
*   ELSE                                                           0043
*     DO;                           /* NOT RELATIVE RECORD FILE      */
         B     @RC00041                                            0043
@RF00041 DS    0H                                                  0044
*       IF DMCBKPC=OFF THEN         /* DID KEY POINTER CHANGE        */
         TM    DMCBKPC(CBPTR),B'10000000'                          0044
         BNZ   @RF00044                                            0044
*         CALL MOD(ARG,DMCBKEYP);                                  0045
         LA    @10,@CH00878                                        0045
         ST    @10,@AL00001                                        0045
         LA    @10,DMCBKEYP(,CBPTR)                                0045
         ST    @10,@AL00001+4                                      0045
         LA    @01,@AL00001                                        0045
         BAL   @14,MOD                                             0045
*       IF DMCBKEYL>=F0C THEN       /* DID THE KEYLENGTH CHANGE      */
@RF00044 L     @10,DMCBKEYL(,CBPTR)                                0046
         LTR   @10,@10                                             0046
         BM    @RF00046                                            0046
*         CALL MOD(KEYL,DMCBKEYL);                                 0047
         LA    @10,@CH00880                                        0047
         ST    @10,@AL00001                                        0047
         LA    @10,DMCBKEYL(,CBPTR)                                0047
         ST    @10,@AL00001+4                                      0047
         LA    @01,@AL00001                                        0047
         BAL   @14,MOD                                             0047
*     END;                          /* NOT RELATIVE RECORD FILE      */
@RF00046 DS    0H                                                  0049
*   IF DMCBORL>=F0C THEN            /* DID THE OUTPUT RECORD LENGTH
*                                      CHANGE                        */
@RC00041 L     @10,DMCBORL(,CBPTR)                                 0049
         LTR   @10,@10                                             0049
         BM    @RF00049                                            0049
*     CALL MOD(RECLEN,DMCBORL);                                    0050
         LA    @10,@CH00882                                        0050
         ST    @10,@AL00001                                        0050
         LA    @10,DMCBORL(,CBPTR)                                 0050
         ST    @10,@AL00001+4                                      0050
         LA    @01,@AL00001                                        0050
         BAL   @14,MOD                                             0050
*   IF DMCBLRQ=DMCBOPR              /* FIRST REQUEST AFTER OPEN ?    */
*       ×DMCBOPTS^=DMCBLOPT THEN    /* DID THE OPTIONS CHANGE        */
@RF00049 CLI   DMCBLRQ(CBPTR),1                                    0051
         BE    @RT00051                                            0051
         CLC   DMCBOPTS(1,CBPTR),DMCBLOPT(CBPTR)                   0051
         BE    @RF00051                                            0051
@RT00051 DS    0H                                                  0052
*     CALL OPTMOD;                                                 0052
*                                                                  0052
         BAL   @14,OPTMOD                                          0052
*   /*****************************************************************/
*   /*                                                               */
*   /* NOW THAT ALL KEYWORDS HAVE BEEN CHECKED FOR CHANGE A HEADER   */
*   /* ENTRY SHOULD BE MADE IF ANY CHANGES WERE DETECTED             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0053
*   IF PLACEHDR>=F2C THEN           /* HAVE ANY CHANGES BEEN MADE    */
@RF00051 C     PLACEHDR,@CF00072                                   0053
         BL    @RF00053                                            0053
*     DO;                           /* CHANGES HAVE BEEN MADE        */
*       MODPLAST(PLACEHDR)=ON;      /* FLAG THE LAST ENTRY           */
         LR    @10,PLACEHDR                                        0055
         SLA   @10,2                                               0055
         LA    @04,MODPLAST-4(@10)                                 0055
         OI    0(@04),B'10000000'                                  0055
*       MODPPTR(F1C)=ADDR(MODLIST); /* INSERT PTR TO HEADER          */
         LA    @10,MODLIST                                         0056
         ST    @10,MODPPTR                                         0056
*       RFY                                                        0057
*         MODC BASED(ADDR(MODLIST));                               0057
*       MODBTC='C0'X;               /* INDICATE THAT RPL IS TO BE  0058
*                                      MODIFIED                      */
         MVI   MODBTC(@10),X'C0'                                   0058
*       MODFTC='02'X;               /* INDICATE MODCB FUNCTION       */
         MVI   MODFTC(@10),X'02'                                   0059
*       MODBLAD=ADDR(DMCBRPL);      /* POINT TO THE RPL IN THE DMCB  */
*                                                                  0060
         LA    @04,DMCBRPL(,CBPTR)                                 0060
         ST    @04,MODBLAD(,@10)                                   0060
*       /*************************************************************/
*       /*                                                           */
*       /* PASS CONTROL TO THE VSAM CONTROL BLOCKS ROUTINE           */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0061
*       RFY                                                        0061
*         GPR01P RSTD;                                             0061
*       GPR01P=ADDR(MODPLIST);                                     0062
         LA    GPR01P,MODPLIST                                     0062
*       CALL IDA019C1;              /* CALL THE VSAM CONTROL BLOCK 0063
*                                      ROUTINE LOCATED BY THE CVT    */
         L     @10,CVTPTR                                          0063
         L     @10,CVTCBSP(,@10)                                   0063
         L     @15,CBSCBP(,@10)                                    0063
         BALR  @14,@15                                             0063
*       RFY                                                        0064
*         GPR01P UNRSTD;                                           0064
*       RFY                                                        0065
*        (GPR00F,                                                  0065
*         GPR15F) RSTD;                                            0065
*       IF GPR15F^=F0C THEN         /* WAS MODCB SUCCESSFUL          */
         LTR   GPR15F,GPR15F                                       0066
         BZ    @RF00066                                            0066
*         DO;                       /* MODCB ERROR                   */
*           DMCBRTC=INVRQ;          /* INVALID REQUEST               */
         MVC   DMCBRTC(4,CBPTR),@CF00037                           0068
*           DMCBREJ=F11C;                                          0069
         MVI   DMCBREJ(CBPTR),X'0B'                                0069
*           DMCBRPLF=GPR00F;        /* STORE MODCB ERROR CODE AS RPL
*                                      FDBACK                        */
         ST    GPR00F,DMCBRPLF(,CBPTR)                             0070
*         END;                      /* MODCB ERROR                   */
*       RFY                                                        0072
*        (GPR00F,                                                  0072
*         GPR15F) UNRSTD;                                          0072
@RF00066 DS    0H                                                  0073
*     END;                          /* CHANGES HAVE BEEN MADE        */
*   RETURN CODE(DMCBRTC);                                          0074
@RF00053 L     @10,DMCBRTC(,CBPTR)                                 0074
         L     @13,4(,@13)                                         0074
         L     @00,@SIZDATD                                        0074
         LR    @01,@11                                             0074
         BLSCFRES R,LV=(0),A=(1)
         LR    @15,@10                                             0074
         L     @14,12(,@13)                                        0074
         LM    @00,@12,20(@13)                                     0074
         BR    @14                                                 0074
         EJECT
*OPTMOD:                                                           0075
*   PROCEDURE;                      /* MODIFY THE RPL OPTIONS        */
*                                                                  0075
OPTMOD   STM   @14,@01,12(@13)                                     0075
         STM   @03,@12,32(@13)                                     0075
*   /*****************************************************************/
*   /*                                                               */
*   /* PROCEDURE TO MODIFY THE RPL OPTIONS                           */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0076
*   PLACEHDR=PLACEHDR+F1C;          /* INCREMENT PLACEHDR            */
         AL    PLACEHDR,@CF00046                                   0076
*   ELEMKWTC=OPTCD;                 /* INDICATE OPTCD KEYWORD        */
         LR    @10,PLACEHDR                                        0077
         SLA   @10,3                                               0077
         LA    @10,ELEMS-8(@10)                                    0077
         MVC   ELEMKWTC(2,@10),@CH00884                            0077
*   ELEMBLEN=F0C;                   /* INDICATE LENGTH OF VALUE FIELD*/
         SLR   @04,@04                                             0078
         STH   @04,ELEMBLEN(,@10)                                  0078
*   ELEMBSTR=''B;                   /* ZERO OUT VALUE                */
         XC    ELEMBSTR(4,@10),ELEMBSTR(@10)                       0079
*   IF DMCBUPD=ON THEN              /* IS THIS AN UPDATE REQUEST     */
         TM    DMCBUPD(CBPTR),B'00010000'                          0080
         BNO   @RF00080                                            0080
*     ELEMBSTR(UPD:UPD)=ON;                                        0081
         OI    ELEMBSTR+1(@10),B'01000000'                         0081
*   ELSE                                                           0082
*     ELEMBSTR(NSP:NSP)=ON;         /* REMEMBER POSITION             */
         B     @RC00080                                            0082
@RF00080 LR    @10,PLACEHDR                                        0082
         SLA   @10,3                                               0082
         LA    @10,ELEMS-8(@10)                                    0082
         OI    ELEMBSTR+1(@10),B'00100000'                         0082
*   IF DMCBBWD=ON THEN              /* IS BACKWARD PROCESSING DESIRED*/
@RC00080 TM    DMCBBWD(CBPTR),B'00001000'                          0083
         BNO   @RF00083                                            0083
*     ELEMBSTR(BWD:BWD)=ON;                                        0084
         LR    @10,PLACEHDR                                        0084
         SLA   @10,3                                               0084
         LA    @10,ELEMS-8(@10)                                    0084
         OI    ELEMBSTR+2(@10),B'00100000'                         0084
*   IF DMCBLRD=ON THEN              /* IS POINT TO LAST RECORD     0085
*                                      DESIRED                       */
@RF00083 TM    DMCBLRD(CBPTR),B'00000100'                          0085
         BNO   @RF00085                                            0085
*     ELEMBSTR(LRD:LRD)=ON;                                        0086
         LR    @10,PLACEHDR                                        0086
         SLA   @10,3                                               0086
         LA    @10,ELEMS-8(@10)                                    0086
         OI    ELEMBSTR+2(@10),B'00001000'                         0086
*   IF DMCBKYD=ON THEN              /* KEYED TYPE ACCESS             */
@RF00085 TM    DMCBKYD(CBPTR),B'10000000'                          0087
         BNO   @RF00087                                            0087
*     DO;                           /* KEYED ACCESS                  */
*       ELEMBSTR(DIR:DIR)=ON;       /* DIRECT ACCESS                 */
         LR    @10,PLACEHDR                                        0089
         SLA   @10,3                                               0089
         LA    @10,ELEMS-8(@10)                                    0089
         OI    ELEMBSTR(@10),B'00001000'                           0089
*       IF DMCBAPX=ON THEN          /* TEST FOR APPROXIMATE KEY      */
         TM    DMCBAPX(CBPTR),B'00100000'                          0090
         BNO   @RF00090                                            0090
*         ELEMBSTR(KGE:KGE)=ON;     /* APPROX KEY                    */
         OI    ELEMBSTR+1(@10),B'00001000'                         0091
*       ELSE                                                       0092
*         ELEMBSTR(KEQ:KEQ)=ON;     /* EXACT KEY                     */
         B     @RC00090                                            0092
@RF00090 LR    @10,PLACEHDR                                        0092
         SLA   @10,3                                               0092
         LA    @10,ELEMS-8(@10)                                    0092
         OI    ELEMBSTR+1(@10),B'00010000'                         0092
*       IF DMCBGKY=ON THEN          /* TEST FOR GENERIC KEY          */
@RC00090 TM    DMCBGKY(CBPTR),B'01000000'                          0093
         BNO   @RF00093                                            0093
*         ELEMBSTR(GRC:GRC)=ON;     /* GENERIC KEY                   */
         LR    @10,PLACEHDR                                        0094
         SLA   @10,3                                               0094
         LA    @10,ELEMS-8(@10)                                    0094
         OI    ELEMBSTR+1(@10),B'00000010'                         0094
*       ELSE                                                       0095
*         ELEMBSTR(FKS:FKS)=ON;     /* FULL KEY                      */
         B     @RC00093                                            0095
@RF00093 LR    @10,PLACEHDR                                        0095
         SLA   @10,3                                               0095
         LA    @10,ELEMS-8(@10)                                    0095
         OI    ELEMBSTR+1(@10),B'00000100'                         0095
*     END;                          /* KEYED TYPE ACCESS             */
*   ELSE                                                           0097
*     DO;                           /* NOT KEYED ACCESS              */
         B     @RC00087                                            0097
@RF00087 DS    0H                                                  0098
*       IF DMCBRNO=ON THEN          /* RELATIVE RECORD ACCESS        */
         TM    DMCBRNO(CBPTR),B'01000000'                          0098
         BNO   @RF00098                                            0098
*         ELEMBSTR(DIR:DIR)=ON;     /* INDICATE DIRECT               */
         LR    @10,PLACEHDR                                        0099
         SLA   @10,3                                               0099
         LA    @10,ELEMS-8(@10)                                    0099
         OI    ELEMBSTR(@10),B'00001000'                           0099
*       ELSE                                                       0100
*         ELEMBSTR(SEQ:SEQ)=ON;     /* SEQUENTIAL                    */
         B     @RC00098                                            0100
@RF00098 LR    @10,PLACEHDR                                        0100
         SLA   @10,3                                               0100
         LA    @10,ELEMS-8(@10)                                    0100
         OI    ELEMBSTR(@10),B'00010000'                           0100
*     END;                          /* NOT KEYED ACCESS              */
@RC00098 DS    0H                                                  0102
*   MODPPTR(PLACEHDR)=ADDR(ELEM);   /* PLACE THIS ELEMENT IN THE   0102
*                                      MODCB PARAMETER LIST          */
@RC00087 LR    @10,PLACEHDR                                        0102
         SLA   @10,2                                               0102
         LR    @04,PLACEHDR                                        0102
         SLA   @04,3                                               0102
         LA    @04,ELEMS-8(@04)                                    0102
         ST    @04,MODPPTR-4(@10)                                  0102
*   END OPTMOD;                                                    0103
@EL00002 DS    0H                                                  0103
@EF00002 DS    0H                                                  0103
@ER00002 LM    @14,@01,12(@13)                                     0103
         LM    @03,@12,32(@13)                                     0103
         BR    @14                                                 0103
         EJECT
*MOD:                                                              0104
*   PROCEDURE(KEYWORD,VALUE);       /* MODIFY RPL PARAMETER          */
*                                                                  0104
MOD      STM   @14,@01,12(@13)                                     0104
         STM   @03,@12,32(@13)                                     0104
         MVC   @PC00003(8),0(@01)                                  0104
*   /*****************************************************************/
*   /*                                                               */
*   /* MOD BUILDS AN ELEMENT IN THE MODCB PARAMETER LIST             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0105
*   DECLARE                                                        0105
*     KEYWORD FIXED(15),                                           0105
*     VALUE FIXED(31);                                             0105
*   PLACEHDR=PLACEHDR+F1C;          /* INCREMENT NUMBER OF ELEMENTS  */
         AL    PLACEHDR,@CF00046                                   0106
*   ELEMKWTC=KEYWORD;               /* INDICATE THE PROPER KEYWORD 0107
*                                      FOR FOR THIS ELEMENT          */
         LR    @10,PLACEHDR                                        0107
         SLA   @10,3                                               0107
         LA    @10,ELEMS-8(@10)                                    0107
         L     @03,@PC00003                                        0107
         LH    @03,KEYWORD(,@03)                                   0107
         STH   @03,ELEMKWTC(,@10)                                  0107
*   ELEMBLEN=F0C;                   /* INDICATE LENGTH OF VALUE FIELD*/
         SLR   @03,@03                                             0108
         STH   @03,ELEMBLEN(,@10)                                  0108
*   ELEMPTR=VALUE;                  /* PUT VALUE IN PARAMETER LIST 0109
*                                      ELEMENT                       */
         L     @03,@PC00003+4                                      0109
         L     @15,VALUE(,@03)                                     0109
         ST    @15,ELEMPTR(,@10)                                   0109
*   MODPPTR(PLACEHDR)=ADDR(ELEM);   /* PLACE THIS ELEMENT IN MODCB 0110
*                                      PARAMETER LIST                */
         LR    @14,PLACEHDR                                        0110
         SLA   @14,2                                               0110
         ST    @10,MODPPTR-4(@14)                                  0110
*   VALUE=VALUE×'80000000'X;        /* INDICATE THAT THE VALUE HAS 0111
*                                      BEEN PLACED IN THE RPL        */
         O     @15,@CF00942                                        0111
         ST    @15,VALUE(,@03)                                     0111
*   RETURN;                                                        0112
@EL00003 DS    0H                                                  0112
@EF00003 DS    0H                                                  0112
@ER00003 LM    @14,@01,12(@13)                                     0112
         LM    @03,@12,32(@13)                                     0112
         BR    @14                                                 0112
*   END MOD;                                                       0113
         EJECT
*                                                                  0114
*   /*****************************************************************/
*   /*                                                               */
*   /* END OF EXECUTABLE PROCEDURE STATEMENTS                        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0114
*   DECLARE                         /* GENERAL PURPOSE REGISTERS     */
*     GPR00F FIXED(31) REG(0),                                     0114
*     GPR15F FIXED(31) REG(15),                                    0114
*     GPR01P PTR(31) REG(1);                                       0114
*   DECLARE                         /* COMMON VARIABLES              */
*     I256C CHAR(256) BASED,                                       0115
*     I031F FIXED(31) BASED,                                       0115
*     I031P PTR(31) BASED,                                         0115
*     I015F FIXED(15) BASED,                                       0115
*     I015P PTR(15) BASED,                                         0115
*     I008P PTR(8) BASED,                                          0115
*     I001C CHAR(1) BASED;                                         0115
*   DCL                                                            0116
*     1 DASPATCH LOCAL BDY(DWORD),  /* PATCH AREA                    */
*      2 DASPATA(DASPATLN) PTR INIT((DASPATLN)0);                  0116
*   END BLSCSETT                                                   0117
*                                                                  0117
*/* THE FOLLOWING INCLUDE STATEMENTS WERE FOUND IN THIS PROGRAM.     */
*/*%INCLUDE SYSLIB  (CVT     )                                       */
*/*%INCLUDE SYSLIB  (IDAMODC )                                       */
*/*%INCLUDE SYSLIB  (IDAELEM )                                       */
*                                                                  0117
*       ;                                                          0117
@DATA    DS    0H
@CH00119 DC    H'44'
@CH00876 DC    H'45'
@CH00878 DC    H'46'
@CH00880 DC    H'48'
@CH00884 DC    H'52'
@CH00882 DC    H'53'
@DATD    DSECT
         DS    0F
@SA00001 DS    18F
@PC00003 DS    2F
@AL00001 DS    2A
@AFTEMPS DS    1F
BLSCSETT CSECT
         DS    0F
@CF00046 DC    F'1'
@CF00072 DC    F'2'
@CF00037 DC    F'4'
@CF00942 DC    XL4'80000000'
@DATD    DSECT
         DS    0D
MODLIST  DS    CL8
MODSTAT  DS    CL12
         ORG   MODSTAT
MODNAME  DS    CL8
SUBCODE  DS    FL4
         ORG   MODSTAT+12
         DS    CL4
ELEMS    DS    6CL8
MODPLIST DS    CL4
         ORG   MODPLIST+0
MODPPTR  DS    AL4
         ORG   MODPPTR+0
@NM00003 DS    BL1
         ORG   @NM00003+0
MODPLAST DS    BL1
@NM00004 EQU   @NM00003+0
         ORG   MODPPTR+1
MODPP24  DS    AL3
         ORG   MODPLIST+28
         ORG   *+1-(*-@DATD)/(*-@DATD) INSURE DSECT DATA
@ENDDATD EQU   *
BLSCSETT CSECT
