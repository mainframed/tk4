*                                      OPEN                          */
         CLI   DMCBOPN(CBPTR),X'00'                                0022
         BE    @RF00022                                            0022
*     DO;                           /* DATA SET IS OPEN              */
*       DMCBCPC=F0C;                /* CLEAR THE ABEND COMPLETION  0024
*                                      CODE                          */
*                                                                  0024
         ST    @10,DMCBCPC(,CBPTR)                                 0024
*       /*************************************************************/
