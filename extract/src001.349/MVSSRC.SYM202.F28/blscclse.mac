*       /*                                                           */
*       /* CANCEL THE ESTAE                                          */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0076
*       ESTAEPRM=ESTAELST;                                         0076
@RF00069 MVC   ESTAEPRM(16),ESTAELST                               0076
*       RFY                                                        0077
*         GPR02P RSTD;                                             0077
*       GPR02P=ADDR(ESTAEPRM);      /* ADDRESS ESTAE PARM LIST       */
         LA    GPR02P,ESTAEPRM                                     0078
*       GEN REFS(GPR02P)(ESTAE 0,MF=(E,(GPR02P)));                 0079
         ESTAE 0,MF=(E,(GPR02P))
*       RFY                                                        0080
*         GPR02P UNRSTD;                                           0080
