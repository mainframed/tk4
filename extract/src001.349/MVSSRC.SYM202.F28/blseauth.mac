                        '
*      /* CHANGE ACTIVITY */
* THIS MODULE IS WRITTEN FOR @G57LPRD
BLSEAUTH CSECT ,                                                   0002
@MAINENT DS    0H                                                  0002
         USING *,@15                                               0002
         B     @PROLOG                                             0002
         DC    AL1(16)                                             0002
         DC    C'BLSEAUTH  78.072'                                 0002
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0002
         BALR  @12,0                                               0002
@PSTART  DS    0H                                                  0002
         USING @PSTART,@12                                         0002
         L     @00,@SIZDATD                                        0002
         BLSUALLS R,LV=(0)
         LR    @11,@01                                             0002
         USING @DATD,@11                                           0002
         ST    @13,@SA00001+4                                      0002
         LM    @00,@01,20(@13)                                     0002
         ST    @11,8(,@13)                                         0002
         LR    @13,@11                                             0002
         MVC   @PC00001(12),0(@01)                                 0002
*                                                                  0125
*   /*****************************************************************/
*   /*                                                               */
*   /* 1. OBTAIN TSO USERID.                                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0126
*   ZZ2PTR=ADDR(ZZ2PARM);           /* LOCALLY ADDRESS ZZ2 CONTROL 0126
*                                      BLOCK                         */
         L     ZZ2PTR,@PC00001                                     0126
*   USERIDP=ADDR(PSCBUSER);         /* LOCALLY ADDRESS USERID IN PSCB*/
         L     USERIDP,CPPLPSCB(,ZZ2PTR)                           0127
*   STATRECP=ADDR(SRECORD);         /* LOCALLY ADDRESS STATUS RECORD */
         L     STATRECP,@PC00001+4                                 0128
*   RULE=RULEP;                     /* MAKE LOCAL COPY OF RULE       */
*                                                                  0129
         L     @10,@PC00001+8                                      0129
         MVC   RULE(1),RULEP(@10)                                  0129
*   /*****************************************************************/
*   /*                                                               */
*   /* 2. IF DELETER RULE THEN IF FPBLOK CONTAINS A USERID IN THE    */
*   /* FPDELID FIELD THEN RETURN CODE 0 IF USERID = FPDELNM CODE 4   */
*   /* OTHERWISE.                                                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0130
*   IF RULE=DELRULE                 /* IS DELETE RULE IN EFFECT      */
*     THEN                                                         0130
         CLI   RULE,C'D'                                           0130
         BNE   @RF00130                                            0130
*     IF FPDELLN^=0                 /* IS THERE SOMEONE WITH DELETE
*                                      AUTH                          */
*       THEN                        /* THERE IS SOMEONE WITH DELETE
*                                      AUTHORITY                     */
         L     @10,ZZ2ZZ1P(,ZZ2PTR)                                0131
         L     @10,ZZ1FPP(,@10)                                    0131
         CLI   FPDELLN(@10),0                                      0131
         BE    @RF00131                                            0131
*       IF USERID=FPDELNM           /* DOES THIS USER HAVE DELETE  0132
*                                      AUTHORITY                     */
*         THEN                      /* THIS USER DOES NOT HAVE DELETE
*                                      AUTHORITY                     */
         CLC   USERID(8,USERIDP),FPDELNM(@10)                      0132
         BNE   @RF00132                                            0132
*         RETURN CODE(ZERO);                                       0133
         L     @13,4(,@13)                                         0133
         L     @00,@SIZDATD                                        0133
         LR    @01,@11                                             0133
         BLSUFRES R,LV=(0),A=(1)
         SLR   @15,@15                                             0133
         L     @14,12(,@13)                                        0133
         LM    @00,@12,20(@13)                                     0133
         BR    @14                                                 0133
*       ELSE                        /* THIS USER DOES NOT HAVE DELETE
*                                      AUTHORITY                     */
*         RETURN CODE(FOUR);                                       0134
@RF00132 L     @13,4(,@13)                                         0134
         L     @00,@SIZDATD                                        0134
         LR    @01,@11                                             0134
         BLSUFRES R,LV=(0),A=(1)
         LA    @15,4                                               0134
         L     @14,12(,@13)                                        0134
         LM    @00,@12,20(@13)                                     0134
         BR    @14                                                 0134
*     ELSE                                                         0135
*       ;                           /* THERE IS NO ONE WITH DELETE 0135
*                                      AUTHORITY CONTINUE AS FOR USER
*                                      RULE                          */
@RF00131 DS    0H                                                  0136
*   ELSE                            /* DELETE RULE IS NOT IN EFFECT  */
*     IF RULE^=OWNRULE              /* WAS OWN RULE SPECIFIED        */
*       THEN                        /* INVALID RULE SPECIFIED      0136
*                                      INTERNAL IPCS ERROR           */
         B     @RC00130                                            0136
@RF00130 CLI   RULE,C'O'                                           0136
         BE    @RF00136                                            0136
*       DO;                         /* ABEND(RULECC) DUMP            */
*         RESPECIFY                                                0138
*          (GPR01F) RESTRICTED;                                    0138
*         GPR01F=RULECC;            /* COMP CODE IN REG 1            */
         LA    GPR01F,107                                          0139
*         GPR01F=GPR01F×'80000000'X;/* DUMP/STEP/DUMPOPTS FLAG       */
         O     GPR01F,@CF00066                                     0140
*         SVC(13);                  /* ISSUE ABEND SVC               */
         SVC   13                                                  0141
*         RESPECIFY                                                0142
*          (GPR01F) UNRESTRICTED;                                  0142
*       END;                        /* ABEND(RULECC) DUMP            */
*     ELSE                                                         0144
*       ;                           /* CONTINUE WITH OWNER RULE      */
*                                                                  0144
@RF00136 DS    0H                                                  0145
*   /*****************************************************************/
*   /*                                                               */
*   /* 3. IF USERID = FPADMID THEN RETURN CODE 0.                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0145
*   IF FPADMLN^=0                   /* IS AN ADMINISTRATOR DEFINED   */
*     THEN                                                         0145
@RC00130 L     @10,ZZ2ZZ1P(,ZZ2PTR)                                0145
         L     @10,ZZ1FPP(,@10)                                    0145
         CLI   FPADMLN(@10),0                                      0145
         BE    @RF00145                                            0145
*     IF USERID=FPADMNM             /* DOES USER HAVE ADMINISTRATIVE
*                                      AUTHORITY                     */
*       THEN                        /* HE DOES HAVE ADMINISTRATIVE 0146
*                                      AUTHORITY                     */
         CLC   USERID(8,USERIDP),FPADMNM(@10)                      0146
         BNE   @RF00146                                            0146
*       RETURN CODE(ZERO);                                         0147
*                                                                  0147
         L     @13,4(,@13)                                         0147
         L     @00,@SIZDATD                                        0147
         LR    @01,@11                                             0147
         BLSUFRES R,LV=(0),A=(1)
         SLR   @15,@15                                             0147
         L     @14,12(,@13)                                        0147
         LM    @00,@12,20(@13)                                     0147
         BR    @14                                                 0147
*   /*****************************************************************/
*   /*                                                               */
*   /* 4. IF PDROWNR = USERID THEN RETURN CODE 0 OTHERWISE RETURN    */
*   /* CODE 4.                                                       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0148
*   IF USERID=PDROWNNM              /* DOES USER OWN THE PROBLEM     */
*     THEN                          /* HE DOES OWN THE PROBLEM       */
@RF00146 DS    0H                                                  0148
@RF00145 CLC   USERID(8,USERIDP),PDROWNNM(STATRECP)                0148
         BNE   @RF00148                                            0148
*     RETURN CODE(ZERO);                                           0149
         L     @13,4(,@13)                                         0149
         L     @00,@SIZDATD                                        0149
         LR    @01,@11                                             0149
         BLSUFRES R,LV=(0),A=(1)
         SLR   @15,@15                                             0149
         L     @14,12(,@13)                                        0149
         LM    @00,@12,20(@13)                                     0149
         BR    @14                                                 0149
*   ELSE                                                           0150
*     RETURN CODE(FOUR);                                           0150
*                                                                  0150
@RF00148 L     @13,4(,@13)                                         0150
         L     @00,@SIZDATD                                        0150
         LR    @01,@11                                             0150
         BLSUFRES R,LV=(0),A=(1)
         LA    @15,4                                               0150
         L     @14,12(,@13)                                        0150
         LM    @00,@12,20(@13)                                     0150
         BR    @14                                                 0150
