         TITLE 'BLSDTIME-OBTAIN CURRENT TIME AND DATE ROUTINE          *
                        '
*          /*        CHANGE ACTIVITY         */
*  THIS MODULE IS WRITTEN FOR @G57LPRM
BLSDTIME CSECT ,                                                   0002
@MAINENT DS    0H                                                  0002
         USING *,@15                                               0002
         B     @PROLOG                                             0002
         DC    AL1(16)                                             0002
         DC    C'BLSDTIME  78.062'                                 0002
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                     0002
         BALR  @12,0                                               0002
@PSTART  DS    0H                                                  0002
         USING @PSTART,@12                                         0002
         L     @00,@SIZDATD                                        0002
         BLSUALLS R,LV=(0)
         LR    @11,@01                                             0002
         USING @DATD,@11                                           0002
         ST    @13,@SA00001+4                                      0002
         LM    @00,@01,20(@13)                                     0002
         ST    @11,8(,@13)                                         0002
         LR    @13,@11                                             0002
         MVC   @PC00001(12),0(@01)                                 0002
*   RETC=ZERO;                      /* INITIALIZE THE RETC           */
*                                                                  0166
         SLR   RETC,RETC                                           0166
*   /*****************************************************************/
*   /*                                                               */
*   /* ISSUE THE TIME MACRO FOR CURRENT DATE AND TIME                */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0167
*   DO;                             /* TIMEP (DEC) ERRET(ERROR)    0167
*                                      ZONE(LT)                      */
*     RFY                                                          0168
*      (GPR00P,                                                    0168
*       GPR01F,                                                    0168
*       GPR15F) RSTD;               /* RESTRICT REGISTERS            */
*     GPR01F='00000042'X;           /* SET REGISTER ONE              */
         LA    GPR01F,66                                           0169
*     GPR00P=0;                     /* SET REGISTER ZERO             */
         SLR   GPR00P,GPR00P                                       0170
*     SVC(11);                      /* ISSUE TIME SVC                */
         SVC   11                                                  0171
*     IF GPR15F>4 THEN              /* WAS SVC SUCCESSFUL            */
         C     GPR15F,@CF00043                                     0172
         BH    @RT00172                                            0172
*       GOTO ERROR;                 /* BRANCH TO ERROR RETURN        */
*     RFY                                                          0174
*      (GPR00P,                                                    0174
*       GPR01F,                                                    0174
*       GPR15F) UNRSTD;             /* RELEASE REGISTERS             */
*   END;                            /* TIMEP (DEC) ERRET(ERROR)    0175
*                                      ZONE(LT) TIME MACRO           */
*   RESPECIFY                                                      0176
*    (R0,                                                          0176
*     R1) RESTRICTED;               /* TIME AND DATE IN REGS         */
*   STM(R0,R1,TIMEDATE);            /* TIME AND DATE STORED          */
         STM   R0,R1,TIMEDATE                                      0177
*   RESPECIFY                                                      0178
*    (R0,                                                          0178
*     R1) UNRESTRICTED;             /* NO LONGER NEED REGISTERS      */
*                                                                  0178
*   /*****************************************************************/
*   /*                                                               */
*   /* UNPACK THE FIELD OBTAINING TIME AND YEAR CHARACTERS           */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0179
*   UNPK(TEMPFLD,TIMEDATE);         /* UNPACK THE WHOLE FIELD        */
*                                                                  0179
         UNPK  TEMPFLD(15),TIMEDATE(8)                             0179
*   /*****************************************************************/
*   /*                                                               */
*   /* MOVE THE TIME INTO THE PARAMETER LIST                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0180
*   HOURFLD=HOUR;                   /* MOVE IN HOUR                  */
         L     @10,@PC00001+4                                      0180
         MVC   HOURFLD(2,@10),HOUR                                 0180
*   FDOT=DOT;                       /* MOVE IN PERIOD                */
         MVI   FDOT(@10),C':'                                      0181
*   MINCHAR=MINUTE;                 /* MOVE IN MINUTE                */
         MVC   MINCHAR(2,@10),MINUTE                               0182
*   SDOT=DOT;                       /* MOVE IN SECOND PERIOD         */
         MVI   SDOT(@10),C':'                                      0183
*   SECCHAR=SECOND;                 /* MOVE IN SECOND CHAR           */
*                                                                  0184
         MVC   SECCHAR(2,@10),SECOND                               0184
*   /*****************************************************************/
*   /*                                                               */
*   /* CONVERT THE DATE TO THE FORM MM/DD/YY                         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0185
*   YEARCHAR=YEAR;                  /* MOVE IN THE YEAR              */
         L     @10,@PC00001+8                                      0185
         MVC   YEARCHAR(2,@10),YEAR                                0185
*   FSLASH=SLASH;                   /* MOVE IN THE FIRST SLASH       */
         MVI   FSLASH(@10),C'/'                                    0186
*   SSLASH=SLASH;                   /* MOVE IN THE SECOND SLASH      */
*                                                                  0187
         MVI   SSLASH(@10),C'/'                                    0187
*   /*****************************************************************/
*   /*                                                               */
*   /* CONVERT THE DATE FIELD SO IT CAN BE USED FOR PROCESSING       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0188
*   CVB(DATESAVE,DATEFLD);          /* CONVERT FIELD TO BINARY       */
         XC    @TS00001(8),@TS00001                                0188
         MVC   @TS00001+4(4),DATEFLD                               0188
         CVB   DATESAVE,@TS00001                                   0188
*   YEARBIN=DATESAVE/F1000;         /* FIND THE YEAR                 */
         LA    @10,1000                                            0189
         LR    @00,DATESAVE                                        0189
         SRDA  @00,32                                              0189
         DR    @00,@10                                             0189
         LR    YEARBIN,@01                                         0189
*   DAYBIN=DATESAVE//F1000;         /* THE LEFT OVER IS THE DAY      */
*                                                                  0190
         LR    @00,DATESAVE                                        0190
         SRDA  @00,32                                              0190
         DR    @00,@10                                             0190
         LR    DAYBIN,@00                                          0190
*   /*****************************************************************/
*   /*                                                               */
*   /* CALCULATE THE NUMBER OF DAYS FOR FEB                          */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0191
*   IF YEARBIN//4=0 THEN            /* IS THIS A LEAP YEAR?          */
         LR    @00,YEARBIN                                         0191
         SRDA  @00,32                                              0191
         D     @00,@CF00043                                        0191
         LTR   @00,@00                                             0191
         BNZ   @RF00191                                            0191
*     FEB=29;                       /* YES, CHANGE NUMBER OF DAYS TO
*                                      29                            */
         LA    FEB,29                                              0192
*   ELSE                                                           0193
*     FEB=28;                       /* NO, NUMBER OF DAYS IS 28      */
*                                                                  0193
         B     @RC00191                                            0193
@RF00191 LA    FEB,28                                              0193
*   /*****************************************************************/
*   /*                                                               */
*   /* CALCULATE THE NUMBER OF DAYS FOR GROUPS OF MONTHS             */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0194
*   MON1=JAN;                       /* JAN FIRST GROUP               */
@RC00191 LA    @15,31                                              0194
         LR    MON1,@15                                            0194
*   MON2=MON1+FEB;                  /* 2ND GROUP                     */
         LR    MON2,MON1                                           0195
         ALR   MON2,FEB                                            0195
*   MON3=MON2+MAR;                  /* 3RD GROUP                     */
         LR    MON3,MON2                                           0196
         ALR   MON3,@15                                            0196
*   MON4=MON3+APR;                  /* 4TH GROUP                     */
         LA    @14,30                                              0197
         LR    MON4,MON3                                           0197
         ALR   MON4,@14                                            0197
*   MON5=MON4+MAY;                  /* 5TH GROUP                     */
         LR    MON5,MON4                                           0198
         ALR   MON5,@15                                            0198
*   MON6=MON5+JUN;                  /* 6TH GROUP                     */
         LR    MON6,MON5                                           0199
         ALR   MON6,@14                                            0199
*   MON7=MON6+JUL;                  /* 7TH GROUP                     */
         LR    @01,MON6                                            0200
         ALR   @01,@15                                             0200
         ST    @01,MON7                                            0200
*   MON8=MON7+AUG;                  /* 8TH GROUP                     */
         ALR   @01,@15                                             0201
         ST    @01,MON8                                            0201
*   MON9=MON8+SEP;                  /* 9TH GROUP                     */
         ALR   @01,@14                                             0202
         ST    @01,MON9                                            0202
*   MON10=MON9+OCT;                 /* 10TH GROUP                    */
         LR    @00,@01                                             0203
         ALR   @00,@15                                             0203
         ST    @00,MON10                                           0203
*   MON11=MON10+NOV;                /* 11TH GROUP                    */
         ALR   @14,@00                                             0204
         ST    @14,MON11                                           0204
*   MON12=MON11+DEC;                /* 12TH GROUP                    */
*                                                                  0205
         ALR   @15,@14                                             0205
         LR    MON12,@15                                           0205
*   /*****************************************************************/
*   /*                                                               */
*   /* CALCULATE THE DATE FIRST                                      */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0206
*   IF DAYBIN>MON6 THEN             /* IS DAY PAST 6 MONTHS?         */
         CR    DAYBIN,MON6                                         0206
         BNH   @RF00206                                            0206
*     IF DAYBIN>MON9 THEN           /* YES, THEN IS DAY PASS 9     0207
*                                      MONTHS?                       */
         CR    DAYBIN,@01                                          0207
         BNH   @RF00207                                            0207
*       IF DAYBIN>MON10 THEN        /* YES, IS MONTH GREATER THAN  0208
*                                      OCT?                          */
         CR    DAYBIN,@00                                          0208
         BNH   @RF00208                                            0208
*         IF DAYBIN>MON11 THEN      /* YES, MUST BE MONTH OF DEC     */
         CR    DAYBIN,@14                                          0209
         BNH   @RF00209                                            0209
*           DO;                     /* DECEMBER                      */
*             MONTHFLD=MONTH12;     /* INDICATE DECEMBER             */
         L     @10,@PC00001+8                                      0211
         MVC   MONTHFLD(2,@10),MONTH12                             0211
*             PACKFLD=DAYBIN-MON11; /* GET THE NUMBER OF DAYS        */
         LCR   @14,@14                                             0212
         ALR   @14,DAYBIN                                          0212
         LR    PACKFLD,@14                                         0212
*             CALL UNPACKIT;        /* UNPACK THE FIELD              */
         BAL   @14,UNPACKIT                                        0213
*             DAYCHAR=UNPKCHAR;     /* UNPACK FIELD INTO PARAMETER   */
         L     @10,@PC00001+8                                      0214
         MVC   DAYCHAR(2,@10),UNPKCHAR                             0214
*           END;                    /* END OF DEC                    */
*         ELSE                      /* NO MUST BE NOVEMBER           */
*           DO;                     /* NOVEMBER                      */
         B     @RC00209                                            0216
@RF00209 DS    0H                                                  0217
*             MONTHFLD=MONTH11;     /* INDICATE NOVEMBER             */
         L     @10,@PC00001+8                                      0217
         MVC   MONTHFLD(2,@10),MONTH11                             0217
*             PACKFLD=DAYBIN-MON10; /* GET NUMBER OF DAYS            */
         LR    PACKFLD,DAYBIN                                      0218
         SL    PACKFLD,MON10                                       0218
*             CALL UNPACKIT;        /* UNPACK THE FIELD              */
         BAL   @14,UNPACKIT                                        0219
*             DAYCHAR=UNPKCHAR;     /* UNPACKED FIELD TO PARAMETER   */
         L     @10,@PC00001+8                                      0220
         MVC   DAYCHAR(2,@10),UNPKCHAR                             0220
*           END;                    /* END OF NOV                    */
*       ELSE                        /* NO MUST BE OCTOBER            */
*         DO;                       /* OCTOBER                       */
         B     @RC00208                                            0222
@RF00208 DS    0H                                                  0223
*           MONTHFLD=MONTH10;       /* INDICATE OCTOBER              */
         L     @10,@PC00001+8                                      0223
         MVC   MONTHFLD(2,@10),MONTH10                             0223
*           PACKFLD=DAYBIN-MON9;    /* GET NUMBER OF DAYS            */
         LR    PACKFLD,DAYBIN                                      0224
         SL    PACKFLD,MON9                                        0224
*           CALL UNPACKIT;          /* UNPACK THE FIELD              */
         BAL   @14,UNPACKIT                                        0225
*           DAYCHAR=UNPKCHAR;       /* UNPACKED FIELD TO PARAMETER   */
         L     @10,@PC00001+8                                      0226
         MVC   DAYCHAR(2,@10),UNPKCHAR                             0226
*         END;                      /* END OF OCT                    */
*     ELSE                          /* LESS THAN 9 BUT GREATER THAN 6*/
*       IF DAYBIN>MON7 THEN         /* YES, IS MONTH GREATER THAN  0228
*                                      JUL?                          */
         B     @RC00207                                            0228
@RF00207 C     DAYBIN,MON7                                         0228
         BNH   @RF00228                                            0228
*         IF DAYBIN>MON8 THEN       /* YES, GREATER THAN AUG         */
         L     @10,MON8                                            0229
         CR    DAYBIN,@10                                          0229
         BNH   @RF00229                                            0229
*           DO;                     /* YES, MUST BE SEPTEMBER        */
*             MONTHFLD=MONTH9;      /* INDICATE DECEMBER             */
         L     @08,@PC00001+8                                      0231
         MVC   MONTHFLD(2,@08),MONTH9                              0231
*             PACKFLD=DAYBIN-MON8;  /* GET THE NUMBER OF DAYS        */
         LCR   @10,@10                                             0232
         ALR   @10,DAYBIN                                          0232
         LR    PACKFLD,@10                                         0232
*             CALL UNPACKIT;        /* UNPACK THE FIELD              */
         BAL   @14,UNPACKIT                                        0233
*             DAYCHAR=UNPKCHAR;     /* UNPACK FIELD INTO PARAMETER   */
         L     @10,@PC00001+8                                      0234
         MVC   DAYCHAR(2,@10),UNPKCHAR                             0234
*           END;                    /* END OF SEP                    */
*         ELSE                      /* NO MUST BE AUGUST             */
*           DO;                     /* AUGUST                        */
         B     @RC00229                                            0236
@RF00229 DS    0H                                                  0237
*             MONTHFLD=MONTH8;      /* INDICATE NOVEMBER             */
         L     @10,@PC00001+8                                      0237
         MVC   MONTHFLD(2,@10),MONTH8                              0237
*             PACKFLD=DAYBIN-MON7;  /* GET NUMBER OF DAYS            */
         LR    PACKFLD,DAYBIN                                      0238
         SL    PACKFLD,MON7                                        0238
*             CALL UNPACKIT;        /* UNPACK THE FIELD              */
         BAL   @14,UNPACKIT                                        0239
*             DAYCHAR=UNPKCHAR;     /* UNPACKED FIELD TO PARAMETER   */
         L     @10,@PC00001+8                                      0240
         MVC   DAYCHAR(2,@10),UNPKCHAR                             0240
*           END;                    /* END OF AUG                    */
*       ELSE                        /* NO MUST BE JULY               */
*         DO;                       /* JULY                          */
         B     @RC00228                                            0242
@RF00228 DS    0H                                                  0243
*           MONTHFLD=MONTH7;        /* INDICATE JULY                 */
         L     @08,@PC00001+8                                      0243
         MVC   MONTHFLD(2,@08),MONTH7                              0243
*           PACKFLD=DAYBIN-MON6;    /* GET NUMBER OF DAYS            */
         LR    PACKFLD,DAYBIN                                      0244
         SLR   PACKFLD,MON6                                        0244
*           CALL UNPACKIT;          /* UNPACK THE FIELD              */
         BAL   @14,UNPACKIT                                        0245
*           DAYCHAR=UNPKCHAR;       /* UNPACKED FIELD TO PARAMETER   */
         L     @10,@PC00001+8                                      0246
         MVC   DAYCHAR(2,@10),UNPKCHAR                             0246
*         END;                      /* END OF JULY                   */
*   ELSE                            /* FIELD IS IN THE FIRST 6 MONTHS*/
*     IF DAYBIN>MON3 THEN           /* YES, THEN IS DAY PASS 3     0248
*                                      MONTHS?                       */
         B     @RC00206                                            0248
@RF00206 CR    DAYBIN,MON3                                         0248
         BNH   @RF00248                                            0248
*       IF DAYBIN>MON4 THEN         /* YES, IS MONTH GREATER THAN  0249
*                                      APR?                          */
         CR    DAYBIN,MON4                                         0249
         BNH   @RF00249                                            0249
*         IF DAYBIN>MON5 THEN       /* YES, IS MONTH GREATER THAN  0250
*                                      MAY?                          */
         CR    DAYBIN,MON5                                         0250
         BNH   @RF00250                                            0250
*           DO;                     /* YES, MUST BE JUNE             */
*             MONTHFLD=MONTH6;      /* INDICATE JUNE                 */
         L     @10,@PC00001+8                                      0252
         MVC   MONTHFLD(2,@10),MONTH6                              0252
*             PACKFLD=DAYBIN-MON5;  /* GET THE NUMBER OF DAYS        */
         LR    PACKFLD,DAYBIN                                      0253
         SLR   PACKFLD,MON5                                        0253
*             CALL UNPACKIT;        /* UNPACK THE FIELD              */
         BAL   @14,UNPACKIT                                        0254
*             DAYCHAR=UNPKCHAR;     /* UNPACK FIELD INTO PARAMETER   */
         L     @10,@PC00001+8                                      0255
         MVC   DAYCHAR(2,@10),UNPKCHAR                             0255
*           END;                    /* END OF JUN                    */
*         ELSE                      /* NO MUST BE MAY                */
*           DO;                     /* MAY                           */
         B     @RC00250                                            0257
@RF00250 DS    0H                                                  0258
*             MONTHFLD=MONTH5;      /* INDICATE MAY                  */
         L     @10,@PC00001+8                                      0258
         MVC   MONTHFLD(2,@10),MONTH5                              0258
*             PACKFLD=DAYBIN-MON4;  /* GET NUMBER OF DAYS            */
         LR    PACKFLD,DAYBIN                                      0259
         SLR   PACKFLD,MON4                                        0259
*             CALL UNPACKIT;        /* UNPACK THE FIELD              */
         BAL   @14,UNPACKIT                                        0260
*             DAYCHAR=UNPKCHAR;     /* UNPACKED FIELD TO PARAMETER   */
         L     @10,@PC00001+8                                      0261
         MVC   DAYCHAR(2,@10),UNPKCHAR                             0261
*           END;                    /* END OF MAY                    */
*       ELSE                        /* NO MUST BE APRIL              */
*         DO;                       /* APRIL                         */
         B     @RC00249                                            0263
@RF00249 DS    0H                                                  0264
*           MONTHFLD=MONTH4;        /* INDICATE APRIL                */
         L     @10,@PC00001+8                                      0264
         MVC   MONTHFLD(2,@10),MONTH4                              0264
*           PACKFLD=DAYBIN-MON3;    /* GET NUMBER OF DAYS            */
         LR    PACKFLD,DAYBIN                                      0265
         SLR   PACKFLD,MON3                                        0265
*           CALL UNPACKIT;          /* UNPACK THE FIELD              */
         BAL   @14,UNPACKIT                                        0266
*           DAYCHAR=UNPKCHAR;       /* UNPACKED FIELD TO PARAMETER   */
         L     @10,@PC00001+8                                      0267
         MVC   DAYCHAR(2,@10),UNPKCHAR                             0267
*         END;                      /* END OF APR                    */
*     ELSE                          /* LESS THAN 3 BUT GREATER THAN 1*/
*       IF DAYBIN>MON1 THEN         /* YES, IS MONTH GREATER THAN  0269
*                                      JAN?                          */
         B     @RC00248                                            0269
@RF00248 CR    DAYBIN,MON1                                         0269
         BNH   @RF00269                                            0269
*         IF DAYBIN>MON2 THEN       /* YES, GREATER THAN FEB         */
         CR    DAYBIN,MON2                                         0270
         BNH   @RF00270                                            0270
*           DO;                     /* YES, MUST BE MARCH            */
*             MONTHFLD=MONTH3;      /* INDICATE MARCH                */
         L     @10,@PC00001+8                                      0272
         MVC   MONTHFLD(2,@10),MONTH3                              0272
*             PACKFLD=DAYBIN-MON2;  /* GET THE NUMBER OF DAYS        */
         LR    PACKFLD,DAYBIN                                      0273
         SLR   PACKFLD,MON2                                        0273
*             CALL UNPACKIT;        /* UNPACK THE FIELD              */
         BAL   @14,UNPACKIT                                        0274
*             DAYCHAR=UNPKCHAR;     /* UNPACK FIELD INTO PARAMETER   */
         L     @10,@PC00001+8                                      0275
         MVC   DAYCHAR(2,@10),UNPKCHAR                             0275
*           END;                    /* END OF MAR                    */
*         ELSE                      /* NO MUST BE FEBRUARY           */
*           DO;                     /* FEBRUARY                      */
         B     @RC00270                                            0277
@RF00270 DS    0H                                                  0278
*             MONTHFLD=MONTH2;      /* INDICATE FEBRUARY             */
         L     @10,@PC00001+8                                      0278
         MVC   MONTHFLD(2,@10),MONTH2                              0278
*             PACKFLD=DAYBIN-MON1;  /* GET NUMBER OF DAYS            */
         LR    PACKFLD,DAYBIN                                      0279
         SLR   PACKFLD,MON1                                        0279
*             CALL UNPACKIT;        /* UNPACK THE FIELD              */
         BAL   @14,UNPACKIT                                        0280
*             DAYCHAR=UNPKCHAR;     /* UNPACKED FIELD TO PARAMETER   */
         L     @10,@PC00001+8                                      0281
         MVC   DAYCHAR(2,@10),UNPKCHAR                             0281
*           END;                    /* END OF FEB                    */
*       ELSE                        /* NO MUST BE JANUARY            */
*         DO;                       /* JANUARY                       */
         B     @RC00269                                            0283
@RF00269 DS    0H                                                  0284
*           MONTHFLD=MONTH1;        /* INDICATE JULY                 */
         L     @10,@PC00001+8                                      0284
         MVC   MONTHFLD(2,@10),MONTH1                              0284
*           PACKFLD=DAYBIN;         /* GET NUMBER OF DAYS            */
         LR    PACKFLD,DAYBIN                                      0285
*           CALL UNPACKIT;          /* UNPACK THE FIELD              */
         BAL   @14,UNPACKIT                                        0286
*           DAYCHAR=UNPKCHAR;       /* UNPACKED FIELD TO PARAMETER   */
         L     @10,@PC00001+8                                      0287
         MVC   DAYCHAR(2,@10),UNPKCHAR                             0287
*         END;                      /* END OF JULY                   */
*   RETURN CODE(RETC);              /* END OF PROCESSING RETURN TO 0289
*                                      CALLER                        */
@RC00269 DS    0H                                                  0289
@RC00248 DS    0H                                                  0289
@RC00206 L     @13,4(,@13)                                         0289
         L     @00,@SIZDATD                                        0289
         LR    @01,@11                                             0289
         BLSUFRES R,LV=(0),A=(1)
         LR    @15,@09                                             0289
         L     @14,12(,@13)                                        0289
         LM    @00,@12,20(@13)                                     0289
         BR    @14                                                 0289
*UNPACKIT:                                                         0290
*   PROCEDURE;                                                     0290
UNPACKIT STM   @14,@08,12(@13)                                     0290
         STM   @10,@12,60(@13)                                     0290
*   IF PACKFLD>0×PACKFLD<32 THEN    /* CHECK TO SEE THAT DAY IS    0291
*                                      CORRECT                       */
         LTR   PACKFLD,PACKFLD                                     0291
         BP    @RT00291                                            0291
         C     PACKFLD,@CF00065                                    0291
         BNL   @RF00291                                            0291
@RT00291 DS    0H                                                  0292
*     DO;                           /* PROCESS DAY                   */
*       CVD(PACKFLD,TEMPDAY);       /* CONVERT TO DECIMAL            */
         CVD   PACKFLD,@TS00001                                    0293
         MVC   TEMPDAY(4),@TS00001+4                               0293
*       PACKDAY=TEMPDAY(3:4);       /* ONLY INCLUDE THE GOOD DATA    */
         MVC   PACKDAY(2),TEMPDAY+2                                0294
*       UNPK(UNPKDAY,PACKDAY);      /* UNPACK THE INFO               */
         UNPK  UNPKDAY(3),PACKDAY(2)                               0295
*       UNPKDAY(3:3)=UNPKDAY(3:3)×ZEROCHAR(1:1);/* NUMERIC IN LAST 0296
*                                      POSIT                         */
         OC    UNPKDAY+2(1),ZEROCHAR                               0296
*       UNPKCHAR=UNPKDAY(2:3);      /* MOVE IN DAY FIELD             */
         MVC   UNPKCHAR(2),UNPKDAY+1                               0297
*     END;                          /* END OF DAY UNPACKING          */
*   ELSE                            /* DATE NOT BETWEEN 1 AND 31 MUST
*                                      BE ERROR                      */
*     DO;                           /* ERROR FOUND IN DATE           */
         B     @RC00291                                            0299
@RF00291 DS    0H                                                  0300
*       UNPKCHAR=ZEROCHAR;          /* FILL IN THE FIELD             */
         MVC   UNPKCHAR(2),ZEROCHAR                                0300
*       RETC=4;                     /* INDICATE DAY FIELD IS BAD     */
         LA    RETC,4                                              0301
*     END;                          /* END OF ERROR IN DATE          */
*   END UNPACKIT;                   /* END OF DAY PROCESSING         */
@EL00002 DS    0H                                                  0303
@EF00002 DS    0H                                                  0303
@ER00002 LM    @14,@08,12(@13)                                     0303
         LM    @10,@12,60(@13)                                     0303
         BR    @14                                                 0303
*ERROR:                                                            0304
*   ;                               /* ERROR PROC FOR TIME MACRO     */
ERROR    DS    0H                                                  0305
*   RETC=12;                        /* INDICATE AN ERROR CONDITION   */
         LA    RETC,12                                             0305
*   RETURN CODE(RETC);              /* RETURN TO CALLER WITH ERROR   */
         L     @13,4(,@13)                                         0306
         L     @00,@SIZDATD                                        0306
         LR    @01,@11                                             0306
         BLSUFRES R,LV=(0),A=(1)
         LR    @15,@09                                             0306
         L     @14,12(,@13)                                        0306
         LM    @00,@12,20(@13)                                     0306
         BR    @14                                                 0306
*/*BLSUPEND--MVS IPCS MODULE                                         */
*                                                                  0307
*   DECLARE                         /* GENERAL PURPOSE REGISTERS     */
*     GPR01F FIXED(31) REG(1),                                     0307
*     GPR15F FIXED(31) REG(15),                                    0307
*     GPR00P PTR(31) REG(0);                                       0307
