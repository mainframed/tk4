         SR    @F,@F                                              0222
         STH   @F,STATUS                                          0222
         BC    15,@9DD                                            0223
*                                ELSE
*                                  IF ECTRCDF='80'X THEN /* IF ACCOUNT
*                                       ABENDED                       *
@9DE     L     @8,ECTPTR                                          0223
         CLI   0(@8),X'80'                                        0223
         BC    07,@9DC                                            0223
*                                    INDICATR=9; /* SET SYSABEND
*                                       INDICATOR                     *
         LA    @F,9                                               0224
         STH   @F,STATUS                                          0224
         BC    15,@9DB                                            0225
*                                  ELSE
