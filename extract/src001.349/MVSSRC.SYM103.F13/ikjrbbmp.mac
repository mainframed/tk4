@9DC     LA    @F,5                                               0225
         STH   @F,STATUS                                          0225
*                      END;
@9DB     EQU   *                                                  0226
@9DD     EQU   *                                                  0226
@9DF     EQU   *                                                  0226
@9E1     EQU   *                                                  0226
@9E3     EQU   *                                                  0226
@9E5     EQU   *                                                  0226
*                    R1=ADDR (ATTATCB); /* SET TCB POINTER FOR DETACH *
@9E7     LA    @1,ATTATCB                                         0227
*                    GEN (DETACH (1)); /* DETACH COMMAND PROCESSOR    *
         DETACH (1)
         DS    0H
*                  END;
*                CBUFPTR=PGPBIBUF;   /* LOCATE COMMAND BUFFER         *
@9E8     L     @8,PGPBPTR                                         0230
         MVC   CBUFPTR(4),12(@8)                                  0230
*                R0=16777216+CBUFLNG; /* SET SUBPOOL NO & BUFFER LEN  *
         L     @9,CBUFPTR                                         0231
         LH    @F,0(0,@9)                                         0231
         A     @F,@D2                                             0231
         LR    @0,@F                                              0231
*                R1=CBUFPTR;
