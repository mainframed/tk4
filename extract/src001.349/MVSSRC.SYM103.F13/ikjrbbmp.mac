*                      INDICATR=11;  /* ELSE INDICATE EOF             *
         LA    @F,11                                              0238
         STH   @F,STATUS                                          0238
*                  END;
*              END;
@9DA     EQU   *                                                  0240
*          END;
@9EA     EQU   *                                                  0241
