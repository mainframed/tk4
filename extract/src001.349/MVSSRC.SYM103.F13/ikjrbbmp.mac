         DS    0H
*
* /*  THE FOLLOWING STAE ROUTINE IS GIVEN CONTROL WHEN A TASK ON      *
* /*  THE BMP-LEVEL ABENDS.  IF THE ABEND IS A 'B01' USER CODE WHICH  *
* /*  IS ISSUED BY THE I/O SERVICE ROUTINES, A RETRY IS SCHEDULED.    *
* /*  IF ANY OTHER ERROR OCCURS, THE MESSAGE 'UTILITY ABENDED'        *
* /*  IS WRITTEN AND NO RETRY IS ATTEMPTED.                           *
*
*
* IKJRBBMS:
*    PROC (DUMBPARM) OPTIONS(NOSAVEAREA,DONTSAVE);
@EL01    L     @D,4(0,@D)                                         0290
         LR    @1,@C                                              0290
         L     @0,@SIZ001                                         0290
         FREEMAIN R,LV=(0),A=(1)                                  0290
         LM    @E,@C,12(@D)                                       0290
         BCR   15,@E                                              0290
IKJRBBMS EQU   *                                                  0290
*    RESTRICT (R2,STAEWPTR,STAEPTR);
*    GENERATE;
         DROP  @B,@C                 DROP BASE REGISTERS
         LR    @B,@F                 PUT ENTRY PT INTO REG 11
         USING IKJRBBMS,@B           SET ADDRESS FOR STAE EXIT
         DS    0H
*    R2=R14;                         /* SAVE RETURN ADDRESS           *
         LR    @2,@E                                              0293
*    IF R0^=12 THEN                  /* IS STAE WORK AREA BUILT?      *
         CH    @0,@D3                                             0294
         BC    08,@9D0                                            0294
*      DO;                           /* YES, CAN CONTINUE PROCESSING  *
*        STAEWPTR=R1;                /* SET ADDR OF STAE WORK AREA    *
         LR    @4,@1                                              0296
*        STAEPTR=STAEXPRM;           /* SET ADDR OF STAE PARM LIST    *
         L     @5,0(0,@4)                                         0297
*        R11=STAECODE;               /* SET ADDRESS TO CODE           *
         L     @B,0(0,@5)                                         0298
*        R12=STAEDATA;               /* SET ADDRESS TO DATA           *
         L     @C,4(0,@5)                                         0299
*        R13=STAESAVE;               /* SET ADDRESS TO SAVE AREA      *
         L     @D,8(0,@5)                                         0300
*        GENERATE;
         DROP  @B
         USING @PSTART,@B            ADDRESSABILITY TO CODE
         USING @DATD,@C              ADDRESSABILITY TO DATA
         DS    0H
*        IF STAEABND='0B01'X THEN    /* IF I/O SERVICE ROUT ABENDED   *
         L     @F,@X10                                            0302
         MVC   @TEMP3+1(3),5(@4)                                  0302
         C     @F,@TEMP3                                          0302
         BC    07,@9CF                                            0302
*          DO;
*            R0=ADDR (IKJRBBMR);     /* SET ADDR OF STAE RETRY ROUT   *
         LA    @0,IKJRBBMR                                        0304
*            R1=STAEWPTR;            /* SET ADDR OF STAE WORK AREA    *
         LR    @1,@4                                              0305
*            R15=4;                  /* INDICATE RETRY TO BE SCHEDULED*
         LA    @F,4                                               0306
         BC    15,@9CE                                            0308
*          END;
*        ELSE                        /* IF OTHER PROBLEM              *
*          DO;
*            IF SYSPTERR ^= '1'B THEN /* IF SYSPRINT DATA SET IS OK   *
@9CF     TM    BSTAT,B'00001000'                                  0309
         BC    01,@9CD                                            0309
*              DO;                   /* PUT OUT ERROR MESSAGE         *
*                INDICATR=9;         /* INDICATE IS UTILITY ABEND     *
         LA    @F,9                                               0311
         STH   @F,STATUS                                          0311
*                MSGPTR=ADDR (MESSAGE); /* SET MESSAGE POINTER        *
         L     @3,@V1              ADDRESS OF MESSAGE             0312
         ST    @3,MSGPTR                                          0312
*                                    /* CALCULATE MSG TXT ADDR        *
*                MSGQ=MSGS (INDICATR)+ADDR(MESSAGE);
         LR    @F,@3                                              0313
         LH    @8,STATUS                                          0313
         BCTR  @8,0                                               0313
         SLA   @8,1                                               0313
         L     @9,MSGPTR                                          0313
         LA    @A,0(@8,@9)                                        0313
         MVC   @TEMP2+2(2),0(@A)                                  0313
         A     @F,@TEMP2                                          0313
         ST    @F,MSGQ                                            0313
*                MSGSEG=1;           /* NO TEXT INSERTION REQUIRED    *
         LA    @F,1                                               0314
         ST    @F,OLD                                             0314
*                MSGADDR=ADDR (MSGTXT); /* SET MSG TEXT ADDR IN OLD   *
         LA    @F,MSGTXT                                          0315
         ST    @F,OLD+4                                           0315
*                MSGTXT (1:MSGLL)=MSGTXTIN(1:MSGLL); /* MOVE MSGTXT
*                                       INTO BMP STORAGE              *
         L     @6,MSGQ                                            0316
         LR    @E,@6                                              0316
         LH    @7,0(0,@6)                                         0316
         BCTR  @7,0                                               0316
         LA    @A,MSGTXT                                          0316
         EX    @7,@MVC                                            0316
*                GEN (PUTLINE PARM=PTPBA,OUTPUT=OLD,MF=(E,IOPLA));
         PUTLINE PARM=PTPBA,OUTPUT=OLD,MF=(E,IOPLA)
         DS    0H
*              END;
*            R15=0;                  /* CONTINUE WITH ABEND           *
@9CD     SR    @F,@F                                              0319
*          END;
*      END;
         BC    15,@9CC                                            0322
*    ELSE
*      R15=0;                        /* NO STAE WA - CONTINUE ABEND   *
@9D0     SR    @F,@F                                              0322
*    R14=R2;                         /* RESTORE RETURN ADDRESS        *
@9CC     LR    @E,@2                                              0323
*    RETURN;
*    END IKJRBBMS;
@EL02    BCR   15,@E                                              0325
*
* /*  THE FOLLOWING STAE RETRY IS GIVEN CONTROL ONLY AFTER THE        *
* /*  I/O SERVICE ROUTINES HAVE ISSUED A 'B01' ABEND.  IN THIS RETRY  *
* /*  ROUTINE, AN ERROR INDICATOR IS SET SO THAT THE PROPER MESSAGE   *
* /*  IS WRITTEN AND RETURN CODE SET.                                 *
*
*
* IKJRBBMR:
*    PROC (DUMYPARM) OPTIONS(NOSAVEAREA,DONTSAVE);
IKJRBBMR EQU   *                                                  0326
*    RESTRICT (STAEWPTR,STAEPTR);
*    GENERATE;
          DROP  @B                   DROP BASE REGISTERS
          LR    @B,@F                PUT ENTRY PT IN REG 11
          USING IKJRBBMR,@B          SET ADDRESS FOR 4 RTN
         DS    0H
*    IF R0^=12 THEN                  /* IS STAE WORK AREA BUILT?      *
         CH    @0,@D3                                             0329
         BC    08,@9CB                                            0329
*      DO;
*        STAEWPTR=R1;                /* SET ADDR OF STAE WORK AREA    *
         LR    @4,@1                                              0331
*        STAEPTR=STAEXPRM;           /* SET ADDR OF STAE PARM LIST    *
         L     @5,0(0,@4)                                         0332
*        R11=STAECODE;               /* SET ADDRESS TO CODE           *
         L     @B,0(0,@5)                                         0333
*        R12=STAEDATA;               /* SET ADDRESS TO DATA           *
         L     @C,4(0,@5)                                         0334
*        R13=STAESAVE;               /* SET ADDRESS TO SAVEAREA       *
         L     @D,8(0,@5)                                         0335
*        GENERATE;
          DROP  @B
          USING @PSTART,@B           ADDRESS TO CODE
          USING @DATD,@C             ADDRESS TO DATA
         DS    0H
*        IF BADINDCB='1'B THEN       /* IF INPUT DCB INFO INVALID     *
         TM    BSTAT,B'01000000'                                  0337
         BC    12,@9CA                                            0337
*          INDICATR=8;               /* THEN INDICATE THIS            *
         LA    @F,8                                               0338
         STH   @F,STATUS                                          0338
         BC    15,@9C9                                            0339
*        ELSE
*          IF SYSINERR='1'B THEN     /* IF I/O ERROR ON INPUT         *
@9CA     TM    BSTAT,B'00100000'                                  0339
         BC    12,@9C8                                            0339
*            INDICATR=7;             /* THEN INDICATE THIS            *
         LA    @F,7                                               0340
         STH   @F,STATUS                                          0340
         BC    15,@9C7                                            0341
*          ELSE
*            IF SYSPTERR='1'B THEN   /* IF SYSPRINT ERROR             *
@9C8     TM    BSTAT,B'00001000'                                  0341
         BC    12,@9C6                                            0341
*              INDICATR=0;           /* THEN INDICATE THIS            *
         SR    @F,@F                                              0342
         STH   @F,STATUS                                          0342
         BC    15,@9C5                                            0343
*            ELSE
*              IF SYSINBYD='1'B THEN /* IF ATTEMPTED TO READ BYD EOF  *
@9C6     TM    BSTAT,B'00000100'                                  0343
         BC    12,@9C4                                            0343
*                INDICATR=10;        /* THEN INDICATE THIS            *
         LA    @F,10                                              0344
         STH   @F,STATUS                                          0344
         BC    15,@9C3                                            0345
*              ELSE
*                INDICATR=9;         /* INDICATE IS OTHER PROBLEM     *
@9C4     LA    @F,9                                               0345
         STH   @F,STATUS                                          0345
*        R0=104;                     /* INDICATE LEN OF AREA TO FREE  *
@9C3     EQU   *                                                  0346
@9C5     EQU   *                                                  0346
@9C7     EQU   *                                                  0346
@9C9     LA    @0,104                                             0346
*        R1=STAEWPTR;                /* SET ADDR OF AREA TO FREE      *
         LR    @1,@4                                              0347
*        GEN (FREEMAIN R,LV=(0),A=(1)); /* FREE STAE WORK AREA        *
         FREEMAIN R,LV=(0),A=(1)
         DS    0H
*        GO TO EXIT;                 /* WRITE MSG IF POSSIBLE         *
         BC    15,EXIT                                            0349
*                                    /* THEN CLEAN UP                 *
*      END;
*    ELSE
*      RETURN;                       /* IF STAE WORK AREA NOT BUILT   *
*                                    /* RETURN TO ABEND               *
*    END IKJRBBMR;
@9C2     EQU   *                                                  0352
@EL03    BCR   15,@E                                              0352
*    END IKJRBBMP
