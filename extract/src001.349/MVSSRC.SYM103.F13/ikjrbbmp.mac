*                GEN (FREEMAIN R,LV=(0),A=(1)); /* RELEASE COMMAND
*                                       BUFFER                        *
         FREEMAIN R,LV=(0),A=(1)
         DS    0H
*                IF INDICATR=99 THEN /* IF COMMAND PROCESSED NORMALLY *
         LA    @F,99                                              0234
         CH    @F,STATUS                                          0234
