         L     @6,ECTPTR                                          0188
         MVC   12(8,@6),0(@9)                                     0188
         BC    15,@9EC                                            0189
*                      ELSE
*                        INDICATR=13; /* SET CMD NAME ERROR INDICATOR *
@9ED     EQU   *                                                  0189
@9EE     LA    @F,13                                              0189
         STH   @F,STATUS                                          0189
*                  END;
         BC    15,@9EB                                            0191
*                ELSE
*                  INDICATR=12;      /* SET CMDNAME SYNTAX ERROR      *
@9F2     LA    @F,12                                              0191
         STH   @F,STATUS                                          0191
*            IF INDICATR=99 THEN     /* IF VALID COMMAND NAME FOUND   *
@9EB     EQU   *                                                  0192
@9F4     LA    @F,99                                              0192
