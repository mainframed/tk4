@9F6     EQU   *                                                  0242
*
* /*  THE FOLLOWING CODE SECTION PUTS OUT THE FINAL MESSAGE IN THE    *
* /*  BMP'S PROCESSING IF NO ERRORS HAVE OCCURRED WHILE WRITING TO    *
* /*  THE SYSPRINT DATA SET.                                          *
*
*
* EXIT:
*    IF INDICATR^=0 THEN             /* IF INDICATR INDICATES MESSAGE *
@9FC     EQU   *                                                  0243
EXIT     SR    @F,@F                                              0243
         CH    @F,STATUS                                          0243
         BC    08,@9D9                                            0243
*      DO;                           /* TO BE WRITTEN OUT THEN        *
*        MSGPTR=ADDR (MESSAGE);      /* SET MESSAGE POINTER           *
         L     @8,@V1              ADDRESS OF MESSAGE             0245
         ST    @8,MSGPTR                                          0245
*        MSGQ=MSGS (INDICATR)+ADDR(MESSAGE); /* CALCULATE MSG TXT ADDR*
         LR    @F,@8                                              0246
         LH    @9,STATUS                                          0246
         BCTR  @9,0                                               0246
         SLA   @9,1                                               0246
         L     @6,MSGPTR                                          0246
         LA    @A,0(@9,@6)                                        0246
         MVC   @TEMP2+2(2),0(@A)                                  0246
         A     @F,@TEMP2                                          0246
         ST    @F,MSGQ                                            0246
*        MSGSEG=1;                   /* NO TEXT INSERTION REQUIRED    *
         LA    @F,1                                               0247
         ST    @F,OLD                                             0247
*        MSGADDR=ADDR (MSGTXT);      /* SET MSG TEXT ADDR IN OLD      *
         LA    @F,MSGTXT                                          0248
         ST    @F,OLD+4                                           0248
*        IF INDICATR < 5 THEN        /* IF MSG NEED TO PROVIDE ERROR  *
         LA    @F,5                                               0249
         CH    @F,STATUS                                          0249
         BC    12,@9D8                                            0249
*          DO;                       /* CODE THEN                     *
*            RESTRICT (RETREG);      /* RESTRICT RETREG FOR CONVERT   *
*            RETREG=ERRORCDE;        /* GET THE ERROR CODE            *
         LH    @6,STATUS+2                                        0252
*            GEN (CVD  RETREG,PACKDEC); /* AND CONVERT TO EBCDIC      *
         CVD  RETREG,PACKDEC
         DS    0H
*            GEN (UNPK ZONEDEC(UNPKLEN),PACKDEC(UNPKLEN));
         UNPK ZONEDEC(UNPKLEN),PACKDEC(UNPKLEN)
         DS    0H
*            GEN (OI   ZONEDEC+7,X'F0');
         OI   ZONEDEC+7,X'F0'
         DS    0H
*            MSGCODE=ZONEDEC (6:8);  /* MOVE ERROR CODE TO MESSAGE    *
         L     @7,MSGQ                                            0256
         MVC   46(3,@7),ZONEDEC+5                                 0256
*            RELEASE (RETREG);
*          END;
*        MSGTXT (1:MSGLL)=MSGTXTIN(1:MSGLL); /* MOVE MSGTXT INTO BMP
*                                       STORAGE                       *
@9D8     L     @8,MSGQ                                            0259
         LR    @E,@8                                              0259
         LH    @9,0(0,@8)                                         0259
         BCTR  @9,0                                               0259
         LA    @A,MSGTXT                                          0259
         EX    @9,@MVC                                            0259
*        GEN (PUTLINE PARM=PTPBA,OUTPUT=OLD,MF=(E,IOPLA));
         PUTLINE PARM=PTPBA,OUTPUT=OLD,MF=(E,IOPLA)
         DS    0H
*        IF RTNREG^=0 THEN           /* IF PUTLINE NOT SUCCESSFUL     *
         LTR   @F,@F                                              0261
         BC    08,@9D7                                            0261
*          INDICATR=0;               /* THEN SET NOMSG INDICATOR      *
         SR    @F,@F                                              0262
         STH   @F,STATUS                                          0262
*      END;
@9D7     EQU   *                                                  0263
*
* /*  THE FOLLOWING SEGMENT CLEANS UP RESOURCES - DELETES THE         *
* /*  BACKGROUND I/O MODULES AND CLOSES OPEN DCB'S.                   *
*
*    GENERATE;
@9D9     EQU   *                                                  0264
          DELETE  EP=IKJRBBMC        /*DELETE BACKGROUND I/O MODULES*/
         DS    0H
*    DCBPTR=PUTDCB;                  /* LOCATE OUTPUT DCB             *
         MVC   DCBPTR(4),BKPUT+8                                  0265
*    RESTRICT (R2);
*    IF DCBOFOPN='1'B THEN           /* IF OUTPUT DCB IS OPEN         *
         L     @3,DCBPTR                                          0267
         TM    48(@3),B'00010000'                                 0267
         BC    12,@9D6                                            0267
*      DO;
*        R1=ADDR (CLOSLIST);         /* SET ADDR OF LIST FORM         *
         LA    @1,CLOSLIST                                        0269
*        R2=ADDR (DCBP);             /* SET ADDR OF DCB               *
         LA    @2,DCBP                                            0270
*        GEN (CLOSE  ((2)),MF=(E,(1))); /* CLOSE OUTPUT DCB           *
         CLOSE  ((2)),MF=(E,(1))
         DS    0H
*      END;
*    DCBPTR=GETDCB;                  /* LOCATE INPUT DCB              *
@9D6     MVC   DCBPTR(4),BKGET+8                                  0273
*    IF DCBOFOPN='1'B THEN           /* IF INPUT DCB IS OPEN          *
         L     @3,DCBPTR                                          0274
         TM    48(@3),B'00010000'                                 0274
         BC    12,@9D5                                            0274
*      DO;
*        R1=ADDR (CLOSLIST);         /* SET ADDR OF LIST FORM         *
         LA    @1,CLOSLIST                                        0276
*        R2=ADDR (DCBG);             /* SET ADDR OF DCB               *
         LA    @2,DCBG                                            0277
*        GEN (CLOSE  ((2)),MF=(E,(1))); /* CLOSE INPUT DCB            *
         CLOSE  ((2)),MF=(E,(1))
         DS    0H
*      END;
*    RELEASE (R2);
*    RESTRICT (RETREG);
*    IF INDICATR=11 THEN             /* IF UTILITY NORMAL ENDED       *
@9D5     LA    @F,11                                              0282
         CH    @F,STATUS                                          0282
         BC    07,@9D4                                            0282
*      RETREG=0;                     /* THEN SET NORMAL RETURN CODE   *
         SR    @6,@6                                              0283
         BC    15,@9D3                                            0284
*    ELSE
*      IF INDICATR=0 THEN            /* ELSE IF UTILITY ENDED WITH    *
@9D4     SR    @F,@F                                              0284
         CH    @F,STATUS                                          0284
         BC    07,@9D2                                            0284
*        RETREG=8;                   /* OUTPUT ERROR THEN SET MESSAGE
*                                       DATA SET ERROR RETURN CODE    *
         LA    @6,8                                               0285
         BC    15,@9D1                                            0286
*      ELSE
*        RETREG=4;                   /* ELSE SET ERROR RETURN CODE    *
@9D2     LA    @6,4                                               0286
*    RETURN CODE(RETREG);            /* RETURN TO CALLER              *
@9D1     EQU   *                                                  0287
@9D3     L     @D,4(0,@D)                                         0287
         LR    @F,@6                                              0287
         ST    @F,16(0,@D)                                        0287
         LR    @1,@C                                              0287
         L     @0,@SIZ001                                         0287
         FREEMAIN R,LV=(0),A=(1)                                  0287
         LM    @E,@C,12(@D)                                       0287
         BCR   15,@E                                              0287
*    RELEASE (RETREG);
*
* /*  THE FOLLOWING ARE THE LIST FORM OF MACRO INSTRUCTIONS USED      *
* /*  BY THE BMP.                                                     *
