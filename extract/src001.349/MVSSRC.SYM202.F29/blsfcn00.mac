         ST    ZZ2PTR,@AL00001                                  01S0154
         L     @15,@PC00001+4                                   01S0154
         ST    @15,@AL00001+4                                   01S0154
         LA    @15,MSGPTR                                       01S0154
         ST    @15,@AL00001+8                                   01S0154
         L     @01,ZZ2MSGSP(,ZZ2PTR)                            01S0154
         ST    @01,@AL00001+12                                  01S0154
         LA    @15,@CB00826                                     01S0154
         ST    @15,@AL00001+16                                  01S0154
         LA    @15,@CF00049                                     01S0154
         ST    @15,@AL00001+20                                  01S0154
         L     @15,ZZ2MSG0P(,ZZ2PTR)                            01S0154
         LA    @01,@AL00001                                     01S0154
         BALR  @14,@15                                          01S0154
*       END;                        /* BLSDMSG                  01S0155
*                                      (ZZ2,MSGNR,MSGPTR,BLSDMSGS)
*                                      LIFO                          */
*       RESPECIFY                                               01S0156
*         RF RESTRICTED;            /* RESTRICT USED OF REG 15 TO
*                                      RETURN CODES                  */
*       IF RF=ZERO THEN             /* HAS MESSAGE BEEN FOUND?       */
         LTR   RF,RF                                            01S0157
         BNZ   @RF00157                                         01S0157
*         DO;                       /* YES, PROCESS MESSAGE          */
*                                                               01S0158
*           /*********************************************************/
*           /*                                                       */
*           /* BUILD MESSAGE IN BUFFER FOR OUTPUT                    */
*           /*                                                       */
*           /*********************************************************/
*                                                               01S0159
*           MYMBUF=MYMBUF&&MYMBUF;  /* CLEAN THE AREA                */
         XC    MYMBUF(256),MYMBUF                               01S0159
*           MYMLEN=(MSGWLEN-ONE);   /* MOVE IN LENGTH                */
         L     @14,MSGPTR                                       01S0160
         LH    @05,MSGWLEN(,@14)                                01S0160
         LR    @04,@05                                          01S0160
         BCTR  @04,0                                            01S0160
         STH   @04,MYMLEN                                       01S0160
*           MYMOFF=MSGWPAD;         /* MOVE IN THE OFFSET            */
         LH    @04,MSGWPAD(,@14)                                01S0161
         STH   @04,MYMOFF                                       01S0161
*           MYMTXT(ONE:(MSGWLEN-FIVE))=MSGWTXT(TWO:(MSGWLEN-FIVE));
*                                                               01S0162
         SL    @05,@CF00181                                     01S0162
         EX    @05,@SM00847                                     01S0162
*           /*********************************************************/
*           /*                                                       */
*           /* PUT OUT THE MESSAGE USING TSO PUTGET                  */
*           /*                                                       */
*           /*********************************************************/
*                                                               01S0163
*           CALL PRMPTMSG;          /* PUT OUT THE MESSAGE           */
         BAL   @14,PRMPTMSG                                     01S0163
*           IF RETCODE=FOUR THEN    /* IS REPROMPT NEEDED?           */
         C     RETCODE,@CF00041                                 01S0164
         BNE   @RF00164                                         01S0164
*             DO;                   /* YES, REPROMPT IS REQUIRED     */
*               DO;                 /* BLSDMSG                  01S0166
*                                      (ZZ2,MONE,MSGPTR,BLSDMSGS)    */
*                 CALL BLSDMSG0(ZZ2,MONE,MSGPTR,BLSDMSGS,'00000000'B,0)
*                     ;             /* BUILD MESSAGE                 */
         ST    ZZ2PTR,@AL00001                                  01S0167
         LA    @14,@CF00818                                     01S0167
         ST    @14,@AL00001+4                                   01S0167
         LA    @14,MSGPTR                                       01S0167
         ST    @14,@AL00001+8                                   01S0167
         L     @14,ZZ2MSGSP(,ZZ2PTR)                            01S0167
         ST    @14,@AL00001+12                                  01S0167
         LA    @14,@CB00828                                     01S0167
         ST    @14,@AL00001+16                                  01S0167
         LA    @14,@CF00049                                     01S0167
         ST    @14,@AL00001+20                                  01S0167
         L     @15,ZZ2MSG0P(,ZZ2PTR)                            01S0167
         LA    @01,@AL00001                                     01S0167
         BALR  @14,@15                                          01S0167
*               END;                /* BLSDMSG                  01S0168
*                                      (ZZ2,MONE,MSGPTR,BLSDMSGS)
*                                      PURGE MSG CHAIN               */
*               MSGPTR=ZERO;        /* ZERO POINTER FOR NEXT MESSAGE */
         SLR   @14,@14                                          01S0169
         ST    @14,MSGPTR                                       01S0169
*               DO;                 /* BLSDMSG                  01S0170
*                                      (ZZ2,04067,MSGPTR,BLSDMSGS)
*                                      LIFO                          */
*                 CALL BLSDMSG0(ZZ2,04067,MSGPTR,BLSDMSGS,'10000000'B,0
*                     );            /* BUILD MESSAGE                 */
         ST    ZZ2PTR,@AL00001                                  01S0171
         LA    @14,@CF00829                                     01S0171
         ST    @14,@AL00001+4                                   01S0171
         LA    @14,MSGPTR                                       01S0171
         ST    @14,@AL00001+8                                   01S0171
         L     @14,ZZ2MSGSP(,ZZ2PTR)                            01S0171
         ST    @14,@AL00001+12                                  01S0171
         LA    @14,@CB00826                                     01S0171
         ST    @14,@AL00001+16                                  01S0171
         LA    @14,@CF00049                                     01S0171
         ST    @14,@AL00001+20                                  01S0171
         L     @15,ZZ2MSG0P(,ZZ2PTR)                            01S0171
         LA    @01,@AL00001                                     01S0171
         BALR  @14,@15                                          01S0171
*               END;                /* BLSDMSG                  01S0172
*                                      (ZZ2,04067,MSGPTR,BLSDMSGS)
*                                      LIFO REPROMPT MSG             */
*               IF RF=ZERO THEN     /* IS MESSAGE TEXT OK?           */
         LTR   RF,RF                                            01S0173
         BNZ   @RF00173                                         01S0173
*                 DO;               /* BUILD THE MESSAGE             */
*                                                               01S0174
*                   /*************************************************/
*                   /*                                               */
*                   /* BUILD MESSAGE IN BUFFER FOR OUTPUT            */
*                   /*                                               */
*                   /*************************************************/
*                                                               01S0175
*                   MYMBUF=MYMBUF&&MYMBUF;/* CLEAN THE AREA          */
         XC    MYMBUF(256),MYMBUF                               01S0175
*                   MYMLEN=(MSGWLEN-ONE);/* MOVE IN LENGTH           */
         L     @14,MSGPTR                                       01S0176
         LH    @09,MSGWLEN(,@14)                                01S0176
         LR    @05,@09                                          01S0176
         BCTR  @05,0                                            01S0176
         STH   @05,MYMLEN                                       01S0176
*                   MYMOFF=MSGWPAD; /* MOVE IN THE OFFSET            */
         LH    @05,MSGWPAD(,@14)                                01S0177
         STH   @05,MYMOFF                                       01S0177
*                   MYMTXT(ONE:(MSGWLEN-FIVE))=MSGWTXT(TWO:(MSGWLEN-
*                       FIVE));                                 01S0178
*                                                               01S0178
         SL    @09,@CF00181                                     01S0178
         EX    @09,@SM00847                                     01S0178
*                   /*************************************************/
*                   /*                                               */
*                   /* PUT OUT THE MESSAGE USING TSO PUTGET          */
*                   /*                                               */
*                   /*************************************************/
*                                                               01S0179
*                   DO ANSWER=1 TO 5 WHILE(RETC^=TWELVE&RETCODE=FOUR);
         LA    ANSWER,1                                         01S0179
@DL00179 C     RETC,@CF00034                                    01S0179
         BE    @DC00179                                         01S0179
         C     RETCODE,@CF00041                                 01S0179
         BNE   @DC00179                                         01S0179
*                                   /* DO LOOP                       */
*                     CALL PRMPTMSG;/* PUT OUT MSG                   */
         BAL   @14,PRMPTMSG                                     01S0180
*                     IF ZZ1EVEP='1'B THEN/* HAS USER REQESTED TERM? */
         L     @14,ZZ2ZZ1P(,ZZ2PTR)                             01S0181
         TM    ZZ1EVEP(@14),B'01000000'                         01S0181
         BNO   @RF00181                                         01S0181
*                       RETC=TWELVE;/* YES, GET OUT OF LOOP          */
         LA    RETC,12                                          01S0182
*                   END;            /* END OF DO LOOP                */
@RF00181 AL    ANSWER,@CF00044                                  01S0183
         C     ANSWER,@CF00094                                  01S0183
         BNH   @DL00179                                         01S0183
@DC00179 DS    0H                                               01S0184
*                 END;              /* END OF BUILD THE MESSAGE      */
*               ELSE                /* NO, REPROMPT MSG LOGIC ERROR  */
*                 RETC=TWELVE;      /* REQ TERMINATION               */
         B     @RC00173                                         01S0185
@RF00173 LA    RETC,12                                          01S0185
*             END;                  /* END OF REPROMPT REQUIRED      */
@RC00173 DS    0H                                               01S0187
*           DO;                     /* BLSDMSG                  01S0187
*                                      (ZZ2,MONE,MSGPTR,BLSDMSGS)    */
@RF00164 DS    0H                                               01S0188
*             CALL BLSDMSG0(ZZ2,MONE,MSGPTR,BLSDMSGS,'00000000'B,0);
         ST    ZZ2PTR,@AL00001                                  01S0188
         LA    @14,@CF00818                                     01S0188
         ST    @14,@AL00001+4                                   01S0188
         LA    @14,MSGPTR                                       01S0188
         ST    @14,@AL00001+8                                   01S0188
         L     @14,ZZ2MSGSP(,ZZ2PTR)                            01S0188
         ST    @14,@AL00001+12                                  01S0188
         LA    @14,@CB00828                                     01S0188
         ST    @14,@AL00001+16                                  01S0188
         LA    @14,@CF00049                                     01S0188
         ST    @14,@AL00001+20                                  01S0188
         L     @15,ZZ2MSG0P(,ZZ2PTR)                            01S0188
         LA    @01,@AL00001                                     01S0188
         BALR  @14,@15                                          01S0188
*                                   /* BUILD MESSAGE                 */
*           END;                    /* BLSDMSG                  01S0189
*                                      (ZZ2,MONE,MSGPTR,BLSDMSGS)
*                                      PURGE MSG CHAIN               */
*         END;                      /* END OF MSG FOUND              */
*       ELSE                        /* NO, END PROCESSING            */
*         RETC=TWELVE;                                          01S0191
         B     @RC00157                                         01S0191
@RF00157 LA    RETC,12                                          01S0191
*     END;                          /* END OF PROCESS MESSAGE        */
*   ELSE                            /* YES, RETURN TO CALLER         */
*     RETC=TWELVE;                  /* TERMINATE COMMAND             */
         B     @RC00151                                         01S0193
@RF00151 LA    RETC,12                                          01S0193
*   RETURN CODE(RETC);              /* RETURN TO CALLER              */
@RC00151 L     @13,4(,@13)                                      01S0194
         L     @00,@SIZDATD                                     01S0194
         LR    @01,@11                                          01S0194
         BLSUFRES R,LV=(0),A=(1)
         LR    @15,@07                                          01S0194
         L     @14,12(,@13)                                     01S0194
         LM    @00,@12,20(@13)                                  01S0194
         BR    @14                                              01S0194
*PRMPTMSG:                                                      01S0195
*   PROCEDURE;                      /* ISSUE MESSAGE AND VERIFY REPLY*/
*                                                               01S0195
PRMPTMSG STM   @14,@06,@SA00002                                 01S0195
         STM   @09,@12,@SA00002+36                              01S0195
*   /*****************************************************************/
*   /*                                                               */
*   /* ISSUE MESSAGE VIA PUTGET FOR CONFIRMATION PROMPT              */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0196
*   IF ZZ1EVEP='0'B THEN            /* HAS USER REQUESTED END?       */
         L     @14,ZZ2ZZ1P(,ZZ2PTR)                             01S0196
         TM    ZZ1EVEP(@14),B'01000000'                         01S0196
         BNZ   @RF00196                                         01S0196
*     DO;                           /* NO, PROCESS REQUEST           */
*       MSGOUT1=MSGOUT;             /* COPY THE LIST FORM OF MACRO   */
         MVC   MSGOUT1(16),MSGOUT                               01S0198
*       MSGTXT=ADDR(MYMBUF);        /* POINT TO MSG TEXT-OLD FORMAT  */
         LA    @05,MYMBUF                                       01S0199
         ST    @05,MSGTXT                                       01S0199
*       RESPECIFY                                               01S0200
*        (R2,                                                   01S0200
*         R3,                                                   01S0200
*         R4) RESTRICTED;           /* PROTECT REGS FOR CALL TO PUT
*                                      GET                           */
*       R2=ADDR(ZZ1EVE);            /* ADDRESS OF ECB IN REG2        */
         LA    R2,ZZ1EVE(,@14)                                  01S0201
*       R3=CPPLUPT;                 /* PUT ADDR OF UPT IN REG 3      */
         L     R3,CPPLUPT(,ZZ2PTR)                              01S0202
*       R4=CPPLECT;                 /* PUT ECT ADDR IN REG 4         */
         L     R4,CPPLECT(,ZZ2PTR)                              01S0203
*       GEN;                                                    01S0204
*              /* ISSUE PUT GET MESSAGE */
     PUTGET PARM=MSGOUT1,UPT=(3),ECT=(4),ECB=(2),                      C
               OUTPUT=(MSGOLD,SINGLE,TERM),MF=(E,INOUTPL)
*       RESPECIFY                                               01S0205
*        (R2,                                                   01S0205
*         R3,                                                   01S0205
*         R4) UNRESTRICTED;         /* FREE REGS                     */
*       IF RF=ZERO THEN             /* DID THE PROMPT GET ANSWERED?  */
         LTR   RF,RF                                            01S0206
         BNZ   @RF00206                                         01S0206
*         DO;                       /* YES, ANSWER                   */
*                                                               01S0207
*           /*********************************************************/
*           /*                                                       */
*           /* PROCESS THE PROMPT REPLY                              */
*           /*                                                       */
*           /*********************************************************/
*                                                               01S0208
*           IF(ANS×BLANK)=YES THEN  /* WAS THE ANSWER YES            */
         L     @14,PGPBIBUF(,PGPBPTR)                           01S0208
         MVC   @TS00001(1),ANS(@14)                             01S0208
         OC    @TS00001(1),BLANK                                01S0208
         CLI   @TS00001,C'Y'                                    01S0208
         BNE   @RF00208                                         01S0208
*             DO;                   /* ANSWER YES                    */
*               RETC=ZERO;          /* YES, INDICATE YES             */
         SLR   RETC,RETC                                        01S0210
*               RETCODE=ZERO;       /* REPROMPT NOT NEEDED           */
         SLR   RETCODE,RETCODE                                  01S0211
*             END;                  /* END OF ANSWER YES             */
*           ELSE                    /* NO                            */
*             IF(ANS×BLANK)=NO THEN /* WAS THE ANSWER NO?            */
         B     @RC00208                                         01S0213
@RF00208 L     @14,PGPBIBUF(,PGPBPTR)                           01S0213
         MVC   @TS00001(1),ANS(@14)                             01S0213
         OC    @TS00001(1),BLANK                                01S0213
         CLI   @TS00001,C'N'                                    01S0213
         BNE   @RF00213                                         01S0213
*               DO;                 /* ANSWER NO                     */
*                 RETC=FOUR;        /* YES, INDICATE NO ANSWER       */
         LA    RETC,4                                           01S0215
*                 RETCODE=ZERO;     /* REPROMPT NOT NEEDED           */
         SLR   RETCODE,RETCODE                                  01S0216
*               END;                /* END OF ANSWER NO              */
*             ELSE                  /* NO                            */
*               IF ANSWER=FIVE THEN /* HAVE WE TRIED TOO MANY TIMES? */
         B     @RC00213                                         01S0218
@RF00213 C     ANSWER,@CF00094                                  01S0218
         BNE   @RF00218                                         01S0218
*                 RETC=TWELVE;      /* TERMINATE THE REQUEST         */
         LA    RETC,12                                          01S0219
*               ELSE                /* NO TRY AGAIN                  */
*                 RETCODE=FOUR;     /* REPROMPT IS NEEDED            */
         B     @RC00218                                         01S0220
@RF00218 LA    RETCODE,4                                        01S0220
*         END;                      /* END OF ANSWER                 */
*       ELSE                        /* NO ANSWER TREAT AS ERROR      */
*         DO;                       /* PUTGET ERROR MSG              */
         B     @RC00206                                         01S0222
@RF00206 DS    0H                                               01S0223
*           RCSAVE=RF;              /* SAVE THE RETCODE              */
         LR    RCSAVE,RF                                        01S0223
*           CVD(RCSAVE,TEMPRC);     /* CONVERT TO DEC                */
         CVD   RCSAVE,@TS00001                                  01S0224
         MVC   TEMPRC(4),@TS00001+4                             01S0224
*           PACKRC=TEMPRC(3:4);     /* REDUCE THE SIZE               */
         MVC   PACKRC(2),TEMPRC+2                               01S0225
*           UNPK(UNPKRC,PACKRC);    /* UNPACK THE NUMBER             */
         UNPK  UNPKRC(3),PACKRC(2)                              01S0226
*           UNPKRC(3:3)=UNPKRC(3:3)×ZEROCHAR;/* FIXED THE SIGN       */
         OC    UNPKRC+2(1),ZEROCHAR                             01S0227
*           DO;                     /* BLSDMSG                  01S0228
*                                      (ZZ2,04066,MSGPTR,BLSDMSGS)
*                                      FIFO ROUTE(TERM)INSERT(UNPKRC)*/
*             IOPTLIST(1)=0;        /* LAST INSERT                   */
         SLR   @14,@14                                          01S0229
         ST    @14,IOPTLIST                                     01S0229
*             IOPTLIST(2)=ADDR(UNPKRC);/* INSERT NAME POINTER        */
         LA    @05,UNPKRC                                       01S0230
         ST    @05,IOPTLIST+4                                   01S0230
*             IOPTLIST(3)=0;        /* ZERO RESERVED FIELD           */
         ST    @14,IOPTLIST+8                                   01S0231
*             RFY                                               01S0232
*               I015F BASED(ADDR(IOPTLIST(3)));/* ACCESS BYTES 1-2   */
*             I015F=LENGTH(UNPKRC); /* INSERT LENGTH                 */
         LA    @14,IOPTLIST+8                                   01S0233
         MVC   I015F(2,@14),@CH00097                            01S0233
*             CALL BLSDMSG0(ZZ2,04066,MSGPTR,BLSDMSGS,'00010000'B,ADDR(
*                 IOPTLIST));       /* BUILD MESSAGE                 */
         ST    ZZ2PTR,@AL00001                                  01S0234
         LA    @14,@CF00839                                     01S0234
         ST    @14,@AL00001+4                                   01S0234
         LA    @14,MSGPTR                                       01S0234
         ST    @14,@AL00001+8                                   01S0234
         L     @14,ZZ2MSGSP(,ZZ2PTR)                            01S0234
         ST    @14,@AL00001+12                                  01S0234
         LA    @14,@CB00840                                     01S0234
         ST    @14,@AL00001+16                                  01S0234
         LA    @14,IOPTLIST                                     01S0234
         ST    @14,@AFTEMPS                                     01S0234
         LA    @14,@AFTEMPS                                     01S0234
         ST    @14,@AL00001+20                                  01S0234
         L     @15,ZZ2MSG0P(,ZZ2PTR)                            01S0234
         LA    @01,@AL00001                                     01S0234
         BALR  @14,@15                                          01S0234
*           END;                    /* BLSDMSG                  01S0235
*                                      (ZZ2,04066,MSGPTR,BLSDMSGS)
*                                      FIFO ROUTE(TERM)INSERT(UNPKRC)*/
*                                                               01S0235
*           /*********************************************************/
*           /*                                                       */
*           /* CONFLICTS EXIST, UNABLE TO GET CONFIRMATION, ADD DSN  */
*           /* TERM                                                  */
*           /*                                                       */
*           /*********************************************************/
*                                                               01S0236
*           RETC=TWELVE;            /* INDICATE TERMINATION REQUESTED*/
         LA    RETC,12                                          01S0236
*         END;                      /* END OF PUTGET ERROR MSG       */
*     END;                          /* END OF USER DID NOT REQ END   */
*   ELSE                            /* YES, TERMINATE                */
*     RETC=TWELVE;                  /* TERMINATE THE SUB COMMAND     */
         B     @RC00196                                         01S0239
@RF00196 LA    RETC,12                                          01S0239
*   END PRMPTMSG;                   /* END OF PRMPTMSG PROC          */
@EL00002 DS    0H                                               01S0240
@EF00002 DS    0H                                               01S0240
@ER00002 LM    @14,@06,@SA00002                                 01S0240
         LM    @09,@12,@SA00002+36                              01S0240
         BR    @14                                              01S0240
