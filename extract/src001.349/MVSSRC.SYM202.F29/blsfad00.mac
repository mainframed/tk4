*                                      POINTER                       */
         LA    @14,CURDSN                                       01S0292
         ST    @14,IOPTLIST+4                                   01S0292
*                           IOPTLIST(3)=0;/* ZERO RESERVED FIELD     */
         SLR   @14,@14                                          01S0293
         ST    @14,IOPTLIST+8                                   01S0293
*                           RFY                                 01S0294
*                             I015F BASED(ADDR(IOPTLIST(3)));/* 01S0294
*                                      ACCESS BYTES 1-2              */
*                           I015F=LENGTH(CURDSN);/* INSERT LENGTH    */
         LA    @10,IOPTLIST+8                                   01S0295
         MVC   I015F(2,@10),@CH00085                            01S0295
*                           IOPTLIST(4)=ADDR(IOPTLIST(7));/* CHAIN TO
*                                      NEXT INSERT                   */
         LA    @10,IOPTLIST+24                                  01S0296
         ST    @10,IOPTLIST+12                                  01S0296
*                           IOPTLIST(5)=ADDR(LPARNF);/* INSERT NAME
*                                      POINTER                       */
         LA    @10,LPARNF                                       01S0297
         ST    @10,IOPTLIST+16                                  01S0297
*                           IOPTLIST(6)=0;/* ZERO RESERVED FIELD     */
         ST    @14,IOPTLIST+20                                  01S0298
*                           RFY                                 01S0299
*                             I015F BASED(ADDR(IOPTLIST(6)));/* 01S0299
*                                      ACCESS BYTES 1-2              */
*                           I015F=LENGTH(LPARNF);/* INSERT LENGTH    */
         LA    @10,1                                            01S0300
         LA    @06,IOPTLIST+20                                  01S0300
         STH   @10,I015F(,@06)                                  01S0300
*                           IOPTLIST(7)=ADDR(IOPTLIST(10));/* CHAIN
*                                      TO NEXT INSERT                */
         LA    @06,IOPTLIST+36                                  01S0301
         ST    @06,IOPTLIST+24                                  01S0301
*                           IOPTLIST(8)=ADDR(CURMEM);/* INSERT NAME
*                                      POINTER                       */
         LA    @06,CURMEM                                       01S0302
         ST    @06,IOPTLIST+28                                  01S0302
*                           IOPTLIST(9)=0;/* ZERO RESERVED FIELD     */
         ST    @14,IOPTLIST+32                                  01S0303
*                           RFY                                 01S0304
*                             I015F BASED(ADDR(IOPTLIST(9)));/* 01S0304
*                                      ACCESS BYTES 1-2              */
*                           I015F=LENGTH(CURMEM);/* INSERT LENGTH    */
         LA    @06,IOPTLIST+32                                  01S0305
         MVC   I015F(2,@06),@CH00048                            01S0305
*                           IOPTLIST(10)=ADDR(IOPTLIST(13));/* CHAIN
*                                      TO NEXT INSERT                */
         LA    @06,IOPTLIST+48                                  01S0306
         ST    @06,IOPTLIST+36                                  01S0306
