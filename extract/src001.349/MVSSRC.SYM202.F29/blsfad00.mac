*       /*                                                           */
*       /* UNABLE TO PARSE                                           */
*       /*                                                           */
*       /*************************************************************/
*                                                               01S0237
*       DO;                         /* UNABLE TO PARSE               */
*         RETC=GPR15F;              /* RETURN CODE                   */
         LR    RETC,GPR15F                                      01S0238
*         GO TO EXITNORL;           /* EXIT WITHOUT PDL              */
         B     EXITNORL                                         01S0239
*       END;                                                    01S0240
*     RESPECIFY                                                 01S0241
*      (GPR15F) UNRESTRICTED;                                   01S0241
@RF00236 DS    0H                                               01S0242
