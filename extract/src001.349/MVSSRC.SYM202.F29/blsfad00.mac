*       DSDBSKEY,OLDSTPDR,ENQFLAG); /* GET CURRENT PROB AND DSN (READ
*                                      IN PDR STATUS RECORD, IF 01S0255
*                                      AVAILABLE)                    */
         ST    ZZ2PTR,@AL00001                                  01S0255
         LA    @10,DSNS                                         01S0255
         ST    @10,@AL00001+4                                   01S0255
         LA    @10,PRN                                          01S0255
         ST    @10,@AL00001+8                                   01S0255
         LA    @10,MAN                                          01S0255
         ST    @10,@AL00001+12                                  01S0255
         LA    @10,CURDSN                                       01S0255
         ST    @10,@AL00001+16                                  01S0255
         LA    @10,CURMEM                                       01S0255
         ST    @10,@AL00001+20                                  01S0255
         LA    @10,CURPID                                       01S0255
         ST    @10,@AL00001+24                                  01S0255
         LA    @10,PDRSTKEY                                     01S0255
         ST    @10,@AL00001+28                                  01S0255
         LA    @10,DSDBSKEY                                     01S0255
         ST    @10,@AL00001+32                                  01S0255
         LA    @10,OLDSTPDR                                     01S0255
         ST    @10,@AL00001+36                                  01S0255
         LA    @10,ENQFLAG                                      01S0255
         ST    @10,@AL00001+40                                  01S0255
         L     @15,@CV01151                                     01S0255
         LA    @01,@AL00001                                     01S0255
         BALR  @14,@15                                          01S0255
*   RESPECIFY                                                   01S0256
*     RF RESTRICTED;                /* SAVE REG 15                   */
*   FAVCODE=RF;                     /* SAVE THE CODE FOR TRACK AND
*                                      TEST                          */
         ST    RF,FAVCODE                                       01S0257
*   IF ENQDONE='1'B THEN            /* HAS ENQUEUE BEEN COMPLETED?   */
         TM    ENQDONE,B'10000000'                              01S0258
         BNO   @RF00258                                         01S0258
*     ENQ='1'B;                     /* YES, MARK IT FOR FUTURE  01S0259
*                                      PROCESSING                    */
         OI    ENQ,B'00010000'                                  01S0259
*   IF RF<TWELVE THEN               /* NO TERMINATING ERROR?         */
@RF00258 C     RF,@CF00032                                      01S0260
         BNL   @RF00260                                         01S0260
*     DO;                           /* CONTINUE PROCESSING           */
*       IF RF=EIGHT THEN            /* MEMBER SPECIFIED - UNMANAGED
*                                      FORCED                        */
         C     RF,@CF00048                                      01S0262
         BNE   @RF00262                                         01S0262
*         FORUNMAN='1'B;            /* FORCE UNMANAGED ON            */
         OI    FORUNMAN,B'00100000'                             01S0263
*       DO;                         /* CONTINUE PROCESSING           */
*                                                               01S0264
@RF00262 DS    0H                                               01S0265
*         /***********************************************************/
*         /*                                                         */
*         /* READ IN THE DSD BASE RECORD FOR THIS DATA SET IF ONE    */
*         /* EXISTS.                                                 */
*         /*                                                         */
