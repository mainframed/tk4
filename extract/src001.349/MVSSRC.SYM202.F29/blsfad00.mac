*                                      ACCESS BYTES 1-2              */
*                           I015F=LENGTH(CURPROB);/* INSERT LENGTH   */
         LA    @14,IOPTLIST+68                                  01S0320
         MVC   I015F(2,@14),@CH00090                            01S0320
*                           CALL BLSDMSG0(ZZ2,04083,MSGPTR,BLSDMSGS,
*                               '10010000'B,ADDR(IOPTLIST));/* BUILD
*                                      MESSAGE                       */
         ST    ZZ2PTR,@AL00001                                  01S0321
         LA    @14,@CF01169                                     01S0321
         ST    @14,@AL00001+4                                   01S0321
         LA    @14,MSGPTR                                       01S0321
         ST    @14,@AL00001+8                                   01S0321
         L     @14,ZZ2MSGSP(,ZZ2PTR)                            01S0321
         ST    @14,@AL00001+12                                  01S0321
         LA    @14,@CB01170                                     01S0321
         ST    @14,@AL00001+16                                  01S0321
         LA    @14,IOPTLIST                                     01S0321
         ST    @14,@AFTEMPS+4                                   01S0321
         LA    @14,@AFTEMPS+4                                   01S0321
         ST    @14,@AL00001+20                                  01S0321
         L     @15,ZZ2MSG0P(,ZZ2PTR)                            01S0321
         LA    @01,@AL00001                                     01S0321
         BALR  @14,@15                                          01S0321
*                         END;      /* BLSDMSG                  01S0322
*                                      (ZZ2,04083,MSGPTR,BLSDMSGS)
*                                      LIFO ROUTE(TERM)INSERT(CURDSN,
*                                      LPARNF,CURMEM,RPARNF,FPPIDP,CU
