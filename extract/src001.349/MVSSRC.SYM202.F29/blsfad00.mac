*                             I015F BASED(ADDR(IOPTLIST(15)));/*
*                                      ACCESS BYTES 1-2              */
*                           I015F=LENGTH(FPPIDP);/* INSERT LENGTH    */
         LA    @10,IOPTLIST+56                                  01S0315
         MVC   I015F(2,@10),@CH00093                            01S0315
*                           IOPTLIST(16)=0;/* LAST INSERT            */
         ST    @14,IOPTLIST+60                                  01S0316
*                           IOPTLIST(17)=ADDR(CURPROB);/* INSERT NAME
*                                      POINTER                       */
         LA    @10,CURPROB                                      01S0317
         ST    @10,IOPTLIST+64                                  01S0317
*                           IOPTLIST(18)=0;/* ZERO RESERVED FIELD    */
         ST    @14,IOPTLIST+68                                  01S0318
