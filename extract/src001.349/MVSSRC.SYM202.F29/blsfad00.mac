*                                      POINTER                       */
         LA    @06,RPARNF                                       01S0307
         ST    @06,IOPTLIST+40                                  01S0307
*                           IOPTLIST(12)=0;/* ZERO RESERVED FIELD    */
         ST    @14,IOPTLIST+44                                  01S0308
*                           RFY                                 01S0309
*                             I015F BASED(ADDR(IOPTLIST(12)));/*
*                                      ACCESS BYTES 1-2              */
*                           I015F=LENGTH(RPARNF);/* INSERT LENGTH    */
         LA    @06,IOPTLIST+44                                  01S0310
         STH   @10,I015F(,@06)                                  01S0310
*                           IOPTLIST(13)=ADDR(IOPTLIST(16));/* CHAIN
*                                      TO NEXT INSERT                */
         LA    @10,IOPTLIST+60                                  01S0311
         ST    @10,IOPTLIST+48                                  01S0311
*                           IOPTLIST(14)=ADDR(FPPIDP);/* INSERT NAME
*                                      POINTER                       */
         L     @10,ZZ2ZZ1P(,ZZ2PTR)                             01S0312
         L     @10,ZZ1FPP(,@10)                                 01S0312
         LA    @10,FPPIDP(,@10)                                 01S0312
         ST    @10,IOPTLIST+52                                  01S0312
*                           IOPTLIST(15)=0;/* ZERO RESERVED FIELD    */
         ST    @14,IOPTLIST+56                                  01S0313
