         ST    ZZ2PTR,@AL00001                                  01S0234
         L     @10,PCPTR                                        01S0234
         ST    @10,@AL00001+4                                   01S0234
         LA    @10,PDAPTR                                       01S0234
         ST    @10,@AL00001+8                                   01S0234
