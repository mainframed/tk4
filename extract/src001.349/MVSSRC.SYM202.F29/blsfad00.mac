*/*   GET SEQ NUMBER FROM STATUS RECORD AND UPDATE IT TO THE NEXT
*     NUMBER                                                         */
*                                                               01S0546
*   GSTPDR='1'B;                    /* GET STATUS RECORD PROC TRACKED*/
*                                                               01S0546
         OI    GSTPDR,B'00001000'                               01S0546
*   /*****************************************************************/
*   /*                                                               */
*   /* MOVE THE STATUS RECORD INTO BUFFER TO BE WRITTEN OUT          */
*   /*                                                               */
*   /*****************************************************************/
*                                                               01S0547
*   IF ZZ1EVEP='0'B THEN            /* USER REQUEST TERMINATION?     */
         L     @14,ZZ2ZZ1P(,ZZ2PTR)                             01S0547
         TM    ZZ1EVEP(@14),B'01000000'                         01S0547
         BNZ   @RF00547                                         01S0547
*     DO;                           /* NO, GET SEQ NUMBER            */
*       RESPECIFY                                               01S0549
*        (R2,                                                   01S0549
*         R3,                                                   01S0549
*         R4,                                                   01S0549
*         R5) RESTRICTED;           /* SAVE REGS USED DURING THE MOVE*/
*                                                               01S0549
*       /*************************************************************/
*       /*                                                           */
*       /* CLEAR AREA FOR STATUS RECORD                              */
*       /*                                                           */
*       /*************************************************************/
*                                                               01S0550
*       R2=ADDR(NEWSTPDR);          /* POINT TO NEW STATUS RECORD    */
         LA    R2,NEWSTPDR                                      01S0550
*       R3=LENGTH(NEWSTPDR);        /* LENGTH OF RECORD              */
         LA    @14,416                                          01S0551
         LR    R3,@14                                           01S0551
*       R4=ADDR(OLDSTPDR);          /* FROM FIELD                    */
         LA    R4,OLDSTPDR                                      01S0552
*       R5=LENGTH(OLDSTPDR);        /* LENGTH OF CHARACTER           */
         LR    R5,@14                                           01S0553
*       R5=R5×PADCHAR;              /* INDICATE A PAD CHAR OF BLANK  */
         O     R5,@CF01063                                      01S0554
*       MVCL(R2,R4);                /* CLEAR OUT THE RECORD          */
         MVCL  R2,R4                                            01S0555
*       RESPECIFY                                               01S0556
*        (R2,                                                   01S0556
*         R3,                                                   01S0556
*         R4,                                                   01S0556
*         R5) UNRESTRICTED;         /* FREE REGS USED DURING THE MOVE*/
*                                                               01S0556
*       /*************************************************************/
*       /*                                                           */
*       /* OBTAIN THE SEQ NUMBER FOR THIS ASSOCIATION                */
*       /*                                                           */
*       /*************************************************************/
*                                                               01S0557
*       RESPECIFY                                               01S0557
*         PDREC BASED(ADDR(NEWSTPDR));/* ESTABLISH NEW PDR RECORD    */
*       SEQNUM=PDRDSSEQ;            /* SEQUENCE NUMBER FOR THE DATA
*                                      SET RECORD                    */
         LA    @14,NEWSTPDR                                     01S0558
         MVC   SEQNUM(4),PDRDSSEQ(@14)                          01S0558
*       IF(SEQNUM&SEQMASK)=SEQMASK THEN/* ARE THEY ALL NUMBERS?      */
         MVC   @TS00001(4),SEQNUM                               01S0559
         NC    @TS00001(4),@CC01052                             01S0559
         CLC   @TS00001(4),@CC01052                             01S0559
         BNE   @RF00559                                         01S0559
*         DO;                       /* YES, PACK AND CONVERT         */
*           PACK(PKINSEQ,SEQNUM);   /* PACK THE SEQ NUMBER           */
         PACK  PKINSEQ(4),SEQNUM(4)                             01S0561
*           CVB(CVBINSEQ,PKINSEQ);  /* CONVERT TO BINARY             */
*                                                               01S0562
         XC    @TS00001(8),@TS00001                             01S0562
         MVC   @TS00001+4(4),PKINSEQ                            01S0562
         CVB   CVBINSEQ,@TS00001                                01S0562
*           /*********************************************************/
*           /*                                                       */
*           /* ADD ONE TO THE SEQ NUMBER FOR NEXT RECORD ASSOCIATION */
*           /*                                                       */
*           /*********************************************************/
*                                                               01S0563
*           CVBINSEQ=CVBINSEQ+ONE;  /* ADD ONE FOR NEW SEQ NUMBER    */
*                                                               01S0563
         AL    CVBINSEQ,@CF00040                                01S0563
*           /*********************************************************/
*           /*                                                       */
*           /* CONVERT THE NUMBER TO PRINTABLE CHARACTERS            */
*           /*                                                       */
*           /*********************************************************/
*                                                               01S0564
*           CVD(CVBINSEQ,PKOUTSEQ); /* CONVER TO DECIMAL             */
         CVD   CVBINSEQ,@TS00001                                01S0564
         MVC   PKOUTSEQ(4),@TS00001+4                           01S0564
*           TEMPSEQ=PKOUTSEQ(2:4);  /* SAVE ONLY AMOUNT NEEDED       */
         MVC   TEMPSEQ(3),PKOUTSEQ+1                            01S0565
*           UNPK(NEWSEQNR,TEMPSEQ); /* UNPACK THE NEW SEQUENCE NUMBER*/
*                                                               01S0566
         UNPK  NEWSEQNR(4),TEMPSEQ(3)                           01S0566
*           /*********************************************************/
*           /*                                                       */
*           /* UPDATE THE SEQUENCE NUMBER IN THE NEW PDR RECORD      */
*           /*                                                       */
*           /*********************************************************/
*                                                               01S0567
*           NEWSEQNR(4:4)=NEWSEQNR(4:4)×ZEROCHAR(1:1);/* FIX THE SIGN*/
         OC    NEWSEQNR+3(1),ZEROCHAR                           01S0567
*           PDRDSSEQ=NEWSEQNR;      /* UPDATE THE NUMBER IN THE 01S0568
*                                      STATUS RECORD                 */
         MVC   PDRDSSEQ(4,@14),NEWSEQNR                         01S0568
*         END;                      /* END PACK AND CONVERT          */
*       ELSE                        /* NO, STATUS RECORD IS IN ERROR */
*         DO;                       /* STATUS RECORD - INVALID SEQ
*                                      NUMBER                        */
*                                                               01S0570
         B     @RC00559                                         01S0570
@RF00559 DS    0H                                               01S0571
*           /*********************************************************/
*           /*                                                       */
*           /* INVALID SEQ NUMBER FOUND IN THE STATUS RECORD ISSUE   */
*           /* MESSAGE OF INVALID CONDITION STATUS RECORD FOR PROBLEM*/
*           /* XXXXXXXX INVALID, SUBCOMMAND TERMINATED               */
*           /*                                                       */
*           /*********************************************************/
*                                                               01S0571
*           MSGPTR=ZERO;            /* INDICATE NO MSG CHAIN         */
         SLR   @14,@14                                          01S0571
         ST    @14,MSGPTR                                       01S0571
*           DO;                     /* BLSDMSG                  01S0572
*                                      (ZZ2,04068,MSGPTR,BLSDMSGS)
*                                      LIFO ROUTE(TERM)INSERT(FPPIDP,
*                                      CURPROB,PDRDSSEQ)             */
*             IOPTLIST(1)=ADDR(IOPTLIST(4));/* CHAIN TO NEXT INSERT  */
         LA    @10,IOPTLIST+12                                  01S0573
         ST    @10,IOPTLIST                                     01S0573
*             IOPTLIST(2)=ADDR(FPPIDP);/* INSERT NAME POINTER        */
         L     @10,ZZ2ZZ1P(,ZZ2PTR)                             01S0574
         L     @10,ZZ1FPP(,@10)                                 01S0574
         LA    @10,FPPIDP(,@10)                                 01S0574
         ST    @10,IOPTLIST+4                                   01S0574
*             IOPTLIST(3)=0;        /* ZERO RESERVED FIELD           */
         ST    @14,IOPTLIST+8                                   01S0575
*             RFY                                               01S0576
*               I015F BASED(ADDR(IOPTLIST(3)));/* ACCESS BYTES 1-2   */
*             I015F=LENGTH(FPPIDP); /* INSERT LENGTH                 */
         LA    @10,IOPTLIST+8                                   01S0577
         MVC   I015F(2,@10),@CH00093                            01S0577
*             IOPTLIST(4)=ADDR(IOPTLIST(7));/* CHAIN TO NEXT INSERT  */
         LA    @10,IOPTLIST+24                                  01S0578
         ST    @10,IOPTLIST+12                                  01S0578
*             IOPTLIST(5)=ADDR(CURPROB);/* INSERT NAME POINTER       */
         LA    @10,CURPROB                                      01S0579
         ST    @10,IOPTLIST+16                                  01S0579
*             IOPTLIST(6)=0;        /* ZERO RESERVED FIELD           */
         ST    @14,IOPTLIST+20                                  01S0580
*             RFY                                               01S0581
*               I015F BASED(ADDR(IOPTLIST(6)));/* ACCESS BYTES 1-2   */
*             I015F=LENGTH(CURPROB);/* INSERT LENGTH                 */
         LA    @10,IOPTLIST+20                                  01S0582
         MVC   I015F(2,@10),@CH00090                            01S0582
*             IOPTLIST(7)=0;        /* LAST INSERT                   */
         ST    @14,IOPTLIST+24                                  01S0583
*             IOPTLIST(8)=ADDR(PDRDSSEQ);/* INSERT NAME POINTER      */
         LA    @10,NEWSTPDR                                     01S0584
         LA    @10,PDRDSSEQ(,@10)                               01S0584
         ST    @10,IOPTLIST+28                                  01S0584
*             IOPTLIST(9)=0;        /* ZERO RESERVED FIELD           */
         ST    @14,IOPTLIST+32                                  01S0585
*             RFY                                               01S0586
*               I015F BASED(ADDR(IOPTLIST(9)));/* ACCESS BYTES 1-2   */
*             I015F=LENGTH(PDRDSSEQ);/* INSERT LENGTH                */
         LA    @14,IOPTLIST+32                                  01S0587
         MVC   I015F(2,@14),@CH00037                            01S0587
*             CALL BLSDMSG0(ZZ2,04068,MSGPTR,BLSDMSGS,'10010000'B,ADDR(
*                 IOPTLIST));       /* BUILD MESSAGE                 */
         ST    ZZ2PTR,@AL00001                                  01S0588
         LA    @14,@CF01221                                     01S0588
         ST    @14,@AL00001+4                                   01S0588
         LA    @14,MSGPTR                                       01S0588
         ST    @14,@AL00001+8                                   01S0588
         L     @14,ZZ2MSGSP(,ZZ2PTR)                            01S0588
         ST    @14,@AL00001+12                                  01S0588
         LA    @14,@CB01170                                     01S0588
         ST    @14,@AL00001+16                                  01S0588
         LA    @14,IOPTLIST                                     01S0588
         ST    @14,@AFTEMPS                                     01S0588
         LA    @14,@AFTEMPS                                     01S0588
         ST    @14,@AL00001+20                                  01S0588
         L     @15,ZZ2MSG0P(,ZZ2PTR)                            01S0588
         LA    @01,@AL00001                                     01S0588
         BALR  @14,@15                                          01S0588
*           END;                    /* BLSDMSG                  01S0589
*                                      (ZZ2,04068,MSGPTR,BLSDMSGS)
