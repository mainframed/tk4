*      (PDRPTR) RESTRICTED;                                     01S0242
*     PDRPTR=PDAPTR;                /* ->PDL                         */
         L     PDRPTR,PDAPTR                                    01S0243
*     GENERATE REFS(PD,PDRPTR) CODE(USING PD,PDRPTR);           01S0244
         USING PD,PDRPTR
*     IF PXTEST=PXTEST1 THEN                                    01S0245
         CLC   PXTEST(2),@CH00040                               01S0245
         BNE   @RF00245                                         01S0245
*       ZZ2AFX=ZZZ1;                /* TEST                          */
         OI    ZZ2AFX(ZZ2PTR),B'10000000'                       01S0246
*     ELSE                                                      01S0247
*       IF PXTEST=PXTEST0 THEN                                  01S0247
         B     @RC00245                                         01S0247
@RF00245 CLC   PXTEST(2),@CH00067                               01S0247
         BNE   @RF00247                                         01S0247
*         ZZ2AFX=ZZZ0;              /* NOTEST                        */
         NI    ZZ2AFX(ZZ2PTR),B'01111111'                       01S0248
*     IF PXCONF=PXCONF1 THEN                                    01S0249
@RF00247 DS    0H                                               01S0249
@RC00245 CLC   PXCONF(2),@CH00040                               01S0249
         BNE   @RF00249                                         01S0249
*       ZZ2AFC=ZZZ1;                /* CONFIRM                       */
         OI    ZZ2AFC(ZZ2PTR),B'10000000'                       01S0250
*     ELSE                                                      01S0251
*       IF PXCONF=PXCONF0 THEN                                  01S0251
         B     @RC00249                                         01S0251
@RF00249 CLC   PXCONF(2),@CH00067                               01S0251
         BNE   @RF00251                                         01S0251
*         ZZ2AFC=ZZZ0;              /* NOCONFIRM                     */
         NI    ZZ2AFC(ZZ2PTR),B'01111111'                       01S0252
*   END;                            /* BLSUPARS CONFIRMATION MF(E,PX)
*                                      PARSE ROUTINE CALLED          */
*                                                               01S0253
@RF00251 DS    0H                                               01S0253
@RC00249 DS    0H                                               01S0254
*   /*****************************************************************/
*   /*                                                               */
*   /* RETURNED FROM PARSE WITH GOOD LIST CONTINUE PROCESSING        */
*   /*                                                               */
*   /*****************************************************************/
