         OI    FGD,B'00001000'                                  01S0265
*         CALL BLSFGD00(ZZ2,DSDBSKEY,OLDBSDSD);/* REQ READ OF REC    */
         ST    ZZ2PTR,@AL00001                                  01S0266
         LA    @14,DSDBSKEY                                     01S0266
         ST    @14,@AL00001+4                                   01S0266
         LA    @14,OLDBSDSD                                     01S0266
         ST    @14,@AL00001+8                                   01S0266
         L     @10,ZZ2DVTP(,ZZ2PTR)                             01S0266
         L     @15,DVTGD00P(,@10)                               01S0266
         LA    @01,@AL00001                                     01S0266
         BALR  @14,@15                                          01S0266
*         RESPECIFY                                             01S0267
*           RF RESTRICTED;          /* SAVE REG 15                   */
*         FGDCODE=RF;               /* SAVE RETURN CODE FOR TEST AND
*                                      TRACK                         */
         ST    RF,FGDCODE                                       01S0268
*         IF RF<TWELVE THEN         /* TERMINATING ERROR?            */
         C     RF,@CF00032                                      01S0269
         BNL   @RF00269                                         01S0269
*           DO;                     /* NO, CONTINUE PROCESSING       */
*/************************************************************       */
*/*   THE RETURN CODE FROM BLSFGD00 IS USED IN THE CORRECTION        */
*/*   OF THE DATA BASE.                                              */
*/*   RETCODE = 0 - RECORD ALREADY EXISTS                            */
*/*                 (ERROR DURING LAST ADD PROB PROCESSING )         */
*/*   RETCODE = 8 - NO RECORD FOUND (NORMAL PROCESSING)              */
*/*   RETCODE = 12 - TERMINATING CONDITION                           */
*/************************************************************       */
*                                                               01S0271
*             IF RF=ZERO THEN       /* BASE RECORD IS THERE          */
         LTR   RF,RF                                            01S0271
         BNZ   @RF00271                                         01S0271
*               DO;                                             01S0272
*                                                               01S0272
*                 /***************************************************/
