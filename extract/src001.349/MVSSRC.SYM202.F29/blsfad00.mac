*                             LPARNF=BLANKS(1:1);/* MOVE IN BLANK
*                                      CHARACTER                     */
         MVC   LPARNF(1),BLANKS                                 01S0283
*                             RPARNF=BLANKS(1:1);/* MOVE IN BLANK
*                                      CHARACTER                     */
         MVC   RPARNF(1),BLANKS                                 01S0284
*                           END;    /* MEMBER NOT AVAILABLE          */
*                         ELSE      /* MEMBER IS AVAILABLE           */
*                           DO;     /* MEMBER HERE                   */
         B     @RC00281                                         01S0286
@RF00281 DS    0H                                               01S0287
*                             LPARNF=LPARN;/* MOVE IN ( CHAR         */
         MVI   LPARNF,C'('                                      01S0287
*                             RPARNF=RPARN;/* MOVE IN ) CHAR         */
         MVI   RPARNF,C')'                                      01S0288
*                           END;    /* END OF MEMBER HERE            */
*                         DO;       /* BLSDMSG                  01S0290
*                                      (ZZ2,04083,MSGPTR,BLSDMSGS)
