*                 FFP='1'B;         /* TRACK THE CALL TO BLSFFP00    */
         OI    FFP,B'00000100'                                  01S0273
*                 CALL BLSFFP00(ZZ2,CURDSN,CURMEM,CURPID,OLDPRDSD,
*                     NEWDSPDR);                                01S0274
         ST    ZZ2PTR,@AL00001                                  01S0274
         LA    @14,CURDSN                                       01S0274
         ST    @14,@AL00001+4                                   01S0274
         LA    @14,CURMEM                                       01S0274
         ST    @14,@AL00001+8                                   01S0274
         LA    @14,CURPID                                       01S0274
         ST    @14,@AL00001+12                                  01S0274
         LA    @14,OLDPRDSD                                     01S0274
         ST    @14,@AL00001+16                                  01S0274
         LA    @14,NEWDSPDR                                     01S0274
         ST    @14,@AL00001+20                                  01S0274
         L     @10,ZZ2DVTP(,ZZ2PTR)                             01S0274
         L     @15,DVTFP00P(,@10)                               01S0274
         LA    @01,@AL00001                                     01S0274
         BALR  @14,@15                                          01S0274
*                 FFPCODE=RF;       /* SAVE THE RETURN CODE FROM FFP */
         ST    RF,FFPCODE                                       01S0275
*/****************************************************************** */
*/*      RETURN FROM BLSFFP00 INDICATES WHICH RECORDS ARE THERE -    */
*/*                                                                  */
*/*        RETCODE = 0 DATA SET ALREADY ASSOCIATED WITH PROBLEM   -  */
*/*        RETCODE = 4 DSD PROB ASSOCIATION RECORD NOT FOUND -       */
*/*        RETCODE = 8 PDR DATA SET ASSOCIATION RECORD NOT FOUND     */
*/*        RETCODE = 12 TERMINATING ERROR                            */
*/*                                                                  */
*/****************************************************************** */
*                                                               01S0276
*                 IF RF<TWELVE THEN /* ERROR OCCURRED?               */
         C     RF,@CF00032                                      01S0276
         BNL   @RF00276                                         01S0276
*                   DO;             /* NO, CHECK RETURN CODES FROM
*                                      FFP                           */
*                                                               01S0277
*                     /***********************************************/
*                     /*                                             */
*                     /* RETURN CODE OF FOUR IF DATA SET ALREADY     */
*                     /* ASSOCIATED                                  */
*                     /*                                             */
*                     /***********************************************/
*                                                               01S0278
*                     IF RF=ZERO THEN/* DSN ALREADY ASSOCIATED?      */
         SLR   @14,@14                                          01S0278
         CR    RF,@14                                           01S0278
         BNE   @RF00278                                         01S0278
*                       DO;         /* DSN ALREADY ASSOCIATED        */
*                         MSGPTR=ZERO;/* NO MESSAGE CHAIN            */
         ST    @14,MSGPTR                                       01S0280
*                         IF CURMEM=BLANKS THEN/* IS THERE A MEMBER? */
         CLC   CURMEM(8),BLANKS                                 01S0281
         BNE   @RF00281                                         01S0281
