         LA    @10,OWNERWD                                         0707
         ST    @10,@AL00001+4                                      0707
         LA    @01,@AL00001                                        0707
         BAL   @14,CNTLIST                                         0707
*     END;                          /* OWNER(USERID...) WAS SPECIFIED*/
*   RETURN;                                                        0709
@EL00004 DS    0H                                                  0709
@EF00004 DS    0H                                                  0709
@ER00004 LM    @14,@05,@SA00004                                    0709
         LM    @07,@12,@SA00004+32                                 0709
