@DL00684 DS    0H                                                  0685
*     IF TSTPDE=SPECFIED            /* IS THIS KEYWORD SPECIFIED ?   */
*       THEN                                                       0685
         CLC   TSTPDE(2,TPPTR),@CH00040                            0685
         BNE   @RF00685                                            0685
*       VALUECT=VALUECT+SCLEN;      /* IT WILL TAKE ONE TABLE SLOT   */
         AL    VALUECT,@CF00051                                    0686
*     ELSE                                                         0687
*       ALLSPECD=OFF;               /* WE FOUND ONE THAT WAS NOT   0687
*                                      SPECIFIED                     */
         B     @RC00685                                            0687
@RF00685 NI    ALLSPECD,B'01111111'                                0687
*   END;                                                           0688
@RC00685 AL    TPPTR,@CF00067                                      0688
@DE00684 L     @10,@PC00003+4                                      0688
