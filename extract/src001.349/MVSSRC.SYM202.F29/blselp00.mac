*   SELVALP=ADDR(SCBVALS(1));       /* ADDRESS FIRST SLOT IN SCB     */
         LA    SELVALP,SCBVALS(,SCBPTR)                            0845
*   IF SEV0=ON                      /* WAS SEVERITY(0) SPECIFIED ?   */
*     THEN                                                         0846
         TM    SEV0,B'10000000'                                    0846
         BNO   @RF00846                                            0846
*     DO;                           /* SEVERITY(0) SPECIFIED         */
*       SELVAL='';                  /* BLANK SCB SLOT                */
