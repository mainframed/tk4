*     ROOTPDE FIXED(15),            /* ROOT PDE                      */
*     TSTPDE FIXED(15) BASED(TPPTR),/* PDE TO BE TESTED              */
*     TPPTR POINTER,                                               0681
*     ALLSPECD BIT(1);              /* FLAG INDICATES THAT ALL     0681
*                                      POSSIBLE VALUES WERE SPECIFIED
*                                      AND KEYWORD CAN BE IGNORED    */
*   ALLSPECD=ON;                    /* ASSUME ALL VALUES ARE       0682
*                                      SPECIFIED UNTIL WE FIND ONE 0682
*                                      THAT ISNT                     */
         OI    ALLSPECD,B'10000000'                                0682
*   VALUECT=ZERO;                   /* NO SPACE TAKEN UP SO FAR      */
*                                                                  0683
