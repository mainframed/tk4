         BNO   @RF00851                                            0851
*     DO;                           /* SEVERITY(1) SPECIFIED         */
*       SELVAL='1';                 /* STORE VALUE OF 1 IN SCB SLOT  */
         MVI   SELVAL+1(SELVALP),C' '                              0853
