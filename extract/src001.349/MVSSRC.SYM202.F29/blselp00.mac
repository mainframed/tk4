         MVI   SELVAL(SELVALP),C'1'                                0853
*       SELVALP=SELVALP+SCLEN;      /* ADDRESS NEXT SCB SLOT         */
         AL    SELVALP,@CF00051                                    0854
*     END;                          /* SEVERITY(1) SPECIFIED         */
*   IF SEV2=ON                      /* WAS SEVERITY(2) SPECIFIED ?   */
*     THEN                                                         0856
@RF00851 TM    SEV2,B'00100000'                                    0856
         BNO   @RF00856                                            0856
*     DO;                           /* SEVERITY(2) SPECIFIED         */
*       SELVAL='2';                 /* STORE VALUE OF 2 IN SLOT      */
         MVI   SELVAL+1(SELVALP),C' '                              0858
         MVC   SELVAL+2(14,SELVALP),SELVAL+1(SELVALP)              0858
         MVI   SELVAL(SELVALP),C'2'                                0858
*       SELVALP=SELVALP+SCLEN;      /* ADDRESS NEXT SCB SLOT         */
         AL    SELVALP,@CF00051                                    0859
*     END;                          /* SEVERITY(2) SPECIFIED         */
*   IF SEV3=ON                      /* WAS SEVERITY(3) SPECIFIED ?   */
*     THEN                                                         0861
@RF00856 TM    SEV3,B'00010000'                                    0861
         BNO   @RF00861                                            0861
*     DO;                           /* SEVERITY(3) SPECIFIED         */
*       SELVAL='3';                 /* STORE VALUE OF 3 IN SCB SLOT  */
         MVI   SELVAL+1(SELVALP),C' '                              0863
         MVC   SELVAL+2(14,SELVALP),SELVAL+1(SELVALP)              0863
         MVI   SELVAL(SELVALP),C'3'                                0863
*       SELVALP=SELVALP+SCLEN;      /* ADDRESS NEXT SCB SLOT         */
         AL    SELVALP,@CF00051                                    0864
*     END;                          /* SEVERITY(3) SPECIFIED         */
*   IF SEV4=ON                      /* WAS SEVERITY(4) SPECIFIED ?   */
*     THEN                                                         0866
@RF00861 TM    SEV4,B'00001000'                                    0866
         BNO   @RF00866                                            0866
*     DO;                           /* SEVERITY(4) SPECIFIED         */
*       SELVAL='4';                 /* STORE VALUE OF 4 IN SCB SLOT  */
         MVI   SELVAL+1(SELVALP),C' '                              0868
