         CLI   SEVI(@10),C'1'                                      0719
         BNE   @RF00719                                            0719
*           SEV1=ON;                /* MARK THE WALL                 */
         OI    SEV1,B'01000000'                                    0720
*         ELSE                                                     0721
*           IF SEVI(1)='2'          /* WAS IT 2 ?                    */
*             THEN                                                 0721
         B     @RC00719                                            0721
@RF00719 L     @10,SEVIP(,LISTP)                                   0721
         CLI   SEVI(@10),C'2'                                      0721
         BNE   @RF00721                                            0721
*             SEV2=ON;              /* MARK THE WALL                 */
         OI    SEV2,B'00100000'                                    0722
*           ELSE                                                   0723
*             IF SEVI(1)='3'        /* WAS IT 3 ?                    */
*               THEN                                               0723
         B     @RC00721                                            0723
@RF00721 L     @10,SEVIP(,LISTP)                                   0723
         CLI   SEVI(@10),C'3'                                      0723
         BNE   @RF00723                                            0723
*               SEV3=ON;            /* MARK THE WALL                 */
         OI    SEV3,B'00010000'                                    0724
*             ELSE                                                 0725
*               IF SEVI(1)='4'      /* WAS IT 4 ?                    */
*                 THEN                                             0725
         B     @RC00723                                            0725
@RF00723 L     @10,SEVIP(,LISTP)                                   0725
         CLI   SEVI(@10),C'4'                                      0725
         BNE   @RF00725                                            0725
*                 SEV4=ON;          /* MARK THE WALL                 */
         OI    SEV4,B'00001000'                                    0726
*               ELSE                                               0727
*                 IF SEVI(1)='0'    /* WAS IT 0 ?                    */
*                   THEN                                           0727
         B     @RC00725                                            0727
@RF00725 L     @10,SEVIP(,LISTP)                                   0727
         CLI   SEVI(@10),C'0'                                      0727
         BNE   @RF00727                                            0727
*                   SEV0=ON;        /* MARK THE WALL                 */
         OI    SEV0,B'10000000'                                    0728
*       END;                                                       0729
@RF00727 DS    0H                                                  0729
@RC00725 DS    0H                                                  0729
@RC00723 DS    0H                                                  0729
@RC00721 DS    0H                                                  0729
@RC00719 DS    0H                                                  0730
*     LISTP=GENLISTP;               /* MOVE ON TO THE NEXT PDE       */
@RF00716 L     LISTP,GENLISTP(,LISTP)                              0730
*   END;                            /* LOOP THROUGH SEVERITY PDES    */
@DE00715 CL    LISTP,@CF00062                                      0731
         BNE   @DL00715                                            0731
*   RESPECIFY                                                      0732
*     SEVX;                         /* REVERT BASING ON SEVERITY PDES*/
*   IF SEVFLG1^=ALLSEV              /* WERE ANY SEVERITIES MISSING ? */
*     THEN                                                         0733
         TM    SEVFLG1,B'11111000'                                 0733
         BO    @RF00733                                            0733
*     DO;                           /* SOME MISSING SO BUILD SCB     */
*       VALUECT=ZERO;               /* INITIALIZE TABLE SPACE COUNTER*/
         SLR   VALUECT,VALUECT                                     0735
*       IF SEV0=ON                  /* WAS SEVERITY(0) SPECIFIED ?   */
*         THEN                                                     0736
         TM    SEV0,B'10000000'                                    0736
         BNO   @RF00736                                            0736
*         VALUECT=VALUECT+SCLEN;                                   0737
         AL    VALUECT,@CF00051                                    0737
*       IF SEV1=ON                  /* WAS SEVERITY(1) SPECIFIED ?   */
*         THEN                                                     0738
@RF00736 TM    SEV1,B'01000000'                                    0738
         BNO   @RF00738                                            0738
*         VALUECT=VALUECT+SCLEN;                                   0739
         AL    VALUECT,@CF00051                                    0739
*       IF SEV2=ON                  /* WAS SEVERITY(2) SPECIFIED ?   */
*         THEN                                                     0740
@RF00738 TM    SEV2,B'00100000'                                    0740
         BNO   @RF00740                                            0740
*         VALUECT=VALUECT+SCLEN;                                   0741
         AL    VALUECT,@CF00051                                    0741
*       IF SEV3=ON                  /* WAS SEVERITY(3) SPECIFIED ?   */
*         THEN                                                     0742
@RF00740 TM    SEV3,B'00010000'                                    0742
         BNO   @RF00742                                            0742
*         VALUECT=VALUECT+SCLEN;                                   0743
         AL    VALUECT,@CF00051                                    0743
*       IF SEV4=ON                  /* WAS SEVERITY(4) SPECIFIED ?   */
*         THEN                                                     0744
@RF00742 TM    SEV4,B'00001000'                                    0744
         BNO   @RF00744                                            0744
*         VALUECT=VALUECT+SCLEN;                                   0745
         AL    VALUECT,@CF00051                                    0745
*       IF VALUECT^=ZERO            /* WERE ANY SPECIFIED ?          */
*         THEN                                                     0746
@RF00744 LTR   VALUECT,VALUECT                                     0746
         BZ    @RF00746                                            0746
*         COUNT=COUNT+VALUECT+LENGTH(BLSESCB);/* RESERVE SPACE       */
         LR    @10,COUNT                                           0747
         ALR   @10,VALUECT                                         0747
         AL    @10,@CF00051                                        0747
         LR    COUNT,@10                                           0747
*     END;                                                         0748
*   ELSE                                                           0749
*     SEVWD=UNSPECD;                /* ALL WERE SPECIFIED DONT BUILD
*                                      SCB                           */
         B     @RC00733                                            0749
@RF00733 SLR   @10,@10                                             0749
         STH   @10,SEVWD                                           0749
*   RETURN;                                                        0750
@EL00005 DS    0H                                                  0750
@EF00005 DS    0H                                                  0750
@ER00005 LM    @14,@05,12(@13)                                     0750
         LM    @07,@12,48(@13)                                     0750
         BR    @14                                                 0750
*   END;                                                           0751
         B     @EL00005                                            0751
         EJECT
*BLDLIST:                                                          0752
*   PROCEDURE(PDEB,OFFL,LENL);                                     0752
*                                                                  0752
BLDLIST  STM   @14,@05,12(@13)                                     0752
         STM   @07,@08,48(@13)                                     0752
         STM   @10,@12,60(@13)                                     0752
         MVC   @PC00006(12),0(@01)                                 0752
*   /*****************************************************************/
*   /*                                                               */
*   /* PROCEDURE TO BUILD SCB FOR LIST SELECTION CRITERIA. VALUES ARE*/
*   /* MOVED INTO SCB VALUE TABLE. THE TABLE IS THEN SORTED SINCE    */
*   /* LIST CAN BE INPUT IN ANY ORDER                                */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0753
*   DCL                                                            0753
*     PDEB CHARACTER(*),            /* FIRST PDE IN LIST             */
*     OFFL FIXED,                   /* OFFSET OF FIELD IN STATUS   0753
*                                      RECORD                        */
*     LENL FIXED;                   /* LENGTH OF FIELD IN STATUS   0753
*                                      RECORD                        */
*                                                                  0753
*   /*****************************************************************/
*   /*                                                               */
*   /* POINTERS USED FOR SORTING                                     */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0754
*   DCL                                                            0754
*     SORTTOP POINTER,              /* PTR TO LAST ELEMENT NOT KNOWN
*                                      TO BE IN POSITION             */
*     SORTBOT POINTER,              /* PTR TO JUST BEFORE TABLE, SORT
*                                      COMPLETE WHEN SORTTOP =     0754
*                                      SORTBOT                       */
*     SORTSTRT POINTER,             /* PTR TO FIRST ELEMENT OF TABLE */
*     SORTEND POINTER,              /* PTR TO ELEMENT BEFORE SORTTOP */
*     SORTT POINTER;                /* TEMPORARY USED TO DEVELOP NEXT
*                                      VALUE FOR SORTTOP             */
*   LISTP=ADDR(PDEB);               /* ADDRESS FIRST PDE             */
         L     LISTP,@PC00006                                      0755
*   SCBPTR=AVAILP;                  /* ADDRESS OF NEW SCB            */
         LR    SCBPTR,AVAILP                                       0756
*   SELVALP=ADDR(SCBVALS(1));       /* FIRST SELECTION VALUE ADDRESS */
*                                                                  0757
         LA    SELVALP,SCBVALS(,SCBPTR)                            0757
*   /*****************************************************************/
*   /*                                                               */
*   /* MOVE VALUES TO SCB                                            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0758
*   DO WHILE(LISTP^=ZZZPNULL);                                     0758
         B     @DE00758                                            0758
@DL00758 DS    0H                                                  0759
*     IF VALUEFP=ON                 /* IS VALUE PRESENT ?            */
*       THEN                                                       0759
         TM    VALUEFP(LISTP),B'10000000'                          0759
         BNO   @RF00759                                            0759
*       DO;                                                        0760
*         SELVAL=GENVAL(1:VALUEL);  /* MOVE IN VALUE                 */
         MVI   SELVAL+1(SELVALP),C' '                              0761
         MVC   SELVAL+2(14,SELVALP),SELVAL+1(SELVALP)              0761
         LH    @10,VALUEL(,LISTP)                                  0761
         BCTR  @10,0                                               0761
         L     @08,VALUEP(,LISTP)                                  0761
         EX    @10,@SM01446                                        0761
*         SELVALP=SELVALP+SCLEN;    /* BUMP SELVAL PTR               */
         AL    SELVALP,@CF00051                                    0762
*       END;                                                       0763
*     LISTP=GENLISTP;               /* ADDRESS OF NEXT PDE           */
@RF00759 L     LISTP,GENLISTP(,LISTP)                              0764
*   END;                                                           0765
@DE00758 CL    LISTP,@CF00062                                      0765
         BNE   @DL00758                                            0765
*   IF SELVALP>ADDR(SCBVALS(1))     /* WERE VALUES PRESENT ?         */
*     THEN                                                         0766
         LA    @10,SCBVALS(,SCBPTR)                                0766
         CR    SELVALP,@10                                         0766
         BNH   @RF00766                                            0766
*     DO;                           /* VALUES WERE PRESENT           */
*       SCBFLDOF=OFFL;              /* OFFSET TO SELECTION FIELD     */
         L     @08,@PC00006+4                                      0768
         L     @08,OFFL(,@08)                                      0768
         ST    @08,SCBFLDOF(,SCBPTR)                               0768
*       SCBFLDLN=LENL;              /* LENGTH OF SELECTION FIELD     */
         L     @08,@PC00006+8                                      0769
         L     @08,LENL(,@08)                                      0769
         ST    @08,SCBFLDLN(,SCBPTR)                               0769
*       SCBNXTSC=ZERO;              /* -> NEXT SCB                   */
         SLR   @08,@08                                             0770
         ST    @08,SCBNXTSC(,SCBPTR)                               0770
*       SCBLASTP=SELVALP-SCLEN;     /* -> LAST SELECTION CRITERION   */
         LA    @15,16                                              0771
         LR    @14,SELVALP                                         0771
         SLR   @14,@15                                             0771
         ST    @14,SCBLASTP(,SCBPTR)                               0771
*       AVAILP=SELVALP;             /* NEXT AVAIL SCB ADDR           */
         LR    AVAILP,SELVALP                                      0772
*       LINKP=SCBPTR;               /* LINK PTR                      */
         ST    SCBPTR,LINKP(,LINKPP)                               0773
*       LINKPP=ADDR(SCBNXTSC);      /* PTR TO LINK PTR               */
         LA    LINKPP,SCBNXTSC(,SCBPTR)                            0774
*       SCBNXTSC=ZERO;              /* THIS MAY BE THE LAST ONE      */
*                                                                  0775
         ST    @08,SCBNXTSC(,SCBPTR)                               0775
*       /*************************************************************/
*       /*                                                           */
*       /* SORT THE SELECTION VALUES USING BUBBLE SORT               */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0776
*       SORTSTRT=ADDR(SCBVALS(1));  /* SORTSTRT POINTS AT THE FIRST
*                                      TABLE ENTRY                   */
         LR    SORTSTRT,@10                                        0776
*       SORTBOT=SORTSTRT-SCLEN;     /* SORTBOT POINTS JUST BEFORE THE
*                                      TABLE                         */
         LCR   @15,@15                                             0777
         ALR   @15,SORTSTRT                                        0777
         LR    SORTBOT,@15                                         0777
*       SORTTOP=SCBLASTP;           /* AS FAR AS WE KNOW ALL OF THE
*                                      ENTRIES ARE OUT OF ORDER      */
         LR    SORTTOP,@14                                         0778
*       IF SORTTOP>SORTSTRT         /* ONLY SORT IF TWO OR MORE    0779
*                                      ENTRIES IN TABLE              */
*         THEN                                                     0779
         CR    SORTTOP,SORTSTRT                                    0779
         BNH   @RF00779                                            0779
*         DO WHILE(SORTTOP^=SORTBOT);/* LOOP UNTIL ALL ENTRIES ARE 0780
*                                      KNOWN TO BE IN ORDER          */
         B     @DE00780                                            0780
@DL00780 DS    0H                                                  0781
*           SORTT=SORTBOT;          /* UNTIL WE FIND AN ENTRY OUT OF
*                                      SEQUENCE ASSUME ALL ENTRIES 0781
*                                      ARE IN ORDER                  */
         LR    SORTT,SORTBOT                                       0781
*           SORTEND=SORTTOP-SCLEN;  /* SORTEND POINTS JUST BEFORE  0782
*                                      SORTTOP                       */
*                                                                  0782
         LR    SORTEND,SORTTOP                                     0782
         SL    SORTEND,@CF00051                                    0782
*           /*********************************************************/
*           /*                                                       */
*           /* LOOP LOOKING FOR ENTRIES OUT OF SEQUENCE              */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0783
*           DO SELVALP=SORTSTRT TO SORTEND BY SCLEN;               0783
         LR    SELVALP,SORTSTRT                                    0783
         B     @DE00783                                            0783
@DL00783 DS    0H                                                  0784
*             SELVALP2=SELVALP+SCLEN;/* POINT TO NEXT VALUE          */
         LA    SELVALP2,16                                         0784
         ALR   SELVALP2,SELVALP                                    0784
*             IF SELVAL>SELVAL2     /* ARE ADJACENT VALUES OUT OF  0785
*                                      SEQUENCE ?                    */
*               THEN                                               0785
         CLC   SELVAL(16,SELVALP),SELVAL2(SELVALP2)                0785
         BNH   @RF00785                                            0785
*               DO;                 /* ADJACENT VALUES ARE OUT OF  0786
*                                      SEQUENCE                      */
*                                                                  0786
*                 /***************************************************/
*                 /*                                                 */
*                 /* INTERCHANGE SELVAL AND SELVAL2                  */
*                 /*                                                 */
*                 /***************************************************/
*                                                                  0787
*                 SELVAL=SELVAL&&SELVAL2;                          0787
         XC    SELVAL(16,SELVALP),SELVAL2(SELVALP2)                0787
*                 SELVAL2=SELVAL&&SELVAL2;                         0788
         XC    SELVAL2(16,SELVALP2),SELVAL(SELVALP)                0788
*                 SELVAL=SELVAL&&SELVAL2;                          0789
         XC    SELVAL(16,SELVALP),SELVAL2(SELVALP2)                0789
*                 SORTT=SELVALP;    /* ANY VALUES UP TO HERE MIGHT BE
*                                      OUT OF SEQUENCE               */
         LR    SORTT,SELVALP                                       0790
*               END;                                               0791
*           END;                                                   0792
@RF00785 AL    SELVALP,@CF00051                                    0792
@DE00783 CR    SELVALP,SORTEND                                     0792
         BNH   @DL00783                                            0792
*           SORTTOP=SORTT;          /* SORTT POINTS TO LAST ENTRY NOT
*                                      KNOWN TO BE IN ORDER          */
         LR    SORTTOP,SORTT                                       0793
*         END;                                                     0794
@DE00780 CR    SORTTOP,SORTBOT                                     0794
         BNE   @DL00780                                            0794
*     END;                          /* VALUES WERE PRESENT           */
*   RETURN;                                                        0796
@EL00006 DS    0H                                                  0796
@EF00006 DS    0H                                                  0796
@ER00006 LM    @14,@05,12(@13)                                     0796
         LM    @07,@08,48(@13)                                     0796
         LM    @10,@12,60(@13)                                     0796
         BR    @14                                                 0796
*   END;                                                           0797
         B     @EL00006                                            0797
         EJECT
*BLDKEY:                                                           0798
*   PROCEDURE(PDE1BK,PDE2BK,TBL,OFFBK,LENBK);                      0798
*                                                                  0798
BLDKEY   STM   @14,@05,12(@13)                                     0798
         STM   @07,@08,48(@13)                                     0798
         STM   @10,@12,60(@13)                                     0798
         MVC   @PC00007(20),0(@01)                                 0798
*   /*****************************************************************/
*   /*                                                               */
*   /* PROCEDURE TO BUILD SCB FOR KEYWORD SELECTION CRITERIA. NO SORT*/
*   /* IS NEEDED SINCE THE VALUES ARE INSERTED IN PRE-SORTED ORDER   */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0799
*   DCL                                                            0799
*     PDE1BK FIXED(15),             /* PDE OF FIRST VALUE KEYWORD    */
*     PDE2BK FIXED(15),             /* PDE OF LAST VALUE KEYWORD     */
*     TBL CHARACTER(*),             /* TABLE OF STATUS RECORD VALUES,
*                                      COORELATED TO VALUE KEYWORD 0799
*                                      POSITION. THE VALUES IN THIS
*                                      TABLE MUST BE PRE-SORTED,   0799
*                                      HENCE THE REQUIREMENT THAT THE
*                                      KEYWORDS MUST BE SPECIFIED IN
*                                      THE PARSE PARAMETER LIST IN 0799
*                                      THE ORDER OF STATUS FIELD   0799
*                                      VALUE THEY REPRESENT          */
*     OFFBK FIXED,                  /* OFFSET OF STATUS RECORD FIELD */
*     LENBK FIXED,                  /* LENGTH OF STATUS RECORD FIELD */
*     PDEBK FIXED(15) BASED(PDEBKP),/* PDE BEING TESTED              */
*     PDEBKP POINTER,               /* -> PDEBK                      */
*     TBLENT CHARACTER(*) BASED(TBLENTP),/* ENTRY IN TBL           0799
*                                      COORESPONDING TO PDEBK        */
*     TBLENTP POINTER,              /* -> TBLENT                     */
*     LOCLEN FIXED;                 /* LOCAL COPY OF LOCLEN          */
*   SCBPTR=AVAILP;                  /* ADDRESS NEW SCB               */
         LR    SCBPTR,AVAILP                                       0800
*   LINKP=AVAILP;                   /* LINK IT IN                    */
         ST    AVAILP,LINKP(,LINKPP)                               0801
*   LINKPP=ADDR(SCBNXTSC);          /* PREPARE FOR NEXT LINK         */
         LA    LINKPP,SCBNXTSC(,SCBPTR)                            0802
*   SCBNXTSC=ZERO;                  /* THIS MAY BE THE LAST ONE      */
         SLR   @10,@10                                             0803
         ST    @10,SCBNXTSC(,SCBPTR)                               0803
*   SCBFLDOF=OFFBK;                 /* STORE FIELD OFFSET IN SCB     */
         L     @10,@PC00007+12                                     0804
         L     @10,OFFBK(,@10)                                     0804
         ST    @10,SCBFLDOF(,SCBPTR)                               0804
*   LOCLEN=LENBK;                   /* MAKE LOCAL COPY OF FIELD    0805
*                                      LENGTH                        */
         L     @10,@PC00007+16                                     0805
         L     LOCLEN,LENBK(,@10)                                  0805
*   SCBFLDLN=LOCLEN;                /* STORE FIELD LENGTH IN SCB     */
         ST    LOCLEN,SCBFLDLN(,SCBPTR)                            0806
*   SELVALP=ADDR(SCBVALS(1));       /* ADDR FIRST SLOT IN SCB TABLE  */
         LA    SELVALP,SCBVALS(,SCBPTR)                            0807
*   TBLENTP=ADDR(TBL);              /* FIRST KEYWORD COORESPONDS TO
*                                      FIRST ENTRY IN TBL            */
*                                                                  0808
         L     TBLENTP,@PC00007+8                                  0808
*   /*****************************************************************/
*   /*                                                               */
*   /* LOOP THROUGH KEYWORDS, FOR EACH SPECIFIED KEYWORD MOVE ITS    */
*   /* COORESPONDING STATUS RECORD VALUE INTO THE SCB TABLE          */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0809
*   DO PDEBKP=ADDR(PDE1BK) TO ADDR(PDE2BK) BY LENGTH(PDE1BK);      0809
         L     PDEBKP,@PC00007                                     0809
         B     @DE00809                                            0809
@DL00809 DS    0H                                                  0810
*     IF PDEBK=SPECFIED             /* WAS THIS KEYWORD SPECIFIED ?  */
*       THEN                                                       0810
         CLC   PDEBK(2,PDEBKP),@CH00040                            0810
         BNE   @RF00810                                            0810
*       DO;                         /* KEYWORD WAS SPECIFIED         */
*         SELVAL=TBLENT(1:LOCLEN);  /* MOVE VALUE TO SCB             */
         MVI   SELVAL+1(SELVALP),C' '                              0812
         MVC   SELVAL+2(14,SELVALP),SELVAL+1(SELVALP)              0812
         LR    @10,LOCLEN                                          0812
         BCTR  @10,0                                               0812
         EX    @10,@SM01448                                        0812
*         SELVALP=SELVALP+SCLEN;    /* ADDRESS NEXT SLOT IN SCB      */
         AL    SELVALP,@CF00051                                    0813
*       END;                        /* KEYWORD WAS SPECIFIED         */
*     TBLENTP=TBLENTP+LOCLEN;       /* MAINTAIN KEYWORD VALUE      0815
*                                      COORESPONDENCE                */
@RF00810 ALR   TBLENTP,LOCLEN                                      0815
*   END;                                                           0816
         AL    PDEBKP,@CF00067                                     0816
@DE00809 L     @10,@PC00007+4                                      0816
         CR    PDEBKP,@10                                          0816
         BNH   @DL00809                                            0816
*   AVAILP=SELVALP;                 /* ADDRESS OF NEXT SCB TO BE   0817
*                                      BUILT                         */
         LR    AVAILP,SELVALP                                      0817
*   SCBLASTP=SELVALP-SCLEN;         /* STORE ADDRESS OF LAST SCB   0818
*                                      TABLE ENTRY IN SCB            */
         LR    @10,SELVALP                                         0818
         SL    @10,@CF00051                                        0818
         ST    @10,SCBLASTP(,SCBPTR)                               0818
*   RETURN;                                                        0819
@EL00007 DS    0H                                                  0819
@EF00007 DS    0H                                                  0819
@ER00007 LM    @14,@05,12(@13)                                     0819
         LM    @07,@08,48(@13)                                     0819
         LM    @10,@12,60(@13)                                     0819
         BR    @14                                                 0819
*   END;                                                           0820
         EJECT
*BLDOWNR:                                                          0821
*   PROCEDURE;                                                     0821
*                                                                  0821
BLDOWNR  STM   @14,@05,@SA00008                                    0821
         STM   @07,@08,@SA00008+32                                 0821
         STM   @10,@12,@SA00008+40                                 0821
*   /*****************************************************************/
*   /*                                                               */
*   /* PROCEDURE TO BUILD SCB FOR OWNER SELECTION CRITERION. IF THE  */
*   /* OWNER KEYWORD WAS NOT SPECIFIED THEN AN SCB CONTAINING THE    */
*   /* USERID OF THE PERSON ISSUING THE SUBCOMMAND MUST BE BUILT,    */
*   /* OTHERWISE OWNER IS TREATED LIKE A LIST SELECTION CRITERIA     */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0822
*   IF OWNERWD^=OWNER               /* WAS OWNER KEYWORD SPECIFIED ? */
*     THEN                                                         0822
         CLC   OWNERWD(2),@CH00040                                 0822
         BE    @RF00822                                            0822
*     DO;                           /* BUILD SCB FOR CURRENT USERID  */
*       SCBPTR=AVAILP;              /* ADDRESS OF SCB                */
         LR    SCBPTR,AVAILP                                       0824
*       LINKP=AVAILP;               /* LINK SCB INTO CHAIN           */
         ST    AVAILP,LINKP(,LINKPP)                               0825
*       LINKPP=ADDR(SCBNXTSC);      /* PREPARE FOR THE NEXT SCB      */
         LA    LINKPP,SCBNXTSC(,SCBPTR)                            0826
*       SCBNXTSC=ZERO;              /* THIS MAY BE THE LAST ONE      */
         SLR   @10,@10                                             0827
         ST    @10,SCBNXTSC(,SCBPTR)                               0827
*       SCBFLDOF=OWNIDOFF;          /* STORE STATUS FIELD OFFSET IN
*                                      SCB                           */
         L     @10,OWNIDOFF                                        0828
         ST    @10,SCBFLDOF(,SCBPTR)                               0828
*       SCBFLDLN=LENGTH(PDROWNID);  /* STORE STATUS FIELD LENGTH IN
*                                      SCB                           */
         MVC   SCBFLDLN(4,SCBPTR),@CF00416                         0829
*       SELVALP=ADDR(SCBVALS(1));   /* ADDRESS FIRST SLOT IN SCB     */
         LA    SELVALP,SCBVALS(,SCBPTR)                            0830
*       SELVAL=PSCBUSER;            /* MOVE IN USERID                */
         MVI   SELVAL+7(SELVALP),C' '                              0831
         MVC   SELVAL+8(8,SELVALP),SELVAL+7(SELVALP)               0831
         L     @10,CPPLPSCB(,ZZ2PTR)                               0831
         MVC   SELVAL(7,SELVALP),PSCBUSER(@10)                     0831
*       SCBLASTP=SELVALP;           /* THE FIRST AND ONLY VALUE      */
         ST    SELVALP,SCBLASTP(,SCBPTR)                           0832
*       AVAILP=SELVALP+SCLEN;       /* NEXT AVAILABLE ADDR FOR SCB   */
         LA    AVAILP,16                                           0833
         ALR   AVAILP,SELVALP                                      0833
*     END;                          /* BUILD SCB FOR CURRENT USERID  */
*   ELSE                                                           0835
