WTIME    STIMER  WAIT,DINTVL=TENTHSEC
         DS    0H
*         END;
*         GOTO PURGE;                    /* PURGE REQUEST             *
         BCT   @7,@DO9FA                                          0105
         BC    15,PURGE                                           0106
