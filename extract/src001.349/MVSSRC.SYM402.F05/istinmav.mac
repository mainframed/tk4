         TITLE 'ISTINMAV - VS2 ALLOCATION/DEALLOCATION ROUTINE         *
                        '
ISTINMAV CSECT ,                                                   0001
ISTINCAV DS    0H                                                  0001
         ENTRY ISTINCAV
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL2(@EP00001-@MAINENT)
         DC    AL1(16)                                             0001
         DC    C'ISTINMAV  77.069'                                 0001
         DROP  @15
@PROLOG  ST    @14,12(,@13)                                        0001
         STM   @00,@12,20(@13)                                     0001
         BALR  @12,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@12                                         0001
         AH    @15,4(,@15)                                         0001
         BR    @15                                                 0001
@EP00001 DS    0H                                                  0002
*   REGSAVE=R13;                    /* SAVE PRT TO MY SAVEAREA       */
         LR    REGSAVE,R13                                         0681
*   R13=R13-(LENGTH(SAVAREA));      /* CONSTRUCT SAVEAREA FOR NEXT   */
         SL    R13,@CF04038                                        0682
*   BKPTR=REGSAVE;                  /* POINT SAVEAREA BACK           */
         ST    REGSAVE,BKPTR(,R13)                                 0683
*   FWDPTR=0;                       /* ZERO FORWARD PTR              */
         SLR   @11,@11                                             0684
         ST    @11,FWDPTR(,R13)                                    0684
*   REGSAVE->FWDPTR=R13;                                           0685
         ST    R13,FWDPTR(,REGSAVE)                                0685
*   NCSPLPTR=R1;                    /* SAVE PTR TO NCSPL             */
         ST    R1,NCSPLPTR                                         0686
*   RFY                                                            0687
*     R1 UNRSTD;                                                   0687
*   WORKAPTR=WORKA;                 /* SAVE PTR TO WORKAREA          */
         L     @11,NCSPLPTR                                        0688
         L     @15,NCSPLWKA(,@11)                                  0688
         L     WORKAPTR,WORKA(,@15)                                0688
*   WORKA=WORKAPTR+(LENGTH(WORKAREA));/* ADJUST PTR FOR MY WORKAREA  */
         LA    @08,120                                             0689
         ALR   @08,WORKAPTR                                        0689
         ST    @08,WORKA(,@15)                                     0689
*   RDTEPTR=NCSPLRAD;               /* SAVE PTR TO RDTE IN         0690
*                                      NON-VOLATILE REG              */
         L     RDTEPTR,NCSPLRAD(,@11)                              0690
*   RFY                                                            0691
*     ISTATCVT BASED(ATCVTPTR);     /* SET UP ADDRESSABILITY FOR   0691
*                                      ATCVT                         */
*   DO;                             /* LISATCVT                    0692
*                                      CVTBASE(PAGEABLE,(ATCVTPTR))  */
*     ATCVTPTR=ATCLCPTR;            /* SET UP ATCVT POINTER          */
         L     ATCVTPTR,ATCLCPTR                                   0693
*   END;                            /* LISATCVT                      */
*   LDPRMLST=LDPRMLST&&LDPRMLST;    /* ZERO LDPRMLST FOR LOAD DELETE */
         XC    LDPRMLST(28,WORKAPTR),LDPRMLST(WORKAPTR)            0695
*   LDPDCBA=ATCCONFT->CONDCBBA;     /* INITIALIZE DCB ADDRESS        */
         LA    @08,36                                              0696
         ALR   @08,WORKAPTR                                        0696
         L     @15,ATCCONFT(,ATCVTPTR)                             0696
         L     @00,CONDCBBA(,@15)                                  0696
         ST    @00,LDPDCBA(,@08)                                   0696
*   LDPFX='1'B;                     /* INITIALIZE FIX FLAG           */
         OI    LDPFX(@08),B'10000000'                              0697
*   LDPPDS='VTAMLIB';                                              0698
         MVI   LDPPDS+7(@08),C' '                                  0698
         MVC   LDPPDS(7,@08),@CC05532                              0698
*   RC=0;                           /* INITIALIZE RETURN CODE SWITCH */
         MVI   RC(WORKAPTR),X'00'                                  0699
*   IF RPRENTRY=RPRENTLN THEN                                      0700
         CLI   RPRENTRY(RDTEPTR),X'50'                             0700
         BE    @RT00700                                            0700
*     ;                             /* DON'T ALLOCATE PEP LINES    0701
*                                                            @ZM33418*/
*   ELSE                                                           0702
*     DO;                           /* ONLY ALLOCATE 370X'S, 3791'S
*                                      AND 3270'S @Z40GKD1,  @ZM33418*/
*                                                                  0702
*       /*************************************************************/
*       /*                                                           */
*       /* INITIALIZE POINTERS NECESSAY FOR ALLOCATION OR            */
*       /* DEALLOCATION USE CIDCTL TO GET NODE OF 370X OR LOCAL      */
*       /* DEVICE                                                    */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0703
*       RFY                                                        0703
*        (R0,                                                      0703
*         R1,                                                      0703
*         R2,                                                      0703
*         R15) RSTD;                                               0703
*       R1=NCSPLRPH;                /* RPH FOR CIDCTL                */
         L     @14,NCSPLPTR                                        0704
         L     R1,NCSPLRPH(,@14)                                   0704
*       R2=RPRCOMID;                /* COMMUNICATION ID FOR CIDCTL   */
         L     R2,RPRCOMID(,RDTEPTR)                               0705
*       DO;                         /* CIDCTL(FIND) LOCK(XCLUSIVE)   */
*         RFY                                                      0707
*          (EMCR2,                                                 0707
*           EMCR3) RSTD;                                           0707
*         GEN SETS(RPHWORK)                                        0708
*             (STM EMCR0,EMCR15,RPHWORK(RPHPTR) SAVE REGS IN RPH); 0708
         STM EMCR0,EMCR15,RPHWORK(RPHPTR) SAVE REGS IN RPH
*         EMCR3='00'X;              /* CLEAR EXTRA BITS              */
         SLR   EMCR3,EMCR3                                         0709
*         EMCR3='02'X;              /* INDICATE EXCLUSIVE LOCK       */
         LA    EMCR3,2                                             0710
*         CALL ATCLCPTR->ATCCDFND->ISTSDCCF;/* PROCESS REQUEST       */
         L     @08,ATCLCPTR                                        0711
         L     @15,ATCCDFND(,@08)                                  0711
         BALR  @14,@15                                             0711
*         RFY                                                      0712
*          (EMCR2,                                                 0712
*           EMCR3) UNRSTD;                                         0712
*       END;                        /* CIDCTL                        */
*       RFY                                                        0714
*         R1 UNRSTD;                                               0714
*       RFY                                                        0715
*         R2 UNRSTD;                                               0715
*       IF R15=0 THEN               /* TEST IF CIDCTL SUCCESSFUL     */
         LTR   R15,R15                                             0716
         BNZ   @RF00716                                            0716
*         DO;                                                      0717
*           RFY                                                    0718
*             R15 UNRSTD;                                          0718
*           NCBPTR=R0;              /* SAVE NODE PTR IN WORKAREA     */
         ST    R0,NCBPTR(,WORKAPTR)                                0719
*           RFY                                                    0720
*            (ISTNCB,                                              0720
*             ISTICNCB,                                            0720
*             ISTLDNCB) BASED(NCBPTR);                             0720
*           RFY                                                    0721
*             R0 UNRSTD;                                           0721
*           IF NCBTYPE=LDTYPE THEN  /* TEST FOR LOCAL DEVICE         */
         L     @08,NCBPTR(,WORKAPTR)                               0722
         CLI   NCBTYPE(@08),X'07'                                  0722
         BNE   @RF00722                                            0722
*             DO;                                                  0723
*               NCSPLUAD=LDNCUUA;   /* SAVE UNIT ADDR IN NCSPL       */
         L     @15,NCSPLPTR                                        0724
         MVC   NCSPLUAD(3,@15),LDNCUUA+1(@08)                      0724
*             END;                                                 0725
*           ELSE                                                   0726
*             DO;                                                  0726
         B     @RC00722                                            0726
@RF00722 DS    0H                                                  0727
*               IF NCBTYPE=ICTYPE THEN/* TEST FOR 370X               */
         L     @08,NCBPTR(,WORKAPTR)                               0727
         CLI   NCBTYPE(@08),X'06'                                  0727
         BNE   @RF00727                                            0727
*                 DO;                                              0728
*                   IF RPRENTRY=RPRENTRN&RRNRBREM='1'B THEN        0729
         CLI   RPRENTRY(RDTEPTR),X'01'                             0729
         BNE   @RF00729                                            0729
         TM    RRNRBREM(RDTEPTR),B'00000100'                       0729
         BO    @RT00729                                            0729
*                     ;                                            0730
*                                                                  0730
*                   /*************************************************/
*                   /*                                               */
*                   /* TEST FOR REMOTE 370X                  @Z40GKD1*/
*                   /*                                               */
*                   /*************************************************/
*                                                                  0731
*                   ELSE                                           0731
*                     DO;           /* TEST FOR LOCAL 370X OR 3791 0731
*                                                            @Z40GKD1*/
@RF00729 DS    0H                                                  0732
*                       IF(NCSPLUAD=0)×/* TEST FOR OVERRIDE ADDR     */
*                           (NCSPLVBF^=NCSVACT&NCSPLVBF^=NCSVALO)  0732
*                                                                  0732
*                       /*********************************************/
*                       /*                                           */
*                       /* OR DEALLOCATE @ZM33576,@ZM31576           */
*                       /*                                           */
*                       /*********************************************/
*                                                                  0732
*                         THEN                                     0732
         L     @08,NCSPLPTR                                        0732
         ICM   @00,7,NCSPLUAD(@08)                                 0732
         BZ    @RT00732                                            0732
         CLC   NCSPLVBF(2,@08),@CB01860                            0732
         BE    @RF00732                                            0732
         CLC   NCSPLVBF(2,@08),@CB01868                            0732
         BE    @RF00732                                            0732
@RT00732 DS    0H                                                  0733
*                         DO;                                      0733
*                           NCSPLUAD=ICNCUUA;/* SAVE UNIT ADDR IN  0734
*                                      NCSPL                         */
         L     @08,NCBPTR(,WORKAPTR)                               0734
         L     @15,NCSPLPTR                                        0734
         MVC   NCSPLUAD(3,@15),ICNCUUA+1(@08)                      0734
*                         END;                                     0735
*                       ELSE                                       0736
*                         ;         /* NULL                          */
@RF00732 DS    0H                                                  0737
*                     END;                                         0737
*                 END;                                             0738
*               ELSE                                               0739
*                 DO;               /* NOT 3270 OR 370X RELEASE CID  */
*                                                                  0739
         B     @RC00727                                            0739
@RF00727 DS    0H                                                  0740
*                   /*************************************************/
*                   /*                                               */
*                   /* CIDCTL (FINISH) LOCK(RELEASE)                 */
*                   /*                                               */
*                   /*************************************************/
*                                                                  0740
*                   R1=NCSPLRPH;    /* SET UP PARAMETERS FOR CIDCTL  */
         L     @08,NCSPLPTR                                        0740
         L     R1,NCSPLRPH(,@08)                                   0740
*                   RFY                                            0741
*                     R2 RSTD;                                     0741
*                   R2=RPRCOMID;                                   0742
         L     R2,RPRCOMID(,RDTEPTR)                               0742
*                   DO;             /* CIDCTL(FINISH) LOCK(RELEASE)  */
*                     RFY                                          0744
*                      (EMCR2,                                     0744
*                       EMCR3) RSTD;                               0744
*                     GEN SETS(RPHWORK)                            0745
*(STM EMCR0,EMCR15,RPHWORK(RPHPTR) SAVE REGS IN RPH);              0745
         STM EMCR0,EMCR15,RPHWORK(RPHPTR) SAVE REGS IN RPH
*                     EMCR3='00'X;  /* CLEAR EXTRA BITS              */
         SLR   EMCR3,EMCR3                                         0746
*                     CALL ATCLCPTR->ATCCDFIN->ISTSDCCR;/* PROCESS 0747
*                                      REQUEST                       */
         L     @08,ATCLCPTR                                        0747
         L     @15,ATCCDFIN(,@08)                                  0747
         BALR  @14,@15                                             0747
*                     RFY                                          0748
*                      (EMCR2,                                     0748
*                       EMCR3) UNRSTD;                             0748
*                   END;            /* CIDCTL                        */
*                   RFY                                            0750
*                     R2 UNRSTD;                                   0750
*                   RC=RCWCNTLB;    /* INVALID ENTRY TYPE    @Z40GKD1*/
         MVI   RC(WORKAPTR),X'1C'                                  0751
*                 END;                                             0752
*             END;                                                 0753
*         END;                                                     0754
*       ELSE                                                       0755
*         DO;                                                      0755
         B     @RC00716                                            0755
@RF00716 DS    0H                                                  0756
*           RC=RCNONCB;             /* NODE NOT OPENED       @Z40GKD1*/
         MVI   RC(WORKAPTR),X'2C'                                  0756
*         END;                                                     0757
*       IF RC=0 THEN                /* TEST FOR ERROR FROM INIT      */
@RC00716 CLI   RC(WORKAPTR),0                                      0758
         BNE   @RF00758                                            0758
*         DO;                                                      0759
*           IF RPRENTRY=RPRENTRN&RRNRBREM='1'B THEN/* FOR REMOTE RN
*                                      JUST INIT ICNCB       @ZM31611*/
         CLI   RPRENTRY(RDTEPTR),X'01'                             0760
         BNE   @RF00760                                            0760
         TM    RRNRBREM(RDTEPTR),B'00000100'                       0760
         BNO   @RF00760                                            0760
*             DO;                                                  0761
*               CALL ICINIT;        /* INITIALIZE ICNCB              */
         BAL   @14,ICINIT                                          0762
*             END;                                                 0763
*           ELSE                                                   0764
*             DO;                                                  0764
*                                                                  0764
         B     @RC00760                                            0764
@RF00760 DS    0H                                                  0765
*               /*****************************************************/
*               /*                                                   */
*               /* THIS SEGMENT PERFORMS THE FOLLOWING FUNCTIONS: 1) */
*               /* CHECKS THE UNIT ADDRESS FOR VALID EBCDIC NUMBERS: */
*               /* 0-9 (F0-F9), A-F (C1-C6). 2) CONVERTS AND PACKS   */
*               /* THE VALIDATED EBCDIC UNIT ADDRESS INTO            */
*               /* HEXADECIMAL. 3) FINDS THE ADDRESS OF THE UCB      */
*               /* ASSOCIATED WITH THE UNIT ADDRESS, IF ANY, BY USING*/
*               /* THE IOSGEN UCBLOOKUP MACRO. 4) CHECKS THE DEVICE  */
*               /* TYPE OF THE UCB TO ENSURE THAT IT CORRESPONDS TO  */
*               /* THE ENTRY TYPE IN THE RDT ENTRY.                  */
*               /*                                                   */
*               /*****************************************************/
*                                                                  0765
*               RFY                                                0765
*                 R14 RSTD;         /* IOSGEN MACRO DESTROYS R14     */
*               SAV14=R14;          /* SO SAVE R14                   */
         ST    R14,SAV14(,WORKAPTR)                                0766
*               R13=R13+12;         /* IOSGEN WILL DESTROY PTRS SO 0767
*                                      ADD                           */
*                                                                  0767
         AL    R13,@CF02186                                        0767
*               /*****************************************************/
*               /*                                                   */
*               /* VALIDATE UNIT ADDRESS                             */
*               /*                                                   */
*               /*****************************************************/
*                                                                  0768
*               WKNCSUAD=NCSPLUAD;  /* MOVE DEVICE ADDR INTO WORKAREA*/
         L     @15,NCSPLPTR                                        0768
         MVC   WKNCSUAD(3,WORKAPTR),NCSPLUAD(@15)                  0768
*               DO K=1 TO 3;        /* 3 CHARACTER UNIT ADDRESS      */
         LA    K,1                                                 0769
@DL00769 DS    0H                                                  0770
*                 IF(UAD(K)>239)&   /* F0-F9 VALID                   */
*                     (UAD(K)<250) THEN                            0770
         LR    @15,WORKAPTR                                        0770
         BCTR  @15,0                                               0770
         SLR   @08,@08                                             0770
         IC    @08,UAD(K,@15)                                      0770
         C     @08,@CF05541                                        0770
         BNH   @RF00770                                            0770
         C     @08,@CF05542                                        0770
         BNL   @RF00770                                            0770
*                   DO;                                            0771
*                     UAD(K)=UAD(K)-240;/* SUBTRACT F0               */
         SL    @08,@CF05543                                        0772
         LR    @15,WORKAPTR                                        0772
         BCTR  @15,0                                               0772
         STC   @08,UAD(K,@15)                                      0772
*                   END;                                           0773
*                 ELSE                                             0774
*                   DO;                                            0774
         B     @RC00770                                            0774
@RF00770 DS    0H                                                  0775
*                     IF(UAD(K)>192)&/* C1-C6 VALID                  */
*                         (UAD(K)<199) THEN                        0775
         LR    @15,WORKAPTR                                        0775
         BCTR  @15,0                                               0775
         SLR   @08,@08                                             0775
         IC    @08,UAD(K,@15)                                      0775
         C     @08,@CF05544                                        0775
         BNH   @RF00775                                            0775
         C     @08,@CF05545                                        0775
         BNL   @RF00775                                            0775
*                       DO;                                        0776
*                         UAD(K)=UAD(K)-183;/* SUBTRACT B7           */
         SL    @08,@CF05546                                        0777
         LR    @15,WORKAPTR                                        0777
         BCTR  @15,0                                               0777
         STC   @08,UAD(K,@15)                                      0777
*                       END;                                       0778
*                     ELSE                                         0779
*                       DO;                                        0779
         B     @RC00775                                            0779
@RF00775 DS    0H                                                  0780
*                         RC=RCINVCUA;/* INVALID DEVICE ADDR @Z40GKD1*/
         MVI   RC(WORKAPTR),X'04'                                  0780
*                       END;                                       0781
*                   END;                                           0782
@RC00775 DS    0H                                                  0783
*               END;                                               0783
@RC00770 AL    K,@CF00071                                          0783
         C     K,@CF00076                                          0783
         BNH   @DL00769                                            0783
*               IF RC=0 THEN        /* TEST FOR VALID DEVICE ADDRESS */
         CLI   RC(WORKAPTR),0                                      0784
         BNE   @RF00784                                            0784
*                 DO;                                              0785
*                   PACK(DEV16,WKNCSUAD);/* PACK HEX ADDRESS         */
         PACK  DEV16(2,WORKAPTR),WKNCSUAD(3,WORKAPTR)              0786
*                   RFY                                            0787
*                    (R6,                                          0787
*                     R7,                                          0787
*                     R15) RSTD;                                   0787
*                   R6=DEV16;       /* PUT IN REG 6 FOR IOSGEN MACRO */
         LH    R6,DEV16(,WORKAPTR)                                 0788
         N     R6,@CF05606                                         0788
*                   R6=R6/16;       /* SHIFT OUT SIGN IN LOW ORDER 4
*                                      BITS                          */
         SRL   R6,4                                                0789
*                   GEN REFS(CVTILK1,CVTILK2,CVTIXAVL,CVTMAP,IOCOMEX,
*                       CVTPTR,IOCOM,IOX,IOCUCBBM,IOXHICH);        0790
              IOSGEN UCBLOOK
*                   IF R15=0 THEN   /* TEST FOR UCB FOUND            */
         LTR   R15,R15                                             0791
         BNZ   @RF00791                                            0791
*                     DO;                                          0792
*                       RFY                                        0793
*                         R6 UNRSTD;                               0793
*                       RFY                                        0794
*                         R15 UNRSTD;                              0794
*                       UCBPTR=R7;  /* IOSGEN MACRO RETURNS UCB ADDR
*                                      IN REG 7                      */
         ST    R7,UCBPTR(,WORKAPTR)                                0795
*                       RFY                                        0796
*                         R7 UNRSTD;                               0796
*                                                                  0796
*                       /*********************************************/
*                       /*                                           */
*                       /* LINE ENTRY SHOULD HAVE UCB WITH           */
*                       /* COMMUNICATIONS FLAG ON LOCAL DEVICE ENTRY */
*                       /* SHOULD HAVE UCB WITH DISPLAY FLAG ON      */
*                       /*                                           */
*                       /*********************************************/
*                                                                  0797
*                       IF(RPRENTRY=RPRENTLC)&(UCB3DISP='1'B) THEN 0797
         CLI   RPRENTRY(RDTEPTR),X'45'                             0797
         BNE   @RF00797                                            0797
         L     @15,UCBPTR(,WORKAPTR)                               0797
         TM    UCB3DISP(@15),B'00010000'                           0797
         BNO   @RF00797                                            0797
*                         DO;                                      0798
*                                                                  0798
*                           /*****************************************/
*                           /*                                       */
*                           /* UCB WITH DEVICE FLAG ON SHOULD ALSO   */
*                           /* HAVE THE FOLLOWING VALUES IN UCBTYP   */
*                           /* BYTE 4: 09 - 3277 0A - 3284 0B - 3286 */
*                           /* 0C - 3158                             */
*                           /*                                       */
*                           /*****************************************/
*                                                                  0799
*                           IF(UCBTBYT4>'08'X)&(UCBTBYT4<'0D'X) THEN
         CLI   UCBTBYT4(@15),X'08'                                 0799
         BNH   @RF00799                                            0799
         CLI   UCBTBYT4(@15),X'0D'                                 0799
         BL    @RT00799                                            0799
*                             ;                                    0800
*                           ELSE                                   0801
*                             DO;                                  0801
@RF00799 DS    0H                                                  0802
*                               RC=RCWDTYPE;/* INVALID DEV TYPE    0802
*                                                            @Z40GKD1*/
         MVI   RC(WORKAPTR),X'0C'                                  0802
*                             END;                                 0803
*                         END;                                     0804
*                       ELSE                                       0805
*                         DO;                                      0805
*                                                                  0805
         B     @RC00797                                            0805
@RF00797 DS    0H                                                  0806
*                           /*****************************************/
*                           /*                                       */
*                           /* 370X ENTRY SHOULD HAVE UCB WITH       */
*                           /* COMMUNICATIONS FLAG ON                */
*                           /*                                       */
*                           /*****************************************/
*                                                                  0806
*                           IF(RPRENTRY=RPRENTRN)&(UCB3COMM='1'B) THEN
         CLI   RPRENTRY(RDTEPTR),X'01'                             0806
         BNE   @RF00806                                            0806
         L     @15,UCBPTR(,WORKAPTR)                               0806
         TM    UCB3COMM(@15),B'01000000'                           0806
         BNO   @RF00806                                            0806
*                             DO;                                  0807
*                                                                  0807
*                               /*************************************/
*                               /*                                   */
*                               /* VALID UCBTYP BYTE 4 CODES FOR 370X*/
*                               /* ARE: 15 - 3705 CHANNEL ADAPTOR    */
*                               /* TYPE 1 25 - 3705 CHANNEL ADAPTOR  */
*                               /* TYPE 2                            */
*                               /*                                   */
*                               /*************************************/
*                                                                  0808
*                               IF(UCBTBYT4='15'X)×(UCBTBYT4='25'X)
*                                 THEN                             0808
         CLI   UCBTBYT4(@15),X'15'                                 0808
         BE    @RT00808                                            0808
         CLI   UCBTBYT4(@15),X'25'                                 0808
         BE    @RT00808                                            0808
*                                 ;                                0809
*                               ELSE                               0810
*                                 DO;                              0810
*                                   RC=RCWDTYPE;/* INVALID DEVICE  0811
*                                      TYPE                  @Z40GKD1*/
         MVI   RC(WORKAPTR),X'0C'                                  0811
*                                 END;                             0812
*                             END;                                 0813
*                           ELSE                                   0814
*                             DO;                                  0814
*                                                                  0814
         B     @RC00806                                            0814
@RF00806 DS    0H                                                  0815
*/*   3791 SHOULD HAVE UCB WITH COMMUNICATIONS FLAG ON       @Z40GKD1*/
*                                                                  0815
*                               IF(RPRENTRY=RPRENTDA)&/*     @Z40GKD1*/
*                                   (UCB3COMM='1'B)&/*       @Z40GKD1*/
*                                   (UCBTBYT4='F1'X) THEN          0815
         CLI   RPRENTRY(RDTEPTR),X'51'                             0815
         BNE   @RF00815                                            0815
         L     @15,UCBPTR(,WORKAPTR)                               0815
         TM    UCB3COMM(@15),B'01000000'                           0815
         BNO   @RF00815                                            0815
         CLI   UCBTBYT4(@15),X'F1'                                 0815
         BE    @RT00815                                            0815
*                                 ; /*                       @Z40GKD1*/
*                               ELSE/*                       @Z40GKD1*/
*                                 DO;/*                      @Z40GKD1*/
@RF00815 DS    0H                                                  0818
*                                   RC=RCWDTYPE;/* INVALID DEV TYPE
*                                                            @Z40GKD1*/
         MVI   RC(WORKAPTR),X'0C'                                  0818
*                                 END;/*                     @Z40GKD1*/
*                             END;                                 0820
*                         END;                                     0821
*                     END;                                         0822
*                   ELSE                                           0823
*                     DO;                                          0823
         B     @RC00791                                            0823
@RF00791 DS    0H                                                  0824
*                       RC=RCNXDEV; /* UCB NOT FOUND         @Z40GKD1*/
         MVI   RC(WORKAPTR),X'08'                                  0824
*                     END;                                         0825
*                 END;                                             0826
*               ELSE                                               0827
*                 ;                 /* NULL                          */
@RF00784 DS    0H                                                  0828
*               R13=R13-12;         /* RESTORE SAVE AREA ADDR        */
@RC00784 SL    R13,@CF02186                                        0828
*               R14=SAV14;          /* RESTORE RETURN ADDR           */
         L     R14,SAV14(,WORKAPTR)                                0829
*               RFY                                                0830
*                 R14 UNRSTD;                                      0830
