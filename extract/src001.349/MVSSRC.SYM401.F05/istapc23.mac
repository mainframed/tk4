         CLI   CRXETYP(CRXPTR),X'00'                               0432
         BNE   @RF00432                                            0432
*             DO;                                                  0433
*               GEN(BAL   REG12,SPECREQ);/* BRANCH FOR SPECIFIC      */
         BAL   REG12,SPECREQ
*             END;                                                 0435
*           ELSE                                                   0436
*             IF CRXETYP=CRXANY     /* CHECK FOR ANY REQUEST         */
*               THEN                                               0436
         B     @RC00432                                            0436
@RF00432 CLI   CRXETYP(CRXPTR),X'04'                               0436
         BNE   @RF00436                                            0436
*               DO;                                                0437
*                 GEN(BAL   REG12,ANYREQ);/* BRANCH FOR ANY          */
         BAL   REG12,ANYREQ
*               END;                                               0439
*             ELSE                                                 0440
*               IF CRXETYP=CRXOPEN  /* CHECK FOR OPEN REQUEST        */
*                 THEN                                             0440
         B     @RC00436                                            0440
@RF00436 CLI   CRXETYP(CRXPTR),X'08'                               0440
         BNE   @RF00440                                            0440
*                 DO;                                              0441
*                   GEN(BAL   REG12,OPNDREQ);/* BRANCH FOR OPEN      */
         BAL   REG12,OPNDREQ
*                 END;                                             0443
*               ELSE                                               0444
*                 IF CRXETYP=CRXSC THEN/* SESSION CONTROL REQUEST    */
         B     @RC00440                                            0444
@RF00440 CLI   CRXETYP(CRXPTR),X'10'                               0444
         BNE   @RF00444                                            0444
*                   GEN CODE REFS(REG12,SCREQ) SETS(REG12) FLOWS(SCREQ)
*                       (BAL     REG12,SCREQ);                     0445
         BAL     REG12,SCREQ
*                 ELSE                                             0446
*                   DO;                                            0446
         B     @RC00444                                            0446
@RF00444 DS    0H                                                  0447
*                     GEN(BAL   REG12,TPPOSREQ);/* BRANCH FOR TPPOST */
         BAL   REG12,TPPOSREQ
*                   END;                                           0448
*         END;                      /* END PROCESS REQUEST   @Z40GKD1*/
*                                                                  0449
@RC00444 DS    0H                                                  0449
@RC00440 DS    0H                                                  0449
@RC00436 DS    0H                                                  0449
@RC00432 DS    0H                                                  0450
