         SLR   @09,@09                                             0483
         IC    @09,RPHPABOF(,GPR04P)                               0483
         ALR   GPR05P,@09                                          0483
*     GPR14P=0;                     /* LOAD 0 INTO REG 14            */
         SLR   GPR14P,GPR14P                                       0484
