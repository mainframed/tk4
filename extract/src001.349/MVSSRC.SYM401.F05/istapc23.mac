*       RFY                                                        0422
*         COMPREG RSTD;             /* RESTRICT COMPARE REG          */
*       COMPREG=MPSQEL;             /* OBTAIN MQL HEADER             */
         L     @09,PSTMPSTP(,@09)                                  0423
         L     COMPREG,MPSQEL(,@09)                                0423
*CSLOOP:                                                           0424
*       ;                                                          0424
CSLOOP   DS    0H                                                  0425
*       MQLCHAIN=COMPREG;           /* SET CHAIN FIELD IN MQL        */
         ST    COMPREG,MQLCHAIN(,MQLPTR)                           0425
*       CS(COMPREG,MQLPTR,MPSQEL);  /* SWAP MQL ON QUEUE             */
         L     @09,RPHTSKID(,REG1)                                 0426
         L     @09,PSTMPSTP(,@09)                                  0426
         CS    COMPREG,@06,MPSQEL(@09)                             0426
*       BC(FAIL,CSLOOP);            /* BRANCH FOR SWAP UNSUCCESSFUL  */
         BC    7,CSLOOP                                            0427
*       RFY                                                        0428
*        (COMPREG,                                                 0428
*         MQLPTR) UNRSTD;           /* RELEASE COMPARE REG           */
*       CRXEMQL=ON;                 /* INDICATE SWAP DONE            */
*                                                                  0429
         OI    CRXEMQL(CRXPTR),B'10000000'                         0429
*       /*************************************************************/
*       /*                                                           */
*       /* CHECK FOR ABEND OR CLOSE IN PROGRESS              @Z40GKD1*/
*       /*                                                           */
*       /*************************************************************/
*                                                                  0430
*       IF(ACDABEND=OFF)&(ACDCACB=OFF) THEN                        0430
         TM    ACDABEND(DEBPTR),B'11000000'                        0430
         BNZ   @RF00430                                            0430
