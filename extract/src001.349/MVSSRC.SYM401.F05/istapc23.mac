         BC    8,FMZT0016                                          0494
*       END;                        /* IF POST FLAG IS ON, FALL THRU
*                                      BOTTOM OF LOOP                */
*     ELSE                                                         0496
*       ;                           /* WT/EXT CONTENTION FLAG OFF    */
@RF00488 DS    0H                                                  0497
*     IF GPR04P->RPHTSKID=GPR01P->RPHTSKID THEN                    0497
         CLC   RPHTSKID(4,GPR04P),RPHTSKID(GPR01P)                 0497
         BNE   @RF00497                                            0497
*       DO;                                                        0498
*         GPR05P->PABCHNGP=GPR01P->RPHPABQP;/* PUT PAB QUEUE ADDRESS
