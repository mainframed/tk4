*       LM(REG1,BASEREG,CRXSAVEA);  /* RESTORE REGS                  */
         LM    REG1,BASEREG,CRXSAVEA(CRXPTR)                       0403
*       RFY                                                        0404
*        (ECBCODE,                                                 0404
*         SAVEREGS,                                                0404
*         SRBECB) UNRSTD;                                          0404
