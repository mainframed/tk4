*    KEYSPEC(1)='1'B;                  /* INDICATE KEYWORD SPECIFIED */
         OI    KEYSPEC,B'10000000'                              02.0442
*  END;                                                         02.0443
*ELSE                                                           02.0444
*  KEYRTNCD=4;                         /* KEYWORD LENGTH INCORRECT   */
         B     @RC00437                                         02.0444
@RF00437 LA    KEYRTNCD,4                                       02.0444
