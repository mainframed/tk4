*    ELSE                                                       02.0423
*      IF PARMVALU(1)='2' THEN         /* IS TEMP D.S. INFO     02.0423
*                                         WANTED                     */
         B     @RC00421                                         02.0423
@RF00421 CLI   PARMVALU(KEYPTR),C'2'                            02.0423
         BNE   @RF00423                                         02.0423
*        SMCATDS='1'B;                 /* YES, REC TYPE 17 SHOULD
*                                         BE ISSUED                  */
         OI    SMCATDS(SMCAPTR),B'00000010'                     02.0424
*      ELSE                                                     02.0425
