*/********************************************************************/
*/*                                                                  */
*/*SEGMENT NAME= WRITEMSG                                            */
*/*                                                                  */
*/*DESCRIPTIVE NAME= WRITER MESSAGE PROCESSOR                        */
*/*                                                                  */
*/*FUNCTION=                                                         */
*/*  CONSTRUCT AND ISSUE A MESSAGE TO THE OPERATOR WHEN REQUESTED    */
*/*  BY THE CALLER.                                                  */
*/*                                                                  */
*/*  OPERATION= NO SPECIAL ALGORITHMS ARE USED                       */
*/*                                                                  */
*/*ENTRY POINT= WRITEMSG                                             */
*/*  INPUT=                                                          */
*/*    MSGSWS- CONTAINS BIT INDICATING WHICH MESSAGE TO ISSUE        */
*/*    SMCAPDEV- VOLUME SERIAL NUMBER                                */
*/*    SMCAXORY- EBCDIC 'X' OR 'Y'                                   */
*/*    MESSAGE TEXT FROM IEEMB828                                    */
*/*                                                                  */
*/*EXITS=                                                            */
*/*  CONDITION= NORMAL- MESSAGE WRITTEN TO OPERATOR                  */
*/*  OUTPUT=                                                         */
*/*    MESSAGE:                                                      */
*/*    IEE360I SMF NOW RECORDING ON SYS1.MANX/Y ON XXXXXX TIME=      */
*/*    IEE361I SMF DATA LOST - SYS1.MANX/Y NOT AVAILABLE TIME=       */
*/*    IEE362A SMF ENTER DUMP FOR SYS1.MANX/Y ON XXXXXX              */
*/*    IEE364I SMF I/O ERROR ON XXXXXX                               */
*/*  RETURN CODES= NONE                                              */
*/*                                                                  */
*/********************************************************************/
*MSGTIME(3:3)=CONCOLON;                /* INITIALIZE TIME            */
         MVC   MSGTIME+2(1),CONCOLON                               0338
*MSGTIME(6:6)=CONCOLON;                /* RECEIVING AREA             */
         MVC   MSGTIME+5(1),CONCOLON                               0339
*IF RECGMSG='1'B  LOSTMSG='1'B THEN   /* THIS MSG REQUIRE TIME?     */
         TM    RECGMSG,B'00110000'                                 0340
         BZ    @RF00340                                            0340
*  DO;                                 /* YES, OBTAIN TIME FOR MSG   */
*    RESPECIFY (REG0,REG1) RESTRICTED;                             0342
*    GEN(TIME  DEC);                                               0343
         TIME  DEC
*    TIMEPK=REG0;                      /* TIME IN PACKED FORMAT      */
         ST    REG0,TIMEPK                                         0344
*    RESPECIFY (REG0,REG1) UNRESTRICTED;                           0345
*    UNPK(TIME,TIMEPK);                /* UNPACK TIME                */
         UNPK  TIME(7),TIMEPK(4)                                   0346
*    MSGTIME(1:2)=HOURS;               /* HOURS TO  HH:XX:XX         */
         MVC   MSGTIME(2),HOURS                                    0347
*    MSGTIME(4:5)=MINS;                /* MINUTES TO  XX:MM:XX       */
         MVC   MSGTIME+3(2),MINS                                   0348
*    MSGTIME(7:8)=SECS;                /* SECONDS TO  XX:XX:SS       */
         MVC   MSGTIME+6(2),SECS                                   0349
*    IF RECGMSG='1'B THEN              /* ISSUE MSG IEE360I?         */
         TM    RECGMSG,B'00010000'                                 0350
         BNO   @RF00350                                            0350
*      DO;                             /* YES                        */
*        MSGBUF(1:LEN360I+4)=I360I;    /* MSG TEXT TO BUFFER         */
         L     @10,@CV00151                                        0352
         LA    @04,3                                               0352
         AH    @04,LEN360I(,@10)                                   0352
         L     @10,@CV00151                                        0352
         EX    @04,@SM02260                                        0352
*        MSGBUF(42:42)=SMCAXORY;       /* EBCDIC X OR Y FOR MANX/Y   */
         MVC   MSGBUF+41(1),SMCAXORY(SMCAPTR)                      0353
*        MSGBUF(47:52)=SMCAPDEV;       /* VOLUME SERIAL TO MSG       */
         MVC   MSGBUF+46(6),SMCAPDEV(SMCAPTR)                      0354
*        MSGBUF(59:66)=MSGTIME;        /* TIME TO MSG                */
         MVC   MSGBUF+58(8),MSGTIME                                0355
*        RFY REG1 RSTD;                /* NEEDED FOR DOM MACRO     0356
*                                                            @ZA07764*/
*        IF SMCAXORY=CONX THEN         /* IF MANX BEING USED @ZA07764*/
         CLC   SMCAXORY(1,SMCAPTR),CONX                            0357
         BNE   @RF00357                                            0357
*           REG1=SMCADOMX;             /* GET WTO ID TO DOM  @ZA07764*/
         L     REG1,SMCADOMX(,SMCAPTR)                             0358
*        ELSE                          /* GET MANY           @ZA07764*/
*           REG1=SMCADOMY;             /* DOM ID             @ZA07764*/
         B     @RC00357                                            0359
@RF00357 L     REG1,SMCADOMY(,SMCAPTR)                             0359
*        GEN (DOM MSG=(1));            /* DOM DUMP WTO       @ZA07764*/
@RC00357 DS    0H                                                  0360
         DOM MSG=(1)
*        RFY REG1 UNRSTD;              /* FREE REGISTER      @ZA07764*/
*      END;                                                        0362
*    ELSE                                                          0363
*      DO;                             /* MSG IS IEE361I             */
         B     @RC00350                                            0363
@RF00350 DS    0H                                                  0364
*        MSGBUF(1:LEN361I+4)=I361I;    /* MSG TEXT TO BUFFER         */
         L     @10,@CV00152                                        0364
         LA    @04,3                                               0364
         AH    @04,LEN361I(,@10)                                   0364
         L     @10,@CV00152                                        0364
         EX    @04,@SM02262                                        0364
*        MSGBUF(37:37)=SMCAXORY;       /* EBCDIC X OR Y FOR MANX/Y   */
         MVC   MSGBUF+36(1),SMCAXORY(SMCAPTR)                      0365
*        MSGBUF(58:65)=MSGTIME;        /* TIME TO MSG                */
         MVC   MSGBUF+57(8),MSGTIME                                0366
*      END;                                                        0367
*  END;                                                            0368
*ELSE                                                              0369
*  DO;                                 /* MSGS DO NOT NEED TIME      */
         B     @RC00340                                            0369
@RF00340 DS    0H                                                  0370
*    IF DUMPMSG='1'B THEN              /* ISSUE MSG IEE362A?         */
         TM    DUMPMSG,B'10000000'                                 0370
         BNO   @RF00370                                            0370
*      DO;                             /* YES                        */
*        MSGBUF(1:LEN362A+4)=I362A;    /* MSG TEXT TO BUFFER         */
         L     @10,@CV00153                                        0372
         LA    @04,3                                               0372
         AH    @04,LEN362A(,@10)                                   0372
         L     @10,@CV00153                                        0372
         EX    @04,@SM02264                                        0372
*        MSGBUF(40:40)=SMCAYORX;       /* FOR THE ALT MAN DATA SET   */
         MVC   MSGBUF+39(1),SMCAYORX(SMCAPTR)                      0373
*        MSGBUF(45:50)=SMCAADEV;       /* VOLUME SERIAL TO MSG       */
         MVC   MSGBUF+44(6),SMCAADEV(SMCAPTR)                      0374
*      END;                                                        0375
*    ELSE                                                          0376
*      DO;                             /* MSG IEE364I                */
         B     @RC00370                                            0376
@RF00370 DS    0H                                                  0377
*        MSGBUF(1:LEN364I+4)=I364I;    /* MSG TEXT TO BUFFER         */
         L     @10,@CV00154                                        0377
         LA    @04,3                                               0377
         AH    @04,LEN364I(,@10)                                   0377
         L     @10,@CV00154                                        0377
         EX    @04,@SM02266                                        0377
*        MSGBUF(30:35)=SMCAPDEV;       /* VOLUME SERIAL TO MSG       */
         MVC   MSGBUF+29(6),SMCAPDEV(SMCAPTR)                      0378
*      END;                                                        0379
*  END;                                                            0380
@RC00370 DS    0H                                                  0381
*GEN(WTO   ,MF=(E,MSGBUF));            /* WRITE MSG TO OPERATOR      */
@RC00340 DS    0H                                                  0381
         WTO   ,MF=(E,MSGBUF)
*IF DUMPMSG='1'B THEN                  /* IF MSG IEE362A     @ZA07764*/
         TM    DUMPMSG,B'10000000'                                 0382
         BNO   @RF00382                                            0382
*  DO;                                 /* WAS ISSUED THEN    @ZA07764*/
*    RFY REG1 RSTD;                    /* SAVE WTO MSG ID    @ZA07764*/
*    IF SMCAYORX=CONX THEN             /* IF MANX MSG        @ZA07764*/
         CLC   SMCAYORX(1,SMCAPTR),CONX                            0385
         BNE   @RF00385                                            0385
*       SMCADOMX=REG1;                 /* SAVE HERE          @ZA07764*/
         ST    REG1,SMCADOMX(,SMCAPTR)                             0386
*    ELSE                              /* ELSE SAVE MANY     @ZA07764*/
*       SMCADOMY=REG1;                 /* MSG ID HERE        @ZA07764*/
         B     @RC00385                                            0387
@RF00385 ST    REG1,SMCADOMY(,SMCAPTR)                             0387
*    RFY REG1 UNRSTD;                  /* IFASMFDP WILL ASSOCIATE  0388
*                                                            @ZA07764*/
@RC00385 DS    0H                                                  0389
*  END;                                /* CORRECT ID TO VOLID@ZA07764*/
*MSGSWS=MSGSWS && MSGSWS;              /* CLEAR MSG SWITCHES         */
@RF00382 XC    MSGSWS(1),MSGSWS                                    0390
*END WRITEMSG;                                                     0391
@EL00002 DS    0H                                                  0391
@EF00002 DS    0H                                                  0391
@ER00002 LM    @14,@12,@SA00002                                    0391
         BR    @14                                                 0391
*BLKCALC:PROC;                                                     0392
BLKCALC  STM   @14,@12,12(@13)                                     0392
*/********************************************************************/
*/*                                                                  */
*/*SEGMENT NAME= BLKCALC                                             */
*/*                                                                  */
*/*DESCRIPTIVE NAME= CALCULATE RECORDING DEVICE OVERHEAD             */
*/*                                                                  */
*/*FUNCTION=                                                         */
*/*  CALCULATE THE FOLLOWING THREE VALUES FOR EACH OPENED SMF DATA   */
*/*  SET FOR USE IN THE SPACE CHECKING SEGMENTS:                     */
*/*  1- SIZE OF A BUFFER + THE OVERHEAD IF THE LAST BLOCK ON A TRACK */
*/*  2- SIZE OF A BUFFER + THE OVERHEAD IF NOT LAST BLOCK ON A TRACK */
*/*  3- NUMBER OF BUFFER SIZE BLOCKS(PLUS OVERHEAD) THAT CAN BE      */
*/*     WRITTEN ON ONE TRACK OF THIS DEVICE                          */
*/*                                                                  */
*/*  OPERATION=                                                      */
*/*    THE DEVICE ENTRY FOR THE SMF DATA SET IN THE DEVICE           */
*/*    CHARACTERISTICS TABLE PROVIDES INPUT TO CALCULATE THE ABOVE   */
*/*    VALUES.                                                       */
*/*    FIRST, OVERHEAD VALUES FOR NON-KEYED BLOCKS ARE CALCULATED-   */
*/*    ONE FOR THE LAST BLOCK ON A TRACK AND ONE FOR NOT LAST BLOCK  */
*/*    ON A TRACK. THE APPROPRIATE VALUE ADDED TO THE SMF BUFFER SIZE*/
*/*    GIVES THE BLOCK + OVERHEAD VALUE FOR A BLOCK BEING THE LAST   */
*/*    BLOCK ON A TRACK.                                             */
*/*    IF THE DEVICE ENTRY INDICATES THAT NO TOLERANCE FACTOR NEED   */
*/*    BE APPLIED, THEN THE BLOCK + OVERHEAD SIZE IS OBTAINED BY     */
*/*    ADDING THE APPROPRIATE OVERHEAD VALUE TO THE SMF BUFFER SIZE. */
*/*    IF A TOLERANCE FACTOR MUST BE APPLIED, THE BLOCK + OVERHEAD   */
*/*    VALUE FOR A BLOCK THAT IS NOT THE LAST ON A TRACK IS CALCU-   */
*/*    LATED AS FOLLOWS:                                             */
*/*    BLOCKSIZE NOT LAST = SMF BUFFER SIZE                          */
*/*                         TIMES                                    */
*/*                         DEV TOLERANCE FACTOR                     */
*/*                         DIVIDED BY 512                           */
*/*                         PLUS                                     */
*/*                         OVERHEAD VALUE FOR NOT LAST BLOCK        */
*/*    THEN THE NUMBER OF BLOCKS THAT FIT ON A TRACK IS COMPUTED FOR */
*/*    USE BY THE SPACE CHECK SEGMENTS.                              */
*/*                                                                  */
*/*ENTRY POINT= BLKCALC                                              */
*/*  INPUT=                                                          */
*/*    DEVICE CHARACTERISTICS TABLE ENTRY FOR THE SMF RECORDING DEV  */
*/*    SMCABSIZ- SMF BUFFER SIZE                                     */
*/*                                                                  */
*/*EXITS=                                                            */
*/*  CONDITION= NORMAL- SPACE CHECK INPUT CONSTANTS CALCULATED       */
*/*  OUTPUT=                                                         */
*/*    BLKSIZNL- BUFFER SIZE + OVERHEAD IF NOT LAST BLOCK ON A TRACK */
*/*    BLKSILST- BUFFER SIZE + OVERHEAD IF THE LAST BLOCK ON A TRACK */
*/*    BLKONTRK- NO. OF BUFFER SIZE BLOCKS(PLUS OVERHEAD) THAT CAN   */
*/*              BE WRITTEN ON ONE TRACK OF THIS DEVICE.             */
*/*  RETURN CODES= NONE                                              */
*/*                                                                  */
*/********************************************************************/
*RESPECIFY DEVTABLE BASED(DCBDVTBL);                               0393
*IF DEVADDRH='1'B THEN                 /* THIS DEV ENTRY HAVE HALF   */
         L     @10,SMCAPDCB(,SMCAPTR)                              0394
         LA    @10,DCBFDAD(,@10)                                   0394
         L     @10,DCBDVTBL+7(,@10)                                0394
         TM    DEVADDRH(@10),B'00001000'                           0394
         BNO   @RF00394                                            0394
*  DO;                                 /* WORD OVERHEAD CONSTANT?    */
*    BLKOVRHD=OVERHEAD-DEVOVHDK;       /* YES, CALC OVERHEAD FOR BLKS*/
         LH    BLKOVRHD,OVERHEAD+6(,@10)                           0396
         SLR   @04,@04                                             0396
         IC    @04,DEVOVHDK(,@10)                                  0396
         SLR   BLKOVRHD,@04                                        0396
*    BLKOVLST=BLKOVRHD;                /* NOT LAST ON TRK AND FOR    */
         LR    BLKOVLST,BLKOVRHD                                   0397
*  END;                                /* LAST BLK ON A TRK          */
*ELSE                                  /* NO, OVERHEAD CONSTANTS     */
*  DO;                                 /* ARE BYTE VALUES            */
         B     @RC00394                                            0399
@RF00394 DS    0H                                                  0400
*    BLKOVRHD=DEVOVHDI-DEVOVHDK;       /* OVERHEAD NOT LAST BLK      */
         L     @10,SMCAPDCB(,SMCAPTR)                              0400
         LA    @10,DCBFDAD(,@10)                                   0400
         L     @10,DCBDVTBL+7(,@10)                                0400
         SLR   @04,@04                                             0400
         IC    @04,DEVOVHDK(,@10)                                  0400
         SLR   BLKOVRHD,BLKOVRHD                                   0400
         IC    BLKOVRHD,DEVOVHDI(,@10)                             0400
         SLR   BLKOVRHD,@04                                        0400
*    BLKOVLST=DEVOVHDL-DEVOVHDK;       /* OVERHEAD LAST BLK ON TRK   */
         LCR   @04,@04                                             0401
         SLR   BLKOVLST,BLKOVLST                                   0401
         IC    BLKOVLST,DEVOVHDL(,@10)                             0401
         ALR   @04,BLKOVLST                                        0401
         LR    BLKOVLST,@04                                        0401
*  END;                                                            0402
*BLKSILST=SMCABSIZ+BLKOVLST;           /* BLKSIZE- LAST BLK ON A TRK */
@RC00394 L     @10,SMCABSIZ(,SMCAPTR)                              0403
         LR    @04,BLKOVLST                                        0403
         ALR   @04,@10                                             0403
         STH   @04,BLKSILST                                        0403
*IF DEVTOLFG='1'B THEN                 /* TOLERANCE FACTOR TO APPLY?
*                                         YES- CALC BLKSIZE NOT LAST */
         L     @07,SMCAPDCB(,SMCAPTR)                              0404
         LA    @07,DCBFDAD(,@07)                                   0404
         L     @07,DCBDVTBL+7(,@07)                                0404
         TM    DEVTOLFG(@07),B'00000001'                           0404
         BNO   @RF00404                                            0404
*  BLKSIZNL=(SMCABSIZ*DEVTOLER/512)+BLKOVRHD;    /* ON A TRACK       */
         LR    @01,@10                                             0405
         LH    @10,DEVTOLER(,@07)                                  0405
         MR    @00,@10                                             0405
         D     @00,@CF02213                                        0405
         ALR   @01,BLKOVRHD                                        0405
         STH   @01,BLKSIZNL                                        0405
*ELSE                                  /* NO TOLERANCE FACTOR, CALC  */
*  BLKSIZNL=SMCABSIZ+BLKOVRHD;         /* BLKSIZE NOT LAST ON TRK    */
         B     @RC00404                                            0406
@RF00404 LR    @10,BLKOVRHD                                        0406
         AL    @10,SMCABSIZ(,SMCAPTR)                              0406
         STH   @10,BLKSIZNL                                        0406
*SPACELFT=DEVTRKLN-BLKSILST;           /* DECREMENT TRK CAPACITY BY
*                                         SIZE OF LAST BLK ON A TRK  */
@RC00404 L     @10,SMCAPDCB(,SMCAPTR)                              0407
         LA    @10,DCBFDAD(,@10)                                   0407
         L     @10,DCBDVTBL+7(,@10)                                0407
         LH    SPACELFT,DEVTRKLN(,@10)                             0407
         SH    SPACELFT,BLKSILST                                   0407
*BLKONTRK=0;                                                       0408
         SLR   @10,@10                                             0408
         STH   @10,BLKONTRK                                        0408
*IF SPACELFT>=BLKSIZNL THEN            /* IF ENUF SPACE FOR MORE BLKS*/
         LH    @10,BLKSIZNL                                        0409
         CR    SPACELFT,@10                                        0409
         BL    @RF00409                                            0409
*  BLKONTRK=SPACELFT/BLKSIZNL;         /* CALC NO. OF BLKS THAT FIT  */
         LR    @00,SPACELFT                                        0410
         SRDA  @00,32                                              0410
         DR    @00,@10                                             0410
         STH   @01,BLKONTRK                                        0410
*BLKONTRK=BLKONTRK+1;                  /* NO. OF BUFFER SIZE BLKS  0411
*                                         THAT FIT ON A TRK          */
@RF00409 LH    @10,BLKONTRK                                        0411
         LA    @10,1(,@10)                                         0411
         STH   @10,BLKONTRK                                        0411
*END BLKCALC;                                                      0412
@EL00003 DS    0H                                                  0412
@EF00003 DS    0H                                                  0412
@ER00003 LM    @14,@12,12(@13)                                     0412
         BR    @14                                                 0412
*BWRITER:PROC;                                                     0413
BWRITER  STM   @14,@12,@SA00004                                    0413
*/********************************************************************/
*/*                                                                  */
*/*SEGMENT NAME= BWRITER                                             */
*/*                                                                  */
*/*DESCRIPTIVE NAME= SMF RECORD WRITER                               */
*/*                                                                  */
*/*FUNCTION=                                                         */
*/*  WRITE THE SMF BUFFER TO THE SMF DATA SET.                       */
*/*  INSURE THAT THE BUFFER DESCRIPTOR WORD CONTAINS A LENGTH GREATER*/
*/*  THAN FOUR THEN MARK THE DATA SET AS NOT EMPTY IN SMCA. ISSUE    */
*/*  A BSAM WRITE/CHECK AND CLOSE-T TRANSFERRING THE BUFFER TO THE   */
*/*  DATA SET. SHOULD AN I/O ERROR BE DETECTED ATTEMPT TO SWITCH THE */
*/*  DATA SETS AND ISSUE MESSAGE IEE364I SMF I/O ERROR. THEN RE-TRY  */
*/*  THE WRITE OPERATION EXCEPT IN CASES WHEN THE I/O ERROR CAUSED A */
*/*  DATA SET SWITCH WHILE SEGMENTED RECORDS WERE BEING WRITTEN AS   */
*/*  SEGMENTS OF THE SAME RECORD CANNOT SPAN THE TWO DATA SETS.      */
*/*                                                                  */
*/*  OPERATION= NO SPECIAL ALGORITHMS ARE USED                       */
*/*                                                                  */
*/*ENTRY POINT= BWRITER                                              */
*/*  INPUT=                                                          */
*/*    SMF BUFFER TO BE WRITTEN                                      */
*/*                                                                  */
*/*EXITS=                                                            */
*/*  CONDITION= NORMAL- SMF BUFFER WRITTEN TO THE DATA SET           */
*/*  OUTPUT=                                                         */
*/*    SMCAPMTY- SWITCH SET INDICATING DATA SET NOT EMPTY            */
*/*  RETURN CODES= NONE                                              */
*/*                                                                  */
*/*  CONDITION= I/O ERROR WRITING SMF BUFFER                         */
*/*  OUTPUT=                                                         */
*/*    MESSAGE IEE364I SMF I/O ERROR                                 */
*/*  RETURN CODES= NONE                                              */
*/*                                                                  */
*/********************************************************************/
*WRITDONE='0'B;                        /* INITIALIZE WRITER SWITCH   */
         NI    WRITDONE,B'11111101'                                0414
*RESPECIFY SMFBUFFR BASED(BUFPTR(BUFTOWRT));                       0415
*IF BUFLEN>4 THEN                      /* BUFFER CONTAIN ANY RECORDS?*/
         LR    @10,BUFTOWRT                                        0416
         SLA   @10,2                                               0416
         L     @10,BUFPTR-4(@10)                                   0416
         LH    @07,BUFLEN(,@10)                                    0416
         CH    @07,@CH00043                                        0416
         BNH   @RF00416                                            0416
*  DO;                                 /* YES, WRITE IT              */
*    RESPECIFY (REG1,REG2,REG3) RESTRICTED;                        0418
*    REG2=BUFPTR(BUFTOWRT);            /* ADDR OF BUFFER TO WRITE    */
         LR    REG2,@10                                            0419
*    DO K=1 TO 2 WHILE(SMCAPNAV='0'B & WRITDONE='0'B);             0420
         LA    @10,1                                               0420
         STC   @10,K                                               0420
@DL00420 TM    SMCAPNAV(SMCAPTR),B'10000000'                       0420
         BNZ   @DC00420                                            0420
         TM    WRITDONE,B'00000010'                                0420
         BNZ   @DC00420                                            0420
*      SMCAPMTY='0'B;                  /* DATA SET EMPTY BIT = OFF   */
         NI    SMCAPMTY(SMCAPTR),B'11101111'                       0421
*      REG3=SMCAPDCB;                  /* DCB ADDRESS                */
         L     REG3,SMCAPDCB(,SMCAPTR)                             0422
*      GEN(MVC   OPENLIST(MACLEN),OPENMAP); /* MOVE LIST FORMS FOR 0423
*                                         WRITE & CLOSE TO DYNAM AREA*/
         MVC   OPENLIST(MACLEN),OPENMAP
*      GEN(WRITE WRITDECB,SF,(REG3),(REG2),MF=E); /* WRITE BUFFER    */
         WRITE WRITDECB,SF,(REG3),(REG2),MF=E
*      GEN(CHECK WRITDECB);                                        0425
         CHECK WRITDECB
*      GEN(CLOSE ((REG3),LEAVE),TYPE=T,MF=(E,OPENLIST)); /* CLOSE-T  */
         CLOSE ((REG3),LEAVE),TYPE=T,MF=(E,OPENLIST)
*      IF WRITSTAT^='1'B THEN          /* I/O ERROR OCCUR?           */
         TM    WRITSTAT,B'00000001'                                0427
         BO    @RF00427                                            0427
*        DO;                           /* NO, INITIALIZE THE BUFFER  */
*          BUFBDW=EMPTY;               /* JUST WRITTEN               */
         LR    @10,BUFTOWRT                                        0429
         SLA   @10,2                                               0429
         L     @10,BUFPTR-4(@10)                                   0429
         MVC   BUFBDW(4,@10),EMPTY                                 0429
*          DCBOFLWR='1'B;              /* LAST I/O WAS A WRITE       */
         L     @10,SMCAPDCB(,SMCAPTR)                              0430
         OI    DCBOFLWR+40(@10),B'10000000'                        0430
*          WRITDONE='1'B;              /* INDICATE WRITE COMPLETE    */
         OI    WRITDONE,B'00000010'                                0431
*        END;                                                      0432
*      ELSE                            /* I/O ERROR OCCURRED         */
*        DO;                                                       0433
         B     @RC00427                                            0433
@RF00427 DS    0H                                                  0434
*          WRITSTAT='0'B;              /* RESET I/O ERROR INDICATOR  */
         NI    WRITSTAT,B'11111110'                                0434
*          IOERRMSG='1'B;              /* INDICATE I/O ERROR MSG IS  */
         OI    IOERRMSG,B'01000000'                                0435
*          CALL WRITEMSG;              /* TO BE WRITTEN  ISSUE MSG   */
         BAL   @14,WRITEMSG                                        0436
*          CALL DSSWITCH;              /* ATTEMPT DATA SET SWITCH    */
         BAL   @14,DSSWITCH                                        0437
*          IF RECSEG^=0 THEN           /* BUFFER TO BE WRITTEN HAVE
*                                         RECORD SEGMENT IN IT?      */
         LR    @10,BUFTOWRT                                        0438
         SLA   @10,2                                               0438
         L     @10,BUFPTR-4(@10)                                   0438
         CLI   RECSEG(@10),0                                       0438
         BE    @RF00438                                            0438
*            WRITDONE='1'B;            /* YES, DONT WRITE ANY MORE OF
*                                         THE SEGMENTS SINCE RECORDING
*                                         SWITCHED TO A NEW DATA SET */
         OI    WRITDONE,B'00000010'                                0439
*        END;                                                      0440
@RF00438 DS    0H                                                  0441
*    END;                                                          0441
@RC00427 LA    @10,1                                               0441
         SLR   @07,@07                                             0441
         IC    @07,K                                               0441
         ALR   @10,@07                                             0441
         STC   @10,K                                               0441
         CH    @10,@CH00047                                        0441
         BNH   @DL00420                                            0441
@DC00420 DS    0H                                                  0442
*    RESPECIFY (REG1,REG2,REG3) UNRESTRICTED;                      0442
*  END;                                                            0443
*END BWRITER;                                                      0444
@EL00004 DS    0H                                                  0444
@EF00004 DS    0H                                                  0444
@ER00004 LM    @14,@12,@SA00004                                    0444
         BR    @14                                                 0444
*POSTBECB:PROC;                                                    0445
POSTBECB STM   @14,@12,@SA00005                                    0445
*/********************************************************************/
*/*                                                                  */
*/*SEGMENT NAME= POSTBECB                                            */
*/*                                                                  */
*/*DESCRIPTIVE NAME=  XMPOST SMF RECORD MANAGER                      */
*/*                                                                  */
*/*FUNCTION=                                                         */
*/*  POST THE SMF RECORD MANAGER (IEEMB830) WHEN THE SERVICE RE-     */
*/*  QUESTED HAS BEEN PERFORMED. SET UP THE INTERFACE TO AND ISSUE   */
*/*  A CROSS MEMORY POST TO ALLOW IEEMB830 TO CONTINUE PROCESSING.   */
*/*                                                                  */
*/*  OPERATION= NO SPECIAL ALGORITHMS ARE USED                       */
*/*                                                                  */
*/*ENTRY POINT= POSTBECB                                             */
*/*  INPUT=                                                          */
*/*    SMCABECB- IEEMB380 WAIT ECB                                   */
*/*    SMCAASCB- ADDR OF ASCB FOR WAITING MEMORY                     */
*/*                                                                  */
*/*EXITS=                                                            */
*/*  CONDITION= NORMAL- IEEMB830 IS POSTED                           */
*/*  OUTPUT= NONE                                                    */
*/*  RETURN CODES=                                                   */
*/*    POST CODE= 4 IF A DATA SET SWITCH OPERATION TOOK PLACE        */
*/*               0 NORMAL                                           */
*/*                                                                  */
*/********************************************************************/
*RESPECIFY (REG0,REG1,REG2,REG3,REG4) RESTRICTED;                  0446
*IF POSTCODE='1'B THEN                 /* IS POST TO INDICATE THAT A
*                                         DATA SET SWITCH OCCURRED?  */
         TM    POSTCODE,B'00001000'                                0447
         BNO   @RF00447                                            0447
*  REG0=4;                             /* YES, SET POST CODE = 4     */
         LA    REG0,4                                              0448
*ELSE                                                              0449
*  REG0=0;                             /* NO, SET POST CODE TO ZERO  */
         B     @RC00447                                            0449
@RF00447 SLR   REG0,REG0                                           0449
*REG2=ADDR(SMCABECB);                  /* ADDR OF ECB TO POST        */
@RC00447 LA    REG2,SMCABECB(,SMCAPTR)                             0450
*REG3=SMCAASCB;                        /* ADDR OF ASCB OF POSTED   0451
*                                         MEMORY             @YM00441*/
         L     REG3,SMCAASCB(,SMCAPTR)                             0451
*REG4=ADDR(IEEMB827);                  /* ADDR OF POST ERROR ROUTINE */
         L     REG4,@CV00082                                       0452
*GEN(MVC   POSTLIST(POSTLEN),POSTDATA);                            0453
         MVC   POSTLIST(POSTLEN),POSTDATA
*GEN(POST  (2),(0),ASCB=(3),ERRET=(4),MF=(E,POSTLIST));            0454
         POST  (2),(0),ASCB=(3),ERRET=(4),MF=(E,POSTLIST)
*POSTCODE='0'B;                                                    0455
         NI    POSTCODE,B'11110111'                                0455
*RESPECIFY (REG0,REG1,REG2,REG3,REG4) UNRESTRICTED;                0456
*END POSTBECB;                                                     0457
@EL00005 DS    0H                                                  0457
@EF00005 DS    0H                                                  0457
@ER00005 LM    @14,@12,@SA00005                                    0457
         BR    @14                                                 0457
*SMFOPEN: PROCEDURE;                                               0458
SMFOPEN  STM   @14,@12,@SA00006                                    0458
*/********************************************************************/
*/*                                                                  */
*/*SEGMENT NAME= SMFOPEN                                             */
*/*                                                                  */
*/*DESCRIPTIVE NAME= SMF DATA SET OPENER                             */
*/*                                                                  */
*/*FUNCTION=                                                         */
*/*  TO OPEN AN SMF DATA SET FOR RECORDING. IT IS CALLED BY 1)       */
*/*  INITOPEN TO INITIALLY OPEN THE SMF DATA SETS AT SMF INITIALIZA- */
*/*  TION:                                                           */
*/*  THE DATA SET AREAS IN SMCA ARE EXCHANGED SO THAT THE AREA OF    */
*/*  THE DATA SET TO BE OPENED APPEARS FIRST. A READ JFCB IS DONE    */
*/*  AND THE VOLUME SERIAL NUMBER THAT THE DATA SET RESIDES ON IS    */
*/*  PLACED IN THE DATA AREA IN SMCA. AN ENQ IS DONE TO      @YM02763*/
*/*  SECURE THE RESOURCE 'SYSSMF01 DATASET' TO PREVENT       @YM02763*/
*/*  POSSIBLE CONFLICT SHOULD THE SMF DUMP PROGRAM BE        @YM02763*/
*/*  OPERATING ON THE DATASET. AN OBTAIN IS ISSUED TO GET THE@YM02763*/
*/*  FORMAT 1 DSCB OF THE SMF DATA SET TO BE OPENED. THE TTR OF THE  */
*/*  LAST BLOCK WRITTEN IS EXAMINED AND IF ZERO THE DATA SET IS      */
*/*  MARKED AS BEING EMPTY IN SMCA. AN OPEN-J IS PERFORMED FOR THE   */
*/*  DATA SET.                                                       */
*/*  2) DSSWITCH TO CLOSE THE RECORDING DATA SET AND OPEN THE OTHER: */
*/*  THE THE RECORDING DATA SET IS CLOSED AND A MESSAGE IS ISSUED    */
*/*  INFORMING THE OPERATOR TO DUMP THIS DATA SET. THE OTHER DATA SET*/
*/*  IS THEN OPENED AS DESCRIBED ABOVE.                              */
*/*                                                                  */
*/*  OPERATION= NO SPECIAL ALGORITHMS ARE USED                       */
*/*                                                                  */
*/*ENTRY POINT= SMFOPEN                                              */
*/*  INPUT=                                                          */
*/*    SMF DATA SET AREAS IN SMCA                                    */
*/*    SMCAFIRT- ON INDICATES SMF INITIALIZATION                     */
*/*    JFCB- JOB STEP CONTROL BLOCK                                  */
*/*    DSCB- DATA SET CONTROL BLOCK  FORMAT1                         */
*/*                                                                  */
*/*EXITS=                                                            */
*/*  CONDITION= NORMAL- SMF DATA SET OPENED FOR RECORDING            */
*/*  OUTPUT=                                                         */
*/*    SMCAPNAV- SET OFF INDICATING SUCCESSFUL OPEN OF DATA SET      */
*/*    SMCAPMTY- SET ON IF DATA SET IS EMPTY                         */
*/*    MESSAGE IEE362A- SMF ENTER DUMP.....  (IF CALLED FOR DATA     */
*/*                                          SET SWITCHING)          */
*/*  RETURN CODES= NONE                                              */
*/*                                                                  */
*/*  CONDITION= SMF DATA SET NOT SUCCESSFULLY OPENED                 */
*/*  OUTPUT=                                                         */
*/*    SMCAPNAV- SET ON INDICATING DATA SET NOT AVAILABLE            */
*/*    SMCADSNF- SET ON FOR DATA SET NOT FOUND (OBTAIN FAILURE)      */
*/*    SMCAOPFL- SET ON FOR OPEN FAILURE                             */
*/*  RETURN CODES= NONE                                              */
*/*                                                                  */
*/********************************************************************/
*SMCADSNF='0'B;                        /* TURN D.S. NOT FOUND SW OFF */
         NI    SMCADSNF(SMCAPTR),B'11111110'                       0459
*SMCAOPFL='0'B;                        /* SET OPEN FAIL SWITCH OFF   */
         NI    SMCAOPFL(SMCAPTR),B'11011111'                       0460
*DEVAREAA=DEVAREAA && DEVAREAB;        /* SWITCH THE DATA SET AREAS  */
         LA    @10,24                                              0461
         ALR   @10,SMCAPTR                                         0461
         LA    @04,40                                              0461
         ALR   @04,SMCAPTR                                         0461
         XC    DEVAREAA(16,@10),DEVAREAB(@04)                      0461
*DEVAREAB=DEVAREAB && DEVAREAA;        /* IN SMCA SO THAT THE OTHER  */
         XC    DEVAREAB(16,@04),DEVAREAA(@10)                      0462
*DEVAREAA=DEVAREAA && DEVAREAB;        /* D.S. TO BE OPENED IS FIRST */
         XC    DEVAREAA(16,@10),DEVAREAB(@04)                      0463
*SMCAPNAV='1'B;                        /* SET NOT AVAILABLE SWITCH ON*/
         OI    SMCAPNAV(SMCAPTR),B'10000000'                       0464
*IF SMCAFIRT='0'B THEN                 /* IF NOT ENTERED TO OPEN SMF
*                                         DATA SETS BUT TO DO DATA   */
         TM    SMCAFIRT(SMCAPTR),B'00010000'                       0465
         BNZ   @RF00465                                            0465
*  DO;                                 /* SET SWITCHING THEN DO:     */
*    GEN (MVC OPENLIST(OPENLEN),OPENMAP); /* MOVE OPEN LIST FORM     */
         MVC OPENLIST(OPENLEN),OPENMAP
*    RESPECIFY (REG0,REG1,REG2) RESTRICTED;                        0468
*    REG2=SMCAADCB;                     /* GET DCB ADDRESS FOR CLOSE */
         L     REG2,SMCAADCB(,SMCAPTR)                             0469
*    GEN (CLOSE ((REG2)),MF=(E,OPENLIST)); /* CLOSE SMF DATA SET     */
         CLOSE ((REG2)),MF=(E,OPENLIST)
*    RESPECIFY (REG0,REG1,REG2) UNRESTRICTED;                      0471
*    IF SMCADSTR='0'B THEN             /* IF A DISASTER CONDITION HAS
*                                         NOT OCCURRED THEN ISSUE A  */
         TM    SMCADSTR(SMCAPTR),B'01000000'                       0472
         BNZ   @RF00472                                            0472
*      DO;                             /* MESSAGE TO DUMP THE SMF    */
*        DUMPMSG='1'B;                 /* DATA SET WHICH HAS BEEN    */
         OI    DUMPMSG,B'10000000'                                 0474
*        CALL WRITEMSG;                /* CLOSED.                    */
         BAL   @14,WRITEMSG                                        0475
*        CALL INVKEXIT;                /* SET FOR USER EXIT  @Z40FPPF*/
         BAL   @14,INVKEXIT                                        0476
*      END;                                                        0477
*  END;                                                            0478
@RF00472 DS    0H                                                  0479
*IF DCBOFOPN='1'B THEN                 /* IF DATA SET              0479
*                                         ALREADY OPENED, THEN TURN  */
@RF00465 L     @10,SMCAPDCB(,SMCAPTR)                              0479
         TM    DCBOFOPN+40(@10),B'00010000'                        0479
         BNO   @RF00479                                            0479
*  SMCAPNAV='0'B;                      /* OFF UNAVAIL BIT & RETURN   */
         NI    SMCAPNAV(SMCAPTR),B'01111111'                       0480
*ELSE                                  /* OTHERWISE, CONTINUE WITH   */
*  DO;                                 /* REST OF THIS SEGMENT       */
         B     @RC00479                                            0481
@RF00479 DS    0H                                                  0482
*    RESPECIFY REG15 RESTRICTED;       /*                    @YM02763*/
*    DSHAVE='0'B;                      /* INIT RESOURCE SW   @YM02763*/
         NI    DSHAVE,B'01111111'                                  0483
*    IF SMCAFIRT='1'B THEN             /* IF SMF INIT ASSUME @YM02763*/
         TM    SMCAFIRT(SMCAPTR),B'00010000'                       0484
         BNO   @RF00484                                            0484
*      DSHAVE='1'B;                    /* NO D.S. CONFLICT   @YM02763*/
         OI    DSHAVE,B'10000000'                                  0485
*    ELSE                              /* SMF DUMP PROGRAM   @YM02763*/
*      DO;                             /* MAY HAVE SMF D.S.  @YM02763*/
         B     @RC00484                                            0486
@RF00484 DS    0H                                                  0487
*        GEN(MVC ENQL(ENQLN),ENQCORE); /* MOVE ENQ LIST FORM @YM02763*/
         MVC ENQL(ENQLN),ENQCORE
*        GEN(ENQ MF=(E,ENQL));         /* ENQ EXCL. ON 'SYSSMF01   0488
*                                         DATASET'           @YM02763*/
         ENQ MF=(E,ENQL)
*        IF REG15=0 THEN               /* IF RESOURCE AVAIL  @YM02763*/
         LTR   REG15,REG15                                         0489
         BNZ   @RF00489                                            0489
*          DSHAVE='1'B;                /* SET INDICATOR      @YM02763*/
         OI    DSHAVE,B'10000000'                                  0490
*        ELSE;                         /* RESOURCE NOT AVAIL @YM02763*/
@RF00489 DS    0H                                                  0492
*      END;                            /*                    @YM02763*/
*    IF DSHAVE='1'B THEN               /* ALL RIGHT TO GO    @YM02763*/
@RC00484 TM    DSHAVE,B'10000000'                                  0493
         BNO   @RF00493                                            0493
*      DO;                             /* AHEAD WITH OBTAIN  @YM02763*/
*        JFCBPTR=ADDR(JFCBAREA);       /* SET UP TO READ THE JFCB    */
         LA    JFCBPTR,JFCBAREA                                    0495
*        EXJFCBPT=JFCBPTR;             /* HAVE EXITLIST PT TO JFCB   */
         STCM  JFCBPTR,7,EXJFCBPT                                  0496
*        EXLSTIND='87'X;               /* OPEN TYPE J                */
         MVI   EXLSTIND,X'87'                                      0497
*        DCBEXL=ADDR(EXITLIST);        /* HAVE DCB POINT TO EXITLIST */
         L     @14,SMCAPDCB(,SMCAPTR)                              0498
         LA    @14,DCBEXLST+28(,@14)                               0498
         LA    @10,EXITLIST                                        0498
         STCM  @10,7,DCBEXL+1(@14)                                 0498
*        GEN (MVC OPENLIST(OPENLEN),OPENMAP); /* MOVE OPEN LIST FORM */
         MVC OPENLIST(OPENLEN),OPENMAP
*        RESPECIFY (REG0,REG1,REG2) RESTRICTED;                    0500
*        REG2=SMCAPDCB;                /* GET DCB ADDR FOR JFCB READ */
         L     REG2,SMCAPDCB(,SMCAPTR)                             0501
*        GEN (RDJFCB ((REG2),(OUTIN,LEAVE)),MF=(E,OPENLIST));      0502
         RDJFCB ((REG2),(OUTIN,LEAVE)),MF=(E,OPENLIST)
*                                      /* READ IN THE JFCB           */
*        RESPECIFY (REG0,REG1,REG2) UNRESTRICTED;                  0503
*        SMCAPDEV=JFCBVOLS;            /* PUT VOLSER OF D.S. IN SMCA */
         MVC   SMCAPDEV(6,SMCAPTR),JFCBVOLS(JFCBPTR)               0504
*        JFCDISP='10'B;                /* SET DISP TO MOD IN JFCB    */
         NI    JFCDISP(JFCBPTR),B'10111111'                        0505
         OI    JFCDISP(JFCBPTR),B'10000000'                        0505
*        DCBLRECL=SMCABUF;             /* MAKE DCB LOGICAL RECORD LEN*/
         L     @14,SMCAPDCB(,SMCAPTR)                              0506
         L     @10,SMCABUF(,SMCAPTR)                               0506
         STH   @10,DCBLRECL+72(,@14)                               0506
*        DCBBLKSI=SMCABUF;             /* & BLKSIZE = SMF BUFFER SIZE*/
         STH   @10,DCBBLKSI+52(,@14)                               0507
*        OBTWORK=ADDR(OBTPOOL);        /* INITIALIZE A CAMLIST IN    */
         LA    @14,OBTPOOL                                         0508
         ST    @14,OBTWORK                                         0508
*        OBTNAME=JFCBPTR;              /* ORDER TO DO AN OBTAIN ON   */
         ST    JFCBPTR,OBTNAME                                     0509
*        OBTVSER=ADDR(SMCAPDEV);       /* THE SMF DATA SET TO BE     */
         LA    @14,SMCAPDEV(,SMCAPTR)                              0510
         ST    @14,OBTVSER                                         0510
*        OBTOPTN=SEARCH;               /* OPENED                     */
         MVC   OBTOPTN(4),SEARCH                                   0511
*        RESPECIFY REG1 RESTRICTED;                                0512
*        REG1=ADDR(OBTLIST);           /* ADDR OF CAMLIST FOR OBTAIN */
         LA    REG1,OBTLIST                                        0513
*        GEN (OBTAIN (REG1));          /* ISSUE THE OBTAIN           */
         OBTAIN (REG1)
*        RESPECIFY REG1 UNRESTRICTED;                              0515
*        RESPECIFY IECSDSF1 BASED(ADDR(OBTPOOL)-44);               0516
*        SAVERET=REG15;                /* SAVE OBT RET CODE  @YM02763*/
         LR    SAVERET,REG15                                       0517
*        IF SMCAFIRT='0'B THEN         /* IF NOT SMF INIT    @YM02763*/
         TM    SMCAFIRT(SMCAPTR),B'00010000'                       0518
         BNZ   @RF00518                                            0518
*          DO;                         /* DEQ FROM D.S. RESOURCE   0519
*                                                            @YM02763*/
*            GEN(MVC DEQL(DEQLN),DEQCORE); /* MOVE DEQ LIST  @YM02763*/
         MVC DEQL(DEQLN),DEQCORE
*            GEN(DEQ MF=(E,DEQL));     /* DEQ SMF DATASET    @YM02763*/
         DEQ MF=(E,DEQL)
*          END;                        /*                    @YM02763*/
*        IF SAVERET^=0 THEN            /* IF OBTAIN FAILED TURN ON 0523
*                                                            @YM02763*/
@RF00518 LTR   SAVERET,SAVERET                                     0523
         BZ    @RF00523                                            0523
*          SMCADSNF='1'B;              /* THE DATA SET NOT FOUND BIT */
         OI    SMCADSNF(SMCAPTR),B'00000001'                       0524
*        ELSE                                                      0525
*          DO;                                                     0525
         B     @RC00523                                            0525
@RF00523 DS    0H                                                  0526
*            IF SMCAFIRT='1'B THEN     /* IF INITIAL OPEN OF SMF DATA
*                                         SETS THEN SAVE TTR OF LAST */
         TM    SMCAFIRT(SMCAPTR),B'00010000'                       0526
         BNO   @RF00526                                            0526
*              TTRS(TTRINDEX)=DS1LSTAR; /* BLK WRITTEN ON D.S.(GOTTEN
*                                         FROM DSCB VIA OBTAIN)      */
         SLR   @14,@14                                             0527
         IC    @14,TTRINDEX                                        0527
         MH    @14,@CH00115                                        0527
         LA    @10,TTRS-3(@14)                                     0527
         LA    @14,OBTPOOL                                         0527
         MVC   0(3,@10),DS1LSTAR-44(@14)                           0527
*            IF SMCAFIRT='1'B  DS1LSTAR=0 THEN  /* IF INITIAL OPEN OR
*                                         DATA SET SWITCHING & D.S.  */
@RF00526 TM    SMCAFIRT(SMCAPTR),B'00010000'                       0528
         BO    @RT00528                                            0528
         LA    @14,OBTPOOL                                         0528
         ICM   @10,7,DS1LSTAR-44(@14)                              0528
         BNZ   @RF00528                                            0528
@RT00528 DS    0H                                                  0529
*              DO;                     /* EMPTY THEN...              */
*                IF DS1LSTAR=0 THEN    /* IF D.S. EMPTY THEN TURN ON */
         LA    @14,OBTPOOL                                         0530
         ICM   @10,7,DS1LSTAR-44(@14)                              0530
         BNZ   @RF00530                                            0530
*                  SMCAPMTY='1'B;      /* THE EMPTY BIT IN SMCA.     */
         OI    SMCAPMTY(SMCAPTR),B'00010000'                       0531
*                RESPECIFY (REG0,REG1,REG2) RESTRICTED;            0532
@RF00530 DS    0H                                                  0533
*                REG2=SMCAPDCB;        /* GET DCB ADDRESS FOR OPEN   */
         L     REG2,SMCAPDCB(,SMCAPTR)                             0533
*                GENERATE;                                         0534
         OPEN ((REG2),(OUTPUT)),MF=(E,OPENLIST),TYPE=J
*                RESPECIFY (REG0,REG1,REG2) UNRESTRICTED;          0535
*                IF DCBOFOPN='1'B THEN /* IF THE OPEN DID NOT FAIL 0536
*                                         THEN TURN OFF THE 'DEVICE  */
         L     @14,SMCAPDCB(,SMCAPTR)                              0536
         TM    DCBOFOPN+40(@14),B'00010000'                        0536
         BNO   @RF00536                                            0536
*                  SMCAPNAV='0'B;      /* NOT AVAILABLE' BIT IN SMCA */
         NI    SMCAPNAV(SMCAPTR),B'01111111'                       0537
*                ELSE                  /* IF OPEN FAILED THEN TURN   */
*                  SMCAOPFL='1'B;      /* ON OPEN FLAG BIT IN SMCA   */
         B     @RC00536                                            0538
@RF00536 OI    SMCAOPFL(SMCAPTR),B'00100000'                       0538
*              END;                                                0539
@RC00536 DS    0H                                                  0540
*          END;                                                    0540
@RF00528 DS    0H                                                  0541
*        RESPECIFY REG15 UNRESTRICTED;                             0541
@RC00523 DS    0H                                                  0542
*      END;                            /*                    @YM02763*/
*  END;                                                            0543
*END SMFOPEN;                                                      0544
@EL00006 DS    0H                                                  0544
@EF00006 DS    0H                                                  0544
@ER00006 LM    @14,@12,@SA00006                                    0544
         BR    @14                                                 0544
*DSSWITCH: PROCEDURE;                                              0545
DSSWITCH STM   @14,@12,@SA00007                                    0545
*/********************************************************************/
*/*                                                                  */
*/*SEGMENT NAME= DSSWITCH                                            */
*/*                                                                  */
*/*DESCRIPTIVE NAME= DATA SET SWITCHING SEGMENT                      */
*/*                                                                  */
*/*FUNCTION= THE FUNCTION OF DSSWITCH IS TO SWITCH RECORDING FROM    */
*/*          ONE SMF DATA SET TO THE OTHER SMF DATA SET.  THIS IS    */
*/*          ACCOMPLISHED BY THE FOLLOWING:                          */
*/*          1.  THE SMFOPEN ROUTINE IS CALLED TO CLOSE THE DATA SET */
*/*              CURRENTLY BEING RECORDED ON AND OPEN THE OTHER DATA */
*/*              SET FOR RECORDING.                                  */
*/*          2.  IF SMFOPEN ROUTINE WAS SUCCESSFUL (SMCAPNAV=OFF),   */
*/*              THEN A RECORDING MESSAGE IS ISSUED FOR THE OPENED   */
*/*              DATA SET.  ALSO, THE NUMBER OF BUFFER SIZE SEGMENTS */
*/*              WHICH WILL FIT ON A TRACK OF THE NEW DATA SET IS    */
*/*              CALCULATED BY CALLING THE BLKCALC ROUTINE.          */
*/*          3.  IF THE SMFOPEN ROUTINE WAS UNSUCCESSFUL, THEN THE   */
*/*              DISASTER BIT IN SMCA (SMCADSTR) IS SET ON, THE      */
*/*              DISASTER COUNT (SMCADSCT) IS SET TO ZERO, THE TIME  */
*/*              AND DATE OF THE DISASTER (VIA TIME SVC) IS PLACED IN*/
*/*              SMCA (SMCADSTM) AND A DATA LOST MESSAGE IS ISSUED.  */
*/*                                                                  */
*/*  OPERATION= NO SPECIAL ALGORITHMS ARE USED                       */
*/*                                                                  */
*/*ENTRY POINT= DSSWITCH                                             */
*/*  INPUT= NONE                                                     */
*/*                                                                  */
*/*EXITS=                                                            */
*/*  CONDITION= NORMAL- RECORDING SWITCHED TO THE OTHER SMF DATA SET */
*/*  OUTPUT=                                                         */
*/*    MESSAGE IEE360I- SMF NOW RECORDING.....                       */
*/*  RETURN CODES= NONE                                              */
*/*                                                                  */
*/*  CONDITION= SWITCH UNSUCCESSFUL-DATA SET NOT AVAILABLE           */
*/*    OUTPUT= IF NOT ALREADY IN DISASTER MODE                       */
*/*      SMCADSTR- DISASTER BIT SET ON INDICATING DATA LOST MODE     */
*/*      SMCADSCT- RECORD LOST COUNT INITIALIZED TO ZERO             */
*/*      SMCADSTM- TIME AND DATE OF DISASTER                         */
*/*      MESSAGE IEE361I- SMF DATA LOST.....                         */
*/*                                                                  */
*/********************************************************************/
*CALL SMFOPEN;                         /* CLOSE CUR D.S.& OPEN OTHER */
         BAL   @14,SMFOPEN                                         0546
*IF SMCAPNAV='0'B THEN                 /* IF THE OPEN WAS SUCCESSFUL */
         TM    SMCAPNAV(SMCAPTR),B'10000000'                       0547
         BNZ   @RF00547                                            0547
*  DO;                                                             0548
*    POSTCODE='1'B;                    /* INDICATE SWITCH TOOK PLACE */
         OI    POSTCODE,B'00001000'                                0549
*    IF SMCADSTR='0'B THEN             /* IF NOT ENTERED DURING DSTR */
         TM    SMCADSTR(SMCAPTR),B'01000000'                       0550
         BNZ   @RF00550                                            0550
*      DO;                             /* THEN ISSUE THE RECORDING   */
*        RECGMSG='1'B;                 /* MESSAGE FOR NEW DATA SET,  */
         OI    RECGMSG,B'00010000'                                 0552
*        CALL WRITEMSG;                /* IEE360I SMF RECORDING..... */
         BAL   @14,WRITEMSG                                        0553
*      END;                                                        0554
*    CALL BLKCALC;                     /* CALL THE ROUTINE TO COMPUTE*/
@RF00550 BAL   @14,BLKCALC                                         0555
*  END;                                /* THE NO. OF BUF SIZE SEGMENTS
*                                         WHICH WILL FIT ON A TRACK  */
*ELSE                                  /* IF THE OPEN FAILED DO THE  */
*  DO;                                 /* FOLLOWING                  */
         B     @RC00547                                            0557
@RF00547 DS    0H                                                  0558
*    IF SMCADSTR='0'B THEN             /* IF A DISASTER HAS NOT PRE-
*                                         VIOUSLY OCCURRED, THEN     */
         TM    SMCADSTR(SMCAPTR),B'01000000'                       0558
         BNZ   @RF00558                                            0558
*      DO;                             /* TURN ON THE DISASTER INDI- */
*        SMCADSTR='1'B;                /* CATOR BIT IN SMCA, SET THE */
         OI    SMCADSTR(SMCAPTR),B'01000000'                       0560
*        SMCADSCT=0;                   /* DISASTER COUNT TO ZERO     */
         SLR   @10,@10                                             0561
         ST    @10,SMCADSCT(,SMCAPTR)                              0561
*        RESPECIFY (REG0,REG1) RESTRICTED;                         0562
*        GEN (TIME BIN);               /* ISSUE TIME SVC             */
         TIME BIN
*        DSTRTIME(2)=REG0;             /* DISASTER TIME IN SMCADSTM  */
         LA    @10,108                                             0564
         ALR   @10,SMCAPTR                                         0564
         ST    REG0,DSTRTIME+4(,@10)                               0564
*        DSTRTIME(1)=REG1;             /* DISASTER DATE IN SMCADSTM  */
         ST    REG1,DSTRTIME(,@10)                                 0565
*        RESPECIFY (REG0,REG1) UNRESTRICTED;                       0566
*        LOSTMSG='1'B;                 /* SET LOST MSG INDICATOR     */
         OI    LOSTMSG,B'00100000'                                 0567
*        CALL WRITEMSG;                /* ISSUE THE RECORD LOST MSG  */
         BAL   @14,WRITEMSG                                        0568
*      END;                                                        0569
*  END;                                                            0570
*END DSSWITCH;                                                     0571
@EL00007 DS    0H                                                  0571
