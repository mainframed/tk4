         TITLE 'IEEMB822 - SMF OPEN INITIALIZER                        *
                        '
IEEMB822 CSECT ,                                                01S0001
@MAINENT DS    0H                                               01S0001
         USING *,@15                                            01S0001
         B     @PROLOG                                          01S0001
         DC    AL1(16)                                          01S0001
         DC    C'IEEMB822  76.209'                              01S0001
         DROP  @15
@PROLOG  STM   @14,@12,12(@13)                                  01S0001
         BALR  @12,0                                            01S0001
@PSTART  DS    0H                                               01S0001
         USING @PSTART,@12                                      01S0001
         L     @00,@SIZDATD                                     01S0001
         GETMAIN  R,LV=(0)
         LR    @11,@01                                          01S0001
         USING @DATD,@11                                        01S0001
         ST    @13,@SA00001+4                                   01S0001
         LM    @00,@01,20(@13)                                  01S0001
         ST    @11,8(,@13)                                      01S0001
         LR    @13,@11                                          01S0001
         MVC   @PC00001(12),0(@01)                              01S0001
*                                                               03S0051
*                                                               03S0051
* /*                   END OF TCB                                    */
*SMCAPTR=SMCAPT;                       /*OPTIMIZE SMCA ADDRESSABILITY*/
         L     @10,@PC00001                                     01S0052
         L     SMCAPTR,SMCAPT(,@10)                             01S0052
*/********************************************************************/
*/*                                                                  */
*/*SEGMENT NAME= INITIAL                                             */
*/*                                                                  */
*/*DESCRIPTIVE NAME= SPECIFICATIONS FOR SMCA INITIALIZATION SEGMENT  */
*/*                                                                  */
*/*FUNCTION =                                                        */
*/*   * GET CORE FOR BUFFER LENGTH SPECIFIED BY BUF= .               */
*/*   * CALCULATE WORKING BUFFER SIZE (SMCABSIZ=SMCABUF/2)           */
*/*   * INITIALIZE BOTH BUFFERS.                                     */
*/*   * GET CORE FOR BOTH DCBS AND INITIALIZE WITH DCB EXPANSION.    */
*/*   * INITIALIZE DEVICE ENTRIES TO INDICATE WHICH IS RESIDENT DCB  */
*/*   * LOAD THE SVC'S USER EXIT IEFU83 TO OBTAIN ADDRESS FOR SVC.   */
*/*                                                                  */
*/*OPERATION=                                                        */
*/*  NO SPECIAL ALGORITHMS ARE EMPLOYED IN THIS ROUTINE              */
*/*                                                                  */
*/*ENTRY POINT= INITIAL                                              */
*/*                                                                  */
*/* INPUT= SMCABUF - TOTAL SIZE OF BOTH BUFFERS                      */
*/*                                                                  */
*/* EXIT                                                             */
*/*  CONDITION= PROCESSING COMPLETE                                  */
*/*  OUTPUT= SMCABUFP - POINTER TO THE FIRST OF TWO BUFFERS          */
*/*          SMCABSIZ - SIZE OF A WORKING BUFFER                     */
*/*          SMCAU83  - POINTER TO IEFU83 ENTRY POINT                */
*/*          SMCAPDCB - POINTER TO MANX DCB                          */
*/*          SMCAADCB - POINTER TO MANY DCB CORE                     */
*/*          SMCAXORY - X INDICATOR                                  */
*/*          SMCAYORX - Y INDICATOR                                  */
*/*          DCBS IN SUBPOOL 241                                     */
*/*  RETURN CODE = N/A                                               */
*/********************************************************************/
*RFY (REG0,REG1) RSTD;                                          02S0053
*REG1=SMCABUF;                         /* LENGTH FOR BUFFER GETMAIN  */
         L     REG1,SMCABUF(,SMCAPTR)                           02S0054
*GEN (GETMAIN R,LV=(REG1),SP=231);     /* GET BOTH BUFFERS   @ZA09178*/
         GETMAIN R,LV=(REG1),SP=231
*SMCABUFP=REG1;                        /* SAVE BUFFER POINTER IN SMCA*/
         ST    REG1,SMCABUFP(,SMCAPTR)                          02S0056
*SMCABSIZ=SMCABUF/2;                   /* HALF OF TOTAL BUFFER SIZE
*                                         FOR EACH WORKING BUFFER.   */
         L     @04,SMCABUF(,SMCAPTR)                            02S0057
         SRDA  @04,32                                           02S0057
         D     @04,@CF00065                                     02S0057
         ST    @05,SMCABSIZ(,SMCAPTR)                           02S0057
*RFY (REG0,REG1) UNRSTD;               /* RELEASE GETMAIN REGISTERS  */
*BUFPTR=SMCABUFP;                      /* ADDRESS FIRST BUFFER       */
         L     @10,SMCABUFP(,SMCAPTR)                           02S0059
         LR    BUFPTR,@10                                       02S0059
*BDWLEN=INITBUF;                       /* INITIALIZE FIRST BUFFER    */
         MVC   BDWLEN(4,BUFPTR),INITBUF                         02S0060
*BUFPTR=BUFPTR+SMCABSIZ;               /* ADDRESS SECOND BUFFER      */
         ALR   BUFPTR,@05                                       02S0061
*BDWLEN=INITBUF;                       /* INITIALIZE SECOND BUFFER   */
         MVC   BDWLEN(4,BUFPTR),INITBUF                         02S0062
*RFY (REG0,REG1) RSTD;                 /* RESERVE GETMAIN REGISTERS  */
*REG0=XDCBSIZE;                        /* DCB LENGTH AND SUBPOOL     */
         L     REG0,XDCBSIZE                                    02S0064
*GEN (GETMAIN R,LV=(0));               /* GET DCB CORE FOR MANX      */
         GETMAIN R,LV=(0)
*SMCAPDCB=REG1;                        /* ADDRESS OF MANX DCB        */
         ST    REG1,SMCAPDCB(,SMCAPTR)                          02S0066
*MANXDCB(1:XSIZE)=DCBMANX(1:XSIZE);    /* DCB TO GOTTEN CORE         */
         L     @10,SMCAPDCB(,SMCAPTR)                           02S0067
         LH    @03,XSIZE                                        02S0067
         BCTR  @03,0                                            02S0067
         EX    @03,@SM01163                                     02S0067
*SMCAXORY='X';                         /* MANX INDICATOR TO DEV ENTRY*/
         MVI   SMCAXORY(SMCAPTR),C'X'                           02S0068
*REG0= YDCBSIZE;                       /* DCB LENGTH AND SUBPOOL     */
         L     REG0,YDCBSIZE                                    02S0069
*GEN (GETMAIN R,LV=(0));               /* GET MANY DCB CORE          */
         GETMAIN R,LV=(0)
*SMCAADCB=REG1;                        /*ADDRESS OF MANY DCB         */
         ST    REG1,SMCAADCB(,SMCAPTR)                          02S0071
*MANYDCB(1:YSIZE)=DCBMANY(1:YSIZE);     /*DCB TO GOTTEN CORE         */
         L     @10,SMCAADCB(,SMCAPTR)                           02S0072
         LH    @03,YSIZE                                        02S0072
         BCTR  @03,0                                            02S0072
         EX    @03,@SM01165                                     02S0072
*SMCAYORX='Y';                         /* MANY INDICATOR TO DEV ENTRY*/
         MVI   SMCAYORX(SMCAPTR),C'Y'                           02S0073
*SMCAWTCB=WTRTCBPT;                    /* WTR'S TCB POINTER FOR MB827*/
         L     @10,@PC00001+4                                   02S0074
         MVC   SMCAWTCB(4,SMCAPTR),WTRTCBPT(@10)                02S0074
*GEN (LOAD  EP=IEFU83);                /* USER EXIT FOR SVC LOADED   */
         LOAD  EP=IEFU83
*SMCAU83=REG0;                         /* EXIT ADDRESS SAVED FOR SVC */
         ST    REG0,SMCAU83(,SMCAPTR)                           02S0076
*RFY (REG0,REG1) UNRSTD;               /* RELEASE SYSTEM SVC'S REGS  */
*ERROR='0'B;                           /* INIT. OPENING ERROR SWITCH */
         NI    ERROR,B'01111111'                                01S0078
*/*                                                                  */
*/********************************************************************/
*/*                                                                  */
*/*SEGMENT NAME= MANCHECK                                            */
*/*                                                                  */
*/*DESCRIPTIVE NAME= SMF DATA SET CHARACTERISTICS CHECKING           */
*/*                                                                  */
*/*FUNCTION=                                                         */
*/*  BOTH DATA SET'S CHARACTERISTICS ARE CHECKED BEFORE THE SMF WTR  */
*/*  IS SET DISPATCHABLE.  A DEVTYPE IS ISSUED TO DETERMINE IF THE   */
*/*  DATA SETS EXIST AND IS ON A DIRECT ACCESS DEVICE.  IF IT DOES   */
*/*  NOT EXIST THE DATA SET NOT OPEN MESSAGE WILL BE CONSTRUCTED. IF */
*/*  IT IS NOT ON A DIRECT ACCESS DEVICE THE NOT D. A. MESSAGE IS    */
*/*  CONSTRUCTED.                                                    */
*/*                                                                  */
*/*OPERATION= NO SPECIAL ALGORITHMS ARE EMPLOYED.                    */
*/*                                                                  */
*/*ENTRY POINT= MANCHECK                                             */
*/*                                                                  */
*/* INPUT=                                                           */
*/*      JFCBVOLS - FOR THE X OR Y DATA SET THAT WAS NOT D. A.       */
*/*                                                                  */
*/*EXIT                                                              */
*/* CONDITION= DATA SET WAS NOT ON A DIRECT ACCESS DEVICE            */
*/* OUTPUT= MESSAGE IEE363I TO MSGBUF                                */
*/*         ERROR SWITCH = ON                                        */
*/* RETURN CODE= N/A                                                 */
*/* CONDITION= DATA SET IS NON-EXISTENT(NOT AVAILABLE FOR OPENING)   */
*/* OUTPUT= MESSAGE IEE365I TO MSGBUF                                */
*/* RETURN CODE= N/A                                                 */
*/* CONDITION= BOTH DATA SETS CHECKED AND NO ERRORS FOUND            */
*/* OUTPUT= NONE                                                     */
*/* RETURN CODE= N/A                                                 */
*/*                                                                  */
*/********************************************************************/
*/*                                                                  */
*DDNAME=SMFMAN;                        /* SET UP MANX×Y DDNAME       */
         MVC   DDNAME(8),@CC00120                               02S0079
*DO LOOP=1 TO 2 WHILE(ERROR='0'B);     /* BOTH MANX&Y ARE TO BE DONE */
         LA    @10,1                                            02S0080
         STC   @10,LOOP                                         02S0080
@DL00080 TM    ERROR,B'10000000'                                02S0080
         BNZ   @DC00080                                         02S0080
*  DDNAME(7)=XYARRAY(LOOP);            /* DDNAME UPDATED FOR DEVTYPE */
         LA    @03,XYARRAY-1(@10)                               02S0081
         MVC   DDNAME+6(1),0(@03)                               02S0081
*  GEN (DEVTYPE DDNAME,RETAREA);       /* REQUEST DEVICE CHAR.       */
         DEVTYPE DDNAME,RETAREA
*  RFY (REG5,REG15) RSTD;                                       02S0083
*  IF REG15=0 THEN                     /* WAS DEVTYPE SUCCESSFUL?    */
         LTR   REG15,REG15                                      02S0084
         BNZ   @RF00084                                         02S0084
*    DO;                                                        02S0085
*      RFY REG15 UNRSTD;               /* RELEASE RETURN CODE REG    */
*      IF TYPDA^='1'B THEN             /* IS IT A DIRECT ACCESS DEV? */
         TM    TYPDA,B'00100000'                                02S0087
         BO    @RF00087                                         02S0087
*        DO;                                                    02S0088
*          ERROR='1'B;                 /*SET ERROR ENCOUNTERED SWITCH*/
         OI    ERROR,B'10000000'                                02S0089
*          IF XYARRAY(LOOP)='X' THEN   /* DETREMINE WHICH DATA SET
*                                         ENCOUNTERED THE ERROR.     */
         SLR   @10,@10                                          02S0090
         IC    @10,LOOP                                         02S0090
         LA    @04,XYARRAY-1(@10)                               02S0090
         CLI   0(@04),C'X'                                      02S0090
         BNE   @RF00090                                         02S0090
*            REG5=SMCAPDCB;            /* MANX DCB POINTER           */
         L     REG5,SMCAPDCB(,SMCAPTR)                          02S0091
*          ELSE                                                 02S0092
*            REG5=SMCAADCB;            /* MANY DCB POINTER           */
         B     @RC00090                                         02S0092
@RF00090 L     REG5,SMCAADCB(,SMCAPTR)                          02S0092
*          RFY IHADCB BASED(REG5);                              02S0093
@RC00090 DS    0H                                               02S0094
*          DCBEXLSA=ADDR(RJFCBLST);    /* POINT TO RDJFCB EXIT LIST  */
         LA    @10,RJFCBLST                                     02S0094
         STCM  @10,7,DCBEXLSA+28(REG5)                          02S0094
*          EXITLIST=ADDR(JFCBAREA);    /* ADDRESS OF JFCB AREA       */
         LA    @10,JFCBAREA                                     02S0095
         STCM  @10,7,EXITLIST                                   02S0095
*          INDIC='87'X;                /* 80 INDICATES LAST LIST ENTRY
*                                         WHILE 07 INDICATES ENTRY FOR
*                                         RDJFCB.                    */
         MVI   INDIC,X'87'                                      02S0096
*          JFCBREAD=JFCBREAS;          /* INITIALIZE RDJFCB PARM LIST*/
         MVC   JFCBREAD(4),JFCBREAS                             02S0097
*          GEN (RDJFCB ((REG5)),MF=(E,JFCBREAD)); /* JFCB FOR VOLSER */
         RDJFCB ((REG5)),MF=(E,JFCBREAD)
*          RFY REG5 UNRSTD;            /* RELEASE DCB POINTER        */
*          MSGBUF(1:L363I+4)=          /* OBTAIN GENERAL MSG TEXT    */
*             NOTDAMSG;                /* FOR TEXT INSERTION         */
         L     @10,@CV00101                                     02S0100
         LA    @03,3                                            02S0100
         AH    @03,L363I(,@10)                                  02S0100
         L     @10,@CV00101                                     02S0100
         EX    @03,@SM01169                                     02S0100
*          MSGBUF(17:22)=              /* VOLSER FROM JFCB           */
*             JFCBVOLS(1:6);           /*  TO MESSAGE IEE363I        */
         ICM   @10,7,EXITLIST                                   02S0101
         MVC   MSGBUF+16(6),JFCBVOLS(@10)                       02S0101
*        END;                                                   02S0102
*      ELSE;                                                    02S0103
@RF00087 DS    0H                                               02S0104
*    END;                                                       02S0104
*  ELSE                                /* DEVTYPE FAILED             */
*    DO;                                                        02S0105
         B     @RC00084                                         02S0105
@RF00084 DS    0H                                               02S0106
*      ERROR='1'B;                     /*SET ERROR ENCOUNTERED SWITCH*/
         OI    ERROR,B'10000000'                                02S0106
*      MSGBUF(1:L365I+4)=              /* OBTAIN GENERAL MSG TEXT    */
*         NOTOPEN;                     /* FOR TEXT INSERTION         */
         L     @10,@CV00103                                     02S0107
         LA    @03,3                                            02S0107
         AH    @03,L365I(,@10)                                  02S0107
         L     @10,@CV00103                                     02S0107
         EX    @03,@SM01171                                     02S0107
*      MSGBUF(25)=XYARRAY(LOOP);       /* MSG TO REFLECT WHICH D.S.
*                                         HAD THE ERROR.             */
         SLR   @10,@10                                          02S0108
         IC    @10,LOOP                                         02S0108
         LA    @03,XYARRAY-1(@10)                               02S0108
         MVC   MSGBUF+24(1),0(@03)                              02S0108
*    END;                                                       02S0109
*END;                                                           02S0110
@RC00084 LA    @10,1                                            02S0110
         SLR   @03,@03                                          02S0110
         IC    @03,LOOP                                         02S0110
         ALR   @10,@03                                          02S0110
         STC   @10,LOOP                                         02S0110
         CH    @10,@CH00065                                     02S0110
         BNH   @DL00080                                         02S0110
@DC00080 DS    0H                                               02S0111
*IF ERROR='0'B THEN                    /* CONTINUE IF NO ERROR FOUND */
         TM    ERROR,B'10000000'                                01S0111
         BNZ   @RF00111                                         01S0111
*  DO;                                                          01S0112
*/*                                                                  */
*/********************************************************************/
*/*                                                                  */
*/*SEGMENT NAME= WTRINIT                                             */
*/*                                                                  */
*/*DESCRIPTIVE NAME= SMF WTR TASK - DISPATCHER                       */
*/*                                                                  */
*/*FUNCTION=                                                         */
*/*  * SET THE WTR DISPATCHABLE VIA ISSUANCE OF THE STATUS SVC.      */
*/*  * WAIT FOR WTR INITIALIZATION COMPLETION.                       */
*/*                                                                  */
*/* OPERATION= NO SPECIAL ALGORITHMS ARE EMPLOYED.                   */
*/*                                                                  */
*/*ENTRY POINT= WTRINIT                                              */
*/*                                                                  */
*/* INPUT= POINTER TO WTR TCB                                        */
*/*        POINTER TO WTR ATTACH ECB                                 */
*/*                                                                  */
*/*EXIT                                                              */
*/* CONDITION= PROCESSING COMPLETE                                   */
*/* OUTPUT= WTR TASK DISPATCHED                                      */
*/* RETURN CODE= N/A                                                 */
*/*                                                                  */
*/********************************************************************/
*/*                                                                  */
*ATTCHECB=WTRECBPT;                    /* WTR ATTACH ECB TO WAIT LIST*/
         L     @10,@PC00001+8                                   02S0113
         MVC   ATTCHECB(4),WTRECBPT(@10)                        02S0113
*WAITECB=ADDR(SMCABECB);               /* WTR POST ECB ADDRESS TOLIST*/
         LA    @10,SMCABECB(,SMCAPTR)                           02S0114
         STCM  @10,7,WAITECB                                    02S0114
*LASTIND='80'X;                        /* HIGH ORDER BIT INDICATES
*                                         LAST LIST ENTRY.           */
         MVI   LASTIND,X'80'                                    02S0115
*RFY REG5 RSTD;                        /* GET REGISTER CONTROL       */
*REG5=WTRTCBPT;                        /* GET WRITER TCB POINTER     */
         L     @10,@PC00001+4                                   02S0117
         L     REG5,WTRTCBPT(,@10)                              02S0117
*GEN (STATUS RESET,ND,(REG5),(12),E);  /* DISPATCH WTR TASK          */
         STATUS RESET,ND,(REG5),(12),E
*RFY REG5 UNRSTD;                      /* RELEASE REGISTER CONTROL   */
*GEN (WAIT 1,ECBLIST=WECBLIST);        /* WAIT FOR COMPLETION        */
         WAIT 1,ECBLIST=WECBLIST
*/*                                                                  */
*/********************************************************************/
*/*                                                                  */
*/*SEGMENT NAME= ERRORDET                                            */
*/*                                                                  */
*/*DESCRIPTIVE NAME= SMF WTR ERROR DETECTION ROUTINE                 */
*/*                                                                  */
*/*FUNCTION=                                                         */
*/*   * THIS SEGMENT WILL DETERMINE IF THE  SMF WTR ENCOUNTERED ANY  */
*/*     PROBLEMS WHILE INITIALIZING ITSELF, AND IT WILL CONSTRUCT    */
*/*     APPROPRIATE MESSAGES FOR THE ERROR CONDITIONS.  IF THE WTR'S */
*/*     ATTACH ECB IS POSTED, THE WTR TERMINATED IN IT'S INITIALIZA- */
*/*     TION,THERFORE NO RECORDING WILL BE ATTEMPTED.                */
*/*   * IF SMCADSNF IS ON, ONE OF THE SMF DATA SETS WAS MISSING.     */
*/*   * IF SMCAOPFL IS ON THEN A PROBLEM OCCURRED IN OPENING         */
*/*     ONE OF THE DATA SETS.                                        */
*/*                                                                  */
*/*OPERATION= NO SPECIAL ALGORITHMS ARE EMPLOYED.                    */
*/*                                                                  */
*/*ENTRY POINT=ERRORDET                                              */
*/*                                                                  */
*/* INPUT=                                                           */
*/*        SMCAUSER                                                  */
*/*        SMCADSNF                                                  */
*/*        SMCAOPFL                                                  */
*/*        WTR ATTACH ECB                                            */
*/*                                                                  */
*/*EXIT                                                              */
*/* CONDITION= WTR ATTACH ECB POSTED AND SMCASDNF,SMCAOPFL BITS=OFF  */
*/* OUTPUT= IEE480I MESSAGE TO MSGBUF                                */
*/*         ERROR SWITCH = ON                                        */
*/* RETURN CODE= N/A                                                 */
*/* CONDITION= SMCADSNF = ON (DATA SET NOT FOUND)                    */
*/* OUTPUT= IEE359I MESSAGE TO MSGBUF                                */
*/*         ERROR SWITCH = ON                                        */
*/* RETURN CODE= N/A                                                 */
*/* CONDITION= SMCAOPFL = ON (OPEN FAILURE)                          */
*/* OUTPUT= IEE365I MESSAGE TO MSGBUF                                */
*/*         ERROR SWITCH = ON                                        */
*/* RETURN CODE= N/A                                                 */
*/*                                                                  */
*/********************************************************************/
*/*                                                                  */
*IF POSTED='1'B THEN                   /* HAS THE WTR ATTACH ECB     */
         L     @10,@PC00001+8                                   02S0121
         L     @10,WTRECBPT(,@10)                               02S0121
         TM    POSTED(@10),B'01000000'                          02S0121
         BNO   @RF00121                                         02S0121
*  DO;                                 /* BEEN POSTED?               */
*    IF SMCADSNF='1'B THEN             /* WAS EITHER D.S. MISSING?   */
         TM    SMCADSNF(SMCAPTR),B'00000001'                    02S0123
         BNO   @RF00123                                         02S0123
*      DO;                                                      02S0124
*        ERROR='1'B;                   /*ERROR ENCOUNTERED SWITCH ON */
         OI    ERROR,B'10000000'                                02S0125
*        MSGBUF(1:L358I+4)=            /* OBTAIN GENERAL MSG TEXT    */
*         DSNOTFND;                    /* FOR TEXT INSERTION         */
         L     @10,@CV00102                                     02S0126
         LA    @03,3                                            02S0126
         AH    @03,L358I(,@10)                                  02S0126
         L     @10,@CV00102                                     02S0126
         EX    @03,@SM01173                                     02S0126
*        MSGBUF(25)=SMCAXORY;          /* DSN THAT HAD ERROR TO BUF. */
         MVC   MSGBUF+24(1),SMCAXORY(SMCAPTR)                   02S0127
*        MSGBUF(40:45)=SMCAPDEV;       /* VOLSER OF ERROR DSN        */
         MVC   MSGBUF+39(6),SMCAPDEV(SMCAPTR)                   02S0128
*      END;                                                     02S0129
*    ELSE                                                       02S0130
*      IF SMCAOPFL='1'B THEN           /* WERE BOTH DATA SETS OPENED?*/
         B     @RC00123                                         02S0130
@RF00123 TM    SMCAOPFL(SMCAPTR),B'00100000'                    02S0130
         BNO   @RF00130                                         02S0130
*        DO;                                                    02S0131
*          ERROR='1'B;                 /* SET ERROR SWITCH ON        */
         OI    ERROR,B'10000000'                                02S0132
*          MSGBUF(1:L365I+4)=          /* OBTAIN GENERAL MSG TEXT    */
*           NOTOPEN;                   /* FOR TEXT INSERTION         */
         L     @10,@CV00103                                     02S0133
         LA    @03,3                                            02S0133
         AH    @03,L365I(,@10)                                  02S0133
         L     @10,@CV00103                                     02S0133
         EX    @03,@SM01171                                     02S0133
*          MSGBUF(25)=SMCAXORY;        /* DISTINGUISH BETWEEN XORY   */
         MVC   MSGBUF+24(1),SMCAXORY(SMCAPTR)                   02S0134
*        END;                                                   02S0135
*      ELSE                                                     02S0136
*        DO;                                                    02S0136
         B     @RC00130                                         02S0136
@RF00130 DS    0H                                               02S0137
*          ERROR='1'B;                 /*ERROR ENCOUNTERED SWITCH ON */
         OI    ERROR,B'10000000'                                02S0137
*          MSGBUF(1:L480I+4)=          /* OBTAIN MESSAGE FOR ERROR   */
*             SMFAILED;                /* ROUTINES WTO               */
         L     @10,@CV00100                                     02S0138
         LA    @03,3                                            02S0138
         AH    @03,L480I(,@10)                                  02S0138
         L     @10,@CV00100                                     02S0138
         EX    @03,@SM01175                                     02S0138
*        END;                                                   02S0139
*  END;                                                         02S0140
@RC00130 DS    0H                                               02S0140
@RC00123 DS    0H                                               02S0141
*  END;                                                         01S0141
@RF00121 DS    0H                                               01S0142
*IF ERROR='1'B THEN                    /* DO ERROR PROCESSING IF ANY */
@RF00111 TM    ERROR,B'10000000'                                01S0142
         BNO   @RF00142                                         01S0142
*  DO;                                 /* PROBLEMS AT ALL WERE FOUND.*/
*/*                                                                  */
*/********************************************************************/
*/*                                                                  */
*/*SEGMENT NAME= ERRORRTN                                            */
*/*                                                                  */
*/*DESCRIPTIVE NAME= ERROR MSG ROUTINE                               */
*/*                                                                  */
*/*FUNCTION=                                                         */
*/*   * ISSUE THE MESSAGE CONTAINED IN MSGBUF.                       */
*/*   * FREE THE WORKING BUFFERS FOR LENGTH(SMCABSIZ*2)              */
*/*   * FREE THE MANX DCB CORE                                       */
*/*   * FREE THE MANY DCB CORE                                       */
*/*   * DELETE THE SVC'S USER EXIT IEFU83                            */
*/*                                                                  */
*/* OPERATION= NO SPECIAL ALGORITHMS ARE EMPLOYED.                   */
*/*                                                                  */
*/*ENTRY POINT= ERRORRTN                                             */
*/*                                                                  */
*/* INPUT=MSGBUF - CONTAINS MESSAGE TO BE ISSUED VIA WTOR            */
*/*       SMCABUFP                                                   */
*/*       SMCABSIZ                                                   */
*/*       SMCAPCDB                                                   */
*/*       SMCAAADCB                                                  */
*/*                                                                  */
*/*EXIT                                                              */
*/* CONDITION= ERROR PROCESSING COMPLETE FOR WTR INIT. FAILURE       */
*/* OUTPUT= SMCAUSER - OFF                                           */
*/*         SMCAMAN  - OFF                                           */
*/*         SMCAPDCB - 0                                             */
*/*         SMCAADCB - 0                                             */
*/*         MSG TO OPERATOR                                          */
*/* RETURN CODE= N/A                                                 */
*/*                                                                  */
*/********************************************************************/
*/*                                                                  */
*SMCAUSER='0'B;                        /* ALL RECORDING IS TURNED OFF*/
*SMCAMAN='0'B;                         /* FOR ANY OPEN ERROR.        */
         NI    SMCAUSER(SMCAPTR),B'00111111'                    02S0145
*GEN (WTO MF=(E,MSGBUF));              /* ISSUE MESSAGE              */
         WTO MF=(E,MSGBUF)
*IF SMCABUFP^=0 THEN                   /* WAS BUFFER ALLOCATED?      */
         SLR   @10,@10                                          02S0147
         L     @03,SMCABUFP(,SMCAPTR)                           02S0147
         CR    @03,@10                                          02S0147
         BE    @RF00147                                         02S0147
*  DO;                                                          02S0148
*    RFY (REG0,REG1,REG5) RSTD;        /* FREEMAIN SVC REGISTERS     */
*    REG1=SMCABUFP;                    /* START ADDRESS OF BUFFER    */
         LR    REG1,@03                                         02S0150
*    REG5=SMCABSIZ*2;                  /* BUFFERS TOTAL SIZE         */
         L     REG5,SMCABSIZ(,SMCAPTR)                          02S0151
         ALR   REG5,REG5                                        02S0151
