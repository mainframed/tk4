*/********************************************************************/
*/*                                                                  */
*/* ROUTINE NAME : READMAC                                           */
*/*                                                                  */
*/* FUNCTION :                                                       */
*/*  READMAC RECEIVES CONTROL ON READ OR SOLICIT FEEDBACK.           */
*/*  IF CRRSW0 IS SET, READMAC WILL QUEUE THE DATA OR ATTEMPT TO     */
*/*  SCHEDULE AN ASYNCHRONOUS INPUT EXIT.                            */
*/*                                                                  */
*/*  IF ENTRY WAS FOR A READ, THE FEEDBACK STATUS IS MOVED FROM      */
*/*  THE FSB TO THE CRR.  THE PARM LIST FOR THE MOVE ROUTINE-        */
*/*  ISTDCC60 - IS BUILT IN THE CRR USING THE INFORMATION IN THE     */
*/*  RPL OR IN THE DCLCP.  THE DCLCP IS USED IF THE DATA TO BE       */
*/*  MOVED WAS SAVED FROM A PREVIOUS REQUEST.                        */
*/*                                                                  */
*/* INPUT :  FSBPTR REG(4) - ADDRESS OF FSB                          */
*/*                                                                  */
*/* OUTPUT : SAME                                                    */
*/*                                                                  */
*/* SUBROUTINES CALLED : SETCRR   - COPY STATUS TO CRR               */
*/*                      MOVEDATA - MOVE DATA TO USER AREA           */
*/*                      XFRFSB   - QUEUE FSB TO DATA QUEUE          */
*/*                      SOLMAC1  - CHECK FOR ASYNCH INPUT EXIT      */
*/*                                                                  */
*/* MACROS USED :  NONE                                              */
*/*                                                                  */
*/********************************************************************/
*                                                                  0542
*READMAC:                                                          0542
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0542
         B     @PB00002                                            0542
READMAC  DS    0H                                                  0543
*   CRRRTN1=RTNREG;                 /* SAVE RETURN REGISTER          */
         ST    RTNREG,CRRRTN1(,CRRPTR)                             0543
*   DO WHILE(CRRSW1=OFF);           /*                       @D31KKNL*/
         B     @DE00544                                            0544
@DL00544 DS    0H                                                  0545
*     IF CRRSW0=OFF THEN            /* IF RPL EXISTS         @D31KKNL*/
         TM    CRRSW0(CRRPTR),B'10000000'                          0545
         BNZ   @RF00545                                            0545
*       DO;                         /* PROCESS THE DATA      @D31KKNL*/
*         DCLRPLA=ZEROWORD;         /* CLEAR RPL ADDRESS     @D31KKNL*/
         L     @15,CRRLCPB(,CRRPTR)                                0547
         SLR   @05,@05                                             0547
         ST    @05,DCLRPLA(,@15)                                   0547
*         CALL SETCRR;              /* COPY FEEDBACK STATUS  @D31KKNL*/
         BAL   @14,SETCRR                                          0548
*         CALL MOVEDATA;            /* MOVE DATA TO USER BUFFERS   0549
*                                                            @D31KKNL*/
         BAL   @14,MOVEDATA                                        0549
*         CRRSW1=ON;                /* END THIS LOOP         @D31KKNL*/
         OI    CRRSW1(CRRPTR),B'01000000'                          0550
*       END;                        /* END-RPL EXISTS        @D31KKNL*/
*     ELSE                                                         0552
*       DO;                         /* NO RPL EXISTS         @D31KKNL*/
         B     @RC00545                                            0552
@RF00545 DS    0H                                                  0553
*         IF(FMCFBA=ZEROWORD×FMCFBA=GATEBIT)&/* NO DATA              */
*             FMCFLGA1=OFF THEN     /* AND SPECIFIC MODE             */
         L     @15,CRRFMCB(,CRRPTR)                                0553
         L     @15,FMCFBA(,@15)                                    0553
         LTR   @15,@15                                             0553
         BZ    @GL00002                                            0553
         CL    @15,@CF01547                                        0553
         BNE   @RF00553                                            0553
@GL00002 L     @15,CRRFMCB(,CRRPTR)                                0553
         TM    FMCFLGA1(@15),B'01000000'                           0553
         BNZ   @RF00553                                            0553
*           CALL SOLMAC1;           /* CHECK ASY. INPUT              */
         BAL   @14,SOLMAC1                                         0554
*         ELSE                                                     0555
*           DO;                     /* THERE IS DATA                 */
         B     @RC00553                                            0555
@RF00553 DS    0H                                                  0556
*             CRRSW6=ON;            /* SET DATA EXISTS FLG           */
         OI    CRRSW6(CRRPTR),B'00000010'                          0556
*             CALL XFRFSB;          /* QUEUE PAGEABLE FSB            */
         BAL   @14,XFRFSB                                          0557
*             CRRSW1=ON;            /* SET EXIT SWITCH       @DM9361K*/
         OI    CRRSW1(CRRPTR),B'01000000'                          0558
*           END;                                                   0559
*       END;                        /* END - NO RPL EXISTS           */
@RC00553 DS    0H                                                  0561
*   END;                            /* END DO WHILE                  */
@RC00545 DS    0H                                                  0561
@DE00544 TM    CRRSW1(CRRPTR),B'01000000'                          0561
         BZ    @DL00544                                            0561
*   RTNREG=CRRRTN1;                 /* RESTORE RETURN REG            */
         L     RTNREG,CRRRTN1(,CRRPTR)                             0562
*   END READMAC;                                                   0563
@EL00002 DS    0H                                                  0563
@EF00002 DS    0H                                                  0563
@ER00002 BR    @14                                                 0563
*                                                                  0564
*/********************************************************************/
*/*                                                                  */
*/* ROUTINE NAME : WRTMAC                                            */
*/*                                                                  */
*/* FUNCTION :                                                       */
*/*  WRTMAC RECEIVES CONTROL FROM THE MAINLINE ROUTINE               */
*/*  TO PROCESS A WRITE FEEDBACK.  FEEDBACK STATUS IS                */
*/*  MOVED FROM THE FSB TO THE CRR.  IF THE WRITE WAS                */
*/*  CONVERSATIONAL AND INPUT WAS RECEIVED, THE MOVEDATA             */
*/*  SUBROUTINE IS CALLED TO PROCESS THE DATA.  IF THE               */
*/*  OPERATION WAS A NORMAL WRITE AND DATA WAS RECEIVED              */
*/*  THE FSB IS QUEUED IS THE FMCB DATA QUEUE AND THE                */
*/*  UNSOLICITED INPUT FLAG IS SET IN THE CRR.                       */
*/*                                                                  */
*/* INPUT : FSBPTR REG(4) - ADDRESS FSB                              */
*/*                                                                  */
*/* OUTPUT : SAME                                                    */
*/*                                                                  */
*/* SUBROUTINES CALLED : SETCRR   - COPY STATUS TO CRR               */
*/*                      MOVEDATA - MOVE DATA TO USER AREA           */
*/*                      FREEBUF  - FREE FSB CHAIN                   */
*/*                      XFRFSB   - QUEUE FSB TO DATA QUEUE          */
*/* MACROS USED : NONE                                               */
*/*                                                                  */
*/********************************************************************/
*                                                                  0564
*WRTMAC:                                                           0564
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0564
WRTMAC   DS    0H                                                  0565
*   CRRRTN1=RTNREG;                 /* SAVE RETURN REG               */
         ST    RTNREG,CRRRTN1(,CRRPTR)                             0565
*   CALL SETCRR;                    /* MOVE STATUS TO CRR    @D31KKNL*/
         BAL   @14,SETCRR                                          0566
*   RFY                                                            0567
*     FDBPTR RSTD;                                                 0567
*   FDBPTR=ADDR(FSBFDB);            /* COVER THE FDB DSECT           */
         LA    FDBPTR,FSBFDB(,FSBPTR)                              0568
*   IF RPLWROPT=ON THEN             /* WRITE CONVERSATIONAL          */
         L     @15,CRRARPL(,CRRPTR)                                0569
         TM    RPLWROPT(@15),B'00000001'                           0569
         BNO   @RF00569                                            0569
*     IF FDBDATCT>ZEROWORD THEN     /* DATA RECEIVED                 */
*                                                                  0570
         SLR   @15,@15                                             0570
         CH    @15,FDBDATCT(,FDBPTR)                               0570
         BNL   @RF00570                                            0570
*       /*************************************************************/
*       /*                                                           */
*       /* WRITE CONVERSATIONAL WITH DATA RECEIVED                   */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0571
*       DO;                         /* PROCESS IT            @DM9258K*/
*         DCLRPLA=ZEROWORD;         /* CLEAR RPL ADDRESS     @DM9258K*/
         L     @09,CRRLCPB(,CRRPTR)                                0572
         ST    @15,DCLRPLA(,@09)                                   0572
*         CALL MOVEDATA;            /* MOVE DATA TO USER BUFFERS   0573
*                                                            @D31KKNL*/
         BAL   @14,MOVEDATA                                        0573
*       END;                        /* END-DATA REC'D        @DM9258K*/
*     ELSE                                                         0575
*       DO;                                                        0575
*                                                                  0575
         B     @RC00570                                            0575
@RF00570 DS    0H                                                  0576
*         /***********************************************************/
*         /*                                                         */
*         /* FINISHED PROCESSING-WRITE CONVERSATIONAL BUT NO DATA    */
*         /* RECEIVED                                                */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0576
*         CRRPOST=ON;               /* SET DO POST SWITCH            */
         OI    CRRPOST(CRRPTR),B'00010000'                         0576
*         CALL EOBANAL;             /*                       @ZA28667*/
         BAL   @14,EOBANAL                                         0577
*         DCLRPLA=ZEROWORD;         /* CLEAR RPL ADDRESS     @ZA28667*/
         L     @15,CRRLCPB(,CRRPTR)                                0578
         SLR   @09,@09                                             0578
         ST    @09,DCLRPLA(,@15)                                   0578
*         IF CRRRTNCD=ZEROWORD THEN /* IF NO ERROR                   */
         CLI   CRRRTNCD(CRRPTR),0                                  0579
         BNE   @RF00579                                            0579
*           CRRRTNCD=CRRNGRCC;      /* SET CONDITIONAL OPERATION     */
         MVI   CRRRTNCD(CRRPTR),X'10'                              0580
*         CALL FREEBUF;                                            0581
@RF00579 BAL   @14,FREEBUF                                         0581
*       END;                                                       0582
*   ELSE                                                           0583
*     DO;                                                          0583
*                                                                  0583
         B     @RC00569                                            0583
@RF00569 DS    0H                                                  0584
*       /*************************************************************/
*       /*                                                           */
*       /* NONCONVERSATIONAL WRITE                                   */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0584
*       CRRPOST=ON;                 /* SET DO POST SWITCH            */
*       CRRENDLW=ON;                /* SET END OF IO LCCW            */
         OI    CRRPOST(CRRPTR),B'00110000'                         0585
*       IF FDBDATCT>ZEROWORD THEN   /* DATA RECEIVED                 */
         SLR   @15,@15                                             0586
         CH    @15,FDBDATCT(,FDBPTR)                               0586
         BNL   @RF00586                                            0586
*         DO;                                                      0587
*                                                                  0587
*           /*********************************************************/
*           /*                                                       */
*           /* DATA RECEIVED-TREAT AS UNSOLICITED INPUT              */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0588
*           IF FSBELI=OFF THEN      /* CHECK FOR END OF LCP          */
         TM    FSBELI(FSBPTR),B'10000000'                          0588
         BNZ   @RF00588                                            0588
*             DO;                                                  0589
*               DCLRPLA=ZEROWORD;                                  0590
         L     @09,CRRLCPB(,CRRPTR)                                0590
         ST    @15,DCLRPLA(,@09)                                   0590
*               CRRENDLW=OFF;       /* RESET END OF LCP FLAG         */
         NI    CRRENDLW(CRRPTR),B'11011111'                        0591
*             END;                                                 0592
*           ELSE                                                   0593
*             ;                     /* LEAVE END OF LCP SW SET       */
@RF00588 DS    0H                                                  0594
*           FSBECI=OFF;                                            0594
         NI    FSBECI(FSBPTR),B'10111111'                          0594
*           CRRUINPT=ON;            /* INDICATE UNSOLICITED INPUT    */
*           CRRRDEOB=OFF;           /* RESET EOB                     */
*           CRRRDEOM=OFF;           /* RESET EOM                     */
*           CRRRDEOT=OFF;           /* RESET EOT                     */
*           CRRRLG=OFF;             /* RESET LEADING GRAPHICS        */
         OI    CRRUINPT(CRRPTR),B'10000000'                        0599
         NI    CRRRDEOB(CRRPTR),B'11000101'                        0599
*           CALL XFRFSB;            /* SAVE FSB ON FMCB DATA Q       */
         BAL   @14,XFRFSB                                          0600
*           FSBELI=ON;                                             0601
         OI    FSBELI(FSBPTR),B'10000000'                          0601
*         END;                                                     0602
*       ELSE                                                       0603
*                                                                  0603
*         /***********************************************************/
*         /*                                                         */
*         /* NORMAL WRITE COMPLETED                                  */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0603
*         CALL FREEBUF;                                            0603
         B     @RC00586                                            0603
@RF00586 BAL   @14,FREEBUF                                         0603
*     END;                                                         0604
@RC00586 DS    0H                                                  0605
*   RFY                                                            0605
*     FDBPTR UNRSTD;                                               0605
@RC00569 DS    0H                                                  0606
*   RTNREG=CRRRTN1;                 /* RESTORE RETURN REG            */
         L     RTNREG,CRRRTN1(,CRRPTR)                             0606
*   END WRTMAC;                                                    0607
@EL00003 DS    0H                                                  0607
@EF00003 DS    0H                                                  0607
@ER00003 BR    @14                                                 0607
*                                                                  0608
