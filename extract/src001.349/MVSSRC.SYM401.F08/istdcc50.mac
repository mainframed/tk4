         L     GPR02P,CRRFMCB(,CRRPTR)                             0498
         LA    GPR02P,FMCDCLOK(,GPR02P)                            0498
*         GPR04P=GPR01P->RPHCRR;    /* SET PARAM - ISTCRA PTR        */
         L     GPR04P,RPHCRR(,GPR01P)                              0499
*         GPR00P=CRALMS08;          /* SET PARAM - LOCK LEVEL FLAG   */
         LA    GPR00P,128                                          0500
*         GPR03P=ADDR(GPR04P->CRAL8PTR);/* SET PARAM - LOCK LEVEL PTR*/
         LA    GPR03P,CRAL8PTR(,GPR04P)                            0501
*         GPR15P=ATCLCPTR->ATCAP36; /* ADDRESS OF LOCK ROUTINE       */
         L     @05,ATCLCPTR                                        0502
         L     GPR15P,ATCAP36(,@05)                                0502
*         DCL                                                      0503
*           FMZL0007 BASED(GPR15P) LABEL VALUERANGE(*);            0503
*         CALL FMZL0007;                                           0504
         BALR  @14,@15                                             0504
*         GENERATE REFS(GPR00P,GPR01P,GPR14P,RPHWORK) SETS(GPR00P, 0505
*             GPR01P,GPR02P,GPR03P,GPR04P,GPR05P,GPR06P,GPR07P,GPR08P,
*             GPR09P,GPR10P,GPR11P,GPR12P,GPR13P,GPR14P,GPR15P);   0505
         LM GPR00P,GPR15P,RPHWORK(GPR01P)
*         RFY                                                      0506
*          (GPR00P,                                                0506
*           GPR01P,                                                0506
*           GPR02P,                                                0506
*           GPR03P,                                                0506
*           GPR04P,                                                0506
*           GPR15P) UNRSTD;                                        0506
*       END;                                                       0507
*                                                                  0507
*/*** END -TPLOCK (CTLLOCK)TYPE(XCLSIVE)- **                         */
*                                                                  0508
*                                   /* GET LOCK              @ZA33377*/
*       CRRFMCLK=ON;                /* FMCB LOCK HELD        @ZA33377*/
         OI    CRRFMCLK(CRRPTR),B'10000000'                        0508
*     END;                          /* LOCK FMCB             @ZA33377*/
*   IF FMCFLGA3=ON THEN             /* FLUSH BIT ON IN FMCB          */
@RF00493 L     @15,CRRFMCB(,CRRPTR)                                0510
         TM    FMCFLGA3(@15),B'00010000'                           0510
         BNO   @RF00510                                            0510
*     IF FSBMJRCD='00'X THEN        /* RETURN CODE NORMAL?   @ZA33102*/
*                                                                  0511
         CLI   FSBMJRCD(FSBPTR),X'00'                              0511
         BNE   @RF00511                                            0511
*       /*************************************************************/
*       /*                                                           */
*       /* THROW AWAY FSB                                            */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0512
*       CALL FLUSH;                                                0512
         BAL   @14,FLUSH                                           0512
*     ELSE                          /*                       @ZA33102*/
*       DO;                         /* TURN OFF FLUSH BIT    @ZA33102*/
         B     @RC00511                                            0513
@RF00511 DS    0H                                                  0514
*         RFY                                                      0514
*          (REG2,                                                  0514
*           REG3) RSTD;             /*                       @ZA33102*/
*CS10:                                                             0515
*         REG2=FMCCMPS2;            /* GET FLAG VALUE        @ZA33102*/
CS10     L     @15,CRRFMCB(,CRRPTR)                                0515
         L     REG2,FMCCMPS2(,@15)                                 0515
*         REG3=REG2&FLGA3OFF;       /* RESET FLUSH FLAG      @ZA33102*/
         LR    REG3,REG2                                           0516
         N     REG3,@CF01563                                       0516
*         CS(REG2,REG3,FMCCMPS2);   /* SET NEW VAL IN FMCB   @ZA33102*/
         CS    REG2,@03,FMCCMPS2(@15)                              0517
*         BC(7,CS10);               /* TRY AGAIN IF FAILURE  @ZA33102*/
         BC    7,CS10                                              0518
*         RFY                                                      0519
*          (REG2,                                                  0519
*           REG3) UNRSTD;           /*                       @ZA33102*/
*       END;                        /* TURN OFF FLUSH BIT    @ZA33102*/
*   ELSE                                                           0521
*     ;                                                            0521
@RF00510 DS    0H                                                  0522
*   IF CRRSW4=ON THEN               /* IF NOTHING FLUSHED            */
@RC00510 TM    CRRSW4(CRRPTR),B'00001000'                          0522
         BNO   @RF00522                                            0522
*     DO;                                                          0523
*       IF DCLRPLA=ZEROWORD THEN    /* RPL PTR IN DCLCP      @DM9272K*/
         L     @15,CRRLCPB(,CRRPTR)                                0524
         L     @15,DCLRPLA(,@15)                                   0524
         LTR   @15,@15                                             0524
         BNZ   @RF00524                                            0524
*         CRRSW0=ON;                /* REQUEST IS SOLICIT            */
         OI    CRRSW0(CRRPTR),B'10000000'                          0525
*       ELSE                                                       0526
*         ;                                                        0526
@RF00524 DS    0H                                                  0527
*       IF DCLRQTP=DCLREAD×CRRSW0=ON THEN                          0527
*                                                                  0527
         L     @15,CRRLCPB(,CRRPTR)                                0527
         CLI   DCLRQTP(@15),X'1D'                                  0527
         BE    @RT00527                                            0527
         TM    CRRSW0(CRRPTR),B'10000000'                          0527
         BNO   @RF00527                                            0527
@RT00527 DS    0H                                                  0528
*         /***********************************************************/
*         /*                                                         */
*         /* READ/SOL REQUEST                                        */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0528
*         CALL READMAC;                                            0528
         BAL   @14,READMAC                                         0528
*       ELSE                                                       0529
*                                                                  0529
*         /***********************************************************/
*         /*                                                         */
*         /* WRITE REQUEST                                           */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0529
*         CALL WRTMAC;                                             0529
         B     @RC00527                                            0529
@RF00527 BAL   @14,WRTMAC                                          0529
*     END;                                                         0530
*   ELSE                                                           0531
*     ;                             /* FSB WAS FLUSHED               */
@RF00522 DS    0H                                                  0532
*/*TPESC                                                             */
*                                                                  0532
*   DO;                                                            0532
*                                                                  0532
@RC00522 DS    0H                                                  0533
*/* ATBUILD (ISTDYPAB,ISTAVT,ISTATCVT)                               */
*/* END OF ATBUILD                                                   */
*                                                                  0533
*     RFY                                                          0533
*       GPR15P RSTD;                                               0533
*     DO;                           /* LISATCVT                    0534
*                                      CVTBASE(PAGEABLE,(GPR15P))    */
*       GPR15P=ATCLCPTR;            /* SET UP ATCVT POINTER          */
         L     GPR15P,ATCLCPTR                                     0535
*     END;                          /* LISATCVT                      */
*     GPR15P=GPR15P->ATCESC01;                                     0537
         L     GPR15P,ATCESC01(,GPR15P)                            0537
*     DCL                                                          0538
*       FMZL0012 BASED(GPR15P) LABEL VALUERANGE(*);                0538
*     CALL FMZL0012;                                               0539
         BALR  @14,@15                                             0539
*     RESPECIFY                                                    0540
