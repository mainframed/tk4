*     /***************************************************************/
*     /*                                                             */
*     /* RELSTORE BUFADDR(FSBPTR)CLEAR(YES)CHAIN(YES)                */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0990
*     DO;                                                          0990
*                                                                  0990
         B     @RC00969                                            0990
@RF00969 DS    0H                                                  0991
*/* ATBUILD (ISTATCVT,ISTRPH)                                        */
*/* END OF ATBUILD                                                   */
*                                                                  0991
*       RFY                                                        0991
*        (GPR02P,                                                  0991
*         GPR03P,                                                  0991
*         GPR04P,                                                  0991
*         GPR15P) RSTD;                                            0991
*       RFY                                                        0992
*        (GPR01P,                                                  0992
*         GPR13P) RSTD;                                            0992
*       GENERATE REFS(GPR01P,GPR02P,GPR13P) SETS(RPHWORK);         0993
          STM GPR02P,GPR13P,RPHWORK(GPR01P)
*       RFY                                                        0994
*         GPR13P UNRSTD;                                           0994
*       GPR04P=FSBPTR×'80000000'X;                                 0995
         LR    @05,FSBPTR                                          0995
         O     @05,@CF01547                                        0995
         LR    GPR04P,@05                                          0995
*       DCL                                                        0996
*         1 FMZA0118 FIXED(31) STATIC LOCAL,                       0996
*          2 * FIXED(8) INIT(80),                                  0996
*          2 * PTR(24) INIT(0);                                    0996
*       GPR03F=FMZA0118;                                           0997
         L     GPR03F,FMZA0118                                     0997
