*       CS(REG2,REG3,FMCCMPS2);     /* SET NEW VALUE IN FMCB         */
         CS    REG2,@03,FMCCMPS2(@15)                              1108
*       BC(7,CS6);                  /* TRY AGAIN IF FAILURE          */
         BC    7,CS6                                               1109
*       RFY                                                        1110
*        (REG2,                                                    1110
*         REG3) UNRSTD;                                            1110
