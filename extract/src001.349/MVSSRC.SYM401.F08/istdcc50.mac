         NI    CRRSW0(CRRPTR),B'01111110'                          0878
*           END;                                                   0879
*       END;                                                       0880
@RF00861 DS    0H                                                  0881
*   END;                                                           0881
@RC00688 DS    0H                                                  0881
@DE00674 TM    CRRSW7(CRRPTR),B'00000001'                          0881
         BO    @DL00674                                            0881
*   RTNREG=CRRRTN3;                 /* RESTORE RETURN REG            */
         L     RTNREG,CRRRTN3(,CRRPTR)                             0882
*   END SOLMAC1;                                                   0883
@EL00005 DS    0H                                                  0883
@EF00005 DS    0H                                                  0883
