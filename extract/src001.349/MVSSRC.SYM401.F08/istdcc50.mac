*/********************************************************************/
*/*                                                                  */
*/* ROUTINE NAME : EOBANAL                                           */
*/*                                                                  */
*/* FUNCTION :                                                       */
*/*  EOBANAL DETERMINES IF AN EOB, EOM, OR EOT HAS BEEN  RECIEVED.   */
*/*   IF SO, THE DATA EXPECTED FLAG IS SET OFF IN THE FSB AND THE    */
*/*   END OF I/O LCCW IS SET ON IN THE CRR.                          */
*/*                                                                  */
*/* INPUT :  FSBPTR REG(4) - ADDRESS OF FSB                          */
*/*                                                                  */
*/* OUTPUT : SAME                                                    */
*/*                                                                  */
*/* SUBROUTINES CALLED : NONE                                        */
*/*                                                                  */
*/* MACROS USED : NONE                                               */
*/*                                                                  */
*/********************************************************************/
*                                                                  0884
*EOBANAL:                                                          0884
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0884
EOBANAL  DS    0H                                                  0885
*   IF FSBELI=ON THEN               /* CHECK FOR END OF LCP          */
         TM    FSBELI(FSBPTR),B'10000000'                          0885
         BNO   @RF00885                                            0885
*     DO;                           /* END OF LCP                    */
*       RFY                                                        0887
*        (REG2,                                                    0887
*         REG3) RSTD;                                              0887
*CS3:                                                              0888
*       REG2=FMCCMPS2;              /* REFERENCE FLAG VALUE          */
CS3      L     @15,CRRFMCB(,CRRPTR)                                0888
         L     REG2,FMCCMPS2(,@15)                                 0888
*       REG3=REG2&FLGA0OFF;         /* RESET DATA EXPECTED           */
         LR    REG3,REG2                                           0889
         N     REG3,@CF01545                                       0889
*       CS(REG2,REG3,FMCCMPS2);     /* SET NEW VALUE IN FMCB         */
         CS    REG2,@03,FMCCMPS2(@15)                              0890
*       BC(7,CS3);                  /* TRY AGAIN IF FAILURE          */
         BC    7,CS3                                               0891
*       RFY                                                        0892
*        (REG2,                                                    0892
*         REG3) UNRSTD;                                            0892
*       CRRENDLW=ON;                /* SET END OF I/O LCCW IN CRR    */
         OI    CRRENDLW(CRRPTR),B'00100000'                        0893
*     END;                                                         0894
*   ELSE                                                           0895
*     ;                             /* MORE DATA YET TO COME         */
@RF00885 DS    0H                                                  0896
*   END EOBANAL;                                                   0896
@EL00006 DS    0H                                                  0896
@EF00006 DS    0H                                                  0896
@ER00006 BR    @14                                                 0896
*                                                                  0897
*/********************************************************************/
*/*                                                                  */
*/* ROUTINE NAME : XFRFSB                                            */
*/*                                                                  */
*/* FUNCTION :                                                       */
*/*  THE FSB IS MOVED TO PAGEABLE STORAGE AND QUEUED TO THE FMCB     */
*/*   DATA QUEUE AND THE NUMBER OF BUFFERS IN THE CHAIN IS ADDED     */
*/*   TO THE BUFFER COUNT IN THE FMCB.  A RESET COMMAND IS ISSUED    */
*/*   IF SUFFICIENT BUFFERS EXIST AND NO PURGE IS IN PROGRESS.       */
*/*   DATA WILL BE THROWN AWAY IF FLUSH IS INDICATED.                */
*/*                                                                  */
*/* INPUT : FSBPTR REG(4) - ADDRESS OF FSB                           */
*/*                                                                  */
*/* OUTPUT : SAME                                                    */
*/*                                                                  */
*/* SUBROUTINES CALLED : EOBANAL  - CHECK FOR ENDING STATUS          */
*/*                      ISTDCC61 - MOVE FSB TO PAGEABLE STORAGE     */
*/*                      LCPBINIT - BUILD AND SCHEDULE RESET LCPB    */
*/*                      FLUSH    - FREE DATA BUFFERS                */
*/*                                                                  */
*/* MACROS USED : TPQUE                                              */
*/*                                                                  */
*/********************************************************************/
*                                                                  0897
*XFRFSB:                                                           0897
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0897
XFRFSB   DS    0H                                                  0898
*   CRRRTN9=RTNREG;                 /* SAVE RETURN REG               */
         ST    RTNREG,CRRRTN9(,CRRPTR)                             0898
*   CALL EOBANAL;                   /* CHECK FOR LAST FSB            */
         BAL   @14,EOBANAL                                         0899
*   FSBELI=OFF;                     /* CLEAR UNSOL INPUT FLAG        */
*   FSBECI=OFF;                     /* CLEAR UNUSED FLAG             */
*                                                                  0901
         NI    FSBELI(FSBPTR),B'00111111'                          0901
*   /*****************************************************************/
*   /*                                                               */
*   /* MOVE FSB TO PAGEABLE BUFFER                                   */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0902
*   RFY                                                            0902
*     CVTPTR RSTD;                  /* RESTRICT PTR TO ATCVT         */
*   CVTPTR=CRRATCVT;                /* SET PTR FOR ATCVT             */
         L     CVTPTR,CRRATCVT(,CRRPTR)                            0903
*   CRRPTR=ADDR(CRRSAVE1);          /* SAVEAREA FOR MOVE ROUTINE     */
         LA    @15,CRRSAVE1(,CRRPTR)                               0904
         LR    CRRPTR,@15                                          0904
*   CALL MOVE61;                    /* MOVE FSB TO PAGEABLE BUFFER   */
         L     @15,ATCDCC61(,CVTPTR)                               0905
         BALR  @14,@15                                             0905
*   RFY                                                            0906
*     CVTPTR UNRSTD;                /* UNRESTRICT PTR TO ATCVT       */
*   CRRPTR=ADDR(CRRSPACE);          /* REESTABLISH BASE              */
         L     CRRPTR,RPHCRR(,RPHPTR)                              0907
         LA    CRRPTR,CRRSPACE+312(,CRRPTR)                        0907
*   FSBPTR=CRRFSB;                  /* GET NEW FSB (PAGEABLE) ADDRESS*/
*                                                                  0908
         L     FSBPTR,CRRFSB(,CRRPTR)                              0908
*   /*****************************************************************/
*   /*                                                               */
*   /* ACCUMULATE COUNT OF BUFFERS IN USE. IF BUFFER LIMIT REACHED,  */
*   /* RESET ANY OUTSTANDING I/O.                                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0909
*   RFY                                                            0909
*    (COUNTER,                                                     0909
*     FDBPTR) RSTD;                                                0909
*   FDBPTR=ADDR(FSBFDB);            /* REFERENCE FIRST FDB           */
         LA    FDBPTR,FSBFDB(,FSBPTR)                              0910
*   COUNTER=0;                      /* INITIALIZE BUFFER ACCUMULATOR */
         SLR   COUNTER,COUNTER                                     0911
*   DO WHILE FDBPTR^=ZEROWORD;      /* LOOP THROUGH ALL FDB'S        */
         B     @DE00912                                            0912
@DL00912 DS    0H                                                  0913
*     COUNTER=COUNTER+1;            /* COUNT BUFFERS USED            */
         AL    COUNTER,@CF00067                                    0913
*     FDBPTR=FDBFDBA;               /* REFERENCE NEXT FDB            */
         L     FDBPTR,FDBFDBA(,FDBPTR)                             0914
*   END;                                                           0915
@DE00912 LTR   FDBPTR,FDBPTR                                       0915
         BNZ   @DL00912                                            0915
*   RFY                                                            0916
*    (REG2,                                                        0916
*     REG3) RSTD;                                                  0916
*CS4:                                                              0917
*   REG2=FMCCMPS1;                  /* GET CURRENT BUFFER COUNT      */
CS4      L     @15,CRRFMCB(,CRRPTR)                                0917
         L     REG2,FMCCMPS1(,@15)                                 0917
*   REG3=REG2+COUNTER;              /* UPDATE BUFFER COUNT           */
         LR    REG3,REG2                                           0918
         ALR   REG3,COUNTER                                        0918
*   CS(REG2,REG3,FMCCMPS1);         /* SET NEW COUNT IN FMCB         */
         CS    REG2,@03,FMCCMPS1(@15)                              0919
*   BC(7,CS4);                      /* TRY AGAIN IF FAILURE          */
         BC    7,CS4                                               0920
*   RFY                                                            0921
*    (COUNTER,                                                     0921
*     FDBPTR,                                                      0921
*     REG2,                                                        0921
*     REG3) UNRSTD;                                                0921
*   FSBPTR=CRRFSB;                  /* RESTORE FSB POINTER (USED AS
*                                      COUNTER)                      */
         L     FSBPTR,CRRFSB(,CRRPTR)                              0922
*/*  TPQUE ((FSBPTR),FMCFBA)                                         */
*                                                                  0923
*   DO;                                                            0923
*     RFY                                                          0924
*      (GPR00P,                                                    0924
*       GPR01P,                                                    0924
*       GPR02P,                                                    0924
*       GPR14P,                                                    0924
*       GPR15P) RSTD;                                              0924
*     GEN REFS(GPR01P) SETS(RPHWORK)(STM GPR02P,GPR08P,RPHWORK(GPR01P))
*         ;                                                        0925
         STM GPR02P,GPR08P,RPHWORK(GPR01P)
*     GPR15P=(FSBPTR);              /* GET WORK ELEM POINTER         */
         LR    GPR15P,FSBPTR                                       0926
*     GPR14P=ADDR(FMCFBA);          /* ADDRESS PAB/QUEUE POINTER     */
         L     GPR14P,CRRFMCB(,CRRPTR)                             0927
         LA    GPR14P,FMCFBA(,GPR14P)                              0927
*     GPR15P=GPR15P×PABWEQG1;                                      0928
         O     GPR15P,@CF01547                                     0928
*     GPR02P=GPR14P->PABWEQA;       /* INITIALIZE WITH QUEUE HEADER  */
         L     GPR02P,PABWEQA(,GPR14P)                             0929
*CMZA0096:                                                         0930
*     ;                             /* CS FAIL TARGET                */
CMZA0096 DS    0H                                                  0931
*     GPR00P=GPR02P;                /* SAVE QUEUE CONTENTS           */
         LR    GPR00P,GPR02P                                       0931
*     GPR15P->PABCHAIN=GPR02P×PABWEQG1;/* FORWARD CHAIN WORK ELEMENT */
         LR    @05,GPR02P                                          0932
         O     @05,@CF01547                                        0932
         ST    @05,PABCHAIN(,GPR15P)                               0932
*     CS(GPR02P,GPR15P,GPR14P->PABWEQA);/* CHECK FOR END OF QUEUE    */
         CS    GPR02P,@15,PABWEQA(GPR14P)                          0933
*     BC(7,CMZA0096);                                              0934
         BC    7,CMZA0096                                          0934
*     GEN REFS(RPHWORK,GPR01P) NOSETS                              0935
*         (LM    GPR02P,GPR08P,RPHWORK(GPR01P));                   0935
         LM    GPR02P,GPR08P,RPHWORK(GPR01P)
*     RFY                                                          0936
*      (GPR00P,                                                    0936
*       GPR01P,                                                    0936
*       GPR02P,                                                    0936
*       GPR14P,                                                    0936
*       GPR15P) UNRSTD;                                            0936
*   END;                            /* *** END OF TPQUE GENERATION 0937
*                                      SAVE FSB ON FMCB DATA Q       */
*   IF FMCBUFNB>FMCBUFLM THEN       /* BUF NUMBER EXCEEDS LIMIT?     */
         L     @15,CRRFMCB(,CRRPTR)                                0938
         CLC   FMCBUFNB(2,@15),FMCBUFLM(@15)                       0938
         BNH   @RF00938                                            0938
*     IF FMCFLGA5=OFF THEN          /* BUFFER THRESHOLD EXCEEDED?    */
         TM    FMCFLGA5(@15),B'00000100'                           0939
         BNZ   @RF00939                                            0939
*       IF FMCFLGA4=OFF THEN        /* PURGE IN PROGRESS?            */
         TM    FMCFLGA4(@15),B'00001000'                           0940
         BNZ   @RF00940                                            0940
*         IF FMCZF019=OFF THEN      /* RESET ORDERLY         @ZA28324*/
         TM    FMCZF019(@15),B'01000000'                           0941
         BNZ   @RF00941                                            0941
*           DO;                     /* IN PROGRESS           @ZA28324*/
*             RFY                                                  0943
*              (REG2,                                              0943
*               REG3) RSTD;                                        0943
*CS5:                                                              0944
*             REG2=FMCCMPS2;        /* REFERENCE FLAG VALUE          */
CS5      L     @15,CRRFMCB(,CRRPTR)                                0944
         L     REG2,FMCCMPS2(,@15)                                 0944
*             REG3=REG2×FLGA5ON;    /* BUFFER THRESHOLD EXCEEDED     */
         LR    REG3,REG2                                           0945
         O     REG3,@CF01561                                       0945
*             CS(REG2,REG3,FMCCMPS2);/* SET NEW VALUE IN FMCB        */
         CS    REG2,@03,FMCCMPS2(@15)                              0946
*             BC(7,CS5);            /* TRY AGAIN IF FAILURE          */
         BC    7,CS5                                               0947
*             RFY                                                  0948
*              (REG2,                                              0948
*               REG3) UNRSTD;                                      0948
*             DCLBTEIS=ON;          /* THRESHOLD EXCEEDED            */
*                                                                  0949
         L     @15,CRRLCPB(,CRRPTR)                                0949
         OI    DCLBTEIS(@15),B'00010000'                           0949
*             /*******************************************************/
*             /*                                                     */
*             /* CALL LCPBINIT TO GET STORAGE FOR A LCPB. BUILD RESET*/
*             /* ACCORDING TO DEVICE TYPE AND QUEUE ON TPIOS OUTBOUND*/
*             /* PAB WITH RESET OPTION.                              */
*             /*                                                     */
*             /*******************************************************/
*                                                                  0950
*             CALL LCPBINIT;        /* GET LCPB FOR RESET            */
         BAL   @14,LCPBINIT                                        0950
*           END;                                                   0951
*         ELSE                                                     0952
*           ;                       /* ALREADY EXCEEDED              */
@RF00941 DS    0H                                                  0953
*       ELSE                                                       0953
*         ;                         /* BUFF THRESHOLD NOT PASSED     */
@RF00940 DS    0H                                                  0954
*     ELSE                                                         0954
*       ;                           /* PURGE IN PROGRESS             */
@RF00939 DS    0H                                                  0955
*   IF CRRSW6=ON THEN               /* WAS FSB QUEUED                */
@RF00938 TM    CRRSW6(CRRPTR),B'00000010'                          0955
         BNO   @RF00955                                            0955
*     IF FMCFLGA3=ON THEN           /* FLUSH FLAG SET?               */
*                                                                  0956
         L     @15,CRRFMCB(,CRRPTR)                                0956
         TM    FMCFLGA3(@15),B'00010000'                           0956
         BNO   @RF00956                                            0956
*       /*************************************************************/
*       /*                                                           */
*       /* IF DATA FROM A SOLICIT AND THE FLUSH INDICATOR NOW SET,   */
*       /* FLUSH FSB UNTIL EOB.                                      */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0957
*       CALL FLUSH;                 /* FLUSH DATA TILL EOB           */
         BAL   @14,FLUSH                                           0957
*     ELSE                                                         0958
*       ;                           /* NO FLUSH                      */
@RF00956 DS    0H                                                  0959
*   ELSE                                                           0959
*     ;                                                            0959
@RF00955 DS    0H                                                  0960
*   RTNREG=CRRRTN9;                 /* RESTORE RETURN REG            */
         L     RTNREG,CRRRTN9(,CRRPTR)                             0960
*   END XFRFSB;                                                    0961
@EL00007 DS    0H                                                  0961
@EF00007 DS    0H                                                  0961
@ER00007 BR    @14                                                 0961
*                                                                  0962
*/********************************************************************/
*/*                                                                  */
*/* ROUTINE NAME : FREEBUF                                           */
*/*                                                                  */
*/* FUNCTION :                                                       */
*/*  FREEBUF IS CALLED TO FREE THE FSB AND FDBS.  THE RELSTORE       */
*/*   MACRO IS ISSUED WITH THE ERASE OPTION IF THE FDB CONTAINED     */
*/*   CONFIDENTIAL DATA.                                             */
*/* INPUT : FSBPTR REG(4) - ADDRESS OF FSB                           */
*/*                                                                  */
*/* OUTPUT :  SAME                                                   */
*/*                                                                  */
*/* SUBROUTINES CALLED :   NONE                                      */
*/*                                                                  */
*/* MACROS USED :   RELSTORE                                         */
*/*                                                                  */
*/********************************************************************/
*                                                                  0962
*FREEBUF:                                                          0962
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0962
FREEBUF  DS    0H                                                  0963
*   CRRRTN7=RTNREG;                 /* SAVE RETURN REGISTER          */
         ST    RTNREG,CRRRTN7(,CRRPTR)                             0963
*   RFY                                                            0964
*     FDBPTR RSTD;                                                 0964
*   FDBPTR=ADDR(FSBFDB);            /* ADDR OF FDB                   */
         LA    FDBPTR,FSBFDB(,FSBPTR)                              0965
*   FSBPTR=FDBPTR-FDBOFSET;         /* COMPUTE AREA TO FREE          */
         LR    FSBPTR,FDBPTR                                       0966
         SH    FSBPTR,FDBOFSET(,FDBPTR)                            0966
*   FSBPTR->FDBFDBA=FDBFDBA;        /* SET UP CHAIN OF FDB'S         */
         L     @15,FDBFDBA(,FDBPTR)                                0967
         ST    @15,FDBFDBA(,FSBPTR)                                0967
*   RFY                                                            0968
*     FDBPTR UNRSTD;                                               0968
*   IF PROCFTX=OFF THEN             /* NOT CONF TEXT                 */
*                                                                  0969
         L     @15,CRRFMCB(,CRRPTR)                                0969
         TM    PROCFTX+48(@15),B'00000001'                         0969
         BNZ   @RF00969                                            0969
*     /***************************************************************/
*     /*                                                             */
*     /* RELSTORE BUFADDR(FSBPTR)CHAIN(YES)                          */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0970
*     DO;                                                          0970
*                                                                  0970
*/* ATBUILD (ISTATCVT,ISTRPH)                                        */
*/* END OF ATBUILD                                                   */
*                                                                  0971
*       RFY                                                        0971
*        (GPR02P,                                                  0971
*         GPR03P,                                                  0971
*         GPR04P,                                                  0971
*         GPR15P) RSTD;                                            0971
*       RFY                                                        0972
*        (GPR01P,                                                  0972
*         GPR13P) RSTD;                                            0972
*       GENERATE REFS(GPR01P,GPR02P,GPR13P) SETS(RPHWORK);         0973
          STM GPR02P,GPR13P,RPHWORK(GPR01P)
*       RFY                                                        0974
*         GPR13P UNRSTD;                                           0974
*       GPR04P=FSBPTR;                                             0975
*       DCL                                                        0976
*         1 FMZA0102 FIXED(31) STATIC LOCAL,                       0976
*          2 * FIXED(8) INIT(80),                                  0976
*          2 * PTR(24) INIT(0);                                    0976
*       GPR03F=FMZA0102;                                           0977
         L     GPR03F,FMZA0102                                     0977
*       DO;                         /* LISATCVT                    0978
*                                      CVTBASE(PAGEABLE,(GPR15P))    */
*         GPR15P=ATCLCPTR;          /* SET UP ATCVT POINTER          */
         L     GPR15P,ATCLCPTR                                     0979
*       END;                        /* LISATCVT                      */
*       GPR02P=GPR15P->ATCBPDA;                                    0981
         L     GPR02P,ATCBPDA(,GPR15P)                             0981
*       GPR15P=GPR15P->ATCSMRS;                                    0982
         L     GPR15P,ATCSMRS(,GPR15P)                             0982
*       DCL                                                        0983
*         FMZL0102 BASED(GPR15P) LABEL VALUERANGE(*);              0983
*       CALL FMZL0102;                                             0984
         BALR  @14,@15                                             0984
*       RFY                                                        0985
*         GPR13P RSTD;                                             0985
*       GENERATE REFS(GPR01P,RPHWORK) SETS(GPR02P,GPR13P);         0986
          LM GPR02P,GPR13P,RPHWORK(GPR01P)
*       RFY                                                        0987
*        (GPR01P,                                                  0987
*         GPR13P) UNRSTD;                                          0987
*       RFY                                                        0988
*        (GPR02P,                                                  0988
*         GPR03P,                                                  0988
*         GPR04P,                                                  0988
*         GPR15P) UNRSTD;                                          0988
*     END;                          /* *********END OF RELSTORE    0989
*                                      EXPANSION*********            */
*   ELSE                                                           0990
