*CS7:                                                              1138
*       REG2=FMCCMPS1;              /* GET CURRENT BUFFER COUNT      */
CS7      L     @15,CRRFMCB(,CRRPTR)                                1138
         L     REG2,FMCCMPS1(,@15)                                 1138
*       REG3=REG2&BUFNB0;           /* SET COUNT TO ZERO             */
         LR    REG3,REG2                                           1139
         N     REG3,@CF04668                                       1139
*       CS(REG2,REG3,FMCCMPS1);     /* SET NEW COUNT IN FMCB         */
         CS    REG2,@03,FMCCMPS1(@15)                              1140
*       BC(7,CS7);                  /* TRY AGAIN IF FAILURE          */
         BC    7,CS7                                               1141
*       RFY                                                        1142
*        (REG2,                                                    1142
*         REG3) UNRSTD;                                            1142
*       IF FSBPTR^=ZEROWORD&FSBPTR^=GATEBIT THEN/* WAS FSB DEQUED?   */
         LTR   FSBPTR,FSBPTR                                       1143
         BZ    @RF01143                                            1143
         CL    FSBPTR,@CF01547                                     1143
         BE    @RF01143                                            1143
*         CALL FREEBUF;             /* YES, RELEASE IT               */
         BAL   @14,FREEBUF                                         1144
*       ELSE                                                       1145
*         ;                         /* NO FSB ON DATA Q              */
@RF01143 DS    0H                                                  1146
*     END;                                                         1146
*   ELSE                                                           1147
*     CALL FREEBUF;                 /* FREE CURRENT FSB              */
         B     @RC01113                                            1147
@RF01113 BAL   @14,FREEBUF                                         1147
*   IF CRRSW5=ON THEN               /* WAS ERROR LOCK SET?           */
*                                                                  1148
@RC01113 TM    CRRSW5(CRRPTR),B'00000100'                          1148
         BNO   @RF01148                                            1148
*     /***************************************************************/
*     /*                                                             */
*     /* IF THE ERROR LOCK IS SET IN THE FSB BEING FLUSHED, THE ERROR*/
*     /* LOCK MUST BE RESET INTERNALLY SINCE THE USER WILL NOT KNOW  */
*     /* IT IS SET. CALL LCPBINIT TO GET STORAGE FOR A LCPB. BUILD   */
*     /* RESET ERROR LOCK AND QUEUE TO TPIOS OUTBOUND PAB WITH RESET */
*     /* OPTION.                                                     */
*     /*                                                             */
*     /***************************************************************/
*                                                                  1149
*     CALL LCPBINIT;                /* GET LCPB FOR RESET            */
         BAL   @14,LCPBINIT                                        1149
*   ELSE                                                           1150
*     ;                             /* ERROR LOCK NOT SET            */
@RF01148 DS    0H                                                  1151
*   RTNREG=CRRRTN1;                 /* RESTORE RETURN REG            */
         L     RTNREG,CRRRTN1(,CRRPTR)                             1151
*   END FLUSH;                                                     1152
@EL00010 DS    0H                                                  1152
@EF00010 DS    0H                                                  1152
@ER00010 BR    @14                                                 1152
*                                                                  1153
*/********************************************************************/
*/*                                                                  */
*/* ROUTINE NAME : TRUNC                                             */
*/*                                                                  */
*/*  FUNCTITION :                                                    */
*/* TRUNC IS CALLED BY MOVEDATA TO PROCESS THE USER TRUNCATE         */
*/* OPTION.  IF THERE WAS NO ERROR IN THE MOVE, SUBROUTINE           */
*/* TRUNC1 IS CALLED.   ON RETURN FROM TRUNC1, THE FSB AND FDBS      */
*/* ARE FREED AND THE SUBROUTINE RETURNS TO MOVEDATA.                */
*/*                                                                  */
*/* IF THERE WAS AN ERROR DURING THE MOVE, ERROR FLGAS ARE SET IN    */
*/* THE CRR(CRRTRUN AND CRRLOGIC).  THE DO POST SWITCH,              */
*/* CRRPOST, IS SET AND SUBROUTINE EOBANAL IS CALLED.  IF            */
*/* EOB WAS NOT RECEIVED, THE FLUSH BIT IS SET.  IN EITHER           */
*/* CASE, THE FSB AND FDBS ARE FREED AND THE ROUTINE                 */
*/* RETURNS TO MOVEDATA VIA REG 14.                                  */
*/*                                                                  */
*/* INPUT :  FSBPTR REG(4) - ADDRESS OF FSB                          */
*/*                                                                  */
*/* OUTPUT : SAME                                                    */
*/*                                                                  */
*/* SUBROUTINES CALLED : EOBANAL  - CHECK FOR ENDING STATUS          */
*/*                      TRUNC1   - MORE TRUNC PROCESSING NECESSARY  */
*/* MACROS USED : NONE                                               */
*/*                                                                  */
*/********************************************************************/
*                                                                  1153
*TRUNC:                                                            1153
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1153
TRUNC    DS    0H                                                  1154
*   CRRRTN3=RTNREG;                 /* SAVE RETURN REG               */
         ST    RTNREG,CRRRTN3(,CRRPTR)                             1154
*   IF CRRBRTCD=CRRBRSUC THEN       /* RETURN CODE FROM MOVE OK      */
*                                                                  1155
         CLI   CRRBRTCD(CRRPTR),0                                  1155
         BNE   @RF01155                                            1155
*     /***************************************************************/
*     /*                                                             */
*     /* ALL THE DATA FROM ONE FSB MOVED                             */
*     /*                                                             */
*     /***************************************************************/
*                                                                  1156
*     CALL TRUNC1;                                                 1156
         BAL   @14,TRUNC1                                          1156
*   ELSE                                                           1157
*     DO;                                                          1157
*                                                                  1157
         B     @RC01155                                            1157
@RF01155 DS    0H                                                  1158
*       /*************************************************************/
*       /*                                                           */
*       /* DATA TRUNCATED                                            */
*       /*                                                           */
*       /*************************************************************/
*                                                                  1158
*                                   /* D31JKMQ                       */
*       CRRPOST=ON;                 /* SET DO POST SWITCH            */
         OI    CRRPOST(CRRPTR),B'00010000'                         1158
*       CALL EOBANAL;               /* CHECK FOR EOB, EOM, EOT       */
         BAL   @14,EOBANAL                                         1159
*       IF FSBEBI=ON×               /* EOM,EOT,EOB                   */
*           FSBLGI=ON×              /* LEADING GRAPHICS?             */
*           FSBSOHI=ON×             /* START OF HEADING (SOH)      1160
*                                      INDICATOR                     */
*           FSBMJRCD^=FRNORM THEN                                  1160
         TM    FSBEBI(FSBPTR),B'00100011'                          1160
         BNZ   @RT01160                                            1160
         CLI   FSBMJRCD(FSBPTR),X'00'                              1160
         BNE   @RT01160                                            1160
*         ;                         /* ERROR CODE IN FSB?            */
*       ELSE                                                       1162
*         DO;                                                      1162
*                                                                  1162
*           /*********************************************************/
*           /*                                                       */
*           /* SUBBLOCK-INDICATE REMAINING SUBBLOCKS SHOULD BE       */
*           /* FLUSHED AS THEY ARRIVE                                */
*           /*                                                       */
*           /*********************************************************/
*                                                                  1163
*           RFY                                                    1163
*            (REG2,                                                1163
*             REG3) RSTD;                                          1163
*CS8:                                                              1164
*           REG2=FMCCMPS2;          /* REFERENCE FLAG VALUE          */
CS8      L     @15,CRRFMCB(,CRRPTR)                                1164
         L     REG2,FMCCMPS2(,@15)                                 1164
*           REG3=REG2×FLGA3ON;      /* SET FLUSH INDICATOR           */
         LR    REG3,REG2                                           1165
         O     REG3,@CF01565                                       1165
*           CS(REG2,REG3,FMCCMPS2); /* SET NEW VALUE IN FMCB         */
         CS    REG2,@03,FMCCMPS2(@15)                              1166
*           BC(7,CS8);              /* TRY AGAIN IF FAILURE          */
         BC    7,CS8                                               1167
*           RFY                                                    1168
*            (REG2,                                                1168
*             REG3) UNRSTD;                                        1168
*         END;                                                     1169
*     END;                                                         1170
@RT01160 DS    0H                                                  1171
*   CALL FREEBUF;                   /* FREE FSB AND FDBS             */
@RC01155 BAL   @14,FREEBUF                                         1171
*   RTNREG=CRRRTN3;                 /* RESTORE RETURN REG            */
         L     RTNREG,CRRRTN3(,CRRPTR)                             1172
*   END TRUNC;                                                     1173
@EL00011 DS    0H                                                  1173
@EF00011 DS    0H                                                  1173
@ER00011 BR    @14                                                 1173
*                                                                  1174
*/********************************************************************/
*/*                                                                  */
*/* ROUTINE NAME : TRUNC1                                            */
*/*                                                                  */
*/* FUNCTION:                                                        */
*/* TRUNC1 IS CALLED BY TRUNC IF THE USER SPECIFIED THE TRUNC        */
*/* OPTION AND NO ERROR CODE WAS RETURNED BY THE MOVE ROUTINE.       */
*/* IF EOB WAS NOT RECEIVED IN THE FSB AND THERE IS MORE SPACE       */
*/* IN THE USER AREA, THE BUFFER INFORMATION IS MOVED FROM THE       */
*/* CRR TO THE DCLCP AND THE RPL PTR IS PUT BACK IN THE DCLCP.       */
*/* IF THERE IS NO SPACE LEFT IN THE USER AREA, ERROR INDICATORS     */
*/* (CRRTRUN AND CRRLOGIC) ARE SET IN THE CRR.  THE FLUSH BIT        */
*/* IS SET TO FREE THE FSB AND FDBS.  THE SUBROUTINE RETURNS TO      */
*/*                                                                  */
*/* INPUT :  FSBPTR REG(4) - ADDRESS OF FSB                          */
*/*                                                                  */
*/* OUTPUT : SAME                                                    */
*/*                                                                  */
*/* SUBROUTINES CALLED : EOBANAL  - CHECK FOR ENDING STATUS          */
*/*                                                                  */
*/* MACROS USED :  NONE                                              */
*/*                                                                  */
*/********************************************************************/
*                                                                  1174
*TRUNC1:                                                           1174
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1174
TRUNC1   DS    0H                                                  1175
*   CRRRTN4=RTNREG;                 /* SAVE RETURN REG               */
         ST    RTNREG,CRRRTN4(,CRRPTR)                             1175
*   CALL EOBANAL;                   /* CHECK FOR EOM,EOB,EOT         */
         BAL   @14,EOBANAL                                         1176
*   IF FSBEBI=ON×FSBECI=ON×         /* EOM,EOB,EOT           @OX11109*/
*       FSBLGI=ON×                  /* LEADING GRAPHICS IN FSB       */
*       FSBSOHI=ON×                 /* START OF HEADING(SOH)INDICATOR*/
*       FSBMJRCD^=FRNORM THEN       /* ERROR RETURN CODE IN FSB      */
*                                                                  1177
         TM    FSBEBI(FSBPTR),B'01100011'                          1177
         BNZ   @RT01177                                            1177
         CLI   FSBMJRCD(FSBPTR),X'00'                              1177
         BE    @RF01177                                            1177
@RT01177 DS    0H                                                  1178
*     /***************************************************************/
*     /*                                                             */
*     /* ENDING STATUS IN FSB-DO NOT EXPECT ANOTHER FSB. PROCESSING  */
*     /* FINISHED                                                    */
*     /*                                                             */
*     /***************************************************************/
*                                                                  1178
*     CRRPOST=ON;                   /* SET DO POST SWITCH            */
         OI    CRRPOST(CRRPTR),B'00010000'                         1178
*   ELSE                                                           1179
*     IF CRRBLEN>ZEROWORD THEN      /* MORE SPACE IN USER DATA AREA  */
         B     @RC01177                                            1179
@RF01177 LH    @15,CRRBLEN(,CRRPTR)                                1179
         LTR   @15,@15                                             1179
         BNP   @RF01179                                            1179
*       IF CRRERLK=ON THEN          /* TEST FOR ERROR LOCK           */
*                                                                  1180
         TM    CRRERLK(CRRPTR),B'10000000'                         1180
         BNO   @RF01180                                            1180
*         /***********************************************************/
*         /*                                                         */
*         /* ERROR LOCK SET IN FSB JUST PROCESSED. PROCESSING        */
*         /* FINISHED                                                */
*         /*                                                         */
*         /***********************************************************/
*                                                                  1181
*         CRRPOST=ON;               /* POST USET WITH ERR LOCK       */
         OI    CRRPOST(CRRPTR),B'00010000'                         1181
*       ELSE                                                       1182
*         DO;                       /* ERROR LOCK NOT SET            */
*                                                                  1182
         B     @RC01180                                            1182
@RF01180 DS    0H                                                  1183
*           /*********************************************************/
*           /*                                                       */
*           /* FSB JUST PROCESSED WAS A SUBBLOCK AND THERE IS MORE   */
*           /* ROOM IN THE USER'S BUFFER. SAVE STATUS IN LCPB TO     */
*           /* RESUME MOVE WHEN NEXT FSB ARRIVES                     */
*           /*                                                       */
*           /*********************************************************/
*                                                                  1183
*           DCLIOBUF=CRRBARAD;      /* MOVE ADDR OF NEXT AVAIL SPACE
*                                      IN USER BUFFER TO DCLCP     1183
*                                      EXTENSION                     */
         L     @15,CRRLCPB(,CRRPTR)                                1183
         L     @05,CRRBARAD(,CRRPTR)                               1183
         ST    @05,DCLIOBUF(,@15)                                  1183
*           DCLIOLEN=CRRBLEN;       /* MOVE RESIDUAL COUNT TO DCLCP  */
         LH    @05,CRRBLEN(,CRRPTR)                                1184
         STH   @05,DCLIOLEN(,@15)                                  1184
*           DCLBTLEN=CRRBTLEN;      /* PUT LENGTH MOVED IN DCLCP     */
         L     @05,CRRBTLEN(,CRRPTR)                               1185
         ST    @05,DCLBTLEN(,@15)                                  1185
*           DCLRPLA=CRRARPL;        /* PUT ADR OF RPL IN DCLCP       */
         L     @05,CRRARPL(,CRRPTR)                                1186
         ST    @05,DCLRPLA(,@15)                                   1186
*         END;                                                     1187
*     ELSE                                                         1188
*       DO;                                                        1188
*                                                                  1188
         B     @RC01179                                            1188
@RF01179 DS    0H                                                  1189
*         /***********************************************************/
*         /*                                                         */
*         /* FSB JUST PROCESSED WAS A SUBBLOCK BUT THE USER'S BUFFER */
*         /* IS FILLED.                                              */
*         /*                                                         */
*         /***********************************************************/
*                                                                  1189
*                                   /* D31JKMQ                       */
*         CRRPOST=ON;               /* SET DO POST SWITCH            */
         OI    CRRPOST(CRRPTR),B'00010000'                         1189
*         RFY                                                      1190
*          (REG2,                                                  1190
*           REG3) RSTD;                                            1190
*CS9:                                                              1191
*         REG2=FMCCMPS2;            /* REFERENCE FLAG VALUE          */
CS9      L     @15,CRRFMCB(,CRRPTR)                                1191
         L     REG2,FMCCMPS2(,@15)                                 1191
*         REG3=REG2×FLGA3ON;        /* SET FLUSH INDICATOR           */
         LR    REG3,REG2                                           1192
         O     REG3,@CF01565                                       1192
*         CS(REG2,REG3,FMCCMPS2);   /* SET NEW VALUE IN FMCB         */
         CS    REG2,@03,FMCCMPS2(@15)                              1193
*         BC(7,CS9);                /* TRY AGAIN IF FAILURE          */
         BC    7,CS9                                               1194
*         RFY                                                      1195
*          (REG2,                                                  1195
*           REG3) UNRSTD;                                          1195
*       END;                                                       1196
*   RTNREG=CRRRTN4;                 /* RESTORE RETURN REG            */
@RC01179 DS    0H                                                  1197
@RC01177 L     RTNREG,CRRRTN4(,CRRPTR)                             1197
*   END TRUNC1;                                                    1198
@EL00012 DS    0H                                                  1198
@EF00012 DS    0H                                                  1198
@ER00012 BR    @14                                                 1198
*                                                                  1199
*/********************************************************************/
*/*                                                                  */
*/* ROUTINE NAME : KEEP                                              */
*/*                                                                  */
*/* FUNCTION :                                                       */
*/* KEEP IS CALLED BY MOVEDATA IF THE USER HAS NOT SPECIFIED THE     */
*/* TRUNC OPTION.  IF THERE WERE NO ERRORS DURING THE MOVE KEEP1 IS  */
*/* CALLED.  OTHERWISE, THE EOB, EOM, EOT INDICATORS ARE TURNED OFF  */
*/* IN THE CRR, THE DO POST FLAG IS SET AND THE FSB AND FDBS ARE     */
*/* QUEUED TO THE FMCB DATA QUEUE.  RETURN IS TO MOVEDATA VIA        */
*/* REG 14.                                                          */
*/*                                                                  */
*/* INPUT : FSBPTR REG(4) - ADDRESS OF FSB                           */
*/*                                                                  */
*/* OUTPUT : SAME                                                    */
*/*                                                                  */
*/* SUBROUTINES CALLED : KEEP1    - FURTHER KEEP PROCESSING          */
*/*                      XFRFSB   - QUEUE FSB ON FMCB DATA QUEUE     */
*/*                      FREEBUF  - RELEASE FSB AND FDB BUFFERS      */
*/*                                                                  */
*/* MACROS USED :                                                    */
*/*                                                                  */
*/********************************************************************/
*                                                                  1199
*KEEP:                                                             1199
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1199
KEEP     DS    0H                                                  1200
*   CRRRTN3=RTNREG;                 /* SAVE RETURN REG               */
         ST    RTNREG,CRRRTN3(,CRRPTR)                             1200
*   IF CRRBRTCD=CRRBRSUC THEN       /* 0 RETURN CODE FROM MOVE       */
         CLI   CRRBRTCD(CRRPTR),0                                  1201
         BNE   @RF01201                                            1201
*     DO;                                                          1202
*                                                                  1202
*       /*************************************************************/
*       /*                                                           */
*       /* ALL DATA FROM ONE FSB MOVED- CHECK IF MORE DATA EXPECTED  */
*       /*                                                           */
*       /*************************************************************/
*                                                                  1203
*       CALL KEEP1;                                                1203
         BAL   @14,KEEP1                                           1203
*       CALL FREEBUF;               /* FREE FSB AND FDBS             */
         BAL   @14,FREEBUF                                         1204
*     END;                                                         1205
*   ELSE                                                           1206
*     DO;                                                          1206
*                                                                  1206
         B     @RC01201                                            1206
@RF01201 DS    0H                                                  1207
*       /*************************************************************/
*       /*                                                           */
*       /* BUFFER WAS FILLED BEFORE ALL DATA WAS MOVED. RESET ANY    */
*       /* ENDING STATUS IN THE FEEDBACK TO INFORM USER OF THIS FACT.*/
*       /* SAVE FSB TO BE PROCESSED DURING NEXT READ.                */
*       /*                                                           */
*       /*************************************************************/
*                                                                  1207
*       CRRPOST=ON;                 /* SET DO POST SWITCH            */
         OI    CRRPOST(CRRPTR),B'00010000'                         1207
*       CRRRDEOB=OFF;               /* TURN OFF EOB,EOM, EOT         */
*       CRRRDEOM=OFF;                                              1209
*       CRRRDEOT=OFF;                                              1210
         NI    CRRRDEOB(CRRPTR),B'11000111'                        1210
*       CALL XFRFSB;                /* PUT FSB ON FMCB DATA Q        */
         BAL   @14,XFRFSB                                          1211
*     END;                                                         1212
*   RTNREG=CRRRTN3;                 /* RESTORE RETURN REG            */
@RC01201 L     RTNREG,CRRRTN3(,CRRPTR)                             1213
*   END KEEP;                                                      1214
@EL00013 DS    0H                                                  1214
@EF00013 DS    0H                                                  1214
@ER00013 BR    @14                                                 1214
*                                                                  1215
*/********************************************************************/
*/*                                                                  */
*/* ROUTINE NAME : KEEP1                                             */
*/*                                                                  */
*/* FUNCTION :                                                       */
*/* KEEP1 IS CALLED BY KEEP IF THERE WERE NO ERRORS DURING THE MOVE. */
*/* IF EOB WAS RECEIVED IN THE FSB, THE DO POST SWITCH IS SET AND    */
*/* THE SUBROUTINE RETURNS TO KEEP.                                  */
*/*                                                                  */
*/* OTHERWISE, IF THERE IS MORE SPACE LEFT IN THE USER AREA, THE     */
*/* BUFFER INFORMATION IS MOVED FROM THE CRR TO THE DCLCP AND THE    */
*/* RPL PTR IS PUT BACK INTO THE DCLCP.  IF THERE IS NO MORE SPACE   */
*/* IN THE USER AREA THE EOB, EOM, EOT INDICATORS ARE TURNED OFF IN  */
*/* THE CRR AND THE DO POST FLAG IS SET.  THE SUBROUTINE RETURNS TO  */
*/* KEEP VIA REG 14.                                                 */
*/*                                                                  */
*/* INPUT : FSBPTR REG(4) - ADDRESS OF FSB                           */
*/*                                                                  */
*/* OUTPUT : SAME                                                    */
*/*                                                                  */
*/* SUBROUTINES CALLED : EOBANAL  - CHECK FOR ENDING STATUS          */
*/*                                                                  */
*/* MACROS USED : NONE                                               */
*/*                                                                  */
*/********************************************************************/
*                                                                  1215
*KEEP1:                                                            1215
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1215
KEEP1    DS    0H                                                  1216
*   CRRRTN4=RTNREG;                 /* SAVE RETURN REG               */
         ST    RTNREG,CRRRTN4(,CRRPTR)                             1216
*   CALL EOBANAL;                   /* CHECK FOR EOM, EOB, EOT       */
         BAL   @14,EOBANAL                                         1217
*   IF FSBEBI=ON×FSBECI=ON THEN     /* CHECK EOB & END CHAN PGM    1218
*                                                            @OX11109*/
*                                                                  1218
         TM    FSBEBI(FSBPTR),B'01100000'                          1218
         BZ    @RF01218                                            1218
*     /***************************************************************/
*     /*                                                             */
*     /* FSB JUST PROCESSED WAS NOT A SUBBLOCK SO MOVE PROCESSING    */
*     /* COMPLETE.                                                   */
*     /*                                                             */
*     /***************************************************************/
*                                                                  1219
*     CRRPOST=ON;                   /* SET DO POST SWITCH            */
         OI    CRRPOST(CRRPTR),B'00010000'                         1219
*   ELSE                                                           1220
*     IF CRRBLEN>ZEROWORD THEN      /* MORE SPACE LEFT IN USER AREA  */
         B     @RC01218                                            1220
@RF01218 LH    @15,CRRBLEN(,CRRPTR)                                1220
         LTR   @15,@15                                             1220
         BNP   @RF01220                                            1220
*       IF CRRERLK=ON×              /* CHECK FOR ERROR LOCK          */
*           FSBMJRCD^=FRNORM×       /* ERROR RETURN CODE             */
*           FSBLGI=ON THEN          /* LEADING GRAPHICS RECEIVED     */
*                                                                  1221
         TM    CRRERLK(CRRPTR),B'10000000'                         1221
         BO    @RT01221                                            1221
         CLI   FSBMJRCD(FSBPTR),X'00'                              1221
         BNE   @RT01221                                            1221
         TM    FSBLGI(FSBPTR),B'00000010'                          1221
         BNO   @RF01221                                            1221
@RT01221 DS    0H                                                  1222
*         /***********************************************************/
*         /*                                                         */
*         /* ERROR RETURN CODE OR LEADING GRAPHICS RECEIVED SO MOVE  */
*         /* PROCESSING COMPLETE.                                    */
*         /*                                                         */
*         /***********************************************************/
*                                                                  1222
*         CRRPOST=ON;               /* POST USER WITH ERROR LOCK     */
         OI    CRRPOST(CRRPTR),B'00010000'                         1222
*       ELSE                        /* ERROR LOCK NOT SET-CONTINUE   */
*         DO;                                                      1223
*                                                                  1223
         B     @RC01221                                            1223
@RF01221 DS    0H                                                  1224
*           /*********************************************************/
*           /*                                                       */
*           /* FSB JUST PROCESSED WAS A SUBBLOCK AND THERE IS ROOM IN*/
*           /* THE USER'S BUFFER FOR MORE DATA. SAVE STATUS IN THE   */
*           /* DCLCP FOR WHEN ANOTHER FSB ARRIVES SO THE MOVE CAN BE */
*           /* RESUMED.                                              */
*           /*                                                       */
*           /*********************************************************/
*                                                                  1224
*           DCLIOBUF=CRRBARAD;      /* MOVE ADDR OF NEXT AVAIL SPACE
*                                      IN USER BUFFER TO DCLCP     1224
*                                      EXTENSION                     */
         L     @15,CRRLCPB(,CRRPTR)                                1224
         L     @05,CRRBARAD(,CRRPTR)                               1224
         ST    @05,DCLIOBUF(,@15)                                  1224
*           DCLIOLEN=CRRBLEN;       /* MOVE RESIDUAL COUNT TO DCLCP  */
         LH    @05,CRRBLEN(,CRRPTR)                                1225
         STH   @05,DCLIOLEN(,@15)                                  1225
*           DCLBTLEN=CRRBTLEN;      /* PUT LENGTH MOVED IN DCLCP     */
         L     @05,CRRBTLEN(,CRRPTR)                               1226
         ST    @05,DCLBTLEN(,@15)                                  1226
*           DCLRPLA=CRRARPL;        /* PUT ADR OF RPL IN DCLCP       */
         L     @05,CRRARPL(,CRRPTR)                                1227
         ST    @05,DCLRPLA(,@15)                                   1227
*         END;                                                     1228
*     ELSE                                                         1229
*       DO;                                                        1229
*                                                                  1229
         B     @RC01220                                            1229
@RF01220 DS    0H                                                  1230
*         /***********************************************************/
*         /*                                                         */
*         /* FSB JUST PROCESSED WAS A SUBBLOCK BUT THE USER'S BUFFER */
*         /* IS FILLED. RESET FEEDBACK STATUS TO INDICATE NOT ALL    */
*         /* DATA WAS RECEIVED. SAVE FSB TO BE USED FOR NEXT READ.   */
*         /*                                                         */
*         /***********************************************************/
*                                                                  1230
*         CRRPOST=ON;               /* SET DO POST SWITCH            */
         OI    CRRPOST(CRRPTR),B'00010000'                         1230
*         CRRRDEOB=OFF;             /* TURN OFF EOB,EOM,EOT          */
*         CRRRDEOM=OFF;                                            1232
*         CRRRDEOT=OFF;                                            1233
         NI    CRRRDEOB(CRRPTR),B'11000111'                        1233
*       END;                                                       1234
*   RTNREG=CRRRTN4;                 /* RESTORE RETURN REG            */
@RC01220 DS    0H                                                  1235
@RC01218 L     RTNREG,CRRRTN4(,CRRPTR)                             1235
*   END KEEP1;                                                     1236
@EL00014 DS    0H                                                  1236
@EF00014 DS    0H                                                  1236
@ER00014 BR    @14                                                 1236
*LCPBINIT:                                                         1237
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1237
*                                                                  1237
LCPBINIT DS    0H                                                  1238
