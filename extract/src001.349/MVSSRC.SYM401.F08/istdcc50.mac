         L     FSBPTR,CRRFSB(,CRRPTR)                              0492
*   IF CRRFMCLK=OFF THEN            /* FMCB LOCKED?          @ZA33377*/
         TM    CRRFMCLK(CRRPTR),B'10000000'                        0493
         BNZ   @RF00493                                            0493
*     DO;                           /* LOCK FMCB             @ZA33377*/
*/*** START -TPLOCK (CTLLOCK)TYPE(XCLSIVE)- **                       */
*                                                                  0495
*       DO;                                                        0495
*         RFY                                                      0496
*          (GPR00P,                                                0496
*           GPR01P,                                                0496
*           GPR02P,                                                0496
*           GPR03P,                                                0496
*           GPR04P,                                                0496
*           GPR15P) RSTD;                                          0496
*         GENERATE REFS(GPR00P,GPR01P,GPR15P,RPHWORK) SETS(RPHWORK);
         STM GPR00P,GPR14P,RPHWORK(GPR01P)
