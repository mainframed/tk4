*/********************************************************************/
*/*                                                                  */
*/* ROUTINE NAME : HDATTN - HANDLE ATTENTION EXITS                   */
*/*                                                                  */
*/* FUNCTION : IF THE USER SPECIFIED A RTN AT OPNDST TO HANDLE ATTNS,*/
*/*      THEN SCHEDULE THAT RTN AND SET SWITCHES. OTHERWISE BUILD AN */
*/*      LCCW TO SCHEDULE TPIOS TO UNLOCK THE TERMINAL SINCE IT CAN- */
*/*      NOT HANDLE ATTENTIONS.                                      */
*/*                                                                  */
*/* INPUT : RPHPTR (REG 1) - PT TO THE RPH                           */
*/*         DVTPTR (REG 11) - ZERO                                   */
*/*                                                                  */
*/* OUTPUT : DVTPTR (REG 11) - ZERO                                  */
*/*                                                                  */
*/* SUBROUTINES CALLED : LCPBINIT                                    */
*/*                                                                  */
*/* MACROS USED : TPQUE                                              */
*/*                                                                  */
*/********************************************************************/
*                                                                  0609
*HDATTN:                                                           0609
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0609
         B     @PB00003                                            0609
HDATTN   DS    0H                                                  0610
*   CRRRTN1=RTNREG;                 /* SAVE RET REG                  */
         ST    RTNREG,CRRRTN1(,CRRPTR)                             0610
*   RFY                                                            0611
*    (WRKREG,                                                      0611
*     WRKREG1,                                                     0611
*     WRKREG2) RSTD;                                               0611
*   RESPECIFY                                                      0612
*     ISTACDEB BASED(WRKREG);       /* COVER ACDEB FROM              */
*   WRKREG=FMCDEBA;                 /* FMCB,GET ACB ADDR             */
         L     @15,CRRFMCB(,CRRPTR)                                0613
         L     WRKREG,FMCDEBA(,@15)                                0613
*   WRKREG1=ACDACB;                 /* FROM ACDEB                    */
         L     WRKREG1,ACDACB-1(,WRKREG)                           0614
         LA    WRKREG1,0(,WRKREG1)                                 0614
*   RESPECIFY                                                      0615
*     IFGACB BASED(WRKREG1);                                       0615
*   CRRSW1=ON;                      /* SET CB CHECK SW ON            */
*                                                                  0616
         OI    CRRSW1(CRRPTR),B'01000000'                          0616
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK THE ACB TO MAKE SURE IT HAS NOT BEEN OVERLAID. IF IT HAS*/
*   /* NOT, PICK UP THE ADDRESS OF THE EXIT LIST.                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0617
*   IF ACBID='A0'X THEN                                            0617
         CLI   ACBID(WRKREG1),160                                  0617
         BNE   @RF00617                                            0617
*     DO;                                                          0618
*       WRKREG2=ACBUEL;             /* GET EXLIST ADR                */
         L     WRKREG2,ACBUEL(,WRKREG1)                            0619
*       RESPECIFY                                                  0620
*         IFGEXLST BASED            /* COVER EXLIST                  */
*             (WRKREG2);                                           0620
*                                                                  0620
*       /*************************************************************/
*       /*                                                           */
*       /* IF THE EXLST EXIST AND HAS NOT BEEN OVERLAID, CHECK TO SEE*/
*       /* IF THE ATTENTION EXIT EXIST AND IS ACTIVE, IF SO, GO      */
*       /* INITIALIZE A UECB TO SCHEDULE AN EXIT.                    */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0621
*       IF WRKREG2^=0               /* IF EXLIST ADDR                */
*         THEN                      /* NOT ZERO                      */
         LTR   WRKREG2,WRKREG2                                     0621
         BZ    @RF00621                                            0621
*         IF EXLID=129 THEN                                        0622
         CLI   EXLID(WRKREG2),129                                  0622
         BNE   @RF00622                                            0622
*           DO;                                                    0623
*             IF EXLLEN2>=60        /* EXIT SPECIFIED                */
*               THEN                                               0624
         LH    @15,EXLLEN2(,WRKREG2)                               0624
         C     @15,@CF02698                                        0624
         BL    @RF00624                                            0624
*               DO;                                                0625
*                 CRRSW1=OFF;       /* TURN OFF CB ERR SW            */
         NI    CRRSW1(CRRPTR),B'10111111'                          0626
*                 CALL UECBINIT;    /* CREATE & INIT UECB            */
         BAL   @14,UECBINIT                                        0627
*               END;                /* END ATTN ACTIVE               */
*             ELSE                                                 0629
*               ;                   /* ATTN NOT ACTIVE               */
@RF00624 DS    0H                                                  0630
*           END;                    /* END XLIST ID OK               */
*         ELSE                                                     0631
*           DO;                     /* XLIST ID NOT OK               */
         B     @RC00622                                            0631
@RF00622 DS    0H                                                  0632
*             CRRRTNCD=CRRLOGIC;    /* SET ERROR CODE                */
         MVI   CRRRTNCD(CRRPTR),X'08'                              0632
*             CRRFDB2=CRRIVEXL;                                    0633
         MVI   CRRFDB2(CRRPTR),X'23'                               0633
*           END;                    /* END XLIST NOT OK              */
*     END;                          /* END ACB ID OK                 */
*   ELSE                                                           0636
*     DO;                           /* ACB ID NOT OK                 */
         B     @RC00617                                            0636
@RF00617 DS    0H                                                  0637
*       CRRRTNCD=CRRLOGIC;          /* SET ERROR CODE                */
         MVI   CRRRTNCD(CRRPTR),X'08'                              0637
*       CRRFDB2=CRRIVACB;                                          0638
         MVI   CRRFDB2(CRRPTR),X'22'                               0638
*     END;                          /* END ACB ID NOT OK             */
*   IF CRRSW1=ON                    /* IF SW ON                      */
*     THEN                          /* CHECK UECB BUILT              */
@RC00617 TM    CRRSW1(CRRPTR),B'01000000'                          0640
         BNO   @RF00640                                            0640
*     CALL LCPBINIT;                /* CREATE LCPB                   */
         BAL   @14,LCPBINIT                                        0641
*   RTNREG=CRRRTN1;                 /* RESTORE RET REG               */
@RF00640 L     RTNREG,CRRRTN1(,CRRPTR)                             0642
*   RETURN;                                                        0643
@EL00003 DS    0H                                                  0643
@EF00003 DS    0H                                                  0643
@ER00003 BR    @14                                                 0643
*   END HDATTN;                                                    0644
@PB00003 DS    0H                                                  0644
*   GEN NOSETS NOREFS(EJECT);                                      0645
*                                                                  0645
         EJECT
*/********************************************************************/
*/*                                                                  */
*/* ROUTINE NAME : CKUNSD - CHECK FOR UNSOLICITED DATA               */
*/*                                                                  */
*/* FUNCTION : CHECKS TO SEE IF THE DEVICE IS DICONNECTED. IF NOT THE*/
*/*      UNSEXT SUB ROUTINE IS CALLED TO HANDLE THE UNSOLICITED DATA */
*/*      OTHERWISE THE TERMINAL HAS BEEN LOST, AND THIS ROUTINE CHECK*/
*/*      S TO SEE IF THE USER SPECIFIED A LOST TERMINAL EXIT.        */
*/*                                                                  */
*/* INPUT : FSBPTR (REG 5) - PTR TO THE FSB                          */
*/*                                                                  */
*/* OUTPUT : CRRRTNCD MAY BE MODIFIED                                */
*/*                                                                  */
*/* SUBROUTINES CALLED : UNSEXT , UECBINIT , FREEFSB                 */
*/*                                                                  */
*/* MACROS USED : NONE                                               */
*/*                                                                  */
*/********************************************************************/
*                                                                  0646
*CKUNSD:                                                           0646
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);                          0646
         B     @PB00004                                            0646
CKUNSD   DS    0H                                                  0647
*   CRRRTN1=RTNREG;                 /* SAVE THE RETURN ADDRESS       */
*                                                                  0647
         ST    RTNREG,CRRRTN1(,CRRPTR)                             0647
*   /*****************************************************************/
*   /*                                                               */
*   /* IF THE DEVICE IS DISCONNECTED, CHECK FOR A LOSTERM EXIT       */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0648
*   IF FSBMJRCD=FRDISCNT THEN                                      0648
         CLI   FSBMJRCD(FSBPTR),X'28'                              0648
         BNE   @RF00648                                            0648
*     DO;                           /* FSB INDICATES DISCONNECT      */
*       RESPECIFY                                                  0650
*         ISTACDEB BASED(WRKREG);   /* COVER ACDEB FROM              */
*                                                                  0650
*       /*************************************************************/
*       /*                                                           */
*       /* GET THE ADDR OF THE ACB FROM THE ACDEB                    */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0651
*       WRKREG=FMCDEBA;             /* FMCB,GET ACB ADDR             */
         L     @15,CRRFMCB(,CRRPTR)                                0651
         L     WRKREG,FMCDEBA(,@15)                                0651
*       WRKREG1=ACDACB;             /* FROM ACDEB                    */
         L     WRKREG1,ACDACB-1(,WRKREG)                           0652
         LA    WRKREG1,0(,WRKREG1)                                 0652
*       RESPECIFY                                                  0653
*         IFGACB BASED(WRKREG1);                                   0653
*                                                                  0653
*       /*************************************************************/
*       /*                                                           */
*       /* CRRSW1 IS USED BY UECBINIT TO KNOW TO SCHEDULE THE LOSTERM*/
*       /* EXIT AND NOT THE ATTENTION EXIT (OFF)                     */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0654
*       CRRSW1=ON;                  /* INTERN SWITCH                 */
*                                                                  0654
         OI    CRRSW1(CRRPTR),B'01000000'                          0654
*       /*************************************************************/
*       /*                                                           */
*       /* IF THE ACB HAS NOT BEEN OVERLAID, GET THE EXLST ADDR FROM */
*       /* IT.                                                       */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0655
*       IF ACBID='A0'X THEN                                        0655
         CLI   ACBID(WRKREG1),160                                  0655
         BNE   @RF00655                                            0655
*         DO;                       /* CHECK THE ACB ID              */
*           WRKREG2=ACBUEL;         /* GET EXLIST ADR                */
*                                                                  0657
         L     WRKREG2,ACBUEL(,WRKREG1)                            0657
*           /*********************************************************/
*           /*                                                       */
*           /* IF THE EXLST EXIST AND HAS NOT BEEN OVERLAID, CHECK TO*/
*           /* SEE IF THE LOSTERM EXIT IS AVAILABLE                  */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0658
*           IF WRKREG2^=0           /* IF EXLIST ADDR                */
*             THEN                                                 0658
         LTR   WRKREG2,WRKREG2                                     0658
         BZ    @RF00658                                            0658
*             DO;                   /* NOT ZERO                      */
*               IF EXLID=129 THEN   /* EXLST ID OK                   */
*                                                                  0660
         CLI   EXLID(WRKREG2),129                                  0660
         BNE   @RF00660                                            0660
*                 /***************************************************/
*                 /*                                                 */
*                 /* CHECK THE EXLST LENGTH ,TO SEE IF IT IS LONG    */
*                 /* ENOUGH TO HAVE THE SPECIFIED EXIT & AN ADDR IS  */
*                 /* THERE, SCHEDULE THE EXIT                        */
*                 /*                                                 */
*                 /***************************************************/
*                                                                  0661
*                 IF EXLLEN2>=45    /* EXIT SPECIFIED                */
*                   THEN                                           0661
         LH    @15,EXLLEN2(,WRKREG2)                               0661
         C     @15,@CF04402                                        0661
         BL    @RF00661                                            0661
*                   CALL UECBINIT;  /* GET & INIT UECB               */
         BAL   @14,UECBINIT                                        0662
*                 ELSE                                             0663
*                   ;                                              0663
*                                                                  0663
@RF00661 DS    0H                                                  0664
*               /*****************************************************/
*               /*                                                   */
*               /* THE EXIT LIST HAS BEEN OVERLAID, SET ERROR RETURN */
*               /* CODES                                             */
*               /*                                                   */
*               /*****************************************************/
*                                                                  0664
*               ELSE                                               0664
*                 DO;               /* XLIST ID NOT OK               */
         B     @RC00660                                            0664
@RF00660 DS    0H                                                  0665
*                   CRRRTNCD=CRRLOGIC;/* SET ERROR CODE              */
         MVI   CRRRTNCD(CRRPTR),X'08'                              0665
*                   CRRFDB2=CRRIVEXL;                              0666
         MVI   CRRFDB2(CRRPTR),X'23'                               0666
*                 END;              /* END XLIST NOT OK              */
*             END;                  /* END XLIST ID OK               */
*         END;                      /* END ACB ID OK                 */
*                                                                  0669
*       /*************************************************************/
*       /*                                                           */
*       /* THE ACB HAS BEEN OVERLAID, SET ERROR RETURN CODES         */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0670
*       ELSE                                                       0670
*         DO;                       /* ACB ID NOT OK                 */
         B     @RC00655                                            0670
@RF00655 DS    0H                                                  0671
*           CRRRTNCD=CRRLOGIC;      /* SET ERROR CODE                */
         MVI   CRRRTNCD(CRRPTR),X'08'                              0671
*           CRRFDB2=CRRIVACB;                                      0672
         MVI   CRRFDB2(CRRPTR),X'22'                               0672
*         END;                      /* END ACB ID NOT OK             */
*                                                                  0673
*       /*************************************************************/
*       /*                                                           */
*       /* THE FSB HAS TO BE FREED, NO ONE NEEDS IT                  */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0674
*       CALL FREEFSB;               /* FREE THE FSB                  */
@RC00655 BAL   @14,FREEFSB                                         0674
*     END;                          /* END IS DISCONN ON             */
*                                                                  0675
*   /*****************************************************************/
*   /*                                                               */
*   /* THE DEVICE IS AVAILABLE. GO QUEUE THE DATA TO THE DATA QUEUE  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0676
*   ELSE                                                           0676
*     DO;                           /* NO DISCONN.           @D31KKNL*/
         B     @RC00648                                            0676
@RF00648 DS    0H                                                  0677
*       FSBELI=ON;                  /* END OF LCP            @D31KKNL*/
*       FSBECI=OFF;                 /* END OF COMM'CATION    @D31KKNL*/
         OI    FSBELI(FSBPTR),B'10000000'                          0678
         NI    FSBECI(FSBPTR),B'10111111'                          0678
*       CALL XFRFSB;                /* QUE UP DATA           @D31KKNL*/
         BAL   @14,XFRFSB                                          0679
*       IF FMCFLGA1=OFF THEN                                       0680
         L     @15,CRRFMCB(,CRRPTR)                                0680
         TM    FMCFLGA1(@15),B'01000000'                           0680
         BNZ   @RF00680                                            0680
*         DO;                       /* FMCB IN CA MODE?      @D33SKCF*/
*                                                                  0681
*           /*********************************************************/
*           /*                                                       */
*           /* SET THE FLAGS IN THE FMCB SO ONLY THE SYNCHRONOUS DATA*/
*           /* QUEUE IS CHECKED FOR DATA AND CALL RCC63 TO PUT THE   */
*           /* FMCB ON THE ACDEB READ ANY QUEUE IF NECESSARY.        */
*           /*                                               @D33SKCF*/
*           /*                                                       */
*           /*********************************************************/
*                                                                  0682
*           CRRDFSYN=ON;            /* SYNCHRONOUS DATA      @D33SKCF*/
*           CRRDFASY=OFF;           /* NOT ASYNCHRONOUS      @D33SKCF*/
*           CRRRESP=OFF;            /* NOT RESPONSE          @D33SKCF*/
         OI    CRRDFSYN(CRRPTR),B'00000010'                        0684
         NI    CRRDFASY(CRRPTR),B'11110011'                        0684
*           CALL HSKPANYQ;          /* CALL ISTRCC63         @D33SKCF*/
         L     @04,CRRATCVT(,CRRPTR)                               0685
         L     @15,ATCRCC63(,@04)                                  0685
         BALR  @14,@15                                             0685
*         END;                      /* END CONT ANY MODE     @D33SKCF*/
*       IF FSBMNELI=ON THEN         /* ERROR LOCK            @D31KKNL*/
@RF00680 TM    FSBMNELI(FSBPTR),B'10000000'                        0687
         BNO   @RF00687                                            0687
*         CALL LCPBINIT;            /* RESET IT              @D31KKNL*/
         BAL   @14,LCPBINIT                                        0688
*       ELSE                                                       0689
*         ;                         /*                       @D31KKNL*/
@RF00687 DS    0H                                                  0690
*     END;                          /* END-ON DISC           @D31KKNL*/
*   RTNREG=CRRRTN1;                 /* RESTORE THE RETURN REG        */
@RC00648 L     RTNREG,CRRRTN1(,CRRPTR)                             0691
*   END CKUNSD;                                                    0692
@EL00004 DS    0H                                                  0692
@EF00004 DS    0H                                                  0692
@ER00004 BR    @14                                                 0692
@PB00004 DS    0H                                                  0692
*   GEN NOSETS NOREFS(EJECT);                                      0693
         EJECT
*                                                                  0694
*/********************************************************************/
*/*                                                                  */
*/* ROUTINE NAME : CKTYPE - GET REQUEST TYPE                         */
*/*                                                                  */
*/* FUNCTION : CHECK REQUEST TYPE AND INITIALIZE THE DVTPTR TO PT TO */
*/*      THE PROPER DVT NAME.                                        */
*/*                                                                  */
*/* INPUT : CRRPTR (REG 13) - PTR TO THE CRR                         */
*/*                                                                  */
*/* OUTPUT : DVTPTR (REG 11) - PTR TO A DVT NAME                     */
*/*                                                                  */
*/* SUBROUTINES CALLED : NONE                                        */
*/*                                                                  */
*/* MACROS USED : NONE                                               */
*/*                                                                  */
*/********************************************************************/
*                                                                  0694
*CKTYPE:                                                           0694
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0694
         B     @PB00005                                            0694
CKTYPE   DS    0H                                                  0695
*   RFY                                                            0695
*     LCPBPTR RSTD;                                                0695
*   LCPBPTR=CRRLCPB;                /* PROC LOAD REG                 */
*                                                                  0696
         L     LCPBPTR,CRRLCPB(,CRRPTR)                            0696
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK THE REQUEST TYPE FOR THE DO MACRO. IF IT IS , SET THE   */
*   /* LDO/LCCW PROC SWITCH AND INIT PTR TO ADDR OF DO DVT NAME      */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0697
*   IF DCLRQTP=DCLDO                /* IO REQ A DO                   */
*     THEN                                                         0697
         CLI   DCLRQTP(LCPBPTR),X'13'                              0697
         BNE   @RF00697                                            0697
*     DO;                                                          0698
*       DVTPTR=ADDR(DVTDO);         /* SET UP SID                    */
         LA    DVTPTR,DVTDO                                        0699
*     END;                                                         0700
*                                                                  0700
*   /*****************************************************************/
*   /*                                                               */
*   /* NOT A DO. CHECK FOR SOL/READ/WRITE REQUEST. IF SO, PT TO THE  */
*   /* SOLICIT/READ/WRITE DVT NAME.                                  */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0701
*   ELSE                                                           0701
*     IF DCLRQTP=DCLSLCIT×          /* SOL.RD.WR REQ                 */
*         DCLRQTP=DCLREAD×DCLRQTP=DCLWRITE THEN                    0701
         B     @RC00697                                            0701
@RF00697 CLI   DCLRQTP(LCPBPTR),X'1E'                              0701
         BE    @RT00701                                            0701
         CLI   DCLRQTP(LCPBPTR),X'1D'                              0701
         BE    @RT00701                                            0701
         CLI   DCLRQTP(LCPBPTR),X'11'                              0701
         BNE   @RF00701                                            0701
@RT00701 DS    0H                                                  0702
*       DVTPTR=ADDR(DVTSRW);        /* PTR TO SID                    */
         LA    DVTPTR,DVTSRW                                       0702
*     ELSE                                                         0703
*       DVTPTR=ADDR(DVTRSET);       /* MUST BE A RESET               */
         B     @RC00701                                            0703
@RF00701 LA    DVTPTR,DVTRSET                                      0703
*   RETURN;                                                        0704
@EL00005 DS    0H                                                  0704
@EF00005 DS    0H                                                  0704
@ER00005 BR    @14                                                 0704
*   END CKTYPE;                                                    0705
         B     @EL00005                                            0705
@PB00005 DS    0H                                                  0705
*   GEN NOSETS NOREFS(EJECT);                                      0706
*                                                                  0706
         EJECT
*/********************************************************************/
*/*                                                                  */
*/* ROUTINE NAME : CKCONT - CONTINUE MACRO CHECK                     */
*/*                                                                  */
*/* FUNCTION : CHECKS FOR AN INVALID MACRO REQUEST, IF FOUND ERROR   */
*/*      CODES ARE SET, FSB FLAGS ARE SET , AND THE FSB IS MOVED TO  */
*/*      PAGEABLE STORAGE. OTHERWISE, IF THE TYPE IS NOT A DO THE    */
*/*      SID IS FOUND FOR THE REQUEST. IF A DO REQUEST THE VALIDITY  */
*/*      OF THE LDO IS CHECKED.                                      */
*/*                                                                  */
*/* INPUT : LCPBPTR (REG 4) - PTR TO THE LCPB                        */
*/*         FSBPTR (REG 5) - PTR TO THE FSB                          */
*/*         DVTPTR (REG 11) - 0                                      */
*/*                                                                  */
*/* OUTPUT : DVTPTR (REG 11) - MAY PT TO A DVT NAME                  */
*/*           CRRLDO MAY BE MODIFIED                                 */
*/*           CRR DO POST FLAG MAY BE SET                            */
*/*           CRRRTNCD MAY BE CHANGED                                */
*/*                                                                  */
*/* SUBROUTINES CALLED : CKCONT , CKTYPE , XFRFSB                    */
*/*                                                                  */
*/* MACROS USED : NONE                                               */
*/*                                                                  */
*/********************************************************************/
*                                                                  0707
*CKCONT:                                                           0707
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0707
         B     @PB00006                                            0707
CKCONT   DS    0H                                                  0708
*   CRRRTN1=RTNREG;                                                0708
         ST    RTNREG,CRRRTN1(,CRRPTR)                             0708
*   RFY                                                            0709
*    (WRKREG,                                                      0709
*     WRKREG1) RSTD;                                               0709
*   LCPBPTR=CRRLCPB;                /* GET THE ADDR OF THE LCPB    0710
*                                                            @DM09260*/
         L     LCPBPTR,CRRLCPB(,CRRPTR)                            0710
*   WRKREG1=DCLRPLA;                /* GET NEW RPL ADDR              */
         L     WRKREG1,DCLRPLA(,LCPBPTR)                           0711
*   CRRARPL=WRKREG1;                /* RPL PTR TO CRR                */
         ST    WRKREG1,CRRARPL(,CRRPTR)                            0712
*   RESPECIFY                                                      0713
*     IFGRPL BASED(WRKREG1);        /* COVER RPL LCPB COV IN NOTUNS  */
*                                                                  0713
*   /*****************************************************************/
*   /*                                                               */
*   /* DOES THE REQUEST TYPE FALL IN THE MACRO REQUEST RANGE         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0714
*   IF RPLREQ<='13'X&RPLREQ>='11'X×RPLREQ<='1E'X&RPLREQ>='1D'X/*   0714
*                                      MACRO TYPE VALID              */
*     THEN                                                         0714
         CLI   RPLREQ(WRKREG1),19                                  0714
         BH    @GL00003                                            0714
         CLI   RPLREQ(WRKREG1),17                                  0714
         BNL   @RT00714                                            0714
@GL00003 CLI   RPLREQ(WRKREG1),30                                  0714
         BH    @RF00714                                            0714
         CLI   RPLREQ(WRKREG1),29                                  0714
         BL    @RF00714                                            0714
@RT00714 DS    0H                                                  0715
*     DO;                                                          0715
*       DCLRQTP=RPLREQ;             /* MACRO TYPE TO LCPB            */
*                                                                  0716
         MVC   DCLRQTP(1,LCPBPTR),RPLREQ(WRKREG1)                  0716
*       /*************************************************************/
*       /*                                                           */
*       /* THIS IS A VALID MACRO, IF THIS IS A READ OR A SOLICIT THE */
*       /* NODE FLAG IS CHECKED ,IF ON, THIS IS AN ANY(ALL) REQUEST  */
*       /* AND MUST BE SO NOTED IN THE LCPB TO BE PASSED TO TPIOS    */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0717
*       IF(RPLREQ='1E'X×RPLREQ='1D'X)&/* READ OR SOLICT              */
*           RPLNODE=ON              /* NODE IND                      */
*         THEN                                                     0717
         CLI   RPLREQ(WRKREG1),30                                  0717
         BE    @GL00006                                            0717
         CLI   RPLREQ(WRKREG1),29                                  0717
         BNE   @RF00717                                            0717
@GL00006 TM    RPLNODE(WRKREG1),B'00000010'                        0717
         BNO   @RF00717                                            0717
*         DCLBNSP=ON;               /* SET READ ANY SOL              */
*                                                                  0718
         OI    DCLBNSP(LCPBPTR),B'00000001'                        0718
*       /*************************************************************/
*       /*                                                           */
*       /* IF THIS A DO MACRO REQUEST THE LDO(S) HAVE TO BE CHECKED  */
*       /* FOR VALIDITY. THE CKCONT1 SUBRTN IS CALLED TO DO THIS.    */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0719
*       IF RPLREQ='13'X             /* MACRO A DO                    */
*         THEN                                                     0719
@RF00717 CLI   RPLREQ(WRKREG1),19                                  0719
         BNE   @RF00719                                            0719
*         DO;                                                      0720
*           RFY                                                    0721
*             ISTLDO BASED(WRKREG); /* COVER LDO                     */
*           WRKREG=RPLAREA;         /* GET LDO ADDR                  */
         L     WRKREG,RPLAREA(,WRKREG1)                            0722
*           CALL CKCONT1;           /* CHECK OUT LDOS                */
*                                                                  0723
         BAL   @14,CKCONT1                                         0723
*           /*********************************************************/
*           /*                                                       */
*           /* CRRSW1 IS CHECKED TO SEE IF THE LDO CHAIN WAS VALID.  */
*           /* IF IN- VALID, THE FSB FLAGS ARE SET AND THE XFRFSB IS */
*           /* CALLED TO MOVE THE FSB TO PAGEABLE STORAGE.           */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0724
*           IF CRRSW1=ON            /* IF BAD LDO CHN                */
*             THEN                                                 0724
         TM    CRRSW1(CRRPTR),B'01000000'                          0724
         BNO   @RF00724                                            0724
*             DO;                   /*                               */
*               FSBELI=OFF;         /* TURN OFF END DATA             */
*               FSBECI=OFF;         /* TURN OFF END OF COMMAND     0727
*                                      INDICATOR                     */
         NI    FSBELI(FSBPTR),B'00111111'                          0727
*               CALL XFRFSB;        /* Q PAGEABLE FSB                */
         BAL   @14,XFRFSB                                          0728
*               CRRCLDO=ADDR(ISTLDO);/* LDO ADDR TO CRR              */
         ST    WRKREG,CRRCLDO(,CRRPTR)                             0729
*               CRRPOST=ON;         /* TURN ON DO POST               */
         OI    CRRPOST(CRRPTR),B'00010000'                         0730
*             END;                  /* END BAD LDO CHAIN             */
*           ELSE                                                   0732
*             DO;                   /* IF GOOD LDO CHAIN             */
*                                                                  0732
         B     @RC00724                                            0732
@RF00724 DS    0H                                                  0733
*               /*****************************************************/
*               /*                                                   */
*               /* GET THE ADDRESS OF THE FIRST LDO AND IF WE HAVE A */
*               /* JUMP ,WE WANT TO PUT THE ADDRESS OF THE LDO IN THE*/
*               /* LCCW EXTENSION.                                   */
*               /*                                                   */
*               /*****************************************************/
*                                                                  0733
*               WRKREG=RPLAREA;     /* POINT TO FIRST LDO            */
         L     WRKREG,RPLAREA(,WRKREG1)                            0733
*               IF LDOCMD=LDOJUMP THEN/* IF FIRST IS A JUMP          */
         CLI   LDOCMD(WRKREG),X'08'                                0734
         BNE   @RF00734                                            0734
*                 WRKREG=LDOADDR;   /* GET ADDR OF READ              */
         L     WRKREG,LDOADDR(,WRKREG)                             0735
*               ELSE                                               0736
*                 ;                 /* NOT A JUMP                    */
@RF00734 DS    0H                                                  0737
*               RFY                                                0737
*                 ISTLCCW BASED(ADDR(LCPLCCW));                    0737
*               IF LCCWOP=LTIC      /* IF LCCW IS A TIC              */
*                 THEN                                             0738
         CLI   LCCWOP+48(LCPBPTR),X'08'                            0738
         BNE   @RF00738                                            0738
*                 EVAL(CRRCLCCW+32)->ADDRLDO=WRKREG;/* LDO ADDR TO 0739
*                                      EXTEN.                        */
*                                                                  0739
         LA    @15,32                                              0739
         AL    @15,CRRCLCCW(,CRRPTR)                               0739
         ST    WRKREG,ADDRLDO(,@15)                                0739
*               /*****************************************************/
*               /*                                                   */
*               /* GET THE ADDR OF THE LDO IN THE LCPB OR THE LCCW   */
*               /* EXTENSION AND CALL CKTYPE TO SETUP DVTPTR TO PT TO*/
*               /* THE DVTNAME FOR DCC31                             */
*               /*                                                   */
*               /*****************************************************/
*                                                                  0740
*               ELSE                                               0740
*                 DCLLDO1=WRKREG;   /* LDO ADDR TO LCPB              */
         B     @RC00738                                            0740
@RF00738 ST    WRKREG,DCLLDO1(,LCPBPTR)                            0740
*               CALL CKTYPE;        /* CHECK MACRO TYPE              */
@RC00738 BAL   @14,CKTYPE                                          0741
*             END;                                                 0742
*         END;                                                     0743
*       ELSE                                                       0744
*         CALL CKTYPE;              /* CHECK FOR MACRO T             */
         B     @RC00719                                            0744
@RF00719 BAL   @14,CKTYPE                                          0744
*     END;                          /* END VALID MACRO REQUEST       */
*   ELSE                                                           0746
*     DO;                                                          0746
*                                                                  0746
         B     @RC00714                                            0746
@RF00714 DS    0H                                                  0747
*       /*************************************************************/
*       /*                                                           */
*       /* THE MACRO REQUESTED WAS NOT VALID, SET THE RETURN CODE AND*/
*       /* CALL XFRFSB TO MOVE THE FSB TO PAGEABLE STORAGE AND SET DO*/
*       /* POST                                                      */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0747
*       CRRRTNCD=CRRLOGIC;          /* SET RET CODE                  */
         MVI   CRRRTNCD(CRRPTR),X'08'                              0747
*       CRRFDB2=CRRCRIRT;                                          0748
         MVI   CRRFDB2(CRRPTR),X'1C'                               0748
*       CRRSW1=ON;                  /* SET SW                        */
         OI    CRRSW1(CRRPTR),B'01000000'                          0749
*       FSBELI=OFF;                 /* TURN OFF END DATA             */
*       FSBECI=OFF;                 /* END COMMUNICATIONS            */
         NI    FSBELI(FSBPTR),B'00111111'                          0751
*       CALL XFRFSB;                /* Q PAGEABLE FSB                */
         BAL   @14,XFRFSB                                          0752
*       CRRPOST=ON;                 /* TURN ON DO POST               */
         OI    CRRPOST(CRRPTR),B'00010000'                         0753
*     END;                                                         0754
*                                                                  0754
*   /*****************************************************************/
*   /*                                                               */
*   /* A CHECK IS MADE TO SEE IF THE COMMAND WAS RESET OR A CONTROL  */
*   /* BLOCK ERROR WAS ENCOUNTER. IF EITHER IS FOUND THE PROPER      */
*   /* RETURN CODE IS SET                                            */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0755
*   IF FSBMJRCD=FRCMDRST&FMCFLGA5=ON THEN                          0755
@RC00714 CLI   FSBMJRCD(FSBPTR),X'18'                              0755
         BNE   @RF00755                                            0755
         L     @15,CRRFMCB(,CRRPTR)                                0755
         TM    FMCFLGA5(@15),B'00000100'                           0755
         BNO   @RF00755                                            0755
*     DO;                                                          0756
*       FSBMJRCD=FRPURGE;           /* COMMAND PURGED                */
         MVI   FSBMJRCD(FSBPTR),X'1C'                              0757
*       FSBMNRCD=FRPDBTEX;          /* REASON: BUFFER EXCEEDED       */
         MVI   FSBMNRCD(FSBPTR),X'09'                              0758
*     END;                                                         0759
*   ELSE                                                           0760
*     IF FSBMJRCD=FRCBLKE THEN                                     0760
         B     @RC00755                                            0760
@RF00755 CLI   FSBMJRCD(FSBPTR),X'04'                              0760
         BNE   @RF00760                                            0760
*       FSBMJRCD=FRLOGICE;                                         0761
         MVI   FSBMJRCD(FSBPTR),X'08'                              0761
*   RTNREG=CRRRTN1;                 /* RESTORE RETURN REG            */
@RF00760 DS    0H                                                  0762
@RC00755 L     RTNREG,CRRRTN1(,CRRPTR)                             0762
*   RETURN;                                                        0763
@EL00006 DS    0H                                                  0763
@EF00006 DS    0H                                                  0763
@ER00006 BR    @14                                                 0763
*   END CKCONT;                                                    0764
@PB00006 DS    0H                                                  0764
*   GEN NOSETS NOREFS(EJECT);                                      0765
         EJECT
*                                                                  0766
*/********************************************************************/
*/*                                                                  */
*/* ROUTINE NAME : CKCONT1 - CONTINUE CHECK PART(1)                  */
*/*                                                                  */
*/* FUNCTION : CHECKS FOR INVALID LDO(S). CHECKS MADE ARE AS FOLLOWS:*/
*/*               . LDO IS A JUMP TO ANOTHER JUMP LDO (INVALID)      */
*/*               . FIRST LDO NOT A JUMP AND NOT A READ (INVALID)    */
*/*            IF LDO IS VALID, THE CKCONT SUBRTN IS CALLED.         */
*/*                                                                  */
*/* INPUT : WRKREG (REG 10) - PTR TO THE FIRST LDO                   */
*/*                                                                  */
*/* OUTPUT : CRRSW1 (ERROR SWITCH) - TURNED IN CASE LDO IS INVALID   */
*/*                                                                  */
*/* SUBROUTINES CALLED : CKCONT2                                     */
*/*                                                                  */
*/* MACROS USED : NONE                                               */
*/*                                                                  */
*/********************************************************************/
*                                                                  0766
*CKCONT1:                                                          0766
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0766
         B     @PB00007                                            0766
CKCONT1  DS    0H                                                  0767
*   CRRRTN2=RTNREG;                                                0767
         ST    RTNREG,CRRRTN2(,CRRPTR)                             0767
*   RESPECIFY                                                      0768
*     ISTLDO BASED(WRKREG);                                        0768
*                                                                  0768
*   /*****************************************************************/
*   /*                                                               */
*   /* IF LDO OP CODE IS FOR A JUMP GET THE ADDR OF THE NEXT LDO     */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0769
*   IF LDOCMD=LDOJUMP THEN                                         0769
         CLI   LDOCMD(WRKREG),X'08'                                0769
         BNE   @RF00769                                            0769
*     WRKREG=LDOADDR;                                              0770
*                                                                  0770
         L     WRKREG,LDOADDR(,WRKREG)                             0770
*   /*****************************************************************/
*   /*                                                               */
*   /* CHECK THE LDO POINTED TO. IF NOT A READ IT IS INVALID.        */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0771
*   IF LDOCMD=LDOREAD THEN                                         0771
@RF00769 CLI   LDOCMD(WRKREG),X'02'                                0771
         BNE   @RF00771                                            0771
*     CALL CKCONT2;                 /* LDO MUST BE A READ            */
         BAL   @14,CKCONT2                                         0772
*   ELSE                                                           0773
*     DO;                           /* THE LDO IS INVALID            */
         B     @RC00771                                            0773
@RF00771 DS    0H                                                  0774
*       CRRRTNCD=CRRLOGIC;          /* SET LOGIC ERROR CODE          */
         MVI   CRRRTNCD(CRRPTR),X'08'                              0774
*       CRRFDB2=CRRILDOP;           /* SET INVALID LDO OPCODE        */
         MVI   CRRFDB2(CRRPTR),X'06'                               0775
*       CRRSW1=ON;                  /* SET ERROR SWITCH              */
         OI    CRRSW1(CRRPTR),B'01000000'                          0776
*     END;                                                         0777
*   RTNREG=CRRRTN2;                                                0778
@RC00771 L     RTNREG,CRRRTN2(,CRRPTR)                             0778
*   RETURN;                                                        0779
@EL00007 DS    0H                                                  0779
@EF00007 DS    0H                                                  0779
@ER00007 BR    @14                                                 0779
*   END CKCONT1;                                                   0780
@PB00007 DS    0H                                                  0780
*   GEN NOSETS NOREFS(EJECT);                                      0781
         EJECT
*                                                                  0782
*/********************************************************************/
*/*                                                                  */
*/* ROUTINE NAME : CKCONT2 - CONTINUE LDO CHECK PART(2)              */
*/*                                                                  */
*/* FUNCTION : THIS ROUTINE WILL DO THE FOLLOWING:                   */
*/*                  . COUNT THE NUMBER OF LDOS                      */
*/*                  . INSURE THAT THEY ARE ONLY DATA CHAINED        */
*/*                  . IF NEXT LDO IS A JUMP, CALL CKCONT3           */
*/*                                                                  */
*/* INPUT : WRKREG (REG 10) - PTR TO THE CURRENT LDO                 */
*/*                                                                  */
*/*                                                                  */
*/* OUTPUT : CRRSW1 (ERROR SWITCH) - MAY BE ON IF ERROR FOUND        */
*/*                                                                  */
*/* SUBROUTINES CALLED : CKCONT3                                     */
*/*                                                                  */
*/* MACROS USED : NONE                                               */
*/*                                                                  */
*/********************************************************************/
*                                                                  0782
*CKCONT2:                                                          0782
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0782
         B     @PB00008                                            0782
CKCONT2  DS    0H                                                  0783
*   CRRRTN3=RTNREG;                                                0783
         ST    RTNREG,CRRRTN3(,CRRPTR)                             0783
*   RFY                                                            0784
*     WRKREG2 RSTD;                                                0784
*                                                                  0784
*   /*****************************************************************/
*   /*                                                               */
*   /* WRKREG2 IS USED AS A SWITCH. TO GET OUT OF THE DO LOOP IT WILL*/
*   /* BE CHANGED TO A NON ZERO VALUE (4).                           */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0785
*   WRKREG2=0;                                                     0785
         SLR   @15,@15                                             0785
         LR    WRKREG2,@15                                         0785
*   CRRLDOCT=0;                     /* ZERO CNT OF LDOS      @XM06417*/
         STH   @15,CRRLDOCT(,CRRPTR)                               0786
*   DO WHILE WRKREG2=0;                                            0787
         B     @DE00787                                            0787
@DL00787 DS    0H                                                  0788
*     CRRLDOCT=CRRLDOCT+1;          /* UP COUNT IN CRR               */
*                                                                  0788
         LA    @15,1                                               0788
         AH    @15,CRRLDOCT(,CRRPTR)                               0788
         STH   @15,CRRLDOCT(,CRRPTR)                               0788
*     /***************************************************************/
*     /*                                                             */
*     /* ALSO IF THE COUNT OF LDOS SURPASSES 100 THEN THE CAN IS IN- */
*     /* VALID.                                                      */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0789
*     DO WHILE CRRLDOCT^>LDOLIMIT&  /* DO WHILE LIMIT NOT            */
*           WRKREG2=0;                                             0789
*                                                                  0789
         B     @DE00789                                            0789
@DL00789 DS    0H                                                  0790
*       /*************************************************************/
*       /*                                                           */
*       /* IF THE LDO IS DATA CHAINED THE COUNT CONTINUES. THE PTR TO*/
*       /* THE NEXT LDO IS OBTAINED AND THE CHECK CONTINUES.         */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0790
*       IF LDOACHN=ON               /* CURRENT LDO IS A              */
*         THEN                                                     0790
         TM    LDOACHN(WRKREG),B'10000000'                         0790
         BNO   @RF00790                                            0790
*         DO;                       /* READ IF DATA CHAIN            */
*           WRKREG=WRKREG+LENGTH(ISTLDO);/* EXAMINE NEXT LDO         */
*                                                                  0792
         AL    WRKREG,@CF00038                                     0792
*           /*********************************************************/
*           /*                                                       */
*           /* IF THE CMD IS A JUMP, CKCONT3 MUST BE CHECKED TO MAKE */
*           /* SURE IT IS NOT A JUMP TO A JUMP.                      */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0793
*           IF LDOCMD=LDOJUMP       /* CHECK IT OUT                  */
*             THEN                                                 0793
         CLI   LDOCMD(WRKREG),X'08'                                0793
         BNE   @RF00793                                            0793
*             DO;                   /* IN CKCONT3                    */
*               CALL CKCONT3;                                      0795
         BAL   @14,CKCONT3                                         0795
*               IF CRRSW1=ON        /* IF SEQ AFTER IT               */
*                 THEN                                             0796
         TM    CRRSW1(CRRPTR),B'01000000'                          0796
         BNO   @RF00796                                            0796
*                 WRKREG2=4;        /* BAD, RETURN                   */
         LA    WRKREG2,4                                           0797
*               ELSE                                               0798
*                 WRKREG2=0;                                       0798
         B     @RC00796                                            0798
@RF00796 SLR   WRKREG2,WRKREG2                                     0798
*             END;                                                 0799
*         END;                                                     0800
*       ELSE                                                       0801
*         DO;                       /* FOUND END OF CHAIN            */
*                                                                  0801
         B     @RC00790                                            0801
@RF00790 DS    0H                                                  0802
*           /*********************************************************/
*           /*                                                       */
*           /* IF NOT DATA CHAINED AND NOT CMD CHAINED THE SEARCH AND*/
*           /* COUNT IS COMPLETE. THE REGISTER SWITCH IS CHANGED TO  */
*           /* NON ZERO.                                             */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0802
*           IF LDOCMHN=OFF          /* IF NOT CMD CHAIN              */
*             THEN                                                 0802
         TM    LDOCMHN(WRKREG),B'01000000'                         0802
         BNZ   @RF00802                                            0802
*             DO;                   /* SO LEAVE BY SETTI             */
*               CRRDOCUR=ON;        /* NG SW AND INDICAT             */
         OI    CRRDOCUR(CRRPTR),B'10000000'                        0804
*               WRKREG2=4;          /* E END IN WRKREG2              */
         LA    WRKREG2,4                                           0805
*             END;                  /* WITH NONZERO VALUE            */
*                                                                  0806
*           /*********************************************************/
*           /*                                                       */
*           /* THE LDO IS CMD CHAINED AND READS CANNOT BE CMD CHAINED*/
*           /* .                                                     */
*           /*                                                       */
*           /*********************************************************/
*                                                                  0807
*           ELSE                                                   0807
*             DO;                   /* NOT DATA CHAINED              */
         B     @RC00802                                            0807
@RF00802 DS    0H                                                  0808
*               CRRRTNCD=CRRLOGIC;  /* BUT COMMAND CHANED            */
         MVI   CRRRTNCD(CRRPTR),X'08'                              0808
*               CRRFDB2=CRRRIOCC;                                  0809
         MVI   CRRFDB2(CRRPTR),X'1D'                               0809
*               CRRSW1=ON;          /* SO SET ERROR SW AN            */
         OI    CRRSW1(CRRPTR),B'01000000'                          0810
*               WRKREG2=4;                                         0811
         LA    WRKREG2,4                                           0811
*             END;                                                 0812
*         END;                                                     0813
@RC00802 DS    0H                                                  0814
*     END;                          /* END OF DO WHILE               */
@RC00790 DS    0H                                                  0814
@DE00789 LH    @15,CRRLDOCT(,CRRPTR)                               0814
         C     @15,@CF04336                                        0814
         BH    @DC00789                                            0814
         LTR   WRKREG2,WRKREG2                                     0814
         BZ    @DL00789                                            0814
@DC00789 DS    0H                                                  0815
*     IF WRKREG2=0                  /* LDO LIMIT EXCEEDED            */
*       THEN                                                       0815
         LTR   WRKREG2,WRKREG2                                     0815
         BNZ   @RF00815                                            0815
*       DO;                         /* AND END STILL NOT             */
*                                                                  0816
*         /***********************************************************/
*         /*                                                         */
*         /* THE COUNT OF LDOS EXCEEDED 100. INDICATE ERROR.         */
*         /*                                                         */
*         /***********************************************************/
*                                                                  0817
*         CRRRTNCD=CRRLOGIC;        /* REACHED SET                   */
         MVI   CRRRTNCD(CRRPTR),X'08'                              0817
*         CRRFDB2=CRRM100;          /* ERROR FLAGS                   */
         MVI   CRRFDB2(CRRPTR),X'1A'                               0818
*         CRRSW1=ON;                                               0819
         OI    CRRSW1(CRRPTR),B'01000000'                          0819
*         WRKREG2=4;                                               0820
         LA    WRKREG2,4                                           0820
*       END;                                                       0821
*     ELSE                                                         0822
*       ;                                                          0822
@RF00815 DS    0H                                                  0823
*   END;                                                           0823
@DE00787 LTR   WRKREG2,WRKREG2                                     0823
         BZ    @DL00787                                            0823
*   RTNREG=CRRRTN3;                                                0824
         L     RTNREG,CRRRTN3(,CRRPTR)                             0824
*   RETURN;                                                        0825
@EL00008 DS    0H                                                  0825
@EF00008 DS    0H                                                  0825
@ER00008 BR    @14                                                 0825
*   END CKCONT2;                                                   0826
@PB00008 DS    0H                                                  0826
*   RFY                                                            0827
*     WRKREG2 UNRSTD;                                              0827
*   GEN NOSETS NOREFS(EJECT);                                      0828
         EJECT
*                                                                  0829
*/********************************************************************/
*/*                                                                  */
*/* ROUTINE NAME : CKCONT3 - CONTINE CHECK PART 3                    */
*/*                                                                  */
*/* FUNCTION : MAKE SURE THE LDO JUMPED TO IS NOT ANOTHER JUMP, AND  */
*/*      THIS NEW LDO IS NOT CMD CHAINED.                            */
*/*                                                                  */
*/* INPUT : WRKREG (REG 10) - PTR TO CURRENT LDO                     */
*/*                                                                  */
*/* OUTPUT : CRRSW1 (ERROR SWITCH) - MAY BE SET IF ERROR FOUND       */
*/*                                                                  */
*/* SUBROUTINES CALLED : NONE                                        */
*/*                                                                  */
*/* MACROS USED : NONE                                               */
*/*                                                                  */
*/********************************************************************/
*                                                                  0829
*CKCONT3:                                                          0829
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0829
         B     @PB00009                                            0829
CKCONT3  DS    0H                                                  0830
*   WRKREG=LDOADDR;                 /* AT LDO JUMPED TO              */
         L     WRKREG,LDOADDR(,WRKREG)                             0830
*   IF LDOCMD=LDOJUMP               /* IF JUMP TO JUMP               */
*     THEN                                                         0831
         CLI   LDOCMD(WRKREG),X'08'                                0831
         BNE   @RF00831                                            0831
*     DO;                           /* BAD LDO CHAIN SO              */
*       CRRRTNCD=CRRLOGIC;          /* SET ERROR FLAGS AN            */
         MVI   CRRRTNCD(CRRPTR),X'08'                              0833
*       CRRFDB2=CRRRIOCC;           /* D RET CODE AND                */
         MVI   CRRFDB2(CRRPTR),X'1D'                               0834
*       CRRSW1=ON;                  /* LEAVE                         */
         OI    CRRSW1(CRRPTR),B'01000000'                          0835
*     END;                                                         0836
*   ELSE                                                           0837
*     DO;                           /* IF NOT DATA CHAINE            */
         B     @RC00831                                            0837
@RF00831 DS    0H                                                  0838
*       IF LDOACHN=OFF&             /* D BUT COMMAND CHAI            */
*           LDOCMHN=ON              /* NED THEN SET ERROR            */
*         THEN                                                     0838
         TM    LDOCMHN(WRKREG),B'01000000'                         0838
         BNO   @RF00838                                            0838
         TM    LDOACHN(WRKREG),B'10000000'                         0838
         BNZ   @RF00838                                            0838
*         DO;                       /* FLAGS AND RETURN              */
*           CRRRTNCD=CRRLOGIC;      /* CODE AND LEAVE                */
         MVI   CRRRTNCD(CRRPTR),X'08'                              0840
*           CRRSW1=ON;                                             0841
         OI    CRRSW1(CRRPTR),B'01000000'                          0841
*           CRRFDB2=CRRRIOCC;                                      0842
         MVI   CRRFDB2(CRRPTR),X'1D'                               0842
*         END;                                                     0843
*     END;                                                         0844
*   RETURN;                                                        0845
@EL00009 DS    0H                                                  0845
@EF00009 DS    0H                                                  0845
@ER00009 BR    @14                                                 0845
*   END CKCONT3;                                                   0846
         B     @EL00009                                            0846
@PB00009 DS    0H                                                  0846
*   GEN NOSETS NOREFS(EJECT);                                      0847
         EJECT
*                                                                  0848
*/********************************************************************/
*/*                                                                  */
*/* ROUTINE NAME : UECBINIT - UECB INITIALIZATION                    */
*/*                                                                  */
*/* FUNCTION : INITIALIZE THE UECB  COMMON FIELDS WITH THE COMMON IN-*/
*/*      FORMATION AND THEN DETERMINE WHAT SPECIFIC EXIT IS BEING RE-*/
*/*      QUESTED SO THE UECB ID AND ,IF NECESSARY, DATA COUNT CAN AL-*/
*/*      SO BE FILLED IN. THE UECB IS THEN QUEUED TO ITSELF AND SCHED*/
*/*                                                                  */
*/* INPUT : CRRSW1 - SET FOR LOSTERM, NOT SET FOR ATTN               */
*/*         CRRSW2 - SET FOR LOSTERM OR ATTN, NOT SET FOR UNSIP      */
*/*         WRKREG1 (REG 9) - PTR TO THE ACB                         */
*/*                                                                  */
*/* OUTPUT : USER EXIT SCHEDULED                                     */
*/*                                                                  */
*/* SUBROUTINES CALLED : NONE                                        */
*/*                                                                  */
*/* MACROS USED : REQSTORE , TPQUE                                   */
*/*                                                                  */
*/********************************************************************/
*                                                                  0848
*UECBINIT:                                                         0848
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0848
         B     @PB00010                                            0848
UECBINIT DS    0H                                                  0849
*   CRRRTN3=RTNREG;                                                0849
         ST    RTNREG,CRRRTN3(,CRRPTR)                             0849
*   RFY                                                            0850
*    (UECBPTR,                                                     0850
*     WRKREG1,                                                     0850
*     WRKREG2,                                                     0850
*     RCREG) RSTD;                                                 0850
*   RFY                                                            0851
*     ISTFDB BASED(WRKREG2);                                       0851
*                                                                  0851
*   /*****************************************************************/
*   /*                                                               */
*   /* GET THE STORAGE FOR THE USER EXIT CB. IF STORAGE IS NOT IMMED */
*   /* IATELY AVAILABLE, ISSUE A TPWAIT AND WAIT UNTIL IT IS ACQUIRED*/
*   /* REQSTORE CBID(UECB)RPH(RPHPTR)CLEAR(YES)PRIORITY(YES)         */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0852
*   DO;                                                            0852
*                                                                  0852
*/* ATBUILD (ISTDYPAB,ISTATCVT,ISTRPH)                               */
*/* END OF ATBUILD                                                   */
*                                                                  0853
*     RFY                                                          0853
*      (GPR02P,                                                    0853
*       GPR03P,                                                    0853
*       GPR04P,                                                    0853
*       GPR15P) RSTD;                                              0853
*     RFY                                                          0854
*      (GPR01P,                                                    0854
*       GPR13P) RSTD;                                              0854
*     GENERATE REFS(GPR01P,GPR02P,GPR13P) SETS(RPHWORK);           0855
           STM GPR02P,GPR13P,RPHWORK(GPR01P)
*     RFY                                                          0856
*      (GPR01P,                                                    0856
*       GPR13P) UNRSTD;                                            0856
*     DO;                           /* LISATCVT                    0857
*                                      CVTBASE(PAGEABLE,(GPR15P))    */
*       GPR15P=ATCLCPTR;            /* SET UP ATCVT POINTER          */
         L     GPR15P,ATCLCPTR                                     0858
*     END;                          /* LISATCVT                      */
*     GPR02P=GPR15P->ATCBPDA×'80000000'X;                          0860
         L     GPR02P,ATCBPDA(,GPR15P)                             0860
         O     GPR02P,@CF01364                                     0860
*     DCL                                                          0861
*       1 FMZA0062 FIXED(31) STATIC LOCAL,                         0861
*        2 * FIXED(8) INIT(248),                                   0861
*        2 * PTR(24) INIT(0);                                      0861
*     GPR03F=FMZA0062;                                             0862
         L     GPR03F,FMZA0062                                     0862
*     GPR04F=17;                                                   0863
         LA    GPR04F,17                                           0863
*     GPR15P=GPR15P->ATCSMRQ;                                      0864
         L     GPR15P,ATCSMRQ(,GPR15P)                             0864
*     DCL                                                          0865
*       FMZL0062 BASED(GPR15P) LABEL VALUERANGE(*);                0865
*     CALL FMZL0062;                                               0866
         BALR  @14,@15                                             0866
*     RFY                                                          0867
*      (GPR13P) RSTD;                                              0867
*     GENERATE REFS(GPR01P,RPHWORK) SETS(GPR02P,GPR13P);           0868
           LM GPR02P,GPR13P,RPHWORK(GPR01P)
*     RFY                                                          0869
*      (GPR13P) UNRSTD;                                            0869
*     RFY                                                          0870
*      (GPR02P,                                                    0870
*       GPR03P,                                                    0870
*       GPR04P,                                                    0870
*       GPR15P) UNRSTD;                                            0870
*   END;                            /* ******** END OF REQSTORE    0871
*                                      EXPANSION GET STORAGE FOR UECB*/
*   IF RCREG=8 THEN                                                0872
*                                                                  0872
         C     RCREG,@CF00038                                      0872
         BNE   @RF00872                                            0872
*/*  TPWAIT                                                          */
*                                                                  0873
*     DO;                           /* BEGIN TPWAIT GENERATION       */
*       RFY                                                        0874
*        (GPR01P,                                                  0874
*         GPR14P,                                                  0874
*         GPR15P) RSTD;                                            0874
*       RFY                                                        0875
*        (GPR00P) RSTD;                                            0875
*       GEN(STM   GPR00P,GPR15P,RPHWORK(GPR01P))                   0876
*       REFS(GPR00P,GPR01P,GPR15P) SETS(RPHWORK);                  0876
         STM   GPR00P,GPR15P,RPHWORK(GPR01P)
*       RFY                                                        0877
*        (GPR00P) UNRSTD;                                          0877
*       GPR15P=ADDR(LHAA0081);                                     0878
         LA    GPR15P,LHAA0081                                     0878
*       GPR14P=0;                                                  0879
         SLR   GPR14P,GPR14P                                       0879
*       GPR15P=GPR15P&'00FFFFFF'X;                                 0880
         N     GPR15P,@CF04438                                     0880
*       GPR15P=GPR15P×RPHWT1;                                      0881
         O     GPR15P,@CF01305                                     0881
*       CS(GPR14P,GPR15P,GPR01P->RPHRESMA);                        0882
         CS    GPR14P,@15,RPHRESMA(GPR01P)                         0882
*       BC(7,LHAB0081);                                            0883
*                                                                  0883
         BC    7,LHAB0081                                          0883
*/* TPEXIT  NONE                                                     */
*                                                                  0884
*       DO;                                                        0884
*         RESPECIFY                                                0885
*           GPR15P RSTD;                                           0885
*                                                                  0885
*/* ATBUILD (ISTPAB,ISTDYPAB,ISTATCVT)                               */
*/* END OF ATBUILD                                                   */
*                                                                  0886
*         DO;                       /* LISATCVT                    0886
*                                      CVTBASE(PAGEABLE,(GPR15P))    */
*           GPR15P=ATCLCPTR;        /* SET UP ATCVT POINTER          */
         L     GPR15P,ATCLCPTR                                     0887
*         END;                      /* LISATCVT                      */
