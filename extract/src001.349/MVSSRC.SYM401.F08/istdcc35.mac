*/*                                                                  */
*/* ROUTINE NAME : FREEFSB - FREE THE FSB                            */
*/*                                                                  */
*/* FUNCTION : FREE THE FSB AND ALL ASSOCIATED FDB(S). IF DATA IS    */
*/*      CONFIDENTIAL ,CLEAR THE DATA AREA, OTHERWISE JUST FREE IT.  */
*/*                                                                  */
*/* INPUT : FSBPTR (REG 5) - PTR TO FSB                              */
*/*                                                                  */
*/* OUTPUT : NONE                                                    */
*/*                                                                  */
*/* SUBROUTINES CALLED : NONE                                        */
*/*                                                                  */
*/* MACROS USED : RELSTORE(2)                                        */
*/*                                                                  */
*/********************************************************************/
*                                                                  0555
*FREEFSB:                                                          0555
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0555
         B     @PB00002                                            0555
FREEFSB  DS    0H                                                  0556
*   RFY                                                            0556
*     WRKREG RSTD;                                                 0556
*   CRRRTN6=RTNREG;                 /* FDBS SAVE RETURN              */
         ST    RTNREG,CRRRTN6(,CRRPTR)                             0557
*   RESPECIFY                                                      0558
*     ISTFDB BASED(WRKREG);         /* COVER FDB                     */
*                                                                  0558
*   /*****************************************************************/
*   /*                                                               */
*   /* GET HE OFFSET OF THE FDB FROM THE BEGINNING OF THE FSB AND    */
*   /* SUBTRACT IT . PLACE THE CHAIN PTR TO THE NEXT FDB IN THAT WORD*/
*   /* SO THAT SMS WILL KNOW WHAT OTHER BLOCKS TO FREE.              */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0559
*   WRKREG=ADDR(FSBFDB);            /* GET ADDR OF FDB               */
         LA    WRKREG,FSBFDB(,FSBPTR)                              0559
*   FSBPTR=WRKREG-FDBOFSET;         /* ADD IN OFFSET                 */
         LR    FSBPTR,WRKREG                                       0560
         SH    FSBPTR,FDBOFSET(,WRKREG)                            0560
*   FSBPTR->FDBFDBA=FDBFDBA;        /* POINT TO NEXT FDB             */
         L     @15,FDBFDBA(,WRKREG)                                0561
         ST    @15,FDBFDBA(,FSBPTR)                                0561
*   RFY                                                            0562
*     ISTPROCD BASED(ADDR(FMCPROCD));                              0562
*                                                                  0562
*   /*****************************************************************/
*   /*                                                               */
*   /* IF DATA IS CONFIDENTIAL, CLEAR IT OUT BEFORE RELEASING IT     */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0563
*   IF PROCFTX=ON                   /* IF CONFIDENTIAL               */
*     THEN                                                         0563
*                                                                  0563
         L     @15,CRRFMCB(,CRRPTR)                                0563
         TM    PROCFTX+48(@15),B'00000001'                         0563
         BNO   @RF00563                                            0563
*     /***************************************************************/
*     /*                                                             */
*     /* RELSTORE BUFADDR(FSBPTR)CLEAR(YES)CHAIN(YES)                */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0564
*     DO;                                                          0564
*                                                                  0564
*/* ATBUILD (ISTATCVT,ISTRPH)                                        */
*/* END OF ATBUILD                                                   */
*                                                                  0565
*       RFY                                                        0565
*        (GPR02P,                                                  0565
*         GPR03P,                                                  0565
*         GPR04P,                                                  0565
*         GPR15P) RSTD;                                            0565
*       RFY                                                        0566
*        (GPR01P,                                                  0566
*         GPR13P) RSTD;                                            0566
*       GENERATE REFS(GPR01P,GPR02P,GPR13P) SETS(RPHWORK);         0567
          STM GPR02P,GPR13P,RPHWORK(GPR01P)
*       RFY                                                        0568
*         GPR13P UNRSTD;                                           0568
*       GPR04P=FSBPTR×'80000000'X;                                 0569
         LR    GPR04P,FSBPTR                                       0569
         O     GPR04P,@CF01364                                     0569
*       DCL                                                        0570
*         1 FMZA0030 FIXED(31) STATIC LOCAL,                       0570
*          2 * FIXED(8) INIT(80),                                  0570
*          2 * PTR(24) INIT(0);                                    0570
*       GPR03F=FMZA0030;                                           0571
         L     GPR03F,FMZA0030                                     0571
*       DO;                         /* LISATCVT                    0572
*                                      CVTBASE(PAGEABLE,(GPR15P))    */
*         GPR15P=ATCLCPTR;          /* SET UP ATCVT POINTER          */
         L     GPR15P,ATCLCPTR                                     0573
*       END;                        /* LISATCVT                      */
*       GPR02P=GPR15P->ATCBPDA;                                    0575
         L     GPR02P,ATCBPDA(,GPR15P)                             0575
*       GPR15P=GPR15P->ATCSMRS;                                    0576
         L     GPR15P,ATCSMRS(,GPR15P)                             0576
*       DCL                                                        0577
*         FMZL0030 BASED(GPR15P) LABEL VALUERANGE(*);              0577
*       CALL FMZL0030;                                             0578
         BALR  @14,@15                                             0578
*       RFY                                                        0579
*         GPR13P RSTD;                                             0579
*       GENERATE REFS(GPR01P,RPHWORK) SETS(GPR02P,GPR13P);         0580
          LM GPR02P,GPR13P,RPHWORK(GPR01P)
*       RFY                                                        0581
*        (GPR01P,                                                  0581
*         GPR13P) UNRSTD;                                          0581
*       RFY                                                        0582
*        (GPR02P,                                                  0582
*         GPR03P,                                                  0582
*         GPR04P,                                                  0582
*         GPR15P) UNRSTD;                                          0582
*     END;                          /* *********END OF RELSTORE    0583
*                                      EXPANSION*********            */
*                                                                  0583
*   /*****************************************************************/
*   /*                                                               */
*   /* DATA NOT CONFIDENTIAL ,NO NEED TO DO THE CLEAR.               */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0584
*   ELSE                                                           0584
*                                                                  0584
*     /***************************************************************/
*     /*                                                             */
*     /* RELSTORE BUFADDR(FSBPTR)CHAIN(YES)                          */
*     /*                                                             */
*     /***************************************************************/
*                                                                  0584
*     DO;                                                          0584
*                                                                  0584
         B     @RC00563                                            0584
@RF00563 DS    0H                                                  0585
*/* ATBUILD (ISTATCVT,ISTRPH)                                        */
*/* END OF ATBUILD                                                   */
*                                                                  0585
*       RFY                                                        0585
*        (GPR02P,                                                  0585
*         GPR03P,                                                  0585
*         GPR04P,                                                  0585
*         GPR15P) RSTD;                                            0585
*       RFY                                                        0586
*        (GPR01P,                                                  0586
*         GPR13P) RSTD;                                            0586
*       GENERATE REFS(GPR01P,GPR02P,GPR13P) SETS(RPHWORK);         0587
          STM GPR02P,GPR13P,RPHWORK(GPR01P)
*       RFY                                                        0588
*         GPR13P UNRSTD;                                           0588
*       GPR04P=FSBPTR;                                             0589
         LR    GPR04P,FSBPTR                                       0589
*       DCL                                                        0590
*         1 FMZA0046 FIXED(31) STATIC LOCAL,                       0590
*          2 * FIXED(8) INIT(80),                                  0590
*          2 * PTR(24) INIT(0);                                    0590
*       GPR03F=FMZA0046;                                           0591
         L     GPR03F,FMZA0046                                     0591
*       DO;                         /* LISATCVT                    0592
*                                      CVTBASE(PAGEABLE,(GPR15P))    */
*         GPR15P=ATCLCPTR;          /* SET UP ATCVT POINTER          */
         L     GPR15P,ATCLCPTR                                     0593
*       END;                        /* LISATCVT                      */
*       GPR02P=GPR15P->ATCBPDA;                                    0595
         L     GPR02P,ATCBPDA(,GPR15P)                             0595
*       GPR15P=GPR15P->ATCSMRS;                                    0596
         L     GPR15P,ATCSMRS(,GPR15P)                             0596
*       DCL                                                        0597
*         FMZL0046 BASED(GPR15P) LABEL VALUERANGE(*);              0597
*       CALL FMZL0046;                                             0598
         BALR  @14,@15                                             0598
*       RFY                                                        0599
*         GPR13P RSTD;                                             0599
*       GENERATE REFS(GPR01P,RPHWORK) SETS(GPR02P,GPR13P);         0600
          LM GPR02P,GPR13P,RPHWORK(GPR01P)
*       RFY                                                        0601
*        (GPR01P,                                                  0601
*         GPR13P) UNRSTD;                                          0601
*       RFY                                                        0602
*        (GPR02P,                                                  0602
*         GPR03P,                                                  0602
*         GPR04P,                                                  0602
*         GPR15P) UNRSTD;                                          0602
*     END;                          /* *********END OF RELSTORE    0603
*                                      EXPANSION*********            */
*   RTNREG=CRRRTN6;                 /* RESTORE RETURN REG            */
@RC00563 L     RTNREG,CRRRTN6(,CRRPTR)                             0604
*   RETURN;                                                        0605
@EL00002 DS    0H                                                  0605
@EF00002 DS    0H                                                  0605
@ER00002 BR    @14                                                 0605
*   RFY                                                            0606
*     WRKREG UNRSTD;                                               0606
*   END FREEFSB;                                                   0607
@PB00002 DS    0H                                                  0607
