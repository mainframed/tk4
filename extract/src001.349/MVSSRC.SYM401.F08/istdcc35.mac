*/*                                                                  */
*/* ROUTINE NAME : XFRFSB - TRANSFER FSB                             */
*/*                                                                  */
*/* FUNCTION : TRANSFER THE FSB WHICH IS IN FIXED STORAGE TO PAGEABLE*/
*/*      STORAGE. ALSO XFRFSB GETS A COUNT OF ALL THE FDBS SO THE FMC*/
*/*      BUFFER COUNT CAN BE UPDATED.                                */
*/*                                                                  */
*/* INPUT : FSBPTR (REG 5) - PTR TO THE FIXED FSB                    */
*/*                                                                  */
*/* OUTPUT : FSBPTR (REG 5) - PTR TO THE PAGEABLE FSB                */
*/*                                                                  */
*/* SUBROUTINES CALLED : NONE                                        */
*/*                                                                  */
*/* MACROS USED : NONE                                               */
*/*                                                                  */
*/********************************************************************/
*                                                                  1022
*XFRFSB:                                                           1022
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               1022
         B     @PB00012                                            1022
XFRFSB   DS    0H                                                  1023
*   CRRRTN5=RTNREG;                 /* SAVE RETURN ADDR              */
         ST    RTNREG,CRRRTN5(,CRRPTR)                             1023
*   RFY                                                            1024
*     ISTATCVT BASED(ATCVTPTR);     /* COVER VTAM CVT                */
*   RFY                                                            1025
*     ATCVTPTR RSTD;                                               1025
*   ATCVTPTR=CRRATCVT;              /* GET ADDR OF ATCVT             */
         L     ATCVTPTR,CRRATCVT(,CRRPTR)                          1026
*   CRRPTR=ADDR(CRRSAVE1);          /* EXT SAVE AREA                 */
         LA    @00,CRRSAVE1(,CRRPTR)                               1027
         LR    CRRPTR,@00                                          1027
*   CALL MOVERTN;                   /* CALL MOVE RTN                 */
         L     @07,ATCDCC61(,ATCVTPTR)                             1028
         LR    @15,@07                                             1028
         BALR  @14,@15                                             1028
*   CRRPTR=ADDR(CRRSPACE);          /* RESTORE CRR BASE              */
         L     CRRPTR,RPHCRR(,RPHPTR)                              1029
         LA    CRRPTR,CRRSPACE+312(,CRRPTR)                        1029
*   RFY                                                            1030
