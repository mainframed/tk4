*/*                                                                  */
*/* ROUTINE NAME : LCPBINIT - INITIALIZE THE LCPB                    */
*/*                                                                  */
*/* FUNCTION : REQUEST STORAGE FOR A DCLCPB AND WAIT UNTIL STORAGE   */
*/* IS AVAILABLE.  INITIALIZE ISTDCLCPB FOR A RESET OPERATION.       */
*/* THIS SUBROUTINE IS INVOKED WHENEVER AN INTERNAL RESET NEEDS      */
*/* TO BE ISSUED.  THIS IS DONE IF I/O NEEDS TO BE RESET IF          */
*/* BUFFER THRESHOLD IS EXCEEDED OR IF THE ERROR LOCK IS SET ON      */
*/* IN A FSB BEING FLUSHED.                                          */
*/*                                                                  */
*/* INPUT : RPHPTR (REG 1) - PTR TO THE RPH                          */
*/*                                                                  */
*/* OUTPUT : LCPBPTR (REG 7) - PTR TO THE RESET LCPB                 */
*/*                                                                  */
*/* SUBROUTINES CALLED : NONE                                        */
*/*                                                                  */
*/* MACROS USED : REQSTORE , TPWAIT                                  */
*/*                                                                  */
*/********************************************************************/
*LCPBINIT: PROC OPTIONS(NOSAVE,NOSAVEAREA);                        0947
LCPBINIT DS    0H                                                  0948
*   CRRRTN8=RETPTR;                /* SAVE RETURN ADDRESS           */
         ST    RETPTR,CRRRTN8(,CRRPTR)                             0948
*    /*REQSTORE CBID(DCLCP)RPH(RPHPTR)CLEAR(YES)PRIORITY(YES)*/    0949
*     DO;                                                          0949
*/* ATBUILD (ISTDYPAB,ISTATCVT,ISTRPH)  */                         0950
*/* END OF ATBUILD */                                              0950
*                                                                  0950
*    RFY (GPR02P,GPR03P,GPR04P,GPR15P) RSTD;                       0950
*                                                                  0950
*    RFY (GPR01P,GPR13P) RSTD;                                     0951
*          GENERATE REFS(GPR01P,GPR02P,GPR13P) SETS(RPHWORK);      0952
*                                                                  0952
           STM GPR02P,GPR13P,RPHWORK(GPR01P)
*    RFY (GPR01P,GPR13P) UNRSTD;                                   0953
*                                                                  0953
*    DO; /* LISATCVT CVTBASE(PAGEABLE,(GPR15P)) */                 0954
*      GPR15P = ATCLCPTR; /*SET UP ATCVT POINTER*/                 0955
         L     GPR15P,ATCLCPTR                                     0955
*    END; /*LISATCVT*/                                             0956
*          GPR02P=GPR15P->ATCBPDA × '80000000'X;                   0957
         L     GPR02P,ATCBPDA(,GPR15P)                             0957
         O     GPR02P,@CF02699                                     0957
*          DCL 1 FMZA0075 FIXED(31) STATIC LOCAL,                  0958
*              2 *        FIXED(8) INIT(248),                      0958
*              2 *        PTR(24) INIT(0);                         0958
*          GPR03F=FMZA0075;                                        0959
         L     GPR03F,FMZA0075                                     0959
*          GPR04F=5;                                               0960
         LA    GPR04F,5                                            0960
*          GPR15P=GPR15P->ATCSMRQ;                                 0961
         L     GPR15P,ATCSMRQ(,GPR15P)                             0961
*          DCL FMZL0075 BASED(GPR15P) LABEL VALUERANGE(*);         0962
*          CALL FMZL0075;                                          0963
*                                                                  0963
         BALR  @14,@15                                             0963
*    RFY (GPR13P) RSTD;                                            0964
*          GENERATE REFS(GPR01P,RPHWORK) SETS(GPR02P,GPR13P);      0965
*                                                                  0965
           LM GPR02P,GPR13P,RPHWORK(GPR01P)
*    RFY (GPR13P) UNRSTD;                                          0966
*                                                                  0966
*    RFY (GPR02P,GPR03P,GPR04P,GPR15P) UNRSTD;                     0967
*    END ; /********* END OF REQSTORE EXPANSION **********/        0968
*   IF RETCODE = 8 THEN            /* WAS STORAGE AVAILABLE?        */
*                                                                  0969
         C     RETCODE,@CF00105                                    0969
         BNE   @RF00969                                            0969
*/*  TPWAIT   */                                                   0970
*         DO; /* BEGIN TPWAIT GENERATION*/                         0970
*         RFY (GPR01P,GPR14P,GPR15P) RSTD;                         0971
*         RFY (GPR00P) RSTD;                                       0972
*         GEN (STM   GPR00P,GPR15P,RPHWORK(GPR01P))                0973
*         REFS(GPR00P,GPR01P,GPR15P) SETS(RPHWORK);                0973
         STM   GPR00P,GPR15P,RPHWORK(GPR01P)
*         RFY (GPR00P) UNRSTD;                                     0974
*         GPR15P=ADDR(LHAA0094);                                   0975
         LA    GPR15P,LHAA0094                                     0975
*         GPR14P=0;                                                0976
         SLR   GPR14P,GPR14P                                       0976
*         GPR15P=GPR15P & '00FFFFFF'X;                             0977
         N     GPR15P,@CF04389                                     0977
*         GPR15P=GPR15P × RPHWT1;                                  0978
         O     GPR15P,@CF01423                                     0978
*         CS (GPR14P,GPR15P,GPR01P->RPHRESMA);                     0979
         CS    GPR14P,@15,RPHRESMA(GPR01P)                         0979
*         BC (7,LHAB0094);                                         0980
*                                                                  0980
         BC    7,LHAB0094                                          0980
*/* TPEXIT  NONE */                                                0981
*         DO;                                                      0981
*           RESPECIFY GPR15P RSTD;                                 0982
*                                                                  0982
*/* ATBUILD (ISTPAB,ISTDYPAB,ISTATCVT)  */                         0983
*/* END OF ATBUILD */                                              0983
*                                                                  0983
*         DO; /* LISATCVT CVTBASE(PAGEABLE,(GPR15P)) */            0983
*           GPR15P = ATCLCPTR; /*SET UP ATCVT POINTER*/            0984
         L     GPR15P,ATCLCPTR                                     0984
*         END; /*LISATCVT*/                                        0985
*           RESPECIFY GPR00F RSTD;                                 0986
*           GPR00F=0;     /*SET PARAMETER REGISTER TO 0*/          0987
         SLR   GPR00F,GPR00F                                       0987
*           GPR15P=GPR15P->ATCAP31;                                0988
         L     GPR15P,ATCAP31(,GPR15P)                             0988
*           DCL FMZL0099 BASED(GPR15P) LABEL VALUERANGE(*);        0989
*           CALL FMZL0099;                                         0990
         BALR  @14,@15                                             0990
*           RESPECIFY (GPR00F,GPR15P) UNRSTD;                      0991
*         END; /***** END OF TPEXIT  *****/                        0992
*LHAB0094:;                                                        0993
LHAB0094 DS    0H                                                  0994
*         GPR01P->RPHRESMA = 0;                                    0994
         SLR   @15,@15                                             0994
         ST    @15,RPHRESMA(,GPR01P)                               0994
*         RFY (GPR00P) RSTD;                                       0995
*LHAA0094:;                                                        0996
LHAA0094 DS    0H                                                  0997
*         GEN (LM    GPR00P,GPR15P,RPHWORK(GPR01P))                0997
*         REFS(RPHWORK,GPR01P) SETS(GPR00P,GPR15P);                0997
         LM    GPR00P,GPR15P,RPHWORK(GPR01P)
*         RFY (GPR00P,GPR01P,GPR14P,GPR15P) UNRSTD;                0998
*         END; /*****END OF TPWAIT *****/                  /* NO, WAIT
*OR STORAGE          */                                            0999
*   ELSE;                          /* YES                           */
@RF00969 DS    0H                                                  1001
*   LCPBPTR=RPHSRPRM;              /* REFERENCE STORAGE             */
         L     LCPBPTR,RPHSRPRM(,RPHPTR)                           1001
*   LCPTYPE=DCLTYPE;               /* INDICATE DCLCP                */
         MVI   LCPTYPE(LCPBPTR),X'12'                              1002
*   LCPLNGTH=LENGTH(ISTDCLCP);     /* INITAILIZE LENGTH             */
         MVI   LCPLNGTH(LCPBPTR),X'64'                             1003
*   LCPNXLCW=ADDR(DCLLCCW2);       /* POINT TO RESET LCCW SLOT      */
         LA    @15,DCLLCCW2(,LCPBPTR)                              1004
         ST    @15,LCPNXLCW(,LCPBPTR)                              1004
*   LCPDAF=NCBDAF;                 /* INITIALIZE DESTINATION ID     */
         L     @15,CRRFMCB(,CRRPTR)                                1005
         L     @08,FMCFMCBA(,@15)                                  1005
         MVC   LCPDAF(2,LCPBPTR),NCBDAF(@08)                       1005
*   LCPSAF=FMCSAF;                 /* INITIALIZE SOURCE ID          */
         MVC   LCPSAF(2,LCPBPTR),FMCSAF(@15)                       1006
*   DCLRQTP=DCLRESET;              /* INDICATE LCPB FOR RESET OP    */
*                                                                  1007
         MVI   DCLRQTP(LCPBPTR),X'12'                              1007
*   RETPTR = CRRRTN8;              /* PICK UP RETURN ADDRESS        */
         L     RETPTR,CRRRTN8(,CRRPTR)                             1008
*END LCPBINIT;                     /* RETURN TO CALLER              */
*                                                                  1009
*                                                                  1009
@EL00014 DS    0H                                                  1009
@EF00014 DS    0H                                                  1009
@ER00014 BR    @14                                                 1009
