*                                                                  0463
*   /************************************************************/ 0463
*   /*    INITIALIZE POINTERS AND SET AUDIT TRAIL SWITCHES      */ 0463
*   /************************************************************/ 0463
*                                                                  0463
*   CRRPTR = ADDR(CRRSPACE);      /* REFERENCE CRR                   */
         L     CRRPTR,RPHCRR(,RPHPTR)                              0463
         LA    CRRPTR,CRRSPACE+312(,CRRPTR)                        0463
*   FSBPTR = CRRFSB;              /* REFERENCE FSB                   */
         L     FSBPTR,CRRFSB(,CRRPTR)                              0464
*   CRRC40 = ON;                  /* SET HISTORY SWITCH              */
         OI    CRRC40(CRRPTR),B'10000000'                          0465
*   CRRMAS = CRRMAS&&CRRMAS;      /* CLEAR MODULE AUDIT SWITCHES     */
         XC    CRRMAS(8,CRRPTR),CRRMAS(CRRPTR)                     0466
*   CRRM40 = ON;                  /* INDICATE ISTDCC40 IS PROCESSING */
         OI    CRRM40(CRRPTR),B'10000000'                          0467
*   CRRSW6=OFF;                    /* INITIALIZE CONTROL SWITCH       *
*   CRRSW4=ON;                     /* INITIALIZE CONTROL SWITCH       *
         OI    CRRSW4(CRRPTR),B'00001000'                          0469
         NI    CRRSW6(CRRPTR),B'11111101'                          0469
*   CALL ENDANAL;                 /* TEST FSB FOR LAST FSB RECEIVED  */
*                                                                  0470
         BAL   @14,ENDANAL                                         0470
*   /*****************************************************************/
*   /* IF THE FLUSH INDICATOR IS ON IN THE FMCB, THIS FSB SHOULD BE  */
*   /* THROWN AWAY AND NO FEEDBACK PROCESSING SHOULD BE DONE.  IF    */
*   /* THE LCPB DOES NOT CONTAIN AN RPL ADDRESS, THEN THE REQUEST    */
*   /* WAS FOR A SOLICIT SO ALL THAT SHOULD BE DONE IS TO QUEUE THE  */
*   /* FSB ON THE FMCB DATA QUEUE SO IT CAN BE REFERENCED BY A LATER */
*   /* READ.  OTHERWISE FEEDBACK PROCESSING SHOULD CONTINUE.         */
*   /*****************************************************************/
*                                                                  0471
*   LCPBPTR = FSBLCPB;            /* REFERENCE LCPB                  */
         L     LCPBPTR,FSBLCPB(,FSBPTR)                            0471
*   IF FMCFLGA3=ON THEN           /* TEST FLUSH BIT                  */
*                                                                  0472
         L     @15,CRRFMCB(,CRRPTR)                                0472
         TM    FMCFLGA3(@15),B'00010000'                           0472
         BNO   @RF00472                                            0472
*              /***********************************************/   0473
*              /*   FLUSH INDICATOR IS SET.  THE FSB SHOULD   */   0473
*              /*   NOT BE PROCESSED, BUT INSTEAD BE THROWN   */   0473
*              /*   AWAY.                                     */   0473
*              /***********************************************/   0473
*                                                                  0473
*              CALL FLUSHRTN;     /* IGNORE DATA UNTIL EOB,EOM,OR EOT*/
         BAL   @14,FLUSHRTN                                        0473
*   ELSE;                          /* FSB SHOULD BE PROCESSED         *
@RF00472 DS    0H                                                  0475
*   IF CRRSW4=ON THEN                                              0475
         TM    CRRSW4(CRRPTR),B'00001000'                          0475
         BNO   @RF00475                                            0475
*        DO;                      /* FSB SHOULD BE PROCESSED         */
*        IF DCLRPLA=0 THEN                                         0477
         L     @15,DCLRPLA(,LCPBPTR)                               0477
         LTR   @15,@15                                             0477
         BNZ   @RF00477                                            0477
*           DO;                                                    0478
*                                                                  0478
*              /***********************************************/   0479
*              /*    SOLICIT REQUEST.  NO FEEDBACK PROCESSING */   0479
*              /*    TO DO.  SAVE FSB TO BE PROCESSED BY      */   0479
*              /*    FUTURE READ REQUEST.                     */   0479
*              /***********************************************/   0479
*                                                                  0479
*              CRRSW1=ON;         /* SOLICIT-FSB SHOULD BE QUEUED    */
*              CRRSW6=ON;         /* INDICATE SOLICIT BEING PROCESSED *
         OI    CRRSW1(CRRPTR),B'01000010'                          0480
*           END;                                                   0481
*        ELSE DO;                 /* READ OR WRITE WITH RD OR SOL    */
*                                                                  0482
         B     @RC00477                                            0482
@RF00477 DS    0H                                                  0483
*              /***********************************************/   0483
*              /*   READ REQUEST.  FIND LDO AND MOVE DATA     */   0483
*              /*   TO USER'S BUFFER.                         */   0483
*              /***********************************************/   0483
*                                                                  0483
*             CRRSW1=OFF;         /* CLEAR FSB QUEUE INDICATOR       */
         NI    CRRSW1(CRRPTR),B'10111111'                          0483
*             CALL LDOPROC;       /* CONTINUE PROCESSING             */
         BAL   @14,LDOPROC                                         0484
*             END;                                                 0485
*        IF CRRSW1=ON THEN                                         0486
*                                                                  0486
@RC00477 TM    CRRSW1(CRRPTR),B'01000000'                          0486
         BNO   @RF00486                                            0486
*              /***********************************************/   0487
*              /*   FSB IS TO BE QUEUED ON THE FMCB DATA Q    */   0487
*              /***********************************************/   0487
*                                                                  0487
*              CALL HOLDDATA;     /* SAVE FSB ON FMCB DATA QUEUE     */
         BAL   @14,HOLDDATA                                        0487
*        ELSE;                    /* FSB WAS RELEASED                */
@RF00486 DS    0H                                                  0489
*        END;                                                      0489
*   ELSE;                                                          0490
*                                                                  0490
@RF00475 DS    0H                                                  0491
*/*TPESC*/                                                         0491
*     DO;                                                          0491
*                                                                  0491
*/* ATBUILD (ISTDYPAB,ISTAVT,ISTATCVT)  */                         0492
