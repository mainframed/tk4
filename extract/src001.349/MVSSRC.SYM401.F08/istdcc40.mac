*       GPR15P = ATCLCPTR; /*SET UP ATCVT POINTER*/                0495
         L     GPR15P,ATCLCPTR                                     0495
*     END; /*LISATCVT*/                                            0496
*       GPR15P=GPR15P->ATCESC01;                                   0497
         L     GPR15P,ATCESC01(,GPR15P)                            0497
*       DCL FMZL0005 BASED(GPR15P) LABEL VALUERANGE(*);            0498
*       CALL FMZL0005;                                             0499
         BALR  @14,@15                                             0499
