*FEEDBACK FINISH         */                                        0501
*/********************************************************************/
*/*                                                                  */
*/* ROUTINE NAME : ENDANAL - END OF LCPB ANALYSIS                    */
*/*                                                                  */
*/* FUNCTION : SETS THE END OF LCCW FLAG AND TURNS OFF THE DATA EX-  */
*/* PECTED FLAG IF THIS IS THE END OF THE LCPB.                      */
*/*                                                                  */
*/* INPUT : FSBPTR (REG 6) - PTR TO THE FSB                          */
*/*         CRRPTR (REG 13) - PTR TO THE CRR                         */
*/*                                                                  */
*/* OUTPUT : CRRENDLW POSSIBLY SET                                   */
*/*          FMCFLGA0 TURNED OFF                                     */
*/*                                                                  */
*/* SUBROUTINES CALLED : NONE                                        */
*/*                                                                  */
*/* MACROS USED : NONE                                               */
*/*                                                                  */
*/********************************************************************/
*ENDANAL: PROC OPTIONS(NOSAVE,NOSAVEAREA);                         0502
*                                                                  0502
         B     @PB00002                                            0502
ENDANAL  DS    0H                                                  0503
*   CRRRTN1 = RETPTR;             /* SAVE RETURN ADDRESS             */
         ST    RETPTR,CRRRTN1(,CRRPTR)                             0503
*   IF FSBELI=ON THEN             /* END OF LOGICAL CHANNEL PGM */ 0504
         TM    FSBELI(FSBPTR),B'10000000'                          0504
         BNO   @RF00504                                            0504
*         DO;                     /* ENDING CONDITION MET            */
*                                                                  0505
*         /**************************************/                 0506
*         /*   END OF LOGICAL CHANNEL PROGRAM   */                 0506
*         /*   SET SWITCH IN CRR SO ISTDCC32    */                 0506
*         /*   WILL FREE STORAGE OF ISTDCLCPB.  */                 0506
*         /*   INDICATE NO MORE DATA EXPECTED   */                 0506
*         /*   FROM THIS DEVICE.                */                 0506
*         /**************************************/                 0506
*                                                                  0506
*         RFY (REG10,REG11) RSTD;                                  0506
*    CS1: REG10=FMCCMPS2;          /* REFERENCE OLD FLAG VALUE       */
CS1      L     @15,CRRFMCB(,CRRPTR)                                0507
         L     REG10,FMCCMPS2(,@15)                                0507
*         REG11=REG10&FLGA0OFF;    /* SET NEW FLAG VALUE             */
         LR    REG11,REG10                                         0508
         N     REG11,@CF03439                                      0508
*         CS (REG10,REG11,FMCCMPS2);  /* SET NEW VALUE IN FMCB       */
         CS    REG10,@11,FMCCMPS2(@15)                             0509
*         BC (7,CS1);              /* TRY AGAIN IF FAILURE           */
         BC    7,CS1                                               0510
*         RFY (REG10,REG11) UNRSTD;                                0511
*         CRRENDLW=ON;            /* INDICATE END OF LCCW'S          */
         OI    CRRENDLW(CRRPTR),B'00100000'                        0512
*         END;                                                     0513
*   ELSE;                         /* MORE DATA TO COME               */
*                                                                  0514
@RF00504 DS    0H                                                  0515
*   RETPTR = CRRRTN1;             /* GET RETURN ADDRESS              */
         L     RETPTR,CRRRTN1(,CRRPTR)                             0515
*END ENDANAL;                     /* RETURN TO MAIN PROCEDURE        */
@EL00002 DS    0H                                                  0516
@EF00002 DS    0H                                                  0516
@ER00002 BR    @14                                                 0516
*/********************************************************************/
*/*                                                                  */
*/* ROUTINE NAME : HOLDDATA - PRESERVE FSB                           */
*/*                                                                  */
*/* FUNCTION : HOLDDATA IS ENTERED TO QUEUE A FSB, THE ADDRESS OF    */
*/* WHICH IS IN FSBPTR, TO THE FMCB DATA QUEUE SO IT CAN BE SAVED  */
*/* FOR A LATER READ.  THE FSB WAS RECEIVED FROM TPIOS AND SO        */
*/* RESIDES IN FIXED STORAGE.  THE FSB WILL FIRST BE MOVED           */
*/* TO PAGEABLE STORAGE SO THAT THE FIXED STORAGE WILL NOT BE        */
*/* TIED UP.  THE COUNT OF BUFFERS IN USE WILL BE UPDATED BY THE     */
*/* NUMBER OF BUFFERS OCCUPIED BY THE FSB AND FDBS.  IF THE BUFFER   */
*/* LIMIT HAS BEEN EXCEEDED, A RESET WILL BE ISSUED TO RESET ANY     */
*/* OUTSTANDING I/O SINCE NO BUFFERS ARE AVAILABLE FOR THE           */
*/* FEEDBACK.  IF THE FSB TO BE QUEUED IS FROM A SOLICIT AND THE     */
*/* FLUSH INDICATOR IS NOW SET, INVOKE THE FLUSH ROUTINE TO FREE     */
*/* UP THAT FSB.                                                     */
*/*                                                                  */
*/* INPUT : CRRFMCB - PTR TO THE FMCB                                */
*/*         FSBPTR (REG 6) - PTR TO THE FSB                          */
*/*         LCPBPTR (REG 7) - PTR TO THE LCPB                        */
*/* OUTPUT : FSB QUEUED THE FMCB DATA QUEUE                          */
*/*          POSSIBLY AN LCPB (RESET) QUEUED TO THE TOP              */
*/*                                                                  */
*/* SUBROUTINES CALLED : ISTDCC61 (EXTERNEL RTN) , LCPBINIT          */
*/*                                                                  */
*/* MACROS USED : TPQUE(2)                                           */
*/*                                                                  */
*/********************************************************************/
*HOLDDATA: PROC OPTIONS(NOSAVE,NOSAVEAREA);                        0517
HOLDDATA DS    0H                                                  0518
*RFY ( ATCVTPTR,COUNTER ) RSTD;                                    0518
*                                                                  0518
*   CRRRTN1 = RETPTR;             /* SAVE RETURN ADDRESS             */
         ST    RETPTR,CRRRTN1(,CRRPTR)                             0519
*   ATCVTPTR=CRRATCVT;      /* REFERENCE VTAM CVT                    */
         L     ATCVTPTR,CRRATCVT(,CRRPTR)                          0520
*   CRRPTR=ADDR(CRRSAVE1);  /* POINT TO SAVEAREA FOR MOVE            */
*                                                                  0521
         LA    @15,CRRSAVE1(,CRRPTR)                               0521
         LR    CRRPTR,@15                                          0521
*   /*****************************************************************/
*   /*             MOVE FSB TO PAGEABLE STORAGE                      */
*   /*****************************************************************/
*                                                                  0522
*   CALL ATCDCC61;          /* MOVE FSB TO PAGEABLE STORAGE          */
         L     @15,ATCDCC61(,ATCVTPTR)                             0522
         BALR  @14,@15                                             0522
*   CRRPTR=ADDR(CRRSPACE);  /* REESTABLISH BASE FOR CRR              */
         L     CRRPTR,RPHCRR(,RPHPTR)                              0523
         LA    CRRPTR,CRRSPACE+312(,CRRPTR)                        0523
*   FSBPTR=CRRFSB;          /* REFERENCE NEW FSB                     */
         L     FSBPTR,CRRFSB(,CRRPTR)                              0524
*   FSBELI=OFF;             /* CLEAR SWITCH                          */
*   FSBECI=OFF;             /* CLEAR UNUSED SWITCH                   */
*                                                                  0526
         NI    FSBELI(FSBPTR),B'00111111'                          0526
*   /*****************************************************/        0527
*   /*    ACCUMULATE COUNT OF BUFFERS IN FSB AND FDB'S   */        0527
*   /*    AND UPDATE BUFFER COUNT IN FMCB.               */        0527
*   /*****************************************************/        0527
*                                                                  0527
*   COUNTER=0;              /* INIT BUFFER COUNT ACCUMULATOR         */
         SLR   COUNTER,COUNTER                                     0527
*   FDBPTR=ADDR(FSBFDB);    /* REFERENCE FIRST FDB                   */
         LA    FDBPTR,FSBFDB(,FSBPTR)                              0528
*   DO WHILE (FDBPTR^=0);   /* LOOP THROUGH ALL FDB'S                */
         B     @DE00529                                            0529
@DL00529 DS    0H                                                  0530
*     COUNTER=COUNTER+1;    /* TOTAL NUMBER OF BUFFERS               */
         AL    COUNTER,@CF00107                                    0530
*     FDBPTR=FDBFDBA;       /* REFERENCE NEXT FDB                    */
         L     FDBPTR,FDBFDBA(,FDBPTR)                             0531
*   END;                                                           0532
@DE00529 LTR   FDBPTR,FDBPTR                                       0532
         BNZ   @DL00529                                            0532
*    RFY (REG10,REG11) RSTD;                                       0533
*CS2:REG10=FMCCMPS1;               /* GET OLD BUFFER COUNT           */
CS2      L     @15,CRRFMCB(,CRRPTR)                                0534
         L     REG10,FMCCMPS1(,@15)                                0534
*    REG11=REG10+COUNTER;          /* COMPUTE NEW COUNT              */
         LR    REG11,REG10                                         0535
         ALR   REG11,COUNTER                                       0535
*    CS (REG10,REG11,FMCCMPS1);    /* SET NEW COUNT IN FMCB          */
         CS    REG10,@11,FMCCMPS1(@15)                             0536
*    BC (7,CS2);                   /* TRY AGAIN IF FAILURE           */
         BC    7,CS2                                               0537
*    RFY (REG10,REG11) UNRSTD;                                     0538
*                                                                  0538
*/*  TPQUE (FSBPTR,FMCFBA)  */                                     0539
*                                                                  0539
*    DO;                                                           0539
*    RFY (GPR00P,GPR01P,GPR02P,GPR14P,GPR15P) RSTD;                0540
*    GEN REFS(GPR01P) SETS(RPHWORK)                                0541
*        (STM GPR02P,GPR08P,RPHWORK(GPR01P));                      0541
         STM GPR02P,GPR08P,RPHWORK(GPR01P)
*    GPR15P=FSBPTR; /* GET WORK ELEM POINTER */                    0542
         LR    GPR15P,FSBPTR                                       0542
*    GPR14P=ADDR(FMCFBA); /* ADDRESS PAB/QUEUE POINTER */          0543
         L     GPR14P,CRRFMCB(,CRRPTR)                             0543
         LA    GPR14P,FMCFBA(,GPR14P)                              0543
*    GPR15P=GPR15P×PABWEQG1;                                       0544
         O     GPR15P,@CF02699                                     0544
*    GPR02P=GPR14P->PABWEQA;/* INITIALIZE WITH QUEUE HEADER */     0545
         L     GPR02P,PABWEQA(,GPR14P)                             0545
*CMZA0013:; /* CS FAIL TARGET */                                   0546
CMZA0013 DS    0H                                                  0547
*    GPR00P=GPR02P; /* SAVE QUEUE CONTENTS */                      0547
         LR    GPR00P,GPR02P                                       0547
*    GPR15P->PABCHAIN=GPR02P×PABWEQG1; /*FORWARD CHAIN WORK ELEMENT*/
         LR    @08,GPR02P                                          0548
         O     @08,@CF02699                                        0548
         ST    @08,PABCHAIN(,GPR15P)                               0548
*    CS   (GPR02P,GPR15P,GPR14P->PABWEQA); /* CHECK FOR END OF QUEUE */
         CS    GPR02P,@15,PABWEQA(GPR14P)                          0549
*    BC (7,CMZA0013);                                              0550
         BC    7,CMZA0013                                          0550
*    GEN REFS(RPHWORK,GPR01P) NOSETS                               0551
*        (LM    GPR02P,GPR08P,RPHWORK(GPR01P));                    0551
         LM    GPR02P,GPR08P,RPHWORK(GPR01P)
*    RFY (GPR00P,GPR01P,GPR02P,GPR14P,GPR15P) UNRSTD;              0552
*    END; /**** END OF TPQUE GENERATION ****/        /* QUEUE FSB TO FM
*B DATA QUEUE   */                                                 0553
*   IF FMCBUFNB>FMCBUFLM THEN      /* IS BUFFER LIMIT EXCEEDED?      */
         L     @15,CRRFMCB(,CRRPTR)                                0554
         CLC   FMCBUFNB(2,@15),FMCBUFLM(@15)                       0554
         BNH   @RF00554                                            0554
*      IF FMCFLGA5=OFF THEN        /* IS BUFFER THRESHOLD EXCEEDED?  */
         TM    FMCFLGA5(@15),B'00000100'                           0555
         BNZ   @RF00555                                            0555
*        DO;                                                       0556
*                                                                  0556
*          /*****************************************/             0557
*          /*     BUFFER THRESHOLD EXCEEDED         */             0557
*          /*****************************************/             0557
*                                                                  0557
*          RFY (REG10,REG11) RSTD;                                 0557
*     CS3: REG10=FMCCMPS2;         /* GET OLD FLAG VALUE              *
CS3      L     @15,CRRFMCB(,CRRPTR)                                0558
         L     REG10,FMCCMPS2(,@15)                                0558
*          REG11=REG10×FLGA5ON;    /* SET NEW FLAG VALUE              *
         LR    REG11,REG10                                         0559
         O     REG11,@CF01427                                      0559
*          CS (REG10,REG11,FMCCMPS2); /* SET NEW VALUE IN FMCB        *
         CS    REG10,@11,FMCCMPS2(@15)                             0560
*          BC (7,CS3);             /* TRY AGAIN IF FAILURE            *
         BC    7,CS3                                               0561
*          RFY (REG10,REG11) UNRSTD;                               0562
*          CALL LCPBINIT;          /* GET AND INIT LCCW FOR RESET    */
*                                                                  0563
         BAL   @14,LCPBINIT                                        0563
*          /********************************************/          0564
*          /*  RESET ANY OUTSTANDING I/O. BUILD RESET  */          0564
*          /*  ACCORDING TO DEVICE TYPE AND QUEUE TO   */          0564
*          /*  TPIOS OUTBOUND PAB.                     */          0564
*          /********************************************/          0564
*                                                                  0564
*         IF DEVCHAR2>=DEV2980&DEVCHAR2<=DEV3275 THEN /*2980 OR 3270? *
         L     @15,CRRFMCB(,CRRPTR)                                0564
         LA    @08,36                                              0564
         AL    @08,FMCFMCBA(,@15)                                  0564
         CLI   DEVCHAR2(@08),X'18'                                 0564
         BL    @RF00564                                            0564
         CLI   DEVCHAR2(@08),X'1C'                                 0564
         BH    @RF00564                                            0564
*            LCCWOP=LXEC;         /* RESET AT END OF CMD             */
         L     @15,LCPNXLCW(,LCPBPTR)                              0565
         MVI   LCCWOP(@15),X'4B'                                   0565
*         ELSE IF DEVCRVB=ON THEN                                  0566
         B     @RC00564                                            0566
@RF00564 L     @15,CRRFMCB(,CRRPTR)                                0566
         L     @15,FMCFMCBA(,@15)                                  0566
         TM    DEVCRVB+36(@15),B'00100000'                         0566
         BNO   @RF00566                                            0566
*                  LCCWOP=LXI;     /* RESET IMMEDIATE                */
         L     @15,LCPNXLCW(,LCPBPTR)                              0567
         MVI   LCCWOP(@15),X'2B'                                   0567
*          ELSE IF DEVCBSC=ON THEN                                 0568
         B     @RC00566                                            0568
@RF00566 L     @15,CRRFMCB(,CRRPTR)                                0568
         L     @15,FMCFMCBA(,@15)                                  0568
         TM    DEVCBSC+36(@15),B'10000000'                         0568
         BNO   @RF00568                                            0568
*                  LCCWOP=LXO;     /* RESET ORDERLY                  */
         L     @15,LCPNXLCW(,LCPBPTR)                              0569
         MVI   LCCWOP(@15),X'5B'                                   0569
*               ELSE                                               0570
*                  LCCWOP=LXEC;    /* RESET AT END OF COMMAND        */
         B     @RC00568                                            0570
@RF00568 L     @15,LCPNXLCW(,LCPBPTR)                              0570
         MVI   LCCWOP(@15),X'4B'                                   0570
*          DCLSRESW=ON;            /* INDICATE SPECIAL RESET COMMAND */
@RC00568 DS    0H                                                  0571
@RC00566 DS    0H                                                  0571
@RC00564 DS    0H                                                  0572
*          DCLBTEIS=ON;            /* INDICATE BUFFER THRESHOLD    0572
*                                     EXCEEDED, INPUT STOPPED        */
*                                                                  0572
         OI    DCLSRESW(LCPBPTR),B'10010000'                       0572
*/*  TPQUE (LCPBPTR,FMCPAB3) RESET */                              0573
*                                                                  0573
*           DO;                                                    0573
*           RFY (GPR00P,GPR01P,GPR02P,GPR15P) RSTD;                0574
*           GEN REFS(GPR01P) SETS(RPHWORK)                         0575
*        (STM   GPR14P,GPR09P,RPHWORK(GPR01P));                    0575
         STM   GPR14P,GPR09P,RPHWORK(GPR01P)
*           GPR00P=ADDR(FMCPAB3);                                  0576
         L     @08,CRRFMCB(,CRRPTR)                                0576
         LA    GPR00P,FMCPAB3(,@08)                                0576
*           GPR02P=LCPBPTR;                                        0577
         LR    GPR02P,LCPBPTR                                      0577
*           CALL ATCLCPTR->ATCAP34->ISTAPC34;                      0578
         L     @08,ATCLCPTR                                        0578
         L     @15,ATCAP34(,@08)                                   0578
         BALR  @14,@15                                             0578
*           GEN REFS(RPHWORK,GPR01P) NOSETS                        0579
*        (LM    GPR14P,GPR09P,RPHWORK(GPR01P));                    0579
         LM    GPR14P,GPR09P,RPHWORK(GPR01P)
*           RFY (GPR00P,GPR01P,GPR02P,GPR15P) UNRSTD;              0580
*           END; /**** END OF TPQUE GENERATION ****/ /* SHIP RESET TO T
*IOS    */                                                         0581
*        END;                                                      0582
*      ELSE;                       /* BUFFER THRESHOLD PREVIOUSLY  0583
*                                     EXCEEDED                       */
@RF00555 DS    0H                                                  0584
*   ELSE;                          /* BUFFER LIMIT NOT EXCEEDED      */
@RF00554 DS    0H                                                  0585
*   IF CRRSW6=ON THEN              /* IS DATA FROM SOLICIT           */
         TM    CRRSW6(CRRPTR),B'00000010'                          0585
         BNO   @RF00585                                            0585
*      IF FMCFLGA3=ON THEN         /* IS FLUSH INDICATOR NOW SET?    */
*                                                                  0586
         L     @15,CRRFMCB(,CRRPTR)                                0586
         TM    FMCFLGA3(@15),B'00010000'                           0586
         BNO   @RF00586                                            0586
*          /*****************************************/             0587
*          /*   FSB FROM SOLICIT REQUEST WAS        */             0587
*          /*   QUEUED BUT NOW THE FLUSH INDICATOR  */             0587
*          /*   IS SET.  INVOKE FLUSH ROUTINE TO    */             0587
*          /*   DEQUEUE FSB AND THROW IT AWAY.      */             0587
*          /*****************************************/             0587
*                                                                  0587
*          CALL FLUSHRTN;          /* THROW AWAY DATA                */
         BAL   @14,FLUSHRTN                                        0587
*      ELSE;                       /* FLUSH INDICATOR NOT SET        */
@RF00586 DS    0H                                                  0589
*   ELSE;                          /* DATA NOT FROM SOLICIT          */
@RF00585 DS    0H                                                  0590
*   RETPTR = CRRRTN1;             /* GET RETURN ADDRESS              */
         L     RETPTR,CRRRTN1(,CRRPTR)                             0590
*RFY ( ATCVTPTR,COUNTER ) UNRSTD;                                  0591
*END HOLDDATA;                     /* RETURN TO MAIN PROCEDURE        *
@EL00003 DS    0H                                                  0592
@EF00003 DS    0H                                                  0592
@ER00003 BR    @14                                                 0592
*/********************************************************************/
*/*                                                                  */
*/* ROUTINE NAME : LDOPROC - LDO PROCESSING                          */
*/*                                                                  */
*/* FUNCTION : FIND THE LDO ASSOCIATED WITH THE FSB IF THE           */
*/* DATA RECEIVED IS FOR A SOLICIT, FEEDBACK PROCESSING IS           */
*/* COMPLETE.  THE DATA WILL BE SAVED FOR A SUBSEQUENT READ          */
*/* REQUEST.  OTHERWISE, INVOKE MOVEPROC TO MOVE THE DATA FROM       */
*/* THE FSB TO THE USER'S BUFFER.                                    */
*/*                                                                  */
*/* INPUT : LCPBPTR (REG 7) - PTR TO THE LCPB                        */
*/*         LDOPTR (REG 9) - PTR THE LDO                             */
*/*         CRRPTR (REG 13) - PTR TO THE CRR                         */
*/*                                                                  */
*/* OUTPUT : CRR FEEDBACK FIELDS INITIALIZED                         */
*/*          DATA MOVED TO THE USER'S BUFFER                         */
*/*                                                                  */
*/* SUBROUTINES CALLED : MOVEPROC , FINDLDO , FREEFSB                */
*/*                                                                  */
*/* MACROS USED : NONE                                               */
*/*                                                                  */
*/********************************************************************/
*LDOPROC: PROC OPTIONS(NOSAVE,NOSAVEAREA);                         0593
LDOPROC  DS    0H                                                  0594
*  DO; /*MODESET  EXTKEY(ZERO)*/                                   0594
*    SPKA (0); /*SET PSW KEY*/                                     0595
         SLR   @15,@15                                             0595
         SPKA  0(@15)                                              0595
*  END; /*MODESET  EXTKEY(ZERO)*//*GET SUPER KEY TO REFER LDO @Z30APGE*
*                                                                  0596
*   CRRRTN2 = RETPTR;           /* SAVE RETURN ADDRESS             */
         ST    RETPTR,CRRRTN2(,CRRPTR)                             0597
*   DCLRPLA=DCLRPLA&&DCLRPLA;      /* CLEAR RPL PTR IN LCPB         */
         L     @15,DCLRPLA(,LCPBPTR)                               0598
         LR    @08,@15                                             0598
         XR    @08,@15                                             0598
         ST    @08,DCLRPLA(,LCPBPTR)                               0598
*   CALL FINDLDO;                 /* FIND THE LDO AND VALIDATE IT    */
         BAL   @14,FINDLDO                                         0599
*        IF LDOCMD=LDOSLCT THEN                                    0600
         CLI   LDOCMD(LDOPTR),X'F2'                                0600
         BNE   @RF00600                                            0600
*              DO;                /* SOLICIT REQUEST-PROCESSING DONE */
*                                                                  0601
*              /**********************************************/    0602
*              /*  SOLICIT LDO.  PROCESSING FINISHED.  SET   */    0602
*              /*  NORMAL FEEDBACK STATUS IN CRR.            */    0602
*              /**********************************************/    0602
*                                                                  0602
*              CRRRTNCD=CRRNOERR;  /* SET RETURN CODE OF 0           */
         MVI   CRRRTNCD(CRRPTR),X'00'                              0602
*              CRRFDB2=CRRNOERR;   /* SET MINOR RETURN CODE OF 0     */
         MVI   CRRFDB2(CRRPTR),X'00'                               0603
*              CRRFDB3=CRRNOERR;   /* SET DATA FLAGS TO 0            */
         MVI   CRRFDB3(CRRPTR),X'00'                               0604
*              CRRPOST=ON;        /* RPL SHOULD BE POSTED COMPLETE   */
         OI    CRRPOST(CRRPTR),B'00010000'                         0605
*              CRRSW1=ON;         /* SAVE FSB FOR A LATER READ       */
         OI    CRRSW1(CRRPTR),B'01000000'                          0606
*              END;                                                0607
*        ELSE DO;      /* MOVE DATA TO USER'S BUFFER      */       0608
*                                                                  0608
         B     @RC00600                                            0608
@RF00600 DS    0H                                                  0609
*             /*******************************************/        0609
*             /* MOVE FEEDBACK STATUS FROM FSB TO CRR    */        0609
*             /*******************************************/        0609
*                                                                  0609
*             CRRRTNCD=FSBSRCAT;  /* MOVE MAJOR RETURN CODE          */
         MVC   CRRRTNCD(1,CRRPTR),FSBSRCAT(FSBPTR)                 0609
*             CRRFDB2=FSBMNRCD;   /* MOVE MINOR RETURN CODE          */
         MVC   CRRFDB2(1,CRRPTR),FSBMNRCD(FSBPTR)                  0610
*             CRRFDB3=FSBERC;     /* COPY DATA FLAGS                 */
         MVC   CRRFDB3(1,CRRPTR),FSBERC(FSBPTR)                    0611
*             CRRUINPT=OFF;       /* RESET UNSOLICITED INPUT FLAG    */
*             CRRUNUSD=OFF;       /* RESET UNUSED BIT                */
         NI    CRRUINPT(CRRPTR),B'00111111'                        0613
*             CRRESR2=FSBEXTRS;    /* EXTENDED SYSTEM RESPONSE       */
         MVC   CRRESR2(1,CRRPTR),FSBEXTRS(FSBPTR)                  0614
*             CRRESR1=FSBSYSRS;  /* SYSTEM RESPONSE                 */
         MVC   CRRESR1(1,CRRPTR),FSBSYSRS(FSBPTR)                  0615
*             IF CRRRTNCD=CRRPHYSC THEN                            0616
         CLI   CRRRTNCD(CRRPTR),X'0C'                              0616
         BNE   @RF00616                                            0616
*             IF CRRSTSAV=ON THEN                                  0617
         TM    CRRSTSAV(CRRPTR),B'00000001'                        0617
         BNO   @RF00617                                            0617
*                CRRDSB=FSBDMGRS;  /* COPY SENSE BYTE                */
         MVC   CRRDSB(2,CRRPTR),FSBDMGRS(FSBPTR)                   0618
*             ELSE;                /* NO SENSE DATA AVAILABLE        */
@RF00617 DS    0H                                                  0620
*             ELSE;                                                0620
*                                                                  0620
@RF00616 DS    0H                                                  0621
*             /*******************************************/        0621
*             /*    MOVE DATA TO USER'S BUFFERS          */        0621
*             /*******************************************/        0621
*                                                                  0621
*             CALL MOVEPROC;       /* MOVE DATA TO USER'S BUFFER     */
         BAL   @14,MOVEPROC                                        0621
*             END;                                                 0622
*                                                                  0622
*                                                                  0622
*   /*****************************************************************/
*   /* IF CRRSW1 IS OFF, THEN ALL THE DATA ASSOCIATED WITH THE       */
*   /* FSB HAS BEEN MOVED SO THE FSB CAN BE FREED.  OTHERWISE,       */
*   /* HOLDDATA WILL BE INVOKED IN THE MAIN PROCEEDURE TO SAVE       */
*   /* THE FSB.                                                      */
*   /*****************************************************************/
*                                                                  0623
*   IF CRRSW1=OFF THEN                                             0623
@RC00600 TM    CRRSW1(CRRPTR),B'01000000'                          0623
         BNZ   @RF00623                                            0623
*         CALL FREEFSB;           /* FREE STORAGE OCCUPIED BY FSB    */
         BAL   @14,FREEFSB                                         0624
*   ELSE;                         /* FSB WILL BE SAVED               */
@RF00623 DS    0H                                                  0626
*  DO; /*MODESET  EXTKEY(VTAM)*/                                   0626
*    SPKA (96); /*SET PSW KEY*/                                    0627
         LA    @15,96                                              0627
         SPKA  0(@15)                                              0627
*  END; /*MODESET  EXTKEY(VTAM)*//*GET VTAM KEY @Z30APGE*/         0628
*   RETPTR = CRRRTN2;             /* GET RETURN ADDRESS              */
         L     RETPTR,CRRRTN2(,CRRPTR)                             0629
*END LDOPROC;                      /* RETURN TO CALLER                *
@EL00004 DS    0H                                                  0630
@EF00004 DS    0H                                                  0630
@ER00004 BR    @14                                                 0630
*/********************************************************************/
*/*                                                                  */
*/* ROUTINE NAME : MOVEPROC - MOVE PROCEDURE                         */
*/*                                                                  */
*/* FUNCTION : THIS RTN SETS UP THE INPUT PARAMETERS TO THE MOVE     */
*/* ROUTINE.  IF A PREVIOUS MOVE HAD OCCURRED (RESUME DATA ADDRESS   */
*/* IN LCPB NOT ZERO) THEN THE PARAMETERS ARE REINITIALIZED FROM     */
*/* THE LCPB WHERE THEY WERE SAVED.  OTHERWISE, THEY ARE             */
*/* INITIALIZED FROM FIELDS IN THE CRR AND LDO.  THE MOVE            */
*/* ROUTINE IS INVOKED TO MOVE THE DATA, AND UPON RETURN,            */
*/* MOVEFDBK IS CALLED TO ANALYZE THE RESULTS.                       */
*/*                                                                  */
*/* INPUT : LCPBPTR (REG 7) - PTR TO THE LCPB                        */
*/*                                                                  */
*/* OUTPUT : DATA MOVED                                              */
*/*                                                                  */
*/* SUBROUTINES CALLED : MOVEFDBK , ISTDCC60(EXTERNAL)               */
*/*                                                                  */
*/* MACROS USED : VMDESET(2) , VSETLCK(2)                            */
*/*                                                                  */
*/********************************************************************/
*MOVEPROC: PROC OPTIONS(NOSAVE,NOSAVEAREA);                        0631
MOVEPROC DS    0H                                                  0632
*RFY ATCVTPTR RSTD;                                                0632
*                                                                  0632
*   CRRRTN3 = RETPTR;             /* SAVE RETURN ADDRESS             */
         ST    RETPTR,CRRRTN3(,CRRPTR)                             0633
*   IF DCLIOBUF ^= 0 THEN                                          0634
         L     @15,DCLIOBUF(,LCPBPTR)                              0634
         LTR   @15,@15                                             0634
         BZ    @RF00634                                            0634
*         DO;                     /* CONTINUE PREVIOUS MOVE          */
*                                                                  0635
*         /**********************************************/         0636
*         /*   DATA PARTIALLY MOVED.  SET UP PARAMETERS */         0636
*         /*   FOR MOVE FROM LCPB.                      */         0636
*         /**********************************************/         0636
*                                                                  0636
*         CRRBARAD=DCLIOBUF;      /* RESUME BUFFER ADDRESS           */
         ST    @15,CRRBARAD(,CRRPTR)                               0636
*         CRRBLEN=DCLIOLEN;       /* REMAINING LENGTH                */
         LH    @08,DCLIOLEN(,LCPBPTR)                              0637
         STH   @08,CRRBLEN(,CRRPTR)                                0637
*         CRRBLDAD=DCLCLDOA;      /* RESUME LDO ADDRESS              */
         L     @04,DCLCLDOA(,LCPBPTR)                              0638
         ST    @04,CRRBLDAD(,CRRPTR)                               0638
*         CRRBTLEN=DCLBTLEN;      /* TOTAL LENGTH MOVED SO FAR       */
         L     @02,DCLBTLEN(,LCPBPTR)                              0639
         ST    @02,CRRBTLEN(,CRRPTR)                               0639
*         DCLIOBUF=DCLIOBUF&&DCLIOBUF;  /* CLEAR OUT DATA ADDRESS    */
         LR    @00,@15                                             0640
         XR    @00,@15                                             0640
         ST    @00,DCLIOBUF(,LCPBPTR)                              0640
*         DCLIOLEN=DCLIOLEN&&DCLIOLEN;  /* ZERO RESUME LENGTH        */
         LR    @15,@08                                             0641
         XR    @15,@08                                             0641
         STH   @15,DCLIOLEN(,LCPBPTR)                              0641
*         DCLCLDOA=DCLCLDOA&&DCLCLDOA;  /* ZERO RESUME LENGTH        */
         LR    @15,@04                                             0642
         XR    @15,@04                                             0642
         ST    @15,DCLCLDOA(,LCPBPTR)                              0642
*         DCLBTLEN=DCLBTLEN&&DCLBTLEN;  /* CLEAR LENGTH ACCUMULATOR */
         LR    @15,@02                                             0643
         XR    @15,@02                                             0643
         ST    @15,DCLBTLEN(,LCPBPTR)                              0643
*         END;                                                     0644
*   ELSE DO;                      /* SET UP FOR FIRST MOVE           */
*                                                                  0645
         B     @RC00634                                            0645
@RF00634 DS    0H                                                  0646
*        /**********************************************/          0646
*        /*   NO DATA YET MOVED.  SET UP PARAMETERS    */          0646
*        /*   FOR MOVE FROM LDO.                       */          0646
*        /**********************************************/          0646
*                                                                  0646
*        CRRBLDAD=CRRCLDO;        /* LDO ADDRESS FOR MOVE            */
         L     @15,CRRCLDO(,CRRPTR)                                0646
         ST    @15,CRRBLDAD(,CRRPTR)                               0646
*        CRRBLEN=LDOLEN;          /* SET BUFFER LENGTH FROM LDO      */
         LH    @15,LDOLEN(,LDOPTR)                                 0647
         N     @15,@CF04418                                        0647
         STH   @15,CRRBLEN(,CRRPTR)                                0647
*        CRRBARAD=LDOADDR;        /* SET BUFFER ADDR FROM LDO        */
         L     @15,LDOADDR(,LDOPTR)                                0648
         ST    @15,CRRBARAD(,CRRPTR)                               0648
*        CRRBTLEN=CRRBTLEN&&CRRBTLEN;   /* SET TOTAL LEN MOVED TO 0  */
         L     @15,CRRBTLEN(,CRRPTR)                               0649
         LR    @08,@15                                             0649
         XR    @08,@15                                             0649
         ST    @08,CRRBTLEN(,CRRPTR)                               0649
*        END;                                                      0650
*ATCVTPTR=CRRATCVT;/*REFERENCE VTAM CVT*/                          0651
@RC00634 L     ATCVTPTR,CRRATCVT(,CRRPTR)                          0651
*RFY (GPR10F,GPR14F) RSTD;/*@Z30APGE*/                             0652
*STM (GPR10F,GPR14F,CRRSAVE);/*SAVE REGS ACROSS SETLCK @Z30APGE*/  0653
         STM   GPR10F,GPR14F,CRRSAVE(CRRPTR)                       0653
*GPR10F=CRRPTR;/*SAVE REG 13 @Z30APGE*/                            0654
*                                                                  0654
         LR    GPR10F,CRRPTR                                       0654
* /*****************************************************************/
* /*    CHANGE TO KEY ZERO AND OBTAIN LOCAL MEMORY LOCK            */
* /*****************************************************************/
*                                                                  0655
*                                                                  0655
*C40LOCK:  DO; /* SETLOCK (OBTAIN) MODE(UNCOND) TYPE(LOCAL) RELATED(LOC
*L,ISTDCC40(C40UNLCK)) */                                          0655
C40LOCK  DS    0H                                                  0656
*            RFY (GPR11P,GPR12P,GPR13P,GPR14P) RSTD;               0656
*            GPR13P = PSALITA; /* ADDRESS OF LOCK INTERFACE TABLE. */
         L     GPR13P,PSALITA                                      0657
*            GEN(LM GPR11P,GPR13P,576(GPR13P)); /* LOAD LOCKWORD, CLHT
*ND ENTRY ADRS. */                                                 0658
         LM GPR11P,GPR13P,576(GPR13P)
*            GEN(BALR GPR14P,GPR13P); /* BALR TO SETLOCK RTN. */   0659
         BALR GPR14P,GPR13P
*            RFY (GPR11P,GPR12P,GPR13P,GPR14P) UNRSTD;             0660
*          END; /* SETLOCK (OBTAIN) MODE(UNCOND) TYPE(LOCAL) RELATED(LO
*AL,ISTDCC40(C40UNLCK)) *//*@Z30APGE*/                             0661
* CRRPTR=GPR10F; /*RESTORE REG 13 @Z30APGE*/                       0662
         LR    CRRPTR,GPR10F                                       0662
*  LM(GPR10F,GPR14F,CRRSAVE);/*RESTORE REGS @Z30APGE*/             0663
         LM    GPR10F,GPR14F,CRRSAVE(CRRPTR)                       0663
*  RFY (GPR10F,GPR14F) UNRSTD;/*@Z30APGE*/                         0664
*  CRRPTR=ADDR(CRRSAVE1);/*GET SAVEAREA FOR DCC60 @Z30APGE*/       0665
*                                                                  0665
         LA    @15,CRRSAVE1(,CRRPTR)                               0665
         LR    CRRPTR,@15                                          0665
*   /*********************************************/                0666
*   /*   CALL ISTDCC60 TO MOVE DATA TO USER'S    */                0666
*   /*   BUFFER.                                 */                0666
*   /*********************************************/                0666
*                                                                  0666
*   CALL ATCDCC60;                /* MOVE THE DATA                   */
         L     @15,ATCDCC60(,ATCVTPTR)                             0666
         BALR  @14,@15                                             0666
*   CRRPTR=ADDR(CRRSPACE); /* REESTABLISH BASE FOR CRR       */    0667
         L     CRRPTR,RPHCRR(,RPHPTR)                              0667
         LA    CRRPTR,CRRSPACE+312(,CRRPTR)                        0667
*   RPLRLEN=CRRBTLEN;             /* SAVE AMT MOVED IN USER'S RPL    */
*                                                                  0668
         L     @15,CRRARPL(,CRRPTR)                                0668
         L     @08,CRRBTLEN(,CRRPTR)                               0668
         ST    @08,RPLRLEN(,@15)                                   0668
*  /**********************************************************/    0669
*  /*  RELEASE LOCAL MEMORY LOCK */                                0669
*  /**********************************************************/    0669
*                                                                  0669
* RFY (GPR10F,GPR14F) RSTD;/*@Z30APGE*/                            0669
* STM(GPR10F,GPR14F,CRRSAVE);/*SAVE REGS ACROSS SETLCK @Z30APGE*/  0670
         STM   GPR10F,GPR14F,CRRSAVE(CRRPTR)                       0670
* GPR10F=CRRPTR;/*SAVE REG 13 @Z30APGE*/                           0671
*                                                                  0671
         LR    GPR10F,CRRPTR                                       0671
*C40UNLCK:  DO; /* SETLOCK (RELEASE) TYPE(LOCAL) RELATED(LOCAL,ISTDCC40
*C40LOCK)) */ RFY (GPR11P,GPR12P,GPR13P,GPR14P) RSTD;              0672
C40UNLCK DS    0H                                                  0673
*             GPR13P = PSALITA; /* ADDRESS OF LOCK INTERFACE TABLE. */
         L     GPR13P,PSALITA                                      0674
*             GEN(LM GPR11P,GPR13P,588(GPR13P)); /* LOAD LOCKWORD, CLHT
*AND ENTRY ADRS. */                                                0675
         LM GPR11P,GPR13P,588(GPR13P)
*             GEN(BALR GPR14P,GPR13P); /* BALR TO SETLOCK RTN. */  0676
         BALR GPR14P,GPR13P
*             RFY (GPR11P,GPR12P,GPR13P,GPR14P) UNRSTD;            0677
*           END; /* SETLOCK (RELEASE) TYPE(LOCAL) RELATED(LOCAL,ISTDCC4
*(C40LOCK)) *//*@Z30APGE*/                                         0678
* CRRPTR=GPR10F;/*RESTORE REG 13 @Z30APGE*/                        0679
         LR    CRRPTR,GPR10F                                       0679
* LM (GPR10F,GPR14F,CRRSAVE);/*RESTORE REGS @Z30APGE*/             0680
         LM    GPR10F,GPR14F,CRRSAVE(CRRPTR)                       0680
* RFY (GPR10F,GPR14F) UNRSTD;/*@Z30APGE*/                          0681
*                                                                  0681
*   CALL MOVEFDBK;                /* ANALYZE RESULTS OF MOVE         */
*                                                                  0682
         BAL   @14,MOVEFDBK                                        0682
*   RETPTR = CRRRTN3;             /* GET RETURN ADDRESS              */
         L     RETPTR,CRRRTN3(,CRRPTR)                             0683
*RFY ATCVTPTR UNRSTD;                                              0684
*END MOVEPROC;                    /* RETURN TO CALLER                */
@EL00005 DS    0H                                                  0685
@EF00005 DS    0H                                                  0685
@ER00005 BR    @14                                                 0685
*/********************************************************************/
*/*                                                                  */
*/* ROUTINE NAME : MOVEFDBK - MOVE ROUTINE FEEDBACK                  */
*/*                                                                  */
*/* FUNCTION : ANALYZE THE RESULTS OF THE MOVE ROUTINE. THE MOVE     */
*/* ROUTINE WILL RETURN A CODE OF 8, 4, OR 0 TO INDICATE WHAT        */
*/* HAPPENED.  A CODE OF 8 MEANS A LOGIC ERROR SO THAT NO DATA       */
*/* WAS MOVED.  THE ROUTINE SET THE PROPER ERROR CODES IN THE        */
*/* CRR.  LDO PROCESSING IS FINISHED AND THE FSB WILL BE SAVED       */
*/* FOR A LATER READ.  A RETURN CODE OF 4 MEANS THAT PART OF THE     */
*/* DATA WAS MOVED, AND NOW THE USER'S BUFFER IS FULL.  THE          */
*/* TRUNC/KEEP OPTION WILL DETERMINE HOW THE REMAINING DATA WILL     */
*/* BE TREATED.  A CODE OF 0 INDICATES THAT ALL THE DATA             */
*/* CORRESPONDING TO THE CURRENT FSB WAS MOVED.  WHETHER AN EOB,     */
*/* EOM, OR EOT WAS RECEIVED DETERMINES WHETHER THE REQUEST IS       */
*/* COMPLETE.                                                        */
*/*                                                                  */
*/* INPUT : CRRPTR (REG 13) - PTR TO THE CRR                         */
*/*                                                                  */
*/* OUTPUT : CRRPOST AND CRRSW1 POSSIBLY SET                         */
*/*                                                                  */
*/* SUBROUTINES CALLED : TRUNPROC , KEEPPROC                         */
*/*                                                                  */
*/* MACROS USED : NONE                                               */
*/*                                                                  */
*/********************************************************************/
*MOVEFDBK: PROC OPTIONS(NOSAVE,NOSAVEAREA);                        0686
*                                                                  0686
MOVEFDBK DS    0H                                                  0687
*   CRRRTN4 = RETPTR;             /* SAVE RETURN ADDRESS             */
         ST    RETPTR,CRRRTN4(,CRRPTR)                             0687
*   CRRCLDO = CRRBLDAD;           /* SAVE LAST LDO PROCESSED         */
         L     @15,CRRBLDAD(,CRRPTR)                               0688
         ST    @15,CRRCLDO(,CRRPTR)                                0688
*   IF CRRBRTCD = CRRBEROR THEN                                    0689
         CLI   CRRBRTCD(CRRPTR),8                                  0689
         BNE   @RF00689                                            0689
*         DO;                     /* LOG ERROR-NO DATA WAS MOVED     */
*                                                                  0690
*         /********************************************/           0691
*         /*        ERROR-NO DATA MOVED               */           0691
*         /********************************************/           0691
*                                                                  0691
*         CRRPOST=ON;             /* RPL SHOULD BE POSTED COMPLETE   */
         OI    CRRPOST(CRRPTR),B'00010000'                         0691
*         CRRSW1=ON;              /* FSB IS TO BE SAVED              */
         OI    CRRSW1(CRRPTR),B'01000000'                          0692
*         END;                                                     0693
*   ELSE IF PROTRUNC=ON THEN                                       0694
*                                                                  0694
         B     @RC00689                                            0694
@RF00689 L     @15,CRRFMCB(,CRRPTR)                                0694
         TM    PROTRUNC+48(@15),B'01000000'                        0694
         BNO   @RF00694                                            0694
*              /*****************************************/         0695
*              /*  MOVE SUCCESSFUL-CHECK TRUNCATION     */         0695
*              /*****************************************/         0695
*                                                                  0695
*              CALL TRUNPROC;     /* CODE 0 OR 4 - CHECK TRUNC OPTION*/
*                                                                  0695
         BAL   @14,TRUNPROC                                        0695
*             /******************************************/         0696
*             /*      MOVE SUCCESSFUL-CHECK KEEP        */         0696
*             /******************************************/         0696
*                                                                  0696
*        ELSE CALL KEEPPROC;      /* CODE 0 OR 4 - CHECK KEEP OPTION */
         B     @RC00694                                            0696
@RF00694 BAL   @14,KEEPPROC                                        0696
*   RPLAAREA = CRRCLDO;           /* SET LAST LDO IN RPL FOR FEEDBACK*/
*                                                                  0697
@RC00694 DS    0H                                                  0697
@RC00689 L     @15,CRRARPL(,CRRPTR)                                0697
         L     @08,CRRCLDO(,CRRPTR)                                0697
         ST    @08,RPLAAREA(,@15)                                  0697
*   RETPTR = CRRRTN4;             /* GET RETURN ADDRESS              */
         L     RETPTR,CRRRTN4(,CRRPTR)                             0698
*END MOVEFDBK;                    /* RETURN TO CALLER                */
@EL00006 DS    0H                                                  0699
@EF00006 DS    0H                                                  0699
@ER00006 BR    @14                                                 0699
*/********************************************************************/
*/*                                                                  */
*/* ROUTINE NAME : FINDLDO - FIND THE CURRENT LDO                    */
*/*                                                                  */
*/* FUNCTION : LOCATE THE LDO ADDRESS SAVED WHEN THE LCCW WAS BUILT. */
*/* IF A SINGLE READ LDO WAS ISSUED, THE LDO ADDRESS WAS SAVED       */
*/* IN THE LCPB.  OTHERWISE, IT IS IN THE LCCW EXTENSION AREA.       */
*/* AFTER THE LDO ADDRESS IS FOUND.                                  */
*/*                                                                  */
*/* INPUT : LCPBPTR (REG 7) - PTR TO THE LCPB                        */
*/*                                                                  */
*/* OUTPUT : CRRCLDO - PT RTO THE NEXT LDO                           */
*/*          RPLAAREA - PTR TO THE LAST LDO EXECUTED                 */
*/*                                                                  */
*/* SUBROUTINES CALLED : NONE                                        */
*/*                                                                  */
*/* MACROS USED : NONE                                               */
*/*                                                                  */
*/********************************************************************/
*FINDLDO: PROC OPTIONS(NOSAVE,NOSAVEAREA);                         0700
*                                                                  0700
FINDLDO  DS    0H                                                  0701
*   CRRRTN3 = RETPTR;             /* SAVE RETURN ADDRESS             */
         ST    RETPTR,CRRRTN3(,CRRPTR)                             0701
* /* DCR 1991 IMPLEMENTED ( 10/2/73 ) */                           0702
*                                                                  0702
*   IF EVAL(ADDR(LCPLCCW))->LCCWOP=LTIC THEN                       0702
*                                                                  0702
         LA    @15,LCPLCCW(,LCPBPTR)                               0702
         CLI   LCCWOP(@15),X'08'                                   0702
         BNE   @RF00702                                            0702
*         /************************************/                   0703
*         /*  LDO ADDRESS IS LOCATED IN THE   */                   0703
*         /*  EXTENSION AREA BECAUSE MORE     */                   0703
*         /*  THAN ONE REQUEST WAS ISSUED.    */                   0703
*         /*  FSBLCCWA POINTS TO THE LAST     */                   0703
*         /*  PROCESSED LCCW WHICH IS FOR     */                   0703
*         /*  THE READ.  PICK UP LDO ADDRESS  */                   0703
*         /*  AT SPECIFIC OFFSET FROM LCCW.   */                   0703
*         /************************************/                   0703
*                                                                  0703
*         LDOPTR=(FSBLCCWA+LDOFFSET)->ADDRLDO; /* GET LDO ADDR       */
*                                                                  0703
         L     @15,FSBLCCWA(,FSBPTR)                               0703
         L     LDOPTR,ADDRLDO+32(,@15)                             0703
*        /**********************************/                      0704
*        /*  ONLY ONE REQUEST.  LDO ADDRESS*/                      0704
*        /*  WAS SAVED IN THE DCLCPB.      */                      0704
*        /**********************************/                      0704
*                                                                  0704
*   ELSE LDOPTR=DCLLDO1;          /* GET LDO FROM LCPB               */
         B     @RC00702                                            0704
@RF00702 L     LDOPTR,DCLLDO1(,LCPBPTR)                            0704
*   CRRCLDO=LDOPTR;               /* SAVE LDO IN CRR                 */
@RC00702 ST    LDOPTR,CRRCLDO(,CRRPTR)                             0705
*   RPLAAREA=LDOPTR;              /* SAVE IN RPL AS FEEDBACK         */
         L     @15,CRRARPL(,CRRPTR)                                0706
         ST    LDOPTR,RPLAAREA(,@15)                               0706
* /* LINE 150270 CHANGED FOR ZTR 5332 ( 10/17/73 ) */              0707
*                                                                  0707
*   RETPTR = CRRRTN3;             /* GET RETURN ADDRESS              */
         L     RETPTR,CRRRTN3(,CRRPTR)                             0707
*END FINDLDO;                     /* RETURN TO LDOPROC               */
@EL00007 DS    0H                                                  0708
@EF00007 DS    0H                                                  0708
@ER00007 BR    @14                                                 0708
*/********************************************************************/
*/*                                                                  */
*/* ROUTINE NAME : FREEFSB - FREE UP THE FSB                         */
*/*                                                                  */
*/* FUNCTION : RELEASE THE FSB WHICH HAS BEEN PROCESSED              */
*/* BY THE MOVE ROUTINE.  THE FSB IS FOLLOWED BY A FDB WHICH         */
*/* CONTAINS A POINTER TO THE DATA AND POSSIBLY A POINTER TO         */
*/* ANOTHER FDB.  ALL THE FDB'S AND THE FSB MUST BE RELEASED.        */
*/* A NEGATIVE OFFSET MUST BE SUBTRACTED FROM THE FDB TO GET THE     */
*/* AMOUNT TO RELEASE.  FOR THE FIRST FDB, THE NEGATIVE OFFSET       */
*/* WILL INCLUDE THE FSB.                                            */
*/*                                                                  */
*/* INPUT : FSBPTR (REG 6) - PTR TO THE FSB                          */
*/*         FDBPTR (REG 5) - PTR TO THE FDB                          */
*/*                                                                  */
*/* OUTPUT : FSB FREED                                               */
*/*                                                                  */
*/* SUBROUTINES CALLED : NONE                                        */
*/*                                                                  */
*/* MACROS USED : RELSTORE(2)                                        */
*/*                                                                  */
*/********************************************************************/
*FREEFSB: PROC OPTIONS(NOSAVE,NOSAVEAREA);                         0709
FREEFSB  DS    0H                                                  0710
*RFY WORKA RSTD;                                                   0710
*                                                                  0710
*   CRRRTN3 = RETPTR;             /* SAVE RETURN ADDRESS             */
         ST    RETPTR,CRRRTN3(,CRRPTR)                             0711
*   FSBPTR = CRRFSB;              /* GET FSB TO BE RELEASED          */
*                                                                  0712
         L     FSBPTR,CRRFSB(,CRRPTR)                              0712
*   /*****************************************************************/
*   /* THIS ROUTINE WILL RELEASE THE FSB WHICH HAS BEEN PROCESSED    */
*   /*****************************************************************/
*                                                                  0713
*   FDBPTR=ADDR(FSBFDB);         /* REFERENCE FIRST FDB             */
         LA    FDBPTR,FSBFDB(,FSBPTR)                              0713
*   WORKA=FDBPTR-FDBOFSET;      /* GET START OF AREA TO RELEASE    */
         LR    WORKA,FDBPTR                                        0714
         SH    WORKA,FDBOFSET(,FDBPTR)                             0714
*   WORKA->FDBFDBA=FDBFDBA;        /* SET UP CHAIN FIELD             */
         L     @15,FDBFDBA(,FDBPTR)                                0715
         ST    @15,FDBFDBA(,WORKA)                                 0715
*   IF PROCFTX=ON THEN   /*RELSTORE BUFADDR(WORKA)CLEAR(YES)CHAIN(YES)*
*                                                                  0716
         L     @15,CRRFMCB(,CRRPTR)                                0716
         TM    PROCFTX+48(@15),B'00000001'                         0716
         BNO   @RF00716                                            0716
*                         DO;                                      0717
*/* ATBUILD (ISTATCVT,ISTRPH)  */                                  0718
*/* END OF ATBUILD */    RFY (GPR02P,GPR03P,GPR04P,GPR15P) RSTD;   0718
*         RFY (GPR01P,GPR13P) RSTD;                                0719
*         GENERATE REFS(GPR01P,GPR02P,GPR13P) SETS(RPHWORK);       0720
*                                                                  0720
          STM GPR02P,GPR13P,RPHWORK(GPR01P)
*         RFY GPR13P UNRSTD;                                       0721
*         GPR04P=WORKA × '80000000'X;                              0722
         LR    @08,WORKA                                           0722
         O     @08,@CF02699                                        0722
         LR    GPR04P,@08                                          0722
*         DCL 1 FMZA0033 FIXED(31) STATIC LOCAL,                   0723
*             2 * FIXED(8) INIT(80),                               0723
*             2 * PTR(24) INIT(0);                                 0723
*         GPR03F = FMZA0033;                                       0724
*                                                                  0724
         L     GPR03F,FMZA0033                                     0724
*                        DO; /* LISATCVT CVTBASE(PAGEABLE,(GPR15P)) */
*                          GPR15P = ATCLCPTR; /*SET UP ATCVT POINTER*/
         L     GPR15P,ATCLCPTR                                     0726
*                        END; /*LISATCVT*/                         0727
*         GPR02P = GPR15P->ATCBPDA;                                0728
         L     GPR02P,ATCBPDA(,GPR15P)                             0728
*         GPR15P=GPR15P->ATCSMRS;                                  0729
         L     GPR15P,ATCSMRS(,GPR15P)                             0729
*         DCL FMZL0033 BASED(GPR15P) LABEL VALUERANGE(*);          0730
*         CALL FMZL0033;                                           0731
         BALR  @14,@15                                             0731
*         RFY GPR13P RSTD;                                         0732
*         GENERATE REFS(GPR01P,RPHWORK) SETS(GPR02P,GPR13P);       0733
*                                                                  0733
          LM GPR02P,GPR13P,RPHWORK(GPR01P)
*         RFY (GPR01P,GPR13P) UNRSTD;                              0734
*                        RFY (GPR02P,GPR03P,GPR04P,GPR15P) UNRSTD; 0735
*                        END ;/**********END OF RELSTORE EXPANSION*****
*****/                                                             0736
*   ELSE   /*RELSTORE BUFADDR(WORKA)CHAIN(YES)*/                   0737
*           DO;                                                    0737
         B     @RC00716                                            0737
@RF00716 DS    0H                                                  0738
*/* ATBUILD (ISTATCVT,ISTRPH)  */                                  0738
*/* END OF ATBUILD */                                              0738
*          RFY (GPR02P,GPR03P,GPR04P,GPR15P) RSTD;                 0738
*         RFY (GPR01P,GPR13P) RSTD;                                0739
*         GENERATE REFS(GPR01P,GPR02P,GPR13P) SETS(RPHWORK);       0740
*                                                                  0740
          STM GPR02P,GPR13P,RPHWORK(GPR01P)
*         RFY GPR13P UNRSTD;                                       0741
*         GPR04P=WORKA;                                            0742
*         DCL 1 FMZA0049 FIXED(31) STATIC LOCAL,                   0743
*             2 * FIXED(8) INIT(80),                               0743
*             2 * PTR(24) INIT(0);                                 0743
*         GPR03F = FMZA0049;                                       0744
*                                                                  0744
         L     GPR03F,FMZA0049                                     0744
*          DO; /* LISATCVT CVTBASE(PAGEABLE,(GPR15P)) */           0745
*            GPR15P = ATCLCPTR; /*SET UP ATCVT POINTER*/           0746
         L     GPR15P,ATCLCPTR                                     0746
*          END; /*LISATCVT*/                                       0747
*         GPR02P = GPR15P->ATCBPDA;                                0748
         L     GPR02P,ATCBPDA(,GPR15P)                             0748
*         GPR15P=GPR15P->ATCSMRS;                                  0749
         L     GPR15P,ATCSMRS(,GPR15P)                             0749
*         DCL FMZL0049 BASED(GPR15P) LABEL VALUERANGE(*);          0750
*         CALL FMZL0049;                                           0751
         BALR  @14,@15                                             0751
*         RFY GPR13P RSTD;                                         0752
*         GENERATE REFS(GPR01P,RPHWORK) SETS(GPR02P,GPR13P);       0753
*                                                                  0753
          LM GPR02P,GPR13P,RPHWORK(GPR01P)
*         RFY (GPR01P,GPR13P) UNRSTD;                              0754
*          RFY (GPR02P,GPR03P,GPR04P,GPR15P) UNRSTD;               0755
*          END ;/**********END OF RELSTORE EXPANSION**********/ /* NOT
*ONFIDENTIAL */                                                    0756
*                                                                  0756
*   RETPTR = CRRRTN3;             /* GET RETURN ADDRESS              */
@RC00716 L     RETPTR,CRRRTN3(,CRRPTR)                             0757
*RFY WORKA UNRSTD;                                                 0758
*END FREEFSB;                     /* RETURN TO CALLER                */
@EL00008 DS    0H                                                  0759
@EF00008 DS    0H                                                  0759
@ER00008 BR    @14                                                 0759
*/********************************************************************/
*/*                                                                  */
*/* ROUTINE NAME : TRUNPROC - TRUNCATE PROCESSING                    */
*/*                                                                  */
*/* FUNCTION : ENTERED WITH A RETURN CODE OF 0 OR 4 FROM THE         */
*/* MOVE ROUTINE AND WITH THE TRUNC OPTION SPECIFIED IN THE FMCB.    */
*/* IF THE FSB JUST PROCESSED CONTAINED AN ENDING CONDITION (EOB)    */
*/* THEN MOVE PROCESSING IS COMPLETED. IF THE FSB IS A SUBBLOCK,     */
*/* MORE DATA IS EXPECTED.IF THE BUFFER IS FILLED AND THERE ARE NO   */
*/* MORE LDO'S CHAINED TO THE CURRENT ONE,THE FLUSH BIT IS SET TO    */
*/* THROW AWAY INCOMING FSB'S.IF THERE IS MORE ROOM, THE INFO        */
*/* NEEDED BY THE MOVE ROUTINE IS SAVED IN THE LCPB SO PROCESSING    */
*/* CAN RESUME WHEN THE NEXT FSB ARRIVES.  IN ALL CASES, THE         */
*/* FSB IS RELEASED.                                                 */
*/*                                                                  */
*/* INPUT : FSBPTR (REG 6) - PTR TO THE FSB                          */
*/*         LDOPTR (REG 9) - PTR TO THE LDO                          */
*/*                                                                  */
*/* OUTPUT : CRRPOST POSSIBLY SET                                    */
*/*                                                                  */
*/* SUBROUTINES CALLED : NEXTLDO                                     */
*/*                                                                  */
*/* MACROS USED : NONE                                               */
*/*                                                                  */
*/********************************************************************/
*TRUNPROC: PROC OPTIONS(NOSAVE,NOSAVEAREA);                        0760
*                                                                  0760
TRUNPROC DS    0H                                                  0761
*   CRRRTN5 = RETPTR;             /* SAVE RETURN ADDRESS             */
         ST    RETPTR,CRRRTN5(,CRRPTR)                             0761
*   IF FSBEBI=OFF & FSBLGI=OFF & FSBSOHI=OFF & FSBMJRCD=FRNORM THEN
         TM    FSBEBI(FSBPTR),B'00100011'                          0762
         BNZ   @RF00762                                            0762
         CLI   FSBMJRCD(FSBPTR),X'00'                              0762
         BNE   @RF00762                                            0762
*           DO;                   /* CURRENT FSB IS A SUBBLOCK       */
*                                                                  0763
*           /********************************************/         0764
*           /*  FSB JUST PROCESSES IS A SUBBLOCK        */         0764
*           /********************************************/         0764
*                                                                  0764
*           DCLBTLEN=CRRBTLEN;    /* SAVE TOTAL LENGTH MOVED         */
         L     @15,CRRBTLEN(,CRRPTR)                               0764
         ST    @15,DCLBTLEN(,LCPBPTR)                              0764
*           IF CRRBLEN=ZERO THEN   /* NO MORE ROOM IN BUFFER */    0765
         LH    @15,CRRBLEN(,CRRPTR)                                0765
         LTR   @15,@15                                             0765
         BNZ   @RF00765                                            0765
*              IF LDOACHN=ON THEN  /* LDO IS DATA CHAINED */       0766
         TM    LDOACHN(LDOPTR),B'10000000'                         0766
         BNO   @RF00766                                            0766
*                  IF CRRERLK=ON THEN        /* ERROR LOCK IS SET    */
*                                                                  0767
         TM    CRRERLK(CRRPTR),B'10000000'                         0767
         BNO   @RF00767                                            0767
*                      /**************************************/    0768
*                      /*   PROCESSING COMPLETED.  ERROR     */    0768
*                      /*   LOCK SET IN FSB JUST PROCESSED   */    0768
*                      /**************************************/    0768
*                                                                  0768
*                      CRRPOST=ON;           /* FINISH PROCESSING    */
         OI    CRRPOST(CRRPTR),B'00010000'                         0768
*                  ELSE                                            0769
*                                                                  0769
*                   /*****************************************/    0769
*                   /*   FSB JUST PROCESSED IS A SUBBLOCK,   */    0769
*                   /*   THERE IS NO MORE ROOM IN THE CURRENT*/    0769
*                   /*   BUFFER BUT THE LDO IS DATA CHAINED  */    0769
*                   /*****************************************/    0769
*                                                                  0769
*                   CALL NEXTLDO; /* MORE ROOM-GET NEXT DATA CHND LDO*/
         B     @RC00767                                            0769
@RF00767 BAL   @14,NEXTLDO                                         0769
*              ELSE DO;           /* NO MORE ROOM IN BUFFER          */
*                                                                  0770
         B     @RC00766                                            0770
@RF00766 DS    0H                                                  0771
*                   /******************************************/   0771
*                   /*  FSB JUST PROCESSED IS A SUBBLOCK,     */   0771
*                   /*  BUT THERE IS NO ROOM IN THE USER'S    */   0771
*                   /*  BUFFER.  INFORM USER HIS DATA HAS     */   0771
*                   /*  BEEN TRUNCATED AND SET SWITCH ON TO   */   0771
*                   /*  INDICATE THE REST OF THE SUBBLOCKS    */   0771
*                   /*  SHOULD BE THROWN AWAY.                */   0771
*                   /******************************************/   0771
*                                                                  0771
*                   CRRPOST=ON;   /* INDICATE RPL SHOULD BE POSTED   */
         OI    CRRPOST(CRRPTR),B'00010000'                         0771
*                   RFY (REG10,REG11) RSTD;                        0772
*             CS4:  REG10=FMCCMPS2;          /* GET OLD FLAG VALUE   */
CS4      L     @15,CRRFMCB(,CRRPTR)                                0773
         L     REG10,FMCCMPS2(,@15)                                0773
*                   REG11=REG10×FLGA3ON;     /* SET NEW FLAG VALUE   */
         LR    REG11,REG10                                         0774
         O     REG11,@CF02705                                      0774
*                   CS (REG10,REG11,FMCCMPS2); /* SET IN FMCB        */
         CS    REG10,@11,FMCCMPS2(@15)                             0775
*                   BC (7,CS4);              /* TRY AGAIN IF FAILURE */
         BC    7,CS4                                               0776
*                   RFY (REG10,REG11) UNRSTD;                      0777
*                   END;                                           0778
*           ELSE                  /* MORE ROOM IN CURRENT BUFFER     */
*              IF CRRERLK=ON THEN            /* SEE IF ERROR LOCK SET*/
*                                                                  0779
         B     @RC00765                                            0779
@RF00765 TM    CRRERLK(CRRPTR),B'10000000'                         0779
         BNO   @RF00779                                            0779
*                     /*****************************************/  0780
*                     /*  FSB JUST PROCESSED IS A SUBBLOCK AND */  0780
*                     /*  THERE IS ROOM IN THE BUFFER BUT THE  */  0780
*                     /*  ERROR LOCK IS SET IN THE FSB JUST    */  0780
*                     /*  PROCESSED.  POST USER COMPLETE SO    */  0780
*                     /*  HE KNOWS THAT AN ERROR OCCURRED.     */  0780
*                     /*****************************************/  0780
*                                                                  0780
*                     CRRPOST=ON;            /* FINISHED PROCESSING  */
         OI    CRRPOST(CRRPTR),B'00010000'                         0780
*              ELSE DO;                      /* ERROR LOCK NOT SET   */
*                                                                  0781
         B     @RC00779                                            0781
@RF00779 DS    0H                                                  0782
*                /********************************************/    0782
*                /*  FSB JUST PROCESSED IS A SUBBLOCK AND    */    0782
*                /*  AND THERE IS ROOM IN THE BUFFER FOR     */    0782
*                /*  MORE DATA.  SAVE STATUS IN DCLCP SO     */    0782
*                /*  THAT THE MOVE CAN BE RESUMED WHEN THE   */    0782
*                /*  NEXT SUBBLOCK ARRIVES.                  */    0782
*                /********************************************/    0782
*                                                                  0782
*                DCLCLDOA=CRRBLDAD; /* SAVE ADDR OF LAST LDO PROCESS */
         L     @15,CRRBLDAD(,CRRPTR)                               0782
         ST    @15,DCLCLDOA(,LCPBPTR)                              0782
*                DCLIOLEN=CRRBLEN;  /* SAVE RESIDUAL COUNT IN LCPB   */
         LH    @08,CRRBLEN(,CRRPTR)                                0783
         STH   @08,DCLIOLEN(,LCPBPTR)                              0783
*                DCLIOBUF=CRRBARAD; /* SAVE RESUME BUFFER ADDRESS    */
         L     @08,CRRBARAD(,CRRPTR)                               0784
         ST    @08,DCLIOBUF(,LCPBPTR)                              0784
*                DCLRPLA=CRRARPL;   /* SAVE POINTER TO THE RPL       */
         L     @08,CRRARPL(,CRRPTR)                                0785
         ST    @08,DCLRPLA(,LCPBPTR)                               0785
*                IF ADDR(LCPLCCW)->LCCWOP^=LTIC THEN               0786
         CLI   LCCWOP+48(LCPBPTR),X'08'                            0786
         BE    @RF00786                                            0786
*                   DCLLDO1=CRRBLDAD;        /* SAVE LDO IN LCPB     */
         ST    @15,DCLLDO1(,LCPBPTR)                               0787
*                ELSE                        /* SAVE LDO IN EXTENSION*/
*                   EVAL(CRRCLCCW+LDOFFSET)->ADDRLDO=CRRBLDAD;     0788
         B     @RC00786                                            0788
@RF00786 LA    @15,32                                              0788
         AL    @15,CRRCLCCW(,CRRPTR)                               0788
         L     @08,CRRBLDAD(,CRRPTR)                               0788
         ST    @08,ADDRLDO(,@15)                                   0788
*                END;                                              0789
*           END;                                                   0790
*   ELSE DO;                      /* ENDING CONDITION MET            */
*                                                                  0791
         B     @RC00762                                            0791
@RF00762 DS    0H                                                  0792
*        /*********************************************/           0792
*        /*     CURRENT FSB IS NOT A SUBBLOCK         */           0792
*        /*********************************************/           0792
*                                                                  0792
*        DCLBTLEN=DCLBTLEN&&DCLBTLEN;     /* CLEAR LENGTH ACCUMULATOR *
         L     @15,DCLBTLEN(,LCPBPTR)                              0792
         LR    @08,@15                                             0792
         XR    @08,@15                                             0792
         ST    @08,DCLBTLEN(,LCPBPTR)                              0792
*        CRRPOST=ON;              /* RPL SHOULD BE POSTED COMPLETE   */
         OI    CRRPOST(CRRPTR),B'00010000'                         0793
*        END;                                                      0794
*                                                                  0794
*   RETPTR = CRRRTN5;             /* GET RETURN ADDRESS              */
@RC00762 L     RETPTR,CRRRTN5(,CRRPTR)                             0795
*END TRUNPROC;                     /* RETURN TO CALLER                *
@EL00009 DS    0H                                                  0796
@EF00009 DS    0H                                                  0796
@ER00009 BR    @14                                                 0796
*/********************************************************************/
*/*                                                                  */
*/* ROUTINE NAME : KEEPPROC - KEEP DATA PROCESSING                   */
*/*                                                                  */
*/* FUNCTION : ENTERED WITH A RETURN CODE OF 0 OR 4 FROM THE         */
*/* MOVE ROUTINE AND WITH THE KEEP OPTION SPECIFIED IN THE FMCB      */
*/* IF THE RETURN CODE IS 0 INDICATING THAT ALL THE DATA FOR THE     */
*/* CURRENT FSB WAS MOVED AND THE CURRENT FSB IS NOT A SUBBLOCK,     */
*/* THEN FEEDBACK PROCESSING IS FINISHED.  IF IT IS A SUBBLOCK AND   */
*/* THERE IS MORE ROOM, THE THE INFORMATION NEEDED TO RESUME THE     */
*/* MOVE IS SAVED IN THE LCPB UNTIL THE NEXT FSB ARRIVES. IF THERE   */
*/* IS NO MORE ROOM OR THE RETURN CODE WAS 4 INDICATING THE BUFFER   */
*/* WAS FILLED BEFORE ALL THE DATA WAS MOVED, THE PARTIAL DATA       */
*/* INDICATOR IS SET FOR FEEDBACK STATUS AND PROCESSING IS           */
*/* FINISHED.  WHTH A RETURN CODE OF 0, THE CURRENT FSB IS TO BE     */
*/* RELEASED WHILE WITH A CODE OF 4, IT MUST BE SAVED BECAUSE IT     */
*/* CONTAINS GOOD DATA.                                              */
*/*                                                                  */
*/* INPUT : FSBPTR (REG6) - PTR TO THE FSB                           */
*/*         LCPBPTR (REG 7) - PTR TO THE LCPB                        */
*/*         LDOPTR (REG 9) - PTR TO THE LDO                          */
*/*                                                                  */
*/* OUTPUT : CRRPOST POSSIBLY SET                                    */
*/*                                                                  */
*/* SUBROUTINES CALLED : NEXTLDO                                     */
*/*                                                                  */
*/* MACROS USED : NONE                                               */
*/*                                                                  */
*/********************************************************************/
*KEEPPROC: PROC OPTIONS(NOSAVE,NOSAVEAREA);                        0797
KEEPPROC DS    0H                                                  0798
*   CRRRTN5 = RETPTR;             /* SAVE RETURN ADDRESS             */
         ST    RETPTR,CRRRTN5(,CRRPTR)                             0798
*   IF CRRBRTCD=CRRBRSUC THEN   /* RETURN CODE 0 FORM MOVE ROUTINE  */
         CLI   CRRBRTCD(CRRPTR),0                                  0799
         BNE   @RF00799                                            0799
*       IF FSBEBI=OFF & FSBLGI=OFF & FSBSOHI=OFF & FSBMJRCD=FRNORM THEN
         TM    FSBEBI(FSBPTR),B'00100011'                          0800
         BNZ   @RF00800                                            0800
         CLI   FSBMJRCD(FSBPTR),X'00'                              0800
         BNE   @RF00800                                            0800
*             IF CRRBLEN=ZERO THEN  /* NO ROOM IN BUFFER */        0801
         LH    @15,CRRBLEN(,CRRPTR)                                0801
         LTR   @15,@15                                             0801
         BNZ   @RF00801                                            0801
*                IF LDOACHN=ON THEN /* LDO IS DATA CHAINED       */
         TM    LDOACHN(LDOPTR),B'10000000'                         0802
         BNO   @RF00802                                            0802
*                      IF CRRERLK=ON THEN    /* SEE IF ERROR LOCK SET*/
*                                                                  0803
         TM    CRRERLK(CRRPTR),B'10000000'                         0803
         BNO   @RF00803                                            0803
*                           /************************************/ 0804
*                           /*  CURRENT FSB IS A SUBBLOCK, ALL  */ 0804
*                           /*  THE DATA WAS MOVED, AND THERE IS*/ 0804
*                           /*  ANOTHER LDO, BUT THE ERROR LOCK */ 0804
*                           /*  IS SET.  LET USER KNOW THIS. DO */ 0804
*                           /*  NOT WAIT FOR ANOTHER SUBBLOCK.  */ 0804
*                           /************************************/ 0804
*                                                                  0804
*                           CRRPOST=ON;      /* FINISHED PROCESSING  */
         OI    CRRPOST(CRRPTR),B'00010000'                         0804
*                      ELSE DO;              /* ERROR LOCK NOT SET   */
*                                                                  0805
         B     @RC00803                                            0805
@RF00803 DS    0H                                                  0806
*                           /*************************************/
*                           /*  CURRENT FSB IS A SUBBLOCK, ALL   */
*                           /*  THE DATA WAS MOVED FROM CURRENT  */
*                           /*  FSB.  GET DATA CHAINED LDO AND   */
*                           /*  SAVE STATUS FOR NEXT SUBBLOCK    */
*                           /*************************************/
*                                                                  0806
*                           CALL NEXTLDO; /* GET NEXT LDO FOR MOVE  */
         BAL   @14,NEXTLDO                                         0806
*                           DCLBTLEN=CRRBTLEN; /* SAVE TOTAL LENGTH */
         L     @15,CRRBTLEN(,CRRPTR)                               0807
         ST    @15,DCLBTLEN(,LCPBPTR)                              0807
*                           END;                                   0808
*                ELSE         /* NO MORE ROOM BUT MORE DATA      */
*                                                                  0809
*                     /**************************************/     0809
*                     /*  CURRENT FSB IS A SUBBLOCK.  ALL   */     0809
*                     /*  DATA MOVED.  THERE IS NO ROOM IN  */     0809
*                     /*  BUFFER FOR NEXT SUBBLOCK.  MOVE   */     0809
*                     /*  PROCESSING COMPLETE.              */     0809
*                     /**************************************/     0809
*                                                                  0809
*                     CRRPOST=ON;  /* RPL SHOULD BE POSTED COMPLETE  */
         B     @RC00802                                            0809
@RF00802 OI    CRRPOST(CRRPTR),B'00010000'                         0809
*             ELSE                /* ROOM IN BUF-SAVE INFO FOR MOVE  */
*              IF CRRERLK=ON THEN            /* SEE IF ERROR LOCK SET*/
*                                                                  0810
         B     @RC00801                                            0810
@RF00801 TM    CRRERLK(CRRPTR),B'10000000'                         0810
         BNO   @RF00810                                            0810
*                   /**********************************/           0811
*                   /*  CURRENT FSB IS A SUBBLOCK. ALL*/           0811
*                   /*  THE DATA WAS MOVED.  THERE IS */           0811
*                   /*  MORE ROOM IN THE BUFFER BUT   */           0811
*                   /*  THE ERROR LOCK IS SET.  DO    */           0811
*                   /*  NOT WAIT FOR NEXT SUBBLOCK.   */           0811
*                   /*  INFORM USER WHAT HAS OCCURRED */           0811
*                   /**********************************/           0811
*                                                                  0811
*                   CRRPOST=ON;              /* FINISHED PROCESSING  */
         OI    CRRPOST(CRRPTR),B'00010000'                         0811
*              ELSE DO;                      /* ERROR LOCK NOT SET   */
*                                                                  0812
         B     @RC00810                                            0812
@RF00810 DS    0H                                                  0813
*                  /**********************************/            0813
*                  /*  FSB JUST PROCESSED WAS A      */            0813
*                  /*  SUBBLOCK.  THERE IS MORE ROOM */            0813
*                  /*  IN THE BUFFER.  SAVE STATUS   */            0813
*                  /*  IN DCLCPB SO MOVE CAN BE      */            0813
*                  /*  CONTINUED WHEN NEXT SUBBLOCK  */            0813
*                  /*  ARRIVES.                      */            0813
*                  /**********************************/            0813
*                                                                  0813
*                  DCLCLDOA=CRRBLDAD; /* SAVE ADDR OF LAST LDO  */ 0813
         L     @15,CRRBLDAD(,CRRPTR)                               0813
         ST    @15,DCLCLDOA(,LCPBPTR)                              0813
*                  DCLIOLEN=CRRBLEN;  /* SAVE REMAINING LENGTH       */
         LH    @08,CRRBLEN(,CRRPTR)                                0814
         STH   @08,DCLIOLEN(,LCPBPTR)                              0814
*                  DCLIOBUF=CRRBARAD; /* RESUME BUFFER ADDRESS       */
         L     @08,CRRBARAD(,CRRPTR)                               0815
         ST    @08,DCLIOBUF(,LCPBPTR)                              0815
*                  DCLBTLEN=CRRBTLEN; /* SAVE TOTAL LENGTH ACCUMULATOR*
         L     @08,CRRBTLEN(,CRRPTR)                               0816
         ST    @08,DCLBTLEN(,LCPBPTR)                              0816
*                  DCLRPLA=CRRARPL;   /* SAVE POINTER TO THE RPL     */
         L     @08,CRRARPL(,CRRPTR)                                0817
         ST    @08,DCLRPLA(,LCPBPTR)                               0817
*                  IF ADDR(LCPLCCW)->LCCWOP^=LTIC THEN             0818
         CLI   LCCWOP+48(LCPBPTR),X'08'                            0818
         BE    @RF00818                                            0818
*                   DCLLDO1=CRRBLDAD;        /* SAVE LDO IN LCPB     */
         ST    @15,DCLLDO1(,LCPBPTR)                               0819
*                  ELSE                     /* SAVE LDO IN EXTENSION */
*                   EVAL(CRRCLCCW+LDOFFSET)->ADDRLDO=CRRBLDAD;     0820
         B     @RC00818                                            0820
@RF00818 LA    @15,32                                              0820
         AL    @15,CRRCLCCW(,CRRPTR)                               0820
         L     @08,CRRBLDAD(,CRRPTR)                               0820
         ST    @08,ADDRLDO(,@15)                                   0820
*                  END;                                            0821
*        ELSE                     /* FSB NOT SUBBLOCK-MOVE FINISHED  */
*                                                                  0822
*             /***********************************/                0822
*             /*  FSB JUST PROCESSED IS NOT A    */                0822
*             /*  SUBBLOCK.  ALL THE DATA WAS    */                0822
*             /*  MOVED.  FEEDBACK PROCESSING IS */                0822
*             /*  FINISHED.                      */                0822
*             /***********************************/                0822
*                                                                  0822
*             CRRPOST=ON;         /* RPL SHOULD BE POSTED COMPLETE   */
         B     @RC00800                                            0822
@RF00800 OI    CRRPOST(CRRPTR),B'00010000'                         0822
*   ELSE DO;                      /* CODE 4 - BUFFER FILLED          */
*                                                                  0823
         B     @RC00799                                            0823
@RF00799 DS    0H                                                  0824
*        /**************************************/                  0824
*        /*  FSB JUST PROCESSED IS NOT A       */                  0824
*        /*  SUBBLOCK.  THE USER'S BUFFER WAS  */                  0824
*        /*  FILLED BEFORE ALL THE DATA WAS    */                  0824
*        /*  MOVED.  RESET ANY ENDING FEEDBACK */                  0824
*        /*  STATUS TO INDICATE NOT ALL DATA   */                  0824
*        /*  WAS RECEIVED.  SAVE FSB SO        */                  0824
*        /*  REMAINING DATA WILL BE PROCESSED  */                  0824
*        /*  UPON ISSUANCE OF NEXT READ.       */                  0824
*        /**************************************/                  0824
*                                                                  0824
*        CRRRDEOB=OFF;            /* INDICATE PARTIAL DATA IN BUFFER  *
*        CRRRDEOM=OFF;                                             0825
*        CRRRDEOT=OFF;                                             0826
         NI    CRRRDEOB(CRRPTR),B'11000111'                        0826
*        CRRSW1=ON;               /* SAVE FSB                        */
         OI    CRRSW1(CRRPTR),B'01000000'                          0827
*        CRRPOST=ON;              /* RPL SHOULD BE POSTED COMPLETE   */
         OI    CRRPOST(CRRPTR),B'00010000'                         0828
*        END;                                                      0829
*                                                                  0829
*   RETPTR = CRRRTN5;             /* GET RETURN ADDRESS              */
@RC00799 L     RETPTR,CRRRTN5(,CRRPTR)                             0830
*END KEEPPROC;                     /* RETURN TO CALLER                *
@EL00010 DS    0H                                                  0831
@EF00010 DS    0H                                                  0831
@ER00010 BR    @14                                                 0831
*/********************************************************************/
*/*                                                                  */
*/* ROUTINE NAME : NEXTLDO - FIND THE NEXT LDO                       */
*/*                                                                  */
*/* FUNCTION : THIS ROUTINE IS CALLED BY TRUNPROC OR KEEPPROC WHEN TH*/
*/* CURRENT USER BUFFER IS FILLED, BUT THE CURRENT LDO IS DATA       */
*/* CHAINED.  THE NEXT LDO IS TO BE LOCATED, AND                     */
*/* THE PARAMETERS FOR THE MOVE ROUTINE SAVED IN THE LCPB.  IF AN    */
*/* ERROR IS FOUND, PROCESSING IS TERMINATED.  IF A JUMP LDO IS      */
*/* ENCOUNTERED, JUMPPROC IS INVOKED TO VERIFY IT.                   */
*/*                                                                  */
*/* INPUT : LDOPTR (REG 9) - PTR TO THE LDO                          */
*/*         LCPBPTR (REG 7) - PTR TO THE LCPB                        */
*/*                                                                  */
*/* OUTPUT : CRRCLDO UPDATED                                         */
*/*                                                                  */
*/* SUBROUTINES CALLED : JUMPPROC                                    */
*/*                                                                  */
*/* MACROS USED : NONE                                               */
*/*                                                                  */
*/********************************************************************/
*NEXTLDO: PROC OPTIONS(NOSAVE,NOSAVEAREA);                         0832
NEXTLDO  DS    0H                                                  0833
*   CRRRTN6 = RETPTR;             /* SAVE RETURN ADDRESS             */
         ST    RETPTR,CRRRTN6(,CRRPTR)                             0833
*   LDOPTR = CRRCLDO + LDOSIZE;   /* UPDATE TO NEXT LDO              */
         LA    LDOPTR,8                                            0834
         AL    LDOPTR,CRRCLDO(,CRRPTR)                             0834
*   CRRCLDO = LDOPTR;             /* SAVE CURRENT LDO IN CRR         */
*                                                                  0835
         ST    LDOPTR,CRRCLDO(,CRRPTR)                             0835
*        IF LDOCMD=LDOJUMP THEN                                    0836
*                                                                  0836
         CLI   LDOCMD(LDOPTR),X'08'                                0836
         BNE   @RF00836                                            0836
*              /******************************************/        0837
*              /*   PROCESS DATA CHAINED JUMP LDO        */        0837
*              /******************************************/        0837
*              CALL JUMPPROC;     /* VALIDATE JUMP LDO               */
*                                                                  0837
         BAL   @14,JUMPPROC                                        0837
*        ELSE DO;                 /* NOT JUMP-SAVE MOVE INFORMATION  */
*                                                                  0838
         B     @RC00836                                            0838
@RF00836 DS    0H                                                  0839
*             /*****************************************/          0839
*             /*  VALID DATA CHAINED LDO.  SAVE STATUS */          0839
*             /*  FOR NEW LDO IN DCLCP SO MOVE         */          0839
*             /*  PROCESSING CAN BE CONTINUED WHEN     */          0839
*             /*  NEXT SUBBLOCK ARRIVES.               */          0839
*             /*****************************************/          0839
*                                                                  0839
*             DCLCLDOA=LDOPTR;    /* LDO TO BE PROCESSED NEXT        */
         ST    LDOPTR,DCLCLDOA(,LCPBPTR)                           0839
*             DCLIOBUF=LDOADDR;   /* RESUME DATA ADDRESS             */
         L     @15,LDOADDR(,LDOPTR)                                0840
         ST    @15,DCLIOBUF(,LCPBPTR)                              0840
*             DCLIOLEN=LDOLEN;    /* REMAINING LENGTH TO MOVE        */
         LH    @15,LDOLEN(,LDOPTR)                                 0841
         N     @15,@CF04418                                        0841
         STH   @15,DCLIOLEN(,LCPBPTR)                              0841
*             DCLRPLA=CRRARPL;    /* ADDRESS OF RPL FOR MOVE         */
         L     @15,CRRARPL(,CRRPTR)                                0842
         ST    @15,DCLRPLA(,LCPBPTR)                               0842
*             IF ADDR(LCPLCCW)->LCCWOP^=LTIC THEN                  0843
         CLI   LCCWOP+48(LCPBPTR),X'08'                            0843
         BE    @RF00843                                            0843
*                   DCLLDO1=LDOPTR;        /* SAVE LDO IN LCPB     */
         ST    LDOPTR,DCLLDO1(,LCPBPTR)                            0844
*             ELSE                     /* SAVE LDO IN EXTENSION */ 0845
*                   EVAL(CRRCLCCW+LDOFFSET)->ADDRLDO=LDOPTR;       0845
         B     @RC00843                                            0845
@RF00843 LA    @15,32                                              0845
         AL    @15,CRRCLCCW(,CRRPTR)                               0845
         ST    LDOPTR,ADDRLDO(,@15)                                0845
*             END;                                                 0846
*                                                                  0846
@RC00843 DS    0H                                                  0847
*   RETPTR=CRRRTN6;               /* GET RETURN ADDRESS              */
@RC00836 L     RETPTR,CRRRTN6(,CRRPTR)                             0847
*END NEXTLDO;                      /* RETURN TO CALLER                *
@EL00011 DS    0H                                                  0848
@EF00011 DS    0H                                                  0848
@ER00011 BR    @14                                                 0848
*/********************************************************************/
*/*                                                                  */
*/* ROUTINE NAME : JUMPPROC - JUMP LDO PROCESSING                    */
*/*                                                                  */
*/* FUNCTION : CALLED BY NEXTLDO TO VALIDATE A JUMP LDO.             */
*/* IF THE LDO JUMPED TO IS VALID ( NOT ANOTHER JUMP )  THEN         */
*/* IT IS USED TO INITIALIZE PARAMETERS FOR THE MOVE ROUTINE         */
*/* IN THE LCPB.  IF THE LDO JUMPED TO IS INVALID ( ANOTHER          */
*/* JUMP )  THEN AN ERROR CODE IS SET IN THE CRR AND FEEDBACK        */
*/* PROCESSING IS TERMINATED.                                        */
*/*                                                                  */
*/* INPUT : LDOPTR (REG 9) - PTR TO THE LDO                          */
*/*         LCPBPTR (REG 7) - PTR TO THE LCPB                        */
*/*                                                                  */
*/* OUTPUT : CRRPOST POSSIBLY SET                                    */
*/*                                                                  */
*/* SUBROUTINES CALLED : NONE                                        */
*/*                                                                  */
*/* MACROS USED : NONE                                               */
*/*                                                                  */
*/********************************************************************/
*JUMPPROC: PROC OPTIONS(NOSAVE,NOSAVEAREA);                        0849
JUMPPROC DS    0H                                                  0850
*   CRRRTN7 = RETPTR;             /* SAVE RETURN ADDRESS             */
         ST    RETPTR,CRRRTN7(,CRRPTR)                             0850
*   LDOPTR = LDOADDR;             /* ADDRESS OF JUMPED TO LDO        */
*                                                                  0851
         L     LDOPTR,LDOADDR(,LDOPTR)                             0851
*         IF LDOCMD=LDOJUMP THEN                                   0852
         CLI   LDOCMD(LDOPTR),X'08'                                0852
         BNE   @RF00852                                            0852
*              DO;                /* ERROR - JUMP TO A JUMP          */
*              RFY RETCODE UNRSTD;                                 0854
*                                                                  0854
*              /************************************/              0855
*              /*     ERROR-JUMP TO A JUMP         */              0855
*              /************************************/              0855
*                                                                  0855
*              CRRFDB2=CRRJTOJ;   /* SET REASON CODE FOR FEEDBACK    */
         MVI   CRRFDB2(CRRPTR),X'19'                               0855
*              CRRRTNCD=CRRLOGIC; /* LOGICAL I/O ERROR               */
         MVI   CRRRTNCD(CRRPTR),X'08'                              0856
*              CRRPOST=ON;        /* RPL SHOULD BE POSTED COMPLETE   */
         OI    CRRPOST(CRRPTR),B'00010000'                         0857
*              END;                                                0858
*         ELSE DO;                /* JUMP TO A VALID LDO             */
*                                                                  0859
         B     @RC00852                                            0859
@RF00852 DS    0H                                                  0860
*              /*************************************/             0860
*              /*  VALID JUMP TO LDO.  SAVE STATUS  */             0860
*              /*  FROM NEW LDO IN DCLCPB SO MOVE   */             0860
*              /*  CAN BE CONTINUED WHEN NEXT       */             0860
*              /*  SUBBLOCK ARRIVES.                */             0860
*              /*************************************/             0860
*                                                                  0860
*              DCLCLDOA=LDOPTR;   /* LDO TO BE PROCESSED NEXT        */
         ST    LDOPTR,DCLCLDOA(,LCPBPTR)                           0860
*              DCLIOBUF=LDOADDR;  /* RESUME DATA ADDRESS             */
         L     @15,LDOADDR(,LDOPTR)                                0861
         ST    @15,DCLIOBUF(,LCPBPTR)                              0861
*              DCLIOLEN=LDOLEN;   /* RESUME DATA COUNT               */
         LH    @15,LDOLEN(,LDOPTR)                                 0862
         N     @15,@CF04418                                        0862
         STH   @15,DCLIOLEN(,LCPBPTR)                              0862
*              CRRCLDO=LDOPTR;    /* UPDATE CURRENT LDO POINTER      */
         ST    LDOPTR,CRRCLDO(,CRRPTR)                             0863
*              DCLRPLA=CRRARPL;   /* SAVE RPL ADDRESS FOR NEXT MOVE  */
         L     @15,CRRARPL(,CRRPTR)                                0864
         ST    @15,DCLRPLA(,LCPBPTR)                               0864
*              IF ADDR(LCPLCCW)->LCCWOP^=LTIC THEN                 0865
         CLI   LCCWOP+48(LCPBPTR),X'08'                            0865
         BE    @RF00865                                            0865
*                   DCLLDO1=LDOPTR;        /* SAVE LDO IN LCPB     */
         ST    LDOPTR,DCLLDO1(,LCPBPTR)                            0866
*              ELSE                     /* SAVE LDO IN EXTENSION */
*                   EVAL(CRRCLCCW+LDOFFSET)->ADDRLDO=LDOPTR;       0867
         B     @RC00865                                            0867
@RF00865 LA    @15,32                                              0867
         AL    @15,CRRCLCCW(,CRRPTR)                               0867
         ST    LDOPTR,ADDRLDO(,@15)                                0867
*              END;                                                0868
*                                                                  0868
@RC00865 DS    0H                                                  0869
*   RETPTR = CRRRTN7;             /* GET RETURN ADDRESS              */
@RC00852 L     RETPTR,CRRRTN7(,CRRPTR)                             0869
*END JUMPPROC;                     /* RETURN TO NEXTLDO               *
@EL00012 DS    0H                                                  0870
@EF00012 DS    0H                                                  0870
@ER00012 BR    @14                                                 0870
*/********************************************************************/
*/*                                                                  */
*/* ROUTINE NAME : FLUSHRTN - FLUSH PROCESSING                       */
*/*                                                                  */
*/* FUNCTION : INVOKED WHEN THE FLUSH BIT IS ON IN THE FMCB.         */
*/* ALL THE FSB'S ARE TO BE IGNORED UNTIL AN END COMDITION IS        */
*/* MET. (EOB, EOM, OR EOT INDICATED IN FSB.)  ONCE AN END           */
*/* CONDITION IS MET, THE RPL CAN BE POSTED COMPLETE.  IN EITHER     */
*/* CASE, FREEFSB IS CALLED TO RELEASE THE STORAGE FOR THE FSB       */
*/* AND FDB'S.                                                       */
*/*                                                                  */
*/* INPUT : CRRFMCB - PTR TO THE FMCB                                */
*/*         FSBPTR (REG 6) - PTR TO THE FSB                          */
*/*                                                                  */
*/* OUTPUT : CRRSW5 POSSIBLY SET                                     */
*/*          FMCFLGA3 POSSIBLY TURNED OFF                            */
*/*                                                                  */
*/* SUBROUTINES CALLED : LCPBINIT                                    */
*/*                                                                  */
*/* MACROS USED : TPDEQ , TPQUE                                      */
*/*                                                                  */
*/********************************************************************/
*FLUSHRTN: PROC OPTIONS(NOSAVE,NOSAVEAREA);                        0871
FLUSHRTN DS    0H                                                  0872
*   CRRRTN2 = RETPTR;             /* SAVE RETURN ADDRESS             */
         ST    RETPTR,CRRRTN2(,CRRPTR)                             0872
*   IF FMCFLGA3=ON THEN            /* IS FLUSH INDICATOR STILL SET?  */
         L     @15,CRRFMCB(,CRRPTR)                                0873
         TM    FMCFLGA3(@15),B'00010000'                           0873
         BNO   @RF00873                                            0873
*     DO;                                                          0874
*       IF FSBMNELI=ON THEN        /* IS FSB ERROR LOCK SET?         */
*                                                                  0875
         TM    FSBMNELI(FSBPTR),B'10000000'                        0875
         BNO   @RF00875                                            0875
*          /***************************************/               0876
*          /* ERROR LOCK SET IN FSB BEING FLUSHED */               0876
*          /***************************************/               0876
*                                                                  0876
*          CRRSW5=ON;              /* YES, INDICATE ERRROR LOCK SET  */
         OI    CRRSW5(CRRPTR),B'00000100'                          0876
*       ELSE                                                       0877
*          CRRSW5=OFF;             /* NO, ERROR LOCK NOT SET         */
         B     @RC00875                                            0877
@RF00875 NI    CRRSW5(CRRPTR),B'11111011'                          0877
*       IF FSBEBI=ON ×             /* ENDING CONDITION FOUND IN FSB? */
*          FSBELI=ON THEN               /* END OF CHANNED PROGRAM?    *
@RC00875 TM    FSBEBI(FSBPTR),B'10100000'                          0878
         BZ    @RF00878                                            0878
*          DO;                                                     0879
*                                                                  0879
*            /*************************************/               0880
*            /* RESET INDICATOR TO INDICATE FLUSH */               0880
*            /* FINISHED.  EITHER ENDING CONDITION*/               0880
*            /* (EOB,EOM,EOT) FOUND IN FSB OR END */               0880
*            /* OF LOGICAL CHANNEL PROGRAM REACHED*/               0880
*            /*************************************/               0880
*                                                                  0880
*            RFY (REG10,REG11) RSTD;                               0880
*       CS5: REG10=FMCCMPS2;            /* GET OLD FLAG VALUE        */
CS5      L     @15,CRRFMCB(,CRRPTR)                                0881
         L     REG10,FMCCMPS2(,@15)                                0881
*            REG11=REG10&FLGA3OFF;      /* SET NEW FLAG VALUE        */
         LR    REG11,REG10                                         0882
         N     REG11,@CF03452                                      0882
*            CS (REG10,REG11,FMCCMPS2); /* SET NEW VALUE IN FMCB     */
         CS    REG10,@11,FMCCMPS2(@15)                             0883
*            BC (7,CS5);                /* TRY AGAIN IF FAILURE      */
         BC    7,CS5                                               0884
*            RFY (REG10,REG11) UNRSTD;                             0885
*          END;                                                    0886
*       ELSE;                                                      0887
@RF00878 DS    0H                                                  0888
*       IF CRRSW6=ON THEN                                          0888
         TM    CRRSW6(CRRPTR),B'00000010'                          0888
         BNO   @RF00888                                            0888
*          DO;                     /* FSB HAS BEEN TPQUE'D           */
*                                                                  0889
*          /*************************************/                 0890
*          /*  IF FSB NOW BEING FLUSHED HAD     */                 0890
*          /*  BEEN PREVIOUSLY BEEN QUEUED BY   */                 0890
*          /*  THIS MODULE, IT MUST BE DEQUEUED */                 0890
*          /*  SO IT CAN BE FLUSHED.            */                 0890
*          /*************************************/                 0890
*                                                                  0890
*                                                                  0890
*/* TPDEQ (,FMCFBA) RETPTR(FSBPTR) */                              0890
*           DO; /* BEGIN TPDEQ CODE GENERATION */                  0890
*           DCL FMZCA065 BIT(32) CONSTANT('80000000'X);            0891
*           DCL FMZCB065 BIT(32) CONSTANT('7FFFFFFF'X);            0892
*           RFY (GPR00P,GPR01P,GPR14P,GPR15P) RSTD;                0893
*                                                                  0893
*           RFY ISTWKE  ;                                          0894
*           GPR15P=ADDR(FMCFBA); /*ADDRESS QUEUE POINTER */        0895
         L     GPR15P,CRRFMCB(,CRRPTR)                             0895
         LA    GPR15P,FMCFBA(,GPR15P)                              0895
*           GPR14P=GPR15P->PABWEQA×FMZCA065;                       0896
         L     @08,@CF02699                                        0896
         L     GPR14P,PABWEQA(,GPR15P)                             0896
         OR    GPR14P,@08                                          0896
*           IF GPR14P=FMZCA065 THEN;                               0897
         CLR   GPR14P,@08                                          0897
         BE    @RT00897                                            0897
*           ELSE DO;                                               0899
*FMZL1065:;                                                        0900
FMZL1065 DS    0H                                                  0901
*             DO WHILE(GPR14P->WKECHAIN^=FMZCA065);                0901
         B     @DE00901                                            0901
@DL00901 DS    0H                                                  0902
*               GPR15P=ADDR(GPR14P->WKECHAIN);                     0902
         LA    GPR15P,WKECHAIN(,GPR14P)                            0902
*               GPR14P=GPR15P->PABWEQA;                            0903
         L     GPR14P,PABWEQA(,GPR15P)                             0903
*             END;                                                 0904
@DE00901 L     @08,@CF02699                                        0904
         CL    @08,WKECHAIN(,GPR14P)                               0904
         BNE   @DL00901                                            0904
*             GPR00P=FMZCA065;                                     0905
         LR    GPR00P,@08                                          0905
*             CS(GPR14P,GPR00P,GPR15P->PABWEQA);                   0906
         CS    GPR14P,@00,PABWEQA(GPR15P)                          0906
*             BC(7,FMZL1065);                                      0907
         BC    7,FMZL1065                                          0907
*             GPR14P->WKECHAIN=0;                                  0908
         SLR   @08,@08                                             0908
         ST    @08,WKECHAIN(,GPR14P)                               0908
*           END;                                                   0909
*ISTA0065:;                                                        0910
@RT00897 DS    0H                                                  0910
ISTA0065 DS    0H                                                  0911
*           GPR14P=GPR14P&FMZCB065;                                0911
         N     GPR14P,@CF03439                                     0911
*           FSBPTR=GPR14P;                                         0912
         LR    FSBPTR,GPR14P                                       0912
*           RFY (GPR00P,GPR01P,GPR14P,GPR15P) UNRSTD;              0913
*           END; /* END TPDEQ GENERATION */ /* DEQUE FSB             */
*          RFY (REG10,REG11) RSTD; /* BUFFER THRESH COMMENTED OUT*/
*  C40B:   REG10=FMCCMPS1;         /* CURRENT BUFFER COUNT          */
C40B     L     @15,CRRFMCB(,CRRPTR)                                0916
         L     REG10,FMCCMPS1(,@15)                                0916
*          REG11=REG10&BUFNB0;     /* NEW BUFFER COUNT               */
         LR    REG11,REG10                                         0917
         N     REG11,@CF04271                                      0917
*          CS (REG10,REG11,FMCCMPS1);  /* PUT IN FMCB                */
         CS    REG10,@11,FMCCMPS1(@15)                             0918
*          BC (7,C40B);            /* TRY AGAIN IF FAILED            */
         BC    7,C40B                                              0919
*          RFY (REG10,REG11) UNRSTD;                               0920
*          IF FSBPTR^=0 THEN                                       0921
         LTR   FSBPTR,FSBPTR                                       0921
         BZ    @RF00921                                            0921
*             CALL FREEFSB;        /* FSB WAS DEQUEUED               */
         BAL   @14,FREEFSB                                         0922
*          ELSE;                                                   0923
@RF00921 DS    0H                                                  0924
*          END;                                                    0924
*       ELSE CALL FREEFSB;         /* THROW FSB AWAY                 */
         B     @RC00888                                            0925
@RF00888 BAL   @14,FREEFSB                                         0925
*       CRRSW4=OFF;                /* INDICATE FLUSH OCCURRED        */
@RC00888 NI    CRRSW4(CRRPTR),B'11110111'                          0926
*     END;                                                         0927
*   ELSE CRRSW4=ON;                /* INDICATE FLUSH DID NOT OCCUR   */
         B     @RC00873                                            0928
