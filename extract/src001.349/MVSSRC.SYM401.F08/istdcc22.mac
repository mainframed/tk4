*       ELSE;                                                      0529
@RF00524 DS    0H                                                  0530
*     ELSE;                                                        0530
@RF00523 DS    0H                                                  0531
*    IF RPLEAU = ON THEN            /* ERASE ALL UNPROTECTED         */
         TM    RPLEAU(RPLPTR),B'00001000'                          0531
         BNO   @RF00531                                            0531
*    IF (DEVCHAR2 >= DEV3277 & DEVCHAR2 <= DEV3275) THEN           0532
         LA    @15,36                                              0532
         AL    @15,FMCDNCBA(,FMCBPTR)                              0532
         CLI   DEVCHAR2(@15),X'19'                                 0532
         BL    @RF00532                                            0532
         CLI   DEVCHAR2(@15),X'1C'                                 0532
         BH    @RF00532                                            0532
