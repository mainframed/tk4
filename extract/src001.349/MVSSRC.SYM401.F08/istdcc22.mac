         TITLE 'ISTDCC22, CONTROL LAYER:  WRITE SPECIFIC MACRO PROCESSO*
               OR       '
ISTDCC22 CSECT ,                                                   0001
@MAINENT DS    0H                                                  0001
         USING *,@15                                               0001
         B     @PROLOG                                             0001
         DC    AL1(16)                                             0001
         DC    C'ISTDCC22  77.126'                                 0001
         DROP  @15
@PROLOG  BALR  @12,0                                               0001
@PSTART  DS    0H                                                  0001
         USING @PSTART,@12                                         0001
* CRRPTR = ADDR(CRACRR -> CRRSPACE);/* PLACE ADDR OF CRR IN PTR    */
         L     @15,RPHCRR(,ADDRRPH)                                0456
         L     CRRPTR,CRACRR(,@15)                                 0456
         LA    CRRPTR,CRRSPACE(,CRRPTR)                            0456
* RFY CRRSAREA BASED(CRRPTR);       /* POINT TO CRR                  */
* RPLPTR = CRRARPL;                 /* PLACE ADDR OF RPL IN PTR      */
         L     RPLPTR,CRRARPL(,CRRPTR)                             0458
* RFY IFGRPL BASED(RPLPTR);         /* POINT TO THE RPL              */
* FMCBPTR = CRRFMCB;                /* PLACE ADDR OF FMCB IN PTR     */
         L     FMCBPTR,CRRFMCB(,CRRPTR)                            0460
* RFY ISTFMCB BASED(FMCBPTR);       /* POINT TO FMCB                 */
* RFY ISTNCB BASED(FMCDNCBA); /* ADDRESS NCB FOR DEVCH       @D32CKNL*/
* RFY ISTDEVCH BASED(ADDR(NCBDEVCH)); /* BASE FOR DEVICE CHARSTCS    */
* RFY ISTRPRE BASED(NCBRDTE);       /* RDTE BASE             @ZA19868*/
* RFY ISTRLN BASED(NCBRDTE);        /* RDTE BASE             @ZA19868*/
* RFY ISTRPT BASED(NCBRDTE);        /* RDTE BASE             @ZA19868*/
* LCPBPTR = CRRLCPB;                /* PLACE ADDR OF DCLCP IN PTR    */
         L     LCPBPTR,CRRLCPB(,CRRPTR)                            0467
* RFY ISTDCLCP BASED(LCPBPTR);      /* POINT TO THE DCLCP            */
* RFY ISTLCPB BASED(ADDR(DCLLCPB));                                0469
* LCCWPTR = LCPNXLCW;               /* PLACE 1ST LCCW ADDR IN PTR    */
         L     LCCWPTR,LCPNXLCW(,LCPBPTR)                          0470
* RFY ISTLCCW BASED(LCCWPTR);       /* POINT TO WRITE LCCW           */
*                                                                  0471
* CRRC22 = ON;                      /* SET THE AUDIT TRAIL SW       */
         OI    CRRC22(CRRPTR),B'01000000'                          0472
* CRRMAS = CRRMAS&&CRRMAS;          /* ZERO THE MODULE AUDIT FIELD   */
         XC    CRRMAS(8,CRRPTR),CRRMAS(CRRPTR)                     0473
* CRRM22 = ON;                      /* SET MODULE AUDIT SW           */
         OI    CRRM22(CRRPTR),B'01000000'                          0474
* IF DEVOTPUT = OFF &               /* IF INPUT ONLY DEVICE          */
*    (RPRENTRY^=RPRENTLN ×          /* ENTRY NOT A LINE      @ZA19868*/
*     RLNPORT=OFF ×                 /* THERE IS NO PORT      @ZA19868*/
*     RPTATERM=0 ×                  /* NO TERM               @ZA19868*/
*     ADDR(RPTATERM->RPRDEVCH)->DEVOTPUT=OFF) THEN DO; /*INPUT ONLY  */
         L     @15,FMCDNCBA(,FMCBPTR)                              0475
         TM    DEVOTPUT+36(@15),B'01000000'                        0475
         BNZ   @RF00475                                            0475
         L     @15,NCBRDTE(,@15)                                   0475
         CLI   RPRENTRY(@15),X'50'                                 0475
         BNE   @RT00475                                            0475
         TM    RLNPORT(@15),B'00010000'                            0475
         BZ    @RT00475                                            0475
         L     @15,RPTATERM(,@15)                                  0475
         LTR   @15,@15                                             0475
         BZ    @RT00475                                            0475
         TM    DEVOTPUT+44(@15),B'01000000'                        0475
         BNZ   @RF00475                                            0475
@RT00475 DS    0H                                                  0476
*    CRRRTNCD = CRRLOGIC;           /* CRR RETURN CODE               */
         MVI   CRRRTNCD(CRRPTR),X'08'                              0477
*    CRRFDB2 = CRRWTOI;             /* USER ERROR CODE: WRITE TO     */
         MVI   CRRFDB2(CRRPTR),X'11'                               0478
*                                   /*      INPUT ONLY DEVICE TYPE   */
* END;                              /* END - INPUT-ONLY DEVICE       */
* ELSE DO;                          /* NOT INPUT-ONLY DEVICE         */
         B     @RC00475                                            0480
@RF00475 DS    0H                                                  0481
*      IF DEVCHAR2=DEV3735 & FMCFLGA6=OFF THEN                     0481
         L     @15,FMCDNCBA(,FMCBPTR)                              0481
         CLI   DEVCHAR2+36(@15),X'0B'                              0481
         BNE   @RF00481                                            0481
         TM    FMCFLGA6(FMCBPTR),B'00000010'                       0481
         BNZ   @RF00481                                            0481
*         DO;                                                      0482
*           CRRRTNCD=CRRLOGIC;                                     0483
         MVI   CRRRTNCD(CRRPTR),X'08'                              0483
*           CRRFDB2=CRRRDNFO;                                      0484
         MVI   CRRFDB2(CRRPTR),X'28'                               0484
*         END;                                                     0485
*      ELSE DO;                                                    0486
         B     @RC00481                                            0486
@RF00481 DS    0H                                                  0487
*    IF RPLERACE = ON THEN          /* ERASE WRITE IN RPL            */
         TM    RPLERACE(RPLPTR),B'00000100'                        0487
         BNO   @RF00487                                            0487
*       IF RPLEOB = ON THEN DO;     /* IF BLOCK MODE IN RPL          */
         TM    RPLEOB(RPLPTR),B'10000000'                          0488
         BNO   @RF00488                                            0488
*       CRRRTNCD = CRRLOGIC;        /* CRR RETURN CODE               */
         MVI   CRRRTNCD(CRRPTR),X'08'                              0490
*       CRRFDB2 = CRREWBLK;         /* USER ERROR CODE: BLOCK MODE   */
         MVI   CRRFDB2(CRRPTR),X'1E'                               0491
*                                   /*      WITH ERASE WRITE         */
*       END;                        /* END - BLOCK MODE ERASE WRITE  */
*       ELSE DO;                    /* NOT BLOCK MODE ERASE WRITE    */
         B     @RC00488                                            0493
@RF00488 DS    0H                                                  0494
*        IF RPLWROPT = ON THEN DO;  /* IS CONVERSATIONAL REQUESTED?  */
         TM    RPLWROPT(RPLPTR),B'00000001'                        0494
         BNO   @RF00494                                            0494
*       CRRRTNCD=CRRLOGIC;          /* SET LOGIC ERROR CODE          */
         MVI   CRRRTNCD(CRRPTR),X'08'                              0496
*       CRRFDB2=CRREWCNV;           /* SET ERASE-WRITE-CONV ERROR    */
         MVI   CRRFDB2(CRRPTR),X'15'                               0497
*       END;                        /* END-CONVERSATIONAL ERASE WRITE*/
*     ELSE DO;                      /* NOT ERASE-WRITE-CONVERSATIONAL*/
         B     @RC00494                                            0499
@RF00494 DS    0H                                                  0500
*      IF (DEVCHAR2 >= DEV3277 & DEVCHAR2 <= DEV3275) ×            0500
*          DEVCHAR2 = DEV2265 THEN  /*      OR 2265?                 */
         LA    @15,36                                              0500
         AL    @15,FMCDNCBA(,FMCBPTR)                              0500
         CLI   DEVCHAR2(@15),X'19'                                 0500
         BL    @GL00006                                            0500
         CLI   DEVCHAR2(@15),X'1C'                                 0500
         BNH   @RT00500                                            0500
@GL00006 L     @15,FMCDNCBA(,FMCBPTR)                              0500
         CLI   DEVCHAR2+36(@15),X'7D'                              0500
         BNE   @RF00500                                            0500
@RT00500 DS    0H                                                  0501
*          IF DEVCHAR2 = DEV2265 THEN DO; /* IF 2265                 */
         L     @15,FMCDNCBA(,FMCBPTR)                              0501
         CLI   DEVCHAR2+36(@15),X'7D'                              0501
         BNE   @RF00501                                            0501
*          DCLECHAR = ERASCHAR;     /* MOVE ERASE CHAR TO DCLCP      */
         MVC   DCLECHAR(2,LCPBPTR),@CB03810                        0503
*          LCCWCD = ON;             /* SET CHAINED DATA BIT          */
* /* LINE # 88411 ADDED FOR DCR 2007    */                         0505
*          LCCWNTVC=ON;             /* TURN ON THE DO NOT VALHEK BIT */
         OI    LCCWCD(LCCWPTR),B'10010000'                         0505
*          IF RPLEOM = ON THEN      /* WRITE MESSAGE?                */
         TM    RPLEOM(RPLPTR),B'01000000'                          0506
         BNO   @RF00506                                            0506
*          LCCWRWOP = LEWM;         /* FILL WRITE MESSAGE OP         */
         NI    LCCWRWOP(LCCWPTR),B'11011001'                       0507
         OI    LCCWRWOP(LCCWPTR),B'00011001'                       0507
*          ELSE LCCWRWOP = LEWT;    /* ELSE WRITE TRANMISSION        */
         B     @RC00506                                            0508
@RF00506 NI    LCCWRWOP(LCCWPTR),B'11011101'                       0508
         OI    LCCWRWOP(LCCWPTR),B'00011101'                       0508
*          LCCWCNT = ERASSIZE;      /* FILL IN COUNT                 */
@RC00506 LH    @00,ERASSIZE                                        0509
         STH   @00,LCCWCNT(,LCCWPTR)                               0509
*         LCCWDATA = ADDR(DCLECHAR);/* FILL IN AREA                  */
         LA    @00,DCLECHAR(,LCPBPTR)                              0510
         ST    @00,LCCWDATA(,LCCWPTR)                              0510
*          LCCWPTR = LCCWPTR + LENGTH(ISTLCCW);                    0511
         AL    LCCWPTR,@CF00038                                    0511
*                                   /* POINT TO NEXT LCCW            */
*          END;                     /* END- 2265                     */
*          ELSE;                    /* NOT 2265                      */
@RF00501 DS    0H                                                  0514
*       ELSE DO;                    /* NOT 3270 OR 2265              */
         B     @RC00500                                            0514
@RF00500 DS    0H                                                  0515
*       CRRRTNCD = CRRLOGIC;        /* CRR RETURN CODE               */
         MVI   CRRRTNCD(CRRPTR),X'08'                              0515
*       CRRFDB2 = CRREWNS;          /* USER ERROR CODE: ERASE WRITE  */
         MVI   CRRFDB2(CRRPTR),X'12'                               0516
*                                   /* TO NON 2265 OR 3270 DEV       */
*       END;                        /* END- NOT 3270 OR 2265         */
*      END;                         /* END- NOT ERASE-WRITE-CONV     */
*     END;                          /* END- NOT BLOCK MODE           */
*    ELSE;                          /* NOT ERASE WRITE               */
@RF00487 DS    0H                                                  0521
