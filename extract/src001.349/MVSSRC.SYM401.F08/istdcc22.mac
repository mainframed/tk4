*          LCCWRWOP = LEAU;         /* WRITE/ERASE ALL UNPROTECTED   */
         NI    LCCWRWOP(LCCWPTR),B'11010001'                       0535
         OI    LCCWRWOP(LCCWPTR),B'00010001'                       0535
*          LCCWCNT = RPLRLEN;       /* FILL IN COUNT                 */
         L     @00,RPLRLEN(,RPLPTR)                                0536
         STH   @00,LCCWCNT(,LCCWPTR)                               0536
*          LCCWDATA = RPLAREA;      /* FILL IN AREA                  */
         L     @00,RPLAREA(,RPLPTR)                                0537
         ST    @00,LCCWDATA(,LCCWPTR)                              0537
*          END;                     /* END-3270&ERASE/WRITE ALL UNPROT*
*          ELSE DO;                 /* ERASE WRITE CONVERSATIONAL   */
         B     @RC00533                                            0539
@RF00533 DS    0H                                                  0540
*          CRRRTNCD=CRRLOGIC;                                      0540
         MVI   CRRRTNCD(CRRPTR),X'08'                              0540
*          CRRFDB2=CRREWCNV;       /* ERASE WRITE CONV MINOR RTN CODE*/
         MVI   CRRFDB2(CRRPTR),X'15'                               0541
*          END;                    /* END ERASE WRITE CONVERSATIONAL*/
*      ELSE DO;                     /* NOT 3270                      */
         B     @RC00532                                            0543
@RF00532 DS    0H                                                  0544
*       CRRRTNCD = CRRLOGIC;        /* CRR ERROR CODE                */
         MVI   CRRRTNCD(CRRPTR),X'08'                              0544
*       CRRFDB2 = CRREWAU3;         /* USER ERROR CODE: ERASE WRITE  */
         MVI   CRRFDB2(CRRPTR),X'13'                               0545
*                                   /* ALL UNPROT TO NON-3270 DEV    */
*       END;                        /* END- NOT 3270                 */
*    ELSE DO;                       /* NOT ERASE/WRITE ALL UNPROT    */
         B     @RC00531                                            0547
@RF00531 DS    0H                                                  0548
*       IF RPLEOB = ON THEN         /* IF WRITE BLOCK                */
         TM    RPLEOB(RPLPTR),B'10000000'                          0548
         BNO   @RF00548                                            0548
*       LCCWRWOP = LWB;             /* FILL WRITE BLOCK OP           */
         NI    LCCWRWOP(LCCWPTR),B'11000101'                       0549
         OI    LCCWRWOP(LCCWPTR),B'00000101'                       0549
*       ELSE                                                       0550
*       IF RPLEOM = ON THEN         /* IF WRITE MESSAGE              */
         B     @RC00548                                            0550
@RF00548 TM    RPLEOM(RPLPTR),B'01000000'                          0550
         BNO   @RF00550                                            0550
*       LCCWRWOP = LWM;             /* FILL IN WRITE MESSAGE OP      */
         NI    LCCWRWOP(LCCWPTR),B'11001001'                       0551
         OI    LCCWRWOP(LCCWPTR),B'00001001'                       0551
*       ELSE                                                       0552
* IF RPLEOT=ON THEN LCCWRWOP = LWT; /* FILL IN WRITE TRANSMISSION    */
         B     @RC00550                                            0552
@RF00550 TM    RPLEOT(RPLPTR),B'00100000'                          0552
         BNO   @RF00552                                            0552
         NI    LCCWRWOP(LCCWPTR),B'11001101'                       0553
         OI    LCCWRWOP(LCCWPTR),B'00001101'                       0553
*       LCCWDATA = RPLAREA;         /* FILL IN AREA                  */
@RF00552 DS    0H                                                  0554
@RC00550 DS    0H                                                  0554
@RC00548 L     @00,RPLAREA(,RPLPTR)                                0554
         ST    @00,LCCWDATA(,LCCWPTR)                              0554
*       LCCWCNT  = RPLRLEN;         /* FILL IN COUNT                 */
         L     @00,RPLRLEN(,RPLPTR)                                0555
         STH   @00,LCCWCNT(,LCCWPTR)                               0555
*IF (DEVCHAR2 >= DEV3277 & DEVCHAR2 <= DEV3275) & RPLERACE = ON THEN
         LA    @15,36                                              0556
         AL    @15,FMCDNCBA(,FMCBPTR)                              0556
         CLI   DEVCHAR2(@15),X'19'                                 0556
         BL    @RF00556                                            0556
         CLI   DEVCHAR2(@15),X'1C'                                 0556
         BH    @RF00556                                            0556
         TM    RPLERACE(RPLPTR),B'00000100'                        0556
         BNO   @RF00556                                            0556
