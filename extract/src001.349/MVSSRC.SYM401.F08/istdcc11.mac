@RF00591 L     @15,CRRFMCB(,CRRPTR)                                0601
         LA    @12,36                                              0601
         AL    @12,FMCDNCBA(,@15)                                  0601
         CLI   DEVTCODE(@12),X'19'                                 0601
         BL    @RF00601                                            0601
         CLI   DEVTCODE(@12),X'1C'                                 0601
         BH    @RF00601                                            0601
