         OI    LCCWRWOP(@15),B'00011010'                           0618
*             CALL CSUBR5;          /* CHECK DATA CHAIN AND DIALOGUE */
@RC00616 DS    0H                                                  0619
@RC00614 DS    0H                                                  0619
@RC00612 BAL   @14,CSUBR5                                          0619
*           END;                                                   0620
*         ELSE                                                     0621
*           DO;                     /* INVALID READ MODIFIER         */
