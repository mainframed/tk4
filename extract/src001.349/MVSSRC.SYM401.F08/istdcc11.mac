         L     @15,CRRFMCB(,CRRPTR)                                0605
         CS    REG10,@11,FMCCMPS2(@15)                             0605
*             BC(7,CSB7);           /* FAILED,RETRY          @XM10597*/
         BC    7,CSB7                                              0606
