         L     @15,LDOADDR(,LDOPTR)                                0658
         ST    @15,CRRBARAD(,CRRPTR)                               0658
*   CRRBLDAD=CRRCLDO;               /* CURRENT LDO FOR MOVE          */
         L     @15,CRRCLDO(,CRRPTR)                                0659
         ST    @15,CRRBLDAD(,CRRPTR)                               0659
*   CRRBTLEN=CRRBTLEN&&CRRBTLEN;    /* CLEAR RESIDUAL COUNT          */
         L     @15,CRRBTLEN(,CRRPTR)                               0660
         LR    @12,@15                                             0660
         XR    @12,@15                                             0660
         ST    @12,CRRBTLEN(,CRRPTR)                               0660
*   CRRBLEN=LDOLEN;                 /* SIZE OF CURRENT INPUT AREA    */
         LH    @15,LDOLEN(,LDOPTR)                                 0661
         N     @15,@CF04303                                        0661
         STH   @15,CRRBLEN(,CRRPTR)                                0661
*   DO WHILE(CRRSW3=OFF);           /* CONTROL LOOP FOR MOVE         */
*                                                                  0662
         B     @DE00662                                            0662
@DL00662 DS    0H                                                  0663
*     /***************************************************************/
