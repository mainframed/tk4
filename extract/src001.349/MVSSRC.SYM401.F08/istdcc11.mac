         BC    7,CS0                                               0598
*           CRRDEXP=ON;             /* SET DATA EXPECTED     @XM10597*/
         OI    CRRDEXP(CRRPTR),B'10000000'                         0599
*         END;                      /* END NOT FIRST         @XM10597*/
