*     DO;                           /* ALL DATA WAS MOVED            */
*                                                                  0901
         B     @RC00893                                            0901
@RF00893 DS    0H                                                  0902
*       /*************************************************************/
*       /*                                                           */
*       /* PROCESSING FINISHED-ALL DATA MOVED                        */
*       /*                                                           */
*       /*************************************************************/
*                                                                  0902
*       CALL FREEFSB;               /* DEQUEUE AND FEE FSB           */
         BAL   @14,FREEFSB                                         0902
*       CRRCLDO=CRRBLDAD;           /* SAVE PTR TO LAST LDO USED     */
         L     @12,CRRBLDAD(,CRRPTR)                               0903
         ST    @12,CRRCLDO(,CRRPTR)                                0903
*       RPLAAREA=CRRBLDAD;          /* SAVE LDO POINTER FOR USER     */
         LR    RPLAAREA,@12                                        0904
*       CALL UPLDOPTR;              /* UPDATE TO END-CHECK CMD CHAIN */
         BAL   @14,UPLDOPTR                                        0905
*       CRRSW3=ON;                  /* FINISHED WITH MOVE            */
         OI    CRRSW3(CRRPTR),B'00010000'                          0906
*     END;                                                         0907
*   RETPTR=CRRRTN4;                 /* GET RETURN ADDRESS            */
@RC00893 L     RETPTR,CRRRTN4(,CRRPTR)                             0908
*   END CSUBR9;                     /* GET RETURN ADDRESS            */
@EL00012 DS    0H                                                  0909
@EF00012 DS    0H                                                  0909
@ER00012 BR    @14                                                 0909
*CSUBR10:                                                          0910
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0910
CSUBR10  DS    0H                                                  0911
*   CRRRTN7=RETPTR;                 /* SAVE RETURN ADDRESS           */
*                                                                  0911
         ST    RETPTR,CRRRTN7(,CRRPTR)                             0911
*   /*****************************************************************/
*   /*                                                               */
*   /* THERE IS NO ROOM IN THE USER'S BUFFER, BUT THERE IS MORE DATA */
*   /* TO BE MOVED. IF THE USER SPECIFIED KEEP IN HIS FMCB, AN       */
*   /* INDICATION THAT PARTIAL DATA WAS RECEIVED IS SET AS FEEDBACK  */
*   /* STATUS AND THE FSB IS NOT FREED. IF TRUNC WAS SPECIFIED, A    */
*   /* LOGICAL I/O ERROR IS SET AS STATUS AND ALL THE DATA IS THROWN */
*   /* AWAY UNTIL AND ENDING CONDITION IS FOUND(EOB).                */
*   /*                                                               */
