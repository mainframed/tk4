CSUBR5   DS    0H                                                  0631
*/********************************************************************/
*/*                                                                  */
*/* ROUTINE NAME : CSUBR5 - CHECK DATA CHAIN AND DIALOGUE            */
*/*                                                                  */
*/* FUNCTION :                                                       */
*/*    IF LDO IS DATA CHAINED, UPDATE CURRENT LDO POINTER TO FIRST   */
*/*    NON-DATA CHAINED LDO.  SET SWITCHES TO START I/O.  INDICATE   */
*/*    DATA IS NOW EXPECTED FROM THE DEVICE AND THAT A READ HAS      */
*/*    BEEN ISSUED TO THE DEVICE.  TEST WHETHER USER WISHES TO BE    */
*/*    IN DIALOGUE WITH A SPECIFIC TERMINAL OR WITH ANY TERMINAL.    */
*/*                                                                  */
*/* INPUT :  LDOPTR  (REG 9) - POINTS TO CURRENT LDO                 */
*/*                                                                  */
*/* OUTPUT : SAME                                                    */
*/*                                                                  */
*/* SUBROUTINES CALLED : UPLDOPTR - FIND NON-DATA CHAINED LDO        */
*/*                                                                  */
*/* MACROS USED : NONE                                               */
*/*                                                                  */
*/********************************************************************/
*                                                                  0631
*   CRRRTN4=RETPTR;                 /* SAVE RETURN ADDRESS           */
         ST    RETPTR,CRRRTN4(,CRRPTR)                             0631
*   IF LDOACHN=ON THEN                                             0632
         TM    LDOACHN(LDOPTR),B'10000000'                         0632
         BNO   @RF00632                                            0632
*     CALL UPLDOPTR;                /* DATA CHAINED                  */
         BAL   @14,UPLDOPTR                                        0633
*   ELSE                                                           0634
*     ;                             /* NOT DATA CHAINED              */
@RF00632 DS    0H                                                  0635
*   IF CRRFINI=ON THEN                                             0635
         TM    CRRFINI(CRRPTR),B'00001000'                         0635
         BO    @RT00635                                            0635
*     ;                             /* ERROR - DO NOT START I/O      */
*                                                                  0636
*   /*****************************************************************/
*   /*                                                               */
*   /* INDICATE I/O TO BE STARTED                                    */
*   /*                                                               */
*   /*****************************************************************/
*                                                                  0637
*   ELSE                                                           0637
*     DO;                           /* NO ERRORS FOUND BY UPLDOPTR   */
*       RFY                                                        0638
*        (REG10,                                                   0638
*         REG11) RSTD;                                             0638
*CS1:                                                              0639
*       REG10=FMCCMPS2;             /* REFERENCE OLD FLAG VALUE      */
CS1      L     @15,CRRFMCB(,CRRPTR)                                0639
         L     REG10,FMCCMPS2(,@15)                                0639
*       REG11=REG10×FLGA06ON;       /* SET DATA EXPECTED AND READ  0640
*                                      DONE TO DEVICE                */
         LR    REG11,REG10                                         0640
         O     REG11,@CF04162                                      0640
*       CS(REG10,REG11,FMCCMPS2);   /* SET NEW VALUE IN FMCB         */
         CS    REG10,@11,FMCCMPS2(@15)                             0641
*       BC(7,CS1);                  /* TRY AGAIN IF FAILURE          */
         BC    7,CS1                                               0642
*       CRRDEXP=ON;                 /* SET DATA EXPECTED IN CRR      */
         OI    CRRDEXP(CRRPTR),B'10000000'                         0643
*       CRRINIO=ON;                 /* I/O REQUEST TO BE INITIATED   */
         OI    CRRINIO(CRRPTR),B'00100000'                         0644
*CS2:                                                              0645
*       REG10=FMCCMPS2;             /* REFERENCE OLD FLAG VALUE      */
CS2      L     @15,CRRFMCB(,CRRPTR)                                0645
         L     REG10,FMCCMPS2(,@15)                                0645
*       IF RPLDLGIN=OFF THEN        /* TEST FOR CONTINUE ANY         */
         L     @15,CRRARPL(,CRRPTR)                                0646
         TM    RPLDLGIN(@15),B'10000000'                           0646
         BNZ   @RF00646                                            0646
*         REG11=REG10&FLGA1OFF;     /* RESET SATISFY SPECIFIC RD     */
         LR    REG11,REG10                                         0647
         N     REG11,@CF02553                                      0647
*       ELSE                                                       0648
*         REG11=REG10×FLGA1ON;      /* SET SATISFY SPECIFIC READ     */
         B     @RC00646                                            0648
@RF00646 LR    REG11,REG10                                         0648
         O     REG11,@CF02555                                      0648
*       CS(REG10,REG11,FMCCMPS2);   /* SET NEW VALUE IN FMCB         */
@RC00646 L     @15,CRRFMCB(,CRRPTR)                                0649
         CS    REG10,@11,FMCCMPS2(@15)                             0649
*       BC(7,CS2);                  /* TRY AGAIN IF FAILURE          */
         BC    7,CS2                                               0650
*       RFY                                                        0651
*        (REG10,                                                   0651
*         REG11) UNRSTD;                                           0651
*     END;                                                         0652
*   RETPTR=CRRRTN4;                 /* GET RETURN ADDRESS            */
@RT00635 L     RETPTR,CRRRTN4(,CRRPTR)                             0653
*   END CSUBR5;                     /* RETURN TO CALLER              */
@EL00006 DS    0H                                                  0654
@EF00006 DS    0H                                                  0654
@ER00006 BR    @14                                                 0654
*CSUBR6:                                                           0655
*   PROC OPTIONS(NOSAVE,NOSAVEAREA);                               0655
*                                                                  0655
CSUBR6   DS    0H                                                  0656
