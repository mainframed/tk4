*/* ROUTINE NAME : PROCESSD - SUBBLOCK HANDLER                       */
*/*                                                                  */
*/* FUNCTION : DETERMINES IF ADDITIONAL DATA SHOULD BE MOVED (SETS   */
*/*      CRRSW=0), AND TO CALL PROCESSE TO DEQUEUE FSB(S) & FDB(S)   */
*/*                                                                  */
*/* INPUT : SEE INPUT TABLE BELOW                                    */
*/*                                                                  */
*/* OUTPUT : SEE OUTPUT TABLE BELOW                                  */
*/*                                                                  */
*/* SUBROUTINES CALLED : PROCESSE                                    */
*/*                                                                  */
*/* MACROS USED : TPFEL                                              */
*/*                                                                  */
*/********************************************************************/
*/* * * * * * * * * INPUT TABLE FOR PROCESSD * * * * * * * * * * * * */
*/*                                                                  */
*/*   *FSBPTR * SHOULD CONTAIN THE ADDRESS OF FSB UPON RETURN FROM   */
*/*        FROM TPFEL MACRO CALL                                     */
*/*   CRRBARAD MUST CONTAIN THE RESUME ADDRESS                       */
*/*   CRRBLEN MUST CONTAIN THE RESIDUAL LENGTH                       */
*/*   CRRBTLEN MUST CONTAIN THE TOTAL LENGTH                         */
*/*   RPLPTR (REGISTER 4), SHOULD HAVE BEEN SET IN MAINLINE          */
*/*        TO THE ADDRESS OF RPL                                     */
*/*                                                                  */
*/* * * * * * * * * * OUTPUT FROM PROCESSD* * * * * * * * * * * * * **/
*/*                                                                  */
*/* FIELDS LISTED BELOW COMPRISE POSSIBLE OUTPUT FROM THIS PROCEDURE.*/
*/* IF A FIELD IS ASSIGNED A VALUE FROM AN INPUT FIELD, THE INPUT    */
*/* FIELD IS NOTED BETWEEN ASTERISKS.  IF A FIELD'S BEING SET IS     */
*/* LOGICALLY DEPENDENT UPON A TEST OF AN INPUT FIELD, THE INPUT     */
*/* FIELD IS SHOWN BETWEEN PARENTHESES:                              */
*/*                                                                  */
*/* RESUMEAD*CRRRTN4*   LCPIOBUF*CRRBARAD*  LCPIOLEN(FSBPTR)*CRRBLEN**/
*/* CRRRTN4*RESUMEAD*   CRRFSB(FSBPTR)*FSBPTR*                       */
*/* CRRSW0(FSBPTR)      CRRSW1(FSBEBI  ,FSBEMI  ,FSBETI  )           */
*/* LCPBTLEN(FSBPTR)*CRRBTLEN*              LCPRPLA(FSBPTR)*RPLPTR*  */
*/*                                                                  */
*/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * **/
*                                                                  0658
*PROCESSD:                                                         0658
*   PROCEDURE OPTIONS(NOSAVE,NOSAVEAREA);                          0658
PROCESSD DS    0H                                                  0659
*   CRRRTN4=RESUMEAD;               /* SAVE RETURN ADDR (REG14)      */
         ST    RESUMEAD,CRRRTN4(,CRRPTR)                           0659
*   RFY                                                            0660
*     RESUMEAD UNRSTD;                                             0660
*   CRRSW1=OFF;                     /* INIT SUBBLOCK SWITCH TO BE  0661
*                                      USEWITHIN THIS PROC           */
         NI    CRRSW1(CRRPTR),B'10111111'                          0661
*   IF FSBEBI=OFF&                  /* IF NO ENDING CONDITION IS   0662
*                                      FOUND IN THE FSB -- THE FSB IS
*                                      A SUBBLOCK            @D8355CF*/
*       FSBLGI=OFF&FSBSOHI=OFF&     /* NO LEAD GRAPH OR SOH(D1869)   */
*       FSBMJRCD=FRNORM&            /* NON-ERROR CODE IN FSB         */
*/*  LINE ADDED FOR DCR 2008 ( 9/17/73 )                             */
*                                                                  0662
*       FSBLCPB^=CLEAR THEN         /* FSB IS SOLICITED              */
         TM    FSBEBI(FSBPTR),B'00100011'                          0662
         BNZ   @RF00662                                            0662
         CLI   FSBMJRCD(FSBPTR),X'00'                              0662
         BNE   @RF00662                                            0662
         L     @10,FSBLCPB(,FSBPTR)                                0662
         LTR   @10,@10                                             0662
         BZ    @RF00662                                            0662
*     DO;                                                          0663
*       LCPBPTR=FSBLCPB;            /* SAVE LCPB ADDRESS K0931       */
         LR    LCPBPTR,@10                                         0664
*       RFY                                                        0665
*         ISTDCLCP BASED(LCPBPTR);                                 0665
*       CRRSW1=ON;                  /* SETTING CRRSW1 INDICATES THAT
*                                      A SUBBLOCK WAS FOUND -- THIS
*                                      SW IS SET BECAUSE FSB WILL BE
*                                      FREED IN NEXT STEP            */
         OI    CRRSW1(CRRPTR),B'01000000'                          0666
*     END;                                                         0667
*   ELSE                                                           0668
*     ;                                                            0668
@RF00662 DS    0H                                                  0669
*   CALL PROCESSE;                  /* FREE FSB & FDB(S)             */
         BAL   @14,PROCESSE                                        0669
*   IF CRRSW1=ON THEN               /* CRRSW1=ON MEANS 'FSB FREED WAS
*                                      A SUBBLOCK                    */
         TM    CRRSW1(CRRPTR),B'01000000'                          0670
         BNO   @RF00670                                            0670
*     DO;                                                          0671
*                                                                  0671
*/* TPFEL (,FMCFBA) RETPTR(FSBPTR)                                   */
*                                                                  0672
*       DO;                         /* BEGIN TPFEL CODE GENERATION   */
*         RFY                                                      0673
*          (GPR00P,                                                0673
*           GPR01P,                                                0673
*           GPR14P,                                                0673
*           GPR15P) RSTD;                                          0673
*         GPR15P=ADDR(FMCFBA);      /* ADDRESS QUEUE POINTER         */
         LA    GPR15P,FMCFBA(,FMCBPTR)                             0674
*         GPR14P=GPR15P->PABWEQA×FMZCA005;                         0675
         L     @10,@CF01323                                        0675
         L     GPR14P,PABWEQA(,GPR15P)                             0675
         OR    GPR14P,@10                                          0675
*         IF GPR14P=FMZCA005 THEN                                  0676
         CLR   GPR14P,@10                                          0676
         BE    @RT00676                                            0676
*           ;                                                      0677
*         ELSE                                                     0678
*           DO;                                                    0678
*             GPR00P=FMZCA005;      /* INITIALIZE QUIT FLAG          */
         L     GPR00P,@CF01323                                     0679
*             DO WHILE(GPR00P^=GPR14P->WKECHAIN);                  0680
         B     @DE00680                                            0680
@DL00680 DS    0H                                                  0681
*               GPR14P=GPR14P->WKECHAIN;/* SEARCH FOR END OF CHAIN   */
         L     GPR14P,WKECHAIN(,GPR14P)                            0681
*             END;                                                 0682
@DE00680 C     GPR00P,WKECHAIN(,GPR14P)                            0682
         BNE   @DL00680                                            0682
*           END;                                                   0683
*         GPR14P=GPR14P&FMZCB005;                                  0684
@RT00676 N     GPR14P,@CF01321                                     0684
*         FSBPTR=GPR14P;                                           0685
         LR    FSBPTR,GPR14P                                       0685
*         RFY                                                      0686
*          (GPR00P,                                                0686
*           GPR01P,                                                0686
*           GPR14P,                                                0686
*           GPR15P) UNRSTD;                                        0686
*       END;                        /* END TPFEL GENERATION GET @ FSB
*                                      FROM FMCB                     */
*       IF FSBPTR=CLEAR THEN        /* IF @ FSB IS ZERO, THERE IS NO
*                                      ADDIT'L DATA TO MOVE          */
         SLR   @10,@10                                             0688
         CR    FSBPTR,@10                                          0688
         BNE   @RF00688                                            0688
*         IF LCPBPTR^=CLEAR THEN    /* TEST FOR UNSOLICITED INPUT  0689
*                                      K0931                         */
         CR    LCPBPTR,@10                                         0689
         BE    @RF00689                                            0689
*           IF CRRERLK=ON THEN      /* TEST FOR ERROR LOCK K0931     */
         TM    CRRERLK(CRRPTR),B'10000000'                         0690
         BNO   @RF00690                                            0690
*             DCLRPLA=CLEAR;        /* FINISHED WITH THIS RPL K0931  */
         ST    @10,DCLRPLA(,LCPBPTR)                               0691
*           ELSE                                                   0692
*             DO;                   /* ERROR LOCK NOT SET K0931      */
         B     @RC00690                                            0692
@RF00690 DS    0H                                                  0693
*               DCLIOBUF=CRRBARAD;  /* PUT RESUME ADDR FROM CRR PARM
*                                      LIST INTO LCPB                */
         L     @10,CRRBARAD(,CRRPTR)                               0693
         ST    @10,DCLIOBUF(,LCPBPTR)                              0693
*               DCLIOLEN=CRRBLEN;   /* PUT RESIDUAL LENGTH FROM CRR
*                                      PARM LIST INTO LCPB           */
         LH    @10,CRRBLEN(,CRRPTR)                                0694
         STH   @10,DCLIOLEN(,LCPBPTR)                              0694
*               DCLBTLEN=CRRBTLEN;  /* PUT TOTAL LENGTH FROM CRR PARM
*                                      LIST INTO LCPB                */
         L     @10,CRRBTLEN(,CRRPTR)                               0695
         ST    @10,DCLBTLEN(,LCPBPTR)                              0695
*               DCLRPLA=RPLPTR;     /* PUT ADDR RPL IN LCPB          */
         ST    RPLPTR,DCLRPLA(,LCPBPTR)                            0696
*               DCLRQTP=DCLREAD;    /* MAKE REQUEST CODE READ        */
         MVI   DCLRQTP(LCPBPTR),X'1D'                              0697
*               CRRSUBBK=ON;        /* INDICATE 'AWAITING SUBBLOCK'
*                                      IN CRR                        */
         OI    CRRSUBBK(CRRPTR),B'00000100'                        0698
*             END;                  /* END OF FSBPTR=0 DO GROUP      */
*         ELSE                                                     0700
*           ;                       /* FSB DIDN'T PT TO LCPB K0931   */
@RF00689 DS    0H                                                  0701
*       ELSE                                                       0701
*         DO;                       /* BUT IF THERE ARE SUBBLOCKS    */
         B     @RC00688                                            0701
@RF00688 DS    0H                                                  0702
*           CRRFSB=FSBPTR;          /* PUT FSB ADDR IN CRR           */
         ST    FSBPTR,CRRFSB(,CRRPTR)                              0702
*           CRRSW0=ON;              /* INDICATE TO PROCESSC - 'MORE
*                                      DATA TO PROCESS'              */
         OI    CRRSW0(CRRPTR),B'10000000'                          0703
*         END;                                                     0704
*     END;                                                         0705
*   ELSE                                                           0706
*     ;                                                            0706
@RF00670 DS    0H                                                  0707
*   RFY                                                            0707
*     RESUMEAD RSTD;                                               0707
@RC00670 DS    0H                                                  0708
*   RESUMEAD=CRRRTN4;               /* RESTORE RETURN ADDR (REG14)   */
         L     RESUMEAD,CRRRTN4(,CRRPTR)                           0708
*   END PROCESSD;                   /* RETURN TO PROCESCA            */
@EL00005 DS    0H                                                  0709
@EF00005 DS    0H                                                  0709
@ER00005 BR    @14                                                 0709
