@RF00631 DS    0H                                                  0634
*   IF CRRBRTCD=CRRBMDAT THEN       /* IF MOVE HAS INDICATED 'MORE 0634
*                                      DATA TO BE MOVED'             */
         CLI   CRRBRTCD(CRRPTR),4                                  0634
         BNE   @RF00634                                            0634
*     CRRSW2=ON;                    /* INDICATE THAT TRUNCATION    0635
